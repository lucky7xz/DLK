{"textgrid.poem.26641": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Eroberer", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dir, Eroberer, dir schwellet mein Busen auf,", "tokens": ["Dir", ",", "Er\u00b7o\u00b7be\u00b7rer", ",", "dir", "schwel\u00b7let", "mein", "Bu\u00b7sen", "auf", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Dir zu fluchen den Fluch gl\u00fchenden Rachedursts,", "tokens": ["Dir", "zu", "flu\u00b7chen", "den", "Fluch", "gl\u00fc\u00b7hen\u00b7den", "Ra\u00b7che\u00b7dursts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Vor dem Auge der Sch\u00f6pfung,", "tokens": ["Vor", "dem", "Au\u00b7ge", "der", "Sch\u00f6p\u00b7fung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Vor des Ewigen Angesicht!", "tokens": ["Vor", "des", "E\u00b7wi\u00b7gen", "An\u00b7ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Wenn den horchenden Gang \u00fcber mir Luna geht,", "tokens": ["Wenn", "den", "hor\u00b7chen\u00b7den", "Gang", "\u00fc\u00b7ber", "mir", "Lu\u00b7na", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "PPER", "NE", "VVFIN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Wenn die Sterne der Nacht lauschend heruntersehn,", "tokens": ["Wenn", "die", "Ster\u00b7ne", "der", "Nacht", "lau\u00b7schend", "her\u00b7un\u00b7ter\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Tr\u00e4ume flattern \u2013 umflattern", "tokens": ["Tr\u00e4u\u00b7me", "flat\u00b7tern", "\u2013", "um\u00b7flat\u00b7tern"], "token_info": ["word", "word", "punct", "word"], "pos": ["NN", "VVINF", "$(", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Deine Bilder, o Sieger, mich", "tokens": ["Dei\u00b7ne", "Bil\u00b7der", ",", "o", "Sie\u00b7ger", ",", "mich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "FM", "NN", "$,", "PPER"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Und Entsetzen um sie \u2013 Fahr ich da w\u00fctend auf,", "tokens": ["Und", "Ent\u00b7set\u00b7zen", "um", "sie", "\u2013", "Fahr", "ich", "da", "w\u00fc\u00b7tend", "auf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPER", "$(", "VVIMP", "PPER", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Stampfe gegen die Erd, schalle mit Sturmgeheul", "tokens": ["Stamp\u00b7fe", "ge\u00b7gen", "die", "Erd", ",", "schal\u00b7le", "mit", "Sturm\u00b7ge\u00b7heul"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "$,", "VVFIN", "APPR", "NN"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Deinen Namen, Verworfner,", "tokens": ["Dei\u00b7nen", "Na\u00b7men", ",", "Ver\u00b7worf\u00b7ner", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "In die Ohren der Mitternacht.", "tokens": ["In", "die", "Oh\u00b7ren", "der", "Mit\u00b7ter\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Und mit offenem Schlund, welcher Gebirge schluckt,", "tokens": ["Und", "mit", "of\u00b7fe\u00b7nem", "Schlund", ",", "wel\u00b7cher", "Ge\u00b7bir\u00b7ge", "schluckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,", "PWAT", "NN", "VVFIN", "$,"], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ihn das Weltmeer mir nach \u2013 ihn mir der Orkus nach", "tokens": ["Ihn", "das", "Welt\u00b7meer", "mir", "nach", "\u2013", "ihn", "mir", "der", "Or\u00b7kus", "nach"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "PPER", "APPR", "$(", "PPER", "PPER", "ART", "NN", "APPR"], "meter": "+-++-+-+-+-+", "measure": "unknown.measure.septa"}, "line.3": {"text": "Durch die Hallen des Todes \u2013", "tokens": ["Durch", "die", "Hal\u00b7len", "des", "To\u00b7des", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Deinen Namen, Eroberer!", "tokens": ["Dei\u00b7nen", "Na\u00b7men", ",", "Er\u00b7o\u00b7be\u00b7rer", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Ha! dort schreitet er hin \u2013 dort, der Abscheuliche,", "tokens": ["Ha", "!", "dort", "schrei\u00b7tet", "er", "hin", "\u2013", "dort", ",", "der", "Ab\u00b7scheu\u00b7li\u00b7che", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPER", "ADV", "$(", "ADV", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-++--", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Durch die Schwerter, er ruft (und du, Erhabner, h\u00f6rsts),", "tokens": ["Durch", "die", "Schwer\u00b7ter", ",", "er", "ruft", "(", "und", "du", ",", "Er\u00b7hab\u00b7ner", ",", "h\u00f6rsts", ")", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PPER", "VVFIN", "$(", "KON", "PPER", "$,", "NN", "$,", "VVFIN", "$(", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Ruft, ruft: T\u00f6tet und schont nicht,", "tokens": ["Ruft", ",", "ruft", ":", "T\u00f6\u00b7tet", "und", "schont", "nicht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$.", "VVFIN", "KON", "VVFIN", "PTKNEG", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Und sie t\u00f6ten und schonen nicht.", "tokens": ["Und", "sie", "t\u00f6\u00b7ten", "und", "scho\u00b7nen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "PTKNEG", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Steigt hoch auf das Geheul \u2013 r\u00f6cheln die Sterbenden", "tokens": ["Steigt", "hoch", "auf", "das", "Ge\u00b7heul", "\u2013", "r\u00f6\u00b7cheln", "die", "Ster\u00b7ben\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$(", "VVFIN", "ART", "NN"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Unterm Blutgang des Siegs \u2013 V\u00e4ter, aus Wolken her", "tokens": ["Un\u00b7term", "Blut\u00b7gang", "des", "Siegs", "\u2013", "V\u00e4\u00b7ter", ",", "aus", "Wol\u00b7ken", "her"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "NN", "$(", "NN", "$,", "APPR", "NN", "APZR"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Schaut zur Schlachtbank der Kinder,", "tokens": ["Schaut", "zur", "Schlacht\u00b7bank", "der", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "V\u00e4ter, V\u00e4ter, und fluchet ihm.", "tokens": ["V\u00e4\u00b7ter", ",", "V\u00e4\u00b7ter", ",", "und", "flu\u00b7chet", "ihm", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "KON", "VVFIN", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.7": {"line.1": {"text": "Stolz auf t\u00fcrmt er sich nun, dampfendes Heldenblut", "tokens": ["Stolz", "auf", "t\u00fcrmt", "er", "sich", "nun", ",", "damp\u00b7fen\u00b7des", "Hel\u00b7den\u00b7blut"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "VVFIN", "PPER", "PRF", "ADV", "$,", "ADJA", "NN"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Trieft am Schwert hin, herab schimmerts, wie Meteor,", "tokens": ["Trieft", "am", "Schwert", "hin", ",", "her\u00b7ab", "schim\u00b7merts", ",", "wie", "Me\u00b7te\u00b7or", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PTKVZ", "$,", "ADV", "ADV", "$,", "PWAV", "NE", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Das zum Weltgericht winket \u2013", "tokens": ["Das", "zum", "Welt\u00b7ge\u00b7richt", "win\u00b7ket", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "APPRART", "NN", "VVFIN", "$("], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Erde, fleuch! der Erobrer kommt.", "tokens": ["Er\u00b7de", ",", "fleuch", "!", "der", "E\u00b7rob\u00b7rer", "kommt", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "--+----+", "measure": "anapaest.init"}}, "stanza.8": {"line.1": {"text": "Ha! Eroberer, sprich: Was ist dein hei\u00dfester,", "tokens": ["Ha", "!", "Er\u00b7o\u00b7be\u00b7rer", ",", "sprich", ":", "Was", "ist", "dein", "hei\u00b7\u00dfes\u00b7ter", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NN", "$,", "ADJD", "$.", "PWS", "VAFIN", "PPOSAT", "ADJA", "$,"], "meter": "+-+---+--+--", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Dein gesehntester Wunsch? \u2013 Hoch an des Himmels Saum", "tokens": ["Dein", "ge\u00b7sehn\u00b7tes\u00b7ter", "Wunsch", "?", "\u2013", "Hoch", "an", "des", "Him\u00b7mels", "Saum"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "$(", "ADJD", "APPR", "ART", "NN", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Einen Felsen zu b\u00e4umen,", "tokens": ["Ei\u00b7nen", "Fel\u00b7sen", "zu", "b\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Dessen Stirne der Adler scheut,", "tokens": ["Des\u00b7sen", "Stir\u00b7ne", "der", "Ad\u00b7ler", "scheut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.9": {"line.1": {"text": "Dann hernieder vom Berg, trunken von Siegeslust,", "tokens": ["Dann", "her\u00b7nie\u00b7der", "vom", "Berg", ",", "trun\u00b7ken", "von", "Sie\u00b7ges\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "--+--++--+--", "measure": "anapaest.di.plus"}, "line.2": {"text": "Auf die Tr\u00fcmmer der Welt, auf die Erobrungen", "tokens": ["Auf", "die", "Tr\u00fcm\u00b7mer", "der", "Welt", ",", "auf", "die", "E\u00b7rob\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+--++-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Hinzuschwindeln, im Taumel", "tokens": ["Hin\u00b7zu\u00b7schwin\u00b7deln", ",", "im", "Tau\u00b7mel"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "APPRART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Dieses Anblicks hinweggeschaut.", "tokens": ["Die\u00b7ses", "An\u00b7blicks", "hin\u00b7weg\u00b7ge\u00b7schaut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVPP", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.10": {"line.1": {"text": "O ihr wi\u00dft es noch nicht, welch ein Gef\u00fchl es ist,", "tokens": ["O", "ihr", "wi\u00dft", "es", "noch", "nicht", ",", "welch", "ein", "Ge\u00b7f\u00fchl", "es", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "$,", "PWAT", "ART", "NN", "PPER", "VAFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Welch Elysium schon in dem Gedanken bl\u00fcht,", "tokens": ["Welch", "E\u00b7ly\u00b7si\u00b7um", "schon", "in", "dem", "Ge\u00b7dan\u00b7ken", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Bleicher Feinde Entsetzen,", "tokens": ["Blei\u00b7cher", "Fein\u00b7de", "Ent\u00b7set\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Schrecken zitternder Welt zu sein,", "tokens": ["Schre\u00b7cken", "zit\u00b7tern\u00b7der", "Welt", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "PTKZU", "VAINF", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.11": {"line.1": {"text": "Mit allm\u00e4chtigem Sto\u00df hoch aus dem Pole, dann", "tokens": ["Mit", "all\u00b7m\u00e4ch\u00b7ti\u00b7gem", "Sto\u00df", "hoch", "aus", "dem", "Po\u00b7le", ",", "dann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ADJA", "NN", "ADJD", "APPR", "ART", "NN", "$,", "ADV"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Auszusto\u00dfen die Welt, fliegenden Schiffen gleich", "tokens": ["Aus\u00b7zu\u00b7sto\u00b7\u00dfen", "die", "Welt", ",", "flie\u00b7gen\u00b7den", "Schif\u00b7fen", "gleich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "ADJA", "NN", "ADV"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Sternenan sie zu rudern,", "tokens": ["Ster\u00b7ne\u00b7nan", "sie", "zu", "ru\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Auch der Sterne Monarch zu sein.", "tokens": ["Auch", "der", "Ster\u00b7ne", "Mon\u00b7arch", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "PTKZU", "VAINF", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.12": {"line.1": {"text": "Dann vom obersten Thron, dort wo Jehova stand,", "tokens": ["Dann", "vom", "o\u00b7bers\u00b7ten", "Thron", ",", "dort", "wo", "Je\u00b7ho\u00b7va", "stand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,", "ADV", "PWAV", "NE", "VVFIN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Auf der Himmel Ruin, auf die zertr\u00fcmmerte", "tokens": ["Auf", "der", "Him\u00b7mel", "Ru\u00b7in", ",", "auf", "die", "zer\u00b7tr\u00fcm\u00b7mer\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "$,", "APPR", "ART", "ADJA"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Sph\u00e4ren niederzutaumeln \u2013", "tokens": ["Sph\u00e4\u00b7ren", "nie\u00b7der\u00b7zu\u00b7tau\u00b7meln", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "O das f\u00fchlt der Erobrer nur!", "tokens": ["O", "das", "f\u00fchlt", "der", "E\u00b7rob\u00b7rer", "nur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDS", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.13": {"line.1": {"text": "Wenn die bl\u00fchendste 