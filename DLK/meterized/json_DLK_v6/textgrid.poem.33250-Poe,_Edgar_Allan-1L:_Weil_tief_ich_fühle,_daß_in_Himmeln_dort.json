{"textgrid.poem.33250": {"metadata": {"author": {"name": "Poe, Edgar Allan", "birth": "N.A.", "death": "N.A."}, "title": "1L: Weil tief ich f\u00fchle, da\u00df in Himmeln dort", "genre": "verse", "period": "N.A.", "pub_year": 1829, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil tief ich f\u00fchle, da\u00df in Himmeln dort", "tokens": ["Weil", "tief", "ich", "f\u00fch\u00b7le", ",", "da\u00df", "in", "Him\u00b7meln", "dort"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "PPER", "VVFIN", "$,", "KOUS", "APPR", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Engel, wenn sie Liebe-Worte nennen,", "tokens": ["Die", "En\u00b7gel", ",", "wenn", "sie", "Lie\u00b7be\u00b7Wor\u00b7te", "nen\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kein heilig-hei\u00dfer und kein inniger Wort", "tokens": ["Kein", "hei\u00b7lig\u00b7hei\u00b7\u00dfer", "und", "kein", "in\u00b7ni\u00b7ger", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "KON", "PIAT", "ADJA", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Als \u00bbMutter\u00ab zueinander fl\u00fcstern k\u00f6nnen,", "tokens": ["Als", "\u00bb", "Mut\u00b7ter", "\u00ab", "zu\u00b7ei\u00b7nan\u00b7der", "fl\u00fcs\u00b7tern", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "NN", "$(", "ADV", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Drum gab ich diesen liebsten Namen dir \u2013", "tokens": ["Drum", "gab", "ich", "die\u00b7sen", "liebs\u00b7ten", "Na\u00b7men", "dir", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PDAT", "ADJA", "NN", "PPER", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die \u2013 mehr denn Mutter mir in meinen Schmerzen \u2013", "tokens": ["Die", "\u2013", "mehr", "denn", "Mut\u00b7ter", "mir", "in", "mei\u00b7nen", "Schmer\u00b7zen", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "ADV", "KON", "NN", "PPER", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Tod, als er Virginias Geist von hier", "tokens": ["Der", "Tod", ",", "als", "er", "Vir\u00b7gi\u00b7ni\u00b7as", "Geist", "von", "hier"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "NE", "NN", "APPR", "ADV"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Befreit, zum Horte setzte meinem Herzen.", "tokens": ["Be\u00b7freit", ",", "zum", "Hor\u00b7te", "setz\u00b7te", "mei\u00b7nem", "Her\u00b7zen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die eigne Mutter, die schon fr\u00fch mir starb,", "tokens": ["Die", "eig\u00b7ne", "Mut\u00b7ter", ",", "die", "schon", "fr\u00fch", "mir", "starb", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "War ", "tokens": ["War"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Von ", "tokens": ["Von"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}}, "stanza.4": {"line.1": {"text": "Und so viel mehr, als meiner Seele ist", "tokens": ["Und", "so", "viel", "mehr", ",", "als", "mei\u00b7ner", "See\u00b7le", "ist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ADV", "$,", "KOUS", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein Weib denn meiner Seele eignes Leben,", "tokens": ["Mein", "Weib", "denn", "mei\u00b7ner", "See\u00b7le", "eig\u00b7nes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mu\u00df ich auch dir denn eigner Mutter geben.", "tokens": ["Mu\u00df", "ich", "auch", "dir", "denn", "eig\u00b7ner", "Mut\u00b7ter", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}