{"textgrid.poem.56994": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Das Lied vom blonden Korken", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein blonder Korke spiegelt sich", "tokens": ["Ein", "blon\u00b7der", "Kor\u00b7ke", "spie\u00b7gelt", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "in einem Lacktablett \u2013", "tokens": ["in", "ei\u00b7nem", "Lack\u00b7tab\u00b7lett", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "allein er s\u00e4h sich dennoch nicht,", "tokens": ["al\u00b7lein", "er", "s\u00e4h", "sich", "den\u00b7noch", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PRF", "ADV", "PTKNEG", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "selbst wenn er Augen h\u00e4tt:", "tokens": ["selbst", "wenn", "er", "Au\u00b7gen", "h\u00e4tt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "NN", "VAFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Das macht, dieweil er senkrecht steigt", "tokens": ["Das", "macht", ",", "die\u00b7weil", "er", "sen\u00b7krecht", "steigt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "zu seinem Spiegelbild!", "tokens": ["zu", "sei\u00b7nem", "Spie\u00b7gel\u00b7bild", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn man ihn freilich seitw\u00e4rts neigt,", "tokens": ["Wenn", "man", "ihn", "frei\u00b7lich", "seit\u00b7w\u00e4rts", "neigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "zerf\u00e4llt, was oben gilt.", "tokens": ["zer\u00b7f\u00e4llt", ",", "was", "o\u00b7ben", "gilt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O Mensch, gesetzt du spiegelst dich", "tokens": ["O", "Mensch", ",", "ge\u00b7setzt", "du", "spie\u00b7gelst", "dich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VVPP", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "im, sagen wir, \u2013 im All!", "tokens": ["im", ",", "sa\u00b7gen", "wir", ",", "\u2013", "im", "All", "!"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["APPRART", "$,", "VVFIN", "PPER", "$,", "$(", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und senkrecht! \u2013 w\u00e4rest du dann nicht", "tokens": ["Und", "sen\u00b7krecht", "!", "\u2013", "w\u00e4\u00b7rest", "du", "dann", "nicht"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "$(", "VAFIN", "PPER", "ADV", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ganz in dem gleichen Fall?", "tokens": ["ganz", "in", "dem", "glei\u00b7chen", "Fall", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}