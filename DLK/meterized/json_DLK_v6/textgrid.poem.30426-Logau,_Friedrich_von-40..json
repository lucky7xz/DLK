{"textgrid.poem.30426": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "40.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Weiber sind die Monden; die M\u00e4nner sind die Sonne;", "tokens": ["Die", "Wei\u00b7ber", "sind", "die", "Mon\u00b7den", ";", "die", "M\u00e4n\u00b7ner", "sind", "die", "Son\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$.", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Von diesen haben jene Nutz, Ehre, W\u00e4rmde, Wonne.", "tokens": ["Von", "die\u00b7sen", "ha\u00b7ben", "je\u00b7ne", "Nutz", ",", "Eh\u00b7re", ",", "W\u00e4rm\u00b7de", ",", "Won\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PDAT", "VAFIN", "PDAT", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die Sonne herrscht den Tag; der Monde herrscht die Nacht;", "tokens": ["Die", "Son\u00b7ne", "herrscht", "den", "Tag", ";", "der", "Mon\u00b7de", "herrscht", "die", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$.", "ART", "NE", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bey Nachte hat das Weib, der Mann bey Tage Macht.", "tokens": ["Bey", "Nach\u00b7te", "hat", "das", "Weib", ",", "der", "Mann", "bey", "Ta\u00b7ge", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "$,", "ART", "NN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}