{"textgrid.poem.43340": {"metadata": {"author": {"name": "Leopardi, Giacomo", "birth": "N.A.", "death": "N.A."}, "title": "ViI. An den Fr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun alle Himmelsunbill", "tokens": ["Nun", "al\u00b7le", "Him\u00b7mel\u00b7sun\u00b7bill"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Sonne s\u00fchnt und lauer West gelinde", "tokens": ["Die", "Son\u00b7ne", "s\u00fchnt", "und", "lau\u00b7er", "West", "ge\u00b7lin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "KON", "ADJA", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die kranke Luft belebt, da\u00df fortgescheucht", "tokens": ["Die", "kran\u00b7ke", "Luft", "be\u00b7lebt", ",", "da\u00df", "fort\u00b7ge\u00b7scheucht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,", "KOUS", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Wolken schwerer Schatten niedersinkt,", "tokens": ["Der", "Wol\u00b7ken", "schwe\u00b7rer", "Schat\u00b7ten", "nie\u00b7der\u00b7sinkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die V\u00f6gel neu dem Winde", "tokens": ["Die", "V\u00f6\u00b7gel", "neu", "dem", "Win\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die nackte Brust vertrauen und das Licht", "tokens": ["Die", "nack\u00b7te", "Brust", "ver\u00b7trau\u00b7en", "und", "das", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF", "KON", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Mit neuem Liebessehnen, neuer Hoffnung", "tokens": ["Mit", "neu\u00b7em", "Lie\u00b7bes\u00b7seh\u00b7nen", ",", "neu\u00b7er", "Hoff\u00b7nung"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sogar das Wild auf dunklen Waldespfaden", "tokens": ["So\u00b7gar", "das", "Wild", "auf", "dunk\u00b7len", "Wal\u00b7des\u00b7pfa\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Belebt, wenn kaum der Nebelduft gewichen:", "tokens": ["Be\u00b7lebt", ",", "wenn", "kaum", "der", "Ne\u00b7bel\u00b7duft", "ge\u00b7wi\u00b7chen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Kehrt auch vielleicht zu euch, so grambeladen", "tokens": ["Kehrt", "auch", "viel\u00b7leicht", "zu", "euch", ",", "so", "gram\u00b7be\u00b7la\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "PPER", "$,", "ADV", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und m\u00fcd, ihr Menschenseelen,", "tokens": ["Und", "m\u00fcd", ",", "ihr", "Men\u00b7schen\u00b7see\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Die sch\u00f6ne Zeit, die Ungl\u00fcck und die d\u00fcstre", "tokens": ["Die", "sch\u00f6\u00b7ne", "Zeit", ",", "die", "Un\u00b7gl\u00fcck", "und", "die", "d\u00fcst\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "KON", "ART", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Fackel der Wahrheit euch", "tokens": ["Fa\u00b7ckel", "der", "Wahr\u00b7heit", "euch"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "PPER"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.14": {"text": "So fr\u00fch zerst\u00f6rt? Sind Ph\u00f6bus' goldne Strahlen", "tokens": ["So", "fr\u00fch", "zer\u00b7st\u00f6rt", "?", "Sind", "Ph\u00f6\u00b7bus'", "gold\u00b7ne", "Strah\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVPP", "$.", "VAFIN", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Dem Armen nicht f\u00fcr ew'ge Zeit verdunkelt", "tokens": ["Dem", "Ar\u00b7men", "nicht", "f\u00fcr", "ew'\u00b7ge", "Zeit", "ver\u00b7dun\u00b7kelt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKNEG", "APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Und ausgel\u00f6scht? Und du auch,", "tokens": ["Und", "aus\u00b7ge\u00b7l\u00f6scht", "?", "Und", "du", "auch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "$.", "KON", "PPER", "ADV", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.17": {"text": "Duftender Lenz, willst du die eis'gen Qualen", "tokens": ["Duf\u00b7ten\u00b7der", "Lenz", ",", "willst", "du", "die", "eis'\u00b7gen", "Qua\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VMFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.18": {"text": "Wegthau'n der Brust, die schon in jungen Tagen", "tokens": ["Wegt\u00b7hau'n", "der", "Brust", ",", "die", "schon", "in", "jun\u00b7gen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "ADV", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.19": {"text": "Gelernt das herbe Weh des Alters tragen?", "tokens": ["Ge\u00b7lernt", "das", "her\u00b7be", "Weh", "des", "Al\u00b7ters", "tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Lebst du, o lebst du, heil'ge", "tokens": ["Lebst", "du", ",", "o", "lebst", "du", ",", "heil'\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "$,", "FM", "VVFIN", "PPER", "$,", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Natur? Lebst du, und ist's der Mutter Sprache,", "tokens": ["Na\u00b7tur", "?", "Lebst", "du", ",", "und", "ist's", "der", "Mut\u00b7ter", "Spra\u00b7che", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "$,", "KON", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die lauschend das entw\u00f6hnte Ohr vernimmt?", "tokens": ["Die", "lau\u00b7schend", "das", "ent\u00b7w\u00f6hn\u00b7te", "Ohr", "ver\u00b7nimmt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Einst wohnten holde Nymphen in den Fl\u00fcssen,", "tokens": ["Einst", "wohn\u00b7ten", "hol\u00b7de", "Nym\u00b7phen", "in", "den", "Fl\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dort und im klaren Bache", "tokens": ["Dort", "und", "im", "kla\u00b7ren", "Ba\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KON", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das Antlitz spiegelnd; von geheimen T\u00e4nzen", "tokens": ["Das", "Ant\u00b7litz", "spie\u00b7gelnd", ";", "von", "ge\u00b7hei\u00b7men", "T\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "G\u00f6ttlicher F\u00fc\u00dfe bebten Bergesh\u00f6h'n", "tokens": ["G\u00f6tt\u00b7li\u00b7cher", "F\u00fc\u00b7\u00dfe", "beb\u00b7ten", "Ber\u00b7ges\u00b7h\u00f6h'n"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Und hohe W\u00e4lder, jetzt den St\u00fcrmen nur", "tokens": ["Und", "ho\u00b7he", "W\u00e4l\u00b7der", ",", "jetzt", "den", "St\u00fcr\u00b7men", "nur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$,", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ein \u00f6der Wohnsitz, und der Hirt, im Duft", "tokens": ["Ein", "\u00f6\u00b7der", "Wohn\u00b7sitz", ",", "und", "der", "Hirt", ",", "im", "Duft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "ART", "NN", "$,", "APPRART", "NN"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.10": {"text": "Des Mittags, wenn er durch die blum'ge Flur", "tokens": ["Des", "Mit\u00b7tags", ",", "wenn", "er", "durch", "die", "blum'\u00b7ge", "Flur"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.11": {"text": "Zum Flu\u00df die durst'gen L\u00e4mmer", "tokens": ["Zum", "Flu\u00df", "die", "dur\u00b7st'\u00b7gen", "L\u00e4m\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Hinuntertrieb, vernahm ein helles Lied", "tokens": ["Hin\u00b7un\u00b7ter\u00b7trieb", ",", "ver\u00b7nahm", "ein", "hel\u00b7les", "Lied"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Des Waldgotts l\u00e4ngs dem Ufer,", "tokens": ["Des", "Wald\u00b7gotts", "l\u00e4ngs", "dem", "U\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Sah kr\u00e4useln sich die Flut", "tokens": ["Sah", "kr\u00e4u\u00b7seln", "sich", "die", "Flut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Und stand verdutzt, wenn jedem Blick verh\u00fcllt", "tokens": ["Und", "stand", "ver\u00b7dutzt", ",", "wenn", "je\u00b7dem", "Blick", "ver\u00b7h\u00fcllt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "VVPP", "$,", "KOUS", "PIAT", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Die pfeilbewehrte G\u00f6ttin", "tokens": ["Die", "pfeil\u00b7be\u00b7wehr\u00b7te", "G\u00f6t\u00b7tin"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Stieg in die lauen Wellen, Staub und Blut", "tokens": ["Stieg", "in", "die", "lau\u00b7en", "Wel\u00b7len", ",", "Staub", "und", "Blut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Der hei\u00dfen Jagd vom schneeigen Arm zu sp\u00fclen", "tokens": ["Der", "hei\u00b7\u00dfen", "Jagd", "vom", "schne\u00b7e\u00b7i\u00b7gen", "Arm", "zu", "sp\u00fc\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und ihren jungfr\u00e4ulichen Leib zu k\u00fchlen.", "tokens": ["Und", "ih\u00b7ren", "jung\u00b7fr\u00e4u\u00b7li\u00b7chen", "Leib", "zu", "k\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Es ", "tokens": ["Es"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Es ", "tokens": ["Es"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Die L\u00fcfte, Wolken, Titan's hehre Leuchte", "tokens": ["Die", "L\u00fcf\u00b7te", ",", "Wol\u00b7ken", ",", "Ti\u00b7tan's", "heh\u00b7re", "Leuch\u00b7te"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "$,", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem sterblichen Geschlecht, als \u00fcber Auen", "tokens": ["Dem", "sterb\u00b7li\u00b7chen", "Ge\u00b7schlecht", ",", "als", "\u00fc\u00b7ber", "Au\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und H\u00fcgeln deinem klaren", "tokens": ["Und", "H\u00fc\u00b7geln", "dei\u00b7nem", "kla\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "PPOSAT", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gestirn, o Cypria, der Wandrer folgend", "tokens": ["Ge\u00b7stirn", ",", "o", "Cyp\u00b7ria", ",", "der", "Wand\u00b7rer", "fol\u00b7gend"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "FM", "FM", "$,", "ART", "NN", "ADJD"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Mit Sehnsuchtsblicken in der stillen Nacht", "tokens": ["Mit", "Sehn\u00b7suchts\u00b7bli\u00b7cken", "in", "der", "stil\u00b7len", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Dich als Gesellin seiner Fahrt, voll Huld", "tokens": ["Dich", "als", "Ge\u00b7sel\u00b7lin", "sei\u00b7ner", "Fahrt", ",", "voll", "Huld"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "KOUS", "NN", "PPOSAT", "NN", "$,", "ADJD", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.9": {"text": "Den Menschen tr\u00e4umte. Wenn, entflohn dem Treiben", "tokens": ["Den", "Men\u00b7schen", "tr\u00e4um\u00b7te", ".", "Wenn", ",", "ent\u00b7flohn", "dem", "Trei\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "KOUS", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Der w\u00fcsten St\u00e4dte voller S\u00fcnd' und Schuld", "tokens": ["Der", "w\u00fcs\u00b7ten", "St\u00e4d\u00b7te", "vol\u00b7ler", "S\u00fcnd'", "und", "Schuld"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und Zwist und roher Schmach,", "tokens": ["Und", "Zwist", "und", "ro\u00b7her", "Schmach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Ein Andrer rauhe St\u00e4mme tief im Wald", "tokens": ["Ein", "A\u00b7ndrer", "rau\u00b7he", "St\u00e4m\u00b7me", "tief", "im", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "An seinen Busen dr\u00fcckte,", "tokens": ["An", "sei\u00b7nen", "Bu\u00b7sen", "dr\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "W\u00e4hnt' er zu f\u00fchlen, wie lebend'ges Feuer", "tokens": ["W\u00e4hnt'", "er", "zu", "f\u00fch\u00b7len", ",", "wie", "le\u00b7ben\u00b7d'\u00b7ges", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKZU", "VVINF", "$,", "PWAV", "ADJA", "NN"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.15": {"text": "Blutlosen Stamm durchlodre, wie erbebe", "tokens": ["Blut\u00b7lo\u00b7sen", "Stamm", "durch\u00b7lod\u00b7re", ",", "wie", "er\u00b7be\u00b7be"], "token_info": ["word", "word", "word", "punct