{"textgrid.poem.24363": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Uber den Churf\u00fcrsten von Hannover", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df/ gro\u00df- und theurer F\u00fcrst/ die Chur dein Hau\u00df bestrahlet/", "tokens": ["Da\u00df", "/", "gro\u00df", "und", "theu\u00b7rer", "F\u00fcrst", "/", "die", "Chur", "dein", "Hau\u00df", "be\u00b7strah\u00b7let", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "TRUNC", "KON", "ADJD", "NN", "$(", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das dein Erlauchtes Blut zu Cronen wird ersehn/", "tokens": ["Das", "dein", "Er\u00b7lauch\u00b7tes", "Blut", "zu", "Cro\u00b7nen", "wird", "er\u00b7sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df dich die Welt verehrt/ dir tausend W\u00fcnsche zahlet/", "tokens": ["Da\u00df", "dich", "die", "Welt", "ver\u00b7ehrt", "/", "dir", "tau\u00b7send", "W\u00fcn\u00b7sche", "zah\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$(", "PPER", "CARD", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dein Reich im Seegen mu\u00df/ und du im Gl\u00fccke stehn/", "tokens": ["Dein", "Reich", "im", "See\u00b7gen", "mu\u00df", "/", "und", "du", "im", "Gl\u00fc\u00b7cke", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VMFIN", "$(", "KON", "PPER", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "K\u00f6mt nicht von ohngefehr: Zwey Dinge haben Schuld/", "tokens": ["K\u00f6mt", "nicht", "von", "ohn\u00b7ge\u00b7fehr", ":", "Zwey", "Din\u00b7ge", "ha\u00b7ben", "Schuld", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "ADJD", "$.", "CARD", "NN", "VAFIN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Tugend kluge Macht/ und deines Himmels Huld.", "tokens": ["Der", "Tu\u00b7gend", "klu\u00b7ge", "Macht", "/", "und", "dei\u00b7nes", "Him\u00b7mels", "Huld", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}