{"textgrid.poem.49334": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Reisegesang", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So brech' ich auf von diesem Ort", "tokens": ["So", "brech'", "ich", "auf", "von", "die\u00b7sem", "Ort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und zieh' in deinem Namen fort,", "tokens": ["Und", "zieh'", "in", "dei\u00b7nem", "Na\u00b7men", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Herr Gott, du wirst mich gleiten", "tokens": ["Herr", "Gott", ",", "du", "wirst", "mich", "glei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "$,", "PPER", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und \u00fcber mich, dein liebes Kind,", "tokens": ["Und", "\u00fc\u00b7ber", "mich", ",", "dein", "lie\u00b7bes", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das gar nichts ist als Staub und Wind,", "tokens": ["Das", "gar", "nichts", "ist", "als", "Staub", "und", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PIS", "VAFIN", "KOKOM", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Gnadenfl\u00fcgel breiten,", "tokens": ["Die", "Gna\u00b7den\u00b7fl\u00fc\u00b7gel", "brei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Damit ich mag vor allen Dingen", "tokens": ["Da\u00b7mit", "ich", "mag", "vor", "al\u00b7len", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VMFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Reise gl\u00fccklich vollenbringen.", "tokens": ["Die", "Rei\u00b7se", "gl\u00fcck\u00b7lich", "vol\u00b7len\u00b7brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Gib, da\u00df die lieben Engelein,", "tokens": ["Gib", ",", "da\u00df", "die", "lie\u00b7ben", "En\u00b7ge\u00b7lein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die starken Helden, bei mir sein", "tokens": ["Die", "star\u00b7ken", "Hel\u00b7den", ",", "bei", "mir", "sein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PPER", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Auf allen meinen Wegen", "tokens": ["Auf", "al\u00b7len", "mei\u00b7nen", "We\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Und zwischen die, so dieser Zeit", "tokens": ["Und", "zwi\u00b7schen", "die", ",", "so", "die\u00b7ser", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "$,", "ADV", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Mir nachzustellen sind bereit,", "tokens": ["Mir", "nach\u00b7zu\u00b7stel\u00b7len", "sind", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und zwischen mich sich legen;", "tokens": ["Und", "zwi\u00b7schen", "mich", "sich", "le\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Herr, sch\u00fctze mich durch deine Gnade,", "tokens": ["Herr", ",", "sch\u00fct\u00b7ze", "mich", "durch", "dei\u00b7ne", "Gna\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "So trifft mich weder Schimpf noch Schade.", "tokens": ["So", "trifft", "mich", "we\u00b7der", "Schimpf", "noch", "Scha\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Viel treuer W\u00e4chter hast du mir", "tokens": ["Viel", "treu\u00b7er", "W\u00e4ch\u00b7ter", "hast", "du", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NE", "VAFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Verordnet, da\u00df sie f\u00fcr und f\u00fcr", "tokens": ["Ver\u00b7ord\u00b7net", ",", "da\u00df", "sie", "f\u00fcr", "und", "f\u00fcr"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "APPR", "KON", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Mein Leben wol bewahren;", "tokens": ["Mein", "Le\u00b7ben", "wol", "be\u00b7wah\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Wo sie nun brauchen ihre Macht,", "tokens": ["Wo", "sie", "nun", "brau\u00b7chen", "ih\u00b7re", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Da kan mir weder Tag noch Nacht", "tokens": ["Da", "kan", "mir", "we\u00b7der", "Tag", "noch", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "KON", "NN", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Kein Arges widerfahren,", "tokens": ["Kein", "Ar\u00b7ges", "wi\u00b7der\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Denn diese Geister sind verbunden,", "tokens": ["Denn", "die\u00b7se", "Geis\u00b7ter", "sind", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Vor mich zu wachen alle Stunden.", "tokens": ["Vor", "mich", "zu", "wa\u00b7chen", "al\u00b7le", "Stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Und sol ich denn mein t\u00e4glich Brot,", "tokens": ["Und", "sol", "ich", "denn", "mein", "t\u00e4g\u00b7lich", "Brot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Auch was mir sonst zum Leben not,", "tokens": ["Auch", "was", "mir", "sonst", "zum", "Le\u00b7ben", "not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "PPER", "ADV", "APPRART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "In meinem Haus erwerben,", "tokens": ["In", "mei\u00b7nem", "Haus", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "So bleibe du mein H\u00fclf' und Schutz,", "tokens": ["So", "blei\u00b7be", "du", "mein", "H\u00fcl\u00b7f'", "und", "Schutz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.29": {"text": "Vertreibe weit des Satans Trutz", "tokens": ["Ver\u00b7trei\u00b7be", "weit", "des", "Sa\u00b7tans", "Trutz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Und la\u00df mich nicht verderben.", "tokens": ["Und", "la\u00df", "mich", "nicht", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Wilt du mir nur dein' Hand verleihen,", "tokens": ["Wilt", "du", "mir", "nur", "dein'", "Hand", "ver\u00b7lei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "So darf ich gar kein Ungl\u00fcck scheuen.", "tokens": ["So", "darf", "ich", "gar", "kein", "Un\u00b7gl\u00fcck", "scheu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Sol ich mich aber f\u00fcgen hin,", "tokens": ["Sol", "ich", "mich", "a\u00b7ber", "f\u00fc\u00b7gen", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Wo m\u00fcglich ich ein Fremder bin,", "tokens": ["Wo", "m\u00fcg\u00b7lich", "ich", "ein", "Frem\u00b7der", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Und hin und wieder reisen,", "tokens": ["Und", "hin", "und", "wie\u00b7der", "rei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "KON", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "So wolle ja dein g\u00f6ttlichs Licht", "tokens": ["So", "wol\u00b7le", "ja", "dein", "g\u00f6tt\u00b7lichs", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Mich auf der Fahrt verlassen nicht,", "tokens": ["Mich", "auf", "der", "Fahrt", "ver\u00b7las\u00b7sen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Besondern mir erweisen,", "tokens": ["Be\u00b7son\u00b7dern", "mir", "er\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.39": {"text": "Da\u00df du, mein Gott, zu jeden Zeiten", "tokens": ["Da\u00df", "du", ",", "mein", "Gott", ",", "zu", "je\u00b7den", "Zei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "PPOSAT", "NN", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Zugegen bist den Wandersleuten.", "tokens": ["Zu\u00b7ge\u00b7gen", "bist", "den", "Wan\u00b7ders\u00b7leu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Dieweil auch sind der Feinde viel,", "tokens": ["Die\u00b7weil", "auch", "sind", "der", "Fein\u00b7de", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "So f\u00fchre mich zum rechten Ziel,", "tokens": ["So", "f\u00fch\u00b7re", "mich", "zum", "rech\u00b7ten", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "O Herr, auf allen Stra\u00dfen;", "tokens": ["O", "Herr", ",", "auf", "al\u00b7len", "Stra\u00b7\u00dfen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.44": {"text": "La\u00df deine Diener bei mir stehn,", "tokens": ["La\u00df", "dei\u00b7ne", "Die\u00b7ner", "bei", "mir", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Da\u00df, wie Tobias ist geschehn,", "tokens": ["Da\u00df", ",", "wie", "To\u00b7bias", "ist", "ge\u00b7schehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "NE", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.46": {"text": "Sie nimmer von mir lassen;", "tokens": ["Sie", "nim\u00b7mer", "von", "mir", "las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.47": {"text": "Denn wann mich diese Helden f\u00fchren,", "tokens": ["Denn", "wann", "mich", "die\u00b7se", "Hel\u00b7den", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "So kan kein Unfall mich ber\u00fchren.", "tokens": ["So", "kan", "kein", "Un\u00b7fall", "mich", "be\u00b7r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "Herr, biete mir die Gnadenhand,", "tokens": ["Herr", ",", "bie\u00b7te", "mir", "die", "Gna\u00b7den\u00b7hand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Ich sei zu Wasser oder Land,", "tokens": ["Ich", "sei", "zu", "Was\u00b7ser", "o\u00b7der", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "In Feldern, W\u00e4ldern, Hecken,", "tokens": ["In", "Fel\u00b7dern", ",", "W\u00e4l\u00b7dern", ",", "He\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.52": {"text": "Da wirst du mich in aller Not", "tokens": ["Da", "wirst", "du", "mich", "in", "al\u00b7ler", "Not"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "F\u00fcr R\u00e4ubern, Fallen, Schand und Tod", "tokens": ["F\u00fcr", "R\u00e4u\u00b7bern", ",", "Fal\u00b7len", ",", "Schand", "und", "Tod"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "Mit deiner Macht bedecken;", "tokens": ["Mit", "dei\u00b7ner", "Macht", "be\u00b7de\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.55": {"text": "Wenn du mir nur wilt H\u00fclf' erteilen,", "tokens": ["Wenn", "du", "mir", "nur", "wilt", "H\u00fcl\u00b7f'", "er\u00b7tei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VMFIN", "NN", "VVINF", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.56": {"text": "So kan kein Unfall mich ereilen.", "tokens": ["So", "kan", "kein", "Un\u00b7fall", "mich", "er\u00b7ei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.57": {"text": "Solt ich auch kommen, wo das Gift", "tokens": ["Solt", "ich", "auch", "kom\u00b7men", ",", "wo", "das", "Gift"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.58": {"text": "Der schnellen Pest die Menschen trifft", "tokens": ["Der", "schnel\u00b7len", "Pest", "die", "Men\u00b7schen", "trifft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.59": {"text": "Und durch die L\u00e4nder w\u00fctet,", "tokens": ["Und", "durch", "die", "L\u00e4n\u00b7der", "w\u00fc\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.60": {"text": "So sch\u00fctze mich nach deinem Rat:", "tokens": ["So", "sch\u00fct\u00b7ze", "mich", "nach", "dei\u00b7nem", "Rat", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.61": {"text": "Ich wei\u00df, der dich zum F\u00fchrer hat,", "tokens": ["Ich", "wei\u00df", ",", "der", "dich", "zum", "F\u00fch\u00b7rer", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "PRF", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.62": {"text": "Der bleibet wol beh\u00fctet;", "tokens": ["Der", "blei\u00b7bet", "wol", "be\u00b7h\u00fc\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.63": {"text": "Sind doch mein Haar' also gez\u00e4hlet,", "toke