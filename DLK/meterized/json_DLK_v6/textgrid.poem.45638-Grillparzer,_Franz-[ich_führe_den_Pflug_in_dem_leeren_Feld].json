{"textgrid.poem.45638": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[ich f\u00fchre den Pflug in dem leeren Feld]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich f\u00fchre den Pflug in dem leeren Feld,", "tokens": ["Ich", "f\u00fch\u00b7re", "den", "Pflug", "in", "dem", "lee\u00b7ren", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da wird denn nach mir die Scholle bestellt", "tokens": ["Da", "wird", "denn", "nach", "mir", "die", "Schol\u00b7le", "be\u00b7stellt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Von manchem, der besser und kl\u00fcger.", "tokens": ["Von", "man\u00b7chem", ",", "der", "bes\u00b7ser", "und", "kl\u00fc\u00b7ger", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "$,", "PRELS", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Doch wie sie auch reich die Ernte bringt,", "tokens": ["Doch", "wie", "sie", "auch", "reich", "die", "Ern\u00b7te", "bringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Denk doch, wenn schon wartend die Sichel klingt,", "tokens": ["Denk", "doch", ",", "wenn", "schon", "war\u00b7tend", "die", "Si\u00b7chel", "klingt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "KOUS", "ADV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "An den heimgegangenen Pfl\u00fcger.", "tokens": ["An", "den", "heim\u00b7ge\u00b7gan\u00b7ge\u00b7nen", "Pfl\u00fc\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}}}}