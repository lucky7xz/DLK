{"textgrid.poem.65569": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "42. Die Kammerherrn", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An des Himmels Pforte stehn keine Kammerherrn,", "tokens": ["An", "des", "Him\u00b7mels", "Pfor\u00b7te", "stehn", "kei\u00b7ne", "Kam\u00b7mer\u00b7herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Jeder K\u00f6nig \u00f6ffnet sie ohne Diener gern.", "tokens": ["Je\u00b7der", "K\u00f6\u00b7nig", "\u00f6ff\u00b7net", "sie", "oh\u00b7ne", "Die\u00b7ner", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "APPR", "NN", "ADV", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Aber an der H\u00f6lle Thor wird dem Bettelmann", "tokens": ["A\u00b7ber", "an", "der", "H\u00f6l\u00b7le", "Thor", "wird", "dem", "Bet\u00b7tel\u00b7mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VAFIN", "ART", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Gleich ein Kammerherr bestellt, klopft er selbst nicht an.", "tokens": ["Gleich", "ein", "Kam\u00b7mer\u00b7herr", "be\u00b7stellt", ",", "klopft", "er", "selbst", "nicht", "an", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$,", "VVFIN", "PPER", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}}}}}