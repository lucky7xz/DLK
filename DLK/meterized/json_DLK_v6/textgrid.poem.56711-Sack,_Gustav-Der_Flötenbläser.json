{"textgrid.poem.56711": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Der Fl\u00f6tenbl\u00e4ser", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drum sollt ihr tanzen, bis ihr Hilfe! schreit,", "tokens": ["Drum", "sollt", "ihr", "tan\u00b7zen", ",", "bis", "ihr", "Hil\u00b7fe", "!", "schreit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "VVINF", "$,", "APPR", "PPOSAT", "NN", "$.", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "ihr sollt in uners\u00e4ttlicher Begier", "tokens": ["ihr", "sollt", "in", "un\u00b7er\u00b7s\u00e4tt\u00b7li\u00b7cher", "Be\u00b7gier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "euch \u00fcberfressen noch an mir", "tokens": ["euch", "\u00fc\u00b7berf\u00b7res\u00b7sen", "noch", "an", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und meiner Fl\u00f6te wilden Traurigkeit,", "tokens": ["und", "mei\u00b7ner", "Fl\u00f6\u00b7te", "wil\u00b7den", "Trau\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "die euch in aller Dinge R\u00e4tselstreit", "tokens": ["die", "euch", "in", "al\u00b7ler", "Din\u00b7ge", "R\u00e4t\u00b7sel\u00b7streit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "taumelnd mitrei\u00dft und eure Eselsohren schier", "tokens": ["tau\u00b7melnd", "mit\u00b7rei\u00dft", "und", "eu\u00b7re", "E\u00b7sel\u00b7soh\u00b7ren", "schier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "KON", "PPOSAT", "NN", "ADJD"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "verr\u00fcckt und v\u00f6llig trunken macht, bis ihr", "tokens": ["ver\u00b7r\u00fcckt", "und", "v\u00f6l\u00b7lig", "trun\u00b7ken", "macht", ",", "bis", "ihr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "ADJD", "VVFIN", "$,", "KOUS", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "mich und mein Fl\u00f6tenlied ausspeit", "tokens": ["mich", "und", "mein", "Fl\u00f6\u00b7ten\u00b7lied", "aus\u00b7speit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "und dasteht wie der P\u00f6bel, dem der Rausch", "tokens": ["und", "das\u00b7teht", "wie", "der", "P\u00f6\u00b7bel", ",", "dem", "der", "Rausch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "und Wein der Nacht verflog \u2013", "tokens": ["und", "Wein", "der", "Nacht", "ver\u00b7flog", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "ich nehm den Ruhm der Ewigkeit in Tausch,", "tokens": ["ich", "nehm", "den", "Ruhm", "der", "E\u00b7wig\u00b7keit", "in", "Tausch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "wenn ich nur einmal eure Stumpfheit hoch", "tokens": ["wenn", "ich", "nur", "ein\u00b7mal", "eu\u00b7re", "Stumpf\u00b7heit", "hoch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "gejagt und ihr euch zitternd eingesteht:", "tokens": ["ge\u00b7jagt", "und", "ihr", "euch", "zit\u00b7ternd", "ein\u00b7ge\u00b7steht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "PPER", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "er hat uns wie ein Sturmwind fortgeweht!", "tokens": ["er", "hat", "uns", "wie", "ein", "Sturm\u00b7wind", "fort\u00b7ge\u00b7weht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}