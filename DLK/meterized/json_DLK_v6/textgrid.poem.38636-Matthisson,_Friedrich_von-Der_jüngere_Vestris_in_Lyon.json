{"textgrid.poem.38636": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der j\u00fcngere Vestris in Lyon", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Luftig aus Aether gewoben, wenn du den Kulissen entschwebtest,", "tokens": ["Luf\u00b7tig", "aus", "A\u00b7e\u00b7ther", "ge\u00b7wo\u00b7ben", ",", "wenn", "du", "den", "Ku\u00b7lis\u00b7sen", "ent\u00b7schweb\u00b7test", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVPP", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+--+--+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Lieblicher J\u00fcngling erschien mir deine Sylphengestalt.", "tokens": ["Lieb\u00b7li\u00b7cher", "J\u00fcng\u00b7ling", "er\u00b7schien", "mir", "dei\u00b7ne", "Syl\u00b7phen\u00b7ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+--+--+-+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Nicht so der sch\u00f6nen Laide, die heut' in der Loge mit Bleistift", "tokens": ["Nicht", "so", "der", "sch\u00f6\u00b7nen", "Lai\u00b7de", ",", "die", "heut'", "in", "der", "Lo\u00b7ge", "mit", "Blei\u00b7stift"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "APPR", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+--+--+-+++", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Dir ein Entreebillet schrieb, zeigbar dem Kammermerkur.", "tokens": ["Dir", "ein", "E\u00b7ntree\u00b7bil\u00b7let", "schrieb", ",", "zeig\u00b7bar", "dem", "Kam\u00b7mer\u00b7mer\u00b7kur", "."], "tok