{"dta.poem.11723": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hier schwanken siehest du im Bach der Sonne Bild,", "tokens": ["Hier", "schwan\u00b7ken", "sie\u00b7hest", "du", "im", "Bach", "der", "Son\u00b7ne", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch unbeweglich dort steht fest ihr goldner Schild.", "tokens": ["Doch", "un\u00b7be\u00b7weg\u00b7lich", "dort", "steht", "fest", "ihr", "gold\u00b7ner", "Schild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VVFIN", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Am Abend siehst du dann sie scheinbar untergehn,", "tokens": ["Am", "A\u00b7bend", "siehst", "du", "dann", "sie", "schein\u00b7bar", "un\u00b7ter\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Indes der Erdball nur sich abdreht ihrem Stehn.", "tokens": ["In\u00b7des", "der", "Erd\u00b7ball", "nur", "sich", "ab\u00b7dreht", "ih\u00b7rem", "Stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "PRF", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch, steht sie wirklich fest? sie dreht sich auch bestimmt", "tokens": ["Doch", ",", "steht", "sie", "wirk\u00b7lich", "fest", "?", "sie", "dreht", "sich", "auch", "be\u00b7stimmt"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "VVFIN", "PPER", "ADJD", "PTKVZ", "$.", "PPER", "VVFIN", "PRF", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um einen Mittelpunkt, den man nur wahr nicht nimmt.", "tokens": ["Um", "ei\u00b7nen", "Mit\u00b7tel\u00b7punkt", ",", "den", "man", "nur", "wahr", "nicht", "nimmt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,", "PRELS", "PIS", "ADV", "ADJD", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und so, was die Vernunft sich m\u00fchet zu vernehmen,", "tokens": ["Und", "so", ",", "was", "die", "Ver\u00b7nunft", "sich", "m\u00fc\u00b7het", "zu", "ver\u00b7neh\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PRELS", "ART", "NN", "PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat richtig dein Gef\u00fchl erkannt im Schein und Schemen.", "tokens": ["Hat", "rich\u00b7tig", "dein", "Ge\u00b7f\u00fchl", "er\u00b7kannt", "im", "Schein", "und", "Sche\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPOSAT", "NN", "VVPP", "APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}