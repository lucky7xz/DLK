{"textgrid.poem.64923": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdes Schmerzes wandelnd Zelt, die Stirne tief im Staub,", "tokens": ["\u00bb", "des", "Schmer\u00b7zes", "wan\u00b7delnd", "Zelt", ",", "die", "Stir\u00b7ne", "tief", "im", "Staub", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVPP", "NN", "$,", "ART", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hab' ich dich j\u00fcngst geseh'n, der wild'sten Angst zum Raub.\u00ab", "tokens": ["Hab'", "ich", "dich", "j\u00fcngst", "ge\u00b7seh'n", ",", "der", "wild'\u00b7sten", "Angst", "zum", "Raub", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "PRF", "ADV", "VVPP", "$,", "ART", "ADJA", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie schien dein Wesen da zerrissen, grambeth\u00f6rt!", "tokens": ["Wie", "schien", "dein", "We\u00b7sen", "da", "zer\u00b7ris\u00b7sen", ",", "gram\u00b7be\u00b7th\u00f6rt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "ADV", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Wangen, wie so bleich, die Blicke, wie verst\u00f6rt!\u00ab", "tokens": ["Die", "Wan\u00b7gen", ",", "wie", "so", "bleich", ",", "die", "Bli\u00b7cke", ",", "wie", "ver\u00b7st\u00f6rt", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ADV", "ADJD", "$,", "ART", "NN", "$,", "PWAV", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "\u00bbund jetzt bist du so klar, so ruhig, so besonnen!", "tokens": ["\u00bb", "und", "jetzt", "bist", "du", "so", "klar", ",", "so", "ru\u00b7hig", ",", "so", "be\u00b7son\u00b7nen", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "KON", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bist du dem m\u00e4cht'gen Weh, das dir gedroht, entronnen?\u00ab", "tokens": ["Bist", "du", "dem", "m\u00e4cht'\u00b7gen", "Weh", ",", "das", "dir", "ge\u00b7droht", ",", "ent\u00b7ron\u00b7nen", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Entronnen bin ich nicht und h\u00e4tt' es nicht vermocht,", "tokens": ["Ent\u00b7ron\u00b7nen", "bin", "ich", "nicht", "und", "h\u00e4tt'", "es", "nicht", "ver\u00b7mocht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PTKNEG", "KON", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch that ich Besseres: ich hab' es unterjocht.", "tokens": ["Doch", "that", "ich", "Bes\u00b7se\u00b7res", ":", "ich", "hab'", "es", "un\u00b7ter\u00b7jocht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "$.", "PPER", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}