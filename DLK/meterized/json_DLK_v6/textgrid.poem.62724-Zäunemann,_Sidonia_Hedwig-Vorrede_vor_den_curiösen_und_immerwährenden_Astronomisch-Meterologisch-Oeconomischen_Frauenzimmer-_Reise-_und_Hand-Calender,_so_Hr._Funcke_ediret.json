{"textgrid.poem.62724": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Vorrede vor den curi\u00f6sen und immerw\u00e4hrenden Astronomisch-Meterologisch-Oeconomischen Frauenzimmer- Reise- und Hand-Calender, so Hr. Funcke ediret", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geehrter Leser! Sieh! wie dich das Schicksal liebt,", "tokens": ["Ge\u00b7ehr\u00b7ter", "Le\u00b7ser", "!", "Sieh", "!", "wie", "dich", "das", "Schick\u00b7sal", "liebt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "NE", "$.", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da es dir abermahl ein Buch zu lesen giebt,", "tokens": ["Da", "es", "dir", "a\u00b7ber\u00b7mahl", "ein", "Buch", "zu", "le\u00b7sen", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Zeit und Moder trotzt, und der Verwesung lachet,", "tokens": ["Das", "Zeit", "und", "Mo\u00b7der", "trotzt", ",", "und", "der", "Ver\u00b7we\u00b7sung", "la\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$,", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dieweil es durch sich selbst sich ewig lebend machet.", "tokens": ["Die\u00b7weil", "es", "durch", "sich", "selbst", "sich", "e\u00b7wig", "le\u00b7bend", "ma\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PRF", "ADV", "PRF", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Man liest, da\u00df vor der Zeit im grauen Alterthum", "tokens": ["Man", "liest", ",", "da\u00df", "vor", "der", "Zeit", "im", "grau\u00b7en", "Al\u00b7ter\u00b7thum"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "APPR", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Pyramiden Pracht nicht wenig Ehr und Ruhm", "tokens": ["Der", "Py\u00b7ra\u00b7mi\u00b7den", "Pracht", "nicht", "we\u00b7nig", "Ehr", "und", "Ruhm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "PTKNEG", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und Ansehn nach sich zog. Man wei\u00df, was die Colossen", "tokens": ["Und", "An\u00b7sehn", "nach", "sich", "zog", ".", "Man", "wei\u00df", ",", "was", "die", "Co\u00b7los\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "PRF", "VVFIN", "$.", "PIS", "VVFIN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und Bilder alten Welt vor Lob und Prei\u00df genossen.", "tokens": ["Und", "Bil\u00b7der", "al\u00b7ten", "Welt", "vor", "Lob", "und", "Prei\u00df", "ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie habens auch verdient. Allein, wo ist ihr Pracht?", "tokens": ["Sie", "ha\u00b7bens", "auch", "ver\u00b7dient", ".", "Al\u00b7lein", ",", "wo", "ist", "ihr", "Pracht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$.", "ADV", "$,", "PWAV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wo sind sie jetzo hin? Sie sind zu Staub gemacht.", "tokens": ["Wo", "sind", "sie", "jet\u00b7zo", "hin", "?", "Sie", "sind", "zu", "Staub", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "PTKVZ", "$.", "PPER", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Kaum ist ein Uberrest von ihnen noch zu sehen.", "tokens": ["Kaum", "ist", "ein", "Ub\u00b7er\u00b7rest", "von", "ih\u00b7nen", "noch", "zu", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So mu\u00df das herrlichste zerfallen und vergehen!", "tokens": ["So", "mu\u00df", "das", "herr\u00b7lichs\u00b7te", "zer\u00b7fal\u00b7len", "und", "ver\u00b7ge\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Allein, wer durch die Hand und eine nette Schrift,", "tokens": ["Al\u00b7lein", ",", "wer", "durch", "die", "Hand", "und", "ei\u00b7ne", "net\u00b7te", "Schrift", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "APPR", "ART", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und ein geschicktes Buch sich ein Ged\u00e4chtni\u00df stifft,", "tokens": ["Und", "ein", "ge\u00b7schick\u00b7tes", "Buch", "sich", "ein", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "stifft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Das steht und dauret fest, das kan kein Rost verzehren,", "tokens": ["Das", "steht", "und", "dau\u00b7ret", "fest", ",", "das", "kan", "kein", "Rost", "ver\u00b7zeh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "PTKVZ", "$,", "PDS", "VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Man sucht es ewiglich aufs h\u00f6chste zu verehren.", "tokens": ["Man", "sucht", "es", "e\u00b7wig\u00b7lich", "aufs", "h\u00f6chs\u00b7te", "zu", "ver\u00b7eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wie hat die alte Welt die B\u00fccher hochgesch\u00e4tzt,", "tokens": ["Wie", "hat", "die", "al\u00b7te", "Welt", "die", "B\u00fc\u00b7cher", "hoch\u00b7ge\u00b7sch\u00e4tzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die ein ge\u00fcbter Kiel und kluger Geist gesetzt?", "tokens": ["Die", "ein", "ge\u00b7\u00fcb\u00b7ter", "Kiel", "und", "klu\u00b7ger", "Geist", "ge\u00b7setzt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wie eifrig strebte man ein T\u00e4felgen von Rinden,", "tokens": ["Wie", "eif\u00b7rig", "streb\u00b7te", "man", "ein", "T\u00e4\u00b7fel\u00b7gen", "von", "Rin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PIS", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ein Blat von Pergament und eine Schrift zu finden.", "tokens": ["Ein", "Blat", "von", "Per\u00b7ga\u00b7ment", "und", "ei\u00b7ne", "Schrift", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wie kostbar und wie schwer, wie theuer und wie rar", "tokens": ["Wie", "kost\u00b7bar", "und", "wie", "schwer", ",", "wie", "theu\u00b7er", "und", "wie", "rar"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "KON", "PWAV", "ADJD", "$,", "PWAV", "ADJD", "KON", "PWAV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Nahm man zu dieser Zeit den Kauff der B\u00fccher wahr?", "tokens": ["Nahm", "man", "zu", "die\u00b7ser", "Zeit", "den", "Kauff", "der", "B\u00fc\u00b7cher", "wahr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "PDAT", "NN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Allein nachdem nunmehr die Barbarey verschwunden,", "tokens": ["Al\u00b7lein", "nach\u00b7dem", "nun\u00b7mehr", "die", "Bar\u00b7ba\u00b7rey", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und man die edle Kunst der Druckerey erfunden,", "tokens": ["Und", "man", "die", "ed\u00b7le", "Kunst", "der", "Dru\u00b7cke\u00b7rey", "er\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "So wird auch mancher Kopf, in dem was kluges aufgeweckt.", "tokens": ["So", "wird", "auch", "man\u00b7cher", "Kopf", ",", "in", "dem", "was", "klu\u00b7ges", "auf\u00b7ge\u00b7weckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIAT", "NN", "$,", "APPR", "ART", "PWS", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.26": {"text": "Zur Wissenschaft gereitzt, und kr\u00e4ftig aufgeweckt.", "tokens": ["Zur", "Wis\u00b7sen\u00b7schaft", "ge\u00b7reitzt", ",", "und", "kr\u00e4f\u00b7tig", "auf\u00b7ge\u00b7weckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wer sonst ein Buch gehabt, wer zweene kunte weisen,", "tokens": ["Wer", "sonst", "ein", "Buch", "ge\u00b7habt", ",", "wer", "zwee\u00b7ne", "kun\u00b7te", "wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VAPP", "$,", "PWS", "VVFIN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Der muste sich begl\u00fcckt und reich an Schriften preisen.", "tokens": ["Der", "mus\u00b7te", "sich", "be\u00b7gl\u00fcckt", "und", "reich", "an", "Schrif\u00b7ten", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PRF", "VVPP", "KON", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Nun aber seyd getrost! macht euch die Druckerey,", "tokens": ["Nun", "a\u00b7ber", "seyd", "ge\u00b7trost", "!", "macht", "euch", "die", "Dru\u00b7cke\u00b7rey", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "VVPP", "$.", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Von diesem Seelen-Zwang und B\u00fccher-Armuth frey,", "tokens": ["Von", "die\u00b7sem", "See\u00b7len\u00b7Zwang", "und", "B\u00fc\u00b7cher\u00b7Ar\u00b7muth", "frey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Jetzt k\u00f6nt ihr vor ein Buch, bedenkt die Zeit der Alten!", "tokens": ["Jetzt", "k\u00f6nt", "ihr", "vor", "ein", "Buch", ",", "be\u00b7denkt", "die", "Zeit", "der", "Al\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Den allerreichsten Schatz zu eurer Lust erhalten.", "tokens": ["Den", "al\u00b7ler\u00b7reichs\u00b7ten", "Schatz", "zu", "eu\u00b7rer", "Lust", "er\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Der B\u00fccher sch\u00f6ne Zahl macht zum Studiren Lust,", "tokens": ["Der", "B\u00fc\u00b7cher", "sch\u00f6\u00b7ne", "Zahl", "macht", "zum", "Stu\u00b7di\u00b7ren", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "APPRART", "NN", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Und regt die Geister an, und schafft, da\u00df Sinn und Brust", "tokens": ["Und", "regt", "die", "Geis\u00b7ter", "an", ",", "und", "schafft", ",", "da\u00df", "Sinn", "und", "Brust"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "$,", "KOUS", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Nach Wissenschaften strebt. Die Seele wird vergn\u00fcget", "tokens": ["Nach", "Wis\u00b7sen\u00b7schaf\u00b7ten", "strebt", ".", "Die", "See\u00b7le", "wird", "ver\u00b7gn\u00fc\u00b7get"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "$.", "ART", "NN", "VAFIN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Wenn sich der muntre Fu\u00df zum B\u00fccher Schrank verf\u00fcget.", "tokens": ["Wenn", "sich", "der", "mun\u00b7tre", "Fu\u00df", "zum", "B\u00fc\u00b7cher", "Schrank", "ver\u00b7f\u00fc\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "ADJA", "NN", "APPRART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Hier ist der Seelen Ruh, allhier ist ihre Kost,", "tokens": ["Hier", "ist", "der", "See\u00b7len", "Ruh", ",", "all\u00b7hier", "ist", "ih\u00b7re", "Kost", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Es kan kein reicher Tisch mit Nectar, Wein und Most,", "tokens": ["Es", "kan", "kein", "rei\u00b7cher", "Tisch", "mit", "Nec\u00b7tar", ",", "Wein", "und", "Most", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "ADJD", "NN", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Kein Scherz, Gem\u00e4chlichkeit und andre Wollusts-Gaben,", "tokens": ["Kein", "Scherz", ",", "Ge\u00b7m\u00e4ch\u00b7lich\u00b7keit", "und", "and\u00b7re", "Wol\u00b7lust\u00b7s\u00b7Ga\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.40": {"text": "Und was man reitzend nennt, so s\u00fc\u00df als dieses laben.", "tokens": ["Und", "was", "man", "reit\u00b7zend", "nennt", ",", "so", "s\u00fc\u00df", "als", "die\u00b7ses", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PIS", "ADJD", "VVFIN", "$,", "ADV", "ADJD", "KOKOM", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Wie k\u00f6stlich ist der Schwei\u00df, der von dir den Wangen flie\u00dft,", "tokens": ["Wie", "k\u00f6st\u00b7lich", "ist", "der", "Schwei\u00df", ",", "der", "von", "dir", "den", "Wan\u00b7gen", "flie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.42": {"text": "Wenn man bey Tag und Nacht mit Ernst in B\u00fcchern liest,", "tokens": ["Wenn", "man", "bey", "Tag", "und", "Nacht", "mit", "Ernst", "in", "B\u00fc\u00b7chern", "liest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "KON", "NN", "APPR", "NE", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Und mit den Todten schwatzt. O angenehme Freude!", "tokens": ["Und", "mit", "den", "Tod\u00b7ten", "schwatzt", ".", "O", "an\u00b7ge\u00b7neh\u00b7me", "Freu\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$.", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Man findet hier nach Wunsch die sch\u00f6nste Seelen-Weide.", "tokens": ["Man", "fin\u00b7det", "hier", "nach", "Wunsch", "die", "sch\u00f6ns\u00b7te", "See\u00b7len\u00b7Wei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Es zeigt sich, wie gedacht, auch hier ein neues Buch,", "tokens": ["Es", "zeigt", "sich", ",", "wie", "ge\u00b7dacht", ",", "auch", "hier", "ein", "neu\u00b7es", "Buch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "PWAV", "VVPP", "$,", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Das giebt zur Wissenschaft Gelegenheit genug.", "tokens": ["Das", "giebt", "zur", "Wis\u00b7sen\u00b7schaft", "Ge\u00b7le\u00b7gen\u00b7heit", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Ihr Thoren! sprecht nur nicht: Ein neuer Zeit-Verschwenden!", "tokens": ["Ihr", "Tho\u00b7ren", "!", "sprecht", "nur", "nicht", ":", "Ein", "neu\u00b7er", "Zeit\u00b7Ver\u00b7schwen\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "ADV", "PTKNEG", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Was ists denn vor ein St\u00fcck? O seht doch! ein Calender.", "tokens": ["Was", "ists", "denn", "vor", "ein", "St\u00fcck", "?", "O", "seht", "doch", "!", "ein", "Ca\u00b7len\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "APPR", "ART", "NN", "$.", "NE", "VVFIN", "ADV", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.49": {"text": "So sagt der Aberwitz; so schliesset der Verstand,", "tokens": ["So", "sagt", "der", "A\u00b7ber\u00b7witz", ";", "so", "schlies\u00b7set", "der", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Der nicht gel\u00e4utert ist. Kommt! nehmet ihn zur Hand,", "tokens": ["Der", "nicht", "ge\u00b7l\u00e4u\u00b7tert", "ist", ".", "Kommt", "!", "neh\u00b7met", "ihn", "zur", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "VVPP", "VAFIN", "$.", "VVFIN", "$.", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Und seht ihn erstlich an; lest erst mit Flei\u00df darinnen,", "tokens": ["Und", "seht", "ihn", "erst\u00b7lich", "an", ";", "lest", "erst", "mit", "Flei\u00df", "da\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$.", "VVFIN", "ADV", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Betrachtet seinen Kern, und lasset eure Sinnen", "tokens": ["Be\u00b7trach\u00b7tet", "sei\u00b7nen", "Kern", ",", "und", "las\u00b7set", "eu\u00b7re", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Mit gutem Vorbedacht die rechten Richter seyn.", "tokens": ["Mit", "gu\u00b7tem", "Vor\u00b7be\u00b7dacht", "die", "rech\u00b7ten", "Rich\u00b7ter", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Ich wei\u00df, dies neue Buch nimmt eure Herzen ein.", "tokens": ["Ich", "wei\u00df", ",", "dies", "neu\u00b7e", "Buch", "nimmt", "eu\u00b7re", "Her\u00b7zen", "ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PDS", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Ihr, die ihr die Natur und Himmels-Lauff ergr\u00fcndet,", "tokens": ["Ihr", ",", "die", "ihr", "die", "Na\u00b7tur", "und", "Him\u00b7mels\u00b7Lauff", "er\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ART", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Auch nach der Wirthschaft-Kunst ein gro\u00df Verlangen findet,", "tokens": ["Auch", "nach", "der", "Wirth\u00b7schaft\u00b7Kunst", "ein", "gro\u00df", "Ver\u00b7lan\u00b7gen", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Ihr, die die Rechenkunst und Kauffmannschaft erfreut,", "tokens": ["Ihr", ",", "die", "die", "Re\u00b7chen\u00b7kunst", "und", "Kauf\u00b7fmann\u00b7schaft", "er\u00b7freut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Betrachtet dieses ", "tokens": ["Be\u00b7trach\u00b7tet", "die\u00b7ses"], "token_info": ["word", "word"], "pos": ["VVFIN", "PDAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.59": {"text": "In eure H\u00e4nden spielt, so werdet ihr bekennen,", "tokens": ["In", "eu\u00b7re", "H\u00e4n\u00b7den", "spielt", ",", "so", "wer\u00b7det", "ihr", "be\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Es sey ein n\u00fctzliches und sch\u00f6nes Buch zu nennen.", "tokens": ["Es", "sey", "ein", "n\u00fctz\u00b7li\u00b7ches", "und", "sch\u00f6\u00b7nes", "Buch", "zu", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "KON", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Ihr Frauenzimmer lest, was dieses in sich h\u00e4lt,", "tokens": ["Ihr", "Frau\u00b7en\u00b7zim\u00b7mer", "lest", ",", "was", "die\u00b7ses", "in", "sich", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PWS", "PDAT", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "So wei\u00df ich, da\u00df es euch beliebt und wohl gef\u00e4llt.", "tokens": ["So", "wei\u00df", "ich", ",", "da\u00df", "es", "euch", "be\u00b7liebt", "und", "wohl", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "ADJD", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Und weil kein Druck so leicht wird ohne Fehl geschehen;", "tokens": ["Und", "weil", "kein", "Druck", "so", "leicht", "wird", "oh\u00b7ne", "Fehl", "ge\u00b7sche\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIAT", "NN", "ADV", "ADJD", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "So bitt ich, selbige geneigt zu \u00fcbersehen.", "tokens": ["So", "bitt", "ich", ",", "sel\u00b7bi\u00b7ge", "ge\u00b7neigt", "zu", "\u00fc\u00b7ber\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJA", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}