{"textgrid.poem.55462": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "43", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das holde Tal hat schon die Sonne wieder", "tokens": ["Das", "hol\u00b7de", "Tal", "hat", "schon", "die", "Son\u00b7ne", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Fr\u00fchlingsbl\u00fct' und \u2013 blumen angef\u00fcllt,", "tokens": ["Mit", "Fr\u00fch\u00b7lings\u00b7bl\u00fct'", "und", "\u2013", "blu\u00b7men", "an\u00b7ge\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "$(", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Nachtigall singt immer neue Lieder", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "singt", "im\u00b7mer", "neu\u00b7e", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem Hochgef\u00fchl, das ihr entgegenquillt.", "tokens": ["Dem", "Hoch\u00b7ge\u00b7f\u00fchl", ",", "das", "ihr", "ent\u00b7ge\u00b7gen\u00b7quillt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Erfreue dich der gottverliehnen Gaben!", "tokens": ["Er\u00b7freu\u00b7e", "dich", "der", "gott\u00b7ver\u00b7lieh\u00b7nen", "Ga\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Froh, wie er dich erschuf, will er dich haben.", "tokens": ["Froh", ",", "wie", "er", "dich", "er\u00b7schuf", ",", "will", "er", "dich", "ha\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$,", "VMFIN", "PPER", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}