{"textgrid.poem.39748": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "An den Gesang", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir kommen, uns in dir zu baden,", "tokens": ["Wir", "kom\u00b7men", ",", "uns", "in", "dir", "zu", "ba\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gesang, vor dein krystallnes Haus;", "tokens": ["Ge\u00b7sang", ",", "vor", "dein", "krys\u00b7tall\u00b7nes", "Haus", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Rauschen hat uns eingeladen,", "tokens": ["Dein", "Rau\u00b7schen", "hat", "uns", "ein\u00b7ge\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Geu\u00df nur die klaren Wellen aus;", "tokens": ["Geu\u00df", "nur", "die", "kla\u00b7ren", "Wel\u00b7len", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn deine reiche F\u00fclle beut,", "tokens": ["Denn", "dei\u00b7ne", "rei\u00b7che", "F\u00fcl\u00b7le", "beut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was starke M\u00e4nnerseelen freut.", "tokens": ["Was", "star\u00b7ke", "M\u00e4n\u00b7ner\u00b7see\u00b7len", "freut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Und str\u00f6mt in dir durch jedes Herz;", "tokens": ["Und", "str\u00f6mt", "in", "dir", "durch", "je\u00b7des", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du lehrest ihre Seufzer schwellen,", "tokens": ["Du", "leh\u00b7rest", "ih\u00b7re", "Seuf\u00b7zer", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und l\u00f6sest heilend ihren Schmerz.", "tokens": ["Und", "l\u00f6\u00b7sest", "hei\u00b7lend", "ih\u00b7ren", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Aus deinem Spiegel wallt ihr Gl\u00fcck", "tokens": ["Aus", "dei\u00b7nem", "Spie\u00b7gel", "wallt", "ihr", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In tausendfachem Stral zur\u00fcck.", "tokens": ["In", "tau\u00b7send\u00b7fa\u00b7chem", "Stral", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der feste ", "tokens": ["Der", "fes\u00b7te"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "In deinem Quelle st\u00e4rkt er sich;", "tokens": ["In", "dei\u00b7nem", "Quel\u00b7le", "st\u00e4rkt", "er", "sich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da wachsen Fl\u00fcgel dem Gedanken,", "tokens": ["Da", "wach\u00b7sen", "Fl\u00fc\u00b7gel", "dem", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Auge tagt es wonniglich;", "tokens": ["Dem", "Au\u00b7ge", "tagt", "es", "won\u00b7nig\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es schaut in deiner blauen Flut", "tokens": ["Es", "schaut", "in", "dei\u00b7ner", "blau\u00b7en", "Flut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Himmel und das ew'ge Gut.", "tokens": ["Den", "Him\u00b7mel", "und", "das", "ew'\u00b7ge", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Hat deiner Arche sich vertraut;", "tokens": ["Hat", "dei\u00b7ner", "Ar\u00b7che", "sich", "ver\u00b7traut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PRF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wird ihr das k\u00fchne Wort genommen,", "tokens": ["Wird", "ihr", "das", "k\u00fch\u00b7ne", "Wort", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So tauchet sie sich in den Laut;", "tokens": ["So", "tau\u00b7chet", "sie", "sich", "in", "den", "Laut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie schifft aus Griechenland und Rom,", "tokens": ["Sie", "schifft", "aus", "Grie\u00b7chen\u00b7land", "und", "Rom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein sel'ger Schwan, auf deinem Strom.", "tokens": ["Ein", "sel'\u00b7ger", "Schwan", ",", "auf", "dei\u00b7nem", "Strom", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wenn deine Wogen uns umschlingen,", "tokens": ["Wenn", "dei\u00b7ne", "Wo\u00b7gen", "uns", "um\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So wissen wir, was ", "tokens": ["So", "wis\u00b7sen", "wir", ",", "was"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWS"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "So stark und einig, wie wir singen,", "tokens": ["So", "stark", "und", "ei\u00b7nig", ",", "wie", "wir", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So stark und einig ist ihr Geist.", "tokens": ["So", "stark", "und", "ei\u00b7nig", "ist", "ihr", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Viel Kehlen und ein einz'ger Sang,", "tokens": ["Viel", "Keh\u00b7len", "und", "ein", "einz'\u00b7ger", "Sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Viel Seelen in verbundnem Drang.", "tokens": ["Viel", "See\u00b7len", "in", "ver\u00b7bund\u00b7nem", "Drang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Auch dieses gl\u00fchnde Blut der Reben", "tokens": ["Auch", "die\u00b7ses", "gl\u00fchn\u00b7de", "Blut", "der", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PDAT", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wird erst in deiner Mischung mild;", "tokens": ["Wird", "erst", "in", "dei\u00b7ner", "Misc\u00b7hung", "mild", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du machst, da\u00df mit ihm reinres Leben", "tokens": ["Du", "machst", ",", "da\u00df", "mit", "ihm", "rein\u00b7res", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "APPR", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In allen unsern Adern quillt;", "tokens": ["In", "al\u00b7len", "un\u00b7sern", "A\u00b7dern", "quillt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du stimmest unsern Gl\u00e4serklang:", "tokens": ["Du", "stim\u00b7mest", "un\u00b7sern", "Gl\u00e4\u00b7ser\u00b7klang", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ja, deinen Segen zu verbreiten", "tokens": ["Ja", ",", "dei\u00b7nen", "Se\u00b7gen", "zu", "ver\u00b7brei\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hast du uns Br\u00fcder ausgesandt;", "tokens": ["Hast", "du", "uns", "Br\u00fc\u00b7der", "aus\u00b7ge\u00b7sandt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir wollen deine Str\u00f6me leiten", "tokens": ["Wir", "wol\u00b7len", "dei\u00b7ne", "Str\u00f6\u00b7me", "lei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hinaus ins liebe Vaterland;", "tokens": ["Hin\u00b7aus", "ins", "lie\u00b7be", "Va\u00b7ter\u00b7land", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wo sie flie\u00dfen, wo sie gl\u00fchn,", "tokens": ["Und", "wo", "sie", "flie\u00b7\u00dfen", ",", "wo", "sie", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVINF", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Soll ", "tokens": ["Soll"], "token_info": ["word"], "pos": ["VMFIN"], "meter": "+", "measure": "single.up"}}}}}