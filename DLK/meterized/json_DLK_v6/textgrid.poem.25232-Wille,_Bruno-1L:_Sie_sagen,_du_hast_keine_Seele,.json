{"textgrid.poem.25232": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sie sagen, du hast keine Seele,", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie sagen, du hast keine Seele,", "tokens": ["Sie", "sa\u00b7gen", ",", "du", "hast", "kei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Arm bleiche Birkenmaid.", "tokens": ["Arm", "blei\u00b7che", "Bir\u00b7ken\u00b7maid", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Du kauerst starr und stumm", "tokens": ["Du", "kau\u00b7erst", "starr", "und", "stumm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Auf d\u00fcster struppiger Heid.", "tokens": ["Auf", "d\u00fcs\u00b7ter", "strup\u00b7pi\u00b7ger", "Heid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Du kauerst in der \u00d6de,", "tokens": ["Du", "kau\u00b7erst", "in", "der", "\u00d6\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein ausgesto\u00dfen Kind.", "tokens": ["Ein", "aus\u00b7ge\u00b7sto\u00b7\u00dfen", "Kind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein Haargezweige zaust", "tokens": ["Dein", "Haar\u00b7ge\u00b7zwei\u00b7ge", "zaust"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der rauhe M\u00e4rzenwind.", "tokens": ["Der", "rau\u00b7he", "M\u00e4r\u00b7zen\u00b7wind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sein m\u00fcrrisch Brausen wogt", "tokens": ["Sein", "m\u00fcr\u00b7risch", "Brau\u00b7sen", "wogt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Durch Heidekraut und Ginster.", "tokens": ["Durch", "Hei\u00b7de\u00b7kraut", "und", "Gins\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ins weite Nebelgrau", "tokens": ["Ins", "wei\u00b7te", "Ne\u00b7bel\u00b7grau"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Pilgern Wolken finster.", "tokens": ["Pil\u00b7gern", "Wol\u00b7ken", "fins\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Eine Kr\u00e4he treibt im Sturm", "tokens": ["Ei\u00b7ne", "Kr\u00e4\u00b7he", "treibt", "im", "Sturm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Taumelig vorbei;", "tokens": ["Tau\u00b7me\u00b7lig", "vor\u00b7bei", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Heiser und erstickt", "tokens": ["Hei\u00b7ser", "und", "er\u00b7stickt"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Ihr grimmer Klageschrei ...", "tokens": ["Ihr", "grim\u00b7mer", "Kla\u00b7ge\u00b7schrei", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Kein Bettelkind, o Birke,", "tokens": ["Kein", "Bet\u00b7tel\u00b7kind", ",", "o", "Bir\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist also arm und blo\u00df;", "tokens": ["Ist", "al\u00b7so", "arm", "und", "blo\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "KON", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es hat eine Seele, zu weinen/", "tokens": ["Es", "hat", "ei\u00b7ne", "See\u00b7le", ",", "zu", "wei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PTKZU", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Dich hei\u00dfen sie seelenlos.", "tokens": ["Dich", "hei\u00b7\u00dfen", "sie", "see\u00b7len\u00b7los", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Und doch, in tiefer \u00d6de", "tokens": ["Und", "doch", ",", "in", "tie\u00b7fer", "\u00d6\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sp\u00fcrst du die hohe Trauer", "tokens": ["Sp\u00fcrst", "du", "die", "ho\u00b7he", "Trau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als Seelenfr\u00f6steln s\u00fc\u00df,", "tokens": ["Als", "See\u00b7len\u00b7fr\u00f6s\u00b7teln", "s\u00fc\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Woll\u00fcstig k\u00fchlen Schauer.", "tokens": ["Wol\u00b7l\u00fcs\u00b7tig", "k\u00fch\u00b7len", "Schau\u00b7er", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.7": {"line.1": {"text": "Du kauerst starr und stumm", "tokens": ["Du", "kau\u00b7erst", "starr", "und", "stumm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Auf d\u00fcster struppiger Heid.", "tokens": ["Auf", "d\u00fcs\u00b7ter", "strup\u00b7pi\u00b7ger", "Heid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sie sagen, du hast keine Seele,", "tokens": ["Sie", "sa\u00b7gen", ",", "du", "hast", "kei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Arm bleiche Birkenmaid.", "tokens": ["Arm", "blei\u00b7che", "Bir\u00b7ken\u00b7maid", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}