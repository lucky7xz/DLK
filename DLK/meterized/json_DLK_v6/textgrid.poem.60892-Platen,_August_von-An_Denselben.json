{"textgrid.poem.60892": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "An Denselben", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Birgst du den Namen? Es ist doch immer ein klassischer Name;", "tokens": ["Birgst", "du", "den", "Na\u00b7men", "?", "Es", "ist", "doch", "im\u00b7mer", "ein", "klas\u00b7si\u00b7scher", "Na\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$.", "PPER", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Dich schon redet Horaz \u00bbstinkender M\u00e4vius\u00ab an.", "tokens": ["Dich", "schon", "re\u00b7det", "Ho\u00b7raz", "\u00bb", "stin\u00b7ken\u00b7der", "M\u00e4\u00b7vius", "\u00ab", "an", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "NE", "$(", "ADJA", "NN", "$(", "PTKVZ", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}}}}}