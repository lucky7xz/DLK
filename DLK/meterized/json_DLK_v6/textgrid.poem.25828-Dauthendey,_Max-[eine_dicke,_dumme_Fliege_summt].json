{"textgrid.poem.25828": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[eine dicke, dumme Fliege summt]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine dicke, dumme Fliege summt,", "tokens": ["Ei\u00b7ne", "di\u00b7cke", ",", "dum\u00b7me", "Flie\u00b7ge", "summt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "St\u00f6\u00dft und brummt an die Scheiben.", "tokens": ["St\u00f6\u00dft", "und", "brummt", "an", "die", "Schei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Auf den leeren St\u00fchlen sitzt die Sommersonne.", "tokens": ["Auf", "den", "lee\u00b7ren", "St\u00fch\u00b7len", "sitzt", "die", "Som\u00b7mer\u00b7son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Drau\u00dfen treiben Wolken im blauen K\u00fchlen.", "tokens": ["Drau\u00b7\u00dfen", "trei\u00b7ben", "Wol\u00b7ken", "im", "blau\u00b7en", "K\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}}, "stanza.2": {"line.1": {"text": "Die gro\u00dfen, wei\u00dfen Wolken am Himmel", "tokens": ["Die", "gro\u00b7\u00dfen", ",", "wei\u00b7\u00dfen", "Wol\u00b7ken", "am", "Him\u00b7mel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sind mein Sonntagsschimmel,", "tokens": ["Sind", "mein", "Sonn\u00b7tags\u00b7schim\u00b7mel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ich reite lautlos ins blaue Weite.", "tokens": ["Ich", "rei\u00b7te", "laut\u00b7los", "ins", "blau\u00b7e", "Wei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}}}}