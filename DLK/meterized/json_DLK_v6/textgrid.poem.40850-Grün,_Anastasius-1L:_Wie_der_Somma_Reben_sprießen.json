{"textgrid.poem.40850": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie der Somma Reben sprie\u00dfen", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie der Somma Reben sprie\u00dfen", "tokens": ["Wie", "der", "Som\u00b7ma", "Re\u00b7ben", "sprie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf vesuv'schem Schuttger\u00f6lle,", "tokens": ["Auf", "ve\u00b7su\u00b7v'\u00b7schem", "Schutt\u00b7ge\u00b7r\u00f6l\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als ob eine Saat von Gr\u00fc\u00dfen", "tokens": ["Als", "ob", "ei\u00b7ne", "Saat", "von", "Gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus versunknen Tempeln qu\u00f6lle;", "tokens": ["Aus", "ver\u00b7sun\u00b7knen", "Tem\u00b7peln", "qu\u00f6l\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "H\u00e4tt' es Einer ahnen m\u00f6gen,", "tokens": ["H\u00e4tt'", "es", "Ei\u00b7ner", "ah\u00b7nen", "m\u00f6\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df der Heideng\u00f6tter Grabe", "tokens": ["Da\u00df", "der", "Hei\u00b7den\u00b7g\u00f6t\u00b7ter", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Einst entsteigt solch sch\u00f6ner Segen,", "tokens": ["Einst", "ent\u00b7steigt", "solch", "sch\u00f6\u00b7ner", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dran manch guter Christ sich labe?", "tokens": ["Dran", "manch", "gu\u00b7ter", "Christ", "sich", "la\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie zu Worms der Reben Kette", "tokens": ["Wie", "zu", "Worms", "der", "Re\u00b7ben", "Ket\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um den Dom der Liebenfraue", "tokens": ["Um", "den", "Dom", "der", "Lie\u00b7ben\u00b7frau\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Reich sich rankte an der St\u00e4tte", "tokens": ["Reich", "sich", "rank\u00b7te", "an", "der", "St\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der verbrannten Klosterbaue;", "tokens": ["Der", "ver\u00b7brann\u00b7ten", "Klos\u00b7ter\u00b7bau\u00b7e", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "W\u00e4re Ahnung wem geworden,", "tokens": ["W\u00e4\u00b7re", "Ah\u00b7nung", "wem", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PWS", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df einst gaukelnd um die Gr\u00fcfte", "tokens": ["Da\u00df", "einst", "gau\u00b7kelnd", "um", "die", "Gr\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "B\u00e4rt'ger Kapuzinerhorden", "tokens": ["B\u00e4rt'\u00b7ger", "Ka\u00b7pu\u00b7zi\u00b7ner\u00b7hor\u00b7den"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Solch ein lieblich Tr\u00e4umen d\u00fcfte?", "tokens": ["Solch", "ein", "lieb\u00b7lich", "Tr\u00e4u\u00b7men", "d\u00fcf\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "M\u00f6gt ihr Reben aus dem Schutte", "tokens": ["M\u00f6gt", "ihr", "Re\u00b7ben", "aus", "dem", "Schut\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fort und fort so herrlich wallen,", "tokens": ["Fort", "und", "fort", "so", "herr\u00b7lich", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PTKVZ", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis zu duft'gem, saft'gem Schutte", "tokens": ["Bis", "zu", "duft'\u00b7gem", ",", "saft'\u00b7gem", "Schut\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Selber ihr im Herbst zerfallen!", "tokens": ["Sel\u00b7ber", "ihr", "im", "Herbst", "zer\u00b7fal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "S\u00fcdens Reben, Nordens Reben,", "tokens": ["S\u00fc\u00b7dens", "Re\u00b7ben", ",", "Nor\u00b7dens", "Re\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00dft empor die Ranken schie\u00dfen,", "tokens": ["La\u00dft", "em\u00b7por", "die", "Ran\u00b7ken", "schie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df sie riesenhoch sich heben,", "tokens": ["Da\u00df", "sie", "rie\u00b7sen\u00b7hoch", "sich", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Beider Wipfel sich umschlie\u00dfen!", "tokens": ["Bei\u00b7der", "Wip\u00b7fel", "sich", "um\u00b7schlie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "W\u00f6lbt euch dicht und sch\u00f6n zur Laube", "tokens": ["W\u00f6lbt", "euch", "dicht", "und", "sch\u00f6n", "zur", "Lau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcr die Freunde und den Dichter!", "tokens": ["F\u00fcr", "die", "Freun\u00b7de", "und", "den", "Dich\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "S\u00fcdens Traub' an Nordens Traube", "tokens": ["S\u00fc\u00b7dens", "Traub'", "an", "Nor\u00b7dens", "Trau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "APPR", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dazwischen Sonnenlichter!", "tokens": ["Und", "da\u00b7zwi\u00b7schen", "Son\u00b7nen\u00b7lich\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PAV", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Freunde, la\u00dft uns lagern drunter", "tokens": ["Freun\u00b7de", ",", "la\u00dft", "uns", "la\u00b7gern", "drun\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVIMP", "PPER", "VVFIN", "PAV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem gr\u00fcnen Dom der Zecher!", "tokens": ["In", "dem", "gr\u00fc\u00b7nen", "Dom", "der", "Ze\u00b7cher", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Keltert von den Trauben munter", "tokens": ["Kel\u00b7tert", "von", "den", "Trau\u00b7ben", "mun\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In die tiefen goldnen Becher!", "tokens": ["In", "die", "tie\u00b7fen", "gold\u00b7nen", "Be\u00b7cher", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Und es werden selbst die Frommen,", "tokens": ["Und", "es", "wer\u00b7den", "selbst", "die", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Traun, uns nicht zu schelten taugen,", "tokens": ["Traun", ",", "uns", "nicht", "zu", "schel\u00b7ten", "tau\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da, durch Christi Thr\u00e4n' entglommen,", "tokens": ["Da", ",", "durch", "Chris\u00b7ti", "Thr\u00e4n'", "ent\u00b7glom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "NE", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Milch der Liebenfrau wir saugen!", "tokens": ["Milch", "der", "Lie\u00b7ben\u00b7frau", "wir", "sau\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Oeffn' ein bischen, Laubgewinde,", "tokens": ["Oeffn'", "ein", "bi\u00b7schen", ",", "Laub\u00b7ge\u00b7win\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "ADV", "$,", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Uns zur Aussicht deine Halle,", "tokens": ["Uns", "zur", "Aus\u00b7sicht", "dei\u00b7ne", "Hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df sich durch die sonn'gen Gr\u00fcnde", "tokens": ["Da\u00df", "sich", "durch", "die", "sonn'\u00b7gen", "Gr\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unser Aug', ergehend, walle;", "tokens": ["Un\u00b7ser", "Aug'", ",", "er\u00b7ge\u00b7hend", ",", "wal\u00b7le", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVPP", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Da\u00df wir durch den Kranz von Reben", "tokens": ["Da\u00df", "wir", "durch", "den", "Kranz", "von", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Goldne Saaten wogend schauen,", "tokens": ["Gold\u00b7ne", "Saa\u00b7ten", "wo\u00b7gend", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dorf und Kirchthurm blank sich heben,", "tokens": ["Dorf", "und", "Kirch\u00b7thurm", "blank", "sich", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Strom und ferne Meere blauen.", "tokens": ["Strom", "und", "fer\u00b7ne", "Mee\u00b7re", "blau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Und die Burg mit morschen Warten,", "tokens": ["Und", "die", "Burg", "mit", "mor\u00b7schen", "War\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die als Puppe h\u00e4ngt am H\u00fcgel,", "tokens": ["Die", "als", "Pup\u00b7pe", "h\u00e4ngt", "am", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch vielleicht als Rebengarten", "tokens": ["Doch", "viel\u00b7leicht", "als", "Re\u00b7beng\u00b7ar\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schl\u00e4gt einst sch\u00f6ne Falterfl\u00fcgel!", "tokens": ["Schl\u00e4gt", "einst", "sch\u00f6\u00b7ne", "Fal\u00b7ter\u00b7fl\u00fc\u00b7gel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Seht im Wind das Laub sich kr\u00e4useln!", "tokens": ["Seht", "im", "Wind", "das", "Laub", "sich", "kr\u00e4u\u00b7seln", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00f6g' es einst, wenn H\u00f6rer lauschen,", "tokens": ["M\u00f6g'", "es", "einst", ",", "wenn", "H\u00f6\u00b7rer", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "$,", "KOUS", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ein frisches Laubess\u00e4useln", "tokens": ["Wie", "ein", "fri\u00b7sches", "Lau\u00b7bes\u00b7s\u00e4u\u00b7seln"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch durch unsre Lieder rauschen!", "tokens": ["Auch", "durch", "uns\u00b7re", "Lie\u00b7der", "rau\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Herz an Herz, und Arm' in Armen!", "tokens": ["Herz", "an", "Herz", ",", "und", "Arm'", "in", "Ar\u00b7men", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "KON", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weckt die jungen Keim' im Boden,", "tokens": ["Weckt", "die", "jun\u00b7gen", "Keim'", "im", "Bo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df sie meinen, zu erwarmen", "tokens": ["Da\u00df", "sie", "mei\u00b7nen", ",", "zu", "er\u00b7war\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schon durch Fr\u00fchlings lauen Odem!", "tokens": ["Schon", "durch", "Fr\u00fch\u00b7lings", "lau\u00b7en", "O\u00b7dem", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "La\u00dft ert\u00f6nen die Ges\u00e4nge,", "tokens": ["La\u00dft", "er\u00b7t\u00f6\u00b7nen", "die", "Ge\u00b7s\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df die Rosen in den Tiefen", "tokens": ["Da\u00df", "die", "Ro\u00b7sen", "in", "den", "Tie\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fr\u00fch'r erweckt, als ob die Kl\u00e4nge", "tokens": ["Fr\u00fch'r", "er\u00b7weckt", ",", "als", "ob", "die", "Kl\u00e4n\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "$,", "KOKOM", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eines Lenzes wach sie riefen!", "tokens": ["Ei\u00b7nes", "Len\u00b7zes", "wach", "sie", "rie\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Und umlacht von Bl\u00fcthenscherzen", "tokens": ["Und", "um\u00b7lacht", "von", "Bl\u00fc\u00b7then\u00b7scher\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Und umspielt von Zephyrs Kosen,", "tokens": ["Und", "um\u00b7spielt", "von", "Ze\u00b7phyrs", "Ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NE", "NE", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "S\u00fc\u00dfe Hoffnungen im Herzen,", "tokens": ["S\u00fc\u00b7\u00dfe", "Hoff\u00b7nun\u00b7gen", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sinken wir einst in die Rosen.", "tokens": ["Sin\u00b7ken", "wir", "einst", "in", "die", "Ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "+--++-+-", "measure": "dactylic.init"}}}}}