{"textgrid.poem.26162": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Nie sind der Fr\u00fchlingsnacht die Wege leer", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Nacht macht alle B\u00e4ume gleich,", "tokens": ["Die", "Nacht", "macht", "al\u00b7le", "B\u00e4u\u00b7me", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie stehen wie die dunklen Mauern", "tokens": ["Sie", "ste\u00b7hen", "wie", "die", "dunk\u00b7len", "Mau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von einem unterirdischen Reich", "tokens": ["Von", "ei\u00b7nem", "un\u00b7ter\u00b7ir\u00b7di\u00b7schen", "Reich"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und wie Gestalten, die am Wege kauern.", "tokens": ["Und", "wie", "Ge\u00b7stal\u00b7ten", ",", "die", "am", "We\u00b7ge", "kau\u00b7ern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch ihre Fr\u00fchlingsgeister halten mit dir Schritt.", "tokens": ["Doch", "ih\u00b7re", "Fr\u00fch\u00b7lings\u00b7geis\u00b7ter", "hal\u00b7ten", "mit", "dir", "Schritt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie senden Bl\u00fctenrauch im Dunkeln her", "tokens": ["Sie", "sen\u00b7den", "Bl\u00fc\u00b7ten\u00b7rauch", "im", "Dun\u00b7keln", "her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "APPRART", "NN", "APZR"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und gehen abwechselnd am Wege mit,", "tokens": ["Und", "ge\u00b7hen", "ab\u00b7wech\u00b7selnd", "am", "We\u00b7ge", "mit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und sie verlassen dich nur schwer.", "tokens": ["Und", "sie", "ver\u00b7las\u00b7sen", "dich", "nur", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nie sind der Fr\u00fchlingsnacht die Wege leer.", "tokens": ["Nie", "sind", "der", "Fr\u00fch\u00b7lings\u00b7nacht", "die", "We\u00b7ge", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}