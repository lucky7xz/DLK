{"textgrid.poem.46080": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Nachtrag zum Weihnachtsliede", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das M\u00e4gdlein gieng zur Linde", "tokens": ["Das", "M\u00e4gd\u00b7lein", "gieng", "zur", "Lin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und seufzte gar betr\u00fcbt:", "tokens": ["Und", "seufz\u00b7te", "gar", "be\u00b7tr\u00fcbt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was schenk' ich nur dem Kinde,", "tokens": ["Was", "schenk'", "ich", "nur", "dem", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das mich so treu geliebt?", "tokens": ["Das", "mich", "so", "treu", "ge\u00b7liebt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da schwebte her zur Linde", "tokens": ["Da", "schweb\u00b7te", "her", "zur", "Lin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Engel lieb und rein", "tokens": ["Ein", "En\u00b7gel", "lieb", "und", "rein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Arme, Kranke, Blinde,", "tokens": ["Und", "Ar\u00b7me", ",", "Kran\u00b7ke", ",", "Blin\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die zogen hinterdrein.", "tokens": ["Die", "zo\u00b7gen", "hin\u00b7ter\u00b7drein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie trugen in der Mitte", "tokens": ["Sie", "tru\u00b7gen", "in", "der", "Mit\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl einen Weihnachtsbaum", "tokens": ["Wohl", "ei\u00b7nen", "Weih\u00b7nachts\u00b7baum"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ganz nach der alten Sitte", "tokens": ["Ganz", "nach", "der", "al\u00b7ten", "Sit\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gleich einem Kindertraum.", "tokens": ["Gleich", "ei\u00b7nem", "Kin\u00b7der\u00b7traum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie setzten's B\u00e4umlein nieder", "tokens": ["Sie", "setz\u00b7ten's", "B\u00e4um\u00b7lein", "nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vors arme M\u00e4gdelein,", "tokens": ["Vors", "ar\u00b7me", "M\u00e4g\u00b7del\u00b7ein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sangen Dankeslieder", "tokens": ["Und", "san\u00b7gen", "Dan\u00b7kes\u00b7lie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und sprachen: das ist dein.", "tokens": ["Und", "spra\u00b7chen", ":", "das", "ist", "dein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PDS", "VAFIN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Was Gott dir hat gegeben,", "tokens": ["Was", "Gott", "dir", "hat", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hast du mit uns geteilt,", "tokens": ["Hast", "du", "mit", "uns", "ge\u00b7teilt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein Lieben gab uns Leben,", "tokens": ["Dein", "Lie\u00b7ben", "gab", "uns", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dein Heil hat uns geheilt.", "tokens": ["Dein", "Heil", "hat", "uns", "ge\u00b7heilt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Drum haben wir Elende", "tokens": ["Drum", "ha\u00b7ben", "wir", "E\u00b7len\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am Fest uns auch geregt,", "tokens": ["Am", "Fest", "uns", "auch", "ge\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Dank der kranken H\u00e4nde", "tokens": ["Den", "Dank", "der", "kran\u00b7ken", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ans Kinderherz gelegt.", "tokens": ["Ans", "Kin\u00b7der\u00b7herz", "ge\u00b7legt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Leid ist's von dir mitleidet,", "tokens": ["Leid", "ist's", "von", "dir", "mit\u00b7lei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schmerz ist's von dir gestillt,", "tokens": ["Schmerz", "ist's", "von", "dir", "ge\u00b7stillt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nacktheit von dir bekleidet,", "tokens": ["Nackt\u00b7heit", "von", "dir", "be\u00b7klei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Ist deiner Liebe Bild.", "tokens": ["Ist", "dei\u00b7ner", "Lie\u00b7be", "Bild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Da ward das M\u00e4gdlein stille,", "tokens": ["Da", "ward", "das", "M\u00e4gd\u00b7lein", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dacht': \u00bbo welch s\u00fc\u00dfer Traum!", "tokens": ["Dacht'", ":", "\u00bb", "o", "welch", "s\u00fc\u00b7\u00dfer", "Traum", "!"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "$(", "FM", "PWAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Jetzt in der Zeiten F\u00fclle,", "tokens": ["Jetzt", "in", "der", "Zei\u00b7ten", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Welch reicher Weihnachtsbaum!", "tokens": ["Welch", "rei\u00b7cher", "Weih\u00b7nachts\u00b7baum", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Will gleich dem Kind ihn bringen,", "tokens": ["Will", "gleich", "dem", "Kind", "ihn", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O das wird freudig sein.\u00ab", "tokens": ["O", "das", "wird", "freu\u00b7dig", "sein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "PDS", "VAFIN", "ADJD", "VAINF", "$.", "$("], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Da hob mit s\u00fc\u00dfem Klingen", "tokens": ["Da", "hob", "mit", "s\u00fc\u00b7\u00dfem", "Klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich sanft ein Stimmlein fein.", "tokens": ["Sich", "sanft", "ein", "Stimm\u00b7lein", "fein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Im G\u00e4rtchen sich erhebet", "tokens": ["Im", "G\u00e4rt\u00b7chen", "sich", "er\u00b7he\u00b7bet"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Wachs das Jesulein,", "tokens": ["Von", "Wachs", "das", "Je\u00b7su\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und geht umher und lebet", "tokens": ["Und", "geht", "um\u00b7her", "und", "le\u00b7bet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Patscht in die H\u00e4ndlein klein.", "tokens": ["Patscht", "in", "die", "H\u00e4nd\u00b7lein", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Und spricht mit s\u00fc\u00dfem Lachen,", "tokens": ["Und", "spricht", "mit", "s\u00fc\u00b7\u00dfem", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ach das ist doch was wert,", "tokens": ["Ach", "das", "ist", "doch", "was", "wert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PDS", "VAFIN", "ADV", "PWS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach was f\u00fcr sch\u00f6ne Sachen", "tokens": ["Ach", "was", "f\u00fcr", "sch\u00f6\u00b7ne", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "PWS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hat mir arm Lind beschert!", "tokens": ["Hat", "mir", "arm", "Lind", "be\u00b7schert", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Was Armen sie gegeben,", "tokens": ["Was", "Ar\u00b7men", "sie", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das all sie mir auch giebt,", "tokens": ["Das", "all", "sie", "mir", "auch", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O welch ein sch\u00f6nes Leben", "tokens": ["O", "welch", "ein", "sch\u00f6\u00b7nes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PIAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn Arm den Armen liebt!", "tokens": ["Wenn", "Arm", "den", "Ar\u00b7men", "liebt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Ja weil ich arm, so reichet", "tokens": ["Ja", "weil", "ich", "arm", ",", "so", "rei\u00b7chet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "KOUS", "PPER", "ADJD", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Armut sie, was mir", "tokens": ["Der", "Ar\u00b7mut", "sie", ",", "was", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PPER", "$,", "PWS", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und weil sie arm, so reichet", "tokens": ["Und", "weil", "sie", "arm", ",", "so", "rei\u00b7chet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADJD", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Armut mir, was ihr.", "tokens": ["Die", "Ar\u00b7mut", "mir", ",", "was", "ihr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$,", "PWS", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Nach diesen lieben Worten", "tokens": ["Nach", "die\u00b7sen", "lie\u00b7ben", "Wor\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist in dem Weihnachtsbaum", "tokens": ["Ist", "in", "dem", "Weih\u00b7nachts\u00b7baum"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Herz getr\u00f6stet worden,", "tokens": ["Ein", "Herz", "ge\u00b7tr\u00f6s\u00b7tet", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Traut seinen Ohren kaum.", "tokens": ["Traut", "sei\u00b7nen", "Oh\u00b7ren", "kaum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Es dacht', der armen Linde", "tokens": ["Es", "dacht'", ",", "der", "ar\u00b7men", "Lin\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ward ich vorm Jahr beschert,", "tokens": ["Ward", "ich", "vorm", "Jahr", "be\u00b7schert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und drum dem Jesuskinde", "tokens": ["Und", "drum", "dem", "Je\u00b7sus\u00b7kin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PAV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu gleicher Zeit verehrt.", "tokens": ["Zu", "glei\u00b7cher", "Zeit", "ver\u00b7ehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Und dieses hat gesungen", "tokens": ["Und", "die\u00b7ses", "hat", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDS", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Herz im Weihnachtsbaum", "tokens": ["Das", "Herz", "im", "Weih\u00b7nachts\u00b7baum"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von Armendank umrungen", "tokens": ["Von", "Ar\u00b7men\u00b7dank", "um\u00b7run\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Lamm, N\u00fcssen, goldnem Schaum!", "tokens": ["Lamm", ",", "N\u00fcs\u00b7sen", ",", "gold\u00b7nem", "Schaum", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}