{"textgrid.poem.64657": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "284. Uebersetzung einer Stelle aus Landreau du Maine au Picq Legislation philosophique", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Getrieben von des Pr\u00fcgels Macht", "tokens": ["Ge\u00b7trie\u00b7ben", "von", "des", "Pr\u00fc\u00b7gels", "Macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Marschirt der deutsche Knecht schwerf\u00e4llig in die Schlacht.", "tokens": ["Mar\u00b7schirt", "der", "deut\u00b7sche", "Knecht", "schwer\u00b7f\u00e4l\u00b7lig", "in", "die", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Den freyen Gallier sieht man zu seinen Siegen", "tokens": ["Den", "frey\u00b7en", "Gal\u00b7lier", "sieht", "man", "zu", "sei\u00b7nen", "Sie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Voll Heldenmuth, nicht laufen, wirklich fliegen.", "tokens": ["Voll", "Hel\u00b7den\u00b7muth", ",", "nicht", "lau\u00b7fen", ",", "wirk\u00b7lich", "flie\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "PTKNEG", "VVINF", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Den Flug des Galliers sah'n Weser, Saal' und Rhein,", "tokens": ["Den", "Flug", "des", "Gal\u00b7liers", "sah'n", "We\u00b7ser", ",", "Saal'", "und", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "NN", "$,", "NN", "KON", "NE", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da holten ihn sogar nicht deutsche Kugeln ein.", "tokens": ["Da", "hol\u00b7ten", "ihn", "so\u00b7gar", "nicht", "deut\u00b7sche", "Ku\u00b7geln", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}