{"textgrid.poem.51381": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wann ich nicht bey dir bin, lieg ich in solcher Noth,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann ich nicht bey dir bin, lieg ich in solcher Noth,", "tokens": ["Wann", "ich", "nicht", "bey", "dir", "bin", ",", "lieg", "ich", "in", "sol\u00b7cher", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "APPR", "PPER", "VAFIN", "$,", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als einer, welcher stirbt und ringet mit dem Tod:", "tokens": ["Als", "ei\u00b7ner", ",", "wel\u00b7cher", "stirbt", "und", "rin\u00b7get", "mit", "dem", "Tod", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PRELS", "ADJD", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und komm ich ie zu dir, so kan ich nicht bestehen,", "tokens": ["Und", "komm", "ich", "ie", "zu", "dir", ",", "so", "kan", "ich", "nicht", "be\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$,", "ADV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und sterbe so dahin, und kan doch nicht vergehen:", "tokens": ["Und", "ster\u00b7be", "so", "da\u00b7hin", ",", "und", "kan", "doch", "nicht", "ver\u00b7ge\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PAV", "$,", "KON", "VMFIN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein Abseyn kr\u00e4ncket mich von Liebe gegen dir,", "tokens": ["Mein", "Ab\u00b7seyn", "kr\u00e4n\u00b7cket", "mich", "von", "Lie\u00b7be", "ge\u00b7gen", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dein Beyseyn t\u00f6dtet mich durch H\u00e4rte gegen mir.", "tokens": ["Dein", "Bey\u00b7seyn", "t\u00f6d\u00b7tet", "mich", "durch", "H\u00e4r\u00b7te", "ge\u00b7gen", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}