{"textgrid.poem.35632": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Traumwandler", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo bist du, die mir zur Seite ging,", "tokens": ["Wo", "bist", "du", ",", "die", "mir", "zur", "Sei\u00b7te", "ging", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo bist du, Himmelsangesicht?", "tokens": ["Wo", "bist", "du", ",", "Him\u00b7mel\u00b7san\u00b7ge\u00b7sicht", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein rauher Wind h\u00f6hnt mir ins Ohr: du Narr!", "tokens": ["Ein", "rau\u00b7her", "Wind", "h\u00f6hnt", "mir", "ins", "Ohr", ":", "du", "Narr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "NN", "$.", "NE", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Traum! Ein Traum! Du Tor!", "tokens": ["Ein", "Traum", "!", "Ein", "Traum", "!", "Du", "Tor", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "NN", "$.", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und doch, und doch! Wie war es einst,", "tokens": ["Und", "doch", ",", "und", "doch", "!", "Wie", "war", "es", "einst", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KON", "ADV", "$.", "PWAV", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bevor ich in Nacht und Verlassenheit schritt?", "tokens": ["Be\u00b7vor", "ich", "in", "Nacht", "und", "Ver\u00b7las\u00b7sen\u00b7heit", "schritt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Wei\u00dft du es noch, du Narr, du Tor!", "tokens": ["Wei\u00dft", "du", "es", "noch", ",", "du", "Narr", ",", "du", "Tor", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "$,", "NE", "NE", "$,", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Meiner Seele Echo, der rauhe Wind:", "tokens": ["Mei\u00b7ner", "See\u00b7le", "E\u00b7cho", ",", "der", "rau\u00b7he", "Wind", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "O Narr! O Tor!", "tokens": ["O", "Narr", "!", "O", "Tor", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "NE", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Stand sie mit bittenden H\u00e4nden nicht,", "tokens": ["Stand", "sie", "mit", "bit\u00b7ten\u00b7den", "H\u00e4n\u00b7den", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "PTKNEG", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Ein trauriges L\u00e4cheln um den Mund,", "tokens": ["Ein", "trau\u00b7ri\u00b7ges", "L\u00e4\u00b7cheln", "um", "den", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Und rief in Nacht und Verlassenheit!", "tokens": ["Und", "rief", "in", "Nacht", "und", "Ver\u00b7las\u00b7sen\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Was rief sie nur! Wei\u00dft du es nicht?", "tokens": ["Was", "rief", "sie", "nur", "!", "Wei\u00dft", "du", "es", "nicht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$.", "VVFIN", "PPER", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Wie Liebe klang's. Kein Echo trug", "tokens": ["Wie", "Lie\u00b7be", "klang'", "s.", "Kein", "E\u00b7cho", "trug"], "token_info": ["word", "word", "word", "abbreviation", "word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN", "NE", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Zu ihr zur\u00fcck, zu ihr dies Wort.", "tokens": ["Zu", "ihr", "zu\u00b7r\u00fcck", ",", "zu", "ihr", "dies", "Wort", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "$,", "APPR", "PPER", "PDS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "War's Liebe? Weh, da\u00df ich's verga\u00df!", "tokens": ["Wa\u00b7r's", "Lie\u00b7be", "?", "Weh", ",", "da\u00df", "ich's", "ver\u00b7ga\u00df", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$.", "NN", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.17": {"text": "Nur Nacht um mich und Verlassenheit,", "tokens": ["Nur", "Nacht", "um", "mich", "und", "Ver\u00b7las\u00b7sen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "PPER", "KON", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Und meiner Seele Echo \u2013 der Wind!", "tokens": ["Und", "mei\u00b7ner", "See\u00b7le", "E\u00b7cho", "\u2013", "der", "Wind", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$(", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.19": {"text": "Der h\u00f6hnt und h\u00f6hnt: O Narr! O Tor!", "tokens": ["Der", "h\u00f6hnt", "und", "h\u00f6hnt", ":", "O", "Narr", "!", "O", "Tor", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "$.", "NE", "NE", "$.", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}