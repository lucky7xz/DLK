{"textgrid.poem.28975": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "90.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alsbald ein neues Kind", "tokens": ["Als\u00b7bald", "ein", "neu\u00b7es", "Kind"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die erste Lufft entfindt,", "tokens": ["Die", "ers\u00b7te", "Lufft", "ent\u00b7findt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So hebt es an zu weinen;", "tokens": ["So", "hebt", "es", "an", "zu", "wei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Sonne mu\u00df ihm scheinen", "tokens": ["Die", "Son\u00b7ne", "mu\u00df", "ihm", "schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Den viermahl zehnden Tag,", "tokens": ["Den", "vier\u00b7mahl", "zehn\u00b7den", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Eh als es lachen mag.", "tokens": ["Eh", "als", "es", "la\u00b7chen", "mag."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["NN", "KOUS", "PPER", "VVFIN", "NE"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "O Welt, bey deinen Sachen", "tokens": ["O", "Welt", ",", "bey", "dei\u00b7nen", "Sa\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ist weinen mehr als lachen!", "tokens": ["Ist", "wei\u00b7nen", "mehr", "als", "la\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVINF", "PIAT", "KOKOM", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}