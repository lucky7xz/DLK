{"textgrid.poem.57538": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Am Galgenberge", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein sandiger H\u00fcgel ist es, nackt und kahl,", "tokens": ["Ein", "san\u00b7di\u00b7ger", "H\u00fc\u00b7gel", "ist", "es", ",", "nackt", "und", "kahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein kranker Lindenbaum ist seine Krone,", "tokens": ["Ein", "kran\u00b7ker", "Lin\u00b7den\u00b7baum", "ist", "sei\u00b7ne", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sein schiefer Wuchs, sein Laub, vom Staube fahl,", "tokens": ["Sein", "schie\u00b7fer", "Wuchs", ",", "sein", "Laub", ",", "vom", "Stau\u00b7be", "fahl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gereicht der edlen Abkunft fast zum Hohne;", "tokens": ["Ge\u00b7reicht", "der", "ed\u00b7len", "Ab\u00b7kunft", "fast", "zum", "Hoh\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Am H\u00fcgelgrunde wuchert Heidekraut,", "tokens": ["Am", "H\u00fc\u00b7gel\u00b7grun\u00b7de", "wu\u00b7chert", "Hei\u00b7de\u00b7kraut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dort schwenkt der Ginster seine schlanken Loden,", "tokens": ["Dort", "schwenkt", "der", "Gins\u00b7ter", "sei\u00b7ne", "schlan\u00b7ken", "Lo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und hier und da aus ockergelbem Boden", "tokens": ["Und", "hier", "und", "da", "aus", "oc\u00b7ker\u00b7gel\u00b7bem", "Bo\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KON", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein k\u00fcmmerliches Glockenbl\u00fcmchen schaut.", "tokens": ["Ein", "k\u00fcm\u00b7mer\u00b7li\u00b7ches", "Glo\u00b7cken\u00b7bl\u00fcm\u00b7chen", "schaut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und gibt es hier viel mehr auch nicht zu sehn,", "tokens": ["Und", "gibt", "es", "hier", "viel", "mehr", "auch", "nicht", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ADV", "ADV", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich lieb' es, sinnend in dem Sand zu tr\u00e4umen,", "tokens": ["Ich", "lieb'", "es", ",", "sin\u00b7nend", "in", "dem", "Sand", "zu", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADJD", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn leise Winde durch die Heide wehn", "tokens": ["Wenn", "lei\u00b7se", "Win\u00b7de", "durch", "die", "Hei\u00b7de", "wehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Abendstrahlen ihre Grenzen s\u00e4umen;", "tokens": ["Und", "A\u00b7bend\u00b7strah\u00b7len", "ih\u00b7re", "Gren\u00b7zen", "s\u00e4u\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Den Geist besch\u00e4ftigt dann so mancherlei,", "tokens": ["Den", "Geist", "be\u00b7sch\u00e4f\u00b7tigt", "dann", "so", "man\u00b7cher\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "PIS", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Auch die vergangne Zeit und ihre Schrecken,", "tokens": ["Auch", "die", "ver\u00b7gang\u00b7ne", "Zeit", "und", "ih\u00b7re", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die kein vermorschter Flitter kann bedecken,", "tokens": ["Die", "kein", "ver\u00b7morschter", "Flit\u00b7ter", "kann", "be\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Ich freue mich, da\u00df diese Zeit vorbei.", "tokens": ["Ich", "freu\u00b7e", "mich", ",", "da\u00df", "die\u00b7se", "Zeit", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Man lobt so gern die gute, alte Zeit,", "tokens": ["Man", "lobt", "so", "gern", "die", "gu\u00b7te", ",", "al\u00b7te", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und ruft zur\u00fcck die l\u00e4ngstvergangenen Tage,", "tokens": ["Und", "ruft", "zu\u00b7r\u00fcck", "die", "l\u00e4ngst\u00b7ver\u00b7gan\u00b7ge\u00b7nen", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wo unber\u00fchrt von satter N\u00fcchternheit", "tokens": ["Wo", "un\u00b7be\u00b7r\u00fchrt", "von", "sat\u00b7ter", "N\u00fcch\u00b7tern\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Zeit verflo\u00df, verkl\u00e4rt von Sang und Sage,", "tokens": ["Die", "Zeit", "ver\u00b7flo\u00df", ",", "ver\u00b7kl\u00e4rt", "von", "Sang", "und", "Sa\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wo frommen Schauder jedes Herz empfand \u2013", "tokens": ["Wo", "from\u00b7men", "Schau\u00b7der", "je\u00b7des", "Herz", "emp\u00b7fand", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nach Idealen noch die Menschheit strebte,", "tokens": ["Nach", "I\u00b7dea\u00b7len", "noch", "die", "Menschheit", "streb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-++-", "measure": "unknown.measure.penta"}, "line.7": {"text": "Nicht ganz allein dem Geldgewinne lebte,", "tokens": ["Nicht", "ganz", "al\u00b7lein", "dem", "Geld\u00b7ge\u00b7win\u00b7ne", "leb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und unentweiht der Gottheit Bildnis stand.", "tokens": ["Und", "un\u00b7ent\u00b7weiht", "der", "Got\u00b7theit", "Bild\u00b7nis", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Stets mu\u00df ich l\u00e4cheln, h\u00f6r' ich solch Geschw\u00e4tz", "tokens": ["Stets", "mu\u00df", "ich", "l\u00e4\u00b7cheln", ",", "h\u00f6r'", "ich", "solch", "Ge\u00b7schw\u00e4tz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von Leuten, die noch alte Tugend heucheln", "tokens": ["Von", "Leu\u00b7ten", ",", "die", "noch", "al\u00b7te", "Tu\u00b7gend", "heu\u00b7cheln"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "ADV", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und mit der Biederkeit entlehntem Netz", "tokens": ["Und", "mit", "der", "Bie\u00b7der\u00b7keit", "ent\u00b7lehn\u00b7tem", "Netz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Notd\u00fcrftig ihrem Tugendd\u00fcnkel schmeicheln \u2013", "tokens": ["Not\u00b7d\u00fcrf\u00b7tig", "ih\u00b7rem", "Tu\u00b7gend\u00b7d\u00fcn\u00b7kel", "schmei\u00b7cheln", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ruft sie zur\u00fcck, die gute, alte Zeit,", "tokens": ["Ruft", "sie", "zu\u00b7r\u00fcck", ",", "die", "gu\u00b7te", ",", "al\u00b7te", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Schaut her, ich will die goldne Zeit euch zeigen,", "tokens": ["Schaut", "her", ",", "ich", "will", "die", "gold\u00b7ne", "Zeit", "euch", "zei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER", "VMFIN", "ART", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Nat\u00fcrlich werde ich euch nichts verschweigen", "tokens": ["Na\u00b7t\u00fcr\u00b7lich", "wer\u00b7de", "ich", "euch", "nichts", "ver\u00b7schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "PIS", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Von ihrer blutigroten Biederkeit!", "tokens": ["Von", "ih\u00b7rer", "blu\u00b7ti\u00b7gro\u00b7ten", "Bie\u00b7der\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wenn ich hier an dem siechen Lindenbaum", "tokens": ["Wenn", "ich", "hier", "an", "dem", "sie\u00b7chen", "Lin\u00b7den\u00b7baum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Glieder in der Abendsonne recke,", "tokens": ["Die", "Glie\u00b7der", "in", "der", "A\u00b7bend\u00b7son\u00b7ne", "re\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann scheint's mir oft, als ob wie d\u00fcstrer Traum", "tokens": ["Dann", "scheint's", "mir", "oft", ",", "als", "ob", "wie", "d\u00fcst\u00b7rer", "Traum"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KOKOM", "KOUS", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vergrauter Tage Bild sich neu erwecke;", "tokens": ["Ver\u00b7grau\u00b7ter", "Ta\u00b7ge", "Bild", "sich", "neu", "er\u00b7we\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wie Menschenhaufen w\u00e4lzt es sich heran,", "tokens": ["Wie", "Men\u00b7schen\u00b7hau\u00b7fen", "w\u00e4lzt", "es", "sich", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ein S\u00fcndergl\u00f6ckchen h\u00f6r' ich weinend l\u00e4uten,", "tokens": ["Ein", "S\u00fcn\u00b7der\u00b7gl\u00f6ck\u00b7chen", "h\u00f6r'", "ich", "wei\u00b7nend", "l\u00e4u\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Den Karren keuchen, die Soldaten schreiten \u2013", "tokens": ["Den", "Kar\u00b7ren", "keu\u00b7chen", ",", "die", "Sol\u00b7da\u00b7ten", "schrei\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und auf dem Wagen kniet ein bleicher Mann.", "tokens": ["Und", "auf", "dem", "Wa\u00b7gen", "kniet", "ein", "blei\u00b7cher", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Nicht wahr, das war doch eine sch\u00f6ne Zeit,", "tokens": ["Nicht", "wahr", ",", "das", "war", "doch", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "PDS", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als statt der Linde hier drei Balken standen,", "tokens": ["Als", "statt", "der", "Lin\u00b7de", "hier", "drei", "Bal\u00b7ken", "stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NE", "ADV", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als Seilers T\u00f6chterlein hier ward gefreit,", "tokens": ["Als", "Sei\u00b7lers", "T\u00f6ch\u00b7ter\u00b7lein", "hier", "ward", "ge\u00b7freit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die ihre Liebsten schlang in feste Banden,", "tokens": ["Die", "ih\u00b7re", "Liebs\u00b7ten", "schlang", "in", "fes\u00b7te", "Ban\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wie sch\u00f6n, wenn ein fideles S\u00fcnderpaar", "tokens": ["Wie", "sch\u00f6n", ",", "wenn", "ein", "fi\u00b7de\u00b7les", "S\u00fcn\u00b7der\u00b7paar"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Im Abendwinde gravit\u00e4tisch schaukelt", "tokens": ["Im", "Ab\u00b7end\u00b7win\u00b7de", "gra\u00b7vi\u00b7t\u00e4\u00b7tisch", "schau\u00b7kelt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und in den L\u00fcften heiser kr\u00e4chzend gaukelt", "tokens": ["Und", "in", "den", "L\u00fcf\u00b7ten", "hei\u00b7ser", "kr\u00e4ch\u00b7zend", "gau\u00b7kelt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "VVPP", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die unbezahlte Totengr\u00e4berschar.", "tokens": ["Die", "un\u00b7be\u00b7zahl\u00b7te", "To\u00b7ten\u00b7gr\u00e4\u00b7ber\u00b7schar", "."], "token_info": ["word", "word", "word", "punct"], "