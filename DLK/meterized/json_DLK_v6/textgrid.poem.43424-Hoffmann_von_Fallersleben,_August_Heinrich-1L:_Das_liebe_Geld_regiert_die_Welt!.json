{"textgrid.poem.43424": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das liebe Geld regiert die Welt!", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das liebe Geld regiert die Welt!", "tokens": ["Das", "lie\u00b7be", "Geld", "re\u00b7giert", "die", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So sagen alle Leute.", "tokens": ["So", "sa\u00b7gen", "al\u00b7le", "Leu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wol wahr, denn Alles ist f\u00fcr Geld", "tokens": ["Wol", "wahr", ",", "denn", "Al\u00b7les", "ist", "f\u00fcr", "Geld"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KON", "PIS", "VAFIN", "APPR", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Zu haben auch noch heute.", "tokens": ["Zu", "ha\u00b7ben", "auch", "noch", "heu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VAFIN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Leider Alles, Alles, Alles ist f\u00fcr Geld,", "tokens": ["Lei\u00b7der", "Al\u00b7les", ",", "Al\u00b7les", ",", "Al\u00b7les", "ist", "f\u00fcr", "Geld", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "PIS", "$,", "PIS", "VAFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Doch f\u00fcr Geld zu haben.