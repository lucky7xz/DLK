{"textgrid.poem.33633": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Einsamkeit", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie lang schon trat niemand mehr ein", "tokens": ["Wie", "lang", "schon", "trat", "nie\u00b7mand", "mehr", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADV", "VVFIN", "PIS", "ADV", "ART"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "In dieses stille Zimmer;", "tokens": ["In", "die\u00b7ses", "stil\u00b7le", "Zim\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nur hier das bischen Sonnenschein", "tokens": ["Nur", "hier", "das", "bi\u00b7schen", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gl\u00e4nzt heute noch wie immer.", "tokens": ["Gl\u00e4nzt", "heu\u00b7te", "noch", "wie", "im\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "KOKOM", "ADV", "$."], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}}, "stanza.2": {"line.1": {"text": "Und alles ringsum aufger\u00e4umt", "tokens": ["Und", "al\u00b7les", "ring\u00b7sum", "auf\u00b7ge\u00b7r\u00e4umt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIS", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wie ich's sonst gefunden;", "tokens": ["Und", "wie", "ich's", "sonst", "ge\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Wanduhr nur steht still und tr\u00e4umt", "tokens": ["Die", "Wan\u00b7duhr", "nur", "steht", "still", "und", "tr\u00e4umt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von l\u00e4ngst vergangnen Stunden.", "tokens": ["Von", "l\u00e4ngst", "ver\u00b7gang\u00b7nen", "Stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie still es ist! Nur dann und wann", "tokens": ["Wie", "still", "es", "ist", "!", "Nur", "dann", "und", "wann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPER", "VAFIN", "$.", "ADV", "ADV", "KON", "PWAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sommerfliege Summen.", "tokens": ["Der", "Som\u00b7mer\u00b7flie\u00b7ge", "Sum\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hier sa\u00df ich oft allein und sann", "tokens": ["Hier", "sa\u00df", "ich", "oft", "al\u00b7lein", "und", "sann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In innerem Verstummen.", "tokens": ["In", "in\u00b7ne\u00b7rem", "Ver\u00b7stum\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Entmutigt sein, wenn alles hofft,", "tokens": ["Ent\u00b7mu\u00b7tigt", "sein", ",", "wenn", "al\u00b7les", "hofft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VAINF", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Wenn alles lebt, gebunden \u2013", "tokens": ["Wenn", "al\u00b7les", "lebt", ",", "ge\u00b7bun\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich kenne sie, ich hab' sie oft", "tokens": ["Ich", "ken\u00b7ne", "sie", ",", "ich", "hab'", "sie", "oft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gef\u00fchlt, die bittern Stunden!", "tokens": ["Ge\u00b7f\u00fchlt", ",", "die", "bit\u00b7tern", "Stun\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}