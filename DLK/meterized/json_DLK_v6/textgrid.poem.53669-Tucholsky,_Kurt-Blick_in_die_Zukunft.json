{"textgrid.poem.53669": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Blick in die Zukunft", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du schl\u00e4fst bei mir. Da pl\u00f6tzlich, in der", "tokens": ["Du", "schl\u00e4fst", "bei", "mir", ".", "Da", "pl\u00f6tz\u00b7lich", ",", "in", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$.", "ADV", "ADJD", "$,", "APPR", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nacht, du liebe Dame,", "tokens": ["Nacht", ",", "du", "lie\u00b7be", "Da\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Bist du mit einem Laut mir j\u00e4h erwacht \u2013", "tokens": ["Bist", "du", "mit", "ei\u00b7nem", "Laut", "mir", "j\u00e4h", "er\u00b7wacht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "APPR", "PPER", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "War das ein Name?", "tokens": ["War", "das", "ein", "Na\u00b7me", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich horche. Und du sagst es noch einmal \u2013", "tokens": ["Ich", "hor\u00b7che", ".", "Und", "du", "sagst", "es", "noch", "ein\u00b7mal", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "KON", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "im Halbschlaf: \u00bbLeo . . . \u00ab", "tokens": ["im", "Halb\u00b7schlaf", ":", "\u00bb", "Leo", ".", ".", ".", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "punct", "punct", "punct"], "pos": ["APPRART", "NN", "$.", "$(", "NE", "$.", "$.", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Bleib bei der Sache, G\u00f6ttin meiner Wahl!", "tokens": ["Bleib", "bei", "der", "Sa\u00b7che", ",", "G\u00f6t\u00b7tin", "mei\u00b7ner", "Wahl", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich hei\u00dfe Theo.", "tokens": ["Ich", "hei\u00b7\u00dfe", "Theo", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Noch bin ich bei dir. Wenn die Stunde", "tokens": ["Noch", "bin", "ich", "bei", "dir", ".", "Wenn", "die", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPER", "$.", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "naht, da wir uns trennen:", "tokens": ["naht", ",", "da", "wir", "uns", "tren\u00b7nen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Vielleicht lernt dich dann ein Regierungs-", "tokens": ["Viel\u00b7leicht", "lernt", "dich", "dann", "ein", "Re\u00b7gie\u00b7rungs"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "rat im Teeraum kennen.", "tokens": ["rat", "im", "Tee\u00b7raum", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Und gibst du seinen Armen nachts dich preis,", "tokens": ["Und", "gibst", "du", "sei\u00b7nen", "Ar\u00b7men", "nachts", "dich", "preis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "den stolzen Siegern: \u2013", "tokens": ["den", "stol\u00b7zen", "Sie\u00b7gern", ":", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Dann fl\u00fcstre einmal meinen Namen leis", "tokens": ["Dann", "fl\u00fcst\u00b7re", "ein\u00b7mal", "mei\u00b7nen", "Na\u00b7men", "leis"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und denk an Tigern.", "tokens": ["und", "denk", "an", "Ti\u00b7gern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}