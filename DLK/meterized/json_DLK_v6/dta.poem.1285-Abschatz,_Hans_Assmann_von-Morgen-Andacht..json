{"dta.poem.1285": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Morgen-Andacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Der begl\u00e4nzte Mond erbleichet", "tokens": ["Der", "be\u00b7gl\u00e4nz\u00b7te", "Mond", "er\u00b7blei\u00b7chet"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von der nahen Sonne Pracht/", "tokens": ["Von", "der", "na\u00b7hen", "Son\u00b7ne", "Pracht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aller Sternen Heer entweichet", "tokens": ["Al\u00b7ler", "Ster\u00b7nen", "Heer", "ent\u00b7wei\u00b7chet"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit der hingelegten Nacht:", "tokens": ["Mit", "der", "hin\u00b7ge\u00b7leg\u00b7ten", "Nacht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auff mein Hertz/ und la\u00df der S\u00fcnden", "tokens": ["Auff", "mein", "Hertz", "/", "und", "la\u00df", "der", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$(", "KON", "VVIMP", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Finsterni\u00df und Schlaff dahinden.", "tokens": ["Fins\u00b7ter\u00b7ni\u00df", "und", "Schlaff", "da\u00b7hin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Den gew\u00f6lbten Himmels-Vogen/", "tokens": ["Den", "ge\u00b7w\u00f6lb\u00b7ten", "Him\u00b7mels\u00b7Vo\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den Saffirnen Wunder-Bau/", "tokens": ["Den", "Saf\u00b7fir\u00b7nen", "Wun\u00b7der\u00b7Bau", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Die geraume Sternen-Au", "tokens": ["Die", "ge\u00b7rau\u00b7me", "Ster\u00b7nen\u00b7Au"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hegte zu des H\u00f6chsten Ruhme", "tokens": ["Heg\u00b7te", "zu", "des", "H\u00f6chs\u00b7ten", "Ruh\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Manche Licht- und Feuer-Blume.", "tokens": ["Man\u00b7che", "Licht", "und", "Feu\u00b7er\u00b7Blu\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "TRUNC", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ihre Zier mu\u00df nun erblassen/", "tokens": ["Ih\u00b7re", "Zier", "mu\u00df", "nun", "er\u00b7blas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihr entlehnter Glantz stirbt hin;", "tokens": ["Ihr", "ent\u00b7lehn\u00b7ter", "Glantz", "stirbt", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So mu\u00df auch der Mensch verlassen", "tokens": ["So", "mu\u00df", "auch", "der", "Mensch", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "ART", "NN", "VVINF"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Ehre/ Wollust und Gewinn:", "tokens": ["Eh\u00b7re", "/", "Wol\u00b7lust", "und", "Ge\u00b7winn", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "M\u00fche dich das Licht zu finden/", "tokens": ["M\u00fc\u00b7he", "dich", "das", "Licht", "zu", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das zu keiner Zeit kan schwinden.", "tokens": ["Das", "zu", "kei\u00b7ner", "Zeit", "kan", "schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PIAT", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}