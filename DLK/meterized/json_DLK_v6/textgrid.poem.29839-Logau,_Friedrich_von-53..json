{"textgrid.poem.29839": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "53.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was hilfft es einen Dieb, der morgen hencken sol,", "tokens": ["Was", "hilfft", "es", "ei\u00b7nen", "Dieb", ",", "der", "mor\u00b7gen", "hen\u00b7cken", "sol", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob er mit Spei\u00df und Tranck versorgt ist heute wol?", "tokens": ["Ob", "er", "mit", "Spei\u00df", "und", "Tranck", "ver\u00b7sorgt", "ist", "heu\u00b7te", "wol", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN", "VVPP", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den S\u00fcnder hilfft es nicht, den H\u00f6lle sol verschlingen,", "tokens": ["Den", "S\u00fcn\u00b7der", "hilfft", "es", "nicht", ",", "den", "H\u00f6l\u00b7le", "sol", "ver\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn er gleich in der Welt lebt stets bey guten Dingen.", "tokens": ["Wenn", "er", "gleich", "in", "der", "Welt", "lebt", "stets", "bey", "gu\u00b7ten", "Din\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER