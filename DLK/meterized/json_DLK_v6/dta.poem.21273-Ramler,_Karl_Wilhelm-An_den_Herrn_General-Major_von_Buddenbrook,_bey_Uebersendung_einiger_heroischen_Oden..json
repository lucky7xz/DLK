{"dta.poem.21273": {"metadata": {"author": {"name": "Ramler, Karl Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An  \n  den Herrn General-Major  \n von Buddenbrook,  \n bey Uebersendung einiger heroischen Oden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20476-3", "language": ["de:0.99"], "booktitle": "Ramler, Karl Wilhelm: Oden. Berlin, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Der du den Kriegesgeist in der Ge-\nschichte liebest", "tokens": ["Der", "du", "den", "Krie\u00b7ges\u00b7geist", "in", "der", "Ge", "schich\u00b7te", "lie\u00b7best"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und in der Poesie;", "tokens": ["Und", "in", "der", "Poe\u00b7sie", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und Deutsche Redlichkeit bey Welscher", "tokens": ["Und", "Deut\u00b7sche", "Red\u00b7lich\u00b7keit", "bey", "Wel\u00b7scher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "(die schwerste Harmonie!)", "tokens": ["(", "die", "schwers\u00b7te", "Har\u00b7mo\u00b7nie", "!", ")"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Empfiehl, o Buddenbrook, mir nicht", "tokens": ["Emp\u00b7fiehl", ",", "o", "Bud\u00b7den\u00b7brook", ",", "mir", "nicht"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "FM", "NE", "$,", "PPER", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Sparta, Rom, Athen;", "tokens": ["Von", "Spar\u00b7ta", ",", "Rom", ",", "A\u00b7then", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NE", "$,", "NE", "$,", "NE", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Verlange nicht durch mich auf v\u00e4terlicher", "tokens": ["Ver\u00b7lan\u00b7ge", "nicht", "durch", "mich", "auf", "v\u00e4\u00b7ter\u00b7li\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "APPR", "PPER", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein Lieblingsvolk zu sehn.", "tokens": ["Dein", "Lieb\u00b7lings\u00b7volk", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein Dichter, unerl\u00f6st von fremder Sor-", "tokens": ["Ein", "Dich\u00b7ter", ",", "un\u00b7er\u00b7l\u00f6st", "von", "frem\u00b7der", "Sor"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein leichteres Gedicht;", "tokens": ["Ein", "leich\u00b7te\u00b7res", "Ge\u00b7dicht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kornelljens Diadem, Volt\u00e4rens Kranz er-", "tokens": ["Kor\u00b7nell\u00b7jens", "Di\u00b7a\u00b7dem", ",", "Vol\u00b7t\u00e4\u00b7rens", "Kranz", "er"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "NE", "NE", "TRUNC"], "meter": "----+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der m\u00fcde K\u00e4mpfer nicht.", "tokens": ["Der", "m\u00fc\u00b7de", "K\u00e4mp\u00b7fer", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Als Ludwigs Maler sich des j\u00fcngern", "tokens": ["Als", "Lud\u00b7wigs", "Ma\u00b7ler", "sich", "des", "j\u00fcn\u00b7gern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "NN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Kodomannus Land", "tokens": ["Durch", "Ko\u00b7do\u00b7man\u00b7nus", "Land"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "(dem stolzen Gallier ein Vorbild eigner", "tokens": ["(", "dem", "stol\u00b7zen", "Gal\u00b7lier", "ein", "Vor\u00b7bild", "eig\u00b7ner"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "ADJA", "NN", "ART", "NN", "ADJA"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zu schildern unterwand:", "tokens": ["Zu", "schil\u00b7dern", "un\u00b7ter\u00b7wand", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da richtete sein Arm nicht Fechter ab,", "tokens": ["Da", "rich\u00b7te\u00b7te", "sein", "Arm", "nicht", "Fech\u00b7ter", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PTKNEG", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erzog nicht Ross und Mann;", "tokens": ["Er\u00b7zog", "nicht", "Ross", "und", "Mann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn K\u00fcnste dieser Art, wie sehr sie Krie-", "tokens": ["Denn", "K\u00fcns\u00b7te", "die\u00b7ser", "Art", ",", "wie", "sehr", "sie", "Krie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "PDAT", "NN", "$,", "PWAV", "ADV", "PPER", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Stehn tausend H\u00e4nden an.", "tokens": ["Stehn", "tau\u00b7send", "H\u00e4n\u00b7den", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und h\u00e4tte sein Geschick ihm dieses", "tokens": ["Und", "h\u00e4t\u00b7te", "sein", "Ge\u00b7schick", "ihm", "die\u00b7ses"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "PPER", "PDAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dann h\u00e4tt\u2019 er aus der Schlacht", "tokens": ["Dann", "h\u00e4tt'", "er", "aus", "der", "Schlacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Am Granikus uns nicht den grossen Phi-", "tokens": ["Am", "Gra\u00b7ni\u00b7kus", "uns", "nicht", "den", "gros\u00b7sen", "Phi"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPER", "PTKNEG", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bis Babylon gebracht. \u2014", "tokens": ["Bis", "Ba\u00b7by\u00b7lon", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Freund deines K\u00f6niges, nimm kleine", "tokens": ["Freund", "dei\u00b7nes", "K\u00f6\u00b7ni\u00b7ges", ",", "nimm", "klei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$,", "VVIMP", "ADJA"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Nimm, was ich geben kann,", "tokens": ["Nimm", ",", "was", "ich", "ge\u00b7ben", "kann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ein Opfer ", "tokens": ["Ein", "Op\u00b7fer"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Mein achtes Lustrum an!", "tokens": ["Mein", "ach\u00b7tes", "Lust\u00b7rum", "an", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}