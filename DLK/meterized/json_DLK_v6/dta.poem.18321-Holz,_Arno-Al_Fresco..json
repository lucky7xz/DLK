{"dta.poem.18321": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Al Fresco.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "St\u00fcndlich dr\u00e4ut ihr das Weltgericht", "tokens": ["St\u00fcnd\u00b7lich", "dr\u00e4ut", "ihr", "das", "Welt\u00b7ge\u00b7richt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Und sie schaudert bleich, im Herzen den Tod,", "tokens": ["Und", "sie", "schau\u00b7dert", "bleich", ",", "im", "Her\u00b7zen", "den", "Tod", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Ins blutig verlodernde Abendroth.", "tokens": ["Ins", "blu\u00b7tig", "ver\u00b7lo\u00b7dern\u00b7de", "A\u00b7ben\u00b7droth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Die Zeit ist morsch wie ein Todtenbein \u2014", "tokens": ["Die", "Zeit", "ist", "morsch", "wie", "ein", "Tod\u00b7ten\u00b7bein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KOKOM", "ART", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So ist es gewesen und so wird's sein:", "tokens": ["So", "ist", "es", "ge\u00b7we\u00b7sen", "und", "so", "wird's", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VAPP", "KON", "ADV", "VAFIN", "VAINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Roth vom Weltbaum taumelt das Laub,", "tokens": ["Roth", "vom", "Welt\u00b7baum", "tau\u00b7melt", "das", "Laub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "V\u00f6lker und Kronen zerfallen zu Staub", "tokens": ["V\u00f6l\u00b7ker", "und", "Kro\u00b7nen", "zer\u00b7fal\u00b7len", "zu", "Staub"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "APPR", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Und \u00fcber das christliche ", "tokens": ["Und", "\u00fc\u00b7ber", "das", "christ\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Hintaumelt ein nacktes Venusbild.", "tokens": ["Hin\u00b7tau\u00b7melt", "ein", "nack\u00b7tes", "Ve\u00b7nus\u00b7bild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}