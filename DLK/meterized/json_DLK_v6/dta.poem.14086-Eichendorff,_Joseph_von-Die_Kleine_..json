{"dta.poem.14086": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Kleine .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und ich werfe mich im Bettchen", "tokens": ["Und", "ich", "wer\u00b7fe", "mich", "im", "Bett\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nachts ohn' Unterla\u00df,", "tokens": ["Nachts", "ohn'", "Un\u00b7ter\u00b7la\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Kehr' mich links und kehr' mich rechts hin,", "tokens": ["Kehr'", "mich", "links", "und", "kehr'", "mich", "rechts", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "KON", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Nirgends hab' ich was,", "tokens": ["Nir\u00b7gends", "hab'", "ich", "was", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "lieb Mutter,", "tokens": ["lieb", "Mut\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Nirgends hab' ich was.", "tokens": ["Nir\u00b7gends", "hab'", "ich", "was", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}