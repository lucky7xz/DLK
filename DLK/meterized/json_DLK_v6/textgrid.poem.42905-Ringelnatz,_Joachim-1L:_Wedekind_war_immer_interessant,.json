{"textgrid.poem.42905": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wedekind war immer interessant,", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wedekind war immer interessant,", "tokens": ["We\u00b7de\u00b7kind", "war", "im\u00b7mer", "in\u00b7ter\u00b7es\u00b7sant", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Ein Sto\u00dfhorn in die h\u00e4\u00dflich mittlere Welt.", "tokens": ["Ein", "Sto\u00df\u00b7horn", "in", "die", "h\u00e4\u00df\u00b7lich", "mitt\u00b7le\u00b7re", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Wahrscheinlich hat er mich nie gekannt.", "tokens": ["Wahr\u00b7schein\u00b7lich", "hat", "er", "mich", "nie", "ge\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich bin ihm wohl zehnmal vorgestellt.", "tokens": ["Ich", "bin", "ihm", "wohl", "zehn\u00b7mal", "vor\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das letzte Mal hatten wir eine absurde,", "tokens": ["Das", "letz\u00b7te", "Mal", "hat\u00b7ten", "wir", "ei\u00b7ne", "ab\u00b7sur\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+---+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Mir unverge\u00dfliche Stunde mitnand,", "tokens": ["Mir", "un\u00b7ver\u00b7ge\u00df\u00b7li\u00b7che", "Stun\u00b7de", "mit\u00b7nand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Als ich zum Kriege gerufen wurde", "tokens": ["Als", "ich", "zum", "Krie\u00b7ge", "ge\u00b7ru\u00b7fen", "wur\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVPP", "VAFIN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Nach dem Nordseestrand.", "tokens": ["Nach", "dem", "Nord\u00b7sees\u00b7trand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Und als ich zur\u00fcckkehrte,", "tokens": ["Und", "als", "ich", "zu\u00b7r\u00fcck\u00b7kehr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "War der Verehrte", "tokens": ["War", "der", "Ver\u00b7ehr\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Verstorben.", "tokens": ["Ver\u00b7stor\u00b7ben", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Mehr bek\u00e4mpft als umworben,", "tokens": ["Mehr", "be\u00b7k\u00e4mpft", "als", "um\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "KOKOM", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Hat er doch trotzig gesiegt.", "tokens": ["Hat", "er", "doch", "trot\u00b7zig", "ge\u00b7siegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ehrliche und unehrliche Feinde", "tokens": ["Ehr\u00b7li\u00b7che", "und", "un\u00b7ehr\u00b7li\u00b7che", "Fein\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN"], "meter": "+----+--+-", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Haben doch ihn nicht kleingekriegt.", "tokens": ["Ha\u00b7ben", "doch", "ihn", "nicht", "klein\u00b7ge\u00b7kriegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "In seiner treuen Gemeinde", "tokens": ["In", "sei\u00b7ner", "treu\u00b7en", "Ge\u00b7mein\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Will ich mitgenannt sein.", "tokens": ["Will", "ich", "mit\u00b7ge\u00b7nannt", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVPP", "VAINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Ich senke jetzt meine Nase", "tokens": ["Ich", "sen\u00b7ke", "jetzt", "mei\u00b7ne", "Na\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.2": {"text": "Zu einem stillen Glase", "tokens": ["Zu", "ei\u00b7nem", "stil\u00b7len", "Gla\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wein.", "tokens": ["Wein", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}}, "stanza.7": {"line.1": {"text": "Apropos:", "tokens": ["A\u00b7pro\u00b7pos", ":"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Wein gibt sich anders als Bier. Und wo", "tokens": ["Wein", "gibt", "sich", "an\u00b7ders", "als", "Bier", ".", "Und", "wo"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "ADV", "KOUS", "NN", "$.", "KON", "PWAV"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Ist in M\u00fcnchen die Wedekindstra\u00dfe?", "tokens": ["Ist", "in", "M\u00fcn\u00b7chen", "die", "We\u00b7de\u00b7kinds\u00b7tra\u00b7\u00dfe", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NE", "ART", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}}}}