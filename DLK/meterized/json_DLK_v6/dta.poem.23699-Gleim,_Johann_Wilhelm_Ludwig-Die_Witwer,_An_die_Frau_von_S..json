{"dta.poem.23699": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Witwer,  \n An die Frau von S.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach, seht doch die M&#228;nner!", "tokens": ["Ach", ",", "seht", "doch", "die", "M", "&#228;", "nner", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "ADV", "ART", "NE", "$(", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sie schwimmen in Tr\u00e4nen.", "tokens": ["Sie", "schwim\u00b7men", "in", "Tr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Seht, ", "tokens": ["Seht", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Durch Blumbergs Gefilde!", "tokens": ["Durch", "Blum\u00b7bergs", "Ge\u00b7fil\u00b7de", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Er h\u00f6rt nicht die Lerche,", "tokens": ["Er", "h\u00f6rt", "nicht", "die", "Ler\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Er sieht nicht die Blumen,", "tokens": ["Er", "sieht", "nicht", "die", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Er f\u00fchlt nicht die Weste,", "tokens": ["Er", "f\u00fchlt", "nicht", "die", "Wes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Er w\u00fcnscht sich zu sterben.", "tokens": ["Er", "w\u00fcnscht", "sich", "zu", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Seht, ", "tokens": ["Seht", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.10": {"text": "Kan klagen und weinen!", "tokens": ["Kan", "kla\u00b7gen", "und", "wei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "KON", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Wie ringt er die H\u00e4nde,", "tokens": ["Wie", "ringt", "er", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Am Ufer der Leine!", "tokens": ["Am", "U\u00b7fer", "der", "Lei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Seht ", "tokens": ["Seht"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.14": {"text": "Was weinen die M\u00e4nner?", "tokens": ["Was", "wei\u00b7nen", "die", "M\u00e4n\u00b7ner", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Sie seufzen, sie weinen,", "tokens": ["Sie", "seuf\u00b7zen", ",", "sie", "wei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "Um w\u00fcrdige Damen,", "tokens": ["Um", "w\u00fcr\u00b7di\u00b7ge", "Da\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Um trefliche Sch\u00f6nen.", "tokens": ["Um", "tref\u00b7li\u00b7che", "Sch\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.18": {"text": "Ach lebt doch nur ewig,", "tokens": ["Ach", "lebt", "doch", "nur", "e\u00b7wig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.19": {"text": "Ihr trefliche Sch\u00f6nen!", "tokens": ["Ihr", "tref\u00b7li\u00b7che", "Sch\u00f6\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.20": {"text": "Ach la\u00dft euch, ihr Damen,", "tokens": ["Ach", "la\u00dft", "euch", ",", "ihr", "Da\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.21": {"text": "Vom Tode nicht holen;", "tokens": ["Vom", "To\u00b7de", "nicht", "ho\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.22": {"text": "Sonst weinen die M\u00e4nner.", "tokens": ["Sonst", "wei\u00b7nen", "die", "M\u00e4n\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}