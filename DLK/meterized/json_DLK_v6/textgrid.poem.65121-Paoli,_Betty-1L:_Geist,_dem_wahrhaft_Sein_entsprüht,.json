{"textgrid.poem.65121": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Geist, dem wahrhaft Sein entspr\u00fcht,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geist, dem wahrhaft Sein entspr\u00fcht,", "tokens": ["Geist", ",", "dem", "wahr\u00b7haft", "Sein", "ent\u00b7spr\u00fcht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "PPOSAT", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Senk' dich auf mich nieder! Hauche", "tokens": ["Senk'", "dich", "auf", "mich", "nie\u00b7der", "!", "Hau\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "PTKVZ", "$.", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ins zerfahrene Gem\u00fcth", "tokens": ["Ins", "zer\u00b7fah\u00b7re\u00b7ne", "Ge\u00b7m\u00fcth"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir die St\u00e4rke, die ich brauche!", "tokens": ["Mir", "die", "St\u00e4r\u00b7ke", ",", "die", "ich", "brau\u00b7che", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nimm den Flor der Sinnenwelt", "tokens": ["Nimm", "den", "Flor", "der", "Sin\u00b7nen\u00b7welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fort von der verst\u00f6rten Seele,", "tokens": ["Fort", "von", "der", "ver\u00b7st\u00f6r\u00b7ten", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df, von deinem Licht erhellt,", "tokens": ["Da\u00df", ",", "von", "dei\u00b7nem", "Licht", "er\u00b7hellt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie des Weges nicht verfehle!", "tokens": ["Sie", "des", "We\u00b7ges", "nicht", "ver\u00b7feh\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und vertilg' die letzte Spur", "tokens": ["Und", "ver\u00b7til\u00b7g'", "die", "letz\u00b7te", "Spur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Jenes Bild's, um das vermessen", "tokens": ["Je\u00b7nes", "Bild's", ",", "um", "das", "ver\u00b7mes\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "KOUI", "PDS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die beth\u00f6rte Kreatur", "tokens": ["Die", "be\u00b7th\u00f6r\u00b7te", "Kre\u00b7a\u00b7tur"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dein und ihrer selbst vergessen.", "tokens": ["Dein", "und", "ih\u00b7rer", "selbst", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "KON", "PPOSAT", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dieses Lebens bangen Streit", "tokens": ["Die\u00b7ses", "Le\u00b7bens", "ban\u00b7gen", "Streit"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wollte k\u00fchn mein Herz vers\u00f6hnen", "tokens": ["Woll\u00b7te", "k\u00fchn", "mein", "Herz", "ver\u00b7s\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADJD", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es suchte Ewigkeit", "tokens": ["Und", "es", "such\u00b7te", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bei des Staubes armen S\u00f6hnen.", "tokens": ["Bei", "des", "Stau\u00b7bes", "ar\u00b7men", "S\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Aufgeschreckt aus meinem Traum'", "tokens": ["Auf\u00b7ge\u00b7schreckt", "aus", "mei\u00b7nem", "Traum'"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sah ich, da\u00df, was ich gefordert,", "tokens": ["Sah", "ich", ",", "da\u00df", ",", "was", "ich", "ge\u00b7for\u00b7dert", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Selbst als leise Ahnung kaum", "tokens": ["Selbst", "als", "lei\u00b7se", "Ah\u00b7nung", "kaum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ADJA", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jemals ihre Brust durchlodert.", "tokens": ["Je\u00b7mals", "ih\u00b7re", "Brust", "durch\u00b7lo\u00b7dert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ihres eignen Wesens Bann", "tokens": ["Ih\u00b7res", "eig\u00b7nen", "We\u00b7sens", "Bann"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00f6gen sie sich duldsam f\u00fcgen!", "tokens": ["M\u00f6\u00b7gen", "sie", "sich", "duld\u00b7sam", "f\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber was verg\u00e4nglich, kann", "tokens": ["A\u00b7ber", "was", "ver\u00b7g\u00e4ng\u00b7lich", ",", "kann"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KON", "PWS", "ADJD", "$,", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun und nimmer mir gen\u00fcgen.", "tokens": ["Nun", "und", "nim\u00b7mer", "mir", "ge\u00b7n\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ein Geheimni\u00df will der Schmerz", "tokens": ["Ein", "Ge\u00b7heim\u00b7ni\u00df", "will", "der", "Schmerz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir prophetenhaft enth\u00fcllen:", "tokens": ["Mir", "pro\u00b7phe\u00b7ten\u00b7haft", "ent\u00b7h\u00fcl\u00b7len", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieses schrankenlose Herz", "tokens": ["Die\u00b7ses", "schran\u00b7ken\u00b7lo\u00b7se", "Herz"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kann nur Gott allein erf\u00fcllen!", "tokens": ["Kann", "nur", "Gott", "al\u00b7lein", "er\u00b7f\u00fcl\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}