{"textgrid.poem.58613": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "45.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Harre nur, bedr\u00fcckte Seel', die du in dir selbst gefangen,", "tokens": ["Har\u00b7re", "nur", ",", "be\u00b7dr\u00fcck\u00b7te", "Seel'", ",", "die", "du", "in", "dir", "selbst", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "ADJA", "NN", "$,", "PRELS", "PPER", "APPR", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Deren Geist zur Freiheit nicht, wie er wollte, kann gelangen,", "tokens": ["De\u00b7ren", "Geist", "zur", "Frei\u00b7heit", "nicht", ",", "wie", "er", "woll\u00b7te", ",", "kann", "ge\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "NN", "APPRART", "NN", "PTKNEG", "$,", "PWAV", "PPER", "VMFIN", "$,", "VMFIN", "VVINF", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "H\u00f6re, dein Erl\u00f6ser kommt, nach viel tausend Ach und O", "tokens": ["H\u00f6\u00b7re", ",", "dein", "Er\u00b7l\u00f6\u00b7ser", "kommt", ",", "nach", "viel", "tau\u00b7send", "Ach", "und", "O"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPOSAT", "NN", "VVFIN", "$,", "APPR", "PIAT", "CARD", "NN", "KON", "NE"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Wirst du endlich sein erl\u00f6st, wirst du endlich werden froh!", "tokens": ["Wirst", "du", "end\u00b7lich", "sein", "er\u00b7l\u00f6st", ",", "wirst", "du", "end\u00b7lich", "wer\u00b7den", "froh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VAINF", "VVFIN", "$,", "VAFIN", "PPER", "ADV", "VAFIN", "ADJD", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Dann wird sich dein muntrer Geist frei in seinen Ursprung kehren", "tokens": ["Dann", "wird", "sich", "dein", "mun\u00b7trer", "Geist", "frei", "in", "sei\u00b7nen", "Ur\u00b7sprung", "keh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PRF", "PPOSAT", "ADJA", "NN", "ADJD", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+--+-++-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Und mit Jauchzen deinen Gott in ihm selbst in Zion ehren,", "tokens": ["Und", "mit", "Jauch\u00b7zen", "dei\u00b7nen", "Gott", "in", "ihm", "selbst", "in", "Zi\u00b7on", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PPOSAT", "NN", "APPR", "PPER", "ADV", "APPR", "NE", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "S\u00fc\u00dfe Wonne kr\u00f6nt dein Haupt, Freud' und Leben ohne End',", "tokens": ["S\u00fc\u00b7\u00dfe", "Won\u00b7ne", "kr\u00f6nt", "dein", "Haupt", ",", "Freud'", "und", "Le\u00b7ben", "oh\u00b7ne", "End'", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-++-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.8": {"text": "Alles Seufzen fliehet weg von dem stillen Element.", "tokens": ["Al\u00b7les", "Seuf\u00b7zen", "flie\u00b7het", "weg", "von", "dem", "stil\u00b7len", "E\u00b7le\u00b7ment", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}}}}}