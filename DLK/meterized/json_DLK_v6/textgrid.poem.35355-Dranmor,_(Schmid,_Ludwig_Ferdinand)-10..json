{"textgrid.poem.35355": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Dichters Leier m\u00f6gt ihr gern vergleichen", "tokens": ["Des", "Dich\u00b7ters", "Lei\u00b7er", "m\u00f6gt", "ihr", "gern", "ver\u00b7glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Aeols-Harfe, von des Zephyrs Fl\u00fcstern", "tokens": ["Der", "A\u00b7eol\u00b7sHa\u00b7rfe", ",", "von", "des", "Ze\u00b7phyrs", "Fl\u00fcs\u00b7tern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Geliebkost, die von sehnsuchtsvollen, weichen", "tokens": ["Ge\u00b7lieb\u00b7kost", ",", "die", "von", "sehn\u00b7suchts\u00b7vol\u00b7len", ",", "wei\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "PRELS", "APPR", "VVINF", "$,", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Accorden sich versteigt zu wilden, d\u00fcstern,", "tokens": ["Ac\u00b7cor\u00b7den", "sich", "ver\u00b7steigt", "zu", "wil\u00b7den", ",", "d\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PRF", "VVPP", "PTKZU", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Zu Friedhofsklagen bei dem Gru\u00df des Windes", "tokens": ["Zu", "Fried\u00b7hofs\u00b7kla\u00b7gen", "bei", "dem", "Gru\u00df", "des", "Win\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und bei dem lautern Murmeln eines Baches. \u2013", "tokens": ["Und", "bei", "dem", "lau\u00b7tern", "Mur\u00b7meln", "ei\u00b7nes", "Ba\u00b7ches", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Gleich solcher Harfe ist des Sonntagskindes,", "tokens": ["Gleich", "sol\u00b7cher", "Har\u00b7fe", "ist", "des", "Sonn\u00b7tags\u00b7kin\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Des Dichterlinges Herz ein eitles, schwaches.", "tokens": ["Des", "Dich\u00b7ter\u00b7lin\u00b7ges", "Herz", "ein", "eit\u00b7les", ",", "schwa\u00b7ches", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "$,", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Doch Saiten, die mit brausenden Accorden", "tokens": ["Doch", "Sai\u00b7ten", ",", "die", "mit", "brau\u00b7sen\u00b7den", "Ac\u00b7cor\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "In wunden M\u00e4nnerherzen wiederklingen,", "tokens": ["In", "wun\u00b7den", "M\u00e4n\u00b7ner\u00b7her\u00b7zen", "wie\u00b7der\u00b7klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Die selbst in Wintersst\u00fcrmen nicht zerspringen,", "tokens": ["Die", "selbst", "in", "Win\u00b7terss\u00b7t\u00fcr\u00b7men", "nicht", "zer\u00b7sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Sind nicht an Aeols-Harfen stark geworden.", "tokens": ["Sind", "nicht", "an", "A\u00b7eol\u00b7sHa\u00b7rfen", "stark", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPR", "NN", "ADJD", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}