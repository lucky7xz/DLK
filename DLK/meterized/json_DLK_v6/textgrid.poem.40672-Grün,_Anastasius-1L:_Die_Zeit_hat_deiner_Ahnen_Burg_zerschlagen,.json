{"textgrid.poem.40672": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Zeit hat deiner Ahnen Burg zerschlagen,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Zeit hat deiner Ahnen Burg zerschlagen,", "tokens": ["Die", "Zeit", "hat", "dei\u00b7ner", "Ah\u00b7nen", "Burg", "zer\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein prunkend Pergament verzehrt in Br\u00e4nden;", "tokens": ["Dein", "prun\u00b7kend", "Per\u00b7ga\u00b7ment", "ver\u00b7zehrt", "in", "Br\u00e4n\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was dir an Flittern blieb, wen soll's noch blenden?", "tokens": ["Was", "dir", "an", "Flit\u00b7tern", "blieb", ",", "wen", "soll's", "noch", "blen\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "VVFIN", "$,", "PWS", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Rest, nicht werth, des Volkes Ha\u00df zu tragen! \u2013", "tokens": ["Ein", "Rest", ",", "nicht", "werth", ",", "des", "Vol\u00b7kes", "Ha\u00df", "zu", "tra\u00b7gen", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "PTKNEG", "ADJD", "$,", "ART", "NN", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Lord Spenser selig lie\u00df im Kirchthurmjagen", "tokens": ["Lord", "Spen\u00b7ser", "se\u00b7lig", "lie\u00df", "im", "Kirch\u00b7thurm\u00b7ja\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "ADJD", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Frackes ", "tokens": ["Des", "Fra\u00b7ckes"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Der andre trauert' einsam an den Lenden,", "tokens": ["Der", "and\u00b7re", "trau\u00b7ert'", "ein\u00b7sam", "an", "den", "Len\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als s\u00e4h Orest um Pylades man klagen.", "tokens": ["Als", "s\u00e4h", "O\u00b7rest", "um", "Py\u00b7la\u00b7des", "man", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NE", "APPR", "NE", "PIS", "VVINF", "$."], "meter": "--++-+-+-+-", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Seltsam Kost\u00fcm dem Spotte der Genossen!", "tokens": ["Selt\u00b7sam", "Kos\u00b7t\u00fcm", "dem", "Spot\u00b7te", "der", "Ge\u00b7nos\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Der Lord, eingehend in des Dornstrauchs Possen,", "tokens": ["Der", "Lord", ",", "ein\u00b7ge\u00b7hend", "in", "des", "Dorn\u00b7strauchs", "Pos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Rei\u00dft flink den zweiten Fl\u00fcgel von den Weichen.", "tokens": ["Rei\u00dft", "flink", "den", "zwei\u00b7ten", "Fl\u00fc\u00b7gel", "von", "den", "Wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sein Name schallt volksth\u00fcmlich drum mit Lobe,", "tokens": ["Sein", "Na\u00b7me", "schallt", "volkst\u00b7h\u00fcm\u00b7lich", "drum", "mit", "Lo\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "PAV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein neu Gewand bereichert die Gard'robe,", "tokens": ["Ein", "neu", "Ge\u00b7wand", "be\u00b7rei\u00b7chert", "die", "Gard'\u00b7ro\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Drin steckt f\u00fcr dich ein Zettel: \u00bbThu' desgleichen!\u00ab", "tokens": ["Drin", "steckt", "f\u00fcr", "dich", "ein", "Zet\u00b7tel", ":", "\u00bb", "Thu'", "des\u00b7glei\u00b7chen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "NN", "$.", "$(", "VVIMP", "ADV", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}