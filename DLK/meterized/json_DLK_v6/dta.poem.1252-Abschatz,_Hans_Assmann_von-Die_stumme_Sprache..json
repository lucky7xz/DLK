{"dta.poem.1252": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die stumme Sprache.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wie k\u00f6nnen doch in einem Hertzen", "tokens": ["Wie", "k\u00f6n\u00b7nen", "doch", "in", "ei\u00b7nem", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Lieb und Furcht Geferten seyn?", "tokens": ["Die", "Lieb", "und", "Furcht", "Ge\u00b7fer\u00b7ten", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie kan sich Freude neben Schmertzen", "tokens": ["Wie", "kan", "sich", "Freu\u00b7de", "ne\u00b7ben", "Schmert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PRF", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Lust bey Unlust finden ein?", "tokens": ["Und", "Lust", "bey", "Un\u00b7lust", "fin\u00b7den", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie kan sich plagen und vergn\u00fcgen", "tokens": ["Wie", "kan", "sich", "pla\u00b7gen", "und", "ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PRF", "VVFIN", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "An einen Ort zusammen f\u00fcgen?", "tokens": ["An", "ei\u00b7nen", "Ort", "zu\u00b7sam\u00b7men", "f\u00fc\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer liebet/ wei\u00df hiervon zu sagen:", "tokens": ["Wer", "lie\u00b7bet", "/", "wei\u00df", "hier\u00b7von", "zu", "sa\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$(", "VVFIN", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er redet/ wenn er stille schweigt:", "tokens": ["Er", "re\u00b7det", "/", "wenn", "er", "stil\u00b7le", "schweigt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man darff nicht von dem Munde fragen/", "tokens": ["Man", "darff", "nicht", "von", "dem", "Mun\u00b7de", "fra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was seiner Augen Feuer zeigt.", "tokens": ["Was", "sei\u00b7ner", "Au\u00b7gen", "Feu\u00b7er", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein stiller Seuffzer bricht f\u00fcr Worte", "tokens": ["Ein", "stil\u00b7ler", "Seuff\u00b7zer", "bricht", "f\u00fcr", "Wor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Durch fest-gesperrter Lippen Pforte.", "tokens": ["Durch", "fest\u00b7ge\u00b7sperr\u00b7ter", "Lip\u00b7pen", "Pfor\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er suchet Silvien mit Freuden/", "tokens": ["Er", "su\u00b7chet", "Sil\u00b7vi\u00b7en", "mit", "Freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und findet bey ihr seine Pein.", "tokens": ["Und", "fin\u00b7det", "bey", "ihr", "sei\u00b7ne", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn sich die Augen an ihr weyden/", "tokens": ["Wenn", "sich", "die", "Au\u00b7gen", "an", "ihr", "wey\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So schmacht das Hertz in Flammen ein.", "tokens": ["So", "schmacht", "das", "Hertz", "in", "Flam\u00b7men", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von ihrer s\u00fcssen Augen Blitze", "tokens": ["Von", "ih\u00b7rer", "s\u00fcs\u00b7sen", "Au\u00b7gen", "Blit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Empfindt sein Hertze Frost und Hitze.", "tokens": ["Emp\u00b7findt", "sein", "Hert\u00b7ze", "Frost", "und", "Hit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAINF", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Man kan auff seinen Wangen lesen/", "tokens": ["Man", "kan", "auff", "sei\u00b7nen", "Wan\u00b7gen", "le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was Amor ihm ins Hertze pr\u00e4gt.", "tokens": ["Was", "A\u00b7mor", "ihm", "ins", "Hert\u00b7ze", "pr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "PPER", "APPRART", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im fall er anders soll genesen/", "tokens": ["Im", "fall", "er", "an\u00b7ders", "soll", "ge\u00b7ne\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df Silvia dadurch bewegt", "tokens": ["Mu\u00df", "Sil\u00b7via", "da\u00b7durch", "be\u00b7wegt"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "NE", "PAV", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ihm k\u00fcssend auff die Lippen schreiben/", "tokens": ["Ihm", "k\u00fcs\u00b7send", "auff", "die", "Lip\u00b7pen", "schrei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich will Silvanders eigen bleiben.", "tokens": ["Ich", "will", "Sil\u00b7van\u00b7ders", "ei\u00b7gen", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NE", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}