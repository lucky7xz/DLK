{"textgrid.poem.56234": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1823, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl wandert ich aus in trauriger Stund,", "tokens": ["Wohl", "wan\u00b7dert", "ich", "aus", "in", "trau\u00b7ri\u00b7ger", "Stund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es weinte die Liebe so sehr.", "tokens": ["Es", "wein\u00b7te", "die", "Lie\u00b7be", "so", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Der Fu\u00df ist mir lahm, die Schulter mir wund,", "tokens": ["Der", "Fu\u00df", "ist", "mir", "lahm", ",", "die", "Schul\u00b7ter", "mir", "wund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das Herz, das ist mir so schwer.", "tokens": ["Das", "Herz", ",", "das", "ist", "mir", "so", "schwer", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Was singt ihr, ihr V\u00f6gel, im Morgenlicht?", "tokens": ["Was", "singt", "ihr", ",", "ihr", "V\u00f6\u00b7gel", ",", "im", "Mor\u00b7gen\u00b7licht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ihr wi\u00dft nicht, wie scheiden tut!", "tokens": ["Ihr", "wi\u00dft", "nicht", ",", "wie", "schei\u00b7den", "tut", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Es dr\u00fccken euch Sorgen und Schuhe nicht;", "tokens": ["Es", "dr\u00fc\u00b7cken", "euch", "Sor\u00b7gen", "und", "Schu\u00b7he", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN", "PTKNEG", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ihr V\u00f6gel, ihr habt es gut!", "tokens": ["Ihr", "V\u00f6\u00b7gel", ",", "ihr", "habt", "es", "gut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}