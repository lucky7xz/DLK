{"dta.poem.9471": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Auff den Seeligen Dintritt D. Fran-  \n tzen von Trotha/ F\u00fcrstl. Eysenach. Geheimten  \n Raths- und Oberhauptmanns des Go-  \n tischen Kreyses etc.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr armen Sterblichen/ wolt jhr noch l\u00e4nger leben?", "tokens": ["Ihr", "ar\u00b7men", "Sterb\u00b7li\u00b7chen", "/", "wolt", "jhr", "noch", "l\u00e4n\u00b7ger", "le\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was habt jhr doch nur hier/", "tokens": ["Was", "habt", "jhr", "doch", "nur", "hier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "ADV", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als Wehmuth f\u00fcr und f\u00fcr?", "tokens": ["Als", "Weh\u00b7muth", "f\u00fcr", "und", "f\u00fcr", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "KON", "APPR", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ach seht Jhr nicht den Todt vor euren augen schwe-", "tokens": ["Ach", "seht", "Ihr", "nicht", "den", "Todt", "vor", "eu\u00b7ren", "au\u00b7gen", "schwe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Lieber lieber wolt ich dort ", "tokens": ["Lie\u00b7ber", "lie\u00b7ber", "wolt", "ich", "dort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "VMFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "In dem rechten Friede schweben/", "tokens": ["In", "dem", "rech\u00b7ten", "Frie\u00b7de", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Als hirunten fort f\u00fcr fort", "tokens": ["Als", "hir\u00b7un\u00b7ten", "fort", "f\u00fcr", "fort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PTKVZ", "APPR", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Seyn mit Krieges-noth \u00fcmbgeben.", "tokens": ["Seyn", "mit", "Krie\u00b7ge\u00b7snoth", "\u00fcmb\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie wandelbahr der Mond wie wandelbahr die ster-", "tokens": ["Wie", "wan\u00b7del\u00b7bahr", "der", "Mond", "wie", "wan\u00b7del\u00b7bahr", "die", "ster"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "KOKOM", "ADV", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ist der Menschen zeit ", "tokens": ["So", "ist", "der", "Men\u00b7schen", "zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gantz voll verg\u00e4ngligkeit/", "tokens": ["Gantz", "voll", "ver\u00b7g\u00e4n\u00b7glig\u00b7keit", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Bald nimmet zu bald ab die gro\u00dfe Nacht-Laterne/", "tokens": ["Bald", "nim\u00b7met", "zu", "bald", "ab", "die", "gro\u00b7\u00dfe", "Nacht\u00b7La\u00b7ter\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Balde seyn wir frisch und roht/", "tokens": ["Bal\u00b7de", "seyn", "wir", "frisch", "und", "roht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "H\u00fcbsch und lieblich von Gesichte/", "tokens": ["H\u00fcbsch", "und", "lieb\u00b7lich", "von", "Ge\u00b7sich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "APPR", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Balde seyn wir kranck und todt/", "tokens": ["Bal\u00b7de", "seyn", "wir", "kranck", "und", "todt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Da der Leib wird gar zu nichte.", "tokens": ["Da", "der", "Leib", "wird", "gar", "zu", "nich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "ADV", "PTKA", "PIS", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Wer wolte denn nicht nun mit lust von hinnen schei-", "tokens": ["Wer", "wol\u00b7te", "denn", "nicht", "nun", "mit", "lust", "von", "hin\u00b7nen", "schei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ADV", "PTKNEG", "ADV", "APPR", "NN", "APPR", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gesegnen diese Welt ", "tokens": ["Ge\u00b7seg\u00b7nen", "die\u00b7se", "Welt"], "token_info": ["word", "word", "word"], "pos": ["NN", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das w\u00fcste Trauer-feld/", "tokens": ["Das", "w\u00fcs\u00b7te", "Trau\u00b7e\u00b7rfeld", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In dem der Himmel ist gantz voller Lust und Freuden/", "tokens": ["In", "dem", "der", "Him\u00b7mel", "ist", "gantz", "vol\u00b7ler", "Lust", "und", "Freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VAFIN", "ADV", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da die Auser wehlten seyn/", "tokens": ["Da", "die", "Au\u00b7ser", "wehl\u00b7ten", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da die Seraphinen singen", "tokens": ["Da", "die", "Se\u00b7ra\u00b7phi\u00b7nen", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "und die Eherubinen drein", "tokens": ["und", "die", "E\u00b7he\u00b7ru\u00b7bi\u00b7nen", "drein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PTKVZ"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Jhre Stimmen hocherschwingen?", "tokens": ["Ih\u00b7re", "Stim\u00b7men", "ho\u00b7cher\u00b7schwin\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich bilde mir schon ein/ als wenn ich k\u00f6nte h\u00f6ren", "tokens": ["Ich", "bil\u00b7de", "mir", "schon", "ein", "/", "als", "wenn", "ich", "k\u00f6n\u00b7te", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "$(", "KOKOM", "KOUS", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den David auf der Harf", "tokens": ["Den", "Da\u00b7vid", "auf", "der", "Harf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Seiten schlagen scharf/", "tokens": ["Die", "Sei\u00b7ten", "schla\u00b7gen", "scharf", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ach wolte wolte Gott! da\u00df wir daselbsten weren/", "tokens": ["Ach", "wol\u00b7te", "wol\u00b7te", "Gott", "!", "da\u00df", "wir", "da\u00b7selbs\u00b7ten", "we\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "VMFIN", "NN", "$.", "KOUS", "PPER", "PDS", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "und den sch\u00f6nen Himmels-Saal/", "tokens": ["und", "den", "sch\u00f6\u00b7nen", "Him\u00b7mels\u00b7Saal", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Der von T\u00fcrckis und Rubinen/", "tokens": ["Der", "von", "T\u00fcr\u00b7ckis", "und", "Ru\u00b7bi\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "sam\u0303t der blancken Sterne zahl/", "tokens": ["sam\u0303t", "der", "blan\u00b7cken", "Ster\u00b7ne", "zahl", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Schauten mit den Seraphinen!", "tokens": ["Schau\u00b7ten", "mit", "den", "Se\u00b7ra\u00b7phi\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Dis alles kann nun auch jtzt der von Trotha h\u00f6ren/", "tokens": ["Dis", "al\u00b7les", "kann", "nun", "auch", "jtzt", "der", "von", "Tro\u00b7tha", "h\u00f6\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VMFIN", "ADV", "ADV", "ADV", "ART", "APPR", "NE", "VVINF", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Weil sich nun von uns reist", "tokens": ["Weil", "sich", "nun", "von", "uns", "reist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ADV", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein Adelicher Geist/", "tokens": ["Sein", "A\u00b7de\u00b7li\u00b7cher", "Geist", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Engel werden jhn mit Palmen sch\u00f6n verehren/", "tokens": ["Die", "En\u00b7gel", "wer\u00b7den", "jhn", "mit", "Pal\u00b7men", "sch\u00f6n", "ver\u00b7eh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er empfindet vor die pein/", "tokens": ["Er", "emp\u00b7fin\u00b7det", "vor", "die", "pein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Lauter Freude/ lauter Wonne:", "tokens": ["Lau\u00b7ter", "Freu\u00b7de", "/", "lau\u00b7ter", "Won\u00b7ne", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Hilff Gott! da\u00df wir balde seyn", "tokens": ["Hilff", "Gott", "!", "da\u00df", "wir", "bal\u00b7de", "seyn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "KOUS", "PPER", "ADV", "VAINF"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Bey der rechten Lebens-Sonne!", "tokens": ["Bey", "der", "rech\u00b7ten", "Le\u00b7bens\u00b7Son\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}