{"textgrid.poem.28243": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nicht recht! nicht recht! w\u00fcrd immer schreyn", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht recht! nicht recht! w\u00fcrd immer schreyn", "tokens": ["Nicht", "recht", "!", "nicht", "recht", "!", "w\u00fcrd", "im\u00b7mer", "schreyn"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "$.", "PTKNEG", "ADJD", "$.", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Johannes, solt er wieder seyn;", "tokens": ["Jo\u00b7han\u00b7nes", ",", "solt", "er", "wie\u00b7der", "seyn", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VMFIN", "PPER", "ADV", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch k\u00e4m er, rieth ich, da\u00df er d\u00e4chte,", "tokens": ["Doch", "k\u00e4m", "er", ",", "rieth", "ich", ",", "da\u00df", "er", "d\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie viel er K\u00f6pff in Vorrath br\u00e4chte.", "tokens": ["Wie", "viel", "er", "K\u00f6pff", "in", "Vor\u00b7rath", "br\u00e4ch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}