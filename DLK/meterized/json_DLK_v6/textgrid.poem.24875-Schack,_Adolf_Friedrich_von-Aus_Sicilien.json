{"textgrid.poem.24875": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Aus Sicilien", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier am Berghang wollen wir ruhn,", "tokens": ["Hier", "am", "Berg\u00b7hang", "wol\u00b7len", "wir", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Uns an der Quelle zu laben!", "tokens": ["Uns", "an", "der", "Quel\u00b7le", "zu", "la\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Unter Myrten und Rosen nun", "tokens": ["Un\u00b7ter", "Myr\u00b7ten", "und", "Ro\u00b7sen", "nun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "ADV"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Werde die Sorge begraben!", "tokens": ["Wer\u00b7de", "die", "Sor\u00b7ge", "be\u00b7gra\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Schwer sind dem Wind von des Hirtenrohrs", "tokens": ["Schwer", "sind", "dem", "Wind", "von", "des", "Hir\u00b7ten\u00b7rohrs"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Sterbenden T\u00f6nen die Schwingen,", "tokens": ["Ster\u00b7ben\u00b7den", "T\u00f6\u00b7nen", "die", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "W\u00e4hrend im Laub des Cikadenchors", "tokens": ["W\u00e4h\u00b7rend", "im", "Laub", "des", "Ci\u00b7ka\u00b7denc\u00b7hors"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Schmetternde Stimmen verklingen.", "tokens": ["Schmet\u00b7tern\u00b7de", "Stim\u00b7men", "ver\u00b7klin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Sanft gewiegt von dem s\u00e4uselnden Hauch", "tokens": ["Sanft", "ge\u00b7wiegt", "von", "dem", "s\u00e4u\u00b7seln\u00b7den", "Hauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Zeigt und verh\u00fcllt mit dem Wipfel", "tokens": ["Zeigt", "und", "ver\u00b7h\u00fcllt", "mit", "dem", "Wip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Die Cypresse den wallenden Rauch", "tokens": ["Die", "Cyp\u00b7res\u00b7se", "den", "wal\u00b7len\u00b7den", "Rauch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ueber dem Aetnagipfel.", "tokens": ["Ue\u00b7ber", "dem", "A\u00b7et\u00b7na\u00b7gip\u00b7fel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Schlummer, komm und entf\u00fchre still", "tokens": ["Schlum\u00b7mer", ",", "komm", "und", "ent\u00b7f\u00fch\u00b7re", "still"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "KON", "VVFIN", "PTKVZ"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Uns in die duftenden Weiten,", "tokens": ["Uns", "in", "die", "duf\u00b7ten\u00b7den", "Wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wo durch die Wiesen von Asphodill", "tokens": ["Wo", "durch", "die", "Wie\u00b7sen", "von", "As\u00b7pho\u00b7dill"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "APPR", "NE"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Selige Schatten gleiten!", "tokens": ["Se\u00b7li\u00b7ge", "Schat\u00b7ten", "glei\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}