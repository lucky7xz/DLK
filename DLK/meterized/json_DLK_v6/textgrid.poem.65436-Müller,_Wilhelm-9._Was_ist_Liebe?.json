{"textgrid.poem.65436": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "9. Was ist Liebe?", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.71", "af:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frage, was die Liebe sei,", "tokens": ["Fra\u00b7ge", ",", "was", "die", "Lie\u00b7be", "sei", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Frage den, der liebefrei,", "tokens": ["Fra\u00b7ge", "den", ",", "der", "lie\u00b7be\u00b7frei", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "$,", "PRELS", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Frag' ihn, den die Liebe kost,", "tokens": ["Frag'", "ihn", ",", "den", "die", "Lie\u00b7be", "kost", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Frag' ihn, den die Lieb' erbo\u00dft,", "tokens": ["Frag'", "ihn", ",", "den", "die", "Lieb'", "er\u00b7bo\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lieb' und frage deine Brust \u2013", "tokens": ["Lieb'", "und", "fra\u00b7ge", "dei\u00b7ne", "Brust", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hat's ein Andrer recht gewu\u00dft?", "tokens": ["Hat's", "ein", "A\u00b7ndrer", "recht", "ge\u00b7wu\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}