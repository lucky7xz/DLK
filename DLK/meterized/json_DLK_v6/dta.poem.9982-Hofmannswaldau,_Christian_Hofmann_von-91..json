{"dta.poem.9982": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "91.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich mach da\u00df die fr\u00fcchte wachsen/ un\u0303 der mensch sein leben hat/", "tokens": ["Ich", "mach", "da\u00df", "die", "fr\u00fcch\u00b7te", "wach\u00b7sen", "/", "u\u00f1", "der", "mensch", "sein", "le\u00b7ben", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "ART", "ADJA", "VVINF", "$(", "KON", "ART", "NN", "PPOSAT", "VVINF", "VAFIN", "$("], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Es wird durch mich ausgerichtet manche b\u00f6\u00df und gute that:", "tokens": ["Es", "wird", "durch", "mich", "aus\u00b7ge\u00b7rich\u00b7tet", "man\u00b7che", "b\u00f6\u00df", "und", "gu\u00b7te", "that", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVFIN", "PIAT", "NN", "KON", "ADJA", "VVFIN", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Tod und leben kan ich geben/ bringe sachen auff die bahn/", "tokens": ["Tod", "und", "le\u00b7ben", "kan", "ich", "ge\u00b7ben", "/", "brin\u00b7ge", "sa\u00b7chen", "auff", "die", "bahn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "VMFIN", "PPER", "VVINF", "$(", "VVFIN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Derer man in allen st\u00e4nden n\u00fctzlich sich gebrauchen kan.", "tokens": ["De\u00b7rer", "man", "in", "al\u00b7len", "st\u00e4n\u00b7den", "n\u00fctz\u00b7lich", "sich", "ge\u00b7brau\u00b7chen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "APPR", "PIAT", "ADJA", "ADJD", "PRF", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}