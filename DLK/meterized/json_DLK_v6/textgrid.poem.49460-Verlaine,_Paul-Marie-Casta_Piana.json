{"textgrid.poem.49460": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "Casta Piana", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein schwarzes, rotdurchflimmertes Haar,", "tokens": ["Dein", "schwar\u00b7zes", ",", "rot\u00b7durch\u00b7flim\u00b7mer\u00b7tes", "Haar", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Dein kaltes und s\u00fcsses Augenpaar,", "tokens": ["Dein", "kal\u00b7tes", "und", "s\u00fcs\u00b7ses", "Au\u00b7gen\u00b7paar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Deine Sch\u00f6nheit, die eigentlich keine,", "tokens": ["Dei\u00b7ne", "Sch\u00f6n\u00b7heit", ",", "die", "ei\u00b7gent\u00b7lich", "kei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADV", "PIAT", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Deine Br\u00fcste, geformt vom Teufel, o du!", "tokens": ["Dei\u00b7ne", "Br\u00fcs\u00b7te", ",", "ge\u00b7formt", "vom", "Teu\u00b7fel", ",", "o", "du", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVPP", "APPRART", "NN", "$,", "FM", "PPER", "$."], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Und deine feine Bl\u00e4sse dazu,", "tokens": ["Und", "dei\u00b7ne", "fei\u00b7ne", "Bl\u00e4s\u00b7se", "da\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "PAV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Gestohlen dem Mondenscheine,", "tokens": ["Ge\u00b7stoh\u00b7len", "dem", "Mon\u00b7den\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie halten gefangen mich Nacht und Tag,", "tokens": ["Sie", "hal\u00b7ten", "ge\u00b7fan\u00b7gen", "mich", "Nacht", "und", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Du heilige Jungfrau vom Dachverschlag,", "tokens": ["Du", "hei\u00b7li\u00b7ge", "Jung\u00b7frau", "vom", "Dach\u00b7ver\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Die mit nicht geweihten Kerzen,", "tokens": ["Die", "mit", "nicht", "ge\u00b7weih\u00b7ten", "Ker\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PTKNEG", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit heidnischen Aves man ehren muss,", "tokens": ["Mit", "heid\u00b7ni\u00b7schen", "A\u00b7ves", "man", "eh\u00b7ren", "muss", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NE", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mit Gebeten dazu nach dem Angelus,", "tokens": ["Mit", "Ge\u00b7be\u00b7ten", "da\u00b7zu", "nach", "dem", "An\u00b7ge\u00b7lus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PAV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Das uns ruft zu unheiligen Scherzen.", "tokens": ["Das", "uns", "ruft", "zu", "un\u00b7hei\u00b7li\u00b7gen", "Scher\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Man riecht den Scheiterhaufen dir an,", "tokens": ["Man", "riecht", "den", "Schei\u00b7ter\u00b7hau\u00b7fen", "dir", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Einen Lumpen machst du aus einem Mann,", "tokens": ["Ei\u00b7nen", "Lum\u00b7pen", "machst", "du", "aus", "ei\u00b7nem", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Einen Schemen, durch deine S\u00fcsse,", "tokens": ["Ei\u00b7nen", "Sche\u00b7men", ",", "durch", "dei\u00b7ne", "S\u00fcs\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "In der Zeit, wo ein Ja man stammeln mag,", "tokens": ["In", "der", "Zeit", ",", "wo", "ein", "Ja", "man", "stam\u00b7meln", "mag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "ART", "NN", "PIS", "VVINF", "VMFIN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "In der Zeit f\u00fcr ein lockendes: Guten Tag,", "tokens": ["In", "der", "Zeit", "f\u00fcr", "ein", "lo\u00b7cken\u00b7des", ":", "Gu\u00b7ten", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "ADJA", "$.", "ADJA", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.6": {"text": "In der Zeit dir zu k\u00fcssen die F\u00fcsse.", "tokens": ["In", "der", "Zeit", "dir", "zu", "k\u00fcs\u00b7sen", "die", "F\u00fcs\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "PTKZU", "VVINF", "ART", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.4": {"line.1": {"text": "Deine Dachkammer ist ein Schreckensort,", "tokens": ["Dei\u00b7ne", "Dach\u00b7kam\u00b7mer", "ist", "ein", "Schre\u00b7cken\u00b7sort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-++-+-+-+", "measure": "zehnsilber"}, "line.2": {"text": "Stets bist du auf deinem Lager dort", "tokens": ["Stets", "bist", "du", "auf", "dei\u00b7nem", "La\u00b7ger", "dort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "ADV"], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Und stellst manchem Schelm eine Falle,", "tokens": ["Und", "stellst", "man\u00b7chem", "Schelm", "ei\u00b7ne", "Fal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und wenn die Liebenden weitergehn", "tokens": ["Und", "wenn", "die", "Lie\u00b7ben\u00b7den", "wei\u00b7ter\u00b7gehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVINF"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Mit all' deinen Sakramenten versehn,", "tokens": ["Mit", "all'", "dei\u00b7nen", "Sak\u00b7ra\u00b7men\u00b7ten", "ver\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Dann lachst du \u00fcber sie alle.", "tokens": ["Dann", "lachst", "du", "\u00fc\u00b7ber", "sie", "al\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "PIS", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Recht hast du, doch liebst du sicherlich", "tokens": ["Recht", "hast", "du", ",", "doch", "liebst", "du", "si\u00b7cher\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "PRF"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Mehr als die Alten und Jungen mich,", "tokens": ["Mehr", "als", "die", "Al\u00b7ten", "und", "Jun\u00b7gen", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "NN", "KON", "NN", "PPER", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die dich nicht traktieren k\u00f6nnen ....", "tokens": ["Die", "dich", "nicht", "trak\u00b7tie\u00b7ren", "k\u00f6n\u00b7nen", "...."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich, der ich in deinen K\u00fcnsten gewandt,", "tokens": ["Mich", ",", "der", "ich", "in", "dei\u00b7nen", "K\u00fcns\u00b7ten", "ge\u00b7wandt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Mich, der gen\u00fcgend mit dir bekannt,", "tokens": ["Mich", ",", "der", "ge\u00b7n\u00fc\u00b7gend", "mit", "dir", "be\u00b7kannt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "VVPP", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Um dir jede Feier zu g\u00f6nnen.", "tokens": ["Um", "dir", "je\u00b7de", "Fei\u00b7er", "zu", "g\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Drum diese Falte fort geschwind,", "tokens": ["Drum", "die\u00b7se", "Fal\u00b7te", "fort", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PDAT", "NN", "PTKVZ", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und maule nicht mehr l\u00e4nger, Kind!", "tokens": ["Und", "mau\u00b7le", "nicht", "mehr", "l\u00e4n\u00b7ger", ",", "Kind", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "ADJD", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lass all deinen Balsam mich trinken,", "tokens": ["Lass", "all", "dei\u00b7nen", "Bal\u00b7sam", "mich", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ja, lass vers\u00fcsst, gesalzt und gew\u00fcrzt,", "tokens": ["Ja", ",", "lass", "ver\u00b7s\u00fcsst", ",", "ge\u00b7salzt", "und", "ge\u00b7w\u00fcrzt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "VVFIN", "$,", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Vers\u00fcsst, gepfeffert und unverk\u00fcrzt,", "tokens": ["Ver\u00b7s\u00fcsst", ",", "ge\u00b7pfef\u00b7fert", "und", "un\u00b7ver\u00b7k\u00fcrzt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Deinen heiligen Balsam mich trinken.", "tokens": ["Dei\u00b7nen", "hei\u00b7li\u00b7gen", "Bal\u00b7sam", "mich", "trin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}}}}