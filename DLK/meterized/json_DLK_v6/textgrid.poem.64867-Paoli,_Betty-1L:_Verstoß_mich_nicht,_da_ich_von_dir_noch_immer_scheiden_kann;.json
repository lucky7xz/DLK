{"textgrid.poem.64867": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Versto\u00df mich nicht, da ich von dir noch immer scheiden kann;", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Versto\u00df mich nicht, da ich von dir noch immer scheiden kann;", "tokens": ["Ver\u00b7sto\u00df", "mich", "nicht", ",", "da", "ich", "von", "dir", "noch", "im\u00b7mer", "schei\u00b7den", "kann", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "APPR", "PPER", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Weis' mich nicht an die Welt, an der ich mich nicht weiden kann.", "tokens": ["Weis'", "mich", "nicht", "an", "die", "Welt", ",", "an", "der", "ich", "mich", "nicht", "wei\u00b7den", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "APPR", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "PRF", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Viel theurer, als ihr bestes Gut, gilt meiner wunden Brust,", "tokens": ["Viel", "theu\u00b7rer", ",", "als", "ihr", "bes\u00b7tes", "Gut", ",", "gilt", "mei\u00b7ner", "wun\u00b7den", "Brust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Was sie an bittrem Schmerz und Weh um dich noch leiden kann.", "tokens": ["Was", "sie", "an", "bitt\u00b7rem", "Schmerz", "und", "Weh", "um", "dich", "noch", "lei\u00b7den", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ADJA", "NN", "KON", "NN", "APPR", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Ich ford're Wen'ges nur von dir: \u2013 da\u00df du mich dulden sollst!", "tokens": ["Ich", "ford'\u00b7re", "Wen'\u00b7ges", "nur", "von", "dir", ":", "\u2013", "da\u00df", "du", "mich", "dul\u00b7den", "sollst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADV", "APPR", "PPER", "$.", "$(", "KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Gew\u00e4hr' mir die\u00df, da ich damit mich froh bescheiden kann.", "tokens": ["Ge\u00b7w\u00e4hr'", "mir", "die\u00df", ",", "da", "ich", "da\u00b7mit", "mich", "froh", "be\u00b7schei\u00b7den", "kann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PDS", "$,", "KOUS", "PPER", "KOUS", "PPER", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "Als F\u00fcrst der Liebe schm\u00fcckt mich aus mein reicher Herzenshort,", "tokens": ["Als", "F\u00fcrst", "der", "Lie\u00b7be", "schm\u00fcckt", "mich", "aus", "mein", "rei\u00b7cher", "Her\u00b7zens\u00b7hort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Wer wirft den Purpur weg, mit dem er sich bekleiden kann?", "tokens": ["Wer", "wirft", "den", "Pur\u00b7pur", "weg", ",", "mit", "dem", "er", "sich", "be\u00b7klei\u00b7den", "kann", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PTKVZ", "$,", "APPR", "PRELS", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}