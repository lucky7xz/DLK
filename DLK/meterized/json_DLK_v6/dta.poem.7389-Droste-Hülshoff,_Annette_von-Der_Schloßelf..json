{"dta.poem.7389": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Der Schlo\u00dfelf.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "In monderhellten Weihers Glanz", "tokens": ["In", "mon\u00b7der\u00b7hell\u00b7ten", "Wei\u00b7hers", "Glanz"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Liegt br\u00fctend wie ein Wasserdrach'", "tokens": ["Liegt", "br\u00fc\u00b7tend", "wie", "ein", "Was\u00b7ser\u00b7drach'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Schlo\u00df mit seinem Zackenkranz,", "tokens": ["Das", "Schlo\u00df", "mit", "sei\u00b7nem", "Za\u00b7cken\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Zinnenmoos und Schuppendach.", "tokens": ["Mit", "Zin\u00b7nen\u00b7moos", "und", "Schup\u00b7pen\u00b7dach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die alten Eichen stehn von fern,", "tokens": ["Die", "al\u00b7ten", "Ei\u00b7chen", "stehn", "von", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Respektvoll fl\u00fcsternd mit den Wellen,", "tokens": ["Res\u00b7pekt\u00b7voll", "fl\u00fcs\u00b7ternd", "mit", "den", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wie eine graue Garde gern", "tokens": ["Wie", "ei\u00b7ne", "grau\u00b7e", "Gar\u00b7de", "gern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sich mag um graue Herrscher stellen.", "tokens": ["Sich", "mag", "um", "grau\u00b7e", "Herr\u00b7scher", "stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Am Thore schwenkt, ein Steinkolo\u00df,", "tokens": ["Am", "Tho\u00b7re", "schwenkt", ",", "ein", "Stein\u00b7ko\u00b7lo\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Pannerherr die Kreuzesfahn,", "tokens": ["Der", "Pan\u00b7ner\u00b7herr", "die", "Kreu\u00b7zes\u00b7fahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und courbettirend schnaubt sein Ro\u00df", "tokens": ["Und", "cour\u00b7bet\u00b7ti\u00b7rend", "schnaubt", "sein", "Ro\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jahrhunderte schon himmelan;", "tokens": ["Jahr\u00b7hun\u00b7der\u00b7te", "schon", "him\u00b7me\u00b7lan", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und neben ihm, ein Tantalus,", "tokens": ["Und", "ne\u00b7ben", "ihm", ",", "ein", "Tan\u00b7ta\u00b7lus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Lechzt seit Jahrhunderten sein Docke", "tokens": ["Lechzt", "seit", "Jahr\u00b7hun\u00b7der\u00b7ten", "sein", "Do\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "PPOSAT", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Gesenkten Halses nach dem Flu\u00df,", "tokens": ["Ge\u00b7senk\u00b7ten", "Hal\u00b7ses", "nach", "dem", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Im d\u00fcrren Schlunde Mooses Flocke.", "tokens": ["Im", "d\u00fcr\u00b7ren", "Schlun\u00b7de", "Moo\u00b7ses", "Flo\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ob l\u00e4ngst die Mitternacht verklang,", "tokens": ["Ob", "l\u00e4ngst", "die", "Mit\u00b7ter\u00b7nacht", "ver\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Schlosse bleibt es immer wach;", "tokens": ["Im", "Schlos\u00b7se", "bleibt", "es", "im\u00b7mer", "wach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Streiflichter gleiten rasch entlang", "tokens": ["Streif\u00b7lich\u00b7ter", "glei\u00b7ten", "rasch", "ent\u00b7lang"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "APPO"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Den Corridor und das Gemach,", "tokens": ["Den", "Cor\u00b7ri\u00b7dor", "und", "das", "Ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zuweilen durch des Hofes Raum", "tokens": ["Zu\u00b7wei\u00b7len", "durch", "des", "Ho\u00b7fes", "Raum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein h\u00fcpfendes Laternchen ziehet;", "tokens": ["Ein", "h\u00fcp\u00b7fen\u00b7des", "La\u00b7tern\u00b7chen", "zie\u00b7het", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Dann horcht der Wandrer, der am Saum", "tokens": ["Dann", "horcht", "der", "Wand\u00b7rer", ",", "der", "am", "Saum"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Des Weihers in den Binsen knieet.", "tokens": ["Des", "Wei\u00b7hers", "in", "den", "Bin\u00b7sen", "kni\u00b7e\u00b7et", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u201eave Maria! st\u00e4rke sie!", "tokens": ["\u201e", "a\u00b7ve", "Ma\u00b7ria", "!", "st\u00e4r\u00b7ke", "sie", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "NE", "NE", "$.", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und hilf ihr \u00fcber diese Nacht!\u201c", "tokens": ["Und", "hilf", "ihr", "\u00fc\u00b7ber", "die\u00b7se", "Nacht", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVIMP", "PPER", "APPR", "PDAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein frommer Bauer ist's, der fr\u00fch", "tokens": ["Ein", "from\u00b7mer", "Bau\u00b7er", "ist's", ",", "der", "fr\u00fch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich auf die Wallfahrt hat gemacht.", "tokens": ["Sich", "auf", "die", "Wall\u00b7fahrt", "hat", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wohl wei\u00df er, was der Lichterglanz", "tokens": ["Wohl", "wei\u00df", "er", ",", "was", "der", "Lich\u00b7ter\u00b7glanz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mag seiner gn\u00e4d'gen Frau bedeuten;", "tokens": ["Mag", "sei\u00b7ner", "gn\u00e4d'\u00b7gen", "Frau", "be\u00b7deu\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und eifrig l\u00e4\u00dft den Rosenkranz", "tokens": ["Und", "eif\u00b7rig", "l\u00e4\u00dft", "den", "Ro\u00b7sen\u00b7kranz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Er durch die schwiel'gen Finger gleiten.", "tokens": ["Er", "durch", "die", "schwiel'\u00b7gen", "Fin\u00b7ger", "glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch durch sein christliches Gebet", "tokens": ["Doch", "durch", "sein", "christ\u00b7li\u00b7ches", "Ge\u00b7bet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Manch Heidennebel schwankt und raucht;", "tokens": ["Manch", "Hei\u00b7den\u00b7ne\u00b7bel", "schwankt", "und", "raucht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Ob wirklich, wie die Sage geht,", "tokens": ["Ob", "wirk\u00b7lich", ",", "wie", "die", "Sa\u00b7ge", "geht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Elf sich in den Weiher taucht,", "tokens": ["Der", "Elf", "sich", "in", "den", "Wei\u00b7her", "taucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So oft dem gr\u00e4flichen Geschlecht", "tokens": ["So", "oft", "dem", "gr\u00e4f\u00b7li\u00b7chen", "Ge\u00b7schlecht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der erste Sprosse wird geboren?", "tokens": ["Der", "ers\u00b7te", "Spros\u00b7se", "wird", "ge\u00b7bo\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Bauer glaubt es nimmer recht,", "tokens": ["Der", "Bau\u00b7er", "glaubt", "es", "nim\u00b7mer", "recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Noch minder h\u00e4tt' er es verschworen.", "tokens": ["Noch", "min\u00b7der", "h\u00e4tt'", "er", "es", "ver\u00b7schwo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Scheu blickt er auf \u2014 die Nacht ist klar,", "tokens": ["Scheu", "blickt", "er", "auf", "die", "Nacht", "ist", "klar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "$(", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und g\u00e4nzlich nicht gespensterhaft,", "tokens": ["Und", "g\u00e4nz\u00b7lich", "nicht", "ge\u00b7spens\u00b7ter\u00b7haft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gleich dr\u00fcben an dem Pappelpaar", "tokens": ["Gleich", "dr\u00fc\u00b7ben", "an", "dem", "Pap\u00b7pel\u00b7paar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Z\u00e4hlt man die Zweige l\u00e4ngs dem Schaft;", "tokens": ["Z\u00e4hlt", "man", "die", "Zwei\u00b7ge", "l\u00e4ngs", "dem", "Schaft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch stille! In dem Eichenrund \u2014", "tokens": ["Doch", "stil\u00b7le", "!", "In", "dem", "Ei\u00b7chen\u00b7rund"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sind das nicht Tritte? \u2014 Kindestritte?", "tokens": ["Sind", "das", "nicht", "Trit\u00b7te", "?", "Kin\u00b7de\u00b7strit\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "NN", "$.", "$(", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er h\u00f6rt wie an dem harten Grund", "tokens": ["Er", "h\u00f6rt", "wie", "an", "dem", "har\u00b7ten", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sich wiegen, kurz und stramm, die Schritte.", "tokens": ["Sich", "wie\u00b7gen", ",", "kurz", "und", "stramm", ",", "die", "Schrit\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "$,", "ADJD", "KON", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Still! still! es raschelt \u00fcber'n Rain,", "tokens": ["Still", "!", "still", "!", "es", "ra\u00b7schelt", "\u00fc\u00b7ber'n", "Rain", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "PTKVZ", "$.", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie eine Hinde, die im Thau,", "tokens": ["Wie", "ei\u00b7ne", "Hin\u00b7de", ",", "die", "im", "Thau", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Beherzt gemacht vom Mondenschein,", "tokens": ["Be\u00b7herzt", "ge\u00b7macht", "vom", "Mon\u00b7den\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vorsichtig \u00e4\u00dfet l\u00e4ngs der Au.", "tokens": ["Vor\u00b7sich\u00b7tig", "\u00e4\u00b7\u00dfet", "l\u00e4ngs", "der", "Au", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Bauer stutzt \u2014 die Nacht ist licht,", "tokens": ["Der", "Bau\u00b7er", "stutzt", "die", "Nacht", "ist", "licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Die Bl\u00e4tter gl\u00e4nzen an dem Hagen,", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "gl\u00e4n\u00b7zen", "an", "dem", "Ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und dennoch \u2014 dennoch sieht er nicht,", "tokens": ["Und", "den\u00b7noch", "den\u00b7noch", "sieht", "er", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "A