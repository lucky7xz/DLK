{"dta.poem.159": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Neu-Jahrs-Wunsch an   Bellinen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Brich an erw\u00fcnschter Tag mit neuem Seegens-Schein/", "tokens": ["Brich", "an", "er\u00b7w\u00fcnschter", "Tag", "mit", "neu\u00b7em", "See\u00b7gens\u00b7Schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Erhelle ", "tokens": ["Er\u00b7hel\u00b7le"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Hohl Gold aus ", "tokens": ["Hohl", "Gold", "aus"], "token_info": ["word", "word", "word"], "pos": ["NE", "NN", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Rei\u00df alle K\u00fcmmerni\u00df/ und alles Klagen ein:", "tokens": ["Rei\u00df", "al\u00b7le", "K\u00fcm\u00b7mer\u00b7ni\u00df", "/", "und", "al\u00b7les", "Kla\u00b7gen", "ein", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$(", "KON", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Erf\u00fclle ihre Brust mit Himmels-reichen Freuden/", "tokens": ["Er\u00b7f\u00fcl\u00b7le", "ih\u00b7re", "Brust", "mit", "Him\u00b7mels\u00b7rei\u00b7chen", "Freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nichts als beliebte Lust mu\u00df ihre Augen weiden.", "tokens": ["Nichts", "als", "be\u00b7lieb\u00b7te", "Lust", "mu\u00df", "ih\u00b7re", "Au\u00b7gen", "wei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bring mit dem Neuen-Jahr verj\u00fcngte Lust hervor", "tokens": ["Bring", "mit", "dem", "Neu\u00b7en\u00b7Jahr", "ver\u00b7j\u00fcng\u00b7te", "Lust", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "VVFIN", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Erneu alt\u00e4glich dich in neuen Fr\u00f6lichkeiten/", "tokens": ["Er\u00b7neu", "al\u00b7t\u00e4g\u00b7lich", "dich", "in", "neu\u00b7en", "Fr\u00f6\u00b7lich\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PRF", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Hemm der ", "tokens": ["Hemm", "der"], "token_info": ["word", "word"], "pos": ["NN", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Ruff ihr mit sanfften Hauch der Engel-Thon ins Ohr.", "tokens": ["Ruff", "ihr", "mit", "sanff\u00b7ten", "Hauch", "der", "En\u00b7gel\u00b7Thon", "ins", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Erf\u00fclle ihre Brust mit Himmels-reichen Freuden/", "tokens": ["Er\u00b7f\u00fcl\u00b7le", "ih\u00b7re", "Brust", "mit", "Him\u00b7mels\u00b7rei\u00b7chen", "Freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Nichts als beliebte Lust mu\u00df ihre Augen weiden.", "tokens": ["Nichts", "als", "be\u00b7lieb\u00b7te", "Lust", "mu\u00df", "ih\u00b7re", "Au\u00b7gen", "wei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Bau Freude dir ein Haus bey ihr das ewig steh/", "tokens": ["Bau", "Freu\u00b7de", "dir", "ein", "Haus", "bey", "ihr", "das", "e\u00b7wig", "steh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPER", "ART", "NN", "APPR", "PPOSAT", "ART", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Es bl\u00fch der Jugend. Lentz in ihrem Angesichte/", "tokens": ["Es", "bl\u00fch", "der", "Ju\u00b7gend", ".", "Lentz", "in", "ih\u00b7rem", "An\u00b7ge\u00b7sich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Helena k\u00fcsse sie im vollen Sch\u00f6nheits-Lichte", "tokens": ["He\u00b7le\u00b7na", "k\u00fcs\u00b7se", "sie", "im", "vol\u00b7len", "Sch\u00f6n\u00b7heits\u00b7Lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ruh\u2019 und Zufriedenheit ihr stets zur Seiten geh.", "tokens": ["Ruh'", "und", "Zu\u00b7frie\u00b7den\u00b7heit", "ihr", "stets", "zur", "Sei\u00b7ten", "geh."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["NN", "KON", "NN", "PPER", "ADV", "APPRART", "NN", "NE"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.17": {"text": "Erf\u00fclle ihre Brust mit Himmels-reichen Freuden/", "tokens": ["Er\u00b7f\u00fcl\u00b7le", "ih\u00b7re", "Brust", "mit", "Him\u00b7mels\u00b7rei\u00b7chen", "Freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Nichts als beliebte Lust mu\u00df ihre Augen weiden.", "tokens": ["Nichts", "als", "be\u00b7lieb\u00b7te", "Lust", "mu\u00df", "ih\u00b7re", "Au\u00b7gen", "wei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Besch\u00fctte Himmel sie mit Lust und Wohlergehn/", "tokens": ["Be\u00b7sch\u00fct\u00b7te", "Him\u00b7mel", "sie", "mit", "Lust", "und", "Woh\u00b7ler\u00b7gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPER", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Es schau ihr Auge stets das holde Gl\u00fccke bl\u00fchen/", "tokens": ["Es", "schau", "ihr", "Au\u00b7ge", "stets", "das", "hol\u00b7de", "Gl\u00fc\u00b7cke", "bl\u00fc\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Hinl\u00e4\u00dfig mu\u00df ihr Haar das Silber \u00fcberziehen/", "tokens": ["Hin\u00b7l\u00e4\u00b7\u00dfig", "mu\u00df", "ihr", "Haar", "das", "Sil\u00b7ber", "\u00fc\u00b7ber\u00b7zie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Rund um den zarten Leib mu\u00df lauter Seegen stehn.", "tokens": ["Rund", "um", "den", "zar\u00b7ten", "Leib", "mu\u00df", "lau\u00b7ter", "See\u00b7gen", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "VMFIN", "PIAT", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.23": {"text": "Erf\u00fclle ihre Brust mit Himmels-reichen Freuden/", "tokens": ["Er\u00b7f\u00fcl\u00b7le", "ih\u00b7re", "Brust", "mit", "Him\u00b7mels\u00b7rei\u00b7chen", "Freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Nichts als beliebte Lust mu\u00df ihre Augen weiden.", "tokens": ["Nichts", "als", "be\u00b7lieb\u00b7te", "Lust", "mu\u00df", "ih\u00b7re", "Au\u00b7gen", "wei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}