{"dta.poem.20772": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Die neue Versicherung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bei dieser Sonne schw\u00f6r ichs dir,               ", "tokens": ["Bei", "die\u00b7ser", "Son\u00b7ne", "schw\u00f6r", "ichs", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PIS", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Quell des Lichtes angez\u00fcndet,", "tokens": ["Vom", "Quell", "des", "Lich\u00b7tes", "an\u00b7ge\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die treue Liebe flammt in mir;", "tokens": ["Die", "treu\u00b7e", "Lie\u00b7be", "flammt", "in", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der kalte Winter findet", "tokens": ["Der", "kal\u00b7te", "Win\u00b7ter", "fin\u00b7det"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mich gegen dich, so wie der Mai", "tokens": ["Mich", "ge\u00b7gen", "dich", ",", "so", "wie", "der", "Mai"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "PPER", "$,", "ADV", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Rosenmond mich fanden.", "tokens": ["Und", "Ro\u00b7sen\u00b7mond", "mich", "fan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mein Herz bleibt immer dir getreu", "tokens": ["Mein", "Herz", "bleibt", "im\u00b7mer", "dir", "ge\u00b7treu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und stirbt in deinen Banden.", "tokens": ["Und", "stirbt", "in", "dei\u00b7nen", "Ban\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}