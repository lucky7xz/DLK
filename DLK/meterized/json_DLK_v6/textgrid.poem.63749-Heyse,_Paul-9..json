{"textgrid.poem.63749": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und jenes blassen M\u00e4dchens dacht' ich da", "tokens": ["Und", "je\u00b7nes", "blas\u00b7sen", "M\u00e4d\u00b7chens", "dacht'", "ich", "da"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "ADJA", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In meiner Eltern Haus. Ihr dumpfes Zimmer", "tokens": ["In", "mei\u00b7ner", "El\u00b7tern", "Haus", ".", "Ihr", "dum\u00b7pfes", "Zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sah in den Hof; da sa\u00df sie n\u00e4hend immer,", "tokens": ["Sah", "in", "den", "Hof", ";", "da", "sa\u00df", "sie", "n\u00e4\u00b7hend", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$.", "ADV", "VVFIN", "PPER", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Bis ihre Hand dem Linnen \u00e4hnlich sah.", "tokens": ["Bis", "ih\u00b7re", "Hand", "dem", "Lin\u00b7nen", "\u00e4hn\u00b7lich", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was rings in Stadt und Land und Welt geschah,", "tokens": ["Was", "rings", "in", "Stadt", "und", "Land", "und", "Welt", "ge\u00b7schah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "NN", "KON", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Warf in ihr d\u00e4mmernd Leben keinen Schimmer.", "tokens": ["Warf", "in", "ihr", "d\u00e4m\u00b7mernd", "Le\u00b7ben", "kei\u00b7nen", "Schim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "ADJD", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df sch\u00f6n die Erde sei, erfuhr sie nimmer", "tokens": ["Da\u00df", "sch\u00f6n", "die", "Er\u00b7de", "sei", ",", "er\u00b7fuhr", "sie", "nim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADJD", "ART", "NN", "VAFIN", "$,", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und dacht' an eins nur: da\u00df ihr Ende nah.", "tokens": ["Und", "dacht'", "an", "eins", "nur", ":", "da\u00df", "ihr", "En\u00b7de", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIS", "ADV", "$.", "KOUS", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Am Sonntag kam ein blonder Kammerdiener,", "tokens": ["Am", "Sonn\u00b7tag", "kam", "ein", "blon\u00b7der", "Kam\u00b7mer\u00b7die\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der ihr von Liebe sprach; und schweigend lie\u00df", "tokens": ["Der", "ihr", "von", "Lie\u00b7be", "sprach", ";", "und", "schwei\u00b7gend", "lie\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "$.", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und l\u00e4chelnd sie's geschehn, als w\u00e4r's zum Spa\u00dfe.", "tokens": ["Und", "l\u00e4\u00b7chelnd", "sie's", "ge\u00b7schehn", ",", "als", "w\u00e4r's", "zum", "Spa\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "VVPP", "$,", "KOKOM", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Zuweilen bracht' er Kirschen mit, dann schien er", "tokens": ["Zu\u00b7wei\u00b7len", "bracht'", "er", "Kir\u00b7schen", "mit", ",", "dann", "schien", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "PTKVZ", "$,", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Gott ihr und ein kleines Paradies", "tokens": ["Ein", "Gott", "ihr", "und", "ein", "klei\u00b7nes", "Pa\u00b7ra\u00b7dies"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr Hinterst\u00fcbchen in der Behrenstra\u00dfe.", "tokens": ["Ihr", "Hin\u00b7ter\u00b7st\u00fcb\u00b7chen", "in", "der", "Beh\u00b7ren\u00b7stra\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}