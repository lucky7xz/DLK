{"textgrid.poem.54274": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "Auf ihre schwartzen Augen", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr schwartzen Augen f\u00fchrt zwar Zunder zu der Liebe,", "tokens": ["Ihr", "schwart\u00b7zen", "Au\u00b7gen", "f\u00fchrt", "zwar", "Zun\u00b7der", "zu", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch seyd ihr in der That auch recht geheime Diebe,", "tokens": ["Doch", "seyd", "ihr", "in", "der", "That", "auch", "recht", "ge\u00b7hei\u00b7me", "Die\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPR", "ART", "NN", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "L\u00e4st man nur einen Blick nach euren Creysen gehn,", "tokens": ["L\u00e4st", "man", "nur", "ei\u00b7nen", "Blick", "nach", "eu\u00b7ren", "Crey\u00b7sen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So flieht die Freyheit hin, man mu\u00df gefesselt stehn.", "tokens": ["So", "flieht", "die", "Frey\u00b7heit", "hin", ",", "man", "mu\u00df", "ge\u00b7fes\u00b7selt", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,", "PIS", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "O sch\u00f6ne Dunckelheit, welch angenehme Schatten!", "tokens": ["O", "sch\u00f6\u00b7ne", "Dun\u00b7ckel\u00b7heit", ",", "welch", "an\u00b7ge\u00b7neh\u00b7me", "Schat\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PWAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es will sich alle Welt mit solcher K\u00fchlung gatten.", "tokens": ["Es", "will", "sich", "al\u00b7le", "Welt", "mit", "sol\u00b7cher", "K\u00fch\u00b7lung", "gat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "PIAT", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Beliebte d\u00fcstre Gruft! erw\u00fcnschter Sarg! worein", "tokens": ["Be\u00b7lieb\u00b7te", "d\u00fcst\u00b7re", "Gruft", "!", "er\u00b7w\u00fcnschter", "Sarg", "!", "wo\u00b7rein"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "VVFIN", "NN", "$.", "ADJA", "NN", "$.", "PWAV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Was m\u00e4nnlich ist und heist, sich w\u00fcnscht, gelegt zu seyn.", "tokens": ["Was", "m\u00e4nn\u00b7lich", "ist", "und", "heist", ",", "sich", "w\u00fcnscht", ",", "ge\u00b7legt", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "KON", "VVFIN", "$,", "PRF", "VVFIN", "$,", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es l\u00e4st sich Quaal und Schmertz vor euch gar nicht verschweigen.", "tokens": ["Es", "l\u00e4st", "sich", "Qua\u00b7al", "und", "Schmertz", "vor", "euch", "gar", "nicht", "ver\u00b7schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "NN", "KON", "NN", "APPR", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Wiewohl ihr sucht euch stets heroisch zu bezeigen,", "tokens": ["Wie\u00b7wohl", "ihr", "sucht", "euch", "stets", "he\u00b7ro\u00b7isch", "zu", "be\u00b7zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPER", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wo das heroisch heist, wenn Unempfindlichkeit,", "tokens": ["Wo", "das", "he\u00b7ro\u00b7isch", "heist", ",", "wenn", "Un\u00b7emp\u00b7find\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJD", "VAFIN", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Statt der gehofften Gunst, uns mit Verachtung dr\u00e4ut.", "tokens": ["Statt", "der", "ge\u00b7hoff\u00b7ten", "Gunst", ",", "uns", "mit", "Ver\u00b7ach\u00b7tung", "dr\u00e4ut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.13": {"text": "Die zimmernde Natur hat eure Crey\u00df und Bogen,", "tokens": ["Die", "zim\u00b7mern\u00b7de", "Na\u00b7tur", "hat", "eu\u00b7re", "Crey\u00df", "und", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Vergebens nicht mit Flor und schwarzten Boy umzogen;", "tokens": ["Ver\u00b7ge\u00b7bens", "nicht", "mit", "Flor", "und", "schwarz\u00b7ten", "Boy", "um\u00b7zo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "NN", "KON", "VVFIN", "NE", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Sie sahe zum voraus, ihr w\u00fcrdet M\u00f6rder seyn,", "tokens": ["Sie", "sa\u00b7he", "zum", "vo\u00b7raus", ",", "ihr", "w\u00fcr\u00b7det", "M\u00f6r\u00b7der", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "PTKVZ", "$,", "PPER", "VAFIN", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Drum h\u00fcllte sie mit Flei\u00df euch in die Trauer ein.", "tokens": ["Drum", "h\u00fcll\u00b7te", "sie", "mit", "Flei\u00df", "euch", "in", "die", "Trau\u00b7er", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "NN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}