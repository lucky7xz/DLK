{"dta.poem.21460": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Die Heilige, Einzige, G\u00f6ttliche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Wann werd\u2019 ich dich finden,", "tokens": ["Wann", "werd'", "ich", "dich", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ach endlich dich finden,", "tokens": ["Ach", "end\u00b7lich", "dich", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Dich fiebernd in schauernder", "tokens": ["Dich", "fie\u00b7bernd", "in", "schau\u00b7ern\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVPP", "APPR", "ADJA"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Seelenumarmung,", "tokens": ["See\u00b7le\u00b7num\u00b7ar\u00b7mung", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "F\u00fchlen, ganz f\u00fchlen", "tokens": ["F\u00fch\u00b7len", ",", "ganz", "f\u00fch\u00b7len"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADV", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Du Heilige, Einzige,", "tokens": ["Du", "Hei\u00b7li\u00b7ge", ",", "Ein\u00b7zi\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "G\u00f6ttliche? . . . .", "tokens": ["G\u00f6tt\u00b7li\u00b7che", "?", ".", ".", ".", "."], "token_info": ["word", "punct", "punct", "punct", "punct", "punct"], "pos": ["NN", "$.", "$.", "$.", "$.", "$."], "meter": "+--", "measure": "dactylic.init"}, "line.8": {"text": "Die du bist, weil ich bin,", "tokens": ["Die", "du", "bist", ",", "weil", "ich", "bin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VAFIN", "$,", "KOUS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Mich willst, wie ich dich will \u2026", "tokens": ["Mich", "willst", ",", "wie", "ich", "dich", "will", "\u2026"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PWAV", "PPER", "PPER", "VMFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Die du mit einem Strahl deines Auges,", "tokens": ["Die", "du", "mit", "ei\u00b7nem", "Strahl", "dei\u00b7nes", "Au\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Darin der Himmel gl\u00fcht,", "tokens": ["Da\u00b7rin", "der", "Him\u00b7mel", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "All\u2019 die Schmerzen des Einsamen", "tokens": ["All'", "die", "Schmer\u00b7zen", "des", "Ein\u00b7sa\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Heimath- und Gl\u00fcckfernen", "tokens": ["Hei\u00b7math", "und", "Gl\u00fcck\u00b7fer\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["TRUNC", "KON", "NN"], "meter": "+--++-", "measure": "dactylic.init"}, "line.14": {"text": "Mitf\u00fchlend hinwegk\u00fcssest,", "tokens": ["Mit\u00b7f\u00fch\u00b7lend", "hin\u00b7weg\u00b7k\u00fcs\u00b7sest", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Mit einem Athemhauch deiner Seele,", "tokens": ["Mit", "ei\u00b7nem", "A\u00b7them\u00b7hauch", "dei\u00b7ner", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Darin ewiger Fr\u00fchling bl\u00fcht,", "tokens": ["Da\u00b7rin", "e\u00b7wi\u00b7ger", "Fr\u00fch\u00b7ling", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.17": {"text": "All\u2019 die Thr\u00e4nen ausl\u00f6schst,", "tokens": ["All'", "die", "Thr\u00e4\u00b7nen", "aus\u00b7l\u00f6schst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.18": {"text": "All\u2019 die brennende Qual,", "tokens": ["All'", "die", "bren\u00b7nen\u00b7de", "Qual", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.19": {"text": "Die meine Seele verzehrt,", "tokens": ["Die", "mei\u00b7ne", "See\u00b7le", "ver\u00b7zehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.20": {"text": "Meine unsterbliche Seele \u2026", "tokens": ["Mei\u00b7ne", "uns\u00b7terb\u00b7li\u00b7che", "See\u00b7le", "\u2026"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.21": {"text": "Wo bist du, du Sonne!", "tokens": ["Wo", "bist", "du", ",", "du", "Son\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "PPER", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.22": {"text": "Nur meine Sonne,", "tokens": ["Nur", "mei\u00b7ne", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.23": {"text": "Die du jede Wolke der Schwermuth", "tokens": ["Die", "du", "je\u00b7de", "Wol\u00b7ke", "der", "Schwer\u00b7muth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PIAT", "NN", "ART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.24": {"text": "Von gramtr\u00fcber Stirne", "tokens": ["Von", "gram\u00b7tr\u00fc\u00b7ber", "Stir\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.25": {"text": "Mir l\u00e4chelnd hinwegscheuchst,", "tokens": ["Mir", "l\u00e4\u00b7chelnd", "hin\u00b7weg\u00b7scheuchst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.26": {"text": "Triumphirend verhei\u00dfest", "tokens": ["Tri\u00b7um\u00b7phi\u00b7rend", "ver\u00b7hei\u00b7\u00dfest"], "token_info": ["word", "word"], "pos": ["ADJD", "VVFIN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.27": {"text": "Jeden Traum\u2019s Erf\u00fcllung,", "tokens": ["Je\u00b7den", "Traum's", "Er\u00b7f\u00fcl\u00b7lung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.28": {"text": "Und himmlischer Tr\u00f6stung", "tokens": ["Und", "himm\u00b7li\u00b7scher", "Tr\u00f6s\u00b7tung"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.29": {"text": "Gotts\u00fc\u00dfen Frieden", "tokens": ["Gott\u00b7s\u00fc\u00b7\u00dfen", "Frie\u00b7den"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.30": {"text": "In den sehnenden Busen gie\u00dft,", "tokens": ["In", "den", "seh\u00b7nen\u00b7den", "Bu\u00b7sen", "gie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.31": {"text": "Mir, der ich arm bin,", "tokens": ["Mir", ",", "der", "ich", "arm", "bin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.32": {"text": "So arm bin, wie Niemand?!", "tokens": ["So", "arm", "bin", ",", "wie", "Nie\u00b7mand", "?!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "$,", "PWAV", "PIS", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.33": {"text": "Wann erhebst du dein Haupt,", "tokens": ["Wann", "er\u00b7hebst", "du", "dein", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.34": {"text": "Aus Nebel und Sturm", "tokens": ["Aus", "Ne\u00b7bel", "und", "Sturm"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.35": {"text": "Dein lichtm\u00e4chtiges Haupt,", "tokens": ["Dein", "licht\u00b7m\u00e4ch\u00b7ti\u00b7ges", "Haupt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.36": {"text": "Du Erkenntni\u00df der Wahrheit", "tokens": ["Du", "Er\u00b7kennt\u00b7ni\u00df", "der", "Wahr\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "NN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.37": {"text": "Die ist und die sein wird? \u2026", "tokens": ["Die", "ist", "und", "die", "sein", "wird", "?", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "KON", "ART", "VAINF", "VAFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.38": {"text": "Wann winkst du Oase,", "tokens": ["Wann", "winkst", "du", "O\u00b7a\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.39": {"text": "Du M\u00e4hrcheninsel,", "tokens": ["Du", "M\u00e4hr\u00b7chen\u00b7in\u00b7sel", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.40": {"text": "Voll paradiesischer Auen,", "tokens": ["Voll", "pa\u00b7ra\u00b7die\u00b7si\u00b7scher", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.41": {"text": "Dem W\u00fcstenpilger,", "tokens": ["Dem", "W\u00fcs\u00b7ten\u00b7pil\u00b7ger", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.42": {"text": "Der m\u00fcde des Kampfes", "tokens": ["Der", "m\u00fc\u00b7de", "des", "Kamp\u00b7fes"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.43": {"text": "Des irdischen Kampfes", "tokens": ["Des", "ir\u00b7di\u00b7schen", "Kamp\u00b7fes"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.44": {"text": "Ohne Rettungsstern", "tokens": ["Oh\u00b7ne", "Ret\u00b7tungs\u00b7stern"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.45": {"text": "Hinsinkt, in das Nichts starrt?", "tokens": ["Hin\u00b7sinkt", ",", "in", "das", "Nichts", "starrt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "ART", "PIS", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.46": {"text": "Wann reifst du entgegen", "tokens": ["Wann", "reifst", "du", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.47": {"text": "Dem Labebed\u00fcrftigen", "tokens": ["Dem", "La\u00b7be\u00b7be\u00b7d\u00fcrf\u00b7ti\u00b7gen"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.48": {"text": "O Thaufrucht der Liebe?!", "tokens": ["O", "Thauf\u00b7rucht", "der", "Lie\u00b7be", "?!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.49": {"text": "Wann werd\u2019 ich erwachen,", "tokens": ["Wann", "werd'", "ich", "er\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVINF", "$,"], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.50": {"text": "Holdselig erwachen,", "tokens": ["Hold\u00b7se\u00b7lig", "er\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.51": {"text": "Dir im Schoo\u00dfe erwachen,", "tokens": ["Dir", "im", "Schoo\u00b7\u00dfe", "er\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.52": {"text": "Du unendliche Wonne?!", "tokens": ["Du", "un\u00b7end\u00b7li\u00b7che", "Won\u00b7ne", "?!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.53": {"text": "Wann werd\u2019 ich ", "tokens": ["Wann", "werd'", "ich"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER"], "meter": "+--", "measure": "dactylic.init"}, "line.54": {"text": "In all\u2019 ihrer Sch\u00f6nheit", "tokens": ["In", "all'", "ih\u00b7rer", "Sch\u00f6n\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.55": {"text": "Liebreiz und Anmuth,", "tokens": ["Lieb\u00b7reiz", "und", "An\u00b7muth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.56": {"text": "Die aus dem Kelch jeder Blume", "tokens": ["Die", "aus", "dem", "Kelch", "je\u00b7der", "Blu\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "PIAT", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.57": {"text": "Entgegen mir duftet,", "tokens": ["Ent\u00b7ge\u00b7gen", "mir", "duf\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.58": {"text": "Und zu mir spricht", "tokens": ["Und", "zu", "mir", "spricht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.59": {"text": "Aus der Nachtigall Schluchzen,", "tokens": ["Aus", "der", "Nach\u00b7ti\u00b7gall", "Schluch\u00b7zen", ","], "token_info": ["word", "word", 