{"textgrid.poem.53864": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Geschworene", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "B\u00fcrger! Dummkopf! Steuerzahler!", "tokens": ["B\u00fcr\u00b7ger", "!", "Dumm\u00b7kopf", "!", "Steu\u00b7er\u00b7zah\u00b7ler", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Apotheker! Stubenmaler!", "tokens": ["A\u00b7pot\u00b7he\u00b7ker", "!", "Stu\u00b7ben\u00b7ma\u00b7ler", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Setz dich auf dein B\u00e4nkchen, du,", "tokens": ["Setz", "dich", "auf", "dein", "B\u00e4nk\u00b7chen", ",", "du", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "und h\u00f6r zu.", "tokens": ["und", "h\u00f6r", "zu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "H\u00f6re, was der Richter spricht.", "tokens": ["H\u00f6\u00b7re", ",", "was", "der", "Rich\u00b7ter", "spricht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Frag und unterbrich ihn nicht!", "tokens": ["Frag", "und", "un\u00b7ter\u00b7brich", "ihn", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "F\u00fchl des Schwurgerichtes Weihe!", "tokens": ["F\u00fchl", "des", "Schwur\u00b7ge\u00b7rich\u00b7tes", "Wei\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Blutiger Laie! Blutiger Laie!", "tokens": ["Blu\u00b7ti\u00b7ger", "Lai\u00b7e", "!", "Blu\u00b7ti\u00b7ger", "Lai\u00b7e", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "NN", "NE", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Wer sich angeklagt da r\u00fchrt,", "tokens": ["Wer", "sich", "an\u00b7ge\u00b7klagt", "da", "r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "VVPP", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "ist ja l\u00e4ngst schon \u00fcberf\u00fchrt.", "tokens": ["ist", "ja", "l\u00e4ngst", "schon", "\u00fc\u00b7berf\u00b7\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was der spricht, hat keinen Sinn!", "tokens": ["Was", "der", "spricht", ",", "hat", "kei\u00b7nen", "Sinn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "VVFIN", "$,", "VAFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00f6r nicht hin.", "tokens": ["H\u00f6r", "nicht", "hin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Lausche nur dem Staatsanwalt!", "tokens": ["Lau\u00b7sche", "nur", "dem", "Staats\u00b7an\u00b7walt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Horch, wie seine Suada hallt!", "tokens": ["Horch", ",", "wie", "sei\u00b7ne", "Sua\u00b7da", "hallt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Tanze ja nicht aus der Reihe,", "tokens": ["Tan\u00b7ze", "ja", "nicht", "aus", "der", "Rei\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "+-+++-+-", "measure": "unknown.measure.penta"}, "line.8": {"text": "blutiger Laie! blutiger Laie!", "tokens": ["blu\u00b7ti\u00b7ger", "Lai\u00b7e", "!", "blu\u00b7ti\u00b7ger", "Lai\u00b7e", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Hinten im Beratungsraum,", "tokens": ["Hin\u00b7ten", "im", "Be\u00b7ra\u00b7tungs\u00b7raum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "gibts dich armes Luder kaum.", "tokens": ["gibts", "dich", "ar\u00b7mes", "Lu\u00b7der", "kaum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da bedr\u00fcckt dich immerdar", "tokens": ["Da", "be\u00b7dr\u00fcckt", "dich", "im\u00b7mer\u00b7dar"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "der Talar.", "tokens": ["der", "Ta\u00b7lar", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Du warst manches liebe Mal", "tokens": ["Du", "warst", "man\u00b7ches", "lie\u00b7be", "Mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "gar zu weich und sentimental . . .", "tokens": ["gar", "zu", "weich", "und", "sen\u00b7ti\u00b7men\u00b7tal", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "PTKA", "ADJD", "KON", "ADJD", "$.", "$.", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Doch nie so schlimm wie die Gesichter", "tokens": ["Doch", "nie", "so", "schlimm", "wie", "die", "Ge\u00b7sich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "blutiger Richter, blutiger Richter.", "tokens": ["blu\u00b7ti\u00b7ger", "Rich\u00b7ter", ",", "blu\u00b7ti\u00b7ger", "Rich\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}}}}