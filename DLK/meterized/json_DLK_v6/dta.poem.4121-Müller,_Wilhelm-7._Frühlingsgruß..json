{"dta.poem.4121": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "7.   Fr\u00fchlingsgru\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Flieg hin zu meinem Schatz geschwind,", "tokens": ["Flieg", "hin", "zu", "mei\u00b7nem", "Schatz", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es wird dich nicht verdrie\u00dfen,", "tokens": ["Es", "wird", "dich", "nicht", "ver\u00b7drie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und f\u00e4chl' ihr sanft um Wang' und Kinn,", "tokens": ["Und", "f\u00e4\u00b7chl'", "ihr", "sanft", "um", "Wang'", "und", "Kinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Treib deine j\u00fcngsten D\u00fcfte hin,", "tokens": ["Treib", "dei\u00b7ne", "j\u00fcng\u00b7sten", "D\u00fcf\u00b7te", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und sprich: Der Lenz l\u00e4\u00dft gr\u00fc\u00dfen!", "tokens": ["Und", "sprich", ":", "Der", "Lenz", "l\u00e4\u00dft", "gr\u00fc\u00b7\u00dfen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$.", "ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Laute nehm' ich von der Wand,", "tokens": ["Die", "Lau\u00b7te", "nehm'", "ich", "von", "der", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schlinge drum ein gr\u00fcnes Band,", "tokens": ["Und", "schlin\u00b7ge", "drum", "ein", "gr\u00fc\u00b7nes", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein V\u00f6glein h\u00f6rt' ich schlagen;", "tokens": ["Ein", "V\u00f6\u00b7glein", "h\u00f6rt'", "ich", "schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es schlug: Wer bindet an mit mir", "tokens": ["Es", "schlug", ":", "Wer", "bin\u00b7det", "an", "mit", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PWS", "VVFIN", "APPR", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zu Lieb' und Sang ein Festturnier", "tokens": ["Zu", "Lieb'", "und", "Sang", "ein", "Fest\u00b7tur\u00b7nier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In gr\u00fcnen Rosenhagen?", "tokens": ["In", "gr\u00fc\u00b7nen", "Ro\u00b7sen\u00b7ha\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wohl auf im hellen Mondenschein,", "tokens": ["Wohl", "auf", "im", "hel\u00b7len", "Mon\u00b7den\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch alle Gassen aus und ein,", "tokens": ["Durch", "al\u00b7le", "Gas\u00b7sen", "aus", "und", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Fiedeln und Schalmeien!", "tokens": ["Mit", "Fie\u00b7deln", "und", "Schal\u00b7mei\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Thut auf, thut auf die Fensterlein,", "tokens": ["Thut", "auf", ",", "thut", "auf", "die", "Fens\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr M\u00e4gdlein, la\u00dft den Fr\u00fchling ein!", "tokens": ["Ihr", "M\u00e4gd\u00b7lein", ",", "la\u00dft", "den", "Fr\u00fch\u00b7ling", "ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVIMP", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "D\u00fcrft euch vor ihm nicht scheuen.", "tokens": ["D\u00fcrft", "euch", "vor", "ihm", "nicht", "scheu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.4": {"line.1": {"text": "Er ist ein wohlgezog'ner Gast,", "tokens": ["Er", "ist", "ein", "wohl\u00b7ge\u00b7zo\u00b7g'\u00b7ner", "Gast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ein Kn\u00e4blein jung und bl\u00f6de fast,", "tokens": ["Ein", "Kn\u00e4\u00b7blein", "jung", "und", "bl\u00f6\u00b7de", "fast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJA", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch etwas unerfahren:", "tokens": ["Auch", "et\u00b7was", "un\u00b7er\u00b7fah\u00b7ren", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nehmt Amorn ihm als Lehrer an,", "tokens": ["Nehmt", "A\u00b7morn", "ihm", "als", "Leh\u00b7rer", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PPER", "KOUS", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So wird er bald ein kluger Mann,", "tokens": ["So", "wird", "er", "bald", "ein", "klu\u00b7ger", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Noch eh' er kommt zu Jahren.", "tokens": ["Noch", "eh'", "er", "kommt", "zu", "Jah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du heller linder Abendwind,", "tokens": ["Du", "hel\u00b7ler", "lin\u00b7der", "Ab\u00b7end\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was meint zu dir das liebe Kind,", "tokens": ["Was", "meint", "zu", "dir", "das", "lie\u00b7be", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gef\u00e4llt ihr deine Kunde?", "tokens": ["Ge\u00b7f\u00e4llt", "ihr", "dei\u00b7ne", "Kun\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gut' Nacht, gut' Nacht, die Fenster zu!", "tokens": ["Gut'", "Nacht", ",", "gut'", "Nacht", ",", "die", "Fens\u00b7ter", "zu", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der neue Gast verlangt nach Ruh',", "tokens": ["Der", "neu\u00b7e", "Gast", "ver\u00b7langt", "nach", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der W\u00e4chter bl\u00e4st die Stunde.", "tokens": ["Der", "W\u00e4ch\u00b7ter", "bl\u00e4st", "die", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}