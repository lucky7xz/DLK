{"dta.poem.2628": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Trauer-Ged ancken  \n Bey Beerdigung Fr. R. G. g. K. den 17.  \n Jan. 1671.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So ziehstu/ Seelige/ aus deiner irrdnen H\u00fctten", "tokens": ["So", "zieh\u00b7stu", "/", "See\u00b7li\u00b7ge", "/", "aus", "dei\u00b7ner", "irrd\u00b7nen", "H\u00fct\u00b7ten"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$(", "NN", "$(", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verl\u00e4st des Leibes-Hau\u00df von Erd und Leim erbaut/", "tokens": ["Ver\u00b7l\u00e4st", "des", "Lei\u00b7bes\u00b7Hau\u00df", "von", "Erd", "und", "Leim", "er\u00b7baut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nachdem du mit Gedult den Tod hast \u00fcberstritten/", "tokens": ["Nach\u00b7dem", "du", "mit", "Ge\u00b7dult", "den", "Tod", "hast", "\u00fc\u00b7bers\u00b7trit\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und deine Seele nun das Schlo\u00df der Freuden schaut.", "tokens": ["Und", "dei\u00b7ne", "See\u00b7le", "nun", "das", "Schlo\u00df", "der", "Freu\u00b7den", "schaut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Di\u00df ist ein ander Hau\u00df als unsre Leimne W\u00e4nde/", "tokens": ["Di\u00df", "ist", "ein", "an\u00b7der", "Hau\u00df", "als", "uns\u00b7re", "Leim\u00b7ne", "W\u00e4n\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "KOKOM", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die einer Folterbanck mit Recht zu gleichen sind;", "tokens": ["Die", "ei\u00b7ner", "Fol\u00b7ter\u00b7banck", "mit", "Recht", "zu", "glei\u00b7chen", "sind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "NN", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Di\u00df hellbesternte Schlo\u00df kennt keine Menschen-H\u00e4nde/", "tokens": ["Di\u00df", "hell\u00b7bes\u00b7tern\u00b7te", "Schlo\u00df", "kennt", "kei\u00b7ne", "Men\u00b7schen\u00b7H\u00e4n\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "VVFIN", "PIAT", "NN", "$("], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.8": {"text": "Weil es von Ewigkeit der Sch\u00f6pffer hat gegr\u00fcndt.", "tokens": ["Weil", "es", "von", "E\u00b7wig\u00b7keit", "der", "Sch\u00f6pf\u00b7fer", "hat", "ge\u00b7gr\u00fcndt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du sihst nun Sonn und Mond zu deinen F\u00fcssen ligen/", "tokens": ["Du", "sihst", "nun", "Sonn", "und", "Mond", "zu", "dei\u00b7nen", "F\u00fcs\u00b7sen", "li\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Hast Engel neben dir/ als treue Nachbarn stehn/", "tokens": ["Hast", "En\u00b7gel", "ne\u00b7ben", "dir", "/", "als", "treu\u00b7e", "Nach\u00b7barn", "stehn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "PPER", "$(", "KOUS", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wir leben noch im Kampf/ du prangst in lauter Siegen/", "tokens": ["Wir", "le\u00b7ben", "noch", "im", "Kampf", "/", "du", "prangst", "in", "lau\u00b7ter", "Sie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$(", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da uns das Creutze dr\u00fcckt/ kanst du gekr\u00f6net gehn.", "tokens": ["Da", "uns", "das", "Creut\u00b7ze", "dr\u00fcckt", "/", "kanst", "du", "ge\u00b7kr\u00f6\u00b7net", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$(", "VMFIN", "PPER", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Dich speist des Lebens Brod/ uns Salsen bittrer S\u00fcnden/", "tokens": ["Dich", "speist", "des", "Le\u00b7bens", "Brod", "/", "uns", "Sal\u00b7sen", "bit\u00b7trer", "S\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$(", "PPER", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und deiner Herrligkeit ist keine Hoheit gleich.", "tokens": ["Und", "dei\u00b7ner", "Herr\u00b7lig\u00b7keit", "ist", "kei\u00b7ne", "Ho\u00b7heit", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Der beste Redner wird hier keine Worte finden/", "tokens": ["Der", "bes\u00b7te", "Red\u00b7ner", "wird", "hier", "kei\u00b7ne", "Wor\u00b7te", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Womit er melden kan den Glantz in GOttes Reich.", "tokens": ["Wo\u00b7mit", "er", "mel\u00b7den", "kan", "den", "Glantz", "in", "Got\u00b7tes", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "VMFIN", "ART", "NN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Zu dem/ so ist dein Ruhm auch hier nicht gar gestorben/", "tokens": ["Zu", "dem", "/", "so", "ist", "dein", "Ruhm", "auch", "hier", "nicht", "gar", "ge\u00b7stor\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$(", "ADV", "VAFIN", "PPOSAT", "NN", "ADV", "ADV", "PTKNEG", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Dein frommer Lebens-Lauf ziert deinen Leichen-Sein;", "tokens": ["Dein", "from\u00b7mer", "Le\u00b7bens\u00b7Lauf", "ziert", "dei\u00b7nen", "Lei\u00b7chen\u00b7Sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die wahre Gottesfurcht hat dir den Spruch erworben/", "tokens": ["Die", "wah\u00b7re", "Got\u00b7tes\u00b7furcht", "hat", "dir", "den", "Spruch", "er\u00b7wor\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Da\u00df du ein Muster kanst betagter Witwen seyn.", "tokens": ["Da\u00df", "du", "ein", "Mus\u00b7ter", "kanst", "be\u00b7tag\u00b7ter", "Wit\u00b7wen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VMFIN", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Du hast wie Monica mit Beten und mit Singen", "tokens": ["Du", "hast", "wie", "Mo\u00b7ni\u00b7ca", "mit", "Be\u00b7ten", "und", "mit", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "KOKOM", "NE", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die meiste Lebens-Zeit and\u00e4chtig zugebracht/", "tokens": ["Die", "meis\u00b7te", "Le\u00b7bens\u00b7Zeit", "an\u00b7d\u00e4ch\u00b7tig", "zu\u00b7ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und dir des HErren Furcht vor allen andren Dingen", "tokens": ["Und", "dir", "des", "Her\u00b7ren", "Furcht", "vor", "al\u00b7len", "an\u00b7dren", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "NN", "NN", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Zur Richtschnur f\u00fcrgesetzt/ und dich darnach geacht.", "tokens": ["Zur", "Richt\u00b7schnur", "f\u00fcr\u00b7ge\u00b7setzt", "/", "und", "dich", "dar\u00b7nach", "ge\u00b7acht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$(", "KON", "PRF", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Du bist Zenobia dem Armuth stets gewesen/", "tokens": ["Du", "bist", "Ze\u00b7no\u00b7bia", "dem", "Ar\u00b7muth", "stets", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "ART", "NN", "ADV", "VAPP", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.26": {"text": "Und hast in Leid und Noth best\u00e4ndig dich ge\u00fcbt/", "tokens": ["Und", "hast", "in", "Leid", "und", "Noth", "be\u00b7st\u00e4n\u00b7dig", "dich", "ge\u00b7\u00fcbt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "KON", "NN", "ADJD", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Nun kanstu auch davor die Freuden-Erndt einlesen/", "tokens": ["Nun", "kans\u00b7tu", "auch", "da\u00b7vor", "die", "Freu\u00b7den\u00b7Erndt", "ein\u00b7le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PAV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Ob gleich der Thr\u00e4nen Bach zum \u00f6fftern dich betr\u00fcbt.", "tokens": ["Ob", "gleich", "der", "Thr\u00e4\u00b7nen", "Bach", "zum", "\u00f6ff\u00b7tern", "dich", "be\u00b7tr\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "NE", "APPRART", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wie hastu nicht gew\u00fcnscht in den verlebten Tagen/", "tokens": ["Wie", "has\u00b7tu", "nicht", "ge\u00b7w\u00fcnscht", "in", "den", "ver\u00b7leb\u00b7ten", "Ta\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PTKNEG", "VVPP", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und da die b\u00f6se Welt in Lastern sich verjahrt/", "tokens": ["Und", "da", "die", "b\u00f6\u00b7se", "Welt", "in", "Las\u00b7tern", "sich", "ver\u00b7jahrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "APPR", "NN", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die Hand voll Fleisch und Blut zu seiner Ruh zu tragen!", "tokens": ["Die", "Hand", "voll", "Fleisch", "und", "Blut", "zu", "sei\u00b7ner", "Ruh", "zu", "tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wie hat sich nicht dein Geist geschickt zur letzten Fahrt!", "tokens": ["Wie", "hat", "sich", "nicht", "dein", "Geist", "ge\u00b7schickt", "zur", "letz\u00b7ten", "Fahrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PRF", "PTKNEG", "PPOSAT", "NN", "VVPP", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wol dem/ und aber wol/ der zeitlich lernet sterben!", "tokens": ["Wol", "dem", "/", "und", "a\u00b7ber", "wol", "/", "der", "zeit\u00b7lich", "ler\u00b7net", "ster\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$(", "KON", "ADV", "ADV", "$(", "ART", "ADJD", "VVFIN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.34": {"text": "Es h\u00e4ngt an diesem Punckt die gantze Seeligkeit.", "tokens": ["Es", "h\u00e4ngt", "an", "die\u00b7sem", "Punckt", "die", "gant\u00b7ze", "See\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Es ist kein leichtes St\u00fcck den Himmel zu ererben/", "tokens": ["Es", "ist", "kein", "leich\u00b7tes", "St\u00fcck", "den", "Him\u00b7mel", "zu", "er\u00b7er\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Welt/ Teuffel/ Fleisch und Blut die f\u00fchren uns in", "tokens": ["Welt", "/", "Teuf\u00b7fel", "/", "Fleisch", "und", "Blut", "die", "f\u00fch\u00b7ren", "uns", "in"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "NE", "$(", "NN", "KON", "NN", "ART", "VVFIN", "PPER", "APPR"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.37": {"text": "Ein solcher Feind der stets uns dencket zu ber\u00fccken/", "tokens": ["Ein", "sol\u00b7cher", "Feind", "der", "stets", "uns", "den\u00b7cket", "zu", "be\u00b7r\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ART", "VVFIN", "PPER", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Wann Wollust/ Ehr/ und Gl\u00fcck uns ihre Schlingen", "tokens": ["Wann", "Wol\u00b7lust", "/", "Ehr", "/", "und", "Gl\u00fcck", "uns", "ih\u00b7re", "Schlin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "$(", "NN", "$(", "KON", "NN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Bey so viel Hinterlist/ bey so viel argen T\u00fccken;", "tokens": ["Bey", "so", "viel", "Hin\u00b7ter\u00b7list", "/", "bey", "so", "viel", "ar\u00b7gen", "T\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "NN", "$(", "APPR", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "+--+-+---+-+-", "measure": "iambic.penta.invert"}, "line.40": {"text": "Wie leichte wird doch nicht ein sterblich Mensch gef\u00e4llt.", "tokens": ["Wie", "leich\u00b7te", "wird", "doch", "nicht", "ein", "sterb\u00b7lich", "Mensch", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "VAFIN", "ADV", "PTKNEG", "ART", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Die grosse Sicherheit/ so unsern Sinn beth\u00f6ret/", "tokens": ["Die", "gros\u00b7se", "Si\u00b7cher\u00b7heit", "/", "so", "un\u00b7sern", "Sinn", "be\u00b7th\u00f6\u00b7ret", "/"], "t