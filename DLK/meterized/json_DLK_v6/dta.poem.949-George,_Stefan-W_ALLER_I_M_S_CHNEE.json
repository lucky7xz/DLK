{"dta.poem.949": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "W ALLER   I M   S CHNEE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Wo die strahlen schnell verschleissen               ", "tokens": ["Wo", "die", "strah\u00b7len", "schnell", "ver\u00b7schleis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leichentuch der kahlen auen               ", "tokens": ["Lei\u00b7chen\u00b7tuch", "der", "kah\u00b7len", "au\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wasser sich in furchen stauen               ", "tokens": ["Was\u00b7ser", "sich", "in", "fur\u00b7chen", "stau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den s&#252;mpfen schmelzend gleissen             ", "tokens": ["In", "den", "s", "&#252;", "mp\u00b7fen", "schmel\u00b7zend", "gleis\u00b7sen"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$(", "VVFIN", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und zum strom vereinigt laufen               ", "tokens": ["Und", "zum", "strom", "ver\u00b7ei\u00b7nigt", "lau\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVPP", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "T&#252;rm ich f&#252;r erinnerungen               ", "tokens": ["T", "&#252;", "rm", "ich", "f", "&#252;", "r", "e\u00b7rin\u00b7ne\u00b7run\u00b7gen"], "token_info": ["word", "XML_entity", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Spr\u00f6der freuden die zersprungen               ", "tokens": ["Spr\u00f6\u00b7der", "freu\u00b7den", "die", "zer\u00b7sprun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und f&#252;r dich den scheiterhaufen             ", "tokens": ["Und", "f", "&#252;", "r", "dich", "den", "schei\u00b7ter\u00b7hau\u00b7fen"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word"], "pos": ["KON", "NE", "$(", "VVIMP", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Weg den schritt vom brande lenkend               ", "tokens": ["Weg", "den", "schritt", "vom", "bran\u00b7de", "len\u00b7kend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Greif ich in dem boot die ruder               ", "tokens": ["Greif", "ich", "in", "dem", "boot", "die", "ru\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dr&#252;ben an dem strand ein bruder               ", "tokens": ["Dr", "&#252;", "ben", "an", "dem", "strand", "ein", "bru\u00b7der"], "token_info": ["word", "XML_entity", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "VVPP", "APPR", "ART", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Winkt das frohe banner schwenkend             ", "tokens": ["Winkt", "das", "fro\u00b7he", "ban\u00b7ner", "schwen\u00b7kend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Tauwind f&#228;hrt in ungest&#252;men               ", "tokens": ["Tau\u00b7wind", "f", "&#228;", "hrt", "in", "un\u00b7gest", "&#252;", "men"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "XML_entity", "word"], "pos": ["NE", "NE", "$(", "VVFIN", "APPR", "ADJD", "$(", "NN"], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.2": {"text": "St\u00f6ssen &#252;ber brache schollen               ", "tokens": ["St\u00f6s\u00b7sen", "&#252;", "ber", "bra\u00b7che", "schol\u00b7len"], "token_info": ["word", "XML_entity", "word", "word", "word"], "pos": ["NE", "$(", "ADV", "VVFIN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Mit den welken seelen sollen               ", "tokens": ["Mit", "den", "wel\u00b7ken", "see\u00b7len", "sol\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich die pfade neu bebl&#252;men.             ", "tokens": ["Sich", "die", "pfa\u00b7de", "neu", "be\u00b7bl", "&#252;", "men", "."], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["PRF", "ART", "NN", "ADJD", "NE", "$(", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}