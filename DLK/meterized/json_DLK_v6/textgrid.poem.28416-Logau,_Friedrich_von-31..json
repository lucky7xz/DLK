{"textgrid.poem.28416": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "31.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir m\u00fcssen Haus und Hof au\u00df Furcht und zittern schlissen;", "tokens": ["Wir", "m\u00fcs\u00b7sen", "Haus", "und", "Hof", "au\u00df", "Furcht", "und", "zit\u00b7tern", "schlis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "KON", "NN", "APPR", "NN", "KON", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Man g\u00fcnnt uns wenig Lufft und wil uns nirgend wissen.", "tokens": ["Man", "g\u00fcnnt", "uns", "we\u00b7nig", "Lufft", "und", "wil", "uns", "nir\u00b7gend", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PIAT", "NN", "KON", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wann da wir k\u00fcnnen seyn, wo Christus sagen kan:", "tokens": ["Wann", "da", "wir", "k\u00fcn\u00b7nen", "seyn", ",", "wo", "Chris\u00b7tus", "sa\u00b7gen", "kan", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "KOUS", "PPER", "VVFIN", "VAINF", "$,", "PWAV", "NE", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein Friede sey mit euch! liegt sonsten wenig dran.", "tokens": ["Mein", "Frie\u00b7de", "sey", "mit", "euch", "!", "liegt", "sons\u00b7ten", "we\u00b7nig", "dran", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPO