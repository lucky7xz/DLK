{"dta.poem.5605": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vom Ursprunge der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kan von getheilten Stoff allein", "tokens": ["Kan", "von", "ge\u00b7theil\u00b7ten", "Stoff", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ohn\u2019 einen Geist, der w\u00fcrckt, die Welt formiret seyn?", "tokens": ["Ohn'", "ei\u00b7nen", "Geist", ",", "der", "w\u00fcrckt", ",", "die", "Welt", "for\u00b7mi\u00b7ret", "seyn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "VVFIN", "$,", "ART", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie kan ein C\u00f6rper doch Bewegung kriegen", "tokens": ["Wie", "kan", "ein", "C\u00f6r\u00b7per", "doch", "Be\u00b7we\u00b7gung", "krie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "ART", "NN", "ADV", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ohn\u2019 einen ersten Trieb dazu zu f\u00fcgen?", "tokens": ["Ohn'", "ei\u00b7nen", "ers\u00b7ten", "Trieb", "da\u00b7zu", "zu", "f\u00fc\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}