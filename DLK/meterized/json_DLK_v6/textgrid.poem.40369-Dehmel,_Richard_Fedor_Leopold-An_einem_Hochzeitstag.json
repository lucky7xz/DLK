{"textgrid.poem.40369": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "An einem Hochzeitstag", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier, liebe Leute, in diesem sch\u00f6nen Wagen", "tokens": ["Hier", ",", "lie\u00b7be", "Leu\u00b7te", ",", "in", "die\u00b7sem", "sch\u00f6\u00b7nen", "Wa\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADJA", "NN", "$,", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "fahren zwei Menschen seit vielen sch\u00f6nen Tagen.", "tokens": ["fah\u00b7ren", "zwei", "Men\u00b7schen", "seit", "vie\u00b7len", "sch\u00f6\u00b7nen", "Ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "CARD", "NN", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Sie fahren bei Regen wie bei Sonnenschein", "tokens": ["Sie", "fah\u00b7ren", "bei", "Re\u00b7gen", "wie", "bei", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KOKOM", "APPR", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "immer gradaus ins Blaue hinein.", "tokens": ["im\u00b7mer", "gra\u00b7daus", "ins", "Blau\u00b7e", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "ADJA", "PTKVZ", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Auch das schlechteste Wetter ist ihnen nicht grau;", "tokens": ["Auch", "das", "schlech\u00b7tes\u00b7te", "Wet\u00b7ter", "ist", "ih\u00b7nen", "nicht", "grau", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VAFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "hell lacht der Mann, warm l\u00e4chelt die Frau.", "tokens": ["hell", "lacht", "der", "Mann", ",", "warm", "l\u00e4\u00b7chelt", "die", "Frau", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Sie schaukeln das Gl\u00fcck auf ihren Knien,", "tokens": ["Sie", "schau\u00b7keln", "das", "Gl\u00fcck", "auf", "ih\u00b7ren", "Kni\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "und an einem Hochzeitstag fragt sie ihn:", "tokens": ["und", "an", "ei\u00b7nem", "Hoch\u00b7zeits\u00b7tag", "fragt", "sie", "ihn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Wenn wir so immer weiter reisen", "tokens": ["Wenn", "wir", "so", "im\u00b7mer", "wei\u00b7ter", "rei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "und lassen die Sterne den Weg uns weisen,", "tokens": ["und", "las\u00b7sen", "die", "Ster\u00b7ne", "den", "Weg", "uns", "wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "k\u00fcmmern uns um kein irdisch Ziel,", "tokens": ["k\u00fcm\u00b7mern", "uns", "um", "kein", "ir\u00b7disch", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "ADJD", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "treiben nur mit dem Gl\u00fcck unser Spiel,", "tokens": ["trei\u00b7ben", "nur", "mit", "dem", "Gl\u00fcck", "un\u00b7ser", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "aber endlich wird's uns vom Tod da genommen \u2013", "tokens": ["a\u00b7ber", "end\u00b7lich", "wird's", "uns", "vom", "Tod", "da", "ge\u00b7nom\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "APPRART", "NN", "ADV", "VVPP", "$("], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "was meinst du wohl, wohin wir kommen?", "tokens": ["was", "meinst", "du", "wohl", ",", "wo\u00b7hin", "wir", "kom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Mann blickt nach den milchwei\u00dfen K\u00fchen,", "tokens": ["Der", "Mann", "blickt", "nach", "den", "milch\u00b7wei\u00b7\u00dfen", "K\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-++-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "die den bunten Wagen ruhig ziehen,", "tokens": ["die", "den", "bun\u00b7ten", "Wa\u00b7gen", "ru\u00b7hig", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "er blickt nach dem Kutscher, der Augen macht", "tokens": ["er", "blickt", "nach", "dem", "Kut\u00b7scher", ",", "der", "Au\u00b7gen", "macht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "ART", "NN", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "so unergr\u00fcndlich schwarz wie die Nacht \u2013", "tokens": ["so", "un\u00b7er\u00b7gr\u00fcnd\u00b7lich", "schwarz", "wie", "die", "Nacht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "dann sagt er heiter:", "tokens": ["dann", "sagt", "er", "hei\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Ich meine, wir kommen immer weiter!", "tokens": ["Ich", "mei\u00b7ne", ",", "wir", "kom\u00b7men", "im\u00b7mer", "wei\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Der Kutscher nickt. Der Himmel ist blau;", "tokens": ["Der", "Kut\u00b7scher", "nickt", ".", "Der", "Him\u00b7mel", "ist", "blau", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "warm l\u00e4chelt der Mann, hell lacht die Frau.", "tokens": ["warm", "l\u00e4\u00b7chelt", "der", "Mann", ",", "hell", "lacht", "die", "Frau", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und die wei\u00dfen K\u00fche sagen sich beide:", "tokens": ["Und", "die", "wei\u00b7\u00dfen", "K\u00fc\u00b7he", "sa\u00b7gen", "sich", "bei\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PRF", "PIS", "$."], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "zwei Menschen fahren auf lebensgr\u00fcner Weide.", "tokens": ["zwei", "Men\u00b7schen", "fah\u00b7ren", "auf", "le\u00b7bens\u00b7gr\u00fc\u00b7ner", "Wei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}}}}