{"dta.poem.19829": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Das dumme Br\u00fcderlein .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo soll ich mich hinkehren,               ", "tokens": ["Wo", "soll", "ich", "mich", "hin\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich dummes Br\u00fcderlein,", "tokens": ["Ich", "dum\u00b7mes", "Br\u00fc\u00b7derl\u00b7ein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie soll ich mich ern\u00e4hren", "tokens": ["Wie", "soll", "ich", "mich", "er\u00b7n\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Gut ist viel zu klein;", "tokens": ["Mein", "Gut", "ist", "viel", "zu", "klein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie wir ein Wesen han,", "tokens": ["Wie", "wir", "ein", "We\u00b7sen", "han", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "So mu\u00df ich bald daran,", "tokens": ["So", "mu\u00df", "ich", "bald", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Was ich heut soll verzehren", "tokens": ["Was", "ich", "heut", "soll", "ver\u00b7zeh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ist gestern schon verthan.", "tokens": ["Ist", "ge\u00b7stern", "schon", "ver\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Ich bin zu fr\u00fch geboren,", "tokens": ["Ich", "bin", "zu", "fr\u00fch", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo heute ich hinkomm,", "tokens": ["Wo", "heu\u00b7te", "ich", "hin\u00b7komm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Gl\u00fcck das kommt erst morgen,", "tokens": ["Mein", "Gl\u00fcck", "das", "kommt", "erst", "mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDS", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "H\u00e4tt ich den Schatz im Dom,", "tokens": ["H\u00e4tt", "ich", "den", "Schatz", "im", "Dom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dazu den Zoll am Rhein,", "tokens": ["Da\u00b7zu", "den", "Zoll", "am", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "APPRART", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und w\u00e4r Venedig mein,", "tokens": ["Und", "w\u00e4r", "Ve\u00b7ne\u00b7dig", "mein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NE", "PPOSAT", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "So w\u00e4r es all verloren,", "tokens": ["So", "w\u00e4r", "es", "all", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Es m\u00fc\u00dft verschlemmet seyn.", "tokens": ["Es", "m\u00fc\u00dft", "ver\u00b7schlem\u00b7met", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was hilft, da\u00df ich mag sparen,", "tokens": ["Was", "hilft", ",", "da\u00df", "ich", "mag", "spa\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vielleicht verl\u00f6r ichs gar,", "tokens": ["Viel\u00b7leicht", "ver\u00b7l\u00f6r", "ichs", "gar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sollt mirs ein Dieb ausscharren,", "tokens": ["Sollt", "mirs", "ein", "Dieb", "aus\u00b7schar\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es reute mich ein Jahr,", "tokens": ["Es", "reu\u00b7te", "mich", "ein", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich weis, mein Gut vergeht", "tokens": ["Ich", "weis", ",", "mein", "Gut", "ver\u00b7geht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PTKVZ", "$,", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mit Schlemmen fr\u00fch und sp\u00e4t,", "tokens": ["Mit", "Schlem\u00b7men", "fr\u00fch", "und", "sp\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch der hat einen Sparren,", "tokens": ["Doch", "der", "hat", "ei\u00b7nen", "Spar\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dem was zu Herzen geht.", "tokens": ["Dem", "was", "zu", "Her\u00b7zen", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRELS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich la\u00df die V\u00f6gel sorgen", "tokens": ["Ich", "la\u00df", "die", "V\u00f6\u00b7gel", "sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In diesem Winter kalt,", "tokens": ["In", "die\u00b7sem", "Win\u00b7ter", "kalt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Will uns der Wirth nicht borgen,", "tokens": ["Will", "uns", "der", "Wirth", "nicht", "bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Rock geb ich ihm bald,", "tokens": ["Den", "Rock", "geb", "ich", "ihm", "bald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sein Wein, der mich erzog,", "tokens": ["Sein", "Wein", ",", "der", "mich", "er\u00b7zog", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Hat nur ein h\u00f6lzern Rock,", "tokens": ["Hat", "nur", "ein", "h\u00f6l\u00b7zern", "Rock", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Will mich als Fa\u00df ihm borgen,", "tokens": ["Will", "mich", "als", "Fa\u00df", "ihm", "bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "KOUS", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In meinem rothen Rock.", "tokens": ["In", "mei\u00b7nem", "ro\u00b7then", "Rock", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "R\u00fcck an den Schweinebraten,", "tokens": ["R\u00fcck", "an", "den", "Schwei\u00b7ne\u00b7bra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dazu die H\u00fchner jung,", "tokens": ["Da\u00b7zu", "die", "H\u00fch\u00b7ner", "jung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Darauf mag bas gerathen", "tokens": ["Da\u00b7rauf", "mag", "bas", "ge\u00b7ra\u00b7then"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "NE", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein frischer k\u00fchler Trunk,", "tokens": ["Ein", "fri\u00b7scher", "k\u00fch\u00b7ler", "Trunk", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein Freund, du guter Wein,", "tokens": ["Mein", "Freund", ",", "du", "gu\u00b7ter", "Wein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Willkommen, du bist mein,", "tokens": ["Will\u00b7kom\u00b7men", ",", "du", "bist", "mein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mir ist ein Beut gerathen,", "tokens": ["Mir", "ist", "ein", "Beut", "ge\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das mu\u00df verschlemmet seyn.", "tokens": ["Das", "mu\u00df", "ver\u00b7schlem\u00b7met", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "VVPP", "VAINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Drey W\u00fcrfel, eine Karte,", "tokens": ["Drey", "W\u00fcr\u00b7fel", ",", "ei\u00b7ne", "Kar\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das ist mein Wappen frey,", "tokens": ["Das", "ist", "mein", "Wap\u00b7pen", "frey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sechs h\u00fcbsche Fr\u00e4ulein zarte", "tokens": ["Sechs", "h\u00fcb\u00b7sche", "Fr\u00e4u\u00b7lein", "zar\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "ADJA", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An jeder Seite drey,", "tokens": ["An", "je\u00b7der", "Sei\u00b7te", "drey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "CARD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Komm her du sch\u00f6nes Weib,", "tokens": ["Komm", "her", "du", "sch\u00f6\u00b7nes", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mein Herz freut sich im Leib,", "tokens": ["Mein", "Herz", "freut", "sich", "im", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+---+", "measure": "dactylic.init"}, "line.7": {"text": "Du mu\u00dft heut auf mich warten,", "tokens": ["Du", "mu\u00dft", "heut", "auf", "mich", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Wein ist Zeitvertreib.", "tokens": ["Der", "Wein", "ist", "Zeit\u00b7ver\u00b7treib", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}