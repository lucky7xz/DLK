{"textgrid.poem.47807": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Gr\u00e4ber an der K\u00fcste", "genre": "verse", "period": "N.A.", "pub_year": 1850, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Kr\u00e4nzen haben wir das Grab geschm\u00fcckt,", "tokens": ["Mit", "Kr\u00e4n\u00b7zen", "ha\u00b7ben", "wir", "das", "Grab", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die stille Wiege unsrer jungen Toten;", "tokens": ["Die", "stil\u00b7le", "Wie\u00b7ge", "uns\u00b7rer", "jun\u00b7gen", "To\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den gr\u00fcnsten Efeu haben wir gepfl\u00fcckt,", "tokens": ["Den", "gr\u00fcns\u00b7ten", "E\u00b7feu", "ha\u00b7ben", "wir", "ge\u00b7pfl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die sp\u00e4tsten Astern, die das Jahr geboten.", "tokens": ["Die", "sp\u00e4ts\u00b7ten", "As\u00b7tern", ",", "die", "das", "Jahr", "ge\u00b7bo\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hier ruhn sie waffenlos in ihrer Gruft,", "tokens": ["Hier", "ruhn", "sie", "waf\u00b7fen\u00b7los", "in", "ih\u00b7rer", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die man hinaustrug aus dem Pulverdampfe;", "tokens": ["Die", "man", "hin\u00b7aust\u00b7rug", "aus", "dem", "Pul\u00b7ver\u00b7damp\u00b7fe", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vom Strand her\u00fcber weht der Meeresduft,", "tokens": ["Vom", "Strand", "her\u00b7\u00fc\u00b7ber", "weht", "der", "Mee\u00b7res\u00b7duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Schl\u00e4fer k\u00fchlend nach dem hei\u00dfen Kampfe.", "tokens": ["Die", "Schl\u00e4\u00b7fer", "k\u00fch\u00b7lend", "nach", "dem", "hei\u00b7\u00dfen", "Kamp\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es steigt die Flut; vom Ring des Deiches her", "tokens": ["Es", "steigt", "die", "Flut", ";", "vom", "Ring", "des", "Dei\u00b7ches", "her"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "APPRART", "NN", "ART", "NN", "APZR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im Abendschein entbrennt der Wasserspiegel;", "tokens": ["Im", "A\u00b7bend\u00b7schein", "ent\u00b7brennt", "der", "Was\u00b7ser\u00b7spie\u00b7gel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr schlafet sch\u00f6n! Das heimatliche Meer", "tokens": ["Ihr", "schla\u00b7fet", "sch\u00f6n", "!", "Das", "hei\u00b7mat\u00b7li\u00b7che", "Meer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wirft seinen Glanz auf euren dunklen H\u00fcgel.", "tokens": ["Wirft", "sei\u00b7nen", "Glanz", "auf", "eu\u00b7ren", "dunk\u00b7len", "H\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und rissen sie die Farben auch herab,", "tokens": ["Und", "ris\u00b7sen", "sie", "die", "Far\u00b7ben", "auch", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr die so jung ihr ginget zu den Bleichen,", "tokens": ["F\u00fcr", "die", "so", "jung", "ihr", "gin\u00b7get", "zu", "den", "Blei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJD", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Oh, schlafet ruhig! Denn von Grab zu Grab", "tokens": ["Oh", ",", "schla\u00b7fet", "ru\u00b7hig", "!", "Denn", "von", "Grab", "zu", "Grab"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VVFIN", "ADJD", "$.", "KON", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wehn um euch her der Feinde Wappenzeichen.", "tokens": ["Wehn", "um", "euch", "her", "der", "Fein\u00b7de", "Wap\u00b7pen\u00b7zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADV", "ART", "NN", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "Nicht euch zum Ruhme sind sie aufgesteckt;", "tokens": ["Nicht", "euch", "zum", "Ruh\u00b7me", "sind", "sie", "auf\u00b7ge\u00b7steckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPER", "APPRART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch k\u00fcnden sie, da\u00df eure Kugeln trafen,", "tokens": ["Doch", "k\u00fcn\u00b7den", "sie", ",", "da\u00df", "eu\u00b7re", "Ku\u00b7geln", "tra\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df, als ihr euch zur ew'gen Ruh gestreckt,", "tokens": ["Da\u00df", ",", "als", "ihr", "euch", "zur", "ew'\u00b7gen", "Ruh", "ge\u00b7streckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "PRF", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den Feind ihr zwanget, neben euch zu schlafen.", "tokens": ["Den", "Feind", "ihr", "zwan\u00b7get", ",", "ne\u00b7ben", "euch", "zu", "schla\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ihr aber, denen ohne Trommelschlag", "tokens": ["Ihr", "a\u00b7ber", ",", "de\u00b7nen", "oh\u00b7ne", "Trom\u00b7mel\u00b7schlag"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Durch Feindeshand bereitet ward der Rasen,", "tokens": ["Durch", "Fein\u00b7des\u00b7hand", "be\u00b7rei\u00b7tet", "ward", "der", "Ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "H\u00f6rt dieses Lied! und harret auf den Tag,", "tokens": ["H\u00f6rt", "die\u00b7ses", "Lied", "!", "und", "har\u00b7ret", "auf", "den", "Tag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PDAT", "NN", "$.", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df unsre Reiter hier Reveille blasen! \u2013", "tokens": ["Da\u00df", "uns\u00b7re", "Rei\u00b7ter", "hier", "Re\u00b7veil\u00b7le", "bla\u00b7sen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Doch sollte dieser hei\u00dfe Lebensstreit", "tokens": ["Doch", "soll\u00b7te", "die\u00b7ser", "hei\u00b7\u00dfe", "Le\u00b7bens\u00b7streit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verlorengehn wie euer Blut im Sande", "tokens": ["Ver\u00b7lo\u00b7ren\u00b7gehn", "wie", "eu\u00b7er", "Blut", "im", "San\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und nur im Reiche der Vergangenheit", "tokens": ["Und", "nur", "im", "Rei\u00b7che", "der", "Ver\u00b7gan\u00b7gen\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NE", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Name leben dieser sch\u00f6nen Lande:", "tokens": ["Der", "Na\u00b7me", "le\u00b7ben", "die\u00b7ser", "sch\u00f6\u00b7nen", "Lan\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "In diesem Grabe, wenn das Schwert zerbricht,", "tokens": ["In", "die\u00b7sem", "Gra\u00b7be", ",", "wenn", "das", "Schwert", "zer\u00b7bricht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Liegt deutsche Ehre fleckenlos gebettet!", "tokens": ["Liegt", "deut\u00b7sche", "Eh\u00b7re", "fle\u00b7cken\u00b7los", "ge\u00b7bet\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Besch\u00fctzen konntet ihr die Heimat nicht,", "tokens": ["Be\u00b7sch\u00fct\u00b7zen", "konn\u00b7tet", "ihr", "die", "Hei\u00b7mat", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch habt ihr sterbend sie vor Schmach gerettet.", "tokens": ["Doch", "habt", "ihr", "ster\u00b7bend", "sie", "vor", "Schmach", "ge\u00b7ret\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Nun ruht ihr, wie im Mutterscho\u00df das Kind,", "tokens": ["Nun", "ruht", "ihr", ",", "wie", "im", "Mut\u00b7ter\u00b7scho\u00df", "das", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und schlafet aus auf heimatlichem Kissen;", "tokens": ["Und", "schla\u00b7fet", "aus", "auf", "hei\u00b7mat\u00b7li\u00b7chem", "Kis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wir andern aber, die wir \u00fcbrig sind,", "tokens": ["Wir", "an\u00b7dern", "a\u00b7ber", ",", "die", "wir", "\u00fcb\u00b7rig", "sind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "ADV", "$,", "PRELS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wo werden wir im Elend sterben m\u00fcssen!", "tokens": ["Wo", "wer\u00b7den", "wir", "im", "E\u00b7lend", "ster\u00b7ben", "m\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "APPRART", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Schon hatten wir zu festlichem Empfang", "tokens": ["Schon", "hat\u00b7ten", "wir", "zu", "fest\u00b7li\u00b7chem", "Emp\u00b7fang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit Kr\u00e4nzen in der Hand das Haus verlassen;", "tokens": ["Mit", "Kr\u00e4n\u00b7zen", "in", "der", "Hand", "das", "Haus", "ver\u00b7las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wir standen harrend ganze N\u00e4chte lang,", "tokens": ["Wir", "stan\u00b7den", "har\u00b7rend", "gan\u00b7ze", "N\u00e4ch\u00b7te", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch nur die Toten zogen durch die Gassen.-", "tokens": ["Doch", "nur", "die", "To\u00b7ten", "zo\u00b7gen", "durch", "die", "Gas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "So nehmet denn, ihr Schl\u00e4fer dieser Gruft,", "tokens": ["So", "neh\u00b7met", "denn", ",", "ihr", "Schl\u00e4\u00b7fer", "die\u00b7ser", "Gruft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die sp\u00e4tsten Blumen, die das Jahr geboten!", "tokens": ["Die", "sp\u00e4ts\u00b7ten", "Blu\u00b7men", ",", "die", "das", "Jahr", "ge\u00b7bo\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schon f\u00e4llt das Laub im letzten Sonnenduft \u2013", "tokens": ["Schon", "f\u00e4llt", "das", "Laub", "im", "letz\u00b7ten", "Son\u00b7nen\u00b7duft", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auch dieses Sommers Kranz geh\u00f6rt den Toten.", "tokens": ["Auch", "die\u00b7ses", "Som\u00b7mers", "Kranz", "ge\u00b7h\u00f6rt", "den", "To\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}