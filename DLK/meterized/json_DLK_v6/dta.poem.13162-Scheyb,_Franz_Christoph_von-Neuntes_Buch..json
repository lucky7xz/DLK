{"dta.poem.13162": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nun fuhr die Wahrheit fort: \u201eWer kann sich also schmeicheln", "tokens": ["Nun", "fuhr", "die", "Wahr\u00b7heit", "fort", ":", "\u201e", "Wer", "kann", "sich", "al\u00b7so", "schmei\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$.", "$(", "PWS", "VMFIN", "PRF", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "280\u201dDes Tempels Zier zu seyn? ich rede sonder Heucheln:", "tokens": ["\"", "Des", "Tem\u00b7pels", "Zier", "zu", "seyn", "?", "ich", "re\u00b7de", "son\u00b7der", "Heu\u00b7cheln", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "NN", "PTKZU", "VAINF", "$.", "PPER", "VVFIN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201erath, Tugend, Gl\u00fcck und Hilff, Macht, Beystand, Ehr und Ruhm,", "tokens": ["\u201e", "rath", ",", "Tu\u00b7gend", ",", "Gl\u00fcck", "und", "Hilff", ",", "Macht", ",", "Beys\u00b7tand", ",", "Ehr", "und", "Ruhm", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eja was von euch entspringt, und euerm Eigenthum,", "tokens": ["\u201e", "ja", "was", "von", "euch", "ent\u00b7springt", ",", "und", "eu\u00b7erm", "Ei\u00b7gen\u00b7thum", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PWS", "APPR", "PPER", "VVFIN", "$,", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201emacht meine Sinnen arm: die Vielheit eurer Gaben", "tokens": ["\u201e", "macht", "mei\u00b7ne", "Sin\u00b7nen", "arm", ":", "die", "Viel\u00b7heit", "eu\u00b7rer", "Ga\u00b7ben"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "PPOSAT", "NN", "ADJD", "$.", "ART", "NN", "PPOSAT", "NN"], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.6": {"text": "\u201eist fast wie dieses Herz bi\u00df an die Stern\u2019 erhaben.", "tokens": ["\u201e", "ist", "fast", "wie", "die\u00b7ses", "Herz", "bi\u00df", "an", "die", "Stern'", "er\u00b7ha\u00b7ben", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADV", "KOKOM", "PDAT", "NN", "APPR", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "285\u201dDer ganze Tugend-Chor ist schon so Welt-ber\u00fchmt,", "tokens": ["\"", "Der", "gan\u00b7ze", "Tu\u00b7gen\u00b7dChor", "ist", "schon", "so", "Welt\u00b7be\u00b7r\u00fchmt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VAFIN", "ADV", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201eda\u00df jeder ein Geb\u00e4u, wie dieses Herz, geziemt.", "tokens": ["\u201e", "da\u00df", "je\u00b7der", "ein", "Ge\u00b7b\u00e4u", ",", "wie", "die\u00b7ses", "Herz", ",", "ge\u00b7ziemt", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "KOUS", "PIS", "ART", "NN", "$,", "PWAV", "PDAT", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201eso mehr als ihr den Flei\u00df zu helffen stets vermehret,", "tokens": ["\u201e", "so", "mehr", "als", "ihr", "den", "Flei\u00df", "zu", "helf\u00b7fen", "stets", "ver\u00b7meh\u00b7ret", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "KOUS", "PPER", "ART", "NN", "PTKZU", "VVINF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eund immer euern Prei\u00df durch neue Fr\u00fcchte n\u00e4hret.", "tokens": ["\u201e", "und", "im\u00b7mer", "eu\u00b7ern", "Prei\u00df", "durch", "neu\u00b7e", "Fr\u00fcch\u00b7te", "n\u00e4h\u00b7ret", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ADV", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}