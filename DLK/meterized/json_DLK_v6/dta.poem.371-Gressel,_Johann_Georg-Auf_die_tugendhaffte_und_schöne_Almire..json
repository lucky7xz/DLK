{"dta.poem.371": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Auf die tugendhaffte und sch\u00f6ne  Almire.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "So wie die g\u00fcldne Sonn' am hohen Himmel stehet", "tokens": ["So", "wie", "die", "g\u00fcld\u00b7ne", "Sonn'", "am", "ho\u00b7hen", "Him\u00b7mel", "ste\u00b7het"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mit beliebten Schein desselben Zier erh\u00f6het", "tokens": ["Und", "mit", "be\u00b7lieb\u00b7ten", "Schein", "des\u00b7sel\u00b7ben", "Zier", "er\u00b7h\u00f6\u00b7het"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So mu\u00df auch dein Gesicht der Tugend Wohn-Platz seyn/", "tokens": ["So", "mu\u00df", "auch", "dein", "Ge\u00b7sicht", "der", "Tu\u00b7gend", "Wohn\u00b7Platz", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PPOSAT", "NN", "ART", "NN", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer deine Wunder-Pracht ", "tokens": ["Wer", "dei\u00b7ne", "Wun\u00b7der\u00b7Pracht"], "token_info": ["word", "word", "word"], "pos": ["PWS", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Desselben Hertze wird ins Liebes-Garn ger\u00fccket;", "tokens": ["Des\u00b7sel\u00b7ben", "Hert\u00b7ze", "wird", "ins", "Lie\u00b7bes\u00b7Garn", "ge\u00b7r\u00fc\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nimmt ihn der Tugend-Glantz nun/ oder Sch\u00f6nheit ein?", "tokens": ["Nimmt", "ihn", "der", "Tu\u00b7gen\u00b7dGlantz", "nun", "/", "o\u00b7der", "Sch\u00f6n\u00b7heit", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$(", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}