{"dta.poem.12068": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "88.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00df dich von gl\u00e4nzenden Beweisen nur nicht blenden,", "tokens": ["La\u00df", "dich", "von", "gl\u00e4n\u00b7zen\u00b7den", "Be\u00b7wei\u00b7sen", "nur", "nicht", "blen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Die sie mit viel Geschmack auf Abgeschmacktes wenden.", "tokens": ["Die", "sie", "mit", "viel", "Ge\u00b7schmack", "auf", "Ab\u00b7ge\u00b7schmack\u00b7tes", "wen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PIAT", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn was ein jeder glaubt, das kann er auch beweisen;", "tokens": ["Denn", "was", "ein", "je\u00b7der", "glaubt", ",", "das", "kann", "er", "auch", "be\u00b7wei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "PIS", "VVFIN", "$,", "PDS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wer dasselbe glaubt, wird die Beweise preisen.", "tokens": ["Und", "wer", "das\u00b7sel\u00b7be", "glaubt", ",", "wird", "die", "Be\u00b7wei\u00b7se", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PDAT", "VVFIN", "$,", "VAFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du wei\u00dft: was wirklich ist, mu\u00df m\u00f6glich seyn, und mu\u00df,", "tokens": ["Du", "wei\u00dft", ":", "was", "wirk\u00b7lich", "ist", ",", "mu\u00df", "m\u00f6g\u00b7lich", "seyn", ",", "und", "mu\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PWS", "ADJD", "VAFIN", "$,", "VMFIN", "ADJD", "VAINF", "$,", "KON", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil nichts zuf\u00e4llig ist, nothwendig seyn zum Schlu\u00df.", "tokens": ["Weil", "nichts", "zu\u00b7f\u00e4l\u00b7lig", "ist", ",", "noth\u00b7wen\u00b7dig", "seyn", "zum", "Schlu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VAFIN", "$,", "ADJD", "VAINF", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Darum beweisen sie, was irgend ward ersonnen,", "tokens": ["Da\u00b7rum", "be\u00b7wei\u00b7sen", "sie", ",", "was", "ir\u00b7gend", "ward", "er\u00b7son\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "PRELS", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sobald es Wirklichkeit in ihrem Sinn gewonnen.", "tokens": ["So\u00b7bald", "es", "Wirk\u00b7lich\u00b7keit", "in", "ih\u00b7rem", "Sinn", "ge\u00b7won\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}