{"textgrid.poem.34774": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Diese sch\u00f6nen Gliedermassen", "tokens": ["Die\u00b7se", "sch\u00f6\u00b7nen", "Glie\u00b7der\u00b7mas\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kolossaler Weiblichkeit", "tokens": ["Ko\u00b7los\u00b7sa\u00b7ler", "Weib\u00b7lich\u00b7keit"], "token_info": ["word", "word"], "pos": ["NE", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sind jetzt, ohne Widerstreit,", "tokens": ["Sind", "jetzt", ",", "oh\u00b7ne", "Wi\u00b7der\u00b7streit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "KOUI", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meinen W\u00fcnschen \u00fcberlassen.", "tokens": ["Mei\u00b7nen", "W\u00fcn\u00b7schen", "\u00fc\u00b7ber\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "W\u00e4r ich, leidenschaftentz\u00fcgelt,", "tokens": ["W\u00e4r", "ich", ",", "lei\u00b7den\u00b7schaf\u00b7tent\u00b7z\u00fc\u00b7gelt", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eigenkr\u00e4ftig ihr genaht,", "tokens": ["Ei\u00b7gen\u00b7kr\u00e4f\u00b7tig", "ihr", "ge\u00b7naht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich bereute solche Tat!", "tokens": ["Ich", "be\u00b7reu\u00b7te", "sol\u00b7che", "Tat", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ja, sie h\u00e4tte mich gepr\u00fcgelt.", "tokens": ["Ja", ",", "sie", "h\u00e4t\u00b7te", "mich", "ge\u00b7pr\u00fc\u00b7gelt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Welcher Busen, Hals und Kehle!", "tokens": ["Wel\u00b7cher", "Bu\u00b7sen", ",", "Hals", "und", "Keh\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "(h\u00f6her seh ich nicht genau.)", "tokens": ["(", "h\u00f6\u00b7her", "seh", "ich", "nicht", "ge\u00b7nau", ".", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "VVFIN", "PPER", "PTKNEG", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eh' ich ihr mich anvertrau,", "tokens": ["Eh'", "ich", "ihr", "mich", "an\u00b7ver\u00b7trau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PRF", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gott empfehl ich meine Seele.", "tokens": ["Gott", "emp\u00b7fehl", "ich", "mei\u00b7ne", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}