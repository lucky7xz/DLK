{"textgrid.poem.36812": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An Albert von Thorwaldsen", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So sei gegr\u00fc\u00dft zur heitern Feierstunde,", "tokens": ["So", "sei", "ge\u00b7gr\u00fc\u00dft", "zur", "hei\u00b7tern", "Fei\u00b7er\u00b7stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir nahen dir mit dankbarem Gef\u00fchl,", "tokens": ["Wir", "na\u00b7hen", "dir", "mit", "dank\u00b7ba\u00b7rem", "Ge\u00b7f\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Nur Eine Liebe weht in unserm Bunde,", "tokens": ["Nur", "Ei\u00b7ne", "Lie\u00b7be", "weht", "in", "un\u00b7serm", "Bun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur Ein Gedank' im festlichen Gew\u00fchl:", "tokens": ["Nur", "Ein", "Ge\u00b7dank'", "im", "fest\u00b7li\u00b7chen", "Ge\u00b7w\u00fchl", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Des Meisters Name t\u00f6nt von unserm Munde,", "tokens": ["Des", "Meis\u00b7ters", "Na\u00b7me", "t\u00f6nt", "von", "un\u00b7serm", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Was in den Herzen gl\u00fcht, ist gro\u00df und viel,", "tokens": ["Was", "in", "den", "Her\u00b7zen", "gl\u00fcht", ",", "ist", "gro\u00df", "und", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$,", "VAFIN", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Den leeren Schwall der Worte la\u00dft uns meiden,", "tokens": ["Den", "lee\u00b7ren", "Schwall", "der", "Wor\u00b7te", "la\u00dft", "uns", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Meister ist's, so sind auch wir bescheiden.", "tokens": ["Der", "Meis\u00b7ter", "ist's", ",", "so", "sind", "auch", "wir", "be\u00b7schei\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "ADV", "VAFIN", "ADV", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ernst ist die Zeit und schwere Wolken liegen", "tokens": ["Ernst", "ist", "die", "Zeit", "und", "schwe\u00b7re", "Wol\u00b7ken", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "KON", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An jenem reinen Himmel ausgestreckt,", "tokens": ["An", "je\u00b7nem", "rei\u00b7nen", "Him\u00b7mel", "aus\u00b7ge\u00b7streckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aus dem die G\u00f6tter einst herniederstiegen,", "tokens": ["Aus", "dem", "die", "G\u00f6t\u00b7ter", "einst", "her\u00b7nie\u00b7ders\u00b7tie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die jeden Keim des Irdischen geweckt,", "tokens": ["Die", "je\u00b7den", "Keim", "des", "Ir\u00b7di\u00b7schen", "ge\u00b7weckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und ew'ge M\u00e4chte, die im Himmel siegen,", "tokens": ["Und", "ew'\u00b7ge", "M\u00e4ch\u00b7te", ",", "die", "im", "Him\u00b7mel", "sie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das Haupt mit ird'schem Lorbeer sich bedeckt,", "tokens": ["Das", "Haupt", "mit", "ird'\u00b7schem", "Lor\u00b7beer", "sich", "be\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da brach sich, durch den Erdendunst gezogen,", "tokens": ["Da", "brach", "sich", ",", "durch", "den", "Er\u00b7den\u00b7dunst", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Kunst ihr Bild \u2013 der Sch\u00f6nheit Regenbogen.", "tokens": ["Die", "Kunst", "ihr", "Bild", "\u2013", "der", "Sch\u00f6n\u00b7heit", "Re\u00b7gen\u00b7bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$(", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch wie es kam, da\u00df jene G\u00f6tter schwanden,", "tokens": ["Doch", "wie", "es", "kam", ",", "da\u00df", "je\u00b7ne", "G\u00f6t\u00b7ter", "schwan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "KOUS", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und jene hold lebend'ge Fabelwelt,", "tokens": ["Und", "je\u00b7ne", "hold", "le\u00b7ben\u00b7d'\u00b7ge", "Fa\u00b7bel\u00b7welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Aus der das himmlische Geschlecht erstanden,", "tokens": ["Aus", "der", "das", "himm\u00b7li\u00b7sche", "Ge\u00b7schlecht", "er\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Kunst und Leben, innig sich gesellt,", "tokens": ["Und", "Kunst", "und", "Le\u00b7ben", ",", "in\u00b7nig", "sich", "ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,", "ADJD", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "An Einem Urquell, ihre Kr\u00e4nze wanden,", "tokens": ["An", "Ei\u00b7nem", "Ur\u00b7quell", ",", "ih\u00b7re", "Kr\u00e4n\u00b7ze", "wan\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Von gleicher Sehnsucht, gleicher Lust geschwellt,", "tokens": ["Von", "glei\u00b7cher", "Sehn\u00b7sucht", ",", "glei\u00b7cher", "Lust", "ge\u00b7schwellt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Verschweigen wir's an diesem Freudentage,", "tokens": ["Ver\u00b7schwei\u00b7gen", "wir's", "an", "die\u00b7sem", "Freu\u00b7den\u00b7ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Denn wo Entz\u00fccken ist, verstummt die Klage.", "tokens": ["Denn", "wo", "Ent\u00b7z\u00fc\u00b7cken", "ist", ",", "ver\u00b7stummt", "die", "Kla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NN", "VAFIN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Bist du doch unser, der zu jenen Reichen", "tokens": ["Bist", "du", "doch", "un\u00b7ser", ",", "der", "zu", "je\u00b7nen", "Rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "PPOSAT", "$,", "PRELS", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der abgeschiednen Vorwelt Wege fand,", "tokens": ["Der", "ab\u00b7ge\u00b7schied\u00b7nen", "Vor\u00b7welt", "We\u00b7ge", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Alkmenes Sohn an St\u00e4rke zu vergleichen,", "tokens": ["A\u00b7lkme\u00b7nes", "Sohn", "an", "St\u00e4r\u00b7ke", "zu", "ver\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hernieder stieg, den Schattenw\u00e4chter band,", "tokens": ["Her\u00b7nie\u00b7der", "stieg", ",", "den", "Schat\u00b7ten\u00b7w\u00e4ch\u00b7ter", "band", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dem Orpheus gleich, die Braut dir zu erreichen,", "tokens": ["Dem", "Or\u00b7pheus", "gleich", ",", "die", "Braut", "dir", "zu", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADV", "$,", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Hinaus drang bis an Lethe's Geisterstrand,", "tokens": ["Hin\u00b7aus", "drang", "bis", "an", "Lethe's", "Geis\u00b7ter\u00b7strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Und herrlich, als ein neues Fr\u00fchroth lachte,", "tokens": ["Und", "herr\u00b7lich", ",", "als", "ein", "neu\u00b7es", "Fr\u00fch\u00b7roth", "lach\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die s\u00fc\u00dfe Braut \u2013 die Kunst vom Grabe brachte.", "tokens": ["Die", "s\u00fc\u00b7\u00dfe", "Braut", "\u2013", "die", "Kunst", "vom", "Gra\u00b7be", "brach\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und wenn dein Geist in seiner Sch\u00f6pferf\u00fclle", "tokens": ["Und", "wenn", "dein", "Geist", "in", "sei\u00b7ner", "Sch\u00f6p\u00b7fer\u00b7f\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit ihr am liebsten ew'ge Kinder schafft,", "tokens": ["Mit", "ihr", "am", "liebs\u00b7ten", "ew'\u00b7ge", "Kin\u00b7der", "schafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "PTKA", "ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So stieg ihm doch aus reiner Himmelsstille", "tokens": ["So", "stieg", "ihm", "doch", "aus", "rei\u00b7ner", "Him\u00b7mels\u00b7stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Herab die zarte wunderbare Kraft,", "tokens": ["Her\u00b7ab", "die", "zar\u00b7te", "wun\u00b7der\u00b7ba\u00b7re", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die sich gezeigt in menschlich wahrer H\u00fclle,", "tokens": ["Die", "sich", "ge\u00b7zeigt", "in", "menschlich", "wah\u00b7rer", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVPP", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Der ernste Heiland, und hinweggerafft", "tokens": ["Der", "erns\u00b7te", "Hei\u00b7land", ",", "und", "hin\u00b7weg\u00b7ge\u00b7rafft"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Von seinem \u00fcberm\u00e4chtigen Erscheinen,", "tokens": ["Von", "sei\u00b7nem", "\u00fc\u00b7ber\u00b7m\u00e4ch\u00b7ti\u00b7gen", "Er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Vermochtest du zwei Welten zu vereinen.", "tokens": ["Ver\u00b7moch\u00b7test", "du", "zwei", "Wel\u00b7ten", "zu", "ver\u00b7ei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "CARD", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "La\u00df uns nur Einen hohen Wunsch, den heute", "tokens": ["La\u00df", "uns", "nur", "Ei\u00b7nen", "ho\u00b7hen", "Wunsch", ",", "den", "heu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ART", "ADJA", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die muntre Schaar vor deinem Auge hegt,", "tokens": ["Die", "mun\u00b7tre", "Schaar", "vor", "dei\u00b7nem", "Au\u00b7ge", "hegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nur Einen Stolz, der dir und uns bedeute,", "tokens": ["Nur", "Ei\u00b7nen", "Stolz", ",", "der", "dir", "und", "uns", "be\u00b7deu\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "PPER", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was uns das Herz f\u00fcrs Vaterland bewegt:", "tokens": ["Was", "uns", "das", "Herz", "f\u00fcrs", "Va\u00b7ter\u00b7land", "be\u00b7wegt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wir sind ein gutes Volk, in ew'gem Streite,", "tokens": ["Wir", "sind", "ein", "gu\u00b7tes", "Volk", ",", "in", "ew'\u00b7gem", "Strei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Voll Ernst und Kraft, von Allem angeregt,", "tokens": ["Voll", "Ernst", "und", "Kraft", ",", "von", "Al\u00b7lem", "an\u00b7ge\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "NN", "$,", "APPR", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Was Gro\u00dfes sich erzeugt in gro\u00dfen Seelen, \u2013", "tokens": ["Was", "Gro\u00b7\u00dfes", "sich", "er\u00b7zeugt", "in", "gro\u00b7\u00dfen", "See\u00b7len", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "NN", "PRF", "VVPP", "APPR", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "O la\u00df uns dich zu unserm Volke z\u0