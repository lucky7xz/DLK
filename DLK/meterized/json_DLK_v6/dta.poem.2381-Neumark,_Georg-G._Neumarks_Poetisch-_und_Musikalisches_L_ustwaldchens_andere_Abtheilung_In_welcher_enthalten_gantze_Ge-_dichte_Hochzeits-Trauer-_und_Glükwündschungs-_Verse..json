{"dta.poem.2381": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "G. Neumarks  \n Poetisch- und Musikalisches  \n   \n  L ustwaldchens  \n andere Abtheilung/  \n In welcher enthalten gantze Ge-  \n dichte/ Hochzeits-Trauer- und  \n Gl\u00fckw\u00fcndschungs-  \n Verse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Ewig Ruhm und Lob werdet Jhr davon noch", "tokens": ["E\u00b7wig", "Ruhm", "und", "Lob", "wer\u00b7det", "Ihr", "da\u00b7von", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "NN", "VAFIN", "PPER", "PAV", "ADV"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "bringen/", "tokens": ["brin\u00b7gen", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Weil/ Herr Neumark/ Jhr also trefflich wol k\u00f6nt singe\u0303/", "tokens": ["Weil", "/", "Herr", "Neu\u00b7mark", "/", "Ihr", "al\u00b7so", "treff\u00b7lich", "wol", "k\u00f6nt", "sing\u1ebd", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "NN", "NE", "$(", "PPER", "ADV", "ADJD", "ADV", "VVFIN", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df im deutschen Ronden auch durchdringet Tahl und", "tokens": ["Da\u00df", "im", "deut\u00b7schen", "Ron\u00b7den", "auch", "durch\u00b7drin\u00b7get", "Tahl", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "ADJA", "NN", "ADV", "VVFIN", "NN", "KON"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Wald/", "tokens": ["Wald", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Ja gahr durch die Wolken un\u0303 die L\u00fcfften selbsten schalt.", "tokens": ["Ja", "gahr", "durch", "die", "Wol\u00b7ken", "u\u00f1", "die", "L\u00fcff\u00b7ten", "selbs\u00b7ten", "schalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "APPR", "ART", "NN", "KON", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Solt es Menschen nicht durch die Ohr- und Hertzen", "tokens": ["Solt", "es", "Men\u00b7schen", "nicht", "durch", "die", "Ohr", "und", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "NN", "PTKNEG", "APPR", "ART", "TRUNC", "KON", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "dringen?", "tokens": ["drin\u00b7gen", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Wan jhr mit der Leyr d\u00f6rft so \u00fcberhoch euch schwin-", "tokens": ["Wan", "jhr", "mit", "der", "Leyr", "d\u00f6rft", "so", "\u00fc\u00b7ber\u00b7hoch", "euch", "schwin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "VVFIN", "ADV", "ADV", "PPER", "TRUNC"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.10": {"text": "gen/", "tokens": ["gen", "/"], "token_info": ["word", "punct"], "pos": ["APPR", "$("], "meter": "-", "measure": "single.down"}, "line.11": {"text": "Um den Lorberkrantz mit Apollo selber ringen:", "tokens": ["Um", "den", "Lor\u00b7ber\u00b7krantz", "mit", "A\u00b7pol\u00b7lo", "sel\u00b7ber", "rin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "APPR", "NE", "ADV", "VVFIN", "$."], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "(der euch gerne weichet) so bleibt euch auch dergestalt", "tokens": ["(", "der", "euch", "ger\u00b7ne", "wei\u00b7chet", ")", "so", "bleibt", "euch", "auch", "der\u00b7ge\u00b7stalt"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PRELS", "PPER", "ADV", "VVFIN", "$(", "ADV", "VVFIN", "PPER", "ADV", "ADV"], "meter": "--+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Ewig Ruhm und Lob.", "tokens": ["E\u00b7wig", "Ruhm", "und", "Lob", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.14": {"text": "Ey/ so fahret fort/ Werdt Jhr stetig so vermengen", "tokens": ["Ey", "/", "so", "fah\u00b7ret", "fort", "/", "Werdt", "Ihr", "ste\u00b7tig", "so", "ver\u00b7men\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "ADV", "VVFIN", "PTKVZ", "$(", "VAFIN", "PPER", "ADJD", "ADV", "VVINF"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "Musen und Musik/ so wird euch auch wol gelingen", "tokens": ["Mu\u00b7sen", "und", "Mu\u00b7sik", "/", "so", "wird", "euch", "auch", "wol", "ge\u00b7lin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$(", "ADV", "VAFIN", "PPER", "ADV", "ADV", "VVINF"], "meter": "+---+-+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "Vor den Momusbr\u00fcdern/ und gedoppelt wol bezahlt", "tokens": ["Vor", "den", "Mo\u00b7mus\u00b7br\u00fc\u00b7dern", "/", "und", "ge\u00b7dop\u00b7pelt", "wol", "be\u00b7zahlt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$(", "KON", "VVFIN", "ADV", "VVPP"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.17": {"text": "Euer Fleis und M\u00fche; wan auch ein-die Welt schon f\u00e4lt/", "tokens": ["Eu\u00b7er", "Fleis", "und", "M\u00fc\u00b7he", ";", "wan", "auch", "ein\u00b7die", "Welt", "schon", "f\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$.", "PWAV", "ADV", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.18": {"text": "Sag ich noch einmal/ werdet dennoch Jhr erzwingen", "tokens": ["Sag", "ich", "noch", "ein\u00b7mal", "/", "wer\u00b7det", "den\u00b7noch", "Ihr", "er\u00b7zwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "ADV", "$(", "VAFIN", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ewig Ruhm und Lob.", "tokens": ["E\u00b7wig", "Ruhm", "und", "Lob", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}