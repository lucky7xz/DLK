{"textgrid.poem.24217": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Scherzo lamentoso", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heut, mein Fr\u00e4ulein, in der Morgenstunde,", "tokens": ["Heut", ",", "mein", "Fr\u00e4u\u00b7lein", ",", "in", "der", "Mor\u00b7gen\u00b7stun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Als der Tag mit Amselschlag begunnte", "tokens": ["Als", "der", "Tag", "mit", "Am\u00b7sel\u00b7schlag", "be\u00b7gunn\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und mit vielem, ach so vielem Sonnenschein,", "tokens": ["Und", "mit", "vie\u00b7lem", ",", "ach", "so", "vie\u00b7lem", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "$,", "ITJ", "ADV", "PIS", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Fiel mir dies und das: Wie Ihre Augenbrauen", "tokens": ["Fiel", "mir", "dies", "und", "das", ":", "Wie", "Ih\u00b7re", "Au\u00b7gen\u00b7brau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PDS", "KON", "PDS", "$.", "PWAV", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Sch\u00f6n sind und wie hold sie anzuschauen,", "tokens": ["Sch\u00f6n", "sind", "und", "wie", "hold", "sie", "an\u00b7zu\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "KON", "PWAV", "ADJD", "PPER", "VVIZU", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und wie elend ich bin, fiel mir ein.", "tokens": ["Und", "wie", "e\u00b7lend", "ich", "bin", ",", "fiel", "mir", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "PPER", "VAFIN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Und mir war, als wenn mich etwas stie\u00dfe,", "tokens": ["Und", "mir", "war", ",", "als", "wenn", "mich", "et\u00b7was", "stie\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "$,", "KOKOM", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und mir war, das beste w\u00e4r, ich lie\u00dfe", "tokens": ["Und", "mir", "war", ",", "das", "bes\u00b7te", "w\u00e4r", ",", "ich", "lie\u00b7\u00dfe"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "$,", "ART", "ADJA", "VAFIN", "$,", "PPER", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Mich vom Fenster fallen nieder auf den Stein", "tokens": ["Mich", "vom", "Fens\u00b7ter", "fal\u00b7len", "nie\u00b7der", "auf", "den", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPRART", "NN", "VVFIN", "PTKVZ", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Vor der T\u00fcre. \u2013 In der Morgenstunde,", "tokens": ["Vor", "der", "T\u00fc\u00b7re", ".", "\u2013", "In", "der", "Mor\u00b7gen\u00b7stun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "$(", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Als der Tag mit Amselschlag begunnte,", "tokens": ["Als", "der", "Tag", "mit", "Am\u00b7sel\u00b7schlag", "be\u00b7gunn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Fielen Sie mir, Fr\u00e4ulein, und das ein.", "tokens": ["Fie\u00b7len", "Sie", "mir", ",", "Fr\u00e4u\u00b7lein", ",", "und", "das", "ein", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "NN", "$,", "KON", "PDS", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}