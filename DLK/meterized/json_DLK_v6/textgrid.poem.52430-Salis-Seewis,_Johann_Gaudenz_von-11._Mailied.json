{"textgrid.poem.52430": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "11. Mailied", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Apfelbaum prangt gr\u00fcn und wei\u00df", "tokens": ["Der", "Ap\u00b7fel\u00b7baum", "prangt", "gr\u00fcn", "und", "wei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf zartbegras'ter Weide;", "tokens": ["Auf", "zart\u00b7be\u00b7gras'\u00b7ter", "Wei\u00b7de", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Wonneruf des sch\u00f6nen Mais", "tokens": ["Der", "Won\u00b7ne\u00b7ruf", "des", "sch\u00f6\u00b7nen", "Mais"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weckt uns zu sanfter Freude.", "tokens": ["Weckt", "uns", "zu", "sanf\u00b7ter", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch, wird des Fr\u00fchlings Wiederkehr", "tokens": ["Doch", ",", "wird", "des", "Fr\u00fch\u00b7lings", "Wie\u00b7der\u00b7kehr"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Uns alle hier vereinen?", "tokens": ["Uns", "al\u00b7le", "hier", "ver\u00b7ei\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ach! wessen St\u00e4tte trau'rt dann leer?", "tokens": ["Ach", "!", "wes\u00b7sen", "St\u00e4t\u00b7te", "trau'rt", "dann", "leer", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAT", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und wen mu\u00df man beweinen?", "tokens": ["Und", "wen", "mu\u00df", "man", "be\u00b7wei\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Uns atmen Blumen Wohlgeruch,", "tokens": ["Uns", "at\u00b7men", "Blu\u00b7men", "Wohl\u00b7ge\u00b7ruch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Kelch und Tafel schm\u00fccken;", "tokens": ["Die", "Kelch", "und", "Ta\u00b7fel", "schm\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Noch s\u00fc\u00dfer, die am Busentuch", "tokens": ["Noch", "s\u00fc\u00b7\u00dfer", ",", "die", "am", "Bu\u00b7sen\u00b7tuch"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des holden M\u00e4dchens nicken.", "tokens": ["Des", "hol\u00b7den", "M\u00e4d\u00b7chens", "ni\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ach! Blumen, die, auf welchem Land?", "tokens": ["Ach", "!", "Blu\u00b7men", ",", "die", ",", "auf", "wel\u00b7chem", "Land", "?"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NN", "$,", "PRELS", "$,", "APPR", "PWAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Aus weichem Kraute sprie\u00dfen,", "tokens": ["Aus", "wei\u00b7chem", "Krau\u00b7te", "sprie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wird einst getreuer Freundschaft Hand", "tokens": ["Wird", "einst", "ge\u00b7treu\u00b7er", "Freund\u00b7schaft", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auf unsre H\u00fcgel gie\u00dfen!", "tokens": ["Auf", "uns\u00b7re", "H\u00fc\u00b7gel", "gie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Rose bleicht, die M\u00e4dchen kr\u00f6nt,", "tokens": ["Die", "Ro\u00b7se", "bleicht", ",", "die", "M\u00e4d\u00b7chen", "kr\u00f6nt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es bleicht der M\u00e4dchen Locke;", "tokens": ["Es", "bleicht", "der", "M\u00e4d\u00b7chen", "Lo\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In froher Hirten Fl\u00f6te t\u00f6nt", "tokens": ["In", "fro\u00b7her", "Hir\u00b7ten", "Fl\u00f6\u00b7te", "t\u00f6nt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Dorfes Totenglocke;", "tokens": ["Des", "Dor\u00b7fes", "To\u00b7ten\u00b7glo\u00b7cke", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Jugend tanzt, im Abendlicht,", "tokens": ["Die", "Ju\u00b7gend", "tanzt", ",", "im", "A\u00b7bend\u00b7licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Froh um des Platzes Maie;", "tokens": ["Froh", "um", "des", "Plat\u00b7zes", "Mai\u00b7e", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Doch ihren Reigen unterbricht", "tokens": ["Doch", "ih\u00b7ren", "Rei\u00b7gen", "un\u00b7ter\u00b7bricht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Grabgeleiter Reihe.", "tokens": ["Der", "Grab\u00b7ge\u00b7lei\u00b7ter", "Rei\u00b7he", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der stille Vollmond schien so klar", "tokens": ["Der", "stil\u00b7le", "Voll\u00b7mond", "schien", "so", "klar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch bl\u00fchende Syringen,", "tokens": ["Durch", "bl\u00fc\u00b7hen\u00b7de", "Sy\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.3": {"text": "Wo j\u00fcngst Verlobte, Paar und Paar,", "tokens": ["Wo", "j\u00fcngst", "Ver\u00b7lob\u00b7te", ",", "Paar", "und", "Paar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In lauer D\u00e4mm'rung gingen;", "tokens": ["In", "lau\u00b7er", "D\u00e4m\u00b7m'\u00b7rung", "gin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Seitdem erscholl vom Turm herab", "tokens": ["Seit\u00b7dem", "er\u00b7scholl", "vom", "Turm", "her\u00b7ab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ADJD", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das traurige Gel\u00e4ute;", "tokens": ["Das", "trau\u00b7ri\u00b7ge", "Ge\u00b7l\u00e4u\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der Mond bescheint das frische Grab", "tokens": ["Der", "Mond", "be\u00b7scheint", "das", "fri\u00b7sche", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der fr\u00fch gestorb'nen Br\u00e4ute.", "tokens": ["Der", "fr\u00fch", "ge\u00b7stor\u00b7b'\u00b7nen", "Br\u00e4u\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Gef\u00e4hrten, ach! die Stunde naht,", "tokens": ["Ge\u00b7f\u00e4hr\u00b7ten", ",", "ach", "!", "die", "Stun\u00b7de", "naht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ITJ", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo wir auch m\u00fcssen scheiden!", "tokens": ["Wo", "wir", "auch", "m\u00fcs\u00b7sen", "schei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bestreut indes den kurzen Pfad", "tokens": ["Be\u00b7streut", "in\u00b7des", "den", "kur\u00b7zen", "Pfad"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Bl\u00fcten reiner Freuden.", "tokens": ["Mit", "Bl\u00fc\u00b7ten", "rei\u00b7ner", "Freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Seid gut! Der Unschuld strahlt das Ziel,", "tokens": ["Seid", "gut", "!", "Der", "Un\u00b7schuld", "strahlt", "das", "Ziel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$.", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von Abendrot umgeben,", "tokens": ["Von", "A\u00b7ben\u00b7drot", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und jedes edlere Gef\u00fchl", "tokens": ["Und", "je\u00b7des", "ed\u00b7le\u00b7re", "Ge\u00b7f\u00fchl"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Folgt uns zum bessern Leben.", "tokens": ["Folgt", "uns", "zum", "bes\u00b7sern", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}