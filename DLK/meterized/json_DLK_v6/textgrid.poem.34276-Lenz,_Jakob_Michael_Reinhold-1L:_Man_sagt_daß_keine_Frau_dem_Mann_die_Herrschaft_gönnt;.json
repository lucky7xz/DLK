{"textgrid.poem.34276": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Man sagt da\u00df keine Frau dem Mann die Herrschaft g\u00f6nnt;", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man sagt da\u00df keine Frau dem Mann die Herrschaft g\u00f6nnt;", "tokens": ["Man", "sagt", "da\u00df", "kei\u00b7ne", "Frau", "dem", "Mann", "die", "Herr\u00b7schaft", "g\u00f6nnt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KOUS", "PIAT", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So nicht Frau Magdelone.", "tokens": ["So", "nicht", "Frau", "Mag\u00b7de\u00b7lo\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "NN", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie theilt mit ihm das Regiment:", "tokens": ["Sie", "theilt", "mit", "ihm", "das", "Re\u00b7gi\u00b7ment", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Beh\u00e4lt den Scepter nur und l\u00e4\u00dfet ihm die Krone.", "tokens": ["Be\u00b7h\u00e4lt", "den", "Scep\u00b7ter", "nur", "und", "l\u00e4\u00b7\u00dfet", "ihm", "die", "Kro\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}