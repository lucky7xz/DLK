{"textgrid.poem.25219": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: So heimlich s\u00fc\u00df war unsre Hochzeitsfeier:", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So heimlich s\u00fc\u00df war unsre Hochzeitsfeier:", "tokens": ["So", "heim\u00b7lich", "s\u00fc\u00df", "war", "uns\u00b7re", "Hoch\u00b7zeits\u00b7fei\u00b7er", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir lagen dicht", "tokens": ["Wir", "la\u00b7gen", "dicht"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Beisammen, \u00fcberwallt von einem Schleier;", "tokens": ["Bei\u00b7sam\u00b7men", ",", "\u00fc\u00b7berw\u00b7allt", "von", "ei\u00b7nem", "Schlei\u00b7er", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Man sah uns nicht.", "tokens": ["Man", "sah", "uns", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wir h\u00f6rten, wie die Leute nach uns fragten", "tokens": ["Wir", "h\u00f6r\u00b7ten", ",", "wie", "die", "Leu\u00b7te", "nach", "uns", "frag\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im gleichen Raum.", "tokens": ["Im", "glei\u00b7chen", "Raum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wir unterm Flore blieben reglos, wagten", "tokens": ["Wir", "un\u00b7term", "Flo\u00b7re", "blie\u00b7ben", "reg\u00b7los", ",", "wag\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "APPRART", "NN", "VVFIN", "ADJD", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu atmen kaum.", "tokens": ["Zu", "at\u00b7men", "kaum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Nur unsre H\u00e4nde durften sacht sich dr\u00fccken,", "tokens": ["Nur", "uns\u00b7re", "H\u00e4n\u00b7de", "durf\u00b7ten", "sacht", "sich", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VMFIN", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie k\u00fcssend fand", "tokens": ["Wie", "k\u00fcs\u00b7send", "fand"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Sich Hauch zu Hauch, mein Knie war mit Entz\u00fccken", "tokens": ["Sich", "Hauch", "zu", "Hauch", ",", "mein", "Knie", "war", "mit", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PRF", "NN", "APPR", "NN", "$,", "PPOSAT", "NN", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An deins gebannt.", "tokens": ["An", "deins", "ge\u00b7bannt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Mein gl\u00fchend Auge, das im Dunkeln schaute,", "tokens": ["Mein", "gl\u00fc\u00b7hend", "Au\u00b7ge", ",", "das", "im", "Dun\u00b7keln", "schau\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$,", "PRELS", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Versank in deins;", "tokens": ["Ver\u00b7sank", "in", "deins", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ich war in dir, du warst in mir, uns traute", "tokens": ["Ich", "war", "in", "dir", ",", "du", "warst", "in", "mir", ",", "uns", "trau\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "$,", "PPER", "VAFIN", "APPR", "PPER", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die heilige Eins.", "tokens": ["Die", "hei\u00b7li\u00b7ge", "Eins", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Wohlan, was Edens Glut zusammengl\u00fchte,", "tokens": ["Wo\u00b7hlan", ",", "was", "E\u00b7dens", "Glut", "zu\u00b7sam\u00b7men\u00b7gl\u00fch\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PRELS", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Trennt keine Welt.", "tokens": ["Trennt", "kei\u00b7ne", "Welt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Hinweg denn, Angst, da uns die Hand der G\u00fcte", "tokens": ["Hin\u00b7weg", "denn", ",", "Angst", ",", "da", "uns", "die", "Hand", "der", "G\u00fc\u00b7te"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "NN", "$,", "KOUS", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Geborgen h\u00e4lt.", "tokens": ["Ge\u00b7bor\u00b7gen", "h\u00e4lt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Wir ruhn verh\u00fcllt; zum Baldachin, zum Himmel", "tokens": ["Wir", "ruhn", "ver\u00b7h\u00fcllt", ";", "zum", "Bal\u00b7da\u00b7ch\u00b7in", ",", "zum", "Him\u00b7mel"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "VVPP", "$.", "APPRART", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ward unser Flor.", "tokens": ["Ward", "un\u00b7ser", "Flor", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Uns singt von Fl\u00fcgelk\u00f6pfchen ein Gewimmel", "tokens": ["Uns", "singt", "von", "Fl\u00fc\u00b7gel\u00b7k\u00f6pf\u00b7chen", "ein", "Ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Minnechor.", "tokens": ["Den", "Min\u00b7ne\u00b7chor", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}