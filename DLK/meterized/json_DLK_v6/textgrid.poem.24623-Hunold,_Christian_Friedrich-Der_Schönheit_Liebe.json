{"textgrid.poem.24623": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Sch\u00f6nheit Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Liebe geht nach dem/ was Liebens-w\u00fcrdig ist.", "tokens": ["Die", "Lie\u00b7be", "geht", "nach", "dem", "/", "was", "Lie\u00b7bens\u00b7w\u00fcr\u00b7dig", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "$(", "PWS", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So lange du Natur des Geistes Wohnung bist/", "tokens": ["So", "lan\u00b7ge", "du", "Na\u00b7tur", "des", "Geis\u00b7tes", "Woh\u00b7nung", "bist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "NN", "ART", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So lange noch der Geist Vollkommenheiten achtet/", "tokens": ["So", "lan\u00b7ge", "noch", "der", "Geist", "Voll\u00b7kom\u00b7men\u00b7hei\u00b7ten", "ach\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wird ohne Regung nicht der Sch\u00f6nheit Werck betrachtet.", "tokens": ["Wird", "oh\u00b7ne", "Re\u00b7gung", "nicht", "der", "Sch\u00f6n\u00b7heit", "Werck", "be\u00b7trach\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "PTKNEG", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dr\u00fcm halte Tugend ein/ dein Urtheil ist zu scharff/", "tokens": ["Dr\u00fcm", "hal\u00b7te", "Tu\u00b7gend", "ein", "/", "dein", "Ur\u00b7theil", "ist", "zu", "scharff", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "ART", "$(", "PPOSAT", "NN", "VAFIN", "PTKA", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil Tugend der Natur/ die herrlich ist/ bedarff.", "tokens": ["Weil", "Tu\u00b7gend", "der", "Na\u00b7tur", "/", "die", "herr\u00b7lich", "ist", "/", "be\u00b7darff", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "$(", "ART", "ADJD", "VAFIN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Aus Liebe reget uns die Seele Leib und Glieder/", "tokens": ["Aus", "Lie\u00b7be", "re\u00b7get", "uns", "die", "See\u00b7le", "Leib", "und", "Glie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und Liebe liebet nie/ was der Natur zu wieder.", "tokens": ["Und", "Lie\u00b7be", "lie\u00b7bet", "nie", "/", "was", "der", "Na\u00b7tur", "zu", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "$(", "PWS", "ART", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}