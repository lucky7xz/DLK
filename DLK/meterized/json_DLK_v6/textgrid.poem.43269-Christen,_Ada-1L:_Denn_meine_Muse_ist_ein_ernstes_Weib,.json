{"textgrid.poem.43269": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Denn meine Muse ist ein ernstes Weib,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Denn meine Muse ist ein ernstes Weib,", "tokens": ["Denn", "mei\u00b7ne", "Mu\u00b7se", "ist", "ein", "erns\u00b7tes", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das mich nicht aufsucht, um mit mir zu scherzen,", "tokens": ["Das", "mich", "nicht", "auf\u00b7sucht", ",", "um", "mit", "mir", "zu", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PTKNEG", "VVPP", "$,", "KOUI", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Das nicht mit Flitter sich beh\u00e4ngt den Leib,", "tokens": ["Das", "nicht", "mit", "Flit\u00b7ter", "sich", "be\u00b7h\u00e4ngt", "den", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "APPR", "NN", "PRF", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das jedes Lied holt aus gepr\u00fcftem Herzen.", "tokens": ["Das", "je\u00b7des", "Lied", "holt", "aus", "ge\u00b7pr\u00fcf\u00b7tem", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn sie den Schleier stolz vom Haupte zieht", "tokens": ["Wenn", "sie", "den", "Schlei\u00b7er", "stolz", "vom", "Haup\u00b7te", "zieht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und mich ihr Antlitz l\u00e4\u00dft, ihr wei\u00dfes, schauen,", "tokens": ["Und", "mich", "ihr", "Ant\u00b7litz", "l\u00e4\u00dft", ",", "ihr", "wei\u00b7\u00dfes", ",", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "NN", "VVFIN", "$,", "PPOSAT", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann f\u00fchle j\u00e4h ich, wie die Freude flieht,", "tokens": ["Dann", "f\u00fch\u00b7le", "j\u00e4h", "ich", ",", "wie", "die", "Freu\u00b7de", "flieht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PPER", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und meine Seele fasset hehres Grauen.", "tokens": ["Und", "mei\u00b7ne", "See\u00b7le", "fas\u00b7set", "heh\u00b7res", "Grau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich schreite stumm an ihrer Hand den Pfad,", "tokens": ["Ich", "schrei\u00b7te", "stumm", "an", "ih\u00b7rer", "Hand", "den", "Pfad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der tief hinabf\u00fchrt zu dem schwarzen Flusse,", "tokens": ["Der", "tief", "hin\u00b7ab\u00b7f\u00fchrt", "zu", "dem", "schwar\u00b7zen", "Flus\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und lausche, wenn sie dem Gew\u00e4sser naht,", "tokens": ["Und", "lau\u00b7sche", ",", "wenn", "sie", "dem", "Ge\u00b7w\u00e4s\u00b7ser", "naht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+++-+-+", "measure": "zehnsilber"}, "line.4": {"text": "Ersch\u00fcttert ihrem trauervollen Gru\u00dfe,", "tokens": ["Er\u00b7sch\u00fct\u00b7tert", "ih\u00b7rem", "trau\u00b7er\u00b7vol\u00b7len", "Gru\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Den sie zu H\u00f6rigen der M\u00fchsal schickt,", "tokens": ["Den", "sie", "zu", "H\u00f6\u00b7ri\u00b7gen", "der", "M\u00fch\u00b7sal", "schickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die dr\u00fcben an dem kahlen Ufer harren,", "tokens": ["Die", "dr\u00fc\u00b7ben", "an", "dem", "kah\u00b7len", "U\u00b7fer", "har\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von wo der Gl\u00fccklose her\u00fcberblickt,", "tokens": ["Von", "wo", "der", "Gl\u00fcck\u00b7lo\u00b7se", "her\u00b7\u00fc\u00b7berb\u00b7lickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der viel gehofft vor langen, langen Jahren.", "tokens": ["Der", "viel", "ge\u00b7hofft", "vor", "lan\u00b7gen", ",", "lan\u00b7gen", "Jah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}