{"textgrid.poem.64815": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich dichte nicht in frohen Stunden \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich dichte nicht in frohen Stunden \u2013", "tokens": ["Ich", "dich\u00b7te", "nicht", "in", "fro\u00b7hen", "Stun\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Leben ist an solchen leer!", "tokens": ["Mein", "Le\u00b7ben", "ist", "an", "sol\u00b7chen", "leer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "PIAT", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich dichte nicht, um zu gesunden \u2013", "tokens": ["Ich", "dich\u00b7te", "nicht", ",", "um", "zu", "ge\u00b7sun\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUI", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Genesung gibts f\u00fcr mich nicht mehr.", "tokens": ["Ge\u00b7ne\u00b7sung", "gibts", "f\u00fcr", "mich", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich dichte nicht, um zu erstreben", "tokens": ["Ich", "dich\u00b7te", "nicht", ",", "um", "zu", "er\u00b7stre\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUI", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Ruhmes glei\u00dfnerische Pracht,", "tokens": ["Des", "Ruh\u00b7mes", "glei\u00df\u00b7ne\u00b7ri\u00b7sche", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die, statt Unsterblichkeit zu geben,", "tokens": ["Die", ",", "statt", "U\u00b7nsterb\u00b7lich\u00b7keit", "zu", "ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUI", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Ein zweites Mal nur sterben macht.", "tokens": ["Ein", "zwei\u00b7tes", "Mal", "nur", "ster\u00b7ben", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich dichte nicht, um mich zu kr\u00f6nen", "tokens": ["Ich", "dich\u00b7te", "nicht", ",", "um", "mich", "zu", "kr\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUI", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit meiner Leiden Dorngeflecht;", "tokens": ["Mit", "mei\u00b7ner", "Lei\u00b7den", "Dorn\u00b7ge\u00b7flecht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Menge w\u00fcrde mich verh\u00f6hnen", "tokens": ["Die", "Men\u00b7ge", "w\u00fcr\u00b7de", "mich", "ver\u00b7h\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sprechen: Es geschah Dir Recht!", "tokens": ["Und", "spre\u00b7chen", ":", "Es", "ge\u00b7schah", "Dir", "Recht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$.", "PPER", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mein Lieb quillt aus demselben Borne,", "tokens": ["Mein", "Lieb", "quillt", "aus", "dem\u00b7sel\u00b7ben", "Bor\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus dem das Wort der Pythia brach,", "tokens": ["Aus", "dem", "das", "Wort", "der", "Py\u00b7thia", "brach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als rauh und wild im Siegerzorne", "tokens": ["Als", "rauh", "und", "wild", "im", "Sie\u00b7ger\u00b7zor\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "KON", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Macedonier zu ihr sprach.", "tokens": ["Der", "Ma\u00b7ce\u00b7do\u00b7nier", "zu", "ihr", "sprach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Des Schicksals nachtumflorten Willen,", "tokens": ["Des", "Schick\u00b7sals", "nach\u00b7tum\u00b7flor\u00b7ten", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Zukunft keimevollen Grund", "tokens": ["Der", "Zu\u00b7kunft", "kei\u00b7me\u00b7vol\u00b7len", "Grund"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sollt' ihm ihr Seherspruch enth\u00fcllen,", "tokens": ["Sollt'", "ihm", "ihr", "Se\u00b7her\u00b7spruch", "ent\u00b7h\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Allein verschlossen blieb ihr Mund.", "tokens": ["Al\u00b7lein", "ver\u00b7schlos\u00b7sen", "blieb", "ihr", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch nichts kann sein Verlangen wenden,", "tokens": ["Doch", "nichts", "kann", "sein", "Ver\u00b7lan\u00b7gen", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nichts beugen seinen starren Sinn!", "tokens": ["Nichts", "beu\u00b7gen", "sei\u00b7nen", "star\u00b7ren", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVINF", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit frevelhaft vermessnen H\u00e4nden", "tokens": ["Mit", "fre\u00b7vel\u00b7haft", "ver\u00b7mess\u00b7nen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fa\u00dft er die bleiche Priesterin.", "tokens": ["Fa\u00dft", "er", "die", "blei\u00b7che", "Pries\u00b7te\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Zum Schlunde, dunkel, unergr\u00fcndlich,", "tokens": ["Zum", "Schlun\u00b7de", ",", "dun\u00b7kel", ",", "un\u00b7er\u00b7gr\u00fcnd\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dr\u00e4ngt er sie z\u00fcrnend mit Gewalt,", "tokens": ["Dr\u00e4ngt", "er", "sie", "z\u00fcr\u00b7nend", "mit", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis: \u00bbJa! du bist un\u00fcberwindlich!\u00ab", "tokens": ["Bis", ":", "\u00bb", "Ja", "!", "du", "bist", "un\u00b7\u00fc\u00b7berw\u00b7ind\u00b7lich", "!", "\u00ab"], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "$.", "$(", "PTKANT", "$.", "PPER", "VAFIN", "ADJD", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie angst- und zorndurchschauert lallt. \u2013", "tokens": ["Sie", "angst", "und", "zorn\u00b7durch\u00b7schau\u00b7ert", "lallt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "TRUNC", "KON", "VVFIN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "So ward', was jemals ich gesungen,", "tokens": ["So", "ward'", ",", "was", "je\u00b7mals", "ich", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "PRELS", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Blick gerichtet himmelw\u00e4rts,", "tokens": ["Den", "Blick", "ge\u00b7rich\u00b7tet", "him\u00b7mel\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "ADV", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Mir nur erpre\u00dft und abgedrungen", "tokens": ["Mir", "nur", "er\u00b7pre\u00dft", "und", "ab\u00b7ge\u00b7drun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "KON", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vom wilden Ueberwinder, ", "tokens": ["Vom", "wil\u00b7den", "Ue\u00b7ber\u00b7win\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}