{"textgrid.poem.43559": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Freue dich, mein liebes Herz!", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Singt ein Vogel hoch im Baume \u2013", "tokens": ["Singt", "ein", "Vo\u00b7gel", "hoch", "im", "Bau\u00b7me", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "APPRART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sei gegr\u00fc\u00dft, Frau Nachtigall!", "tokens": ["Sei", "ge\u00b7gr\u00fc\u00dft", ",", "Frau", "Nach\u00b7ti\u00b7gall", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nachtigall,", "tokens": ["Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Sch\u00f6ner Schall!", "tokens": ["Sch\u00f6\u00b7ner", "Schall", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Frohe Herzen giebt es \u00fcberall.", "tokens": ["Fro\u00b7he", "Her\u00b7zen", "giebt", "es", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Und ich wandre meines Weges", "tokens": ["Und", "ich", "wand\u00b7re", "mei\u00b7nes", "We\u00b7ges"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch das sch\u00f6ne gr\u00fcne Feld.", "tokens": ["Durch", "das", "sch\u00f6\u00b7ne", "gr\u00fc\u00b7ne", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gr\u00fcnes Feld", "tokens": ["Gr\u00fc\u00b7nes", "Feld"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Mir gef\u00e4llt,", "tokens": ["Mir", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Mir gef\u00e4llt die ganze Gotteswelt!", "tokens": ["Mir", "ge\u00b7f\u00e4llt", "die", "gan\u00b7ze", "Got\u00b7tes\u00b7welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Dankbar nimm, was Gott gegeben,", "tokens": ["Dank\u00b7bar", "nimm", ",", "was", "Gott", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "PRELS", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Freue dich, mein liebes Herz!", "tokens": ["Freu\u00b7e", "dich", ",", "mein", "lie\u00b7bes", "Herz", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Liebes Herz,", "tokens": ["Lie\u00b7bes", "Herz", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Allerw\u00e4rts", "tokens": ["Al\u00b7ler\u00b7w\u00e4rts"], "token_info": ["word"], "pos": ["ADV"], "meter": "+--", "measure": "dactylic.init"}, "line.5": {"text": "Wechselt hier auf Erden Freud' und Schmerz.", "tokens": ["Wech\u00b7selt", "hier", "auf", "Er\u00b7den", "Freud'", "und", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}