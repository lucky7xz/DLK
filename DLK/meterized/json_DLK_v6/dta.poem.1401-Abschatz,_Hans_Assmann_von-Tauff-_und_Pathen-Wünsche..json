{"dta.poem.1401": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Tauff- und Pathen-W\u00fcnsche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn Wasser durch das Wort des HErren wird geweyht/", "tokens": ["Wenn", "Was\u00b7ser", "durch", "das", "Wort", "des", "Her\u00b7ren", "wird", "ge\u00b7weyht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "(die beyden sind der Grund/) so wird ein Bad bereit/", "tokens": ["(", "die", "bey\u00b7den", "sind", "der", "Grund", "/", ")", "so", "wird", "ein", "Bad", "be\u00b7reit", "/"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "PIAT", "VAFIN", "ART", "NN", "$(", "$(", "ADV", "VAFIN", "ART", "NE", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das unsre zarte Seel ererbter Schuld befreyt/", "tokens": ["Das", "uns\u00b7re", "zar\u00b7te", "Seel", "er\u00b7erb\u00b7ter", "Schuld", "be\u00b7freyt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "ADJA", "NN", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Uns unser Lebenslang zu Nutz und Trost gedeyht:", "tokens": ["Uns", "un\u00b7ser", "Le\u00b7bens\u00b7lang", "zu", "Nutz", "und", "Trost", "ge\u00b7deyht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Di\u00df/ Pathe/ bringe dir auch Heyl und Seligkeit.", "tokens": ["Di\u00df", "/", "Pa\u00b7the", "/", "brin\u00b7ge", "dir", "auch", "Heyl", "und", "Se\u00b7lig\u00b7keit", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$(", "NE", "$(", "VVFIN", "PPER", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}