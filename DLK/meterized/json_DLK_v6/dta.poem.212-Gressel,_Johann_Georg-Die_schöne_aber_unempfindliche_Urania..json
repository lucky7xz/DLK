{"dta.poem.212": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die sch\u00f6ne/ aber unempfindliche  \n  Urania.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "So wie Aurora pflegt mit ihren Rosen-Wangen", "tokens": ["So", "wie", "Au\u00b7ro\u00b7ra", "pflegt", "mit", "ih\u00b7ren", "Ro\u00b7sen\u00b7Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "NE", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bey fr\u00fcher Tages-Zeit an grauer Wolck zu prangen/", "tokens": ["Bey", "fr\u00fc\u00b7her", "Ta\u00b7ges\u00b7Zeit", "an", "grau\u00b7er", "Wolck", "zu", "pran\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "PTKZU", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So wie die Sonne scheint wenn sie aus ", "tokens": ["So", "wie", "die", "Son\u00b7ne", "scheint", "wenn", "sie", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "VVFIN", "KOUS", "PPER", "PTKVZ"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "In Purpur eingeh\u00fcllt mit munterm Lauf aufsteht;", "tokens": ["In", "Pur\u00b7pur", "ein\u00b7ge\u00b7h\u00fcllt", "mit", "mun\u00b7term", "Lauf", "auf\u00b7steht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So wie ein Rosen-Stock mit halb er\u00f6ffnten Rosen", "tokens": ["So", "wie", "ein", "Ro\u00b7sen\u00b7Stock", "mit", "halb", "er\u00b7\u00f6ffn\u00b7ten", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Bey k\u00fchler Morgen-Zeit den Thau pflegt liebzukosen/", "tokens": ["Bey", "k\u00fch\u00b7ler", "Mor\u00b7gen\u00b7Zeit", "den", "Thau", "pflegt", "lieb\u00b7zu\u00b7ko\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So prangt ", "tokens": ["So", "prangt"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Den Alabaster Leib aus weichen Federn tr\u00e4gt.", "tokens": ["Den", "A\u00b7la\u00b7bas\u00b7ter", "Leib", "aus", "wei\u00b7chen", "Fe\u00b7dern", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Wangen stehen weg den lichten Rosen-Morgen/", "tokens": ["Die", "Wan\u00b7gen", "ste\u00b7hen", "weg", "den", "lich\u00b7ten", "Ro\u00b7sen\u00b7Mor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Von ihren Augen mu\u00df die Nacht die Schw\u00e4rtze borgen/", "tokens": ["Von", "ih\u00b7ren", "Au\u00b7gen", "mu\u00df", "die", "Nacht", "die", "Schw\u00e4rt\u00b7ze", "bor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der Sonnen Purpur weicht vor den Rubinen-Mund/", "tokens": ["Der", "Son\u00b7nen", "Pur\u00b7pur", "weicht", "vor", "den", "Ru\u00b7bi\u00b7nen\u00b7Mund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Und h\u00fcllt sich/ als besch\u00e4mt/ in ", "tokens": ["Und", "h\u00fcllt", "sich", "/", "als", "be\u00b7sch\u00e4mt", "/", "in"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "PRF", "$(", "KOUS", "ADJD", "$(", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Narcissen kriechen weg/ vor denen klahren Br\u00fcsten/", "tokens": ["Nar\u00b7cis\u00b7sen", "krie\u00b7chen", "weg", "/", "vor", "de\u00b7nen", "klah\u00b7ren", "Br\u00fcs\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$(", "APPR", "PRELS", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Den Liljen fehlet selbst das Wollen und Gel\u00fcsten/", "tokens": ["Den", "Lil\u00b7jen", "feh\u00b7let", "selbst", "das", "Wol\u00b7len", "und", "Ge\u00b7l\u00fcs\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "An Klahrheit siegt der Leib den heitern Himmel an;", "tokens": ["An", "Klahr\u00b7heit", "siegt", "der", "Leib", "den", "hei\u00b7tern", "Him\u00b7mel", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Was irrets? da\u00df man sie nicht Himmel nennen kan.", "tokens": ["Was", "ir\u00b7rets", "?", "da\u00df", "man", "sie", "nicht", "Him\u00b7mel", "nen\u00b7nen", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "$.", "KOUS", "PIS", "PPER", "PTKNEG", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Vor ihren Strahlen mu\u00df der Strahl der Sonnen weichen/", "tokens": ["Vor", "ih\u00b7ren", "Strah\u00b7len", "mu\u00df", "der", "Strahl", "der", "Son\u00b7nen", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "In dessen Feur zu sehn durch Witz man kan erreichen ", "tokens": ["In", "des\u00b7sen", "Feur", "zu", "sehn", "durch", "Witz", "man", "kan", "er\u00b7rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "PTKZU", "VVINF", "APPR", "NN", "PIS", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Allein wer bleibet hier mit Adlers Augen stehn/", "tokens": ["Al\u00b7lein", "wer", "blei\u00b7bet", "hier", "mit", "Ad\u00b7lers", "Au\u00b7gen", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "ADV", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wer unterwindet sich die Augen anzusehn?", "tokens": ["Wer", "un\u00b7ter\u00b7win\u00b7det", "sich", "die", "Au\u00b7gen", "an\u00b7zu\u00b7sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der nicht so gleich sein Hertz in heisser Gluht befindet/", "tokens": ["Der", "nicht", "so", "gleich", "sein", "Hertz", "in", "heis\u00b7ser", "Gluht", "be\u00b7fin\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADV", "ADV", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die ihrer Augen-Blitz darinnen angez\u00fcndet.", "tokens": ["Die", "ih\u00b7rer", "Au\u00b7gen\u00b7Blitz", "da\u00b7rin\u00b7nen", "an\u00b7ge\u00b7z\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Sie aber bleibet Stahl/ ihr Hertze gleicht dem Ey\u00df/", "tokens": ["Sie", "a\u00b7ber", "blei\u00b7bet", "Stahl", "/", "ihr", "Hert\u00b7ze", "gleicht", "dem", "Ey\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "NN", "$(", "PPER", "VVFIN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Das keine Flammen f\u00e4ngt/ und nichts vom Lieben wei\u00df:", "tokens": ["Das", "kei\u00b7ne", "Flam\u00b7men", "f\u00e4ngt", "/", "und", "nichts", "vom", "Lie\u00b7ben", "wei\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$(", "KON", "PIS", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Dem auch der Kieselstein an der Empfindung weichet/", "tokens": ["Dem", "auch", "der", "Kie\u00b7sel\u00b7stein", "an", "der", "Emp\u00b7fin\u00b7dung", "wei\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Daran ein Regen-Tropff doch mit der Zeit erreichet/", "tokens": ["Da\u00b7ran", "ein", "Re\u00b7gen\u00b7Tropff", "doch", "mit", "der", "Zeit", "er\u00b7rei\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Was ein geschwollner Bach in eile nicht verricht/", "tokens": ["Was", "ein", "ge\u00b7schwoll\u00b7ner", "Bach", "in", "ei\u00b7le", "nicht", "ver\u00b7richt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "APPR", "VVFIN", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Allein sie bleibet hart/ mein Weinen n\u00fctzt mir nicht.", "tokens": ["Al\u00b7lein", "sie", "blei\u00b7bet", "hart", "/", "mein", "Wei\u00b7nen", "n\u00fctzt", "mir", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADJD", "$(", "PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Der Diamant zerspringt in schlechter Thiere Blute", "tokens": ["Der", "Di\u00b7a\u00b7mant", "zer\u00b7springt", "in", "schlech\u00b7ter", "Thie\u00b7re", "Blu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Sie aber wird erh\u00e4rt von meiner Adern-Glute/", "tokens": ["Sie", "a\u00b7ber", "wird", "er\u00b7h\u00e4rt", "von", "mei\u00b7ner", "A\u00b7dern\u00b7Glu\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "VVPP", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Und \u00fcbertrifft darinn den Stein und Diamant/", "tokens": ["Und", "\u00fc\u00b7bert\u00b7rifft", "da\u00b7rinn", "den", "Stein", "und", "Di\u00b7a\u00b7mant", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "ART", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Da\u00df Blut und Thr\u00e4nen sind umsonst an ihr gewandt.", "tokens": ["Da\u00df", "Blut", "und", "Thr\u00e4\u00b7nen", "sind", "um\u00b7sonst", "an", "ihr", "ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VAFIN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}