{"textgrid.poem.61900": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "2. Nordisch", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Luft ist grau und grau das Meer,", "tokens": ["Die", "Luft", "ist", "grau", "und", "grau", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Wind fegt pfeifend dr\u00fcber her,", "tokens": ["Der", "Wind", "fegt", "pfei\u00b7fend", "dr\u00fc\u00b7ber", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PAV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die M\u00f6we kreischt, die Brandung wallt, \u2013", "tokens": ["Die", "M\u00f6\u00b7we", "kreischt", ",", "die", "Bran\u00b7dung", "wallt", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie ward mein Herz so sterbensalt!", "tokens": ["Wie", "ward", "mein", "Herz", "so", "ster\u00b7ben\u00b7salt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Traurig rinnen die Tage.", "tokens": ["Trau\u00b7rig", "rin\u00b7nen", "die", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.2": {"line.1": {"text": "Wohl hab' ich andre Zeit gekannt,", "tokens": ["Wohl", "hab'", "ich", "and\u00b7re", "Zeit", "ge\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir fuhren im Nachen, Hand in Hand,", "tokens": ["Wir", "fuh\u00b7ren", "im", "Na\u00b7chen", ",", "Hand", "in", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das Meer war blau, die Sonne schien,", "tokens": ["Das", "Meer", "war", "blau", ",", "die", "Son\u00b7ne", "schien", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich sah und wu\u00dfte nichts als ihn;", "tokens": ["Ich", "sah", "und", "wu\u00df\u00b7te", "nichts", "als", "ihn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PIS", "KOKOM", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Selig waren die Tage.", "tokens": ["Se\u00b7lig", "wa\u00b7ren", "die", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "Nun liegt der Kahn und fault am Strand,", "tokens": ["Nun", "liegt", "der", "Kahn", "und", "fault", "am", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er aber ging ins fremde Land,", "tokens": ["Er", "a\u00b7ber", "ging", "ins", "frem\u00b7de", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er ging, ein hohes Weib zu frein, \u2013", "tokens": ["Er", "ging", ",", "ein", "ho\u00b7hes", "Weib", "zu", "frein", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "PTKZU", "PTKVZ", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gott geb' ihm Gl\u00fcck! Das Leid ist mein.", "tokens": ["Gott", "geb'", "ihm", "Gl\u00fcck", "!", "Das", "Leid", "ist", "mein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "$.", "ART", "NN", "VAFIN", "PPOSAT", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Traurig rinnen die Tage.", "tokens": ["Trau\u00b7rig", "rin\u00b7nen", "die", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}