{"textgrid.poem.48397": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herr, es ist lange satt, da\u00df ich dich nicht gesehen.", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr, es ist lange satt, da\u00df ich dich nicht gesehen.", "tokens": ["Herr", ",", "es", "ist", "lan\u00b7ge", "satt", ",", "da\u00df", "ich", "dich", "nicht", "ge\u00b7se\u00b7hen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,", "KOUS", "PPER", "PRF", "PTKNEG", "VVPP", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Was mir f\u00fcr K\u00fcmmern\u00fc\u00df dar\u00fcber ist geschehen,", "tokens": ["Was", "mir", "f\u00fcr", "K\u00fcm\u00b7mer\u00b7n\u00fc\u00df", "da\u00b7r\u00fc\u00b7ber", "ist", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "PAV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "wie Angst mir itzt noch ist, das wei\u00df nur ich und du,", "tokens": ["wie", "Angst", "mir", "itzt", "noch", "ist", ",", "das", "wei\u00df", "nur", "ich", "und", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "ADV", "ADV", "VAFIN", "$,", "PDS", "VVFIN", "ADV", "PPER", "KON", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "wir beide wissens nur. Ach, mein Herr, siehe zu,", "tokens": ["wir", "bei\u00b7de", "wis\u00b7sens", "nur", ".", "Ach", ",", "mein", "Herr", ",", "sie\u00b7he", "zu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "ADV", "$.", "ITJ", "$,", "PPOSAT", "NN", "$,", "VVIMP", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "da\u00df mir dein Absein nicht die halbverzehrte Seele,", "tokens": ["da\u00df", "mir", "dein", "Ab\u00b7sein", "nicht", "die", "halb\u00b7ver\u00b7zehr\u00b7te", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "die so nach dir verlangt, bis auf das Sterben qu\u00e4le!", "tokens": ["die", "so", "nach", "dir", "ver\u00b7langt", ",", "bis", "auf", "das", "Ster\u00b7ben", "qu\u00e4\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "VVFIN", "$,", "KOUS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Erzeige dich, mein Arzt! Der wenigste Verzug", "tokens": ["Er\u00b7zei\u00b7ge", "dich", ",", "mein", "Arzt", "!", "Der", "we\u00b7nigs\u00b7te", "Ver\u00b7zug"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "vers\u00e4umt den Kranken oft; ist sie schon auf den Flug", "tokens": ["ver\u00b7s\u00e4umt", "den", "Kran\u00b7ken", "oft", ";", "ist", "sie", "schon", "auf", "den", "Flug"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV", "$.", "VAFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "die Seele, so ists aus. Wie ist doch dieses Kleine", "tokens": ["die", "See\u00b7le", ",", "so", "ists", "aus", ".", "Wie", "ist", "doch", "die\u00b7ses", "Klei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "VAFIN", "PTKVZ", "$.", "PWAV", "VAFIN", "ADV", "PDAT", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "wie ach! wie gro\u00df bei ihr! sie sieht nach dir, die deine,", "tokens": ["wie", "ach", "!", "wie", "gro\u00df", "bei", "ihr", "!", "sie", "sieht", "nach", "dir", ",", "die", "dei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ITJ", "$.", "PWAV", "ADJD", "APPR", "PPOSAT", "$.", "PPER", "VVFIN", "APPR", "PPER", "$,", "PRELS", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "l\u00e4\u00dft keinen Blick vorbei, schickt Sinn und Geist nach dir.", "tokens": ["l\u00e4\u00dft", "kei\u00b7nen", "Blick", "vor\u00b7bei", ",", "schickt", "Sinn", "und", "Geist", "nach", "dir", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKVZ", "$,", "VVFIN", "NN", "KON", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Itzt fleugt sie selbst dir nach. Ach was verbleibt nur mir?", "tokens": ["Itzt", "fleugt", "sie", "selbst", "dir", "nach", ".", "Ach", "was", "ver\u00b7bleibt", "nur", "mir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "PTKVZ", "$.", "ITJ", "PWS", "VVFIN", "ADV", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich bin nun nicht mehr ich. K\u00f6mt sie nicht balde wieder", "tokens": ["Ich", "bin", "nun", "nicht", "mehr", "ich", ".", "K\u00f6mt", "sie", "nicht", "bal\u00b7de", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "ADV", "PPER", "$.", "VVFIN", "PPER", "PTKNEG", "ADV", "ADV"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "und bringt dich, ihren Freund und meinen Trost, hernieder,", "tokens": ["und", "bringt", "dich", ",", "ih\u00b7ren", "Freund", "und", "mei\u00b7nen", "Trost", ",", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "wie? wo? was werd' ich sein? der ich schon itzt vorhin", "tokens": ["wie", "?", "wo", "?", "was", "werd'", "ich", "sein", "?", "der", "ich", "schon", "itzt", "vor\u00b7hin"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "PWAV", "$.", "PWS", "VAFIN", "PPER", "VAINF", "$.", "PRELS", "PPER", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "ein lebendiger Tod und totes Leben bin.", "tokens": ["ein", "le\u00b7ben\u00b7di\u00b7ger", "Tod", "und", "to\u00b7tes", "Le\u00b7ben", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}}}}}