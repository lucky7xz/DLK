{"textgrid.poem.55016": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Wechsel", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf Kieseln im Bache da lieg ich, wie helle!", "tokens": ["Auf", "Kie\u00b7seln", "im", "Ba\u00b7che", "da", "lieg", "ich", ",", "wie", "hel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "ADV", "VVFIN", "PPER", "$,", "PWAV", "ADJA", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Verbreite die Arme der kommenden Welle,", "tokens": ["Ver\u00b7brei\u00b7te", "die", "Ar\u00b7me", "der", "kom\u00b7men\u00b7den", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und buhlerisch dr\u00fcckt sie die sehnende Brust.", "tokens": ["Und", "buh\u00b7le\u00b7risch", "dr\u00fcckt", "sie", "die", "seh\u00b7nen\u00b7de", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Dann f\u00fchrt sie der Leichtsinn im Strome danieder;", "tokens": ["Dann", "f\u00fchrt", "sie", "der", "Leicht\u00b7sinn", "im", "Stro\u00b7me", "da\u00b7nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "PAV", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Es naht sich die zweite, sie streichelt mich wieder:", "tokens": ["Es", "naht", "sich", "die", "zwei\u00b7te", ",", "sie", "strei\u00b7chelt", "mich", "wie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "ADJA", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "So f\u00fchl ich die Freuden der wechselnden Lust.", "tokens": ["So", "f\u00fchl", "ich", "die", "Freu\u00b7den", "der", "wech\u00b7seln\u00b7den", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Und doch, und so traurig, verschleifst du vergebens", "tokens": ["Und", "doch", ",", "und", "so", "trau\u00b7rig", ",", "ver\u00b7schleifst", "du", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "KON", "ADV", "ADJD", "$,", "VVFIN", "PPER", "ADV"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die k\u00f6stlichen Stunden des eilenden Lebens,", "tokens": ["Die", "k\u00f6st\u00b7li\u00b7chen", "Stun\u00b7den", "des", "ei\u00b7len\u00b7den", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Weil dich das geliebteste M\u00e4dchen vergi\u00dft!", "tokens": ["Weil", "dich", "das", "ge\u00b7lieb\u00b7tes\u00b7te", "M\u00e4d\u00b7chen", "ver\u00b7gi\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "O ruf sie zur\u00fccke, die vorigen Zeiten!", "tokens": ["O", "ruf", "sie", "zu\u00b7r\u00fc\u00b7cke", ",", "die", "vo\u00b7ri\u00b7gen", "Zei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Es k\u00fc\u00dft sich so s\u00fc\u00dfe die Lippe der Zweiten,", "tokens": ["Es", "k\u00fc\u00dft", "sich", "so", "s\u00fc\u00b7\u00dfe", "die", "Lip\u00b7pe", "der", "Zwei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Als kaum sich die Lippe der Ersten gek\u00fc\u00dft.", "tokens": ["Als", "kaum", "sich", "die", "Lip\u00b7pe", "der", "Ers\u00b7ten", "ge\u00b7k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PRF", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}