{"textgrid.poem.61757": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Benedikt 13", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf der Burg zu Peniskola, die vom Fels zur \u00d6de blickt,", "tokens": ["Auf", "der", "Burg", "zu", "Pe\u00b7nis\u00b7ko\u00b7la", ",", "die", "vom", "Fels", "zur", "\u00d6\u00b7de", "blickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "$,", "PRELS", "APPRART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Am Altar im Kreis der M\u00f6nche steht der greise Benedikt.", "tokens": ["Am", "Al\u00b7tar", "im", "Kreis", "der", "M\u00f6n\u00b7che", "steht", "der", "grei\u00b7se", "Be\u00b7ne\u00b7dikt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Einst zum Pontifex erkoren, nun entsetzt durch Kaiserwort,", "tokens": ["Einst", "zum", "Pon\u00b7ti\u00b7fex", "er\u00b7ko\u00b7ren", ",", "nun", "ent\u00b7setzt", "durch", "Kai\u00b7ser\u00b7wort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVINF", "$,", "ADV", "VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Barg er, unvers\u00f6hnlich grollend, wie ein wunder Aar sich dort.", "tokens": ["Barg", "er", ",", "un\u00b7ver\u00b7s\u00f6hn\u00b7lich", "grol\u00b7lend", ",", "wie", "ein", "wun\u00b7der", "Aar", "sich", "dort", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADJD", "ADJD", "$,", "PWAV", "ART", "ADJA", "NN", "PRF", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "\u00bbherr, das Amt der ew'gen Schl\u00fcssel, das du deinem Knechte gabst,", "tokens": ["\u00bb", "herr", ",", "das", "Amt", "der", "ew'\u00b7gen", "Schl\u00fcs\u00b7sel", ",", "das", "du", "dei\u00b7nem", "Knech\u00b7te", "gabst", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "ART", "NN", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wer vermag's mir anzutasten! La\u00df sie dr\u00e4un! Ich bin der Papst.", "tokens": ["Wer", "ver\u00b7mag's", "mir", "an\u00b7zu\u00b7tas\u00b7ten", "!", "La\u00df", "sie", "dr\u00e4un", "!", "Ich", "bin", "der", "Papst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "VVIZU", "$.", "VVIMP", "PPER", "VVINF", "$.", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "\u00dcber F\u00fcrstenmacht und V\u00f6lker hast du mir Gewalt verliehn;", "tokens": ["\u00dc\u00b7ber", "F\u00fcrs\u00b7ten\u00b7macht", "und", "V\u00f6l\u00b7ker", "hast", "du", "mir", "Ge\u00b7walt", "ver\u00b7liehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAFIN", "PPER", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wagt zu trotzen mir der Erdkreis, dein Gericht herab auf ihn!\u00ab", "tokens": ["Wagt", "zu", "trot\u00b7zen", "mir", "der", "Erd\u00b7kreis", ",", "dein", "Ge\u00b7richt", "her\u00b7ab", "auf", "ihn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PTKZU", "VVINF", "PPER", "ART", "NN", "$,", "PPOSAT", "NN", "ADV", "APPR", "PPER", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Und empor das Auge wendend, das des Himmels Blitze sucht,", "tokens": ["Und", "em\u00b7por", "das", "Au\u00b7ge", "wen\u00b7dend", ",", "das", "des", "Him\u00b7mels", "Blit\u00b7ze", "sucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVPP", "$,", "PRELS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Spricht er feierlich den Bannfluch, der die ganze Welt verflucht.", "tokens": ["Spricht", "er", "fei\u00b7er\u00b7lich", "den", "Bann\u00b7fluch", ",", "der", "die", "gan\u00b7ze", "Welt", "ver\u00b7flucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Unter Grabgel\u00e4ut' die Kerzen l\u00f6scht er aus am Hochaltar:", "tokens": ["Un\u00b7ter", "Grab\u00b7ge\u00b7l\u00e4ut'", "die", "Ker\u00b7zen", "l\u00f6scht", "er", "aus", "am", "Hoc\u00b7hal\u00b7tar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN", "PPER", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "\u00bbalso seid im Buch des Lebens ausgetan f\u00fcr immerdar!\u00ab", "tokens": ["\u00bb", "al\u00b7so", "seid", "im", "Buch", "des", "Le\u00b7bens", "aus\u00b7ge\u00b7tan", "f\u00fcr", "im\u00b7mer\u00b7dar", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VAFIN", "APPRART", "NN", "ART", "NN", "VVPP", "APPR", "ADV", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.7": {"line.1": {"text": "Dumpf erschallt der Chor der M\u00f6nche: \u00bbTag des Zornes, brich heran!\u00ab \u2013", "tokens": ["Dumpf", "er\u00b7schallt", "der", "Chor", "der", "M\u00f6n\u00b7che", ":", "\u00bb", "Tag", "des", "Zor\u00b7nes", ",", "brich", "he\u00b7ran", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "$.", "$(", "NN", "ART", "NN", "$,", "ADJD", "PTKVZ", "$.", "$(", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Doch die Sonne wallt wie gestern ruhig l\u00e4chelnd ihre Bahn.", "tokens": ["Doch", "die", "Son\u00b7ne", "wallt", "wie", "ge\u00b7stern", "ru\u00b7hig", "l\u00e4\u00b7chelnd", "ih\u00b7re", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "KOKOM", "ADV", "ADJD", "ADJD", "PPOSAT", "NN", "$."], "meter": "--+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}