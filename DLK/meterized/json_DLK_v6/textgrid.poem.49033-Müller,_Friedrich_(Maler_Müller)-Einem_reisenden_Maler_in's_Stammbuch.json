{"textgrid.poem.49033": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "Einem reisenden Maler in's Stammbuch", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gehst hin in eine andre Welt \u2013", "tokens": ["Gehst", "hin", "in", "ei\u00b7ne", "and\u00b7re", "Welt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Herzen Gl\u00fcck, wie's dir gef\u00e4llt!", "tokens": ["Von", "Her\u00b7zen", "Gl\u00fcck", ",", "wie's", "dir", "ge\u00b7f\u00e4llt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,", "KOUS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verleih' dir Gott ein fr\u00f6hlich Blut,", "tokens": ["Ver\u00b7leih'", "dir", "Gott", "ein", "fr\u00f6h\u00b7lich", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zur Arbeit immer frischen Mut!", "tokens": ["Zur", "Ar\u00b7beit", "im\u00b7mer", "fri\u00b7schen", "Mut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "W\u00e4r' ich zur Stund' ein reicher Mann,", "tokens": ["W\u00e4r'", "ich", "zur", "Stund'", "ein", "rei\u00b7cher", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich b\u00f6t dir was Geringes an;", "tokens": ["Ich", "b\u00f6t", "dir", "was", "Ge\u00b7rin\u00b7ges", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ob's gleich nur Quark \u2013 in dieser Welt", "tokens": ["Ob's", "gleich", "nur", "Quark", "\u2013", "in", "die\u00b7ser", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADV", "ADV", "NN", "$(", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Im Glauben manchen noch erh\u00e4lt.", "tokens": ["Im", "Glau\u00b7ben", "man\u00b7chen", "noch", "er\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da ich nichts Bessers geben kann,", "tokens": ["Da", "ich", "nichts", "Bes\u00b7sers", "ge\u00b7ben", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Nimm dreierlei zum Frommen an:", "tokens": ["Nimm", "drei\u00b7er\u00b7lei", "zum", "From\u00b7men", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "CARD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Hab' wahrer K\u00fcnstler Eigensinn,", "tokens": ["Hab'", "wah\u00b7rer", "K\u00fcnst\u00b7ler", "Ei\u00b7gen\u00b7sinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Zu malen nur nach deinem Sinn.", "tokens": ["Zu", "ma\u00b7len", "nur", "nach", "dei\u00b7nem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Wie Gott dir Aug' und Herz gestellt,", "tokens": ["Wie", "Gott", "dir", "Aug'", "und", "Herz", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Darnach betrachte deine Welt.", "tokens": ["Dar\u00b7nach", "be\u00b7trach\u00b7te", "dei\u00b7ne", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Nimm Rat und gute Meinung an,", "tokens": ["Nimm", "Rat", "und", "gu\u00b7te", "Mei\u00b7nung", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Doch schau, wer Rat dir geben kann.", "tokens": ["Doch", "schau", ",", "wer", "Rat", "dir", "ge\u00b7ben", "kann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWS", "NN", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Ein Mancher meint's von Herzen recht,", "tokens": ["Ein", "Man\u00b7cher", "meint's", "von", "Her\u00b7zen", "recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Gibt's drum nicht minder dumm und schlecht.", "tokens": ["Gibt's", "drum", "nicht", "min\u00b7der", "dumm", "und", "schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PAV", "PTKNEG", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Vor allem traue der Natur:", "tokens": ["Vor", "al\u00b7lem", "trau\u00b7e", "der", "Na\u00b7tur", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Bist K\u00fcnstler nur auf ihrer Spur:", "tokens": ["Bist", "K\u00fcnst\u00b7ler", "nur", "auf", "ih\u00b7rer", "Spur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Denn ohne sie, was ist die Kunst?", "tokens": ["Denn", "oh\u00b7ne", "sie", ",", "was", "ist", "die", "Kunst", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "$,", "PWS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Ein Kinderspiel \u2013 nur M\u00fch und Dunst.", "tokens": ["Ein", "Kin\u00b7der\u00b7spiel", "\u2013", "nur", "M\u00fch", "und", "Dunst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}