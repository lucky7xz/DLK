{"dta.poem.6588": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den durchsichtigen und nicht durch-  \n sichtigen Dingen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Allein, erf\u00fcllt die Lufft", "tokens": ["Al\u00b7lein", ",", "er\u00b7f\u00fcllt", "die", "Lufft"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein falber Dufft;", "tokens": ["Ein", "fal\u00b7ber", "Dufft", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und treibt ein tr\u00fcber Schlamm im Wasser; wird so gleich", "tokens": ["Und", "treibt", "ein", "tr\u00fc\u00b7ber", "Schlamm", "im", "Was\u00b7ser", ";", "wird", "so", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$.", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem andern Element der Weg benommen,", "tokens": ["Dem", "an\u00b7dern", "E\u00b7le\u00b7ment", "der", "Weg", "be\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und kan sodann sein Glantz nicht ferner durch sie kommen.", "tokens": ["Und", "kan", "so\u00b7dann", "sein", "Glantz", "nicht", "fer\u00b7ner", "durch", "sie", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PPOSAT", "NN", "PTKNEG", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}