{"textgrid.poem.36781": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Ponte rotto", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine zerbrochene Br\u00fccke, was ist's, kein Wunder am Ende!", "tokens": ["Ei\u00b7ne", "zer\u00b7bro\u00b7che\u00b7ne", "Br\u00fc\u00b7cke", ",", "was", "ist's", ",", "kein", "Wun\u00b7der", "am", "En\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWS", "VAFIN", "$,", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Alles vergeht, und der Welt wird's nicht viel besser geschehn.", "tokens": ["Al\u00b7les", "ver\u00b7geht", ",", "und", "der", "Welt", "wird's", "nicht", "viel", "bes\u00b7ser", "ge\u00b7schehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KON", "ART", "NN", "VAFIN", "PTKNEG", "ADV", "ADJD", "VVPP", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Dererlei merkt man sich nicht, auch wenn die Br\u00fccke sich weigert,", "tokens": ["De\u00b7rer\u00b7lei", "merkt", "man", "sich", "nicht", ",", "auch", "wenn", "die", "Br\u00fc\u00b7cke", "sich", "wei\u00b7gert", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PRF", "PTKNEG", "$,", "ADV", "KOUS", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Mit dem gefallnen Geschlecht \u00fcber die Tiber zu gehn.", "tokens": ["Mit", "dem", "ge\u00b7fall\u00b7nen", "Ge\u00b7schlecht", "\u00fc\u00b7ber", "die", "Ti\u00b7ber", "zu", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}}}}}