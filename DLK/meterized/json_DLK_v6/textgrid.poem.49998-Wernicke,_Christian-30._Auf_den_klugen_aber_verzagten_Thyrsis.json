{"textgrid.poem.49998": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "30. Auf den klugen aber verzagten Thyrsis", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So lang' als Thyrsis gl\u00fccklich war,", "tokens": ["So", "lang'", "als", "Thyr\u00b7sis", "gl\u00fcck\u00b7lich", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So flossen seine ", "tokens": ["So", "flos\u00b7sen", "sei\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Behuttsam zircklend mit der Zeit,", "tokens": ["Be\u00b7hutt\u00b7sam", "zir\u00b7ck\u00b7lend", "mit", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So ", "tokens": ["So"], "token_info": ["word"], "pos": ["ADV"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Als aber ihn das Ungl\u00fcck dr\u00fcckt',", "tokens": ["Als", "a\u00b7ber", "ihn", "das", "Un\u00b7gl\u00fcck", "dr\u00fcckt'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da ward ihm ", "tokens": ["Da", "ward", "ihm"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Und den das Gl\u00fcck beliebt gemacht,", "tokens": ["Und", "den", "das", "Gl\u00fcck", "be\u00b7liebt", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.9": {"text": "So Knechtisch f\u00fchrt' er seine Kett',", "tokens": ["So", "Knech\u00b7tisch", "f\u00fchrt'", "er", "sei\u00b7ne", "Kett'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Dass alle sagten, Thyrsis ", "tokens": ["Dass", "al\u00b7le", "sag\u00b7ten", ",", "Thyr\u00b7sis"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "XY"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}