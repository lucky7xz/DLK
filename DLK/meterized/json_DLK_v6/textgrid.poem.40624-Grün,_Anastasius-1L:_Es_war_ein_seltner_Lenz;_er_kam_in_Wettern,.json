{"textgrid.poem.40624": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es war ein seltner Lenz; er kam in Wettern,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war ein seltner Lenz; er kam in Wettern,", "tokens": ["Es", "war", "ein", "selt\u00b7ner", "Lenz", ";", "er", "kam", "in", "Wet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Donnerkeilen, Welten zu zerschmettern;", "tokens": ["Mit", "Don\u00b7ner\u00b7kei\u00b7len", ",", "Wel\u00b7ten", "zu", "zer\u00b7schmet\u00b7tern", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr saht ihn dr\u00f6hnend \u00fcber Deutschland rollen:", "tokens": ["Ihr", "saht", "ihn", "dr\u00f6h\u00b7nend", "\u00fc\u00b7ber", "Deutschland", "rol\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "NE", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das war des Volkes lang verhaltnes Grollen;", "tokens": ["Das", "war", "des", "Vol\u00b7kes", "lang", "ver\u00b7halt\u00b7nes", "Grol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es war ein sch\u00f6n, gewitterpr\u00e4chtig Z\u00fcrnen,", "tokens": ["Es", "war", "ein", "sch\u00f6n", ",", "ge\u00b7wit\u00b7ter\u00b7pr\u00e4ch\u00b7tig", "Z\u00fcr\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gerechter Zorn verg\u00f6ttlicht M\u00e4nnerstirnen. \u2013", "tokens": ["Ge\u00b7rech\u00b7ter", "Zorn", "ver\u00b7g\u00f6tt\u00b7licht", "M\u00e4n\u00b7ners\u00b7tir\u00b7nen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "VVFIN", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ein armer Lenz! Wer d\u00e4chte jetzt an Rosen,", "tokens": ["Ein", "ar\u00b7mer", "Lenz", "!", "Wer", "d\u00e4ch\u00b7te", "jetzt", "an", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PWS", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "An Nachtigallen und an Bl\u00fcthentriebe?", "tokens": ["An", "Nach\u00b7ti\u00b7gal\u00b7len", "und", "an", "Bl\u00fc\u00b7then\u00b7trie\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wen r\u00fchrt's, da\u00df Wald und Lenzluft fl\u00fcsternd kosen?", "tokens": ["Wen", "r\u00fchrt's", ",", "da\u00df", "Wald", "und", "Lenz\u00b7luft", "fl\u00fcs\u00b7ternd", "ko\u00b7sen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "NN", "KON", "NN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Der Fr\u00fchling starb wie die verschm\u00e4hte Liebe.", "tokens": ["Der", "Fr\u00fch\u00b7ling", "starb", "wie", "die", "ver\u00b7schm\u00e4h\u00b7te", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und dennoch reich ist dieser Lenz vor allen,", "tokens": ["Und", "den\u00b7noch", "reich", "ist", "die\u00b7ser", "Lenz", "vor", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "PDAT", "NN", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Denn \u00fcber seine Blumen schritt die Freiheit,", "tokens": ["Denn", "\u00fc\u00b7ber", "sei\u00b7ne", "Blu\u00b7men", "schritt", "die", "Frei\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Um siegreich in das deutsche Land zu wallen,", "tokens": ["Um", "sieg\u00b7reich", "in", "das", "deut\u00b7sche", "Land", "zu", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und seine L\u00fcfte wehn mit Wohlgefallen", "tokens": ["Und", "sei\u00b7ne", "L\u00fcf\u00b7te", "wehn", "mit", "Wohl\u00b7ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Im Banner lang verp\u00f6nter Farbendreiheit.", "tokens": ["Im", "Ban\u00b7ner", "lang", "ver\u00b7p\u00f6n\u00b7ter", "Far\u00b7ben\u00b7drei\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da sprach das Volk: \u00bbDa\u00df Freiheit, meine Braut,", "tokens": ["Da", "sprach", "das", "Volk", ":", "\u00bb", "Da\u00df", "Frei\u00b7heit", ",", "mei\u00b7ne", "Braut", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "$(", "KOUS", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nicht im Vorbeiflug nur mein Haus ber\u00fchre,", "tokens": ["Nicht", "im", "Vor\u00b7beif\u00b7lug", "nur", "mein", "Haus", "be\u00b7r\u00fch\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Da\u00df sie's zu lieber Wohnstatt sich erk\u00fcre,", "tokens": ["Da\u00df", "sie's", "zu", "lie\u00b7ber", "Wohn\u00b7statt", "sich", "er\u00b7k\u00fc\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sei sie mit goldnem Ring mir angetraut:", "tokens": ["Sei", "sie", "mit", "gold\u00b7nem", "Ring", "mir", "an\u00b7ge\u00b7traut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der starke Ring der Einheit soll es sein,", "tokens": ["Der", "star\u00b7ke", "Ring", "der", "Ein\u00b7heit", "soll", "es", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VMFIN", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sein Zauberkreis schlie\u00df' all mein Deutschland ein!", "tokens": ["Sein", "Zau\u00b7ber\u00b7kreis", "schlie\u00df'", "all", "mein", "Deutschland", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIAT", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dem Hause will ich treue W\u00e4chter stellen", "tokens": ["Dem", "Hau\u00b7se", "will", "ich", "treu\u00b7e", "W\u00e4ch\u00b7ter", "stel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVFIN", "NE", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und einem treuen F\u00fchrer sie gesellen;", "tokens": ["Und", "ei\u00b7nem", "treu\u00b7en", "F\u00fch\u00b7rer", "sie", "ge\u00b7sel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der F\u00fchrer sei des Volkes klarer Spiegel,", "tokens": ["Der", "F\u00fch\u00b7rer", "sei", "des", "Vol\u00b7kes", "kla\u00b7rer", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Der Kern und Mittelpunkt auf deutscher Erde,", "tokens": ["Der", "Kern", "und", "Mit\u00b7tel\u00b7punkt", "auf", "deut\u00b7scher", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Er sei des Einheitrings gefeites Siegel,", "tokens": ["Er", "sei", "des", "Ein\u00b7hei\u00b7trings", "ge\u00b7fei\u00b7tes", "Sie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "Auf da\u00df sein Herz das Herz von Deutschland werde.", "tokens": ["Auf", "da\u00df", "sein", "Herz", "das", "Herz", "von", "Deutschland", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPOSAT", "NN", "ART", "NN", "APPR", "NE", "VAFIN", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.13": {"text": "Ein Muth'ger sei's! Muth gilt es ohne Gleichen,", "tokens": ["Ein", "Muth'\u00b7ger", "sei's", "!", "Muth", "gilt", "es", "oh\u00b7ne", "Glei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$.", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "So vielbedrohte Sch\u00e4tze zu bewachen.", "tokens": ["So", "viel\u00b7be\u00b7droh\u00b7te", "Sch\u00e4t\u00b7ze", "zu", "be\u00b7wa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Ich will ihn stark und gro\u00df und m\u00e4chtig machen!", "tokens": ["Ich", "will", "ihn", "stark", "und", "gro\u00df", "und", "m\u00e4ch\u00b7tig", "ma\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "KON", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Nicht in die K\u00f6nigsgr\u00e4ber will ich schleichen,", "tokens": ["Nicht", "in", "die", "K\u00f6\u00b7nigs\u00b7gr\u00e4\u00b7ber", "will", "ich", "schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Nicht aus dem Kaiserschrein Kleinode fodern;", "tokens": ["Nicht", "aus", "dem", "Kai\u00b7ser\u00b7schrein", "Klein\u00b7o\u00b7de", "fo\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "La\u00dft rosten Karols Schwert, sein Pallium modern,", "tokens": ["La\u00dft", "ros\u00b7ten", "Ka\u00b7rols", "Schwert", ",", "sein", "Pal\u00b7li\u00b7um", "mo\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "NE", "$,", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Die Gruft bestatte alter Ehren Leichen;", "tokens": ["Die", "Gruft", "be\u00b7stat\u00b7te", "al\u00b7ter", "Eh\u00b7ren", "Lei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Was ich ihm biete, mag am Tage wallen,", "tokens": ["Was", "ich", "ihm", "bie\u00b7te", ",", "mag", "am", "Ta\u00b7ge", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVFIN", "$,", "VMFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Es wird nicht an der Luft in Staub zerfallen.", "tokens": ["Es", "wird", "nicht", "an", "der", "Luft", "in", "Staub", "zer\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "APPR", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Sein Haupt beschirmt der B\u00fcrgerkrone Segen,", "tokens": ["Sein", "Haupt", "be\u00b7schirmt", "der", "B\u00fcr\u00b7ger\u00b7kro\u00b7ne", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "All meine Kraft will ich in seinen Degen", "tokens": ["All", "mei\u00b7ne", "Kraft", "will", "ich", "in", "sei\u00b7nen", "De\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "VMFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Und in sein Herz all meine Liebe legen,", "tokens": ["Und", "in", "sein", "Herz", "all", "mei\u00b7ne", "Lie\u00b7be", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "PIAT", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Von Gottes Gnaden herrscht nur Wind und Wolke;", "tokens": ["Von", "Got\u00b7tes", "Gna\u00b7den", "herrscht", "nur", "Wind", "und", "Wol\u00b7ke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Es sei ein gro\u00dfer heil'ger Bund der Seelen,", "tokens": ["Es", "sei", "ein", "gro\u00b7\u00dfer", "heil'\u00b7ger", "Bund", "der", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Wo statt der Sieben jetzt Millionen w\u00e4hlen", "tokens": ["Wo", "statt", "der", "Sie\u00b7ben", "jetzt", "Mil\u00b7lion\u00b7en", "w\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ADV", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Den Ersten der Erkorenen vom Volke!\u00ab", "tokens": ["Den", "Ers\u00b7ten", "der", "Er\u00b7ko\u00b7re\u00b7nen", "vom", "Vol\u00b7ke", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wer sei der Mann? Des Volkes Boten zogen", "tokens": ["Wer", "sei", "der", "Mann", "?", "Des", "Vol\u00b7kes", "Bo\u00b7ten", "zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vorbei an K\u00f6nigsburgen ohne Fragen,", "tokens": ["Vor\u00b7bei", "an", "K\u00f6\u00b7nigs\u00b7bur\u00b7gen", "oh\u00b7ne", "Fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem Prunke sind die Schlichten nicht gewogen,", "tokens": ["Dem", "Prun\u00b7ke", "sind", "die", "Schlich\u00b7ten", "nicht", "ge\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"},