{"dta.poem.14214": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Trennung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ein furchtsam Kind, im stillen Haus erzogen", "tokens": ["Ein", "furcht\u00b7sam", "Kind", ",", "im", "stil\u00b7len", "Haus", "er\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "$,", "APPRART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Konnt' ich am Abendroth die Blicke weiden,", "tokens": ["Konnt'", "ich", "am", "A\u00b7ben\u00b7droth", "die", "Bli\u00b7cke", "wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Tiefathmend in die laue Luft vor Freuden.", "tokens": ["Tie\u00b7fath\u00b7mend", "in", "die", "lau\u00b7e", "Luft", "vor", "Freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Er hat um diese Stille mich betrogen.", "tokens": ["Er", "hat", "um", "die\u00b7se", "Stil\u00b7le", "mich", "be\u00b7tro\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PDAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mit stolzen Augen, fremden sch\u00f6nen Worten", "tokens": ["Mit", "stol\u00b7zen", "Au\u00b7gen", ",", "frem\u00b7den", "sch\u00f6\u00b7nen", "Wor\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Lockt er die W\u00fcnsche aus dem stillen Hafen", "tokens": ["Lockt", "er", "die", "W\u00fcn\u00b7sche", "aus", "dem", "stil\u00b7len", "Ha\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.7": {"text": "Wo sie bei Sternenglanze seelig schlafen,", "tokens": ["Wo", "sie", "bei", "Ster\u00b7nen\u00b7glan\u00b7ze", "see\u00b7lig", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Hinaus ins unbekannte Reich der Wogen;", "tokens": ["Hin\u00b7aus", "ins", "un\u00b7be\u00b7kann\u00b7te", "Reich", "der", "Wo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Da kommen Winde buhlend angeflogen,", "tokens": ["Da", "kom\u00b7men", "Win\u00b7de", "buh\u00b7lend", "an\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Die zarte Hand zwingt nicht die wilden Wellen,", "tokens": ["Die", "zar\u00b7te", "Hand", "zwingt", "nicht", "die", "wil\u00b7den", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Du mu\u00dft, wohin die vollen Segel schwellen.", "tokens": ["Du", "mu\u00dft", ",", "wo\u00b7hin", "die", "vol\u00b7len", "Se\u00b7gel", "schwel\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PWAV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}