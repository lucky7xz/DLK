{"textgrid.poem.32540": {"metadata": {"author": {"name": "Scheffner, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Ein lehrreicher Traum von Amor.", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Liebesgott, geschm\u00fcckt mit allen Reitzen,", "tokens": ["Der", "Lie\u00b7bes\u00b7gott", ",", "ge\u00b7schm\u00fcckt", "mit", "al\u00b7len", "Reit\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erschien mir heut im leichten Morgentraum,", "tokens": ["Er\u00b7schien", "mir", "heut", "im", "leich\u00b7ten", "Mor\u00b7gen\u00b7traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "An seiner Hand ein loses braunes M\u00e4dchen;", "tokens": ["An", "sei\u00b7ner", "Hand", "ein", "lo\u00b7ses", "brau\u00b7nes", "M\u00e4d\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbda, sprach er, nimm die bl\u00fchende Brunette", "tokens": ["\u00bb", "da", ",", "sprach", "er", ",", "nimm", "die", "bl\u00fc\u00b7hen\u00b7de", "Bru\u00b7net\u00b7te"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADV", "$,", "VVFIN", "PPER", "$,", "VVIMP", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "K\u00fc\u00df sie, und dr\u00fcck sie fest in deine Arme.\u00ab", "tokens": ["K\u00fc\u00df", "sie", ",", "und", "dr\u00fcck", "sie", "fest", "in", "dei\u00b7ne", "Ar\u00b7me", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ich that es, und wir sanken auf den Sopha:", "tokens": ["Ich", "that", "es", ",", "und", "wir", "san\u00b7ken", "auf", "den", "So\u00b7pha", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wie schalkhaft l\u00e4chelte der kleine Amor", "tokens": ["Wie", "schalk\u00b7haft", "l\u00e4\u00b7chel\u00b7te", "der", "klei\u00b7ne", "A\u00b7mor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "ADJA", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Als er, gleich Wolken, die die Sonne decken", "tokens": ["Als", "er", ",", "gleich", "Wol\u00b7ken", ",", "die", "die", "Son\u00b7ne", "de\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "ADV", "NN", "$,", "PRELS", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Den Vorhang von dem Sitz der Wollust hob.", "tokens": ["Den", "Vor\u00b7hang", "von", "dem", "Sitz", "der", "Wol\u00b7lust", "hob", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "\u00bbsieh her, dies ist der freudenreiche Becher", "tokens": ["\u00bb", "sieh", "her", ",", "dies", "ist", "der", "freu\u00b7den\u00b7rei\u00b7che", "Be\u00b7cher"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVIMP", "PTKVZ", "$,", "PDS", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "In den einst Bachus bey Ariadnen", "tokens": ["In", "den", "einst", "Ba\u00b7chus", "bey", "A\u00b7riad\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "NE", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Den Nektar go\u00df, und einen Rausch sich trank:", "tokens": ["Den", "Nek\u00b7tar", "go\u00df", ",", "und", "ei\u00b7nen", "Rausch", "sich", "trank", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Betrachte dieses lockigte Gewebe,", "tokens": ["Be\u00b7trach\u00b7te", "die\u00b7ses", "lo\u00b7ckig\u00b7te", "Ge\u00b7we\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Der Venus G\u00fcrtel ist von solchen F\u00e4den,", "tokens": ["Der", "Ve\u00b7nus", "G\u00fcr\u00b7tel", "ist", "von", "sol\u00b7chen", "F\u00e4\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Betracht des Laubwerks Kunst um diesen Becher,", "tokens": ["Be\u00b7tracht", "des", "Laub\u00b7werks", "Kunst", "um", "die\u00b7sen", "Be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Und athme seine Balsamsd\u00fcfte ein.", "tokens": ["Und", "ath\u00b7me", "sei\u00b7ne", "Bal\u00b7sams\u00b7d\u00fcf\u00b7te", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "So gro\u00df ist nicht die Kunst der heilgen Schale", "tokens": ["So", "gro\u00df", "ist", "nicht", "die", "Kunst", "der", "heil\u00b7gen", "Scha\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "PTKNEG", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "In welcher Hebe dort und Ganymed", "tokens": ["In", "wel\u00b7cher", "He\u00b7be", "dort", "und", "Ga\u00b7ny\u00b7med"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "ADV", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Uns G\u00f6ttern des Olymps den Nektar reichen.", "tokens": ["Uns", "G\u00f6t\u00b7tern", "des", "O\u00b7lymps", "den", "Nek\u00b7tar", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "F\u00fcll den Pokal, den Grazien einst schufen", "tokens": ["F\u00fcll", "den", "Po\u00b7kal", ",", "den", "Gra\u00b7zi\u00b7en", "einst", "schu\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "ADV", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.21": {"text": "Zu dem sie Rosen mit Granaten mischten,", "tokens": ["Zu", "dem", "sie", "Ro\u00b7sen", "mit", "Gra\u00b7na\u00b7ten", "mischten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Und den die Neuheit doppelt kostbar macht.", "tokens": ["Und", "den", "die", "Neu\u00b7heit", "dop\u00b7pelt", "kost\u00b7bar", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "F\u00fcll ihn wie Zeus ihn Danaen einst f\u00fcllte", "tokens": ["F\u00fcll", "ihn", "wie", "Zeus", "ihn", "Da\u00b7na\u00b7en", "einst", "f\u00fcll\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "KOKOM", "NE", "PPER", "NE", "ADV", "VVFIN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.24": {"text": "Als er im goldnen Regen auf sie fiel,", "tokens": ["Als", "er", "im", "gold\u00b7nen", "Re\u00b7gen", "auf", "sie", "fiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Und sey dabey entz\u00fcckt wie Jupiter.", "tokens": ["Und", "sey", "da\u00b7bey", "ent\u00b7z\u00fcckt", "wie", "Ju\u00b7pi\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PAV", "VVPP", "KOKOM", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Dies ist, hier wie\u00df er seinen kleinen Scepter,", "tokens": ["Dies", "ist", ",", "hier", "wie\u00df", "er", "sei\u00b7nen", "klei\u00b7nen", "Scep\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Der Heber der die wundert\u00e4thgen S\u00e4fte", "tokens": ["Der", "He\u00b7ber", "der", "die", "wun\u00b7der\u00b7t\u00e4th\u00b7gen", "S\u00e4f\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Woll\u00fcstig eintrinkt, und dann aus sich spritzt;", "tokens": ["Wol\u00b7l\u00fcs\u00b7tig", "ein\u00b7trinkt", ",", "und", "dann", "aus", "sich", "spritzt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "KON", "ADV", "APPR", "PRF", "VVFIN", "$."], "meter": "+--++-+--+", "measure": "iambic.penta.chol"}, "line.29": {"text": "Leg ihn nur an den Rand der Nektarschale", "tokens": ["Leg", "ihn", "nur", "an", "den", "Rand", "der", "Nek\u00b7tarsc\u00b7ha\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "APPR", "ART", "NN", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.30": {"text": "Er wird sich bald mit ihr vertraut vereinigen,", "tokens": ["Er", "wird", "sich", "bald", "mit", "ihr", "ver\u00b7traut", "ver\u00b7ei\u00b7ni\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADV", "APPR", "PPOSAT", "ADJD", "VVINF", "$,"], "meter": "-+--+--+--++", "measure": "amphibrach.tri.plus"}, "line.31": {"text": "Und wei\u00dfer Schaum wird ihn und sie umziehn.", "tokens": ["Und", "wei\u00b7\u00dfer", "Schaum", "wird", "ihn", "und", "sie", "um\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "PPER", "KON", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "F\u00fcll lang, begl\u00fcckter J\u00fcngling, ", "tokens": ["F\u00fcll", "lang", ",", "be\u00b7gl\u00fcck\u00b7ter", "J\u00fcng\u00b7ling", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.33": {"text": "Er \u00f6fne sich wenn du dich d\u00fcrstig n\u00e4herst,", "tokens": ["Er", "\u00f6f\u00b7ne", "sich", "wenn", "du", "dich", "d\u00fcrs\u00b7tig", "n\u00e4\u00b7herst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "KOUS", "PPER", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+--+--+---", "measure": "amphibrach.tri.plus"}, "line.34": {"text": "Wie Rosen wenn sich West und Sonne nah'n,", "tokens": ["Wie", "Ro\u00b7sen", "wenn", "sich", "West", "und", "Son\u00b7ne", "nah'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "KOUS", "PRF", "NE", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Und wenn du gnung aus diesem Kelch getrunken,", "tokens": ["Und", "wenn", "du", "gnung", "aus", "die\u00b7sem", "Kelch", "ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Dann k\u00fc\u00df zur St\u00e4rkung ", "tokens": ["Dann", "k\u00fc\u00df", "zur", "St\u00e4r\u00b7kung"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.37": {"text": "Und trinke Wein aus ihrer hohlen Hand.", "tokens": ["Und", "trin\u00b7ke", "Wein", "aus", "ih\u00b7rer", "hoh\u00b7len", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}