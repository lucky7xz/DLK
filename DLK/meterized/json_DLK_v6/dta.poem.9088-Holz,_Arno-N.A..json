{"dta.poem.9088": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Rote D\u00e4cher!", "tokens": ["Ro\u00b7te", "D\u00e4\u00b7cher", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Aus den Schornsteinen, hier und da, Rauch,", "tokens": ["Aus", "den", "Schorn\u00b7stei\u00b7nen", ",", "hier", "und", "da", ",", "Rauch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "KON", "ADV", "$,", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "oben, hoch, in sonniger Luft, ab und zu, Tauben.", "tokens": ["o\u00b7ben", ",", "hoch", ",", "in", "son\u00b7ni\u00b7ger", "Luft", ",", "ab", "und", "zu", ",", "Tau\u00b7ben", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "$,", "APPR", "ADJA", "NN", "$,", "PTKVZ", "KON", "PTKVZ", "$,", "NN", "$."], "meter": "+-+-+--++--+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Es ist Nachmittag.", "tokens": ["Es", "ist", "Nach\u00b7mit\u00b7tag", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Aus Mohdrickers Garten her gackert eine Henne,", "tokens": ["Aus", "Moh\u00b7dri\u00b7ckers", "Gar\u00b7ten", "her", "ga\u00b7ckert", "ei\u00b7ne", "Hen\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "APZR", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-++-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "die ganze Stadt riecht nach Kaffee.", "tokens": ["die", "gan\u00b7ze", "Stadt", "riecht", "nach", "Kaf\u00b7fee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich bin ein kleiner, achtj\u00e4hriger Junge", "tokens": ["Ich", "bin", "ein", "klei\u00b7ner", ",", "acht\u00b7j\u00e4h\u00b7ri\u00b7ger", "Jun\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "und liege, das Kinn in beide F\u00e4uste,", "tokens": ["und", "lie\u00b7ge", ",", "das", "Kinn", "in", "bei\u00b7de", "F\u00e4us\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "platt auf dem Bauch", "tokens": ["platt", "auf", "dem", "Bauch"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "und kucke durch die Bodenluke.", "tokens": ["und", "ku\u00b7cke", "durch", "die", "Bo\u00b7den\u00b7lu\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Unter mir, steil, der Hof,", "tokens": ["Un\u00b7ter", "mir", ",", "steil", ",", "der", "Hof", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "KOUS", "$,", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "hinter mir, weggeworfen, ein Buch.", "tokens": ["hin\u00b7ter", "mir", ",", "weg\u00b7ge\u00b7wor\u00b7fen", ",", "ein", "Buch", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "VVPP", "$,", "ART", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Franz Hoffmann. Die Sclavenj\u00e4ger.", "tokens": ["Franz", "Hoff\u00b7mann", ".", "Die", "Scla\u00b7ven\u00b7j\u00e4\u00b7ger", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Wie still das ist!", "tokens": ["Wie", "still", "das", "ist", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PDS", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Nur dr\u00fcben in Knorrs Regenrinne", "tokens": ["Nur", "dr\u00fc\u00b7ben", "in", "Knorrs", "Re\u00b7gen\u00b7rin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "NE", "NN"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "zwei Spatzen, die sich um einen Strohhalm zanken,", "tokens": ["zwei", "Spat\u00b7zen", ",", "die", "sich", "um", "ei\u00b7nen", "Stroh\u00b7halm", "zan\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "PRELS", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "ein Mann, der s\u00e4gt,", "tokens": ["ein", "Mann", ",", "der", "s\u00e4gt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "und dazwischen, deutlich von der Kirche her,", "tokens": ["und", "da\u00b7zwi\u00b7schen", ",", "deut\u00b7lich", "von", "der", "Kir\u00b7che", "her", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "$,", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "in kurzen Pausen, regelm\u00e4ssig, h\u00e4mmernd,", "tokens": ["in", "kur\u00b7zen", "Pau\u00b7sen", ",", "re\u00b7gel\u00b7m\u00e4s\u00b7sig", ",", "h\u00e4m\u00b7mernd", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "der Kupferschmied Thiel.", "tokens": ["der", "Kup\u00b7fer\u00b7schmied", "Thiel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Wenn ich unten runtersehe,", "tokens": ["Wenn", "ich", "un\u00b7ten", "run\u00b7ter\u00b7se\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "sehe ich grade auf Mutters Blumenbrett:", "tokens": ["se\u00b7he", "ich", "gra\u00b7de", "auf", "Mut\u00b7ters", "Blu\u00b7men\u00b7brett", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "NN", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "ein Topf Goldlack, zwei T\u00f6pfe Levkoyen, eine Geranie", "tokens": ["ein", "Topf", "Gold\u00b7lack", ",", "zwei", "T\u00f6p\u00b7fe", "Lev\u00b7ko\u00b7yen", ",", "ei\u00b7ne", "Ge\u00b7ra\u00b7nie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "CARD", "NN", "NN", "$,", "ART", "NN"], "meter": "-++--+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "und mittendrin, zierlich in einem Cigarrenkistchen,", "tokens": ["und", "mit\u00b7ten\u00b7drin", ",", "zier\u00b7lich", "in", "ei\u00b7nem", "Ci\u00b7gar\u00b7ren\u00b7kist\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}, "line.5": {"text": "ein H\u00fcmpelchen Reseda.", "tokens": ["ein", "H\u00fcm\u00b7pel\u00b7chen", "Re\u00b7se\u00b7da", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Wie das riecht? Bis zu mir rauf!", "tokens": ["Wie", "das", "riecht", "?", "Bis", "zu", "mir", "rauf", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "VVFIN", "$.", "APPR", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und die Farben!", "tokens": ["Und", "die", "Far\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$."], "meter": "--+-", "measure": "anapaest.init"}, "line.2": {"text": "Jetzt! Wie der Wind dr\u00fcber weht!", "tokens": ["Jetzt", "!", "Wie", "der", "Wind", "dr\u00fc\u00b7ber", "weht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PWAV", "ART", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die wunder, wundersch\u00f6nen Farben!", "tokens": ["Die", "wun\u00b7der", ",", "wun\u00b7der\u00b7sch\u00f6\u00b7nen", "Far\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ich schliesse die Augen. Ich sehe sie noch immer.", "tokens": ["Ich", "schlies\u00b7se", "die", "Au\u00b7gen", ".", "Ich", "se\u00b7he", "sie", "noch", "im\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}}}}}