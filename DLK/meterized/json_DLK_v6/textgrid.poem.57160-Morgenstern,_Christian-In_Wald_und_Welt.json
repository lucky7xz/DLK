{"textgrid.poem.57160": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "In Wald und Welt", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lebendige Wesen schreitend aufzuscheuchen,", "tokens": ["Le\u00b7ben\u00b7di\u00b7ge", "We\u00b7sen", "schrei\u00b7tend", "auf\u00b7zu\u00b7scheu\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "will Lust im Wald wie in der Welt mich deuchen.", "tokens": ["will", "Lust", "im", "Wald", "wie", "in", "der", "Welt", "mich", "deu\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "APPRART", "NN", "KOKOM", "APPR", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein herrlich Wort aus Nietzsches hartem Lachen:", "tokens": ["Ein", "herr\u00b7lich", "Wort", "aus", "Nietz\u00b7sches", "har\u00b7tem", "La\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbdas Individuum unbehaglich machen\u00ab.", "tokens": ["\u00bb", "das", "In\u00b7di\u00b7vi\u00b7du\u00b7um", "un\u00b7be\u00b7hag\u00b7lich", "ma\u00b7chen", "\u00ab", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "ADJD", "VVINF", "$(", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}