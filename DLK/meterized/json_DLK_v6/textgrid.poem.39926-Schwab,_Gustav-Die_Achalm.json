{"textgrid.poem.39926": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Die Achalm", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da steht noch Thurm und Burgverlie\u00df", "tokens": ["Da", "steht", "noch", "Thurm", "und", "Burg\u00b7ver\u00b7lie\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Schlo\u00df, das ich genannt,", "tokens": ["Vom", "Schlo\u00df", ",", "das", "ich", "ge\u00b7nannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch wie es einst vor Zeiten hie\u00df,", "tokens": ["Doch", "wie", "es", "einst", "vor", "Zei\u00b7ten", "hie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist Keinem mehr bekannt.", "tokens": ["Ist", "Kei\u00b7nem", "mehr", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die alte Sage spricht es kaum", "tokens": ["Die", "al\u00b7te", "Sa\u00b7ge", "spricht", "es", "kaum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch halbvernehmlich nach,", "tokens": ["Noch", "halb\u00b7ver\u00b7nehm\u00b7lich", "nach", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie einst die Burg auf diesem Raum", "tokens": ["Wie", "einst", "die", "Burg", "auf", "die\u00b7sem", "Raum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor zorn'ger Fehde brach.", "tokens": ["Vor", "zorn'\u00b7ger", "Feh\u00b7de", "brach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Letzte war es vom Geschlecht,", "tokens": ["Der", "Letz\u00b7te", "war", "es", "vom", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der hier bestritten ward,", "tokens": ["Der", "hier", "be\u00b7strit\u00b7ten", "ward", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von Arme stark, von Sinn gerecht,", "tokens": ["Von", "Ar\u00b7me", "stark", ",", "von", "Sinn", "ge\u00b7recht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach frommer Stammesart.", "tokens": ["Nach", "from\u00b7mer", "Stam\u00b7me\u00b7sart", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er schirmt' und sch\u00fctzte Hof und Haus", "tokens": ["Er", "schirmt'", "und", "sch\u00fctz\u00b7te", "Hof", "und", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lang vor der st\u00e4rkern Macht,", "tokens": ["Lang", "vor", "der", "st\u00e4r\u00b7kern", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da trieben ihn die Flammen aus,", "tokens": ["Da", "trie\u00b7ben", "ihn", "die", "Flam\u00b7men", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mitten in die Schlacht.", "tokens": ["Und", "mit\u00b7ten", "in", "die", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Er lie\u00df den Bau wohl st\u00fcrzen ein,", "tokens": ["Er", "lie\u00df", "den", "Bau", "wohl", "st\u00fcr\u00b7zen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er sah nicht hinter sich,", "tokens": ["Er", "sah", "nicht", "hin\u00b7ter", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Boden wollt' er doch befrei'n,", "tokens": ["Den", "Bo\u00b7den", "wollt'", "er", "doch", "be\u00b7frei'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der keinem Feuer wich. \u2013", "tokens": ["Der", "kei\u00b7nem", "Feu\u00b7er", "wich", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Den Pfeil, den todestr\u00e4chtigen,", "tokens": ["Den", "Pfeil", ",", "den", "to\u00b7des\u00b7tr\u00e4ch\u00b7ti\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Empf\u00e4ngt sein tapfres Herz,", "tokens": ["Emp\u00b7f\u00e4ngt", "sein", "tapf\u00b7res", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein Rufen zum Allm\u00e4chtigen", "tokens": ["Sein", "Ru\u00b7fen", "zum", "All\u00b7m\u00e4ch\u00b7ti\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verschlingt der letzte Schmerz.", "tokens": ["Ver\u00b7schlingt", "der", "letz\u00b7te", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Doch was er rief in letzter Not,", "tokens": ["Doch", "was", "er", "rief", "in", "letz\u00b7ter", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das halbe Wort: ", "tokens": ["Das", "hal\u00b7be", "Wort", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Das hat gewi\u00df get\u00f6nt vor Gott", "tokens": ["Das", "hat", "ge\u00b7wi\u00df", "ge\u00b7t\u00f6nt", "vor", "Gott"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "VVPP", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als wie ein ganzer Psalm.", "tokens": ["Als", "wie", "ein", "gan\u00b7zer", "Psalm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ja selbst dem Feinde klang es sch\u00f6n.", "tokens": ["Ja", "selbst", "dem", "Fein\u00b7de", "klang", "es", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das ernste Scheidewort,", "tokens": ["Das", "erns\u00b7te", "Schei\u00b7de\u00b7wort", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er baute frisch auf diesen H\u00f6hn", "tokens": ["Er", "bau\u00b7te", "frisch", "auf", "die\u00b7sen", "H\u00f6hn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hie\u00df ", "tokens": ["Und", "hie\u00df"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "-+", "measure": "iambic.single"}}, "stanza.9": {"line.1": {"text": "Das Menschenwerk zerfallen ist,", "tokens": ["Das", "Men\u00b7schen\u00b7werk", "zer\u00b7fal\u00b7len", "ist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Berg steht fest und hoch,", "tokens": ["Der", "Berg", "steht", "fest", "und", "hoch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein Gottesname noch.", "tokens": ["Sein", "Got\u00b7tes\u00b7na\u00b7me", "noch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ihr Wandrer, die ihr sinnet viel,", "tokens": ["Ihr", "Wand\u00b7rer", ",", "die", "ihr", "sin\u00b7net", "viel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verge\u00dft nicht jenes ", "tokens": ["Ver\u00b7ge\u00dft", "nicht", "je\u00b7nes"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "PDS"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ihr M\u00e4gdlein hier auf Tanz und Spiel,", "tokens": ["Ihr", "M\u00e4gd\u00b7lein", "hier", "auf", "Tanz", "und", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denkt fromm der ", "tokens": ["Denkt", "fromm", "der"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART"], "meter": "-+-", "measure": "amphibrach.single"}}}}}