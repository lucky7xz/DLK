{"textgrid.poem.42669": {"metadata": {"author": {"name": "Ratschky, Joseph Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf, Br\u00fcder, kr\u00e4nzt mit Epheu die Per\u00fccken", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf, Br\u00fcder, kr\u00e4nzt mit Epheu die Per\u00fccken", "tokens": ["Auf", ",", "Br\u00fc\u00b7der", ",", "kr\u00e4nzt", "mit", "E\u00b7pheu", "die", "Pe\u00b7r\u00fc\u00b7cken"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "$,", "NN", "$,", "VVFIN", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und das Toppee,", "tokens": ["Und", "das", "Top\u00b7pee", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Und jauchzt, Bachanten gleich, mit trunkenem Entz\u00fccken:", "tokens": ["Und", "jauchzt", ",", "Ba\u00b7ch\u00b7an\u00b7ten", "gleich", ",", "mit", "trun\u00b7ke\u00b7nem", "Ent\u00b7z\u00fc\u00b7cken", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "NN", "ADV", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "O Evan Evoe!", "tokens": ["O", "E\u00b7van", "E\u00b7voe", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$."], "meter": "-+---", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Tischt Gl\u00e4ser auf wie M\u00f6rser und Karthaunen", "tokens": ["Tischt", "Gl\u00e4\u00b7ser", "auf", "wie", "M\u00f6r\u00b7ser", "und", "Kar\u00b7thau\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "PTKVZ", "KOKOM", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr jedermann,", "tokens": ["F\u00fcr", "je\u00b7der\u00b7mann", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "PIS", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und k\u00fcndigt allen H\u00fchnern, Enten und Kapaunen", "tokens": ["Und", "k\u00fcn\u00b7digt", "al\u00b7len", "H\u00fch\u00b7nern", ",", "En\u00b7ten", "und", "Ka\u00b7pau\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das Todesurtheil an!", "tokens": ["Das", "To\u00b7des\u00b7urt\u00b7heil", "an", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und ihr, Amphions kunsterfahrne Sch\u00fcler,", "tokens": ["Und", "ihr", ",", "Am\u00b7phi\u00b7ons", "kuns\u00b7ter\u00b7fahr\u00b7ne", "Sch\u00fc\u00b7ler", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die ihr von Haus", "tokens": ["Die", "ihr", "von", "Haus"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Zu Hause klimpernd zieht, zerlumpte Lautenspieler,", "tokens": ["Zu", "Hau\u00b7se", "klim\u00b7pernd", "zieht", ",", "zer\u00b7lump\u00b7te", "Lau\u00b7ten\u00b7spie\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verherrlicht unsern Schmaus!", "tokens": ["Ver\u00b7herr\u00b7licht", "un\u00b7sern", "Schmaus", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Denn heute weihen wir dem Gott der Reben", "tokens": ["Denn", "heu\u00b7te", "wei\u00b7hen", "wir", "dem", "Gott", "der", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Diess Heiligthum:", "tokens": ["Diess", "Hei\u00b7lig\u00b7thum", ":"], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Schon funkeln hundert goldne Lampen. Seht, sie schweben", "tokens": ["Schon", "fun\u00b7keln", "hun\u00b7dert", "gold\u00b7ne", "Lam\u00b7pen", ".", "Seht", ",", "sie", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "CARD", "ADJA", "NN", "$.", "VVFIN", "$,", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von Hand zu Hand herum.", "tokens": ["Von", "Hand", "zu", "Hand", "he\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Komm, Vater Bachus! eine Nektartonne", "tokens": ["Komm", ",", "Va\u00b7ter", "Ba\u00b7chus", "!", "ei\u00b7ne", "Nek\u00b7tar\u00b7ton\u00b7ne"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "NN", "NE", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sey dein Altar:", "tokens": ["Sey", "dein", "Al\u00b7tar", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Entz\u00fcckt bringt unser Schwarm im Taumel seiner Wonne", "tokens": ["Ent\u00b7z\u00fcckt", "bringt", "un\u00b7ser", "Schwarm", "im", "Tau\u00b7mel", "sei\u00b7ner", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dir t\u00e4glich Opfer dar.", "tokens": ["Dir", "t\u00e4g\u00b7lich", "Op\u00b7fer", "dar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Zum Hohenpriester sey der Wirth erkoren;", "tokens": ["Zum", "Ho\u00b7hen\u00b7pries\u00b7ter", "sey", "der", "Wirth", "er\u00b7ko\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sein Domherrnbauch", "tokens": ["Sein", "Dom\u00b7herrn\u00b7bauch"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ist stadtber\u00fcchtigt: Kupfernas' und lange Ohren", "tokens": ["Ist", "stadt\u00b7be\u00b7r\u00fcch\u00b7tigt", ":", "Kup\u00b7fer\u00b7nas'", "und", "lan\u00b7ge", "Oh\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "$.", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gab die Natur ihm auch.", "tokens": ["Gab", "die", "Na\u00b7tur", "ihm", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ja selbst als Wunderth\u00e4ter ist er, Br\u00fcder!", "tokens": ["Ja", "selbst", "als", "Wun\u00b7der\u00b7th\u00e4\u00b7ter", "ist", "er", ",", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKANT", "ADV", "KOUS", "NN", "VAFIN", "PPER", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Uns schon bew\u00e4hrt;", "tokens": ["Uns", "schon", "be\u00b7w\u00e4hrt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Hat er nicht oft genug uns Birnenmost und Cider", "tokens": ["Hat", "er", "nicht", "oft", "ge\u00b7nug", "uns", "Bir\u00b7nen\u00b7most", "und", "Ci\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "ADV", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In reinen Wein verkehrt?", "tokens": ["In", "rei\u00b7nen", "Wein", "ver\u00b7kehrt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wenn du nicht noch vom letzten G\u00f6ttermahle", "tokens": ["Wenn", "du", "nicht", "noch", "vom", "letz\u00b7ten", "G\u00f6t\u00b7ter\u00b7mah\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Halbtaumelnd bist,", "tokens": ["Halb\u00b7tau\u00b7melnd", "bist", ","], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "So sieh, Gott Liber! wie aus sch\u00e4umendem Pokale", "tokens": ["So", "sieh", ",", "Gott", "Li\u00b7ber", "!", "wie", "aus", "sch\u00e4u\u00b7men\u00b7dem", "Po\u00b7ka\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "$,", "NN", "NN", "$.", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Opferwein hier fliesst.", "tokens": ["Der", "Op\u00b7fer\u00b7wein", "hier", "fliesst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Gieb, wie dem K\u00f6nig Midas, unsern Renten", "tokens": ["Gieb", ",", "wie", "dem", "K\u00f6\u00b7nig", "Mi\u00b7das", ",", "un\u00b7sern", "Ren\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "$,", "PWAV", "ART", "NN", "NE", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein gut Gedeihn,", "tokens": ["Ein", "gut", "Ge\u00b7deihn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "So wollen wir mit Lust von unserm Gut den Zehnten", "tokens": ["So", "wol\u00b7len", "wir", "mit", "Lust", "von", "un\u00b7serm", "Gut", "den", "Zehn\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "St\u00e4ts diesem Tempel weihn.", "tokens": ["St\u00e4ts", "die\u00b7sem", "Tem\u00b7pel", "weihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wir wollen ihn zum Wallfahrtsorte w\u00e4hlen:", "tokens": ["Wir", "wol\u00b7len", "ihn", "zum", "Wall\u00b7fahrts\u00b7or\u00b7te", "w\u00e4h\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nie sey er leer,", "tokens": ["Nie", "sey", "er", "leer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und jeder Murrkopf, den Verdruss und Kummer qu\u00e4len,", "tokens": ["Und", "je\u00b7der", "Murr\u00b7kopf", ",", "den", "Ver\u00b7druss", "und", "Kum\u00b7mer", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Verlobe sich hieher!", "tokens": ["Ver\u00b7lo\u00b7be", "sich", "hie\u00b7her", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}