{"dta.poem.8758": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxXVI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Ein krausgelocktes Haar,", "tokens": ["Ein", "kraus\u00b7ge\u00b7lock\u00b7tes", "Haar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und eines Feuerauges dunkler Blitz,", "tokens": ["Und", "ei\u00b7nes", "Feu\u00b7er\u00b7au\u00b7ges", "dunk\u00b7ler", "Blitz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und ach, zum L\u00e4cheln stets bereit,", "tokens": ["Und", "ach", ",", "zum", "L\u00e4\u00b7cheln", "stets", "be\u00b7reit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "APPRART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Rede holder Sitz,", "tokens": ["Der", "Re\u00b7de", "hol\u00b7der", "Sitz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein s\u00fc\u00dfer Mund voll sch\u00f6ner Sinnlichkeit!", "tokens": ["Ein", "s\u00fc\u00b7\u00dfer", "Mund", "voll", "sch\u00f6\u00b7ner", "Sinn\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da w\u00e4hnt' ich noch, als w\u00e4re der Besitz", "tokens": ["Da", "w\u00e4hnt'", "ich", "noch", ",", "als", "w\u00e4\u00b7re", "der", "Be\u00b7sitz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KOKOM", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das einz'ge Gut auf diesem Lebensgang,", "tokens": ["Das", "einz'\u00b7ge", "Gut", "auf", "die\u00b7sem", "Le\u00b7bens\u00b7gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und nach ihm rang", "tokens": ["Und", "nach", "ihm", "rang"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN"], "meter": "++-+", "measure": "iambic.di"}, "line.9": {"text": "Mein junger Sinn und mein beth\u00f6rter Witz.", "tokens": ["Mein", "jun\u00b7ger", "Sinn", "und", "mein", "be\u00b7th\u00f6r\u00b7ter", "Witz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da sah ich bald im Wandel der Gestalt", "tokens": ["Da", "sah", "ich", "bald", "im", "Wan\u00b7del", "der", "Ge\u00b7stalt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vor mir die Jugend alt,", "tokens": ["Vor", "mir", "die", "Ju\u00b7gend", "alt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und jede sch\u00f6n geschwung'ne Form verschwand;", "tokens": ["Und", "je\u00b7de", "sch\u00f6n", "ge\u00b7schwung'\u00b7ne", "Form", "ver\u00b7schwand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und ach, wonach ich griff in Hast,", "tokens": ["Und", "ach", ",", "wo\u00b7nach", "ich", "griff", "in", "Hast", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PWAV", "PPER", "VVFIN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Entfloh dem Unverstand,", "tokens": ["Ent\u00b7floh", "dem", "Un\u00b7ver\u00b7stand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und nie Besess'nes wurde mir zur Last:", "tokens": ["Und", "nie", "Be\u00b7sess'\u00b7nes", "wur\u00b7de", "mir", "zur", "Last", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NE", "VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Bis ich zulezt, nicht ohne Schmerz, empfand,", "tokens": ["Bis", "ich", "zu\u00b7lezt", ",", "nicht", "oh\u00b7ne", "Schmerz", ",", "emp\u00b7fand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "PTKNEG", "APPR", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df alles Sch\u00f6ne, was der Welt geh\u00f6rt,", "tokens": ["Da\u00df", "al\u00b7les", "Sch\u00f6\u00b7ne", ",", "was", "der", "Welt", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Sich selbst zerst\u00f6rt,", "tokens": ["Sich", "selbst", "zer\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Und nicht ertr\u00e4gt die rohe Menschenhand.", "tokens": ["Und", "nicht", "er\u00b7tr\u00e4gt", "die", "ro\u00b7he", "Men\u00b7schen\u00b7hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So ward ich ruhiger und kalt zulezt,", "tokens": ["So", "ward", "ich", "ru\u00b7hi\u00b7ger", "und", "kalt", "zu\u00b7lezt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und gerne m\u00f6cht' ich jezt", "tokens": ["Und", "ger\u00b7ne", "m\u00f6cht'", "ich", "jezt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Welt, wie au\u00dfer ihr, von ferne schau'n:", "tokens": ["Die", "Welt", ",", "wie", "au\u00b7\u00dfer", "ihr", ",", "von", "fer\u00b7ne", "schau'n", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "APPR", "PPER", "$,", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Erlitten hat das bange Herz", "tokens": ["Er\u00b7lit\u00b7ten", "hat", "das", "ban\u00b7ge", "Herz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Begier und Furcht und Grau'n,", "tokens": ["Be\u00b7gier", "und", "Furcht", "und", "Grau'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Erlitten hat es seinen Theil von Schmerz,", "tokens": ["Er\u00b7lit\u00b7ten", "hat", "es", "sei\u00b7nen", "Theil", "von", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und in das Leben sezt es kein Vertrau'n;", "tokens": ["Und", "in", "das", "Le\u00b7ben", "sezt", "es", "kein", "Ver\u00b7trau'n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ihm werde die gewaltige Natur", "tokens": ["Ihm", "wer\u00b7de", "die", "ge\u00b7wal\u00b7ti\u00b7ge", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Zum Mittel nur,", "tokens": ["Zum", "Mit\u00b7tel", "nur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Aus eigner Kraft sich eine Welt zu bau'n.", "tokens": ["Aus", "eig\u00b7ner", "Kraft", "sich", "ei\u00b7ne", "Welt", "zu", "bau'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "ADJA", "NN", "PRF", "ART", "NN", "APPR", "NE", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}