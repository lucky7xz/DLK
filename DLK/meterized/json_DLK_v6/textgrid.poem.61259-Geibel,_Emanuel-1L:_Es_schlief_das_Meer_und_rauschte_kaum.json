{"textgrid.poem.61259": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es schlief das Meer und rauschte kaum", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es schlief das Meer und rauschte kaum", "tokens": ["Es", "schlief", "das", "Meer", "und", "rauschte", "kaum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und war doch allen Schimmers voll,", "tokens": ["Und", "war", "doch", "al\u00b7len", "Schim\u00b7mers", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der durch der Wolken Silberflaum", "tokens": ["Der", "durch", "der", "Wol\u00b7ken", "Sil\u00b7berf\u00b7laum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom lichten Monde niederquoll;", "tokens": ["Vom", "lich\u00b7ten", "Mon\u00b7de", "nie\u00b7der\u00b7quoll", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im Blau verschwamm die ferne Flut,", "tokens": ["Im", "Blau", "ver\u00b7schwamm", "die", "fer\u00b7ne", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie Bernstein flimmerte der Sand;", "tokens": ["Wie", "Bern\u00b7stein", "flim\u00b7mer\u00b7te", "der", "Sand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich aber schritt in ernstem Mut", "tokens": ["Ich", "a\u00b7ber", "schritt", "in", "erns\u00b7tem", "Mut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hinunter und hinauf den Strand.", "tokens": ["Hin\u00b7un\u00b7ter", "und", "hin\u00b7auf", "den", "Strand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "O was in solcher stillen Nacht", "tokens": ["O", "was", "in", "sol\u00b7cher", "stil\u00b7len", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "PWS", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch eine Menschenseele zieht,", "tokens": ["Durch", "ei\u00b7ne", "Men\u00b7schen\u00b7see\u00b7le", "zieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bei Tag hat's keiner nachgedacht,", "tokens": ["Bei", "Tag", "hat's", "kei\u00b7ner", "nach\u00b7ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und spricht es aus kein irdisch Lied.", "tokens": ["Und", "spricht", "es", "aus", "kein", "ir\u00b7disch", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PIAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es ist ein Hauch, der wunderbar", "tokens": ["Es", "ist", "ein", "Hauch", ",", "der", "wun\u00b7der\u00b7bar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Aus unsrer ew'gen Heimat weht,", "tokens": ["Aus", "uns\u00b7rer", "ew'\u00b7gen", "Hei\u00b7mat", "weht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein innig Schauen tief und klar,", "tokens": ["Ein", "in\u00b7nig", "Schau\u00b7en", "tief", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Ein L\u00e4cheln halb und halb Gebet.", "tokens": ["Ein", "L\u00e4\u00b7cheln", "halb", "und", "halb", "Ge\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da sp\u00fcrst du still und k\u00f6rperlos", "tokens": ["Da", "sp\u00fcrst", "du", "still", "und", "k\u00f6r\u00b7per\u00b7los"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein segnend Walten um dich her,", "tokens": ["Ein", "seg\u00b7nend", "Wal\u00b7ten", "um", "dich", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du f\u00fchlst, du ruhst in Gottes Scho\u00df,", "tokens": ["Du", "f\u00fchlst", ",", "du", "ruhst", "in", "Got\u00b7tes", "Scho\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wo du wandelst, wallt auch er;", "tokens": ["Und", "wo", "du", "wan\u00b7delst", ",", "wallt", "auch", "er", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "ADV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Tr\u00e4nen all sind abgetan,", "tokens": ["Die", "Tr\u00e4\u00b7nen", "all", "sind", "ab\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Dornen tragen Rosenglut,", "tokens": ["Die", "Dor\u00b7nen", "tra\u00b7gen", "Ro\u00b7seng\u00b7lut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es taucht die Liebe wie ein Schwan", "tokens": ["Es", "taucht", "die", "Lie\u00b7be", "wie", "ein", "Schwan"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Aus deines Lebens dunkler Flut.", "tokens": ["Aus", "dei\u00b7nes", "Le\u00b7bens", "dunk\u00b7ler", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und was am schwersten dich bedroht,", "tokens": ["Und", "was", "am", "schwers\u00b7ten", "dich", "be\u00b7droht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPRART", "ADJA", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir zeigt's ein liebes Angesicht.", "tokens": ["Dir", "zeigt's", "ein", "lie\u00b7bes", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum Freiheitsherold wird der Tod,", "tokens": ["Zum", "Frei\u00b7heits\u00b7he\u00b7rold", "wird", "der", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der deines Wesens Siegel bricht;", "tokens": ["Der", "dei\u00b7nes", "We\u00b7sens", "Sie\u00b7gel", "bricht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du schaust ins Aug' ihm still vertraut,", "tokens": ["Du", "schaust", "ins", "Aug'", "ihm", "still", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPER", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von heil'gem Schauder nur ber\u00fchrt,", "tokens": ["Von", "heil'\u00b7gem", "Schau\u00b7der", "nur", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gleichwie ein Br\u00e4ut'gam, den die Braut", "tokens": ["Gleich\u00b7wie", "ein", "Br\u00e4ut'\u00b7gam", ",", "den", "die", "Braut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NE", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zum seligsten Geheimnis f\u00fchrt.", "tokens": ["Zum", "se\u00b7ligs\u00b7ten", "Ge\u00b7heim\u00b7nis", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Genug, genug! Halt ein, mein Lied!", "tokens": ["Ge\u00b7nug", ",", "ge\u00b7nug", "!", "Halt", "ein", ",", "mein", "Lied", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$.", "VVIMP", "PTKVZ", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Denn was bei Nacht und Mondenlicht", "tokens": ["Denn", "was", "bei", "Nacht", "und", "Mon\u00b7den\u00b7licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch eine Menschenseele zieht,", "tokens": ["Durch", "ei\u00b7ne", "Men\u00b7schen\u00b7see\u00b7le", "zieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das sagt kein irdisches Gedicht;", "tokens": ["Das", "sagt", "kein", "ir\u00b7di\u00b7sches", "Ge\u00b7dicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Hauch ist's, der da wunderbar", "tokens": ["Ein", "Hauch", "ist's", ",", "der", "da", "wun\u00b7der\u00b7bar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$,", "PRELS", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von Edens Friedenspalmen weht,", "tokens": ["Von", "E\u00b7dens", "Frie\u00b7dens\u00b7pal\u00b7men", "weht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein wortlos Schauen tief und klar,", "tokens": ["Ein", "wort\u00b7los", "Schau\u00b7en", "tief", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein L\u00e4cheln halb und halb Gebet.", "tokens": ["Ein", "L\u00e4\u00b7cheln", "halb", "und", "halb", "Ge\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}