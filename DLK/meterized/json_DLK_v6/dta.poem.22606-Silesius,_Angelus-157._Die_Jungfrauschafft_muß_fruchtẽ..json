{"dta.poem.22606": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "157. Die Jungfrauschafft mu\u00df fruchte\u0303.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Gott liebt die Jungfrausch afft um\u0303 jhrer s\u00fcssen Fr\u00fcchte:", "tokens": ["Gott", "liebt", "die", "Jung\u00b7frausch", "afft", "um\u0303", "jhrer", "s\u00fcs\u00b7sen", "Fr\u00fcch\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Alleine l\u00e4st Er sie nicht f\u00fcr sein Angesichte.", "tokens": ["Al\u00b7lei\u00b7ne", "l\u00e4st", "Er", "sie", "nicht", "f\u00fcr", "sein", "An\u00b7ge\u00b7sich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}