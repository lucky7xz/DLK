{"textgrid.poem.65113": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bist du denn noch so wenig vorgeschritten", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bist du denn noch so wenig vorgeschritten", "tokens": ["Bist", "du", "denn", "noch", "so", "we\u00b7nig", "vor\u00b7ge\u00b7schrit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADV", "PIS", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf der Erkenntni\u00df Bahn, der dornenvollen,", "tokens": ["Auf", "der", "Er\u00b7kennt\u00b7ni\u00df", "Bahn", ",", "der", "dor\u00b7nen\u00b7vol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "PRELS", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df du die Menschen z\u00fcrnen magst und grollen", "tokens": ["Da\u00df", "du", "die", "Men\u00b7schen", "z\u00fcr\u00b7nen", "magst", "und", "grol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "VMFIN", "KON", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr das, was du erduldet und gelitten?", "tokens": ["F\u00fcr", "das", ",", "was", "du", "er\u00b7dul\u00b7det", "und", "ge\u00b7lit\u00b7ten", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PWS", "PPER", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ob auch als Antwort deiner Flehensbitten", "tokens": ["Ob", "auch", "als", "Ant\u00b7wort", "dei\u00b7ner", "Fle\u00b7hens\u00b7bit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "KOUS", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur lauter ihres Hasses Fl\u00fcche schellen,", "tokens": ["Nur", "lau\u00b7ter", "ih\u00b7res", "Has\u00b7ses", "Fl\u00fc\u00b7che", "schel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "PPOSAT", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein dunkles M\u00fcssen war's, kein freies Wollen,", "tokens": ["Ein", "dunk\u00b7les", "M\u00fcs\u00b7sen", "wa\u00b7r's", ",", "kein", "frei\u00b7es", "Wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wenn sie dein Herz vom Gl\u00fccke abgeschnitten.", "tokens": ["Wenn", "sie", "dein", "Herz", "vom", "Gl\u00fc\u00b7cke", "ab\u00b7ge\u00b7schnit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Denn wahllos wandelt Jeder auf den Pfaden,", "tokens": ["Denn", "wahl\u00b7los", "wan\u00b7delt", "Je\u00b7der", "auf", "den", "Pfa\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf welchen Kr\u00e4fte, die sich tief umfloren,", "tokens": ["Auf", "wel\u00b7chen", "Kr\u00e4f\u00b7te", ",", "die", "sich", "tief", "um\u00b7flo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "$,", "PRELS", "PRF", "ADJD", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Mit dunkelm\u00e4cht'gem Spruche ihn geladen.", "tokens": ["Mit", "dun\u00b7kel\u00b7m\u00e4cht'\u00b7gem", "Spru\u00b7che", "ihn", "ge\u00b7la\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Zum Henkerdienste wurden sie geboren.", "tokens": ["Zum", "Hen\u00b7ker\u00b7diens\u00b7te", "wur\u00b7den", "sie", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du aber freue dich, wenn Gottes Gnaden", "tokens": ["Du", "a\u00b7ber", "freu\u00b7e", "dich", ",", "wenn", "Got\u00b7tes", "Gna\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$,", "KOUS", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zum Werkzeug dich des Segens auserkoren.", "tokens": ["Zum", "Werk\u00b7zeug", "dich", "des", "Se\u00b7gens", "au\u00b7ser\u00b7ko\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}