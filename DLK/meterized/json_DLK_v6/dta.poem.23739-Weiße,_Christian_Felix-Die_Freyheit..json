{"dta.poem.23739": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Die Freyheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Warum, du kleine Nachtigall,", "tokens": ["Wa\u00b7rum", ",", "du", "klei\u00b7ne", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00f6r ich nicht deiner Stimme Schall", "tokens": ["H\u00f6r", "ich", "nicht", "dei\u00b7ner", "Stim\u00b7me", "Schall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "PTKNEG", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mehr der Natur zu Ehren?", "tokens": ["Mehr", "der", "Na\u00b7tur", "zu", "Eh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "APPR", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Du sangst in Str\u00e4uchen ja zuvor", "tokens": ["Du", "sangst", "in", "Str\u00e4u\u00b7chen", "ja", "zu\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So wundersch\u00f6n, da\u00df aller V\u00f6gel Chor", "tokens": ["So", "wun\u00b7der\u00b7sch\u00f6n", ",", "da\u00df", "al\u00b7ler", "V\u00f6\u00b7gel", "Chor"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Schwieg, wenn du sangst, um dich zu", "tokens": ["Schwieg", ",", "wenn", "du", "sangst", ",", "um", "dich", "zu"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "VVFIN", "$,", "KOUI", "PRF", "APPR"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "h\u00f6ren.", "tokens": ["h\u00f6\u00b7ren", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Jm goldnen Bauer sitzest du;", "tokens": ["Jm", "gold\u00b7nen", "Bau\u00b7er", "sit\u00b7zest", "du", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich trage dir die Speise zu", "tokens": ["Ich", "tra\u00b7ge", "dir", "die", "Spei\u00b7se", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schon mit dem fr\u00fchsten Morgen.", "tokens": ["Schon", "mit", "dem", "fr\u00fchs\u00b7ten", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kein Sturm und Regen schadet dir;", "tokens": ["Kein", "Sturm", "und", "Re\u00b7gen", "scha\u00b7det", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch du singst nicht, und sitzest traurig", "tokens": ["Doch", "du", "singst", "nicht", ",", "und", "sit\u00b7zest", "trau\u00b7rig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "$,", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "hier,", "tokens": ["hier", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Als h\u00e4ttest du recht schwere Sorgen.", "tokens": ["Als", "h\u00e4t\u00b7test", "du", "recht", "schwe\u00b7re", "Sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie, sollt es dich vielleicht gereun,", "tokens": ["Wie", ",", "sollt", "es", "dich", "viel\u00b7leicht", "ge\u00b7reun", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bey mir hier eingesperrt zu seyn?", "tokens": ["Bey", "mir", "hier", "ein\u00b7ge\u00b7sperrt", "zu", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da flieg in Freyheit wieder!", "tokens": ["Da", "flieg", "in", "Frey\u00b7heit", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O ja, du singst, schon h\u00f6r ich dich", "tokens": ["O", "ja", ",", "du", "singst", ",", "schon", "h\u00f6r", "ich", "dich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "ITJ", "$,", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vom n\u00e4chsten Baum, und du belohnest", "tokens": ["Vom", "n\u00e4chs\u00b7ten", "Baum", ",", "und", "du", "be\u00b7loh\u00b7nest"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "mich", "tokens": ["mich"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Daf\u00fcr durch deine besten Lieder!", "tokens": ["Da\u00b7f\u00fcr", "durch", "dei\u00b7ne", "bes\u00b7ten", "Lie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}