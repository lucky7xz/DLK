{"dta.poem.2150": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Zweifel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sollt unsre Lust an Gottes Werken dem Sch\u00f6pfer", "tokens": ["Sollt", "uns\u00b7re", "Lust", "an", "Got\u00b7tes", "Wer\u00b7ken", "dem", "Sch\u00f6p\u00b7fer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "APPR", "NN", "NN", "ART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "auch gef\u00e4llig seyn?", "tokens": ["auch", "ge\u00b7f\u00e4l\u00b7lig", "seyn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Der Zweifel fiel mir oft vor diesem, und f\u00e4llt mir noch", "tokens": ["Der", "Zwei\u00b7fel", "fiel", "mir", "oft", "vor", "die\u00b7sem", ",", "und", "f\u00e4llt", "mir", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "PDAT", "$,", "KON", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "zuweilen ein,", "tokens": ["zu\u00b7wei\u00b7len", "ein", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Doch find ich, da\u00df er ungegr\u00fcndet. Wir lehren in", "tokens": ["Doch", "find", "ich", ",", "da\u00df", "er", "un\u00b7ge\u00b7gr\u00fcn\u00b7det", ".", "Wir", "leh\u00b7ren", "in"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "$.", "PPER", "VVFIN", "APPR"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "der Christenheit,", "tokens": ["der", "Chris\u00b7ten\u00b7heit", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Da\u00df Gott auf unsern Wandel achte. W\u00e4r es nun eine", "tokens": ["Da\u00df", "Gott", "auf", "un\u00b7sern", "Wan\u00b7del", "ach\u00b7te", ".", "W\u00e4r", "es", "nun", "ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "PPOSAT", "NN", "ADJA", "$.", "VAFIN", "PPER", "ADV", "ART"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Billigkeit,", "tokens": ["Bil\u00b7lig\u00b7keit", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Zu glauben, da\u00df die ewge Liebe uns, blo\u00df zu unsrer Quaal,", "tokens": ["Zu", "glau\u00b7ben", ",", "da\u00df", "die", "ew\u00b7ge", "Lie\u00b7be", "uns", ",", "blo\u00df", "zu", "uns\u00b7rer", "Qua\u00b7al", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KOUS", "ART", "ADJA", "NN", "PPER", "$,", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "betrachte,", "tokens": ["be\u00b7trach\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Und nur auf unser S\u00fcndigen, um uns darum zu strafen,", "tokens": ["Und", "nur", "auf", "un\u00b7ser", "S\u00fcn\u00b7di\u00b7gen", ",", "um", "uns", "da\u00b7rum", "zu", "stra\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "$,", "KOUI", "PRF", "PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.12": {"text": "achte,", "tokens": ["ach\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["ADJA", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Nicht aber auch, wenn wir die Pflichten,", "tokens": ["Nicht", "a\u00b7ber", "auch", ",", "wenn", "wir", "die", "Pflich\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Wozu er uns erschuf, verrichten,", "tokens": ["Wo\u00b7zu", "er", "uns", "er\u00b7schuf", ",", "ver\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Und wenn wir uns, zu seinem Ruhm, uns sein zu freuen,", "tokens": ["Und", "wenn", "wir", "uns", ",", "zu", "sei\u00b7nem", "Ruhm", ",", "uns", "sein", "zu", "freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,", "PPER", "VAINF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "nicht entbrechen?", "tokens": ["nicht", "ent\u00b7bre\u00b7chen", "?"], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Die\u00df scheint der Eigenschaft der Liebe und Huld schnur-", "tokens": ["Die\u00df", "scheint", "der", "Ei\u00b7gen\u00b7schaft", "der", "Lie\u00b7be", "und", "Huld", "schnur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "ART", "NN", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "stracks zu widersprechen.", "tokens": ["stracks", "zu", "wi\u00b7der\u00b7spre\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.19": {"text": "Es scheint vielmehr, er habe selbst uns diese Wahrheit", "tokens": ["Es", "scheint", "viel\u00b7mehr", ",", "er", "ha\u00b7be", "selbst", "uns", "die\u00b7se", "Wahr\u00b7heit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VAFIN", "ADV", "PPER", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "eingepr\u00e4gt,", "tokens": ["ein\u00b7ge\u00b7pr\u00e4gt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.21": {"text": "Es sey ein solcher Gott kein Gott, der lauter Zorn und", "tokens": ["Es", "sey", "ein", "sol\u00b7cher", "Gott", "kein", "Gott", ",", "der", "lau\u00b7ter", "Zorn", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "PIAT", "NN", "PIAT", "NN", "$,", "PRELS", "PIAT", "NN", "KON"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Rachgier hegt.", "tokens": ["Rach\u00b7gier", "hegt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}