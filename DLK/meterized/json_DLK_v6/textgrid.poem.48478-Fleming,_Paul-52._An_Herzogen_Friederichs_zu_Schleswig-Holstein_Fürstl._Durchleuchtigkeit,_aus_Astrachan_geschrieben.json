{"textgrid.poem.48478": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "52. An Herzogen Friederichs zu Schleswig-Holstein F\u00fcrstl. Durchleuchtigkeit, aus Astrachan geschrieben", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schau, edles Holstein, auf, erfreu' dich deiner Ehren", "tokens": ["Schau", ",", "ed\u00b7les", "Hol\u00b7stein", ",", "auf", ",", "er\u00b7freu'", "dich", "dei\u00b7ner", "Eh\u00b7ren"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "PTKVZ", "$,", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "und hilf dir deinen Ruhm auch selbst durch dich vermehren,", "tokens": ["und", "hilf", "dir", "dei\u00b7nen", "Ruhm", "auch", "selbst", "durch", "dich", "ver\u00b7meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PPOSAT", "NN", "ADV", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "indem dein Gl\u00fcck und Preis so hoch und sch\u00f6ne bl\u00fcht,", "tokens": ["in\u00b7dem", "dein", "Gl\u00fcck", "und", "Preis", "so", "hoch", "und", "sch\u00f6\u00b7ne", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "NN", "ADV", "ADJD", "KON", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "da\u00df ganz Europa nun auf dich alleine sieht!", "tokens": ["da\u00df", "ganz", "Eu\u00b7ro\u00b7pa", "nun", "auf", "dich", "al\u00b7lei\u00b7ne", "sieht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NE", "ADV", "APPR", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nicht nur um dieses zwar, da\u00df, seit der harte Himmel,", "tokens": ["Nicht", "nur", "um", "die\u00b7ses", "zwar", ",", "da\u00df", ",", "seit", "der", "har\u00b7te", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "PDAT", "ADV", "$,", "KOUS", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "von S\u00fcnden aufgereizt, ein blutiges Get\u00fcmmel", "tokens": ["von", "S\u00fcn\u00b7den", "auf\u00b7ge\u00b7reizt", ",", "ein", "blu\u00b7ti\u00b7ges", "Ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "auf unser Vaterland, das arme, hat erregt,", "tokens": ["auf", "un\u00b7ser", "Va\u00b7ter\u00b7land", ",", "das", "ar\u00b7me", ",", "hat", "er\u00b7regt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "das achtzehn Jahre nun auf eine Stelle schl\u00e4gt", "tokens": ["das", "acht\u00b7zehn", "Jah\u00b7re", "nun", "auf", "ei\u00b7ne", "Stel\u00b7le", "schl\u00e4gt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "CARD", "NN", "ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "und noch nicht h\u00f6ret auf, \u2013 du einigs aller Landen", "tokens": ["und", "noch", "nicht", "h\u00f6\u00b7ret", "auf", ",", "\u2013", "du", "ei\u00b7nigs", "al\u00b7ler", "Lan\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PTKNEG", "VVFIN", "PTKVZ", "$,", "$(", "PPER", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "in Alemannien bist unverr\u00fcckt bestanden,", "tokens": ["in", "A\u00b7lem\u00b7an\u00b7ni\u00b7en", "bist", "un\u00b7ver\u00b7r\u00fcckt", "be\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "so ganz, da\u00df, da die Glut die n\u00e4chsten Nachbarn fra\u00df", "tokens": ["so", "ganz", ",", "da\u00df", ",", "da", "die", "Glut", "die", "n\u00e4chs\u00b7ten", "Nach\u00b7barn", "fra\u00df"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "$,", "KOUS", "ART", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "und nun der wilde Brand dir auf der Achseln sa\u00df,", "tokens": ["und", "nun", "der", "wil\u00b7de", "Brand", "dir", "auf", "der", "Ach\u00b7seln", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "dich doch kein Schade traf. Wie wenn das Wetter blitzet", "tokens": ["dich", "doch", "kein", "Scha\u00b7de", "traf", ".", "Wie", "wenn", "das", "Wet\u00b7ter", "blit\u00b7zet"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "PIAT", "ADJD", "VVFIN", "$.", "KOKOM", "KOUS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "und auf den dicken Wald viel' Donnerkeile spr\u00fctzet,", "tokens": ["und", "auf", "den", "di\u00b7cken", "Wald", "viel'", "Don\u00b7ner\u00b7kei\u00b7le", "spr\u00fct\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "die steinern' Eiche sp\u00e4lt, der Fichten Kraft zerbricht:", "tokens": ["die", "stei\u00b7nern'", "Ei\u00b7che", "sp\u00e4lt", ",", "der", "Fich\u00b7ten", "Kraft", "zer\u00b7bricht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "blos an den Lorbeerbaum wagt sich kein Donner nicht.", "tokens": ["blos", "an", "den", "Lor\u00b7beer\u00b7baum", "wagt", "sich", "kein", "Don\u00b7ner", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PRF", "PIAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "So steht ein hoher Fels, l\u00e4\u00dft die erbosten Wellen", "tokens": ["So", "steht", "ein", "ho\u00b7her", "Fels", ",", "l\u00e4\u00dft", "die", "er\u00b7bos\u00b7ten", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "an seiner starken Brust umbsonst zur\u00fccke br\u00e4llen;", "tokens": ["an", "sei\u00b7ner", "star\u00b7ken", "Brust", "um\u00b7bsonst", "zu\u00b7r\u00fc\u00b7cke", "br\u00e4l\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADV", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "der Zorn der Flut kehrt umb, und, weil er mehr nicht kan,", "tokens": ["der", "Zorn", "der", "Flut", "kehrt", "umb", ",", "und", ",", "weil", "er", "mehr", "nicht", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$,", "KON", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "so sch\u00e4umt, so brennet er und f\u00e4llt sich selbsten an.", "tokens": ["so", "sch\u00e4umt", ",", "so", "bren\u00b7net", "er", "und", "f\u00e4llt", "sich", "selbs\u00b7ten", "an", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "KON", "VVFIN", "PRF", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Umb dieses zu voraus, da\u00df Fama steigt zu Wagen,", "tokens": ["Umb", "die\u00b7ses", "zu", "vo\u00b7raus", ",", "da\u00df", "Fa\u00b7ma", "steigt", "zu", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PDS", "PTKZU", "PTKVZ", "$,", "KOUS", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "dein unerh\u00f6rtes Lob der Erden anzusagen;", "tokens": ["dein", "un\u00b7er\u00b7h\u00f6r\u00b7tes", "Lob", "der", "Er\u00b7den", "an\u00b7zu\u00b7sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "sie schreit den V\u00f6lkern zu: Wenn, ruft sie, ists geschehn,", "tokens": ["sie", "schreit", "den", "V\u00f6l\u00b7kern", "zu", ":", "Wenn", ",", "ruft", "sie", ",", "ists", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$.", "KOUS", "$,", "VVFIN", "PPER", "$,", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "da\u00df man wol Persien in Holstein hat gesehn,", "tokens": ["da\u00df", "man", "wol", "Per\u00b7si\u00b7en", "in", "Hol\u00b7stein", "hat", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "NE", "APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "als wie es itzund k\u00f6mt? Das seltzame Ger\u00fcchte", "tokens": ["als", "wie", "es", "it\u00b7zund", "k\u00f6mt", "?", "Das", "selt\u00b7za\u00b7me", "Ge\u00b7r\u00fcch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PWAV", "PPER", "ADV", "VVFIN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "erschallet durch und durch. Der deutschen Welt Gesichte", "tokens": ["er\u00b7schal\u00b7let", "durch", "und", "durch", ".", "Der", "deut\u00b7schen", "Welt", "Ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "KON", "PTKVZ", "$.", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "folgt dieser Zeitung nach, schaut, wo denn wir noch sein,", "tokens": ["folgt", "die\u00b7ser", "Zei\u00b7tung", "nach", ",", "schaut", ",", "wo", "denn", "wir", "noch", "sein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "PTKVZ", "$,", "VVFIN", "$,", "PWAV", "KON", "PPER", "ADV", "VAINF", "$,"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.28": {"text": "und zeucht auf einen Tag mit uns zu ", "tokens": ["und", "zeucht", "auf", "ei\u00b7nen", "Tag", "mit", "uns", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "APPR", "PPER", "PTKZU"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Nichts Schlechtes geht hier vor. Kehrt hieher eure Sinnen", "tokens": ["Nichts", "Schlech\u00b7tes", "geht", "hier", "vor", ".", "Kehrt", "hie\u00b7her", "eu\u00b7re", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "NN", "VVFIN", "ADV", "PTKVZ", "$.", "VVFIN", "PAV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und seid mit mir bem\u00fcht, ihr deutschen Kastalinnen!", "tokens": ["und", "seid", "mit", "mir", "be\u00b7m\u00fcht", ",", "ihr", "deut\u00b7schen", "Kas\u00b7ta\u00b7lin\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPER", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bist du nur, Vaterland, nicht gar so \u00fcbel dran,", "tokens": ["Bist", "du", "nur", ",", "Va\u00b7ter\u00b7land", ",", "nicht", "gar", "so", "\u00fc\u00b7bel", "dran", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$,", "NN", "$,", "PTKNEG", "ADV", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "da\u00df du noch einen hast, der dich betauren kan,", "tokens": ["da\u00df", "du", "noch", "ei\u00b7nen", "hast", ",", "der", "dich", "be\u00b7tau\u00b7ren", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIS", "VAFIN", "$,", "PRELS", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "der mag den Schreibezeug in lauter Blute netzen", "tokens": ["der", "mag", "den", "Schrei\u00b7be\u00b7zeug", "in", "lau\u00b7ter", "Blu\u00b7te", "net\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "VMFIN", "ART", "NN", "APPR", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "und auf sein gro\u00dfes Buch die langen Kriege setzen,", "tokens": ["und", "auf", "sein", "gro\u00b7\u00dfes", "Buch", "die", "lan\u00b7gen", "Krie\u00b7ge", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "ein ungeliebtes Werk, mag seufzen einen Streit,", "tokens": ["ein", "un\u00b7ge\u00b7lieb\u00b7tes", "Werk", ",", "mag", "seuf\u00b7zen", "ei\u00b7nen", "Streit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VMFIN", "VVINF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "den wider sich noch f\u00fchrt die arme Christenheit,", "tokens": ["den", "wi\u00b7der", "sich", "noch", "f\u00fchrt", "die", "ar\u00b7me", "Chris\u00b7ten\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PRF", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "ihr eigner Feind und Tod. Ich will hingegen melden", "tokens": ["ihr", "eig\u00b7ner", "Feind", "und", "Tod", ".", "Ich", "will", "hin\u00b7ge\u00b7gen", "mel\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN", "$.", "PPER", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "ein weit viel sch\u00f6ner Tun, die Vorsicht eines Helden.", "tokens": ["ein", "weit", "viel", "sch\u00f6\u00b7ner", "Tun", ",", "die", "Vor\u00b7sicht", "ei\u00b7nes", "Hel\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PIAT", "ADJA", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "der, da Europe selbst ihr' eigne Brust durchstach,", "tokens": ["der", ",", "da", "Eu\u00b7ro\u00b7pe", "selbst", "ih\u00b7r'", "eig\u00b7ne", "Brust", "durch\u00b7stach", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "NE", "ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.12": {"text": "und itzt ihr sch\u00f6ner Leib gleich wie zu St\u00fccken brach,", "tokens": ["und", "itzt", "ihr", "sch\u00f6\u00b7ner", "Leib", "gleich", "wie", "zu", "St\u00fc\u00b7cken", "brach",