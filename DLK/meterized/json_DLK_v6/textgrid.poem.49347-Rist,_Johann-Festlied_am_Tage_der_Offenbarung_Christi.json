{"textgrid.poem.49347": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Festlied am Tage der Offenbarung Christi", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Werde licht, du Stadt der Heiden,", "tokens": ["Wer\u00b7de", "licht", ",", "du", "Stadt", "der", "Hei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "PPER", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und du, Salem, werde licht,", "tokens": ["Und", "du", ",", "Sa\u00b7lem", ",", "wer\u00b7de", "licht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "NE", "$,", "VAFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schaue, welch' ein Glanz mit Freuden", "tokens": ["Schau\u00b7e", ",", "welch'", "ein", "Glanz", "mit", "Freu\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAT", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueber deinem Haubt anbricht!", "tokens": ["Ue\u00b7ber", "dei\u00b7nem", "Haubt", "an\u00b7bricht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gott hat derer nicht vergessen,", "tokens": ["Gott", "hat", "de\u00b7rer", "nicht", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PDS", "PTKNEG", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Welch' im Finstern sind gesessen.", "tokens": ["Welch'", "im", "Fins\u00b7tern", "sind", "ge\u00b7ses\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Dunkelheit die muste weichen,", "tokens": ["Dun\u00b7kel\u00b7heit", "die", "mus\u00b7te", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Als die\u00df Licht kam in die Welt,", "tokens": ["Als", "die\u00df", "Licht", "kam", "in", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Dem kein anders ist zu gleichen,", "tokens": ["Dem", "kein", "an\u00b7ders", "ist", "zu", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADV", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Welches alle Ding' erh\u00e4lt;", "tokens": ["Wel\u00b7ches", "al\u00b7le", "Ding'", "er\u00b7h\u00e4lt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Die nach diesem Glanze sehen,", "tokens": ["Die", "nach", "die\u00b7sem", "Glan\u00b7ze", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "D\u00fcrfen nicht im Finstern gehen.", "tokens": ["D\u00fcr\u00b7fen", "nicht", "im", "Fins\u00b7tern", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Ach, wie waren wir verblendet,", "tokens": ["Ach", ",", "wie", "wa\u00b7ren", "wir", "ver\u00b7blen\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Ehe noch die\u00df Licht brach an!", "tokens": ["E\u00b7he", "noch", "die\u00df", "Licht", "brach", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PDS", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Ja, da hatte sich gewendet", "tokens": ["Ja", ",", "da", "hat\u00b7te", "sich", "ge\u00b7wen\u00b7det"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "VAFIN", "PRF", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Schier vom Himmel jedermann,", "tokens": ["Schier", "vom", "Him\u00b7mel", "je\u00b7der\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PIS", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Unser' Augen und Geberden", "tokens": ["Un\u00b7ser'", "Au\u00b7gen", "und", "Ge\u00b7ber\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Klebten bl\u00f6\u00dflich an der Erden.", "tokens": ["Kleb\u00b7ten", "bl\u00f6\u00df\u00b7lich", "an", "der", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Irdisch waren die Gedanken,", "tokens": ["Ir\u00b7disch", "wa\u00b7ren", "die", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Torheit hielt uns ganz verstrickt;", "tokens": ["Tor\u00b7heit", "hielt", "uns", "ganz", "ver\u00b7strickt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Satan macht' uns sch\u00e4ndlich wanken,", "tokens": ["Sa\u00b7tan", "macht'", "uns", "sch\u00e4nd\u00b7lich", "wan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Wahre Tugend lag verr\u00fcckt;", "tokens": ["Wah\u00b7re", "Tu\u00b7gend", "lag", "ver\u00b7r\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Fleisch und Welt hat uns betrogen", "tokens": ["Fleisch", "und", "Welt", "hat", "uns", "be\u00b7tro\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VAFIN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Und vom Himmel abgezogen.", "tokens": ["Und", "vom", "Him\u00b7mel", "ab\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Finsternis fand sich auf Erden,", "tokens": ["Fins\u00b7ter\u00b7nis", "fand", "sich", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.26": {"text": "Finster war es in der Lehr';", "tokens": ["Fins\u00b7ter", "war", "es", "in", "der", "Lehr'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Alles wolte finster werden", "tokens": ["Al\u00b7les", "wol\u00b7te", "fins\u00b7ter", "wer\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ADJD", "VAINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "So, da\u00df auch des H\u00f6chsten Ehr'", "tokens": ["So", ",", "da\u00df", "auch", "des", "H\u00f6chs\u00b7ten", "Ehr'"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.29": {"text": "Und der Wahrheit unterdessen", "tokens": ["Und", "der", "Wahr\u00b7heit", "un\u00b7ter\u00b7des\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "In dem Finstern ward vergessen.", "tokens": ["In", "dem", "Fins\u00b7tern", "ward", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Gottes Rat war uns verborgen,", "tokens": ["Got\u00b7tes", "Rat", "war", "uns", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Seine Gnade schien uns nicht,", "tokens": ["Sei\u00b7ne", "Gna\u00b7de", "schien", "uns", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Klein' und Gro\u00dfe musten sorgen,", "tokens": ["Klein'", "und", "Gro\u00b7\u00dfe", "mus\u00b7ten", "sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Jedem fehlt es an dem Licht,", "tokens": ["Je\u00b7dem", "fehlt", "es", "an", "dem", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Das zum rechten Himmelsleben", "tokens": ["Das", "zum", "rech\u00b7ten", "Him\u00b7mels\u00b7le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Seinen Glanz uns solte geben;", "tokens": ["Sei\u00b7nen", "Glanz", "uns", "sol\u00b7te", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Aber, wie herf\u00fcr gegangen", "tokens": ["A\u00b7ber", ",", "wie", "her\u00b7f\u00fcr", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "PWAV", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Ist der Aufgang aus der H\u00f6h',", "tokens": ["Ist", "der", "Auf\u00b7gang", "aus", "der", "H\u00f6h'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Haben wir das Licht empfangen,", "tokens": ["Ha\u00b7ben", "wir", "das", "Licht", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Welches so viel Angst und Weh'", "tokens": ["Wel\u00b7ches", "so", "viel", "Angst", "und", "Weh'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "PIAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.41": {"text": "Aus der Welt hinweg getrieben,", "tokens": ["Aus", "der", "Welt", "hin\u00b7weg", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APZR", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Da\u00df nichts Dunkles \u00fcbrig blieben.", "tokens": ["Da\u00df", "nichts", "Dunk\u00b7les", "\u00fcb\u00b7rig", "blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PIS", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Jesu, reines Licht der Seelen,", "tokens": ["Je\u00b7su", ",", "rei\u00b7nes", "Licht", "der", "See\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Du vertreibst die Finsternis,", "tokens": ["Du", "ver\u00b7treibst", "die", "Fins\u00b7ter\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.45": {"text": "Welch' in dieser S\u00fcndenh\u00f6len", "tokens": ["Welch'", "in", "die\u00b7ser", "S\u00fcn\u00b7den\u00b7h\u00f6\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Unsern Tritt macht ungewis;", "tokens": ["Un\u00b7sern", "Tritt", "macht", "un\u00b7ge\u00b7wis", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.47": {"text": "Jesu, deine Lieb' und Segen", "tokens": ["Je\u00b7su", ",", "dei\u00b7ne", "Lieb'", "und", "Se\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Leuchten uns auf unsern Wegen.", "tokens": ["Leuch\u00b7ten", "uns", "auf", "un\u00b7sern", "We\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "Nun, du wollest hie verbleiben,", "tokens": ["Nun", ",", "du", "wol\u00b7lest", "hie", "ver\u00b7blei\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Liebster Jesu, Tag und Nacht,", "tokens": ["Liebs\u00b7ter", "Je\u00b7su", ",", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.51": {"text": "Alles Finstre zu vertreiben,", "tokens": ["Al\u00b7les", "Finst\u00b7re", "zu", "ver\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Das uns so viel Schreckens macht;", "tokens": ["Das", "uns", "so", "viel", "Schre\u00b7ckens", "macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.53": {"text": "La\u00df uns nicht im Dunklen waten", "tokens": ["La\u00df", "uns", "nicht", "im", "Dunk\u00b7len", "wa\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PTKNEG", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Noch ins H\u00f6llenmeer geraten.", "tokens": ["Noch", "ins", "H\u00f6l\u00b7len\u00b7meer", "ge\u00b7ra\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.55": {"text": "Liebster Jesu, la\u00df uns leuchten", "tokens": ["Liebs\u00b7ter", "Je\u00b7su", ",", "la\u00df", "uns", "leuch\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NE", "$,", "VVIMP", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "Dein erfreulichs Angesicht,", "tokens": ["Dein", "er\u00b7freu\u00b7lichs", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.57": {"text": "La\u00df uns deine Gunst befeuchten,", "tokens": ["La\u00df", "uns", "dei\u00b7ne", "Gunst", "be\u00b7feuch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.58": {"text": "Wenn das Kreuzfeur auf uns sticht;", "tokens": ["Wenn", "das", "Kreuz\u00b7feur", "auf", "uns", "sticht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.59": {"text": "La\u00df uns ja wie Christen handlen", "tokens": ["La\u00df", "uns", "ja", "wie", "Chris\u00b7ten", "hand\u00b7len"], "token_info": ["word", 