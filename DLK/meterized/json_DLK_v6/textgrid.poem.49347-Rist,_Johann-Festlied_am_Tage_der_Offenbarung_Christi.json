"text": "Der zu deinem Reich' uns lenket.", "tokens": ["Der", "zu", "dei\u00b7nem", "Reich'", "uns", "len\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.73": {"text": "Gib, Herr Jesu, Kraft und St\u00e4rke,", "tokens": ["Gib", ",", "Herr", "Je\u00b7su", ",", "Kraft", "und", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "NE", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.74": {"text": "Da\u00df wir dir zur jeden Zeit", "tokens": ["Da\u00df", "wir", "dir", "zur", "je\u00b7den", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "APPRART", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.75": {"text": "Durch beliebte Glaubenswerke", "tokens": ["Durch", "be\u00b7lieb\u00b7te", "Glau\u00b7bens\u00b7wer\u00b7ke"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.76": {"text": "Folgen in Gerechtigkeit", "tokens": ["Fol\u00b7gen", "in", "Ge\u00b7rech\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.77": {"text": "Und hernach im Freudenleben", "tokens": ["Und", "her\u00b7nach", "im", "Freu\u00b7den\u00b7le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.78": {"text": "Heller als die Sterne schweben.", "tokens": ["Hel\u00b7ler", "als", "die", "Ster\u00b7ne", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "mea