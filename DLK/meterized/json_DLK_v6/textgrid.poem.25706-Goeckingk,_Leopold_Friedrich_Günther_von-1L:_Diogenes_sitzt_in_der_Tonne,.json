{"textgrid.poem.25706": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Diogenes sitzt in der Tonne,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Diogenes sitzt in der Tonne,", "tokens": ["Dio\u00b7ge\u00b7nes", "sitzt", "in", "der", "Ton\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Legt sie mit Sorgfalt in die Sonne,", "tokens": ["Legt", "sie", "mit", "Sorg\u00b7falt", "in", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Trinkt Wasser, keinen Wein:", "tokens": ["Trinkt", "Was\u00b7ser", ",", "kei\u00b7nen", "Wein", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und das soll n\u00e4rrisch seyn!", "tokens": ["Und", "das", "soll", "n\u00e4r\u00b7risch", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mops aber sitzet vor der Tonne,", "tokens": ["Mops", "a\u00b7ber", "sit\u00b7zet", "vor", "der", "Ton\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Verbirgt sie flei\u00dfig vor der Sonne,", "tokens": ["Ver\u00b7birgt", "sie", "flei\u00b7\u00dfig", "vor", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Trinkt, statt des Wassers, Wein:", "tokens": ["Trinkt", ",", "statt", "des", "Was\u00b7sers", ",", "Wein", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "KOUI", "ART", "NN", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und das soll weise seyn!", "tokens": ["Und", "das", "soll", "wei\u00b7se", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Und ich, nicht in, noch vor der Tonne,", "tokens": ["Und", "ich", ",", "nicht", "in", ",", "noch", "vor", "der", "Ton\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PTKNEG", "APPR", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Ich trink' im Schatten, in der Sonne,", "tokens": ["Ich", "trink'", "im", "Schat\u00b7ten", ",", "in", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Bald Wasser und bald Wein:", "tokens": ["Bald", "Was\u00b7ser", "und", "bald", "Wein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Was wird nun das wohl seyn?", "tokens": ["Was", "wird", "nun", "das", "wohl", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ART", "ADV", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}