{"textgrid.poem.60655": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Gartenfreund, halb St\u00e4dter und halb Bauersmann,", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Gartenfreund, halb St\u00e4dter und halb Bauersmann,", "tokens": ["Ein", "Gar\u00b7ten\u00b7freund", ",", "halb", "St\u00e4d\u00b7ter", "und", "halb", "Bau\u00b7ers\u00b7mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "NN", "KON", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Besa\u00df bei einem Dorf ein h\u00fcbsches Gartenfeld.", "tokens": ["Be\u00b7sa\u00df", "bei", "ei\u00b7nem", "Dorf", "ein", "h\u00fcb\u00b7sches", "Gar\u00b7ten\u00b7feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das hat er voller Flei\u00df bestellt", "tokens": ["Das", "hat", "er", "vol\u00b7ler", "Flei\u00df", "be\u00b7stellt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Ampfer und Salat und was man brauchen kann,", "tokens": ["Mit", "Amp\u00b7fer", "und", "Sa\u00b7lat", "und", "was", "man", "brau\u00b7chen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "PWS", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Um Margot Sonntags einen Strau\u00df zu binden:", "tokens": ["Um", "Mar\u00b7got", "Sonn\u00b7tags", "ei\u00b7nen", "Strau\u00df", "zu", "bin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NE", "ADV", "ART", "NE", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Jasmin und Quendel waren dort zu finden.", "tokens": ["Jas\u00b7min", "und", "Quen\u00b7del", "wa\u00b7ren", "dort", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VAFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da dies Idyll ein Hase st\u00f6rte, ging der Mann", "tokens": ["Da", "dies", "I\u00b7dyll", "ein", "Ha\u00b7se", "st\u00f6r\u00b7te", ",", "ging", "der", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PDS", "NN", "ART", "NN", "VVFIN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Zum Schlo\u00dfherrn, um dar\u00fcber Klage vorzubringen.", "tokens": ["Zum", "Schlo\u00df\u00b7herrn", ",", "um", "da\u00b7r\u00fc\u00b7ber", "Kla\u00b7ge", "vor\u00b7zu\u00b7brin\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUI", "PAV", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u00bbes nimmt\u00ab, so sprach er, \u00bbabends sp\u00e4t und morgens fr\u00fch", "tokens": ["\u00bb", "es", "nimmt", "\u00ab", ",", "so", "sprach", "er", ",", "\u00bb", "a\u00b7bends", "sp\u00e4t", "und", "mor\u00b7gens", "fr\u00fch"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "$(", "$,", "ADV", "VVFIN", "PPER", "$,", "$(", "ADV", "ADJD", "KON", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dort seinen Wechsel das verfluchte Vieh.", "tokens": ["Dort", "sei\u00b7nen", "Wech\u00b7sel", "das", "ver\u00b7fluch\u00b7te", "Vieh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Es spottet aller Schlingen,", "tokens": ["Es", "spot\u00b7tet", "al\u00b7ler", "Schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Und Steine nicht noch Kn\u00fcppel k\u00f6nnen es bezwingen.", "tokens": ["Und", "Stei\u00b7ne", "nicht", "noch", "Kn\u00fcp\u00b7pel", "k\u00f6n\u00b7nen", "es", "be\u00b7zwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKNEG", "ADV", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ein Hexenmeister scheint's zu sein!\u00ab", "tokens": ["Ein", "He\u00b7xen\u00b7meis\u00b7ter", "scheint's", "zu", "sein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKZU", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "\u00bbein Hexenmeister?\u00ab lacht der Herr; \u00bbich hege Zweifel,", "tokens": ["\u00bb", "ein", "He\u00b7xen\u00b7meis\u00b7ter", "?", "\u00ab", "lacht", "der", "Herr", ";", "\u00bb", "ich", "he\u00b7ge", "Zwei\u00b7fel", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$.", "$(", "VVFIN", "ART", "NN", "$.", "$(", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Doch w\u00e4r es selbst der Teufel,", "tokens": ["Doch", "w\u00e4r", "es", "selbst", "der", "Teu\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Mein Hektor fing ihn ein!", "tokens": ["Mein", "Hek\u00b7tor", "fing", "ihn", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Bei Gott, mein Freund, ich werde dich davon befrein!\u00ab", "tokens": ["Bei", "Gott", ",", "mein", "Freund", ",", "ich", "wer\u00b7de", "dich", "da\u00b7von", "be\u00b7fr\u00b7ein", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "$,", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "PRF", "PAV", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "\u00bbund wann?\u00ab \u2013 \u00bbUnd morgen \u2013 unverz\u00fcglich.\u00ab", "tokens": ["\u00bb", "und", "wann", "?", "\u00ab", "\u2013", "\u00bb", "Und", "mor\u00b7gen", "\u2013", "un\u00b7ver\u00b7z\u00fcg\u00b7lich", ".", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "punct", "punct", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "KON", "PWAV", "$.", "$(", "$(", "$(", "KON", "ADV", "$(", "ADJD", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Auf morgen kam man also \u00fcberein.", "tokens": ["Auf", "mor\u00b7gen", "kam", "man", "al\u00b7so", "\u00fc\u00b7be\u00b7re\u00b7in", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PIS", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.20": {"text": "Der Herr erscheint mit Hund und Leuten hinterdrein.", "tokens": ["Der", "Herr", "er\u00b7scheint", "mit", "Hund", "und", "Leu\u00b7ten", "hin\u00b7ter\u00b7drein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "\u00bbfr\u00fchst\u00fccken wir!\u00ab sagt er vergn\u00fcglich.", "tokens": ["\u00bb", "fr\u00fch\u00b7st\u00fc\u00b7cken", "wir", "!", "\u00ab", "sagt", "er", "ver\u00b7gn\u00fcg\u00b7lich", "."], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "$.", "$(", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.22": {"text": "\u00bbsind deine H\u00fchner zart? \u2013 Sieh da, dein T\u00f6chterlein!", "tokens": ["\u00bb", "sind", "dei\u00b7ne", "H\u00fch\u00b7ner", "zart", "?", "\u2013", "Sieh", "da", ",", "dein", "T\u00f6ch\u00b7ter\u00b7lein", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPOSAT", "NN", "ADJD", "$.", "$(", "NE", "PTKVZ", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Tritt n\u00e4her, Sch\u00e4tzchen! Nun, wann soll denn Hochzeit sein?", "tokens": ["Tritt", "n\u00e4\u00b7her", ",", "Sch\u00e4tz\u00b7chen", "!", "Nun", ",", "wann", "soll", "denn", "Hoch\u00b7zeit", "sein", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "NN", "$.", "ADV", "$,", "PWAV", "VMFIN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wann haben wir den Eidam hier?", "tokens": ["Wann", "ha\u00b7ben", "wir", "den", "Ei\u00b7dam", "hier", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Man s\u00e4ume nicht, da\u00df man die Sache vorbereite.\u00ab", "tokens": ["Man", "s\u00e4u\u00b7me", "nicht", ",", "da\u00df", "man", "die", "Sa\u00b7che", "vor\u00b7be\u00b7rei\u00b7te", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "$,", "KOUS", "PIS", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "So kn\u00fcpft er die Bekanntschaft an mit ihr,", "tokens": ["So", "kn\u00fcpft", "er", "die", "Be\u00b7kannt\u00b7schaft", "an", "mit", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Gew\u00e4hrt ihr Platz an seiner Seite,", "tokens": ["Ge\u00b7w\u00e4hrt", "ihr", "Platz", "an", "sei\u00b7ner", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Nimmt ihre Hand, den Arm und hebt gelind", "tokens": ["Nimmt", "ih\u00b7re", "Hand", ",", "den", "Arm", "und", "hebt", "ge\u00b7lind"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "ART", "NN", "KON", "VVFIN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Sich aus Respekt nur zag erwehrt,", "tokens": ["Sich", "aus", "Res\u00b7pekt", "nur", "zag", "er\u00b7wehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Was ihrem Vater kein Gefallen abgewinnt.", "tokens": ["Was", "ih\u00b7rem", "Va\u00b7ter", "kein", "Ge\u00b7fal\u00b7len", "ab\u00b7ge\u00b7winnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Inzwischen wird die K\u00fcche arg geleert.", "tokens": ["I\u00b7nzwi\u00b7schen", "wird", "die", "K\u00fc\u00b7che", "arg", "ge\u00b7leert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.32": {"text": "\u00bbsind deine Schinken frisch? Sie sehn vorz\u00fcglich aus!\u00ab", "tokens": ["\u00bb", "sind", "dei\u00b7ne", "Schin\u00b7ken", "frisch", "?", "Sie", "sehn", "vor\u00b7z\u00fcg\u00b7lich", "aus", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VAFIN", "PPOSAT", "NN", "ADJD", "$.", "PPER", "VVFIN", "ADJD", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "\u00bbherr, sie sind Euer.\u00ab \u2013 \u00bbOh, wir nehmen gern sie mit!\u00ab", "tokens": ["\u00bb", "herr", ",", "sie", "sind", "Eu\u00b7er", ".", "\u00ab", "\u2013", "\u00bb", "Oh", ",", "wir", "neh\u00b7men", "gern", "sie", "mit", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKVZ", "$,", "PPER", "VAFIN", "PPOSAT", "$.", "$(", "$(", "$(", "ITJ", "$,", "PPER", "VVFIN", "ADV", "PPER", "PTKVZ", "$.", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.34": {"text": "Der Herr h\u00e4lt Mahl mit seinem ganzen Haus,", "tokens": ["Der", "Herr", "h\u00e4lt", "Mahl", "mit", "sei\u00b7nem", "gan\u00b7zen", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Auch Hund und Pferde zeigen gro\u00dfen Appetit.", "tokens": ["Auch", "Hund", "und", "Pfer\u00b7de", "zei\u00b7gen", "gro\u00b7\u00dfen", "Ap\u00b7pe\u00b7tit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVINF", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Der Herr befiehlt herum und nimmt sich viel heraus,", "tokens": ["Der", "Herr", "be\u00b7fiehlt", "he\u00b7rum", "und", "nimmt", "sich", "viel", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "VVFIN", "PRF", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Trinkt unsers Landmanns Wein", "tokens": ["Trinkt", "un\u00b7sers", "Land\u00b7manns", "Wein"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.38": {"text": "Und kost sein T\u00f6chterlein.", "tokens": ["Und", "kost", "sein", "T\u00f6ch\u00b7ter\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.39": {"text": "Dem Fr\u00fchst\u00fcck folgt der Jagdtumult.", "tokens": ["Dem", "Fr\u00fch\u00b7st\u00fcck", "folgt", "der", "Jagd\u00b7tu\u00b7mult", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Man r\u00fcstet, l\u00e4rmt voll Ungeduld,", "tokens": ["Man", "r\u00fcs\u00b7tet", ",", "l\u00e4rmt", "voll", "Un\u00b7ge\u00b7duld", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Die H\u00f6rner und Trompeten toben drein,", "tokens": ["Die", "H\u00f6r\u00b7ner", "und", "Trom\u00b7pe\u00b7ten", "to\u00b7ben", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+++-+-+", "measure": "zehnsilber"}, "line.42": {"text": "Es scheint die H\u00f6lle los zu sein!", "tokens": ["Es", "scheint", "die", "H\u00f6l\u00b7le", "los", "zu", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Wie hat man da den armen Garten zugerichtet!", "tokens": ["Wie", "hat", "man", "da", "den", "ar\u00b7men", "Gar\u00b7ten", "zu\u00b7ge\u00b7rich\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PIS", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Ihr sch\u00f6nen Beete all, ade!", "tokens": ["Ihr", "sch\u00f6\u00b7nen", "Bee\u00b7te", "all", ",", "a\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PIAT", "$,", "ADV", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.45": {"text": "Ade, Zichorie und Porree!", 