{"dta.poem.13008": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Seladon an Leandern, als ihn die-  \n ser gefragt, was ihm in der Scudery ih-  \n rer Clelia mi\u00dffallen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Freund! Hesiodus ists, so mir nicht gef\u00e4llt,", "tokens": ["Mein", "Freund", "!", "He\u00b7sio\u00b7dus", "ists", ",", "so", "mir", "nicht", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "NE", "VAFIN", "$,", "ADV", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Weil er kein einzig wort von deutschen dichtern meldt;", "tokens": ["Weil", "er", "kein", "ein\u00b7zig", "wort", "von", "deut\u00b7schen", "dich\u00b7tern", "meldt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "ADJD", "NN", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Insonderheit weil er von dir nichts hat geschrieben,", "tokens": ["In\u00b7son\u00b7der\u00b7heit", "weil", "er", "von", "dir", "nichts", "hat", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "APPR", "PPER", "PIS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und weil Leander ist von ihm verschwiegen blieben.", "tokens": ["Und", "weil", "Le\u00b7an\u00b7der", "ist", "von", "ihm", "ver\u00b7schwie\u00b7gen", "blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NE", "VAFIN", "APPR", "PPER", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich aber will itzund weit besser prophezeyn:", "tokens": ["Ich", "a\u00b7ber", "will", "it\u00b7zund", "weit", "bes\u00b7ser", "pro\u00b7phe\u00b7zeyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "ADV", "ADJD", "ADJD", "VVINF", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Leander wird noch einst ein gro\u00dfer dichter seyn.", "tokens": ["Le\u00b7an\u00b7der", "wird", "noch", "einst", "ein", "gro\u00b7\u00dfer", "dich\u00b7ter", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ADV", "ART", "ADJA", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}