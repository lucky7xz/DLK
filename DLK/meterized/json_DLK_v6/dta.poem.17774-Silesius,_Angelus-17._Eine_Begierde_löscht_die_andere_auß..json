{"dta.poem.17774": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "17.  Eine Begierde l\u00f6scht die andere au\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Je mehr ein Mensch sich freut auf zeitlich Ehr und Gutt/", "tokens": ["Je", "mehr", "ein", "Mensch", "sich", "freut", "auf", "zeit\u00b7lich", "Ehr", "und", "Gutt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PRF", "VVFIN", "APPR", "ADJD", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Je weniger hat er zu ewgen dingen mutt.", "tokens": ["Je", "we\u00b7ni\u00b7ger", "hat", "er", "zu", "ew\u00b7gen", "din\u00b7gen", "mutt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Jemehr hingegen er wartt auf die ewge dinge/", "tokens": ["Je\u00b7mehr", "hin\u00b7ge\u00b7gen", "er", "wartt", "auf", "die", "ew\u00b7ge", "din\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VAFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Jemehr und mehr wird ihm das Zlitliche geringe.", "tokens": ["Je\u00b7mehr", "und", "mehr", "wird", "ihm", "das", "Zlit\u00b7li\u00b7che", "ge\u00b7rin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KO