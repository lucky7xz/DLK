{"dta.poem.19244": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "91.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du hast ein Saitenspiel, ganz rein in allen Saiten", "tokens": ["Du", "hast", "ein", "Sai\u00b7ten\u00b7spiel", ",", "ganz", "rein", "in", "al\u00b7len", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ADV", "ADJD", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gestimmt, die Melodie des Herzens zu begleiten.", "tokens": ["Ge\u00b7stimmt", ",", "die", "Me\u00b7lo\u00b7die", "des", "Her\u00b7zens", "zu", "be\u00b7glei\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nur eine Sait' ist dran, die, wenn du scharf sie r\u00fchrst,", "tokens": ["Nur", "ei\u00b7ne", "Sait'", "ist", "dran", ",", "die", ",", "wenn", "du", "scharf", "sie", "r\u00fchrst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "PAV", "$,", "PRELS", "$,", "KOUS", "PPER", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gibt einen Mi\u00dfton an, den du im Herzen sp\u00fcrst.", "tokens": ["Gibt", "ei\u00b7nen", "Mi\u00df\u00b7ton", "an", ",", "den", "du", "im", "Her\u00b7zen", "sp\u00fcrst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was willst du thun? du mu\u00dft, wenn du die schwachen Saiten", "tokens": ["Was", "willst", "du", "thun", "?", "du", "mu\u00dft", ",", "wenn", "du", "die", "schwa\u00b7chen", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "$.", "PPER", "VMFIN", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht ganz vermeiden kanst, dar\u00fcber leis' hingleiten.", "tokens": ["Nicht", "ganz", "ver\u00b7mei\u00b7den", "kanst", ",", "da\u00b7r\u00fc\u00b7ber", "leis'", "hing\u00b7lei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVINF", "VMFIN", "$,", "PAV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Du hast ein liebes Herz, auch rein dir gleichgestimmt,", "tokens": ["Du", "hast", "ein", "lie\u00b7bes", "Herz", ",", "auch", "rein", "dir", "gleich\u00b7ges\u00b7timmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "ADV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In dessen Widerklang sich deines ganz vernimmt.", "tokens": ["In", "des\u00b7sen", "Wi\u00b7der\u00b7klang", "sich", "dei\u00b7nes", "ganz", "ver\u00b7nimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PRF", "PPOSAT", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Nur eine Sait' ist dran, die, wenn du scharf sie r\u00fchrst,", "tokens": ["Nur", "ei\u00b7ne", "Sait'", "ist", "dran", ",", "die", ",", "wenn", "du", "scharf", "sie", "r\u00fchrst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "PAV", "$,", "PRELS", "$,", "KOUS", "PPER", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gibt einen Mi\u00dfton an, den du im Herzen sp\u00fcrst.", "tokens": ["Gibt", "ei\u00b7nen", "Mi\u00df\u00b7ton", "an", ",", "den", "du", "im", "Her\u00b7zen", "sp\u00fcrst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Willst du dem Herzen wie dem Saitenspiel nicht thun?", "tokens": ["Willst", "du", "dem", "Her\u00b7zen", "wie", "dem", "Sai\u00b7ten\u00b7spiel", "nicht", "thun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "KOKOM", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00df die verstimmende verstimmte Saite ruhn.", "tokens": ["La\u00df", "die", "ver\u00b7stim\u00b7men\u00b7de", "ver\u00b7stimm\u00b7te", "Sai\u00b7te", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "ADJA", "NN", "VVINF", "$."], "meter": "+--+---+-+-+", "measure": "dactylic.di.plus"}}}}}