{"textgrid.poem.37606": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der alte Grenadier", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier auf Leichen mu\u00df ich sitzen", "tokens": ["Hier", "auf", "Lei\u00b7chen", "mu\u00df", "ich", "sit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem w\u00fcsten Auerst\u00e4dt,", "tokens": ["In", "dem", "w\u00fcs\u00b7ten", "Au\u00b7er\u00b7st\u00e4dt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seh die Feinde n\u00e4her blitzen,", "tokens": ["Seh", "die", "Fein\u00b7de", "n\u00e4\u00b7her", "blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ihr Schwert die Preu\u00dfen m\u00e4ht.", "tokens": ["Wie", "ihr", "Schwert", "die", "Preu\u00b7\u00dfen", "m\u00e4ht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Beide F\u00fc\u00dfe sind zerschossen,", "tokens": ["Bei\u00b7de", "F\u00fc\u00b7\u00dfe", "sind", "zer\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sterbend mich der Sohn forttrug,", "tokens": ["Ster\u00b7bend", "mich", "der", "Sohn", "fort\u00b7trug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo mit Friederich dem Gro\u00dfen", "tokens": ["Wo", "mit", "Frie\u00b7de\u00b7rich", "dem", "Gro\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NE", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ich die wilden Feinde schlug.", "tokens": ["Ich", "die", "wil\u00b7den", "Fein\u00b7de", "schlug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Lieber Sohn, auf deinem R\u00fccken", "tokens": ["Lie\u00b7ber", "Sohn", ",", "auf", "dei\u00b7nem", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "W\u00fcrfle ich um das Geschick,", "tokens": ["W\u00fcrf\u00b7le", "ich", "um", "das", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Denn ich kann mich nun nicht b\u00fccken,", "tokens": ["Denn", "ich", "kann", "mich", "nun", "nicht", "b\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Denn ich sah des Alten Blick.", "tokens": ["Denn", "ich", "sah", "des", "Al\u00b7ten", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Hier sollt ich gefangen werden.", "tokens": ["Hier", "sollt", "ich", "ge\u00b7fan\u00b7gen", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVPP", "VAINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Fernhin sah ich Ro\u00dfbachs Feld,", "tokens": ["Fern\u00b7hin", "sah", "ich", "Ro\u00df\u00b7bachs", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Wo sie flohn wie L\u00e4mmerheerden,", "tokens": ["Wo", "sie", "flohn", "wie", "L\u00e4m\u00b7mer\u00b7heer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Wo ein jeder Preu\u00dfe Held.", "tokens": ["Wo", "ein", "je\u00b7der", "Preu\u00b7\u00dfe", "Held", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "PIAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Hieher mit den Grenadieren", "tokens": ["Hie\u00b7her", "mit", "den", "Gre\u00b7na\u00b7die\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.18": {"text": "Zog ich ganz aus freiem St\u00fcck;", "tokens": ["Zog", "ich", "ganz", "aus", "frei\u00b7em", "St\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Keiner wei\u00df uns anzuf\u00fchren,", "tokens": ["Kei\u00b7ner", "wei\u00df", "uns", "an\u00b7zu\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Es ist aus mit unserm Gl\u00fcck.", "tokens": ["Es", "ist", "aus", "mit", "un\u00b7serm", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "W\u00e4r ich nun daheim geblieben,", "tokens": ["W\u00e4r", "ich", "nun", "da\u00b7heim", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Glaubte ich es keinem Freund;", "tokens": ["Glaub\u00b7te", "ich", "es", "kei\u00b7nem", "Freund", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "W\u00e4re ich nur todt geblieben,", "tokens": ["W\u00e4\u00b7re", "ich", "nur", "todt", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Ach schon trommelt nah der Feind.", "tokens": ["Ach", "schon", "trom\u00b7melt", "nah", "der", "Feind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Wie die G\u00e4nse thun sie schreien,", "tokens": ["Wie", "die", "G\u00e4n\u00b7se", "thun", "sie", "schrei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Nein, die Flinte kriegt ihr nicht,", "tokens": ["Nein", ",", "die", "Flin\u00b7te", "kriegt", "ihr", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Alter Ruhm th\u00e4t mir sie weihen,", "tokens": ["Al\u00b7ter", "Ruhm", "th\u00e4t", "mir", "sie", "wei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Die mein Arm in St\u00fccken bricht.", "tokens": ["Die", "mein", "Arm", "in", "St\u00fc\u00b7cken", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Auf dem Heerd das Feuer brennet,", "tokens": ["Auf", "dem", "Heerd", "das", "Feu\u00b7er", "bren\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Hat ein Geist es angemacht?", "tokens": ["Hat", "ein", "Geist", "es", "an\u00b7ge\u00b7macht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Wie es in dem Dorfe rennet,", "tokens": ["Wie", "es", "in", "dem", "Dor\u00b7fe", "ren\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Freiheit weht in Flammenmacht.", "tokens": ["Frei\u00b7heit", "weht", "in", "Flam\u00b7men\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "\u00bbdarum s\u00e4e ich die Kohlen", "tokens": ["\u00bb", "da\u00b7rum", "s\u00e4e", "ich", "die", "Koh\u00b7len"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PAV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Hier im dichten Strohe aus,", "tokens": ["Hier", "im", "dich\u00b7ten", "Stro\u00b7he", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Da\u00df uns nicht die Feinde holen,", "tokens": ["Da\u00df", "uns", "nicht", "die", "Fein\u00b7de", "ho\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Knabe flieh, ich z\u00fcnd das Haus.\u00ab", "tokens": ["Kna\u00b7be", "flieh", ",", "ich", "z\u00fcnd", "das", "Haus", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Kommt der Knabe nicht gelaufen,", "tokens": ["Kommt", "der", "Kna\u00b7be", "nicht", "ge\u00b7lau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Will nicht um sich sehn, er flieht,", "tokens": ["Will", "nicht", "um", "sich", "sehn", ",", "er", "flieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "APPR", "PRF", "VVINF", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Die Blessirten sind in Haufen", "tokens": ["Die", "Bles\u00b7sir\u00b7ten", "sind", "in", "Hau\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.40": {"text": "Und sie l\u00e4cheln wie es gl\u00fcht,", "tokens": ["Und", "sie", "l\u00e4\u00b7cheln", "wie", "es", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KOKOM", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.41": {"text": "Als die Flammen aufw\u00e4rts steigen,", "tokens": ["Als", "die", "Flam\u00b7men", "auf\u00b7w\u00e4rts", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Tausend Seelen ziehn empor,", "tokens": ["Tau\u00b7send", "See\u00b7len", "ziehn", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "Und die Feinde m\u00fcssen weichen", "tokens": ["Und", "die", "Fein\u00b7de", "m\u00fcs\u00b7sen", "wei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Vor dem strahlenreichen Chor", "tokens": ["Vor", "dem", "strah\u00b7len\u00b7rei\u00b7chen", "Chor"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.45": {"text": "Und aus ihren Marterqualen", "tokens": ["Und", "aus", "ih\u00b7ren", "Mar\u00b7ter\u00b7qua\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Brennen die Blessirten auf,", "tokens": ["Bren\u00b7nen", "die", "Bles\u00b7sir\u00b7ten", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.47": {"text": "Ach wer kann den Alten mahlen,", "tokens": ["Ach", "wer", "kann", "den", "Al\u00b7ten", "mah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Wie er reisig steht zum Lauf,", "tokens": ["Wie", "er", "rei\u00b7sig", "steht", "zum", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.49": {"text": "Und in m\u00e4chtigen Gedanken", "tokens": ["Und", "in", "m\u00e4ch\u00b7ti\u00b7gen", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Springet ihm der Scheitel durch,", "tokens": ["Sprin\u00b7get", "ihm", "der", "Schei\u00b7tel", "durch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.51": {"text": "Ewger Muth bricht alle Schranken,", "tokens": ["Ew\u00b7ger", "Muth", "bricht", "al\u00b7le", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Ringet in die Himmelsburg.", "tokens": ["Rin\u00b7get", "in", "die", "Him\u00b7mels\u00b7burg", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}