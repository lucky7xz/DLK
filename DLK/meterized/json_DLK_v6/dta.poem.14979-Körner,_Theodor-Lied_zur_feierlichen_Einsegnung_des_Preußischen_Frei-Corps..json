{"dta.poem.14979": {"metadata": {"author": {"name": "K\u00f6rner, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Lied zur feierlichen Einsegnung des Preu\u00dfischen  \n Frei-Corps.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1814", "urn": "urn:nbn:de:kobv:b4-200905193258", "language": ["de:0.99"], "booktitle": "K\u00f6rner, Theodor: Leyer und Schwerdt. Berlin, 1814."}, "poem": {"stanza.1": {"line.1": {"text": "Mit frommen Muth zusammen.", "tokens": ["Mit", "from\u00b7men", "Muth", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Uns ruft die Pflicht zum Kampf hinaus,", "tokens": ["Uns", "ruft", "die", "Pflicht", "zum", "Kampf", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "APZR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und alle Herzen flammen.", "tokens": ["Und", "al\u00b7le", "Her\u00b7zen", "flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch was uns mahnt zu Sieg und Schlacht,", "tokens": ["Doch", "was", "uns", "mahnt", "zu", "Sieg", "und", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hat Gott ja selber angefacht.", "tokens": ["Hat", "Gott", "ja", "sel\u00b7ber", "an\u00b7ge\u00b7facht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Herr ist unsre Zuversicht,", "tokens": ["Der", "Herr", "ist", "uns\u00b7re", "Zu\u00b7ver\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie schwer der Kampf auch werde!", "tokens": ["Wie", "schwer", "der", "Kampf", "auch", "wer\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir streiten ja f\u00fcr Recht und Pflicht,", "tokens": ["Wir", "strei\u00b7ten", "ja", "f\u00fcr", "Recht", "und", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und f\u00fcr die heil'ge Erde.", "tokens": ["Und", "f\u00fcr", "die", "heil'\u00b7ge", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Drum retten wir das Vaterland,", "tokens": ["Drum", "ret\u00b7ten", "wir", "das", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So that's der Herr durch unsre Hand.", "tokens": ["So", "that's", "der", "Herr", "durch", "uns\u00b7re", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es bricht der freche Uebermuth", "tokens": ["Es", "bricht", "der", "fre\u00b7che", "Ue\u00b7ber\u00b7muth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Tyranney zusammen;", "tokens": ["Der", "Ty\u00b7ran\u00b7ney", "zu\u00b7sam\u00b7men", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es soll der Freiheit heil'ge Gluth,", "tokens": ["Es", "soll", "der", "Frei\u00b7heit", "heil'\u00b7ge", "Gluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In allen Herzen flammen.", "tokens": ["In", "al\u00b7len", "Her\u00b7zen", "flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Drum frisch in Kampfes Ungest\u00fcm!", "tokens": ["Drum", "frisch", "in", "Kamp\u00b7fes", "Un\u00b7ge\u00b7st\u00fcm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gott ist mit uns und wir mit ihm.", "tokens": ["Gott", "ist", "mit", "uns", "und", "wir", "mit", "ihm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPER", "KON", "PPER", "APPR", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Er weckt uns jetzt mit Siegerlust", "tokens": ["Er", "weckt", "uns", "jetzt", "mit", "Sie\u00b7ger\u00b7lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr die gerechte Sache,", "tokens": ["F\u00fcr", "die", "ge\u00b7rech\u00b7te", "Sa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er rief es selbst in unsre Brust:", "tokens": ["Er", "rief", "es", "selbst", "in", "uns\u00b7re", "Brust", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und f\u00fchrt uns w\u00e4rs auch durch den Tod", "tokens": ["Und", "f\u00fchrt", "uns", "w\u00e4rs", "auch", "durch", "den", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "VAFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zu seiner Freiheit Morgenroth.", "tokens": ["Zu", "sei\u00b7ner", "Frei\u00b7heit", "Mor\u00b7gen\u00b7roth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}