{"textgrid.poem.24928": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Matt flackert die Lampe; der Kurf\u00fcrst ringt", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Matt flackert die Lampe; der Kurf\u00fcrst ringt", "tokens": ["Matt", "fla\u00b7ckert", "die", "Lam\u00b7pe", ";", "der", "Kur\u00b7f\u00fcrst", "ringt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "$.", "ART", "NN", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Am Bette des Sohnes die H\u00e4nde", "tokens": ["Am", "Bet\u00b7te", "des", "Soh\u00b7nes", "die", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und fleht, indes er den Liebling umschlingt,", "tokens": ["Und", "fleht", ",", "in\u00b7des", "er", "den", "Lieb\u00b7ling", "um\u00b7schlingt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da\u00df Rettung der Himmel sende.", "tokens": ["Da\u00df", "Ret\u00b7tung", "der", "Him\u00b7mel", "sen\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "O, mu\u00df ihm also zum erstenmal", "tokens": ["O", ",", "mu\u00df", "ihm", "al\u00b7so", "zum", "ers\u00b7ten\u00b7mal"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "VMFIN", "PPER", "ADV", "APPRART", "ADV"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Sein Friedrich Kummer bereiten?", "tokens": ["Sein", "Fried\u00b7rich", "Kum\u00b7mer", "be\u00b7rei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und immer noch will kein Hoffnungsstrahl", "tokens": ["Und", "im\u00b7mer", "noch", "will", "kein", "Hoff\u00b7nungs\u00b7strahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VMFIN", "PIAT", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Durchs n\u00e4chtige Dunkel gleiten!", "tokens": ["Durchs", "n\u00e4ch\u00b7ti\u00b7ge", "Dun\u00b7kel", "glei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Seitdem er am Lager des Kranken gewacht,", "tokens": ["Seit\u00b7dem", "er", "am", "La\u00b7ger", "des", "Kran\u00b7ken", "ge\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.10": {"text": "Ist das die dritte, schrecklichste Nacht.", "tokens": ["Ist", "das", "die", "drit\u00b7te", ",", "schreck\u00b7lichs\u00b7te", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Von Jubel hallen zur selben Zeit", "tokens": ["Von", "Ju\u00b7bel", "hal\u00b7len", "zur", "sel\u00b7ben", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Des Schlosses Erkergem\u00e4cher;", "tokens": ["Des", "Schlos\u00b7ses", "Er\u00b7ker\u00b7ge\u00b7m\u00e4\u00b7cher", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dort schwelgt, als h\u00f6hnt' er des Vaters Leid,", "tokens": ["Dort", "schwelgt", ",", "als", "h\u00f6hnt'", "er", "des", "Va\u00b7ters", "Leid", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Prinz Rupert im Kreise der Zecher.", "tokens": ["Prinz", "Ru\u00b7pert", "im", "Krei\u00b7se", "der", "Ze\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Lust leuchtet in aller G\u00e4ste Blick;", "tokens": ["Lust", "leuch\u00b7tet", "in", "al\u00b7ler", "G\u00e4s\u00b7te", "Blick", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Stromweise gie\u00dfen die Diener", "tokens": ["Strom\u00b7wei\u00b7se", "gie\u00b7\u00dfen", "die", "Die\u00b7ner"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "In die R\u00f6mer das duftende Kirchenst\u00fcck", "tokens": ["In", "die", "R\u00f6\u00b7mer", "das", "duf\u00b7ten\u00b7de", "Kir\u00b7chen\u00b7st\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Und von Forst den goldnen Traminer,", "tokens": ["Und", "von", "Forst", "den", "gold\u00b7nen", "Tra\u00b7mi\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Und fr\u00f6hlich erschallt bei Becherklang", "tokens": ["Und", "fr\u00f6h\u00b7lich", "er\u00b7schallt", "bei", "Be\u00b7cher\u00b7klang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "NE"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}}, "stanza.3": {"line.1": {"text": "\u00bbdu m\u00f6gst nun trauern,\u00ab \u2013 ruft einer aus \u2013", "tokens": ["\u00bb", "du", "m\u00f6gst", "nun", "trau\u00b7ern", ",", "\u00ab", "\u2013", "ruft", "ei\u00b7ner", "aus", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "ADV", "VVINF", "$,", "$(", "$(", "VVFIN", "ART", "APPR", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbdie droben im alten Turme", "tokens": ["\u00bb", "die", "dro\u00b7ben", "im", "al\u00b7ten", "Tur\u00b7me"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Du einsam lange, o Fledermaus,", "tokens": ["Du", "ein\u00b7sam", "lan\u00b7ge", ",", "o", "Fle\u00b7der\u00b7maus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "$,", "FM", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gehaust mit dem B\u00fccherwurme!", "tokens": ["Ge\u00b7haust", "mit", "dem", "B\u00fc\u00b7cher\u00b7wur\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Bald ruht er, so k\u00fcndet sein Horoskop,", "tokens": ["Bald", "ruht", "er", ",", "so", "k\u00fcn\u00b7det", "sein", "Ho\u00b7ros\u00b7kop", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Im Staube mit seinen Pandekten;", "tokens": ["Im", "Stau\u00b7be", "mit", "sei\u00b7nen", "Pan\u00b7dek\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Uns aber stehen, dem Himmel sei Lob,", "tokens": ["Uns", "a\u00b7ber", "ste\u00b7hen", ",", "dem", "Him\u00b7mel", "sei", "Lob", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "ART", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Die Zeichen in guten Aspekten;", "tokens": ["Die", "Zei\u00b7chen", "in", "gu\u00b7ten", "As\u00b7pek\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.9": {"text": "Statt seiner, der Relegation uns schwur,", "tokens": ["Statt", "sei\u00b7ner", ",", "der", "Re\u00b7le\u00b7ga\u00b7ti\u00b7on", "uns", "schwur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "$,", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Erbt unser G\u00f6nner, Prinz Rupert, die Kur.\u00ab", "tokens": ["Erbt", "un\u00b7ser", "G\u00f6n\u00b7ner", ",", "Prinz", "Ru\u00b7pert", ",", "die", "Kur", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "NN", "NE", "$,", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbja, Rupert,\u00ab erhebt ein andrer das Glas,", "tokens": ["\u00bb", "ja", ",", "Ru\u00b7pert", ",", "\u00ab", "er\u00b7hebt", "ein", "an\u00b7drer", "das", "Glas", ","], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "NE", "$,", "$(", "VVFIN", "ART", "ADJA", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbwei\u00df echte Verdienste zu ehren;", "tokens": ["\u00bb", "wei\u00df", "ech\u00b7te", "Ver\u00b7diens\u00b7te", "zu", "eh\u00b7ren", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "La\u00dft uns, ihr Freunde, das gro\u00dfe Fa\u00df", "tokens": ["La\u00dft", "uns", ",", "ihr", "Freun\u00b7de", ",", "das", "gro\u00b7\u00dfe", "Fa\u00df"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Aufs Wohl des Trefflichen leeren!\u00ab \u2013", "tokens": ["Aufs", "Wohl", "des", "Treff\u00b7li\u00b7chen", "lee\u00b7ren", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$.", "$(", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "\u00bbhoch Rupert, der Kronprinz!\u00ab scholl es dann;", "tokens": ["\u00bb", "hoch", "Ru\u00b7pert", ",", "der", "Kron\u00b7prinz", "!", "\u00ab", "scholl", "es", "dann", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "NE", "$,", "ART", "NN", "$.", "$(", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die blinkenden Becher klangen,", "tokens": ["Die", "blin\u00b7ken\u00b7den", "Be\u00b7cher", "klan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und der Prinz stie\u00df mit den Zechern an;", "tokens": ["Und", "der", "Prinz", "stie\u00df", "mit", "den", "Ze\u00b7chern", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Ihm gl\u00fchten vom Weine die Wangen.", "tokens": ["Ihm", "gl\u00fch\u00b7ten", "vom", "Wei\u00b7ne", "die", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.9": {"text": "Im Kreise schweifte sein Blick: \u00bbWarum", "tokens": ["Im", "Krei\u00b7se", "schweif\u00b7te", "sein", "Blick", ":", "\u00bb", "Wa\u00b7rum"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$.", "$(", "PWAV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Ist nur Graf Kuno so finster un