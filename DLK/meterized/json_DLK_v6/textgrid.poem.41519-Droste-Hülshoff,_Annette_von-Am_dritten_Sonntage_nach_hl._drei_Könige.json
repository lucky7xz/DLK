{"textgrid.poem.41519": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am dritten Sonntage nach hl. drei K\u00f6nige", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbgeh hin und dir gescheh' wie du geglaubt!\u00ab", "tokens": ["\u00bb", "geh", "hin", "und", "dir", "ge\u00b7scheh'", "wie", "du", "ge\u00b7glaubt", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "KON", "PPER", "ADJD", "KOKOM", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ja, wer da glaubt, dem wird sein Heil geschehen;", "tokens": ["Ja", ",", "wer", "da", "glaubt", ",", "dem", "wird", "sein", "Heil", "ge\u00b7sche\u00b7hen", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWS", "ADV", "VVFIN", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was aber ihm, dem in verborgnen Wehen", "tokens": ["Was", "a\u00b7ber", "ihm", ",", "dem", "in", "ver\u00b7borg\u00b7nen", "We\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "PPER", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Leben hat sein Heiliges geraubt?", "tokens": ["Das", "Le\u00b7ben", "hat", "sein", "Hei\u00b7li\u00b7ges", "ge\u00b7raubt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Herr, sprich ein Wort, so wird dein Knecht gesund!", "tokens": ["Herr", ",", "sprich", "ein", "Wort", ",", "so", "wird", "dein", "Knecht", "ge\u00b7sund", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ART", "NN", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Herr, sprich das Wort, ich kann ja nichts als wollen;", "tokens": ["Herr", ",", "sprich", "das", "Wort", ",", "ich", "kann", "ja", "nichts", "als", "wol\u00b7len", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ART", "NN", "$,", "PPER", "VMFIN", "ADV", "PIS", "KOKOM", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Liebe kann das Herz dir freudig zollen,", "tokens": ["Die", "Lie\u00b7be", "kann", "das", "Herz", "dir", "freu\u00b7dig", "zol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Glaube wird ja nur als Gnade kund!", "tokens": ["Der", "Glau\u00b7be", "wird", "ja", "nur", "als", "Gna\u00b7de", "kund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "KOUS", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie k\u00f6mmt es, da ich dich am Abend rief,", "tokens": ["Wie", "k\u00f6mmt", "es", ",", "da", "ich", "dich", "am", "A\u00b7bend", "rief", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da ich am Morgen ausging dich zu finden,", "tokens": ["Da", "ich", "am", "Mor\u00b7gen", "aus\u00b7ging", "dich", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df du in Lauheit und des Zweifels S\u00fcnden", "tokens": ["Da\u00df", "du", "in", "Lau\u00b7heit", "und", "des", "Zwei\u00b7fels", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mich sinken lie\u00dfest, tiefer stets und rief.", "tokens": ["Mich", "sin\u00b7ken", "lie\u00b7\u00dfest", ",", "tie\u00b7fer", "stets", "und", "rief", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VVFIN", "$,", "ADJD", "ADV", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ist nicht mein Ruf in meiner h\u00f6chsten Not", "tokens": ["Ist", "nicht", "mein", "Ruf", "in", "mei\u00b7ner", "h\u00f6chs\u00b7ten", "Not"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu dir empor geschollen aus der Tiefe?", "tokens": ["Zu", "dir", "em\u00b7por", "ge\u00b7schol\u00b7len", "aus", "der", "Tie\u00b7fe", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und war es nicht, als ob ich Felsen riefe,", "tokens": ["Und", "war", "es", "nicht", ",", "als", "ob", "ich", "Fel\u00b7sen", "rie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "$,", "KOKOM", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Indes mein Auge stets von Tr\u00e4nen rot?", "tokens": ["In\u00b7des", "mein", "Au\u00b7ge", "stets", "von", "Tr\u00e4\u00b7nen", "rot", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Verzeih, o Herr, was die Bedr\u00e4ngnis spricht,", "tokens": ["Ver\u00b7zeih", ",", "o", "Herr", ",", "was", "die", "Be\u00b7dr\u00e4ng\u00b7nis", "spricht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich habe dich doch oft und s\u00fc\u00df empfunden,", "tokens": ["Ich", "ha\u00b7be", "dich", "doch", "oft", "und", "s\u00fc\u00df", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich war ja eins mit dir zu ganzen Stunden,", "tokens": ["Ich", "war", "ja", "eins", "mit", "dir", "zu", "gan\u00b7zen", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "APPR", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und in der Not gedacht' ich dessen nicht!", "tokens": ["Und", "in", "der", "Not", "ge\u00b7dacht'", "ich", "des\u00b7sen", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "PDS", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und ist mir nun, als sei ich ganz allein", "tokens": ["Und", "ist", "mir", "nun", ",", "als", "sei", "ich", "ganz", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "$,", "KOKOM", "VAFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von deinem weiten Gnadenmahl verloren,", "tokens": ["Von", "dei\u00b7nem", "wei\u00b7ten", "Gna\u00b7den\u00b7mahl", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der ausgesperrte Bettler vor den Toren:", "tokens": ["Der", "aus\u00b7ge\u00b7sperr\u00b7te", "Bett\u00b7ler", "vor", "den", "To\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O Gott! die Schuld ist doch gewi\u00dflich mein!", "tokens": ["O", "Gott", "!", "die", "Schuld", "ist", "doch", "ge\u00b7wi\u00df\u00b7lich", "mein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ART", "NN", "VAFIN", "ADV", "ADV", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "F\u00fchlt' ich in Demut, wie ich nimmer wert,", "tokens": ["F\u00fchlt'", "ich", "in", "De\u00b7mut", ",", "wie", "ich", "nim\u00b7mer", "wert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,", "PWAV", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df ich dein Wort in meinem Geist empfangen,", "tokens": ["Da\u00df", "ich", "dein", "Wort", "in", "mei\u00b7nem", "Geist", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df meine Seufzer an dein Ohr gelangen,", "tokens": ["Da\u00df", "mei\u00b7ne", "Seuf\u00b7zer", "an", "dein", "Ohr", "ge\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df meine Seele dich erkennt und ehrt?", "tokens": ["Da\u00df", "mei\u00b7ne", "See\u00b7le", "dich", "er\u00b7kennt", "und", "ehrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Mein Herr, gedenke meiner S\u00fcnden nicht,", "tokens": ["Mein", "Herr", ",", "ge\u00b7den\u00b7ke", "mei\u00b7ner", "S\u00fcn\u00b7den", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie oft hab' ich auf selbstgew\u00e4hltem Pfade", "tokens": ["Wie", "oft", "hab'", "ich", "auf", "selbst\u00b7ge\u00b7w\u00e4hl\u00b7tem", "Pfa\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Geschrien im Dunkel, Gott, um deine Gnade,", "tokens": ["Ge\u00b7schri\u00b7en", "im", "Dun\u00b7kel", ",", "Gott", ",", "um", "dei\u00b7ne", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "NN", "$,", "KOUI", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wie um ein Recht, und wie um eine Pflicht!", "tokens": ["Wie", "um", "ein", "Recht", ",", "und", "wie", "um", "ei\u00b7ne", "Pflicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "$,", "KON", "PWAV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "O h\u00e4tt' ich ihre Gaben nicht vers\u00e4umt!", "tokens": ["O", "h\u00e4tt'", "ich", "ih\u00b7re", "Ga\u00b7ben", "nicht", "ver\u00b7s\u00e4umt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "H\u00e4tt' ich sie nicht zertreten, und verachtet!", "tokens": ["H\u00e4tt'", "ich", "sie", "nicht", "zer\u00b7tre\u00b7ten", ",", "und", "ver\u00b7ach\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich st\u00e4nde nicht so grauenvoll umnachtet,", "tokens": ["Ich", "st\u00e4n\u00b7de", "nicht", "so", "grau\u00b7en\u00b7voll", "um\u00b7nach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df das entflohne Licht mir wie getr\u00e4umt!", "tokens": ["Da\u00df", "das", "ent\u00b7floh\u00b7ne", "Licht", "mir", "wie", "ge\u00b7tr\u00e4umt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "KOKOM", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Wie oft ist nicht, noch eh die Tat geschah,", "tokens": ["Wie", "oft", "ist", "nicht", ",", "noch", "eh", "die", "Tat", "ge\u00b7schah", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PTKNEG", "$,", "ADV", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die als Gedanke l\u00fcstern mich umflogen,", "tokens": ["Die", "als", "Ge\u00b7dan\u00b7ke", "l\u00fcs\u00b7tern", "mich", "um\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In milder Warnung still vorbeigezogen", "tokens": ["In", "mil\u00b7der", "War\u00b7nung", "still", "vor\u00b7bei\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein Name mir, dein Bild auf Golgatha!", "tokens": ["Dein", "Na\u00b7me", "mir", ",", "dein", "Bild", "auf", "Gol\u00b7ga\u00b7tha", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "$,", "PPOSAT", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Und wenn ich nun mich frevelnd abgewandt,", "tokens": ["Und", "wenn", "ich", "nun", "mich", "fre\u00b7velnd", "ab\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die S\u00fcnde die ich klar erkannt begangen,", "tokens": ["Die", "S\u00fcn\u00b7de", "die", "ich", "klar", "er\u00b7kannt", "be\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "ADJD", "VVPP", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie hast du dann in reuigem Verlangen", "tokens": ["Wie", "hast", "du", "dann", "in", "reu\u00b7i\u00b7gem", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht oft in meiner Seele nachgebrannt!", "tokens": ["Nicht", "oft", "in", "mei\u00b7ner", "See\u00b7le", "nach\u00b7ge\u00b7brannt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Ach, viel und schwere S\u00fcnden \u00fcbt' ich schon,", "tokens": ["Ach", ",", "viel", "und", "schwe\u00b7re", "S\u00fcn\u00b7den", "\u00fcbt'", "ich", "schon", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "KON", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Noch mehr der Fehle, klein in ihren Namen,", "tokens": ["Noch", "mehr", "der", "Feh\u00b7le", ",", "klein", "in", "ih\u00b7ren", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch gro\u00df in der Verderbnis tiefstem Samen,", "tokens": ["Doch", "gro\u00df", "in", "der", "Ver\u00b7derb\u00b7nis", "tiefs\u00b7tem", "Sa\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Taub f\u00fcr des jammernden Gewissens Ton.", "tokens": ["Taub", "f\u00fcr", "des", "jam\u00b7mern\u00b7den", "Ge\u00b7wis\u00b7sens", "Ton", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Nun ist mir endlich alles Licht dahin!", "tokens": ["Nun", "ist", "mir", "end\u00b7lich", "al\u00b7les", "Licht", "da\u00b7hin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PIAT", "NN", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und \u00f6fters deine Stimme ganz verschollen,", "tokens": ["Und", "\u00f6f\u00b7ters", "dei\u00b7ne", "Stim\u00b7me", "ganz", "ver\u00b7schol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch wirf mich, o du siehst ich kann noch wollen,", "tokens": ["Doch", "wirf", "mich", ",", "o", "du", "siehst", "ich", "kann", "noch", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "$,", "FM", "PPER", "VVFIN", "PPER", "VMFIN", "ADV", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht zu den Toten weil ich lebend bin!", "tokens": ["Nicht", "zu", "den", "To\u00b7ten", "weil", "ich", "le\u00b7bend", "bin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Mein Jesu, sieh, ich bin zu Tode wund,", "tokens": ["Mein", "Je\u00b7su", ",", "sieh", ",", "ich", "bin", "zu", "To\u00b7de", "wund", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "$,", "VVFIN", "$,", "PPER", "VAFIN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und kann in der Zerr\u00fcttung nicht gesunden,", "tokens": ["Und", "kann", "in", "der", "Zer\u00b7r\u00fct\u00b7tung", "nicht", "ge\u00b7sun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein Jesu, denk an deine bittern Wunden,", "tokens": ["Mein", "Je\u00b7su", ",", "denk", "an", "dei\u00b7ne", "bit\u00b7tern", "Wun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "$,", "PRELS", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und sprich ein Wort, so wird dein Knecht gesund!", "tokens": ["Und", "sprich", "ein", "Wort", ",", "so", "wird", "dein", "Knecht", "ge\u00b7sund", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "