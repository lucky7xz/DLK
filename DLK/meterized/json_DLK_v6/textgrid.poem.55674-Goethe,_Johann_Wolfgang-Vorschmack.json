{"textgrid.poem.55674": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Vorschmack", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der echte Moslem spricht vom Paradiese,", "tokens": ["Der", "ech\u00b7te", "Mos\u00b7lem", "spricht", "vom", "Pa\u00b7ra\u00b7die\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als wenn er selbst allda gewesen w\u00e4re,", "tokens": ["Als", "wenn", "er", "selbst", "all\u00b7da", "ge\u00b7we\u00b7sen", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "ADV", "VAPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er glaubt dem Koran, wie es der verhie\u00dfe,", "tokens": ["Er", "glaubt", "dem", "Ko\u00b7ran", ",", "wie", "es", "der", "ver\u00b7hie\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PWAV", "PPER", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hierauf begr\u00fcndet sich die reine Lehre.", "tokens": ["Hier\u00b7auf", "be\u00b7gr\u00fcn\u00b7det", "sich", "die", "rei\u00b7ne", "Leh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch der Prophet, Verfasser jenes Buches,", "tokens": ["Doch", "der", "Pro\u00b7phet", ",", "Ver\u00b7fas\u00b7ser", "je\u00b7nes", "Bu\u00b7ches", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wei\u00df unsre M\u00e4ngel droben auszuwittern", "tokens": ["Wei\u00df", "uns\u00b7re", "M\u00e4n\u00b7gel", "dro\u00b7ben", "aus\u00b7zu\u00b7wit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sieht, da\u00df trotz dem Donner seines Fluches", "tokens": ["Und", "sieht", ",", "da\u00df", "trotz", "dem", "Don\u00b7ner", "sei\u00b7nes", "Flu\u00b7ches"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Zweifel oft den Glauben uns verbittern.", "tokens": ["Die", "Zwei\u00b7fel", "oft", "den", "Glau\u00b7ben", "uns", "ver\u00b7bit\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Deshalb entsendet er den ew'gen R\u00e4umen", "tokens": ["Des\u00b7halb", "ent\u00b7sen\u00b7det", "er", "den", "ew'\u00b7gen", "R\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Jugendmuster, alles zu verj\u00fcngen;", "tokens": ["Ein", "Ju\u00b7gend\u00b7mus\u00b7ter", ",", "al\u00b7les", "zu", "ver\u00b7j\u00fcn\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie schwebt heran und fesselt, ohne S\u00e4umen,", "tokens": ["Sie", "schwebt", "he\u00b7ran", "und", "fes\u00b7selt", ",", "oh\u00b7ne", "S\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "ADJD", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um meinen Hals die allerliebsten Schlingen.", "tokens": ["Um", "mei\u00b7nen", "Hals", "die", "al\u00b7ler\u00b7liebs\u00b7ten", "Schlin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Auf meinem Scho\u00df, an meinem Herzen halt ich", "tokens": ["Auf", "mei\u00b7nem", "Scho\u00df", ",", "an", "mei\u00b7nem", "Her\u00b7zen", "halt", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Himmelswesen, mag nichts weiter wissen;", "tokens": ["Das", "Him\u00b7mels\u00b7we\u00b7sen", ",", "mag", "nichts", "wei\u00b7ter", "wis\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VMFIN", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und glaube nun ans Paradies gewaltig,", "tokens": ["Und", "glau\u00b7be", "nun", "ans", "Pa\u00b7ra\u00b7dies", "ge\u00b7wal\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn ewig m\u00f6cht ich sie so treulich k\u00fcssen.", "tokens": ["Denn", "e\u00b7wig", "m\u00f6cht", "ich", "sie", "so", "treu\u00b7lich", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VMFIN", "PPER", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}