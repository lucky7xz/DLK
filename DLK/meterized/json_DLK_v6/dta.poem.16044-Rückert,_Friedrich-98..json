{"dta.poem.16044": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "98.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das zu entwickeln, was Gott in den Keim gelegt,", "tokens": ["Das", "zu", "ent\u00b7wi\u00b7ckeln", ",", "was", "Gott", "in", "den", "Keim", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKZU", "VVINF", "$,", "PRELS", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ist des Erziehers Amt; wohl, wenn ers recht erw\u00e4gt!", "tokens": ["Ist", "des", "Er\u00b7zie\u00b7hers", "Amt", ";", "wohl", ",", "wenn", "ers", "recht", "er\u00b7w\u00e4gt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$.", "ADV", "$,", "KOUS", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Du kanst mit deinem Geist auf einen Geist einflie\u00dfen,", "tokens": ["Du", "kanst", "mit", "dei\u00b7nem", "Geist", "auf", "ei\u00b7nen", "Geist", "ein\u00b7flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um, wie den Pflanzenkeim die Sonn', ihn aufzuschlie\u00dfen.", "tokens": ["Um", ",", "wie", "den", "Pflan\u00b7zen\u00b7keim", "die", "Sonn'", ",", "ihn", "auf\u00b7zu\u00b7schlie\u00b7\u00dfen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "$,", "PWAV", "ART", "NN", "ART", "NN", "$,", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das Licht entwickelt zwar nur was im Keime lag,", "tokens": ["Das", "Licht", "ent\u00b7wi\u00b7ckelt", "zwar", "nur", "was", "im", "Kei\u00b7me", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch ohne Licht w\u00e4rs nicht gekommen an den Tag.", "tokens": ["Doch", "oh\u00b7ne", "Licht", "w\u00e4rs", "nicht", "ge\u00b7kom\u00b7men", "an", "den", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAFIN", "PTKNEG", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So kanst du auch ins Herz, was drin nicht liegt, nicht legen,", "tokens": ["So", "kanst", "du", "auch", "ins", "Herz", ",", "was", "drin", "nicht", "liegt", ",", "nicht", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPRART", "NN", "$,", "PRELS", "ADV", "PTKNEG", "VVFIN", "$,", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch jenachdem du es anregest, wird sichs regen.", "tokens": ["Doch", "je\u00b7nach\u00b7dem", "du", "es", "an\u00b7re\u00b7gest", ",", "wird", "sichs", "re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVFIN", "$,", "VAFIN", "CARD", "ADJA", "$."], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Nur ist ein wirklicher, der unentwickelt blieb,", "tokens": ["Nur", "ist", "ein", "wirk\u00b7li\u00b7cher", ",", "der", "un\u00b7ent\u00b7wi\u00b7ckelt", "blieb", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bei weitem vorzuziehn falschangeregtem Trieb.", "tokens": ["Bei", "wei\u00b7tem", "vor\u00b7zu\u00b7ziehn", "fal\u00b7schan\u00b7ge\u00b7reg\u00b7tem", "Trieb", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVIZU", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Denn Unentwickeltes kann sp\u00e4ter sich entfalten,", "tokens": ["Denn", "Un\u00b7ent\u00b7wi\u00b7ckel\u00b7tes", "kann", "sp\u00e4\u00b7ter", "sich", "ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "ADJD", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch Falschentwickeltes steht fest in Misgestalten.", "tokens": ["Doch", "Fal\u00b7schent\u00b7wi\u00b7ckel\u00b7tes", "steht", "fest", "in", "Mis\u00b7ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}