{"textgrid.poem.41299": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An einen Verfasser weitl\u00e4uftiger Grabschriften", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Gr\u00e4ber Ueberschrift ist sehr dein Werk gewesen;", "tokens": ["Der", "Gr\u00e4\u00b7ber", "Ue\u00b7ber\u00b7schrift", "ist", "sehr", "dein", "Werk", "ge\u00b7we\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch jedes Mal zu lang, und dies ist nicht erlaubt:", "tokens": ["Doch", "je\u00b7des", "Mal", "zu", "lang", ",", "und", "dies", "ist", "nicht", "er\u00b7laubt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PTKA", "ADJD", "$,", "KON", "PDS", "VAFIN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die eine H\u00e4lfte, Freund, wird nimmermehr geglaubt,", "tokens": ["Die", "ei\u00b7ne", "H\u00e4lf\u00b7te", ",", "Freund", ",", "wird", "nim\u00b7mer\u00b7mehr", "ge\u00b7glaubt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "$,", "NN", "$,", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die andre nimmermehr gelesen.", "tokens": ["Die", "and\u00b7re", "nim\u00b7mer\u00b7mehr", "ge\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}