{"textgrid.poem.53394": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Gem\u00fcht sey froh", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Gem\u00fcht sey froh", "tokens": ["Mein", "Ge\u00b7m\u00fcht", "sey", "froh"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Vnd vergi\u00df der Schmertzen,", "tokens": ["Vnd", "ver\u00b7gi\u00df", "der", "Schmert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df die Lebens-Loh", "tokens": ["Da\u00df", "die", "Le\u00b7bens\u00b7Loh"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Dir verlischt im Hertzen,", "tokens": ["Dir", "ver\u00b7lischt", "im", "Hert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Und dein Augen-Liecht", "tokens": ["Und", "dein", "Au\u00b7gen\u00b7Liecht"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Nun f\u00fcr Schwachheit bricht.", "tokens": ["Nun", "f\u00fcr", "Schwach\u00b7heit", "bricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Dieser Erden Stand", "tokens": ["Die\u00b7ser", "Er\u00b7den", "Stand"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Wird nicht lang mehr wehren,", "tokens": ["Wird", "nicht", "lang", "mehr", "weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADJD", "ADV", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Denn der letzte Brand", "tokens": ["Denn", "der", "letz\u00b7te", "Brand"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Alles wird verzehren,", "tokens": ["Al\u00b7les", "wird", "ver\u00b7zeh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Selbst der Sonnen Pracht", "tokens": ["Selbst", "der", "Son\u00b7nen", "Pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Wird seyn finstre Nacht.", "tokens": ["Wird", "seyn", "finst\u00b7re", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Kr\u00e4ncket sich dein Muth,", "tokens": ["Kr\u00e4n\u00b7cket", "sich", "dein", "Muth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Da\u00df du dich beflecket,", "tokens": ["Da\u00df", "du", "dich", "be\u00b7fle\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Vnd de\u00df H\u00f6chsten Glut", "tokens": ["Vnd", "de\u00df", "H\u00f6chs\u00b7ten", "Glut"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Wider dich erwecket,", "tokens": ["Wi\u00b7der", "dich", "er\u00b7we\u00b7cket", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Die mit Ach und Pein", "tokens": ["Die", "mit", "Ach", "und", "Pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "KON", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Brennet H\u00f6llen-ein;", "tokens": ["Bren\u00b7net", "H\u00f6l\u00b7len\u00b7ein", ";"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Zwar dich hat ohn Ruh", "tokens": ["Zwar", "dich", "hat", "ohn", "Ruh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "APPR", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "S\u00fcnde mit genommen,", "tokens": ["S\u00fcn\u00b7de", "mit", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ihrer Brunst bist du", "tokens": ["Ih\u00b7rer", "Brunst", "bist", "du"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Offt zu nah gekommen,", "tokens": ["Offt", "zu", "nah", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKA", "ADJD", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Welcher Wunden blind", "tokens": ["Wel\u00b7cher", "Wun\u00b7den", "blind"], "token_info": ["word", "word", "word"], "pos": ["PWAT", "NN", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Vnd unheilbar sind.", "tokens": ["Vnd", "un\u00b7heil\u00b7bar", "sind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Aber tr\u00f6ste dich,", "tokens": ["A\u00b7ber", "tr\u00f6s\u00b7te", "dich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Dir ist Raht geworden,", "tokens": ["Dir", "ist", "Raht", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "VAPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Christus l\u00e4sset sich", "tokens": ["Chris\u00b7tus", "l\u00e4s\u00b7set", "sich"], "token_info": ["word", "word", "word"], "pos": ["NE", "VVFIN", "PRF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wegen dein ermorden,", "tokens": ["We\u00b7gen", "dein", "er\u00b7mor\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Vnd sein theures Blut", "tokens": ["Vnd", "sein", "theu\u00b7res", "Blut"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Lescht der H\u00f6llen Glut.", "tokens": ["Lescht", "der", "H\u00f6l\u00b7len", "Glut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Das wir w\u00fcrden frey", "tokens": ["Das", "wir", "w\u00fcr\u00b7den", "frey"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "VAFIN", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Unsrer Missethaten,", "tokens": ["Uns\u00b7rer", "Mis\u00b7se\u00b7tha\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "L\u00e4sst er sich aus Treu", "tokens": ["L\u00e4sst", "er", "sich", "aus", "Treu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Mehr als grausam braten,", "tokens": ["Mehr", "als", "grau\u00b7sam", "bra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ein unschuldig Lamm", "tokens": ["Ein", "un\u00b7schul\u00b7dig", "Lamm"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Hoch am Creutzes-Stamm.", "tokens": ["Hoch", "am", "Creut\u00b7zes\u00b7Stamm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Hiedurch nimmt uns Gott", "tokens": ["Hie\u00b7durch", "nimmt", "uns", "Gott"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Wieder auff zu Gnaden,", "tokens": ["Wie\u00b7der", "auff", "zu", "Gna\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df der H\u00f6llen Rott'", "tokens": ["Da\u00df", "der", "H\u00f6l\u00b7len", "Rott'"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Vns nun nicht kan schaden,", "tokens": ["Vns", "nun", "nicht", "kan", "scha\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Vnd der Tod dazu", "tokens": ["Vnd", "der", "Tod", "da\u00b7zu"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PAV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Vns ist s\u00fcsse Ruh.", "tokens": ["Vns", "ist", "s\u00fcs\u00b7se", "Ruh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Schluckt das kalte Grab", "tokens": ["Schluckt", "das", "kal\u00b7te", "Grab"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Dein' erstarrten Glieder", "tokens": ["Dein'", "er\u00b7starr\u00b7ten", "Glie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Eine Weil' hinab,", "tokens": ["Ei\u00b7ne", "Weil'", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Ey die Zeit k\u00f6mpt wieder,", "tokens": ["Ey", "die", "Zeit", "k\u00f6mpt", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Da auch di\u00df Gebein", "tokens": ["Da", "auch", "di\u00df", "Ge\u00b7bein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "PDS", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Liecht und Glantz wird seyn.", "tokens": ["Liecht", "und", "Glantz", "wird", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Denn wirst du erst voll", "tokens": ["Denn", "wirst", "du", "erst", "voll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADJD"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Heiligr Andacht brennen,", "tokens": ["Hei\u00b7ligr", "An\u00b7dacht", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Vnd dein Auge soll", "tokens": ["Vnd", "dein", "Au\u00b7ge", "soll"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VMFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Gott im Grund erkennen,", "tokens": ["Gott", "im", "Grund", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Gott der im Gem\u00fcht", "tokens": ["Gott", "der", "im", "Ge\u00b7m\u00fcht"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "APPRART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Stets von Liebe gl\u00fct.", "tokens": ["Stets", "von", "Lie\u00b7be", "gl\u00fct", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}