{"textgrid.poem.31273": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1L: P\u00e4rlen-Glantz und Ambra-Zihr", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "P\u00e4rlen-Glantz und Ambra-Zihr", "tokens": ["P\u00e4r\u00b7len\u00b7Glantz", "und", "Am\u00b7bra\u00b7Zihr"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "schufen diese Lider mir/", "tokens": ["schu\u00b7fen", "die\u00b7se", "Li\u00b7der", "mir", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "wenn das Hertz f\u00fcr Sehnsucht sprang/", "tokens": ["wenn", "das", "Hertz", "f\u00fcr", "Sehn\u00b7sucht", "sprang", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "wenn der Drauer-Vogel sang.", "tokens": ["wenn", "der", "Drau\u00b7e\u00b7rVo\u00b7gel", "sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wa\u00df man hofft und wa\u00df man lihbt/", "tokens": ["Wa\u00df", "man", "hofft", "und", "wa\u00df", "man", "lihbt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "KON", "VVFIN", "PIS", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "alles wie ein Rauch verstihbt/", "tokens": ["al\u00b7les", "wie", "ein", "Rauch", "ver\u00b7stihbt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "alles d\u00f6rrt und f\u00e4llt zu Staub/", "tokens": ["al\u00b7les", "d\u00f6rrt", "und", "f\u00e4llt", "zu", "Staub", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "VVFIN", "APPR", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "s\u00e4lbst das gr\u00fcnste Lorbeer-Laub!", "tokens": ["s\u00e4lbst", "das", "gr\u00fcns\u00b7te", "Lor\u00b7beer\u00b7Laub", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "lengst schon die Verwesung frisst/", "tokens": ["lengst", "schon", "die", "Ver\u00b7we\u00b7sung", "frisst", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "mussten in den Sand hinein!", "tokens": ["muss\u00b7ten", "in", "den", "Sand", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie ihr Stern sich auch verstieg/", "tokens": ["Wie", "ihr", "Stern", "sich", "auch", "ver\u00b7stieg", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PRF", "ADV", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "for Hann\u00df ", "tokens": ["for", "Hann\u00df"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "s\u00e4lbst ", "tokens": ["s\u00e4lbst"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "wussten sich f\u00fcr ihm nicht Raht!", "tokens": ["wuss\u00b7ten", "sich", "f\u00fcr", "ihm", "nicht", "Raht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "PTKNEG", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Alles/ alles mu\u00df hinab/", "tokens": ["Al\u00b7les", "/", "al\u00b7les", "mu\u00df", "hin\u00b7ab", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$(", "PIS", "VMFIN", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "irgendwo lihgt mahl mein Grab/", "tokens": ["ir\u00b7gend\u00b7wo", "lihgt", "mahl", "mein", "Grab", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "irgendwo rauscht mahl ein Baum", "tokens": ["ir\u00b7gend\u00b7wo", "rauscht", "mahl", "ein", "Baum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "k\u00fchl in meinen lezzten Draum!", "tokens": ["k\u00fchl", "in", "mei\u00b7nen", "lezz\u00b7ten", "Draum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Schade dr\u00fcmb \u00fcmb jede Nacht/", "tokens": ["Scha\u00b7de", "dr\u00fcmb", "\u00fcmb", "je\u00b7de", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PAV", "ADV", "PIAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "die man ohngek\u00fcsst verbracht;", "tokens": ["die", "man", "ohn\u00b7ge\u00b7k\u00fcsst", "ver\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "schade dr\u00fcmb \u00fcmb jede Lust/", "tokens": ["scha\u00b7de", "dr\u00fcmb", "\u00fcmb", "je\u00b7de", "Lust", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "ADV", "PIAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "die man nie gekant/ gewust!", "tokens": ["die", "man", "nie", "ge\u00b7kant", "/", "ge\u00b7wust", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PIS", "ADV", "VVPP", "$(", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Eylends l\u00e4ufft die kortze Frist/", "tokens": ["Ey\u00b7lends", "l\u00e4ufft", "die", "kort\u00b7ze", "Frist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "die uns hihr geg\u00e4ben ist \u2013", "tokens": ["die", "uns", "hihr", "ge\u00b7g\u00e4\u00b7ben", "ist", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "singt euch/ springt euch au\u00df der Noht/", "tokens": ["singt", "euch", "/", "springt", "euch", "au\u00df", "der", "Noht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "schlagt den Dodt mit Rohsen dodt!", "tokens": ["schlagt", "den", "Dodt", "mit", "Roh\u00b7sen", "dodt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}