{"dta.poem.2797": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Marien Magdalenen Bu\u00dflied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Reinestes Wesen/ heiligste Liebes-Glut/", "tokens": ["Rei\u00b7nes\u00b7tes", "We\u00b7sen", "/", "hei\u00b7ligs\u00b7te", "Lie\u00b7bes\u00b7Glut", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "ADJA", "NN", "$("], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Weil mir von S\u00fcnden brennet Seel/ Hertz und Blut/", "tokens": ["Weil", "mir", "von", "S\u00fcn\u00b7den", "bren\u00b7net", "Seel", "/", "Hertz", "und", "Blut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-++-+", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Und nichts entschuldigt mein Verbrechen/", "tokens": ["Und", "nichts", "ent\u00b7schul\u00b7digt", "mein", "Ver\u00b7bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So la\u00df mich noch die Seuffzer sprechen.", "tokens": ["So", "la\u00df", "mich", "noch", "die", "Seuff\u00b7zer", "spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Flie\u00dft bittre Thr\u00e4nen/ fliesset in wahrer Bu\u00df/", "tokens": ["Flie\u00dft", "bitt\u00b7re", "Thr\u00e4\u00b7nen", "/", "flies\u00b7set", "in", "wah\u00b7rer", "Bu\u00df", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$(", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Mein Geist/ O Sch\u00f6pffer/ f\u00e4llt dir zerknirscht zu Fu\u00df.", "tokens": ["Mein", "Geist", "/", "O", "Sch\u00f6pf\u00b7fer", "/", "f\u00e4llt", "dir", "zer\u00b7knirscht", "zu", "Fu\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "NE", "NE", "$(", "VVFIN", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Die Lust/ die mich so oft erquicket/", "tokens": ["Die", "Lust", "/", "die", "mich", "so", "oft", "er\u00b7quic\u00b7ket", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die ists/ die mich wie Felsen dr\u00fccket.", "tokens": ["Die", "ists", "/", "die", "mich", "wie", "Fel\u00b7sen", "dr\u00fc\u00b7cket", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$(", "PRELS", "PPER", "KOKOM", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Nimm mich verirrtes Schaf zu Genaden an!", "tokens": ["Nimm", "mich", "ver\u00b7irr\u00b7tes", "Schaf", "zu", "Ge\u00b7na\u00b7den", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJA", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Der Wollust Weide hat mir die Lebens-Bahn/", "tokens": ["Der", "Wol\u00b7lust", "Wei\u00b7de", "hat", "mir", "die", "Le\u00b7bens\u00b7Bahn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Die zu dem Himmel f\u00fchrt/ verschr\u00e4ncket/", "tokens": ["Die", "zu", "dem", "Him\u00b7mel", "f\u00fchrt", "/", "ver\u00b7schr\u00e4n\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$(", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Dar\u00fcber sich mein Hertze kr\u00e4ncket.", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "sich", "mein", "Hert\u00b7ze", "kr\u00e4n\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PRF", "PPOSAT", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Ach schn\u00f6de Sinnen/ th\u00f6richter Eitelkeit;", "tokens": ["Ach", "schn\u00f6\u00b7de", "Sin\u00b7nen", "/", "th\u00f6\u00b7rich\u00b7ter", "Ei\u00b7tel\u00b7keit", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$(", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Ach kurtze Freuden und ein unendlich Leyd/", "tokens": ["Ach", "kurt\u00b7ze", "Freu\u00b7den", "und", "ein", "un\u00b7end\u00b7lich", "Leyd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "KON", "ART", "ADJD", "NN", "$("], "meter": "-+-+--++--+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Verkaufft die Welt: und unser Wesen", "tokens": ["Ver\u00b7kaufft", "die", "Welt", ":", "und", "un\u00b7ser", "We\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Verdient des H\u00f6chsten Straffungs Besem.", "tokens": ["Ver\u00b7di\u00b7ent", "des", "H\u00f6chs\u00b7ten", "Straf\u00b7fungs", "Be\u00b7sem", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NE", "NE", "$."], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.17": {"text": "Nein. Magdalena soll sich der Welt entziehn/", "tokens": ["Nein", ".", "Mag\u00b7da\u00b7le\u00b7na", "soll", "sich", "der", "Welt", "ent\u00b7ziehn", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "NE", "VMFIN", "PRF", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.18": {"text": "Und allen Zucker s\u00fcssester Wollust fliehn.", "tokens": ["Und", "al\u00b7len", "Zu\u00b7cker", "s\u00fcs\u00b7ses\u00b7ter", "Wol\u00b7lust", "fliehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Die Brunst von der ihr Blut gerauchet/", "tokens": ["Die", "Brunst", "von", "der", "ihr", "Blut", "ge\u00b7rau\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Wird gar zu andern Feur gebrauchet/", "tokens": ["Wird", "gar", "zu", "an\u00b7dern", "Feur", "ge\u00b7brau\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Weg s\u00fcsse K\u00fcsse/ Reitzer der schn\u00f6den Lust/", "tokens": ["Weg", "s\u00fcs\u00b7se", "K\u00fcs\u00b7se", "/", "Reit\u00b7zer", "der", "schn\u00f6\u00b7den", "Lust", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$(", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Weg geile Griffe auf der Schwan-weissen Brust.", "tokens": ["Weg", "gei\u00b7le", "Grif\u00b7fe", "auf", "der", "Schwan\u00b7weis\u00b7sen", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.23": {"text": "Ich hasse nun des Leibes Bl\u00f6sse/", "tokens": ["Ich", "has\u00b7se", "nun", "des", "Lei\u00b7bes", "Bl\u00f6s\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Scharre mich in Erden-Kl\u00f6se.", "tokens": ["Schar\u00b7re", "mich", "in", "Er\u00b7den\u00b7K\u00b7l\u00f6\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "In meinen Armen soll fort kein Mensch mehr ruhn.", "tokens": ["In", "mei\u00b7nen", "Ar\u00b7men", "soll", "fort", "kein", "Mensch", "mehr", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PTKVZ", "PIAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Ich ", "tokens": ["Ich"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.27": {"text": "Und stat der Buhler Engel k\u00fcssen/", "tokens": ["Und", "stat", "der", "Buh\u00b7ler", "En\u00b7gel", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NE", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Die mir das Paradei\u00df auffschliessen.", "tokens": ["Die", "mir", "das", "Pa\u00b7ra\u00b7dei\u00df", "auff\u00b7schlies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "So mu\u00df man streiten wider Welt/ Fleisch und Blut/", "tokens": ["So", "mu\u00df", "man", "strei\u00b7ten", "wi\u00b7der", "Welt", "/", "Fleisch", "und", "Blut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "VVFIN", "APPR", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Umb zu erhalten jenes versprochne Gut.", "tokens": ["Umb", "zu", "er\u00b7hal\u00b7ten", "je\u00b7nes", "ver\u00b7sproch\u00b7ne", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PTKZU", "VVINF", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "Wer dieses will im Himmel erben/", "tokens": ["Wer", "die\u00b7ses", "will", "im", "Him\u00b7mel", "er\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "VMFIN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Der mu\u00df zuvor der Welt absterben.", "tokens": ["Der", "mu\u00df", "zu\u00b7vor", "der", "Welt", "abs\u00b7ter\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Nun schliest ihr Augen eure Crystallen ein/", "tokens": ["Nun", "schliest", "ihr", "Au\u00b7gen", "eu\u00b7re", "Crys\u00b7tal\u00b7len", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPOSAT", "NN", "ART", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Auf meinen Wangen soll keine Rose seyn.", "tokens": ["Auf", "mei\u00b7nen", "Wan\u00b7gen", "soll", "kei\u00b7ne", "Ro\u00b7se", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "Der Mund wird dem nach Wermuth schmecken/", "tokens": ["Der", "Mund", "wird", "dem", "nach", "Wer\u00b7muth", "schme\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "APPR", "NN", "VVINF", "$("], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.36": {"text": "Der ihn aus geiler Lust wird lecken.", "tokens": ["Der", "ihn", "aus", "gei\u00b7ler", "Lust", "wird", "le\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Schrumpfft volle Br\u00fcste/ welckt/ und fallet hin/", "tokens": ["Schrumpfft", "vol\u00b7le", "Br\u00fcs\u00b7te", "/", "welckt", "/", "und", "fal\u00b7let", "hin", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$(", "VVFIN", "$(", "KON", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Die ihr entz\u00fcndet manchen verliebten Sinn.", "tokens": ["Die", "ihr", "ent\u00b7z\u00fcn\u00b7det", "man\u00b7chen", "ver\u00b7lieb\u00b7ten", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.39": {"text": "Vertrockne Schos mit deiner Quelle:", "tokens": ["Ver\u00b7trock\u00b7ne", "Schos", "mit", "dei\u00b7ner", "Quel\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Wachst Nesselin/ wachst auff dieser Stelle.", "tokens": ["Wachst", "Nes\u00b7se\u00b7lin", "/", "wachst", "auff", "die\u00b7ser", "Stel\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.41": {"text": "Des Leibes K\u00fctzel mu\u00df ich vor reissen ein/", "tokens": ["Des", "Lei\u00b7bes", "K\u00fct\u00b7zel", "mu\u00df", "ich", "vor", "reis\u00b7sen", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "PPER", "APPR", "VVFIN", "ART", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.42": {"text": "Eh er ein Tempel soll des Erl\u00f6sers seyn.", "tokens": ["Eh", "er", "ein", "Tem\u00b7pel", "soll", "des", "Er\u00b7l\u00f6\u00b7sers", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VMFIN", "ART", "NN", "VAINF", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.43": {"text": "Mein Fleisch mu\u00df sich in Asche kehren/", "tokens": ["Mein", "Fleisch", "mu\u00df", "sich", "in", "A\u00b7sche", "keh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PRF", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Wo es die Bl\u00fcthen soll gew\u00e4hren.", "tokens": ["Wo", "es", "die", "Bl\u00fc\u00b7then", "soll", "ge\u00b7w\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Ich lig und b\u00fcsse. Ach GOtt mein h\u00f6chstes Licht!", "tokens": ["Ich", "lig", "und", "b\u00fcs\u00b7se", ".", "Ach", "Gott", "mein", "h\u00f6chs\u00b7tes", "Licht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "VVFIN", "$.", "ITJ", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.46": {"text": "Die Magdalena ist mehr kein Welt-Kind nicht.", "tokens": ["Die", "Mag\u00b7da\u00b7le\u00b7na", "ist", "mehr", "kein", "Welt\u00b7Kind", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "ADV", "PIAT", "NN", "PTKNEG", "$."], "meter": "----+-+-+-+", "measure": "unknown.measure.tetra"}, "line.47": {"text": "Sie ist die Braut des Lammes worden/", "tokens": ["Sie", "ist", "die", "Braut", "des", "Lam\u00b7mes", "wor\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "VAPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Und l\u00e4st den alten Liebes-Orden.", "tokens": ["Und", "l\u00e4st", "den", "al\u00b7ten", "Lie\u00b7bes\u00b7Or\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}