{"textgrid.poem.55771": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im sausenden Wettersturm,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im sausenden Wettersturm,", "tokens": ["Im", "sau\u00b7sen\u00b7den", "Wet\u00b7ter\u00b7sturm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Barh\u00e4uptig,", "tokens": ["Bar\u00b7h\u00e4up\u00b7tig", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Bin ich hinangeklettert", "tokens": ["Bin", "ich", "hin\u00b7an\u00b7ge\u00b7klet\u00b7tert"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An nebeltriefenden,", "tokens": ["An", "ne\u00b7bel\u00b7trie\u00b7fen\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NE", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Feuchten Felsen,", "tokens": ["Feuch\u00b7ten", "Fel\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Hinein in schauerliche Kl\u00fcfte,", "tokens": ["Hin\u00b7ein", "in", "schau\u00b7er\u00b7li\u00b7che", "Kl\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "In des Hochgebirges", "tokens": ["In", "des", "Hoch\u00b7ge\u00b7bir\u00b7ges"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Grauenhafteste Einsamkeit", "tokens": ["Grau\u00b7en\u00b7haf\u00b7tes\u00b7te", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.9": {"text": "Hab' ich meiner Seele", "tokens": ["Hab'", "ich", "mei\u00b7ner", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Sch\u00e4umenden Zorn,", "tokens": ["Sch\u00e4u\u00b7men\u00b7den", "Zorn", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Alle verbissenen,", "tokens": ["Al\u00b7le", "ver\u00b7bis\u00b7se\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVINF", "$,"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.12": {"text": "Langverheuchelten Qualen gerufen;", "tokens": ["Lang\u00b7ver\u00b7heu\u00b7chel\u00b7ten", "Qua\u00b7len", "ge\u00b7ru\u00b7fen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "Angeklammert an zackige Felsenbr\u00fcche,", "tokens": ["An\u00b7ge\u00b7klam\u00b7mert", "an", "za\u00b7cki\u00b7ge", "Fel\u00b7sen\u00b7br\u00fc\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.14": {"text": "\u00dcberh\u00e4ngend halb", "tokens": ["\u00dc\u00b7ber\u00b7h\u00e4n\u00b7gend", "halb"], "token_info": ["word", "word"], "pos": ["ADJD", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.15": {"text": "\u00dcber den Abgrund,", "tokens": ["\u00dc\u00b7ber", "den", "Ab\u00b7grund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "Hab' ich todesl\u00fcstern", "tokens": ["Hab'", "ich", "to\u00b7des\u00b7l\u00fcs\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPER", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.17": {"text": "Hinunter gestarrt \u2013", "tokens": ["Hin\u00b7un\u00b7ter", "ge\u00b7starrt", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.18": {"text": "Und doch nicht Mut gefunden,", "tokens": ["Und", "doch", "nicht", "Mut", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Loszulassen.", "tokens": ["Los\u00b7zu\u00b7las\u00b7sen", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.20": {"text": "In starrendes Zwerggestr\u00fcpp", "tokens": ["In", "star\u00b7ren\u00b7des", "Zwerg\u00b7ge\u00b7str\u00fcpp"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.21": {"text": "Schlug ich die fiebernde Stirn", "tokens": ["Schlug", "ich", "die", "fie\u00b7bern\u00b7de", "Stirn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.22": {"text": "Und vergrub meine Z\u00e4hne", "tokens": ["Und", "ver\u00b7grub", "mei\u00b7ne", "Z\u00e4h\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.23": {"text": "In knorrigem Wurzelwerk.", "tokens": ["In", "knor\u00b7ri\u00b7gem", "Wur\u00b7zel\u00b7werk", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.24": {"text": "Aber weder der Schmerz, noch der Zorn,", "tokens": ["A\u00b7ber", "we\u00b7der", "der", "Schmerz", ",", "noch", "der", "Zorn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KON", "ART", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.25": {"text": "Weder die Scham, noch die Reue", "tokens": ["We\u00b7der", "die", "Scham", ",", "noch", "die", "Reu\u00b7e"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "ADV", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.26": {"text": "Tilgte meiner Gedanken", "tokens": ["Tilg\u00b7te", "mei\u00b7ner", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.27": {"text": "Krankhaft verzerrte,", "tokens": ["Krank\u00b7haft", "ver\u00b7zerr\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.28": {"text": "Faulige Brut.", "tokens": ["Fau\u00b7li\u00b7ge", "Brut", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}