{"textgrid.poem.64931": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil doch mein blanker Schild mu\u00df einen Wahlspruch haben,", "tokens": ["Weil", "doch", "mein", "blan\u00b7ker", "Schild", "mu\u00df", "ei\u00b7nen", "Wahl\u00b7spruch", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "NN", "VMFIN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sei weltverachtend nun ihm dieser eingegraben:", "tokens": ["Sei", "welt\u00b7ver\u00b7ach\u00b7tend", "nun", "ihm", "die\u00b7ser", "ein\u00b7ge\u00b7gra\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "ADV", "PPER", "PDS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Tadel dieser Welt, ihr Lob dient mir zum Spott;", "tokens": ["Der", "Ta\u00b7del", "die\u00b7ser", "Welt", ",", "ihr", "Lob", "dient", "mir", "zum", "Spott", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$,", "PPOSAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur Gott blickt in mein Herz \u2013 mein Richter ist nur Gott!", "tokens": ["Nur", "Gott", "blickt", "in", "mein", "Herz", "\u2013", "mein", "Rich\u00b7ter", "ist", "nur", "Gott", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$(", "PPO