{"textgrid.poem.67004": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Gemeinwirtschaft", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist nicht wahr, da\u00df Menschentatkraft leidet,", "tokens": ["Es", "ist", "nicht", "wahr", ",", "da\u00df", "Men\u00b7schen\u00b7tat\u00b7kraft", "lei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn sie dem Ganzen gibt, was ihm geb\u00fchrt,", "tokens": ["Wenn", "sie", "dem", "Gan\u00b7zen", "gibt", ",", "was", "ihm", "ge\u00b7b\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und f\u00fcr des Werkes h\u00f6chsten Wert entscheidet,", "tokens": ["Und", "f\u00fcr", "des", "Wer\u00b7kes", "h\u00f6chs\u00b7ten", "Wert", "ent\u00b7schei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ob es zum Heil der Volksgemeinschaft f\u00fchrt.", "tokens": ["Ob", "es", "zum", "Heil", "der", "Volks\u00b7ge\u00b7mein\u00b7schaft", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}