{"textgrid.poem.38648": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "4. Versoy", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Choiseul baute mir Stra\u00dfen, doch wurden die H\u00e4user nicht fertig", "tokens": ["Choi\u00b7seul", "bau\u00b7te", "mir", "Stra\u00b7\u00dfen", ",", "doch", "wur\u00b7den", "die", "H\u00e4u\u00b7ser", "nicht", "fer\u00b7tig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "PTKNEG", "ADJD"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Und an den Rissen zum Port nagt schon seit Jahren der Wurm.", "tokens": ["Und", "an", "den", "Ris\u00b7sen", "zum", "Port", "nagt", "schon", "seit", "Jah\u00b7ren", "der", "Wurm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPRART", "NE", "VVFIN", "ADV", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "H\u00e4tt' ich nicht noch die Fabrik der ber\u00fchmten Argandischen Lampen,", "tokens": ["H\u00e4tt'", "ich", "nicht", "noch", "die", "Fab\u00b7rik", "der", "be\u00b7r\u00fchm\u00b7ten", "Ar\u00b7gan\u00b7di\u00b7schen", "Lam\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "ART", "NN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+--+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.invert"}, "line.4": {"text": "Sicherlich spr\u00e4che von mir unter der Sonne kein Mensch.", "tokens": ["Si\u00b7cher\u00b7lich", "spr\u00e4\u00b7che", "von", "mir", "un\u00b7ter", "der", "Son\u00b7ne", "kein", "Mensch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "APPR", "ART", "NN", "PIAT", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}}}}}