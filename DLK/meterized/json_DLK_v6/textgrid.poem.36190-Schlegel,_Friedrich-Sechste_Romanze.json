{"textgrid.poem.36190": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Sechste Romanze", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Boten kamen her mit Eile,", "tokens": ["Bo\u00b7ten", "ka\u00b7men", "her", "mit", "Ei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kaiser Karol anzuzeigen,", "tokens": ["Kai\u00b7ser", "Ka\u00b7rol", "an\u00b7zu\u00b7zei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo Garzimes Berg, der steile,", "tokens": ["Wo", "Gar\u00b7zi\u00b7mes", "Berg", ",", "der", "stei\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der Navarr'schen Berge einer,", "tokens": ["Der", "Na\u00b7varr'\u00b7schen", "Ber\u00b7ge", "ei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Stolzer in die Wolken steiget,", "tokens": ["Stol\u00b7zer", "in", "die", "Wol\u00b7ken", "stei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "K\u00e4m' ein F\u00fcrst ihn anzufeinden,", "tokens": ["K\u00e4m'", "ein", "F\u00fcrst", "ihn", "an\u00b7zu\u00b7fein\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "K\u00f6nig Furr, ein wilder Heide.", "tokens": ["K\u00f6\u00b7nig", "Furr", ",", "ein", "wil\u00b7der", "Hei\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "An Garzimes Felsensteine", "tokens": ["An", "Gar\u00b7zi\u00b7mes", "Fel\u00b7sen\u00b7stei\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "NE"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Kam zu Abend hin der Kaiser;", "tokens": ["Kam", "zu", "A\u00b7bend", "hin", "der", "Kai\u00b7ser", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Morgen soll die Schlacht entscheiden,", "tokens": ["Mor\u00b7gen", "soll", "die", "Schlacht", "ent\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Und er bat Gott um ein Zeichen,", "tokens": ["Und", "er", "bat", "Gott", "um", "ein", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "--++--+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "In der dunklen Nacht geheime,", "tokens": ["In", "der", "dunk\u00b7len", "Nacht", "ge\u00b7hei\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Da\u00df er die mag unterscheiden,", "tokens": ["Da\u00df", "er", "die", "mag", "un\u00b7ter\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Die des andern Tags erbleichen", "tokens": ["Die", "des", "an\u00b7dern", "Tags", "er\u00b7blei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Todes sollen von den Feinden,", "tokens": ["To\u00b7des", "sol\u00b7len", "von", "den", "Fein\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Sch\u00f6nen M\u00e4rt'rerkranz erreichen. \u2013", "tokens": ["Sch\u00f6\u00b7nen", "M\u00e4rt'\u00b7rer\u00b7kranz", "er\u00b7rei\u00b7chen", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "In des andern Fr\u00fchrots Scheine,", "tokens": ["In", "des", "an\u00b7dern", "Fr\u00fch\u00b7rots", "Schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Da ger\u00fcstet all' sich zeigen,", "tokens": ["Da", "ge\u00b7r\u00fcs\u00b7tet", "all'", "sich", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Haufen hier und dort sich teilen,", "tokens": ["Hau\u00b7fen", "hier", "und", "dort", "sich", "tei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "ADV", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Sieht ein rotes Kreuze scheinen", "tokens": ["Sieht", "ein", "ro\u00b7tes", "Kreu\u00b7ze", "schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "An der Schulter auf den Kleidern", "tokens": ["An", "der", "Schul\u00b7ter", "auf", "den", "Klei\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Er der auserw\u00e4hlten Seinen,", "tokens": ["Er", "der", "au\u00b7ser\u00b7w\u00e4hl\u00b7ten", "Sei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Die als M\u00e4rtyrer erbleichen", "tokens": ["Die", "als", "M\u00e4r\u00b7ty\u00b7rer", "er\u00b7blei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "KOUS", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Sollen und den Tod erleiden.", "tokens": ["Sol\u00b7len", "und", "den", "Tod", "er\u00b7lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "KON", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Solches sieht allein der Kaiser,", "tokens": ["Sol\u00b7ches", "sieht", "al\u00b7lein", "der", "Kai\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Au\u00dfer ihm gewahrt es keiner.", "tokens": ["Au\u00b7\u00dfer", "ihm", "ge\u00b7wahrt", "es", "kei\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "PPER", "PIS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Wehmut an sein Herz da greifet;", "tokens": ["Weh\u00b7mut", "an", "sein", "Herz", "da", "grei\u00b7fet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Und es regt der Wunsch sich heimlich,", "tokens": ["Und", "es", "regt", "der", "Wunsch", "sich", "heim\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "PRF", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Wie sie lebend m\u00f6chten bleiben,", "tokens": ["Wie", "sie", "le\u00b7bend", "m\u00f6ch\u00b7ten", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Die mit rotem Kreuz gezeichnet.", "tokens": ["Die", "mit", "ro\u00b7tem", "Kreuz", "ge\u00b7zeich\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Alle er alsbald vereinet,", "tokens": ["Al\u00b7le", "er", "als\u00b7bald", "ver\u00b7ei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Schlie\u00dft sie in der Kirchen eine,", "tokens": ["Schlie\u00dft", "sie", "in", "der", "Kir\u00b7chen", "ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ART", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Sie zu retten so vermeinend;", "tokens": ["Sie", "zu", "ret\u00b7ten", "so", "ver\u00b7mei\u00b7nend", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Seine Absicht wu\u00dfte keiner.", "tokens": ["Sei\u00b7ne", "Ab\u00b7sicht", "wu\u00df\u00b7te", "kei\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Mutvoll gehn sie auf die Feinde,", "tokens": ["Mut\u00b7voll", "gehn", "sie", "auf", "die", "Fein\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Schlagen bald die wilden Heiden,", "tokens": ["Schla\u00b7gen", "bald", "die", "wil\u00b7den", "Hei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Auch ihr K\u00f6nig mu\u00df erbleichen,", "tokens": ["Auch", "ihr", "K\u00f6\u00b7nig", "mu\u00df", "er\u00b7blei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Mit dreitausend von den Seinen.", "tokens": ["Mit", "drei\u00b7tau\u00b7send", "von", "den", "Sei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "APPR", "ART", "PPOSS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Freudig zieht mit Siegeszeichen", "tokens": ["Freu\u00b7dig", "zieht", "mit", "Sie\u00b7ges\u00b7zei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Heim das Heer im Abendscheine.", "tokens": ["Heim", "das", "Heer", "im", "A\u00b7bend\u00b7schei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Da nun heimgekehrt der Kaiser,", "tokens": ["Da", "nun", "heim\u00b7ge\u00b7kehrt", "der", "Kai\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Und die Kirche \u00f6ffnet schweigend,", "tokens": ["Und", "die", "Kir\u00b7che", "\u00f6ff\u00b7net", "schwei\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Sieht er hundertfunfzig Leichen,", "tokens": ["Sieht", "er", "hun\u00b7dert\u00b7funf\u00b7zig", "Lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Sanft entseelt und bleich sich seinem", "tokens": ["Sanft", "ent\u00b7seelt", "und", "bleich", "sich", "sei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "KON", "ADJD", "PRF", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Auge strafend allda zeigen.", "tokens": ["Au\u00b7ge", "stra\u00b7fend", "all\u00b7da", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PAV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Bitterlich er die beweinet,", "tokens": ["Bit\u00b7ter\u00b7lich", "er", "die", "be\u00b7wei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Reuevoll und voll Mitleiden.", "tokens": ["Reu\u00b7e\u00b7voll", "und", "voll", "Mit\u00b7lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "O ihr Christusk\u00e4mpfer heilig,", "tokens": ["O", "ihr", "Chris\u00b7tus\u00b7k\u00e4mp\u00b7fer", "hei\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "Traf euch nicht der Arm des Feindes,", "tokens": ["Traf", "euch", "nicht", "der", "Arm", "des", "Fein\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Weil von irdischem Mitleiden", "tokens": ["Weil", "von", "ir\u00b7di\u00b7schem", "Mit\u00b7lei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.51": {"text": "Sich der Kaiser lie\u00df ergreifen,", "tokens": ["Sich", "der", "Kai\u00b7ser", "lie\u00df", "er\u00b7grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Gottes F\u00fcgung wollte meiden,", "tokens": ["Got\u00b7tes", "F\u00fc\u00b7gung", "woll\u00b7te", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.53": {"text": "Bleibt der Kranz euch dennoch eigen!", "tokens": ["Bleibt", "der", "Kranz", "euch", "den\u00b7noch", "ei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}