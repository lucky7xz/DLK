{"textgrid.poem.49359": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "An die H\u00e4nde seines Seligmachers", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebster Jesu, sei gegr\u00fc\u00dfet,", "tokens": ["Liebs\u00b7ter", "Je\u00b7su", ",", "sei", "ge\u00b7gr\u00fc\u00b7\u00dfet", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sei gegr\u00fc\u00dfet tausendmal,", "tokens": ["Sei", "ge\u00b7gr\u00fc\u00b7\u00dfet", "tau\u00b7send\u00b7mal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der du hast vor mich geb\u00fc\u00dfet,", "tokens": ["Der", "du", "hast", "vor", "mich", "ge\u00b7b\u00fc\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VAFIN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als man dir mit gro\u00dfer Qual", "tokens": ["Als", "man", "dir", "mit", "gro\u00b7\u00dfer", "Qual"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deine H\u00e4nd' ans Kreuz geschlagen", "tokens": ["Dei\u00b7ne", "H\u00e4nd'", "ans", "Kreuz", "ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und sie lassen S\u00fcnde tragen.", "tokens": ["Und", "sie", "las\u00b7sen", "S\u00fcn\u00b7de", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Seid gegr\u00fc\u00dfet, o ihr H\u00e4nde,", "tokens": ["Seid", "ge\u00b7gr\u00fc\u00b7\u00dfet", ",", "o", "ihr", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "VVPP", "$,", "FM", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Was vor Rosen stehn in euch!", "tokens": ["Was", "vor", "Ro\u00b7sen", "stehn", "in", "euch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Sch\u00f6ne Rosen, welch' am Ende", "tokens": ["Sch\u00f6\u00b7ne", "Ro\u00b7sen", ",", "welch'", "am", "En\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Christum machen rot und bleich;", "tokens": ["Chris\u00b7tum", "ma\u00b7chen", "rot", "und", "bleich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Ach, ich sehe da mit Haufen", "tokens": ["Ach", ",", "ich", "se\u00b7he", "da", "mit", "Hau\u00b7fen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Blut aus ihren Wunden laufen!", "tokens": ["Blut", "aus", "ih\u00b7ren", "Wun\u00b7den", "lau\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Herr, ich mu\u00df ans Herz itz dr\u00fccken", "tokens": ["Herr", ",", "ich", "mu\u00df", "ans", "Herz", "itz", "dr\u00fc\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VMFIN", "APPRART", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Diese Wunden purpurrot,", "tokens": ["Die\u00b7se", "Wun\u00b7den", "pur\u00b7pur\u00b7rot", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Die mir Leib und Seel' erquicken", "tokens": ["Die", "mir", "Leib", "und", "Seel'", "er\u00b7qui\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "NN", "KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "In der allerh\u00f6chsten Not;", "tokens": ["In", "der", "al\u00b7ler\u00b7h\u00f6chs\u00b7ten", "Not", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Herr, mich d\u00fcrstet, die\u00df sind Gaben,", "tokens": ["Herr", ",", "mich", "d\u00fcrs\u00b7tet", ",", "die\u00df", "sind", "Ga\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "$,", "PDS", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Die mich kr\u00e4ftig k\u00f6nnen laben.", "tokens": ["Die", "mich", "kr\u00e4f\u00b7tig", "k\u00f6n\u00b7nen", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "O, wie bist du doch so g\u00fcnstig", "tokens": ["O", ",", "wie", "bist", "du", "doch", "so", "g\u00fcns\u00b7tig"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "VAFIN", "PPER", "ADV", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Allen S\u00fcndern dieser Welt!", "tokens": ["Al\u00b7len", "S\u00fcn\u00b7dern", "die\u00b7ser", "Welt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Ja, wie liebest du so br\u00fcnstig", "tokens": ["Ja", ",", "wie", "lie\u00b7best", "du", "so", "br\u00fcns\u00b7tig"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PWAV", "VVFIN", "PPER", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Was der Erdkreis in sich h\u00e4lt!", "tokens": ["Was", "der", "Erd\u00b7kreis", "in", "sich", "h\u00e4lt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Herr, du tr\u00e4gest aus Erbarmen", "tokens": ["Herr", ",", "du", "tr\u00e4\u00b7gest", "aus", "Er\u00b7bar\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "B\u00f6 und Gut' in deinen Armen.", "tokens": ["B\u00f6", "und", "Gut'", "in", "dei\u00b7nen", "Ar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Nun, ich stelle dir Geplagten", "tokens": ["Nun", ",", "ich", "stel\u00b7le", "dir", "Ge\u00b7plag\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PPER", "VVFIN", "PPER", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Einen gro\u00dfen S\u00fcnder f\u00fcr:", "tokens": ["Ei\u00b7nen", "gro\u00b7\u00dfen", "S\u00fcn\u00b7der", "f\u00fcr", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Sei barmherzig mir Verzagten,", "tokens": ["Sei", "barm\u00b7her\u00b7zig", "mir", "Ver\u00b7zag\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Oeffne deine Gnadenth\u00fcr;", "tokens": ["Oeff\u00b7ne", "dei\u00b7ne", "Gna\u00b7dent\u00b7h\u00fcr", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Pflegst du doch das einzulassen,", "tokens": ["Pflegst", "du", "doch", "das", "ein\u00b7zu\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PDS", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Was dich kan im Glauben fassen.", "tokens": ["Was", "dich", "kan", "im", "Glau\u00b7ben", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Ziehe mich, der du gezogen", "tokens": ["Zie\u00b7he", "mich", ",", "der", "du", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Mit den H\u00e4nden an den Baum,", "tokens": ["Mit", "den", "H\u00e4n\u00b7den", "an", "den", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Hilf, da\u00df ich, dadurch bewogen,", "tokens": ["Hilf", ",", "da\u00df", "ich", ",", "da\u00b7durch", "be\u00b7wo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "$,", "PAV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Dir in mir stets mache Raum;", "tokens": ["Dir", "in", "mir", "stets", "ma\u00b7che", "Raum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "ADV", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "All mein K\u00f6nnen, Wollen, Wissen", "tokens": ["All", "mein", "K\u00f6n\u00b7nen", ",", "Wol\u00b7len", ",", "Wis\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PIAT", "PPOSAT", "NN", "$,", "VVFIN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Sei nur auf dein Kreuz geflissen.", "tokens": ["Sei", "nur", "auf", "dein", "Kreuz", "ge\u00b7flis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "La\u00df mich deine Liebe schmecken,", "tokens": ["La\u00df", "mich", "dei\u00b7ne", "Lie\u00b7be", "schme\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Weil ich sehnlich nach ihr d\u00fcrst',", "tokens": ["Weil", "ich", "sehn\u00b7lich", "nach", "ihr", "d\u00fcr\u00b7st'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPER", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Ich wil meinen Geist erwecken", "tokens": ["Ich", "wil", "mei\u00b7nen", "Geist", "er\u00b7we\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.40": {"text": "Dir zu Dienst', o Lebensf\u00fcrst;", "tokens": ["Dir", "zu", "Dienst'", ",", "o", "Le\u00b7bens\u00b7f\u00fcrst", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "$,", "FM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.41": {"text": "Alles Tr\u00fcbsal wird mich lassen,", "tokens": ["Al\u00b7les", "Tr\u00fcb\u00b7sal", "wird", "mich", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Kan ich nur die Laster hassen.", "tokens": ["Kan", "ich", "nur", "die", "Las\u00b7ter", "has\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Seid gegr\u00fc\u00dfet, o ihr H\u00e4nde,", "tokens": ["Seid", "ge\u00b7gr\u00fc\u00b7\u00dfet", ",", "o", "ihr", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "VVPP", "$,", "FM", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Gebet mir doch volle Macht,", "tokens": ["Ge\u00b7bet", "mir", "doch", "vol\u00b7le", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.45": {"text": "Da\u00df ich mich im Glauben wende,", "tokens": ["Da\u00df", "ich", "mich", "im", "Glau\u00b7ben", "wen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Euch zu danken Tag und Nacht;", "tokens": ["Euch", "zu", "dan\u00b7ken", "Tag", "und", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.47": {"text": "Lasset doch mit hei\u00dfen Thr\u00e4nen", "tokens": ["Las\u00b7set", "doch", "mit", "hei\u00b7\u00dfen", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Mich nach euren Wunden sehnen!", "tokens": ["Mich", "nach", "eu\u00b7ren", "Wun\u00b7den", "seh\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "Nun, so bin ich rein gebadet,", "tokens": ["Nun", ",", "so", "bin", "ich", "rein", "ge\u00b7ba\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Liebster Herr, in deinem Blut;", "tokens": ["Liebs\u00b7ter", "Herr", ",", "in", "dei\u00b7nem", "Blut", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.51": {"text": "Es ist niemand, der mir schadet,", "tokens": ["Es", "ist", "nie\u00b7mand", ",", "der", "mir", "scha\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Denn ich leb' in deiner Hut.", "tokens": ["Denn", "ich", "leb'", "in", "dei\u00b7ner", "Hut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.53": {"text": "Jesu, nim am letzten Ende", "tokens": ["Je\u00b7su", ",", "nim", "am", "letz\u00b7ten", "En\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.54": {"text": "Meine Seel' in deine H\u00e4nde!", "tokens": ["Mei\u00b7ne", "Seel'", "in", "dei\u00b7ne", "H\u00e4n\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}