{"dta.poem.12103": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "25.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hoch im Gebirge lag ein stiller See, und gab", "tokens": ["Hoch", "im", "Ge\u00b7bir\u00b7ge", "lag", "ein", "stil\u00b7ler", "See", ",", "und", "gab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,", "KON", "VVFIN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Nur einen schmalen Bach dem Flu\u00df im Thal hinab.", "tokens": ["Nur", "ei\u00b7nen", "schma\u00b7len", "Bach", "dem", "Flu\u00df", "im", "Thal", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Er hielt die Spalten eng, daraus sein Abflu\u00df quoll,", "tokens": ["Er", "hielt", "die", "Spal\u00b7ten", "eng", ",", "da\u00b7raus", "sein", "Ab\u00b7flu\u00df", "quoll", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "$,", "PAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und weise M\u00e4\u00dfigkeit erhielt ihn immer voll.", "tokens": ["Und", "wei\u00b7se", "M\u00e4\u00b7\u00dfig\u00b7keit", "er\u00b7hielt", "ihn", "im\u00b7mer", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Da rief zum See hinauf der Strom mit lautem Grollen:", "tokens": ["Da", "rief", "zum", "See", "hin\u00b7auf", "der", "Strom", "mit", "lau\u00b7tem", "Grol\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Warum nicht reicheren Tribut willst du mir zollen?", "tokens": ["Wa\u00b7rum", "nicht", "rei\u00b7che\u00b7ren", "Tri\u00b7but", "willst", "du", "mir", "zol\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "ADJA", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Anstatt in tr\u00e4ger Ruh auf deinem Grund zu stocken,", "tokens": ["An\u00b7statt", "in", "tr\u00e4\u00b7ger", "Ruh", "auf", "dei\u00b7nem", "Grund", "zu", "sto\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "St\u00fcrz dich in mich herab, und la\u00df dein Bette trocken!", "tokens": ["St\u00fcrz", "dich", "in", "mich", "her\u00b7ab", ",", "und", "la\u00df", "dein", "Bet\u00b7te", "tro\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "PPER", "ADV", "$,", "KON", "VVIMP", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der See dagegen sprach: O Strom, du bist so reich;", "tokens": ["Der", "See", "da\u00b7ge\u00b7gen", "sprach", ":", "O", "Strom", ",", "du", "bist", "so", "reich", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "VVFIN", "$.", "NE", "NN", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Soll alles Wasser denn im Thale seyn zugleich?", "tokens": ["Soll", "al\u00b7les", "Was\u00b7ser", "denn", "im", "Tha\u00b7le", "seyn", "zu\u00b7gleich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "KON", "APPRART", "NN", "PPOSAT", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Mit deinen Sch\u00e4tzen magst du rasch und breit hinflie\u00dfen;", "tokens": ["Mit", "dei\u00b7nen", "Sch\u00e4t\u00b7zen", "magst", "du", "rasch", "und", "breit", "hin\u00b7flie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00df eines Spiegels auch die Einsamkeit genie\u00dfen.", "tokens": ["La\u00df", "ei\u00b7nes", "Spie\u00b7gels", "auch", "die", "Ein\u00b7sam\u00b7keit", "ge\u00b7nie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Du tr\u00e4nkest Ro\u00df und Rind, ich tr\u00e4nke Hirsch und Hind;", "tokens": ["Du", "tr\u00e4n\u00b7kest", "Ro\u00df", "und", "Rind", ",", "ich", "tr\u00e4n\u00b7ke", "Hirsch", "und", "Hind", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,", "PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und meine Wogen lind regt Fr\u00fch- und Abendwind.", "tokens": ["Und", "mei\u00b7ne", "Wo\u00b7gen", "lind", "regt", "Fr\u00fch", "und", "Ab\u00b7end\u00b7wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "VVFIN", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Ich w\u00fcrde, folgt' ich dir, tr\u00fcb werden wie du bist,", "tokens": ["Ich", "w\u00fcr\u00b7de", ",", "folgt'", "ich", "dir", ",", "tr\u00fcb", "wer\u00b7den", "wie", "du", "bist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "VVFIN", "PPER", "PPER", "$,", "ADJD", "VAINF", "KOKOM", "PPER", "VAFIN", "$,"], "meter": "-+-+-+++-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Da hier mein tiefes Blau der Neid des Himmels ist.", "tokens": ["Da", "hier", "mein", "tie\u00b7fes", "Blau", "der", "Neid", "des", "Him\u00b7mels", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "ADJA", "NN", "ART", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}