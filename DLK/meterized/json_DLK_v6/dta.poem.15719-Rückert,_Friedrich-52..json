{"dta.poem.15719": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "52.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Blum' im Felde klagt: Weh dieser rauhen Wiese!", "tokens": ["Die", "Blum'", "im", "Fel\u00b7de", "klagt", ":", "Weh", "die\u00b7ser", "rau\u00b7hen", "Wie\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$.", "NN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie anders war mein Stand in jenem Paradiese!", "tokens": ["Wie", "an\u00b7ders", "war", "mein", "Stand", "in", "je\u00b7nem", "Pa\u00b7ra\u00b7die\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPOSAT", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da schwebte leicht der Mensch wie Fr\u00fchlingsengelgru\u00df,", "tokens": ["Da", "schweb\u00b7te", "leicht", "der", "Mensch", "wie", "Fr\u00fch\u00b7ling\u00b7sen\u00b7gel\u00b7gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und trat den Boden nicht und mich mit schwerem Fu\u00df.", "tokens": ["Und", "trat", "den", "Bo\u00b7den", "nicht", "und", "mich", "mit", "schwe\u00b7rem", "Fu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKNEG", "KON", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Alswie der Vogel schwebt, alswie des Vogels Schatten,", "tokens": ["Als\u00b7wie", "der", "Vo\u00b7gel", "schwebt", ",", "als\u00b7wie", "des", "Vo\u00b7gels", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NE", "VVFIN", "$,", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schwebt' er gefl\u00fcgelt ob den ewiggr\u00fcnen Matten;", "tokens": ["Schwebt'", "er", "ge\u00b7fl\u00fc\u00b7gelt", "ob", "den", "e\u00b7wig\u00b7gr\u00fc\u00b7nen", "Mat\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.4": {"line.1": {"text": "Wie Schmetterlinge, die auf schwankem Halm sich gatten.", "tokens": ["Wie", "Schmet\u00b7ter\u00b7lin\u00b7ge", ",", "die", "auf", "schwan\u00b7kem", "Halm", "sich", "gat\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Im Garten war auf vier gestellt kein plumpes Thier,", "tokens": ["Im", "Gar\u00b7ten", "war", "auf", "vier", "ge\u00b7stellt", "kein", "plum\u00b7pes", "Thier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "APPR", "CARD", "VVPP", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur Mensch und Vogel war, lobsingend \u00fcber mir.", "tokens": ["Nur", "Mensch", "und", "Vo\u00b7gel", "war", ",", "lob\u00b7sin\u00b7gend", "\u00fc\u00b7ber", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NE", "VAFIN", "$,", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}