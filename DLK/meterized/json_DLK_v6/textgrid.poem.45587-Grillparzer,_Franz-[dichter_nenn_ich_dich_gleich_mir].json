{"textgrid.poem.45587": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[dichter nenn ich dich gleich mir]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dichter nenn ich dich gleich mir.", "tokens": ["Dich\u00b7ter", "nenn", "ich", "dich", "gleich", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "ADV", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dichten hei\u00dft zumeist doch eben,", "tokens": ["Dich\u00b7ten", "hei\u00dft", "zu\u00b7meist", "doch", "e\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In fremdem Dasein eignes leben,", "tokens": ["In", "frem\u00b7dem", "Da\u00b7sein", "eig\u00b7nes", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und da, err\u00f6tend, weich ich dir.", "tokens": ["Und", "da", ",", "er\u00b7r\u00f6\u00b7tend", ",", "weich", "ich", "dir", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "VVPP", "$,", "KOUS", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}