{"dta.poem.3136": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "181.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O Wunder, oft schon stand hart an des Abgrunds Rand", "tokens": ["O", "Wun\u00b7der", ",", "oft", "schon", "stand", "hart", "an", "des", "Ab\u00b7grunds", "Rand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "ADV", "ADV", "VVFIN", "ADJD", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Mensch, zum Sturz bereit, den er nicht vorempfand.", "tokens": ["Ein", "Mensch", ",", "zum", "Sturz", "be\u00b7reit", ",", "den", "er", "nicht", "vor\u00b7emp\u00b7fand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "ADJD", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ihm gegen\u00fcber steht das drohende Geschick,", "tokens": ["Ihm", "ge\u00b7gen\u00b7\u00fc\u00b7ber", "steht", "das", "dro\u00b7hen\u00b7de", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er wird es nicht gewahr mit unbefangnem Blick.", "tokens": ["Er", "wird", "es", "nicht", "ge\u00b7wahr", "mit", "un\u00b7be\u00b7fang\u00b7nem", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was schlie\u00dfest du daraus? das arme Menschenkind", "tokens": ["Was", "schlie\u00b7\u00dfest", "du", "da\u00b7raus", "?", "das", "ar\u00b7me", "Men\u00b7schen\u00b7kind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "PAV", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sei gegen sein Geschick unm\u00e4chtig, schwach und blind?", "tokens": ["Sei", "ge\u00b7gen", "sein", "Ge\u00b7schick", "un\u00b7m\u00e4ch\u00b7tig", ",", "schwach", "und", "blind", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wie oder schlie\u00dfest du, da\u00df Gottes Gnad' ihm g\u00f6nne", "tokens": ["Wie", "o\u00b7der", "schlie\u00b7\u00dfest", "du", ",", "da\u00df", "Got\u00b7tes", "Gnad'", "ihm", "g\u00f6n\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "KON", "VVFIN", "PPER", "$,", "KOUS", "NN", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Blindheit, da kein Schaun Verh\u00e4ngnis wenden k\u00f6nne?", "tokens": ["Die", "Blind\u00b7heit", ",", "da", "kein", "Schaun", "Ver\u00b7h\u00e4ng\u00b7nis", "wen\u00b7den", "k\u00f6n\u00b7ne", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PIAT", "NN", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ich schlie\u00dfe dies daraus: es m\u00fcsse gar nicht r\u00fchren", "tokens": ["Ich", "schlie\u00b7\u00dfe", "dies", "da\u00b7raus", ":", "es", "m\u00fcs\u00b7se", "gar", "nicht", "r\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDS", "PAV", "$.", "PPER", "VMFIN", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Geist ein \u00e4u\u00dferes Geschick, sonst w\u00fcrd' ers sp\u00fcren.", "tokens": ["Den", "Geist", "ein", "\u00e4u\u00b7\u00dfe\u00b7res", "Ge\u00b7schick", ",", "sonst", "w\u00fcrd'", "ers", "sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,", "ADV", "VAFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ich schlie\u00dfe dies daraus: da\u00df unabh\u00e4ngig frei", "tokens": ["Ich", "schlie\u00b7\u00dfe", "dies", "da\u00b7raus", ":", "da\u00df", "un\u00b7ab\u00b7h\u00e4n\u00b7gig", "frei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDS", "PAV", "$.", "KOUS", "ADJD", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von \u00e4u\u00dferem Geschick des Geistes Leben sei.", "tokens": ["Von", "\u00e4u\u00b7\u00dfe\u00b7rem", "Ge\u00b7schick", "des", "Geis\u00b7tes", "Le\u00b7ben", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}