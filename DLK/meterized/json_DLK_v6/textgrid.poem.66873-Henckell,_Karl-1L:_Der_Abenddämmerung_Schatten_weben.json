{"textgrid.poem.66873": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Abendd\u00e4mmerung Schatten weben", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Abendd\u00e4mmerung Schatten weben", "tokens": ["Der", "A\u00b7bend\u00b7d\u00e4m\u00b7me\u00b7rung", "Schat\u00b7ten", "we\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVINF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Verschwiegen Schleier um die Welt,", "tokens": ["Ver\u00b7schwie\u00b7gen", "Schlei\u00b7er", "um", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die sinnlos um ihr Gl\u00fcck geprellt.", "tokens": ["Die", "sinn\u00b7los", "um", "ihr", "Gl\u00fcck", "ge\u00b7prellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Umkrochen von dem hungrigen Leben,", "tokens": ["Um\u00b7kro\u00b7chen", "von", "dem", "hung\u00b7ri\u00b7gen", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Stumm sitzt des Dichters Weib und s\u00e4ugt \u2013", "tokens": ["Stumm", "sitzt", "des", "Dich\u00b7ters", "Weib", "und", "s\u00e4ugt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "NN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die blutlos blassen Lippen beben.", "tokens": ["Die", "blut\u00b7los", "blas\u00b7sen", "Lip\u00b7pen", "be\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Tief das gequ\u00e4lte Haupt gebeugt,", "tokens": ["Tief", "das", "ge\u00b7qu\u00e4l\u00b7te", "Haupt", "ge\u00b7beugt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "L\u00e4\u00dft starr den Blick am Boden kleben", "tokens": ["L\u00e4\u00dft", "starr", "den", "Blick", "am", "Bo\u00b7den", "kle\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "NN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Der Mann, zur Sch\u00f6pfertat erzeugt.", "tokens": ["Der", "Mann", ",", "zur", "Sch\u00f6p\u00b7fer\u00b7tat", "er\u00b7zeugt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "\u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}}, "stanza.2": {"line.1": {"text": "\u00bbbegeistern soll ich und erheben?!\u00ab", "tokens": ["\u00bb", "be\u00b7geis\u00b7tern", "soll", "ich", "und", "er\u00b7he\u00b7ben", "?!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVINF", "VMFIN", "PPER", "KON", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schreit lautlos er in stummen Grund,", "tokens": ["Schreit", "laut\u00b7los", "er", "in", "stum\u00b7men", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Unf\u00fchlend schweigt der Erde Mund.", "tokens": ["Un\u00b7f\u00fch\u00b7lend", "schweigt", "der", "Er\u00b7de", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur durch der Diele Spalt entrinnen,", "tokens": ["Nur", "durch", "der", "Die\u00b7le", "Spalt", "ent\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Blitzschnell in Nacht verscheucht, zwei Spinnen,", "tokens": ["Blitz\u00b7schnell", "in", "Nacht", "ver\u00b7scheucht", ",", "zwei", "Spin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVPP", "$,", "CARD", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Des Schicksals Spioniererinnen.", "tokens": ["Des", "Schick\u00b7sals", "Spi\u00b7o\u00b7nie\u00b7re\u00b7rin\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "\u00bbneid' ich das Dasein noch dem Hund", "tokens": ["\u00bb", "neid'", "ich", "das", "Da\u00b7sein", "noch", "dem", "Hund"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "VVIMP", "PPER", "ART", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "An seines Herren Futterst\u00e4tte ...?!\u00ab", "tokens": ["An", "sei\u00b7nes", "Her\u00b7ren", "Fut\u00b7ter\u00b7st\u00e4t\u00b7te", "...", "?!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$(", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Aufst\u00f6hnt der Mensch an seiner Kette. \u2013", "tokens": ["Auf\u00b7st\u00f6hnt", "der", "Mensch", "an", "sei\u00b7ner", "Ket\u00b7te", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Zur Feder greift er. Wirft sie hin.", "tokens": ["Zur", "Fe\u00b7der", "greift", "er", ".", "Wirft", "sie", "hin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$.", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "\u00bbwer gab mir diesen stolzen Sinn,", "tokens": ["\u00bb", "wer", "gab", "mir", "die\u00b7sen", "stol\u00b7zen", "Sinn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Dem nicht gen\u00fcgt, mit Menschmaschinen", "tokens": ["Dem", "nicht", "ge\u00b7n\u00fcgt", ",", "mit", "Mensc\u00b7hma\u00b7schi\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "PTKNEG", "VVFIN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Um eklen Hungerlohn zu dienen,", "tokens": ["Um", "ek\u00b7len", "Hun\u00b7ger\u00b7lohn", "zu", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Wer gab mir, da\u00df ich Feuer bin?!", "tokens": ["Wer", "gab", "mir", ",", "da\u00df", "ich", "Feu\u00b7er", "bin", "?!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Wenn etwas ist, das diesem Treiben", "tokens": ["Wenn", "et\u00b7was", "ist", ",", "das", "die\u00b7sem", "Trei\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "VAFIN", "$,", "PRELS", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Vern\u00fcnftigen Einklangs Wert verleiht,", "tokens": ["Ver\u00b7n\u00fcnf\u00b7ti\u00b7gen", "Ein\u00b7klangs", "Wert", "ver\u00b7leiht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Wie zwingt es mich, das Wort zu schreiben,", "tokens": ["Wie", "zwingt", "es", "mich", ",", "das", "Wort", "zu", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Das wider seinen Sch\u00f6pfer schreit?!", "tokens": ["Das", "wi\u00b7der", "sei\u00b7nen", "Sch\u00f6p\u00b7fer", "schreit", "?!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "\u2013 Geist, la\u00df mich bei der Wahrheit bleiben,", "tokens": ["\u2013", "Geist", ",", "la\u00df", "mich", "bei", "der", "Wahr\u00b7heit", "blei\u00b7ben", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "$,", "VVIMP", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Wenn du der Geist der Wahrheit bist,", "tokens": ["Wenn", "du", "der", "Geist", "der", "Wahr\u00b7heit", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Und mag die M\u00fchle mich zerreiben \u2013", "tokens": ["Und", "mag", "die", "M\u00fch\u00b7le", "mich", "zer\u00b7rei\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Bin mehr, denn was da zeugt und fri\u00dft.\u00ab", "tokens": ["Bin", "mehr", ",", "denn", "was", "da", "zeugt", "und", "fri\u00dft", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "$,", "KON", "PWS", "ADV", "VVFIN", "KON", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Sein Atem keucht, die Schl\u00e4fen brennen \u2013", "tokens": ["Sein", "A\u00b7tem", "keucht", ",", "die", "Schl\u00e4\u00b7fen", "bren\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Sein Blick kann kaum das Weib erkennen,", "tokens": ["Sein", "Blick", "kann", "kaum", "das", "Weib", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Umflort von schattenfinsterm Gram \u2013", "tokens": ["Um\u00b7flort", "von", "schat\u00b7ten\u00b7fins\u00b7term", "Gram", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "\u00bbgrausamer Geist, bei deiner Scham,", "tokens": ["\u00bb", "grau\u00b7sa\u00b7mer", "Geist", ",", "bei", "dei\u00b7ner", "Scham", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.27": {"text": "Legst du durch Not dies F\u00fchlen lahm,", "tokens": ["Legst", "du", "durch", "Not", "dies", "F\u00fch\u00b7len", "lahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PDS", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Um das ich aushielt solches Leben,", "tokens": ["Um", "das", "ich", "aus\u00b7hielt", "sol\u00b7ches", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Nie kann dein Opfer dir vergeben,", "tokens": ["Nie", "kann", "dein", "Op\u00b7fer", "dir", "ver\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Fluch, M\u00f6rder, dir, machst du mich zahm!\u00ab", "tokens": ["Fluch", ",", "M\u00f6r\u00b7der", ",", "dir", ",", "machst", "du", "mich", "zahm", "!", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPER", "$,", "VVFIN", "PPER", "PRF", "ADJD", "$.", "$("], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.31": {"text": "Ein K\u00e4mpfer st\u00f6hnend wankt und f\u00e4llt \u2013", "tokens": ["Ein", "K\u00e4mp\u00b7fer", "st\u00f6h\u00b7nend", "wankt", "und", "f\u00e4llt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Sucht sich in Kr\u00e4mpfen zu erheben ...", "tokens": ["Sucht", "sich", "in", "Kr\u00e4mp\u00b7fen", "zu", "er\u00b7he\u00b7ben", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Der Abendd\u00e4mmerung Schatten weben", "tokens": ["Der", "A\u00b7bend\u00b7d\u00e4m\u00b7me\u00b7rung", "Schat\u00b7ten", "we\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVINF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.34": {"text": "Verschwiegen Schleier um die Welt.", "tokens": ["Ver\u00b7schwie\u00b7gen", "Schlei\u00b7er", "um", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}