["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die verlassnen Felsenklippen,", "tokens": ["Die", "ver\u00b7lass\u00b7nen", "Fel\u00b7sen\u00b7klip\u00b7pen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kein Gestr\u00e4uch und keine Blume", "tokens": ["Kein", "Ge\u00b7str\u00e4uch", "und", "kei\u00b7ne", "Blu\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf des Abgrunds bleichen Lippen.", "tokens": ["Auf", "des", "Ab\u00b7grunds", "blei\u00b7chen", "Lip\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}