{"dta.poem.8712": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Ii.  \n  Der n\u00e4chtliche Gang.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Tiefe Nacht; &#8212; der stille Vollmond", "tokens": ["Tie\u00b7fe", "Nacht", ";", "&#8212;", "der", "stil\u00b7le", "Voll\u00b7mond"], "token_info": ["word", "word", "punct", "XML_entity", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "$(", "ART", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Hebt sich jenseits von den Auen,", "tokens": ["Hebt", "sich", "jen\u00b7seits", "von", "den", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Wellen der Durance", "tokens": ["Und", "die", "Wel\u00b7len", "der", "Du\u00b7ran\u00b7ce"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sind ein Silberstrom zu schauen:", "tokens": ["Sind", "ein", "Sil\u00b7ber\u00b7strom", "zu", "schau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Fl\u00fcchtig eilen sie vor\u00fcber", "tokens": ["Fl\u00fcch\u00b7tig", "ei\u00b7len", "sie", "vor\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An den mondbegl\u00e4nzten Riffen,", "tokens": ["An", "den", "mond\u00b7be\u00b7gl\u00e4nz\u00b7ten", "Rif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und von r\u00e4thselhafter Wehmuth", "tokens": ["Und", "von", "r\u00e4th\u00b7sel\u00b7haf\u00b7ter", "Weh\u00b7muth"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchlt der Wandrer sich ergriffen;", "tokens": ["F\u00fchlt", "der", "Wand\u00b7rer", "sich", "er\u00b7grif\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Denn er h\u00f6rt im ruhelosen,", "tokens": ["Denn", "er", "h\u00f6rt", "im", "ru\u00b7he\u00b7lo\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Immergleichen Wellenschlage", "tokens": ["Im\u00b7mer\u00b7glei\u00b7chen", "Wel\u00b7len\u00b7schla\u00b7ge"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ewig an die Sterne t\u00f6nen", "tokens": ["E\u00b7wig", "an", "die", "Ster\u00b7ne", "t\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seines Herzens bange Frage:", "tokens": ["Sei\u00b7nes", "Her\u00b7zens", "ban\u00b7ge", "Fra\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein Verrauschen, ein Verschwinden", "tokens": ["Ein", "Ver\u00b7rau\u00b7schen", ",", "ein", "Ver\u00b7schwin\u00b7den"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles Leben! \u2014 doch von wannen? \u2014", "tokens": ["Al\u00b7les", "Le\u00b7ben", "!", "doch", "von", "wan\u00b7nen", "?"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "$.", "$(", "ADV", "APPR", "ADV", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Doch wohin? \u2014 die Sterne schweigen,", "tokens": ["Doch", "wo\u00b7hin", "?", "die", "Ster\u00b7ne", "schwei\u00b7gen", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "$.", "$(", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und die Welle rauscht von dannen.", "tokens": ["Und", "die", "Wel\u00b7le", "rauscht", "von", "dan\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Cisteron, das St\u00e4dtchen, schlummert;", "tokens": ["Cis\u00b7te\u00b7ron", ",", "das", "St\u00e4dt\u00b7chen", ",", "schlum\u00b7mert", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur im Schlo\u00dfe lassen Worte", "tokens": ["Nur", "im", "Schlo\u00b7\u00dfe", "las\u00b7sen", "Wor\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVINF", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dumpf und eilig sich vernehmen,", "tokens": ["Dumpf", "und", "ei\u00b7lig", "sich", "ver\u00b7neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und es dr\u00f6hnt die Eisenpforte.", "tokens": ["Und", "es", "dr\u00f6hnt", "die", "Ei\u00b7sen\u00b7pfor\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "M\u00e4nner schreiten still und langsam", "tokens": ["M\u00e4n\u00b7ner", "schrei\u00b7ten", "still", "und", "lang\u00b7sam"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dort hinauf zum Felsenhause:", "tokens": ["Dort", "hin\u00b7auf", "zum", "Fel\u00b7sen\u00b7hau\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Waffenknechte sind es, f\u00fchren", "tokens": ["Waf\u00b7fen\u00b7knech\u00b7te", "sind", "es", ",", "f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NN", "VAFIN", "PPER", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den Gefangnen in die Klause.", "tokens": ["Den", "Ge\u00b7fang\u00b7nen", "in", "die", "Klau\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Johann Kasimir von Polen!", "tokens": ["Jo\u00b7hann", "Ka\u00b7si\u00b7mir", "von", "Po\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NE", "$."], "meter": "------+-", "measure": "unknown.measure.single"}, "line.2": {"text": "Hei\u00df durchrollt von K\u00f6nigsblute,", "tokens": ["Hei\u00df", "durch\u00b7rollt", "von", "K\u00f6\u00b7nigs\u00b7blu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Edler Spro\u00df vom Stamme Wasa,", "tokens": ["Ed\u00b7ler", "Spro\u00df", "vom", "Stam\u00b7me", "Wa\u00b7sa", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, wie mag dir seyn zu Muthe!", "tokens": ["Ach", ",", "wie", "mag", "dir", "seyn", "zu", "Mu\u00b7the", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VMFIN", "PPER", "PPOSAT", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Heldenj\u00fcngling, der du k\u00e4mpftest,", "tokens": ["Hel\u00b7den\u00b7j\u00fcng\u00b7ling", ",", "der", "du", "k\u00e4mpf\u00b7test", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruhmbekr\u00e4nzt in manchen Schlachten,", "tokens": ["Ruhm\u00b7be\u00b7kr\u00e4nzt", "in", "man\u00b7chen", "Schlach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In verr\u00e4therischer Fremde", "tokens": ["In", "ver\u00b7r\u00e4\u00b7the\u00b7ri\u00b7scher", "Frem\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00dft du als Gefangner schmachten!", "tokens": ["Mu\u00dft", "du", "als", "Ge\u00b7fang\u00b7ner", "schmach\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "KOUS", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Spricht man so im feinen Frankreich", "tokens": ["Spricht", "man", "so", "im", "fei\u00b7nen", "Fran\u00b7kreich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADV", "APPRART", "ADJA", "NE"], "meter": "----+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Hohn des Gastes heil'gem Rechte,", "tokens": ["Hohn", "des", "Gas\u00b7tes", "heil'\u00b7gem", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df den freundgesinnten F\u00fcrsten", "tokens": ["Da\u00df", "den", "freund\u00b7ge\u00b7sinn\u00b7ten", "F\u00fcrs\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zwingen die Tyrannenknechte?!", "tokens": ["Zwin\u00b7gen", "die", "Ty\u00b7ran\u00b7nen\u00b7knech\u00b7te", "?!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "In des Mondes hellem Scheine", "tokens": ["In", "des", "Mon\u00b7des", "hel\u00b7lem", "Schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gl\u00e4nzen ihre Mordgewehre;", "tokens": ["Gl\u00e4n\u00b7zen", "ih\u00b7re", "Mord\u00b7ge\u00b7weh\u00b7re", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber nicht des Polenf\u00fcrsten", "tokens": ["A\u00b7ber", "nicht", "des", "Po\u00b7len\u00b7f\u00fcrs\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stolz und schnell verwischte Z\u00e4hre.", "tokens": ["Stolz", "und", "schnell", "ver\u00b7wischte", "Z\u00e4h\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.11": {"line.1": {"text": "Auf dem steilen Stufenpfade,", "tokens": ["Auf", "dem", "stei\u00b7len", "Stu\u00b7fen\u00b7pfa\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eingehauen dem Granite,", "tokens": ["Ein\u00b7ge\u00b7hau\u00b7en", "dem", "Gra\u00b7ni\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Heben sich in scheuer Windung", "tokens": ["He\u00b7ben", "sich", "in", "scheu\u00b7er", "Win\u00b7dung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach dem Gipfel ihre Schritte.", "tokens": ["Nach", "dem", "Gip\u00b7fel", "ih\u00b7re", "Schrit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Wagt es wer im schwanken Mondlicht", "tokens": ["Wagt", "es", "wer", "im", "schwan\u00b7ken", "Mond\u00b7licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da den Pfad hinaufzuwallen,", "tokens": ["Da", "den", "Pfad", "hin\u00b7auf\u00b7zu\u00b7wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bebend sieht er seinen Schatten", "tokens": ["Be\u00b7bend", "sieht", "er", "sei\u00b7nen", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den grausen Abgrund fallen.", "tokens": ["In", "den", "grau\u00b7sen", "Ab\u00b7grund", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Sinnend bleibt Johannes stehen,", "tokens": ["Sin\u00b7nend", "bleibt", "Jo\u00b7han\u00b7nes", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und er h\u00f6rt im Niederlauschen", "tokens": ["Und", "er", "h\u00f6rt", "im", "Nie\u00b7der\u00b7lau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Immer leiser dort die Schluchten,", "tokens": ["Im\u00b7mer", "lei\u00b7ser", "dort", "die", "Schluch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leiser die Durance rauschen.", "tokens": ["Lei\u00b7ser", "die", "Du\u00b7ran\u00b7ce", "rau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Horch! ein L\u00fcftchen aus den Auen,", "tokens": ["Horch", "!", "ein", "L\u00fcft\u00b7chen", "aus", "den", "Au\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo die Nachtigallen singen,", "tokens": ["Wo", "die", "Nach\u00b7ti\u00b7gal\u00b7len", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kommt dem Armen nachgeflogen,", "tokens": ["Kommt", "dem", "Ar\u00b7men", "nach\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihm noch einen Laut zu bringen.", "tokens": ["Ihm", "noch", "ei\u00b7nen", "Laut", "zu", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Weither kam das gute L\u00fcftchen,", "tokens": ["Weit\u00b7her", "kam", "das", "gu\u00b7te", "L\u00fcft\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ein Kind, das frohbehende", "tokens": ["Wie", "ein", "Kind", ",", "das", "froh\u00b7be\u00b7hen\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ART", "NN", "$,", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Einem Bettler, wenn er scheidet,", "tokens": ["Ei\