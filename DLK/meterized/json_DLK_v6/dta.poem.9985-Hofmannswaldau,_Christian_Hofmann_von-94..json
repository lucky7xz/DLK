{"dta.poem.9985": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "94.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Meine stimm ein thier erschrecket/ so das st\u00e4rckste wird genant/", "tokens": ["Mei\u00b7ne", "stimm", "ein", "thier", "er\u00b7schre\u00b7cket", "/", "so", "das", "st\u00e4rcks\u00b7te", "wird", "ge\u00b7nant", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ART", "NN", "VVFIN", "$(", "ADV", "ART", "ADJA", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Mit dem rothen barte bin ich auf der welt sehr wohl bekant;", "tokens": ["Mit", "dem", "ro\u00b7then", "bar\u00b7te", "bin", "ich", "auf", "der", "welt", "sehr", "wohl", "be\u00b7kant", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN", "PPER", "APPR", "ART", "NN", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Ich bin muthig/ obgleich klein/ la\u00df mich kein gesetze binden.", "tokens": ["Ich", "bin", "mut\u00b7hig", "/", "ob\u00b7gleich", "klein", "/", "la\u00df", "mich", "kein", "ge\u00b7set\u00b7ze", "bin\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$(", "KOUS", "ADJD", "$(", "VVIMP", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Weiber hab ich ohne zahl/ die zu meiner lust sich finden.", "tokens": ["Wei\u00b7ber", "hab", "ich", "oh\u00b7ne", "zahl", "/", "die", "zu", "mei\u00b7ner", "lust", "sich", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "NN", "$(", "ART", "APPR", "PPOSAT", "VVFIN", "PRF", "VVINF", "$."], "meter": "+-+-+----+-+-+-", "measure": "unknown.measure.hexa"}}}}}