{"textgrid.poem.54881": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "95. Auf Valgi nichts w\u00fcrdiges Thun", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du thust der Deutschen Noht/ du thust den Krig beschreiben/", "tokens": ["Du", "thust", "der", "Deut\u00b7schen", "Noht", "/", "du", "thust", "den", "Krig", "be\u00b7schrei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$(", "PPER", "VVFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du thust die lange Zeit mit lesen offt vertreiben/", "tokens": ["Du", "thust", "die", "lan\u00b7ge", "Zeit", "mit", "le\u00b7sen", "offt", "ver\u00b7trei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "VVFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und was du dichten thust/ thust du den Freunden weisen/", "tokens": ["Und", "was", "du", "dich\u00b7ten", "thust", "/", "thust", "du", "den", "Freun\u00b7den", "wei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADJA", "VVFIN", "$(", "VVFIN", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die thun was du gethan mit langen Reimen preisen/", "tokens": ["Die", "thun", "was", "du", "ge\u00b7than", "mit", "lan\u00b7gen", "Rei\u00b7men", "prei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "PWS", "PPER", "VVPP", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die sagen da\u00df du thust ber\u00fchmte B\u00fccher machen/", "tokens": ["Die", "sa\u00b7gen", "da\u00df", "du", "thust", "be\u00b7r\u00fchm\u00b7te", "B\u00fc\u00b7cher", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KOUS", "PPER", "VVFIN", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn wir die lesen thun/ thun wir unm\u00e4\u00dfig lachen/", "tokens": ["Wenn", "wir", "die", "le\u00b7sen", "thun", "/", "thun", "wir", "un\u00b7m\u00e4\u00b7\u00dfig", "la\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "VVINF", "VVINF", "$(", "VVFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Warumb? Dieweil dein Thun wann wir es recht betrachten", "tokens": ["Wa\u00b7rumb", "?", "Die\u00b7weil", "dein", "Thun", "wann", "wir", "es", "recht", "be\u00b7trach\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "KOUS", "PPOSAT", "NN", "PWAV", "PPER", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ob du gleich alles thust vor ungethan zu achten.", "tokens": ["Ob", "du", "gleich", "al\u00b7les", "thust", "vor", "un\u00b7ge\u00b7than", "zu", "ach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIS", "VVFIN", "APPR", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}