{"textgrid.poem.44796": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Ins Stammbuch der F\u00fcrstin Radzivil", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein schlechter Richter ist das Aug,", "tokens": ["Ein", "schlech\u00b7ter", "Rich\u00b7ter", "ist", "das", "Aug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil mans so leicht besticht,", "tokens": ["Weil", "mans", "so", "leicht", "be\u00b7sticht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der beste, freilich, w\u00e4r das Herz,", "tokens": ["Der", "bes\u00b7te", ",", "frei\u00b7lich", ",", "w\u00e4r", "das", "Herz", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADV", "$,", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch das erreicht dich nicht.", "tokens": ["Doch", "das", "er\u00b7reicht", "dich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein strenger, aber allzumal", "tokens": ["Ein", "stren\u00b7ger", ",", "a\u00b7ber", "all\u00b7zu\u00b7mal"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein bittrer, ist das Ohr,", "tokens": ["Ein", "bit\u00b7trer", ",", "ist", "das", "Ohr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und w\u00e4r ein Fehler noch so klein,", "tokens": ["Und", "w\u00e4r", "ein", "Feh\u00b7ler", "noch", "so", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Scheelsucht f\u00fchrt ihn vor.", "tokens": ["Die", "Scheel\u00b7sucht", "f\u00fchrt", "ihn", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nur Wen'ge, die er schuldlos hei\u00dft,", "tokens": ["Nur", "Wen'\u00b7ge", ",", "die", "er", "schuld\u00b7los", "hei\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und die, in edler Ruh,", "tokens": ["Und", "die", ",", "in", "ed\u00b7ler", "Ruh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Neides Zunge selber preist,", "tokens": ["Des", "Nei\u00b7des", "Zun\u00b7ge", "sel\u00b7ber", "preist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die sind dann gut \u2013 wie du.", "tokens": ["Die", "sind", "dann", "gut", "\u2013", "wie", "du", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "$(", "PWAV", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}