{"dta.poem.21073": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Der Traum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Einen s&#252;ssen Traum hab' ich getr&#228;umet.               ", "tokens": ["Ei\u00b7nen", "s", "&#252;", "ssen", "Traum", "hab'", "ich", "ge\u00b7tr", "&#228;", "u\u00b7met", "."], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["ART", "FM", "FM", "FM", "NN", "VAFIN", "PPER", "ADJD", "$(", "VVFIN", "$."], "meter": "+--++-+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Rosig war sein G\u00fcrtel; goldbes\u00e4umet", "tokens": ["Ro\u00b7sig", "war", "sein", "G\u00fcr\u00b7tel", ";", "gold\u00b7be\u00b7s\u00e4u\u00b7met"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$.", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "War der Fittig, der den Gaukler trug.", "tokens": ["War", "der", "Fit\u00b7tig", ",", "der", "den", "Gauk\u00b7ler", "trug", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Spottend ist der Flattrer nun entflogen,", "tokens": ["Spot\u00b7tend", "ist", "der", "Flatt\u00b7rer", "nun", "ent\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "T\u00fcckisch hat der T\u00e4uscher mich betrogen;", "tokens": ["T\u00fc\u00b7ckisch", "hat", "der", "T\u00e4u\u00b7scher", "mich", "be\u00b7tro\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Dennoch dankt mein Herz ihm den Betrug.", "tokens": ["Den\u00b7noch", "dankt", "mein", "Herz", "ihm", "den", "Be\u00b7trug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Abend war es; und im Abendschimmer", "tokens": ["A\u00b7bend", "war", "es", ";", "und", "im", "A\u00b7bend\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "$.", "KON", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Stand ich auf Arkonens heilger Tr\u00fcmmer,", "tokens": ["Stand", "ich", "auf", "Ar\u00b7ko\u00b7nens", "heil\u00b7ger", "Tr\u00fcm\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Schaute staunend in die weite See.", "tokens": ["Schau\u00b7te", "stau\u00b7nend", "in", "die", "wei\u00b7te", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Schimmernd in des Sp\u00e4troths Widerscheine,", "tokens": ["Schim\u00b7mernd", "in", "des", "Sp\u00e4t\u00b7roths", "Wi\u00b7der\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Stand bey mir die namenlose Eine,", "tokens": ["Stand", "bey", "mir", "die", "na\u00b7men\u00b7lo\u00b7se", "Ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "NN", "ART", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die ich wachend tr\u00e4umend einzig seh.", "tokens": ["Die", "ich", "wa\u00b7chend", "tr\u00e4u\u00b7mend", "ein\u00b7zig", "seh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "ADJD", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Sch\u00f6n bekr\u00e4nzt von Schl\u00fcsselblumenglocken,", "tokens": ["Sch\u00f6n", "be\u00b7kr\u00e4nzt", "von", "Schl\u00fcs\u00b7sel\u00b7blu\u00b7men\u00b7glo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Floss ihr Haar in schweren blonden Locken", "tokens": ["Floss", "ihr", "Haar", "in", "schwe\u00b7ren", "blon\u00b7den", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "APPR", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Von des Zephyrs Odem aufgehaucht.", "tokens": ["Von", "des", "Ze\u00b7phyrs", "O\u00b7dem", "auf\u00b7ge\u00b7haucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Weiss und schwellend, wie des Schwans Gefieder,", "tokens": ["Weiss", "und", "schwel\u00b7lend", ",", "wie", "des", "Schwans", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,", "PWAV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wallt' ihr Schneegewand die H\u00fcften nieder,", "tokens": ["Wallt'", "ihr", "Schnee\u00b7ge\u00b7wand", "die", "H\u00fcf\u00b7ten", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "In der Abendsonne Gold getaucht.", "tokens": ["In", "der", "A\u00b7bend\u00b7son\u00b7ne", "Gold", "ge\u00b7taucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "R\u00f6ther brannten itzt des Sp\u00e4troths Gluthen,", "tokens": ["R\u00f6\u00b7ther", "brann\u00b7ten", "itzt", "des", "Sp\u00e4t\u00b7roths", "Glu\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "D\u00fcstrer donnerten die d\u00fcstern Fluthen,", "tokens": ["D\u00fcst\u00b7rer", "don\u00b7ner\u00b7ten", "die", "d\u00fcs\u00b7tern", "Flut\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Gross und fey'rlich sank die Sonn' hinab.", "tokens": ["Gross", "und", "fey'\u00b7rlich", "sank", "die", "Sonn'", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Rings umrauschte sie des Meeres F\u00fclle;", "tokens": ["Rings", "um\u00b7rauschte", "sie", "des", "Mee\u00b7res", "F\u00fcl\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Aber pl\u00f6tzlich ward es stille, stille,", "tokens": ["A\u00b7ber", "pl\u00f6tz\u00b7lich", "ward", "es", "stil\u00b7le", ",", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wie um eines guten Menschen Grab.", "tokens": ["Wie", "um", "ei\u00b7nes", "gu\u00b7ten", "Men\u00b7schen", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Staunend schauten wir vom schroffen H\u00fcgel", "tokens": ["Stau\u00b7nend", "schau\u00b7ten", "wir", "vom", "schrof\u00b7fen", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nieder in des Meeres Lasurspiegel,", "tokens": ["Nie\u00b7der", "in", "des", "Mee\u00b7res", "La\u00b7sur\u00b7spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Staunender zum Abendroth empor.", "tokens": ["Stau\u00b7nen\u00b7der", "zum", "A\u00b7ben\u00b7droth", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Schon erblassten seine Purpurnelken.", "tokens": ["Schon", "er\u00b7blass\u00b7ten", "sei\u00b7ne", "Pur\u00b7pur\u00b7nel\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Schau! da d\u00e4mmert' aus den Duftgew\u00f6lken", "tokens": ["Schau", "!", "da", "d\u00e4m\u00b7mert'", "aus", "den", "Duft\u00b7ge\u00b7w\u00f6l\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Bleich und lieb der Abendstern hervor.", "tokens": ["Bleich", "und", "lieb", "der", "A\u00b7bends\u00b7tern", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Und mir ward, als h\u00f6rt' ich Angstgest\u00f6hne,", "tokens": ["Und", "mir", "ward", ",", "als", "h\u00f6rt'", "ich", "Angst\u00b7ge\u00b7st\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "$,", "KOUS", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Grabgewimmer, dumpfe Jammert\u00f6ne", "tokens": ["Grab\u00b7ge\u00b7wim\u00b7mer", ",", "dump\u00b7fe", "Jam\u00b7mer\u00b7t\u00f6\u00b7ne"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Von dem blassen Stern her\u00fcberwehn.", "tokens": ["Von", "dem", "blas\u00b7sen", "Stern", "her\u00b7\u00fc\u00b7berw\u00b7ehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "\u201estern der Liebe,\u201c rief ich mit Erstarren,", "tokens": ["\u201e", "stern", "der", "Lie\u00b7be", ",", "\u201c", "rief", "ich", "mit", "Er\u00b7star\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "$,", "$(", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "\u201esiehst du auch, du Blasser, Gr\u00e4ber scharren,", "tokens": ["\u201e", "siehst", "du", "auch", ",", "du", "Blas\u00b7ser", ",", "Gr\u00e4\u00b7ber", "schar\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ADV", "$,", "PPER", "NN", "$,", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "\u201eherzen brechen, Leben untergehn?\u201c", "tokens": ["\u201e", "her\u00b7zen", "bre\u00b7chen", ",", "Le\u00b7ben", "un\u00b7ter\u00b7gehn", "?", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "VVINF", "VVINF", "$,", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Schw\u00e4rmend rief ichs, und die Edle blickte", "tokens": ["Schw\u00e4r\u00b7mend", "rief", "ichs", ",", "und", "die", "Ed\u00b7le", "blick\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PIS", "$,", "KON", "ART", "ADJA", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schweigend mir ins Auge. Schweigend dr\u00fcckte", "tokens": ["Schwei\u00b7gend", "mir", "ins", "Au\u00b7ge", ".", "Schwei\u00b7gend", "dr\u00fcck\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "PPER", "APPRART", "NN", "$.", "ADJD", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sie die Hand mir. Und vom s\u00fcssen Schmerz", "tokens": ["Sie", "die", "Hand", "mir", ".", "Und", "vom", "s\u00fcs\u00b7sen", "Schmerz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "PPER", "$.", "KON", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "\u00dcberw\u00e4ltigt, sank die Tadellose,", "tokens": ["\u00dc\u00b7berw\u00b7\u00e4l\u00b7tigt", ",", "sank", "die", "Ta\u00b7del\u00b7lo\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Eine blasse sturmgebeugte Rose,", "tokens": ["Ei\u00b7ne", "blas\u00b7se", "sturm\u00b7ge\u00b7beug\u00b7te", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Angesichts des Weltalls mir ans Herz.", "tokens": ["An\u00b7ge\u00b7sichts", "des", "Welt\u00b7alls", "mir", "ans", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Horch, da wandelte das Angstgest\u00f6hne", "tokens": ["Horch", ",", "da", "wan\u00b7del\u00b7te", "das", "Angst\u00b7ge\u00b7st\u00f6h\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Pl\u00f6tzlich sich in Hymen\u00e4ent\u00f6ne.", "tokens": ["Pl\u00f6tz\u00b7lich", "sich", "in", "Hy\u00b7me\u00b7n\u00e4\u00b7en\u00b7t\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Brautges\u00e4nge schallten Chor um Chor.", "tokens": ["Braut\u00b7ge\u00b7s\u00e4n\u00b7ge", "schall\u00b7ten", "Chor", "um", "Chor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "T\u00f6ne, wie sie ", "tokens": ["T\u00f6\u00b7ne", ",", "wie", "sie"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PWAV", "PPER"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Wie sie ", "tokens": ["Wie", "sie"], "token_info": ["word", "word"], "pos": ["PWAV", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Schlichen schmelzend in mein trunknes Ohr.", "tokens": ["Schli\u00b7chen", "schmel\u00b7zend", "in", "mein", "trunk\u00b7nes", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Von der Sterne Schimmerlicht umflossen,", "tokens": ["Von", "der", "Ster\u00b7ne", "Schim\u00b7mer\u00b7licht", "um\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Von der Locken Goldgew\u00f6lk umgossen,", "tokens": ["Von", "der", "Lo\u00b7cken", "Gold\u00b7ge\u00b7w\u00f6lk", "um\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Lag die Edle athmend mir im Arm.", "tokens": ["Lag", "die", "Ed\u00b7le", "ath\u00b7mend", "mir", "im", "Arm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Weggeschwemmt war aus dem selgen Herzen,", "tokens": ["Weg\u00b7ge\u00b7schwemmt", "war", "aus", "dem", "sel\u00b7gen", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Das an ihrem schlug, die Fluth der Schmerzen,", "tokens": ["Das", "an", "ih\u00b7rem", "schlug", ",", "die", "Fluth", "der", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPOSAT", "VVFIN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "----+-+-+-", "measure": "unknown.measure.tri"}, "line.6": {"text": "Weggewaschen jeder alte Harm.", "tokens": ["Weg\u00b7ge\u00b7wa\u00b7schen", "je\u00b7der", "al\u00b7te", "Harm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Eine grosse, selige Minute", "tokens": ["Ei\u00b7ne", "gros\u00b7se", ",", "se\u00b7li\u00b7ge", "Mi\u00b7nu\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hielt ich so das Sch\u00f6ne und das Gute", "tokens": ["Hielt", "ich", "so", "das", "Sch\u00f6\u00b7ne", "und", "das", "Gu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Angeschmiegt an die getreue Brust.", "tokens": ["An\u00b7ge\u00b7schmiegt", "an", "die", "ge\u00b7treu\u00b7e", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Aber ach, der beerenreichen Trauben", "tokens": ["A\u00b7ber", "ach", ",", "der", "bee\u00b7ren\u00b7rei\u00b7chen", "Trau\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "XY", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Keine dem gew\u00fcnschten Baum zu rauben,", "tokens": ["Kei\u00b7ne", "dem", "ge\u00b7w\u00fcnschten", "Baum", "zu", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "War zu lockend die verbotne Lust.", "tokens": ["War", "zu", "lo\u00b7ckend", "die", "ver\u00b7bot\u00b7ne", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKA", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Nur den leisesten der K\u00fcsse dr\u00fcckte", "tokens": ["Nur", "den", "lei\u00b7ses\u00b7ten", "der", "K\u00fcs\u00b7se", "dr\u00fcck\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "ART", "NN", "VVFIN"], "meter": "+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Ich auf ihre Lippen. Pl\u00f6tzlich z\u00fcckte", "tokens": ["Ich", "auf", "ih\u00b7re", "Lip\u00b7pen", ".", "Pl\u00f6tz\u00b7lich", "z\u00fcck\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "$.", "ADJD", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Mir es r\u00e4cherisch durch Mark und Bein.", "tokens": ["Mir", "es", "r\u00e4\u00b7che\u00b7risch", "durch", "Mark", "und", "Bein", "."], "token_info": ["word", "word", "word", "