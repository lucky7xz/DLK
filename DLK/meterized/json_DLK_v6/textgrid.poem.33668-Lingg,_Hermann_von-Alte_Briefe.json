{"textgrid.poem.33668": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Alte Briefe", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine Schrift gibt's, deren Z\u00fcge", "tokens": ["Ei\u00b7ne", "Schrift", "gibt's", ",", "de\u00b7ren", "Z\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ohne Tr\u00e4nen ich nicht sehen kann,", "tokens": ["Oh\u00b7ne", "Tr\u00e4\u00b7nen", "ich", "nicht", "se\u00b7hen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Denn sie redet keine sch\u00f6ne L\u00fcge,", "tokens": ["Denn", "sie", "re\u00b7det", "kei\u00b7ne", "sch\u00f6\u00b7ne", "L\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die ein leeres Herz ersann.", "tokens": ["Die", "ein", "lee\u00b7res", "Herz", "er\u00b7sann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle Worte sind nur Zeugen", "tokens": ["Al\u00b7le", "Wor\u00b7te", "sind", "nur", "Zeu\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Einer Liebe, tief, unwandelbar,", "tokens": ["Ei\u00b7ner", "Lie\u00b7be", ",", "tief", ",", "un\u00b7wan\u00b7del\u00b7bar", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Einer Liebe, die durch nichts zu beugen,", "tokens": ["Ei\u00b7ner", "Lie\u00b7be", ",", "die", "durch", "nichts", "zu", "beu\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PIS", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Die die Liebe meiner Mutter war.", "tokens": ["Die", "die", "Lie\u00b7be", "mei\u00b7ner", "Mut\u00b7ter", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Ob ich dich auch nicht mehr habe,", "tokens": ["Ob", "ich", "dich", "auch", "nicht", "mehr", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "PTKNEG", "ADV", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine stille, treue Liebe blieb,", "tokens": ["Dei\u00b7ne", "stil\u00b7le", ",", "treu\u00b7e", "Lie\u00b7be", "blieb", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Modert auch die Hand im Grabe,", "tokens": ["Mo\u00b7dert", "auch", "die", "Hand", "im", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Die mir diese Tr\u00e4nen schrieb.", "tokens": ["Die", "mir", "die\u00b7se", "Tr\u00e4\u00b7nen", "schrieb", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PDAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie vor Jahren noch bewegen", "tokens": ["Wie", "vor", "Jah\u00b7ren", "noch", "be\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deine Sorgen heut' mein Herz,", "tokens": ["Dei\u00b7ne", "Sor\u00b7gen", "heut'", "mein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Lindernd flie\u00dft um mich dein Segen,", "tokens": ["Lin\u00b7dernd", "flie\u00dft", "um", "mich", "dein", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ach, zu hart w\u00e4r' sonst um dich der Schmerz!", "tokens": ["Ach", ",", "zu", "hart", "w\u00e4r'", "sonst", "um", "dich", "der", "Schmerz", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PTKA", "ADJD", "VAFIN", "ADV", "APPR", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}