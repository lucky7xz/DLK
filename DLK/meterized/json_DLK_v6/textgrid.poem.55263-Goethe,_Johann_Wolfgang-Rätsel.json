{"textgrid.poem.55263": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "R\u00e4tsel", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Bruder ist's von vielen Br\u00fcdern,", "tokens": ["Ein", "Bru\u00b7der", "ist's", "von", "vie\u00b7len", "Br\u00fc\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In allem ihnen v\u00f6llig gleich,", "tokens": ["In", "al\u00b7lem", "ih\u00b7nen", "v\u00f6l\u00b7lig", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PPER", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein n\u00f6tig Glied von vielen Gliedern", "tokens": ["Ein", "n\u00f6\u00b7tig", "Glied", "von", "vie\u00b7len", "Glie\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In eines gro\u00dfen Vaters Reich;", "tokens": ["In", "ei\u00b7nes", "gro\u00b7\u00dfen", "Va\u00b7ters", "Reich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jedoch erblickt man ihn nur selten,", "tokens": ["Je\u00b7doch", "er\u00b7blickt", "man", "ihn", "nur", "sel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Fast wie ein eingeschobnes Kind:", "tokens": ["Fast", "wie", "ein", "ein\u00b7ge\u00b7schob\u00b7nes", "Kind", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die andern lassen ihn nur gelten", "tokens": ["Die", "an\u00b7dern", "las\u00b7sen", "ihn", "nur", "gel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da, wo sie unverm\u00f6gend sind.", "tokens": ["Da", ",", "wo", "sie", "un\u00b7ver\u00b7m\u00f6\u00b7gend", "sind", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}