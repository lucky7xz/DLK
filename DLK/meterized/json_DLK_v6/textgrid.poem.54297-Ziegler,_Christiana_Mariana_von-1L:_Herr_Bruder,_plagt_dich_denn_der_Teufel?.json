{"textgrid.poem.54297": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herr Bruder, plagt dich denn der Teufel?", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Bruder, plagt dich denn der Teufel?", "tokens": ["Herr", "Bru\u00b7der", ",", "plagt", "dich", "denn", "der", "Teu\u00b7fel", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie f\u00e4llt dir denn das Lieben ein?", "tokens": ["Wie", "f\u00e4llt", "dir", "denn", "das", "Lie\u00b7ben", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich glaub es sicher ohne Zweifel;", "tokens": ["Ich", "glaub", "es", "si\u00b7cher", "oh\u00b7ne", "Zwei\u00b7fel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du wei\u00dft, da\u00df wir versprochen seyn,", "tokens": ["Du", "wei\u00dft", ",", "da\u00df", "wir", "ver\u00b7spro\u00b7chen", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und dennoch kanst du dich so wagen;", "tokens": ["Und", "den\u00b7noch", "kanst", "du", "dich", "so", "wa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Es scheint, es ist dir einerley.", "tokens": ["Es", "scheint", ",", "es", "ist", "dir", "ei\u00b7ner\u00b7ley", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das kan ich nicht von dir vertragen,", "tokens": ["Das", "kan", "ich", "nicht", "von", "dir", "ver\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PTKNEG", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ich l\u00e4ugn es nicht, ich sag es frey.", "tokens": ["Ich", "l\u00e4ugn", "es", "nicht", ",", "ich", "sag", "es", "frey", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "La\u00df dich noch von dem Irrthum bringen,", "tokens": ["La\u00df", "dich", "noch", "von", "dem", "Irr\u00b7thum", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ich rathe dir recht br\u00fcderlich;", "tokens": ["Ich", "ra\u00b7the", "dir", "recht", "br\u00fc\u00b7der\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und solt ich tauben Ohren singen,", "tokens": ["Und", "solt", "ich", "tau\u00b7ben", "Oh\u00b7ren", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Selbst das Gesetze hindert dich,", "tokens": ["Selbst", "das", "Ge\u00b7set\u00b7ze", "hin\u00b7dert", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.13": {"text": "Dein Unternehmen auszuf\u00fchren:", "tokens": ["Dein", "Un\u00b7ter\u00b7neh\u00b7men", "aus\u00b7zu\u00b7f\u00fch\u00b7ren", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Wer seines N\u00e4chsten Weib begehrt;", "tokens": ["Wer", "sei\u00b7nes", "N\u00e4chs\u00b7ten", "Weib", "be\u00b7gehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Ich will es weiter nicht ber\u00fchren,", "tokens": ["Ich", "will", "es", "wei\u00b7ter", "nicht", "be\u00b7r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Du wei\u00dft, was das Geboth dich lehrt.", "tokens": ["Du", "wei\u00dft", ",", "was", "das", "Ge\u00b7both", "dich", "lehrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Vielleicht kanst du dich nicht besinnen,", "tokens": ["Viel\u00b7leicht", "kanst", "du", "dich", "nicht", "be\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Das Sechste meyn ich, li\u00df es nur,", "tokens": ["Das", "Sechs\u00b7te", "meyn", "ich", ",", "li\u00df", "es", "nur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "PPER", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Ach untersuche dein Beginnen,", "tokens": ["Ach", "un\u00b7ter\u00b7su\u00b7che", "dein", "Be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Und flieh in Zeiten Amors Spur.", "tokens": ["Und", "flieh", "in", "Zei\u00b7ten", "A\u00b7mors", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Gesetzt, du l\u00e4\u00dft dich nicht abhalten,", "tokens": ["Ge\u00b7setzt", ",", "du", "l\u00e4\u00dft", "dich", "nicht", "ab\u00b7hal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "So schlag ich mich noch um die Braut,", "tokens": ["So", "schlag", "ich", "mich", "noch", "um", "die", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Und la\u00df darbey das Schicksal walten;", "tokens": ["Und", "la\u00df", "dar\u00b7bey", "das", "Schick\u00b7sal", "wal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Mir schauert warlich nicht die Haut.", "tokens": ["Mir", "schau\u00b7ert", "war\u00b7lich", "nicht", "die", "Haut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Pistohl, und Pulver, Bley und Eisen,", "tokens": ["Pis\u00b7tohl", ",", "und", "Pul\u00b7ver", ",", "Bley", "und", "Ei\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Die sollen unser Schiedsmann seyn,", "tokens": ["Die", "sol\u00b7len", "un\u00b7ser", "Schieds\u00b7mann", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Ich werde tapfer mich erweisen,", "tokens": ["Ich", "wer\u00b7de", "tap\u00b7fer", "mich", "er\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Und demnach ist sie noch nicht dein.", "tokens": ["Und", "dem\u00b7nach", "ist", "sie", "noch", "nicht", "dein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VAFIN", "PPER", "ADV", "PTKNEG", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Indem ich mich behertzt erzeige,", "tokens": ["In\u00b7dem", "ich", "mich", "be\u00b7hertzt", "er\u00b7zei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "So f\u00e4llt der Muth auf einmahl hin,", "tokens": ["So", "f\u00e4llt", "der", "Muth", "auf", "ein\u00b7mahl", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Ich werde stumm, betr\u00fcbt und schweige,", "tokens": ["Ich", "wer\u00b7de", "stumm", ",", "be\u00b7tr\u00fcbt", "und", "schwei\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "VVPP", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Mir schwindet Feuer, Muth und Sinn.", "tokens": ["Mir", "schwin\u00b7det", "Feu\u00b7er", ",", "Muth", "und", "Sinn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Du darffst nicht nach der Ursach fragen,", "tokens": ["Du", "darffst", "nicht", "nach", "der", "Ur\u00b7sach", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Sie wird dir l\u00e4ngstens wissend seyn.", "tokens": ["Sie", "wird", "dir", "l\u00e4ngs\u00b7tens", "wis\u00b7send", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Ich darff ja keine Kleider tragen,", "tokens": ["Ich", "darff", "ja", "kei\u00b7ne", "Klei\u00b7der", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Die mit dir k\u00e4men \u00fcberein.", "tokens": ["Die", "mit", "dir", "k\u00e4\u00b7men", "\u00fc\u00b7be\u00b7re\u00b7in", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.37": {"text": "Mein gantz Geschlecht, will es nicht leiden,", "tokens": ["Mein", "gantz", "Ge\u00b7schlecht", ",", "will", "es", "nicht", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "NN", "$,", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Es ist bereits schon ausgemacht.", "tokens": ["Es", "ist", "be\u00b7reits", "schon", "aus\u00b7ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "So will ich dich nicht mehr beneiden,", "tokens": ["So", "will", "ich", "dich", "nicht", "mehr", "be\u00b7nei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Ich bin gar nicht mehr aufgebracht.", "tokens": ["Ich", "bin", "gar", "nicht", "mehr", "auf\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "An statt, da\u00df ich mich mit dir schlage,", "tokens": ["An", "statt", ",", "da\u00df", "ich", "mich", "mit", "dir", "schla\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "$,", "KOUS", "PPER", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-++-+-+-", "measure": "unknown.measure.penta"}, "line.42": {"text": "So w\u00fcnsch ich dir so vieles Gl\u00fcck.", "tokens": ["So", "w\u00fcnsch", "ich", "dir", "so", "vie\u00b7les", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "PIS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Der Himmel zehle deine Tage,", "tokens": ["Der", "Him\u00b7mel", "zeh\u00b7le", "dei\u00b7ne", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "In dem Grad, als wie dein Geschick.", "tokens": ["In", "dem", "Grad", ",", "als", "wie", "dein", "Ge\u00b7schick", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "KOKOM", "PPOSAT", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.45": {"text": "Denn dieses wirst du selber preisen.", "tokens": ["Denn", "die\u00b7ses", "wirst", "du", "sel\u00b7ber", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Wie h\u00e4lts, mein sonst geliebtes Weib?", "tokens": ["Wie", "h\u00e4lts", ",", "mein", "sonst", "ge\u00b7lieb\u00b7tes", "Weib", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "$,", "PPOSAT", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Frau Schwester wird es k\u00fcnfftig heissen,", "tokens": ["Frau", "Schwes\u00b7ter", "wird", "es", "k\u00fcnff\u00b7tig", "heis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Bey dem erwehlten Zeitvertreib.", "tokens": ["Bey", "dem", "er\u00b7wehl\u00b7ten", "Zeit\u00b7ver\u00b7treib", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Gedenck sie noch manchmahl zur\u00fccke,", "tokens": ["Ge\u00b7denck", "sie", "noch", "manch\u00b7mahl", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.50": {"text": "Bey ihrer angenehmen Lust,", "tokens": ["Bey", "ih\u00b7rer", "an\u00b7ge\u00b7neh\u00b7men", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Und dann und wann auf meine Blicke,", "tokens": ["Und", "dann", "und", "wann", "auf", "mei\u00b7ne", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KON", "PWAV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "Mein Bild verwahr sie in der Brust.", "tokens": ["Mein", "Bild", "ver\u00b7wahr", "sie", "in", "der", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "We