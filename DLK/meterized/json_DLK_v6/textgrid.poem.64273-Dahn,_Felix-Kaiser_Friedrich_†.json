{"textgrid.poem.64273": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Kaiser Friedrich \u2020", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auch du dahin! \u2013 Verstummt nun, ihr Gedichte!", "tokens": ["Auch", "du", "da\u00b7hin", "!", "\u2013", "Ver\u00b7stummt", "nun", ",", "ihr", "Ge\u00b7dich\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "PAV", "$.", "$(", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Euch \u00fcberdr\u00f6hnt der Gang der Weltgeschichte.", "tokens": ["Euch", "\u00fc\u00b7berd\u00b7r\u00f6hnt", "der", "Gang", "der", "Welt\u00b7ge\u00b7schich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er schreitet schnell! \u2013 Wir m\u00fcssen's stumm ertragen.", "tokens": ["Er", "schrei\u00b7tet", "schnell", "!", "\u2013", "Wir", "m\u00fcs\u00b7sen's", "stumm", "er\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "$(", "PPER", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn ", "tokens": ["Denn"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Vermag kein Mund! \u2013 Greift fester Schild und Schwert,", "tokens": ["Ver\u00b7mag", "kein", "Mund", "!", "\u2013", "Greift", "fes\u00b7ter", "Schild", "und", "Schwert", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$.", "$(", "NN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und sei's zum Siege, sei es zum Verderben,", "tokens": ["Und", "sei's", "zum", "Sie\u00b7ge", ",", "sei", "es", "zum", "Ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "APPRART", "NN", "$,", "VAFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Im Schweigen, Dulden, K\u00e4mpfen, Siegen, Sterben,", "tokens": ["Im", "Schwei\u00b7gen", ",", "Dul\u00b7den", ",", "K\u00e4mp\u00b7fen", ",", "Sie\u00b7gen", ",", "Ster\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "F\u00fchrt sie ", "tokens": ["F\u00fchrt", "sie"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+-", "measure": "trochaic.single"}}}}}