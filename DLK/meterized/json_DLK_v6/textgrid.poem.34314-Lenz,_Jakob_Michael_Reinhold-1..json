{"textgrid.poem.34314": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herrliche Donna mein! ich sehe", "tokens": ["Herr\u00b7li\u00b7che", "Don\u00b7na", "mein", "!", "ich", "se\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "PPOSAT", "$.", "PPER", "VVFIN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "In eurer Augen Bewegung s\u00fcssen Lichtschein,", "tokens": ["In", "eu\u00b7rer", "Au\u00b7gen", "Be\u00b7we\u00b7gung", "s\u00fcs\u00b7sen", "Licht\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der mir geradeswegs zum Himmel leuchtet,", "tokens": ["Der", "mir", "ge\u00b7ra\u00b7des\u00b7wegs", "zum", "Him\u00b7mel", "leuch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weil durch die lange Gewohnheit", "tokens": ["Weil", "durch", "die", "lan\u00b7ge", "Ge\u00b7wohn\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "In diesen Sonnenstralen der Liebe", "tokens": ["In", "die\u00b7sen", "Son\u00b7nen\u00b7stra\u00b7len", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Eure Seele sich sichtbar wei\u00df.", "tokens": ["Eu\u00b7re", "See\u00b7le", "sich", "sicht\u00b7bar", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "ADJD", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Dies ist das Auge, das mich zum Guten verf\u00fchret", "tokens": ["Dies", "ist", "das", "Au\u00b7ge", ",", "das", "mich", "zum", "Gu\u00b7ten", "ver\u00b7f\u00fch\u00b7ret"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Und meinem r\u00fchmlichen Zweck entgegen geiselt.", "tokens": ["Und", "mei\u00b7nem", "r\u00fchm\u00b7li\u00b7chen", "Zweck", "ent\u00b7ge\u00b7gen", "gei\u00b7selt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "PTKVZ", "VVPP", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Keine menschliche Zunge beschreibt es", "tokens": ["Kei\u00b7ne", "menschli\u00b7che", "Zun\u00b7ge", "be\u00b7schreibt", "es"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Was diese Lichter des Himmels f\u00fchlen mich machen", "tokens": ["Was", "die\u00b7se", "Lich\u00b7ter", "des", "Him\u00b7mels", "f\u00fch\u00b7len", "mich", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PDAT", "NN", "ART", "NN", "VVFIN", "PRF", "VVINF"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Wenn der Winter Flocken ausstreut", "tokens": ["Wenn", "der", "Win\u00b7ter", "Flo\u00b7cken", "aus\u00b7streut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN", "VVFIN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.12": {"text": "Oder wenn das Jahr sich verj\u00fcngt,", "tokens": ["O\u00b7der", "wenn", "das", "Jahr", "sich", "ver\u00b7j\u00fcngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "Die heilige Zeit meiner ersten Wunde.", "tokens": ["Die", "hei\u00b7li\u00b7ge", "Zeit", "mei\u00b7ner", "ers\u00b7ten", "Wun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Oft denck ich wenn droben", "tokens": ["Oft", "denck", "ich", "wenn", "dro\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "KOUS", "ADV"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Von da der ewige Beweger der Sterne", "tokens": ["Von", "da", "der", "e\u00b7wi\u00b7ge", "Be\u00b7we\u00b7ger", "der", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Von seiner Kunst dies uns zu zeigen gew\u00fcrdigt,", "tokens": ["Von", "sei\u00b7ner", "Kunst", "dies", "uns", "zu", "zei\u00b7gen", "ge\u00b7w\u00fcr\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PDS", "PPER", "PTKZU", "VVINF", "VVPP", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wenn droben der Meisterst\u00fcck' mehr sind,", "tokens": ["Wenn", "dro\u00b7ben", "der", "Meis\u00b7ter\u00b7st\u00fcck", "mehr", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "VAFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Warum nicht den Kerker er\u00f6fnen, der mich einschlie\u00dft,", "tokens": ["Wa\u00b7rum", "nicht", "den", "Ker\u00b7ker", "er\u00b7\u00f6f\u00b7nen", ",", "der", "mich", "ein\u00b7schlie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "ART", "NN", "VVINF", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.6": {"text": "Und Weg mir machen hinauf zum ewigen Leben?", "tokens": ["Und", "Weg", "mir", "ma\u00b7chen", "hin\u00b7auf", "zum", "e\u00b7wi\u00b7gen", "Le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Geht dann der innere Streit mit mir an", "tokens": ["Geht", "dann", "der", "in\u00b7ne\u00b7re", "Streit", "mit", "mir", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "APPR", "PPER", "PTKVZ"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und ich seegne die Natur und den Tag, und die Stunde,", "tokens": ["Und", "ich", "seeg\u00b7ne", "die", "Na\u00b7tur", "und", "den", "Tag", ",", "und", "die", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$,", "KON", "ART", "NN", "$,"], "meter": "--+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Die zu so hohem Gl\u00fcck mich aufgespahrt hat,", "tokens": ["Die", "zu", "so", "ho\u00b7hem", "Gl\u00fcck", "mich", "auf\u00b7ge\u00b7spahrt", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADV", "ADJA", "NN", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.10": {"text": "Wo sie dies Herz mit der Hofnung empor hub", "tokens": ["Wo", "sie", "dies", "Herz", "mit", "der", "Hof\u00b7nung", "em\u00b7por", "hub"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PDS", "NN", "APPR", "ART", "NN", "PTKVZ", "VVFIN"], "meter": "-+-+--+--++", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Das sonst unbehelfsam, mir selbst beschwerlich war.", "tokens": ["Das", "sonst", "un\u00b7be\u00b7helf\u00b7sam", ",", "mir", "selbst", "be\u00b7schwer\u00b7lich", "war", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "$,", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Nur von dem Tag' an gefiel ich mir selber,", "tokens": ["Nur", "von", "dem", "Tag'", "an", "ge\u00b7fiel", "ich", "mir", "sel\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "VVFIN", "PPER", "PPER", "ADV", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "F\u00fcllt ein grosser sch\u00f6ner Gedanke meine Seele,", "tokens": ["F\u00fcllt", "ein", "gros\u00b7ser", "sch\u00f6\u00b7ner", "Ge\u00b7dan\u00b7ke", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.14": {"text": "Zu der ihr Auge den Schl\u00fcssel hat.", "tokens": ["Zu", "der", "ihr", "Au\u00b7ge", "den", "Schl\u00fcs\u00b7sel", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Niemals beschied Liebe oder Gl\u00fcck", "tokens": ["Nie\u00b7mals", "be\u00b7schied", "Lie\u00b7be", "o\u00b7der", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Zwey Freunden solche Wonne, die ich nicht hing\u00e4b' gegen eine", "tokens": ["Zwey", "Freun\u00b7den", "sol\u00b7che", "Won\u00b7ne", ",", "die", "ich", "nicht", "hin\u00b7g\u00e4b'", "ge\u00b7gen", "ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "PIAT", "NN", "$,", "PRELS", "PPER", "PTKNEG", "ADV", "APPR", "ART"], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Bewegung ihrer Augen, von denen meine Ruhe", "tokens": ["Be\u00b7we\u00b7gung", "ih\u00b7rer", "Au\u00b7gen", ",", "von", "de\u00b7nen", "mei\u00b7ne", "Ru\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$,", "APPR", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wie ein Baum aus der Wurzel kommt.", "tokens": ["Wie", "ein", "Baum", "aus", "der", "Wur\u00b7zel", "kommt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Heilige seelige gl\u00fcckliche Funken,", "tokens": ["Hei\u00b7li\u00b7ge", "see\u00b7li\u00b7ge", "gl\u00fcck\u00b7li\u00b7che", "Fun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Die ihr mein Leben entz\u00fcndt und alle mein Vergn\u00fcgen,", "tokens": ["Die", "ihr", "mein", "Le\u00b7ben", "ent\u00b7z\u00fcndt", "und", "al\u00b7le", "mein", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVPP", "KON", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Die ihr entz\u00fckend mich aufl\u00f6\u00dft und langsam t\u00f6dtet:", "tokens": ["Die", "ihr", "ent\u00b7z\u00fc\u00b7kend", "mich", "au\u00b7fl\u00f6\u00dft", "und", "lang\u00b7sam", "t\u00f6d\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "PPER", "VVFIN", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie jedes andere Licht verbleicht, wo ihr blitzet,", "tokens": ["Wie", "je\u00b7des", "an\u00b7de\u00b7re", "Licht", "ver\u00b7bleicht", ",", "wo", "ihr", "blit\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "VVPP", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "So weicht aus meinem Herzen,", "tokens": ["So", "weicht", "aus", "mei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Wenn diese S\u00fcssigkeit sich drein herabstr\u00f6mt,", "tokens": ["Wenn", "die\u00b7se", "S\u00fcs\u00b7sig\u00b7keit", "sich", "drein", "her\u00b7ab\u00b7str\u00f6mt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Jeder andere nothwendige Gedancke,", "tokens": ["Je\u00b7der", "an\u00b7de\u00b7re", "noth\u00b7wen\u00b7di\u00b7ge", "Ge\u00b7dan\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.12": {"text": "Und ihr allein bleibt darin mit der Liebe.", "tokens": ["Und", "ihr", "al\u00b7lein", "bleibt", "da\u00b7rin", "mit", "der", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVFIN", "PAV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}}}}