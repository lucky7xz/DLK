{"textgrid.poem.39498": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Oft will die Seele ihre Fl\u00fcgel dehnen,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft will die Seele ihre Fl\u00fcgel dehnen,", "tokens": ["Oft", "will", "die", "See\u00b7le", "ih\u00b7re", "Fl\u00fc\u00b7gel", "deh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gest\u00e4rkt von der Betrachtung reiner Speise;", "tokens": ["Ge\u00b7st\u00e4rkt", "von", "der", "Be\u00b7trach\u00b7tung", "rei\u00b7ner", "Spei\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr d\u00fcnkt, im engen wiederholten Gleise,", "tokens": ["Ihr", "d\u00fcnkt", ",", "im", "en\u00b7gen", "wie\u00b7der\u00b7hol\u00b7ten", "Glei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr Thun vergeblich, und ihr Wi\u00dfen W\u00e4hnen.", "tokens": ["Ihr", "Thun", "ver\u00b7geb\u00b7lich", ",", "und", "ihr", "Wi\u00b7\u00dfen", "W\u00e4h\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$,", "KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie f\u00fchlet tief ein unbezwinglich Sehnen", "tokens": ["Sie", "f\u00fch\u00b7let", "tief", "ein", "un\u00b7be\u00b7zwing\u00b7lich", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach h\u00f6hern Welten, freierm Thatenkrei\u00dfe,", "tokens": ["Nach", "h\u00f6\u00b7hern", "Wel\u00b7ten", ",", "frei\u00b7erm", "Tha\u00b7ten\u00b7krei\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und glaubt, am Schlu\u00df der Bahn nach ird'scher Weise,", "tokens": ["Und", "glaubt", ",", "am", "Schlu\u00df", "der", "Bahn", "nach", "ird'\u00b7scher", "Wei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPRART", "NN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Roll' erst der Vorhang auf zu lichtern Scenen.", "tokens": ["Roll'", "erst", "der", "Vor\u00b7hang", "auf", "zu", "lich\u00b7tern", "Sce\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch r\u00fchrt der Tod den Leib ihr, da\u00df sie scheide,", "tokens": ["Doch", "r\u00fchrt", "der", "Tod", "den", "Leib", "ihr", ",", "da\u00df", "sie", "schei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So schaudert sie, und sieht zur\u00fcck mit Zagen", "tokens": ["So", "schau\u00b7dert", "sie", ",", "und", "sieht", "zu\u00b7r\u00fcck", "mit", "Za\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "VVFIN", "PTKVZ", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf Erdenlust und sterbliche Gespielen.", "tokens": ["Auf", "Er\u00b7den\u00b7lust", "und", "sterb\u00b7li\u00b7che", "Ge\u00b7spie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie einst Proserpina, von Enna's Weide", "tokens": ["Wie", "einst", "Pro\u00b7ser\u00b7pi\u00b7na", ",", "von", "En\u00b7na's", "Wei\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADV", "NE", "$,", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In Pluto's Arm entf\u00fchrt, kindlich im Klagen,", "tokens": ["In", "Plu\u00b7to's", "Arm", "ent\u00b7f\u00fchrt", ",", "kind\u00b7lich", "im", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVPP", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um Blumen weinte, die dem Schoo\u00df entfielen.", "tokens": ["Um", "Blu\u00b7men", "wein\u00b7te", ",", "die", "dem", "Schoo\u00df", "ent\u00b7fie\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VVFIN", "$,", "PRELS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}