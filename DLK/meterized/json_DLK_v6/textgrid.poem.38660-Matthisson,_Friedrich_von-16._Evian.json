{"textgrid.poem.38660": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "16. Evian", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nichts h\u00e4tt' ich Aermste vor dieser erlauchten Versammlung zu sagen,", "tokens": ["Nichts", "h\u00e4tt'", "ich", "A\u00b7erms\u00b7te", "vor", "die\u00b7ser", "er\u00b7lauch\u00b7ten", "Ver\u00b7samm\u00b7lung", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "NN", "APPR", "PDAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.2": {"text": "Str\u00f6mte nicht segnend ein Born hier mineralisches Heil.", "tokens": ["Str\u00f6m\u00b7te", "nicht", "seg\u00b7nend", "ein", "Born", "hier", "mi\u00b7ne\u00b7ra\u00b7li\u00b7sches", "Heil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADJD", "ART", "NE", "ADV", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Meinen durchl\u00f6cherten Leibrock, umflattert vom cynischen Mantel,", "tokens": ["Mei\u00b7nen", "durch\u00b7l\u00f6\u00b7cher\u00b7ten", "Lei\u00b7brock", ",", "um\u00b7flat\u00b7tert", "vom", "cy\u00b7ni\u00b7schen", "Man\u00b7tel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "H\u00e4lt man, als Landeskostum, mir Savoyardin zu gut.", "tokens": ["H\u00e4lt", "man", ",", "als", "Lan\u00b7des\u00b7ko\u00b7stum", ",", "mir", "Sa\u00b7vo\u00b7yar\u00b7din", "zu", "gut", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "NN", "$,", "PPER", "NN", "PTKA", "ADJD", "$."], "meter": "+--+-++-+-+--+", "measure": "iambic.septa.invert"}}}}}