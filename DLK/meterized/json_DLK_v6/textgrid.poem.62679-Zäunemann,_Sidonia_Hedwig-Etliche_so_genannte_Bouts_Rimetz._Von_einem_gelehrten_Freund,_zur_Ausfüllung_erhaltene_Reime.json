{"textgrid.poem.62679": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Etliche so genannte Bouts Rimetz. Von einem gelehrten Freund, zur Ausf\u00fcllung erhaltene Reime", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Niemand schwatze mir vom Lieben und von Hochzeitmachen \u2013 \u2013 vor,", "tokens": ["Nie\u00b7mand", "schwat\u00b7ze", "mir", "vom", "Lie\u00b7ben", "und", "von", "Hoch\u00b7zeit\u00b7ma\u00b7chen", "\u2013", "\u2013", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPRART", "ADJA", "KON", "APPR", "NN", "$(", "$(", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Cypripors Gesang und Liedern weyh ich weder Mund noch \u2013 \u2013 Ohr.", "tokens": ["Cyp\u00b7ri\u00b7pors", "Ge\u00b7sang", "und", "Lie\u00b7dern", "weyh", "ich", "we\u00b7der", "Mund", "noch", "\u2013", "\u2013", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "VVFIN", "PPER", "KON", "NN", "ADV", "$(", "$(", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Ich erwehl zu meiner Lust eine Cutt- und Nonnen \u2013 \u2013 M\u00fctze,", "tokens": ["Ich", "er\u00b7wehl", "zu", "mei\u00b7ner", "Lust", "ei\u00b7ne", "Cut\u00b7t", "und", "Non\u00b7nen", "\u2013", "\u2013", "M\u00fct\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "TRUNC", "KON", "NN", "$(", "$(", "NN", "$,"], "meter": "+-+-+-+--++-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.4": {"text": "Da ich mich in Einsamkeit wieder manches l\u00e4stern \u2013 \u2013 sch\u00fctze.", "tokens": ["Da", "ich", "mich", "in", "Ein\u00b7sam\u00b7keit", "wie\u00b7der", "man\u00b7ches", "l\u00e4s\u00b7tern", "\u2013", "\u2013", "sch\u00fct\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "NN", "ADV", "PIS", "VVINF", "$(", "$(", "VVFIN", "$."], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Ich will lieber Sauer-Kraut und die ungeschmeltzten \u2013 \u2013 R\u00fcben", "tokens": ["Ich", "will", "lie\u00b7ber", "Sau\u00b7e\u00b7rKraut", "und", "die", "un\u00b7ge\u00b7schmeltz\u00b7ten", "\u2013", "\u2013", "R\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["PPER", "VMFIN", "ADV", "NN", "KON", "ART", "ADJA", "$(", "$(", "NN"], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "In dem Kloster, vor das Fleisch in dem Ehstands-Hause \u2013 \u2013 lieben.", "tokens": ["In", "dem", "Klos\u00b7ter", ",", "vor", "das", "Fleisch", "in", "dem", "Eh\u00b7stands\u00b7Hau\u00b7se", "\u2013", "\u2013", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "APPR", "ART", "NN", "$(", "$(", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Mein Vergn\u00fcgen sey das Chor, wo ich sing und beten \u2013 \u2013 tuhe,", "tokens": ["Mein", "Ver\u00b7gn\u00fc\u00b7gen", "sey", "das", "Chor", ",", "wo", "ich", "sing", "und", "be\u00b7ten", "\u2013", "\u2013", "tu\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$(", "$(", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Denn dasselbe wirkt und schafft mir die wahre Seelen \u2013 \u2013 Ruhe,", "tokens": ["Denn", "das\u00b7sel\u00b7be", "wirkt", "und", "schafft", "mir", "die", "wah\u00b7re", "See\u00b7len", "\u2013", "\u2013", "Ru\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["KON", "PDAT", "VVFIN", "KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$(", "$(", "NN", "$,"], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Will mir den gefa\u00dften Schlu\u00df weder Mann noch J\u00fcngling \u2013 \u2013 glauben,", "tokens": ["Will", "mir", "den", "ge\u00b7fa\u00df\u00b7ten", "Schlu\u00df", "we\u00b7der", "Mann", "noch", "J\u00fcng\u00b7ling", "\u2013", "\u2013", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "KON", "NN", "ADV", "NN", "$(", "$(", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Immerhin, es wird die Zeit euch doch diesen Zweifel \u2013 \u2013 rauben.", "tokens": ["Im\u00b7mer\u00b7hin", ",", "es", "wird", "die", "Zeit", "euch", "doch", "die\u00b7sen", "Zwei\u00b7fel", "\u2013", "\u2013", "rau\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VAFIN", "ART", "NN", "PPER", "ADV", "PDAT", "NN", "$(", "$(", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "Geht nur hin, und sucht mit Flei\u00df Amors Pfeile, Amors \u2013 \u2013 Waffen,", "tokens": ["Geht", "nur", "hin", ",", "und", "sucht", "mit", "Flei\u00df", "A\u00b7mors", "Pfei\u00b7le", ",", "A\u00b7mors", "\u2013", "\u2013", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "NN", "NE", "NN", "$,", "NE", "$(", "$(", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "Und geberdet euch darbey als wie die verliebten \u2013 \u2013 Affen!", "tokens": ["Und", "ge\u00b7ber\u00b7det", "euch", "dar\u00b7bey", "als", "wie", "die", "ver\u00b7lieb\u00b7ten", "\u2013", "\u2013", "Af\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PAV", "KOUS", "KOKOM", "ART", "ADJA", "$(", "$(", "NN", "$."], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Dorten stund in einem Carmen auf den Herrn von Ober \u2013 \u2013 n\u00fctz:", "tokens": ["Dor\u00b7ten", "stund", "in", "ei\u00b7nem", "Car\u00b7men", "auf", "den", "Herrn", "von", "O\u00b7ber", "\u2013", "\u2013", "n\u00fctz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "APPR", "ART", "NN", "APPR", "NN", "$(", "$(", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Kriegt das sch\u00f6ne Jungfer-R\u00f6ckgen einen Flecken, Ritz und \u2013 \u2013 Schlitz,", "tokens": ["Kriegt", "das", "sch\u00f6\u00b7ne", "Jung\u00b7fer\u00b7R\u00f6ck\u00b7gen", "ei\u00b7nen", "Fle\u00b7cken", ",", "Ritz", "und", "\u2013", "\u2013", "Schlitz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,", "NN", "KON", "$(", "$(", "NE", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.15": {"text": "So mu\u00df auch der Jungfern Gl\u00fcck und die edle Freyheit \u2013 \u2013 weichen,", "tokens": ["So", "mu\u00df", "auch", "der", "Jung\u00b7fern", "Gl\u00fcck", "und", "die", "ed\u00b7le", "Frey\u00b7heit", "\u2013", "\u2013", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "ART", "NN", "NN", "KON", "ART", "ADJA", "NN", "$(", "$(", "VVINF", "$,"], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Und dargegen sucht die Angst sich gar eilend einzu \u2013 \u2013 schleichen.", "tokens": ["Und", "dar\u00b7ge\u00b7gen", "sucht", "die", "Angst", "sich", "gar", "ei\u00b7lend", "ein\u00b7zu", "\u2013", "\u2013", "schlei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "ART", "NN", "PRF", "ADV", "ADJD", "PTKVZ", "$(", "$(", "VVINF", "$."], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Dieser Vers hat recht gesagt, Jungfern k\u00f6nnen k\u00fchnlich \u2013 \u2013 lachen;", "tokens": ["Die\u00b7ser", "Vers", "hat", "recht", "ge\u00b7sagt", ",", "Jung\u00b7fern", "k\u00f6n\u00b7nen", "k\u00fchn\u00b7lich", "\u2013", "\u2013", "la\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "ADJD", "VVPP", "$,", "NN", "VMFIN", "ADJD", "$(", "$(", "VVINF", "$."], "meter": "+-+-+-++-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.18": {"text": "Dahingegen manches Weib sich mu\u00df Angst und Sorge \u2013 \u2013 machen.", "tokens": ["Da\u00b7hin\u00b7ge\u00b7gen", "man\u00b7ches", "Weib", "sich", "mu\u00df", "Angst", "und", "Sor\u00b7ge", "\u2013", "\u2013", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PAV", "PIAT", "NN", "PRF", "VMFIN", "NN", "KON", "NN", "$(", "$(", "VVINF", "$."], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Kriegt die Noth durch Gegen-Mittel eine Lindrung und ein \u2013 \u2013 Loch,", "tokens": ["Kriegt", "die", "Noth", "durch", "Ge\u00b7gen\u00b7Mit\u00b7tel", "ei\u00b7ne", "Lind\u00b7rung", "und", "ein", "\u2013", "\u2013", "Loch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "ART", "NN", "KON", "ART", "$(", "$(", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.20": {"text": "Ey, so w\u00e4hrt es doch nicht lange, und man schauet immer \u2013 \u2013 noch", "tokens": ["Ey", ",", "so", "w\u00e4hrt", "es", "doch", "nicht", "lan\u00b7ge", ",", "und", "man", "schau\u00b7et", "im\u00b7mer", "\u2013", "\u2013", "noch"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "$,", "KON", "PIS", "VVFIN", "ADV", "$(", "$(", "ADV"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.21": {"text": "Eben so viel Bitterkeit als in Erfurt Mannes \u2013 \u2013 Krausen,", "tokens": ["E\u00b7ben", "so", "viel", "Bit\u00b7ter\u00b7keit", "als", "in", "Er\u00b7furt", "Man\u00b7nes", "\u2013", "\u2013", "Krau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "KOKOM", "APPR", "NE", "NN", "$(", "$(", "NN", "$,"], "meter": "+---+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.22": {"text": "Leid und Tr\u00fcbsal, Gram und Pein will die armen Weiber \u2013 \u2013 zausen.", "tokens": ["Leid", "und", "Tr\u00fcb\u00b7sal", ",", "Gram", "und", "Pein", "will", "die", "ar\u00b7men", "Wei\u00b7ber", "\u2013", "\u2013", "zau\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "VMFIN", "ART", "ADJA", "NN", "$(", "$(", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.23": {"text": "Kriegt ein Weib von ihrem Mann manchen Tag ein Dutzend \u2013 \u2013 M\u00e4ulgen,", "tokens": ["Kriegt", "ein", "Weib", "von", "ih\u00b7rem", "Mann", "man\u00b7chen", "Tag", "ein", "Dut\u00b7zend", "\u2013", "\u2013", "M\u00e4ul\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "PIAT", "NN", "ART", "NN", "$(", "$(", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.24": {"text": "Ey! so sagt, was folgt darauf? Uber gar ein kleines \u2013 \u2013 Weilgen", "tokens": ["Ey", "!", "so", "sagt", ",", "was", "folgt", "da\u00b7rauf", "?", "U\u00b7ber", "gar", "ein", "klei\u00b7nes", "\u2013", "\u2013", "Weil\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["NN", "$.", "ADV", "VVFIN", "$,", "PWS", "VVFIN", "PAV", "$.", "APPR", "ADV", "ART", "ADJA", "$(", "$(", "NN"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.25": {"text": "Brennt des Mannes Zorn wie Feuer, und er schw\u00f6ret beym \u2013 \u2013 Parna\u00df:", "tokens": ["Brennt", "des", "Man\u00b7nes", "Zorn", "wie", "Feu\u00b7er", ",", "und", "er", "schw\u00f6\u00b7ret", "beym", "\u2013", "\u2013", "Par\u00b7na\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "KOKOM", "NN", "$,", "KON", "PPER", "VVFIN", "APPRART", "$(", "$(", "NE", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.26": {"text": "Frau! ich werde dich noch pr\u00fcgeln, oder stecke dich ins \u2013 \u2013 Fa\u00df.", "tokens": ["Frau", "!", "ich", "wer\u00b7de", "dich", "noch", "pr\u00fc\u00b7geln", ",", "o\u00b7der", "ste\u00b7cke", "dich", "ins", "\u2013", "\u2013", "Fa\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["NN", "$.", "PPER", "VAFIN", "PPER", "ADV", "VVINF", "$,", "KON", "VVFIN", "PPER", "APPRART", "$(", "$(", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.27": {"text": "Dieser Weiber Noth und Pein will ich mich bey Zeit ent \u2013 \u2013 schlagen,", "tokens": ["Die\u00b7ser", "Wei\u00b7ber", "Noth", "und", "Pein", "will", "ich", "mich", "bey", "Zeit", "ent", "\u2013", "\u2013", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PDAT", "NN", "NN", "KON", "NN", "VMFIN", "PPER", "PRF", "APPR", "NN", "VVPP", "$(", "$(", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.28": {"text": "Denn so darf kein Herzens-Wurm jemahls meine Seele \u2013 \u2013 nagen.", "tokens": ["Denn", "so", "darf", "kein", "Her\u00b7zens\u00b7Wurm", "je\u00b7mahls", "mei\u00b7ne", "See\u00b7le", "\u2013", "\u2013", "na\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PIAT", "NN", "ADV", "PPOSAT", "NN", "$(", "$(", "VVFIN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.29": {"text": "Drum so sag ich noch einmahl: Gute Nacht, du Scherz und \u2013 \u2013 K\u00fcssen,", "tokens": ["Drum", "so", "sag", "ich", "noch", "ein\u00b7mahl", ":", "Gu\u00b7te", "Nacht", ",", "du", "Scherz", "und", "\u2013", "\u2013", "K\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "PPER", "ADV", "ADV", "$.", "ADJA", "NN", "$,", "PPER", "NN", "KON", "$(", "$(", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.30": {"text": "Ich will deine Eitelkeit bis in meine Gruft ver \u2013 \u2013 missen.", "tokens": ["Ich", "will", "dei\u00b7ne", "Ei\u00b7tel\u00b7keit", "bis", "in", "mei\u00b7ne", "Gruft", "ver", "\u2013", "\u2013", "mis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "APPR", "APPR", "PPOSAT", "NN", "NE", "$(", "$(", "VVPP", "$."], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}}}}}