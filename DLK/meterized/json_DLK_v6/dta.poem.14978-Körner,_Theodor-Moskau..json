{"dta.poem.14978": {"metadata": {"author": {"name": "K\u00f6rner, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Moskau.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1814", "urn": "urn:nbn:de:kobv:b4-200905193258", "language": ["de:0.99"], "booktitle": "K\u00f6rner, Theodor: Leyer und Schwerdt. Berlin, 1814."}, "poem": {"stanza.1": {"line.1": {"text": "Wie schimmern der Pall\u00e4ste goldne W\u00e4nde!", "tokens": ["Wie", "schim\u00b7mern", "der", "Pal\u00b7l\u00e4s\u00b7te", "gold\u00b7ne", "W\u00e4n\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es schw\u00e4rmt der Blick, wohin ich ihn versende,", "tokens": ["Es", "schw\u00e4rmt", "der", "Blick", ",", "wo\u00b7hin", "ich", "ihn", "ver\u00b7sen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von einer Pracht zur andern fortgeflogen. \u2014", "tokens": ["Von", "ei\u00b7ner", "Pracht", "zur", "an\u00b7dern", "fort\u00b7ge\u00b7flo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da w\u00e4lzen sich auf einmal gl\u00fch'nde Wogen,", "tokens": ["Da", "w\u00e4l\u00b7zen", "sich", "auf", "ein\u00b7mal", "gl\u00fch'n\u00b7de", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Es schleudern deiner B\u00fcrger eigne H\u00e4nde", "tokens": ["Es", "schleu\u00b7dern", "dei\u00b7ner", "B\u00fcr\u00b7ger", "eig\u00b7ne", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aufs eigne Dach die spr\u00fch'nden Fackelbr\u00e4nde,", "tokens": ["Aufs", "eig\u00b7ne", "Dach", "die", "spr\u00fch'n\u00b7den", "Fa\u00b7ckel\u00b7br\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Feuerkreis hat prasselnd dich umzogen. \u2014", "tokens": ["Ein", "Feu\u00b7er\u00b7kreis", "hat", "pras\u00b7selnd", "dich", "um\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "PPER", "VVIZU", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O la\u00df dich nur vom Aberwitz verdammen; \u2014", "tokens": ["O", "la\u00df", "dich", "nur", "vom", "A\u00b7ber\u00b7witz", "ver\u00b7dam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Kirchen st\u00fcrzt! Pall\u00e4ste brecht zusammen!", "tokens": ["Ihr", "Kir\u00b7chen", "st\u00fcrzt", "!", "Pal\u00b7l\u00e4s\u00b7te", "brecht", "zu\u00b7sam\u00b7men", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Ph\u00f6nix Ru\u00dflands wirft sich in die Flammen! \u2014", "tokens": ["Der", "Ph\u00f6\u00b7nix", "Ru\u00df\u00b7lands", "wirft", "sich", "in", "die", "Flam\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "PRF", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch hochverkl\u00e4rt aus seinem Feuerkranze", "tokens": ["Doch", "hoch\u00b7ver\u00b7kl\u00e4rt", "aus", "sei\u00b7nem", "Feu\u00b7er\u00b7kran\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wird er erstehn im frischen Jugendglanze,", "tokens": ["Wird", "er", "er\u00b7stehn", "im", "fri\u00b7schen", "Ju\u00b7gend\u00b7glan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Sankt Georg schwingt siegend seine Lanze.", "tokens": ["Und", "Sankt", "Ge\u00b7org", "schwingt", "sie\u00b7gend", "sei\u00b7ne", "Lan\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}