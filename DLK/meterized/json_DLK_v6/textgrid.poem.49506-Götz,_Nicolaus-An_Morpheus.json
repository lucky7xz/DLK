{"textgrid.poem.49506": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "An Morpheus", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott des Schlafes, du bist schlau,", "tokens": ["Gott", "des", "Schla\u00b7fes", ",", "du", "bist", "schlau", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sagte meines Nachbars Frau.", "tokens": ["Sag\u00b7te", "mei\u00b7nes", "Nach\u00b7bars", "Frau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sanft in deinen Arm gewiegt,", "tokens": ["Sanft", "in", "dei\u00b7nen", "Arm", "ge\u00b7wiegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat der Nachbar mich besiegt:", "tokens": ["Hat", "der", "Nach\u00b7bar", "mich", "be\u00b7siegt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn im Traum, von dir geschickt,", "tokens": ["Denn", "im", "Traum", ",", "von", "dir", "ge\u00b7schickt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ward ich schwach, und er begl\u00fcckt.", "tokens": ["Ward", "ich", "schwach", ",", "und", "er", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "$,", "KON", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mu\u00dft' er mich so weich, als sch\u00f6n,", "tokens": ["Mu\u00dft'", "er", "mich", "so", "weich", ",", "als", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "ADJD", "$,", "KOUS", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und nicht erst recht grausam sehn!", "tokens": ["Und", "nicht", "erst", "recht", "grau\u00b7sam", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}