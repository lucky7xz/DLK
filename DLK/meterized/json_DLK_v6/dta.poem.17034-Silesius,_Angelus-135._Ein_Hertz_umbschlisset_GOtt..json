{"dta.poem.17034": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "135.  Ein Hertz umbschlisset GOtt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Gar unau\u00dfm\u00e4\u00dflich ist der H\u00f6chste wie wir wissen:", "tokens": ["Gar", "un\u00b7au\u00df\u00b7m\u00e4\u00df\u00b7lich", "ist", "der", "H\u00f6chs\u00b7te", "wie", "wir", "wis\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "KOKOM", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dannoch kan jhn gantz ein Menschlich Hertz umb-", "tokens": ["Und", "dan\u00b7noch", "kan", "jhn", "gantz", "ein", "Menschlich", "Hertz", "um\u00b7b"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "NN", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "schlissen!", "tokens": ["schlis\u00b7sen", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}