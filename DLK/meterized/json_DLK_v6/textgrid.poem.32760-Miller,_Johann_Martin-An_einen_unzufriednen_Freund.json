{"textgrid.poem.32760": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "An einen unzufriednen Freund", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was irrst du, Schwermut im Gesicht,", "tokens": ["Was", "irrst", "du", ",", "Schwer\u00b7mut", "im", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Freund, durchs Leben hin;", "tokens": ["O", "Freund", ",", "durchs", "Le\u00b7ben", "hin", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und siehst des Fr\u00fchlings Blumen nicht", "tokens": ["Und", "siehst", "des", "Fr\u00fch\u00b7lings", "Blu\u00b7men", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu deiner Freude bl\u00fchn?", "tokens": ["Zu", "dei\u00b7ner", "Freu\u00b7de", "bl\u00fchn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Siehst nicht des goldnen Sommers Zier,", "tokens": ["Siehst", "nicht", "des", "gold\u00b7nen", "Som\u00b7mers", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht seiner \u00c4hren Pracht;", "tokens": ["Nicht", "sei\u00b7ner", "\u00c4h\u00b7ren", "Pracht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Herbstes Segen nicht, der dir", "tokens": ["Des", "Herbs\u00b7tes", "Se\u00b7gen", "nicht", ",", "der", "dir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "$,", "PRELS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Baum und Rebe lacht.", "tokens": ["Von", "Baum", "und", "Re\u00b7be", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dankst nicht des Winters kaltem Hauch", "tokens": ["Dankst", "nicht", "des", "Win\u00b7ters", "kal\u00b7tem", "Hauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein frisches, leichtes Blut;", "tokens": ["Dein", "fri\u00b7sches", ",", "leich\u00b7tes", "Blut", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fchlst nicht, da\u00df in der Erde Bauch", "tokens": ["F\u00fchlst", "nicht", ",", "da\u00df", "in", "der", "Er\u00b7de", "Bauch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "$,", "KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schon wieder Segen ruht.", "tokens": ["Schon", "wie\u00b7der", "Se\u00b7gen", "ruht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Umf\u00e4ngst in deinem Bruder nicht", "tokens": ["Um\u00b7f\u00e4ngst", "in", "dei\u00b7nem", "Bru\u00b7der", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Sch\u00f6pfers Ebenbild;", "tokens": ["Des", "Sch\u00f6p\u00b7fers", "E\u00b7ben\u00b7bild", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fchlst nichts, wenn seinem Angesicht", "tokens": ["F\u00fchlst", "nichts", ",", "wenn", "sei\u00b7nem", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PIS", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Freundschaft Thr\u00e4n' entquillt.", "tokens": ["Der", "Freund\u00b7schaft", "Thr\u00e4n'", "ent\u00b7quillt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Beutst br\u00fcderlich ihm nicht die Hand;", "tokens": ["Beutst", "br\u00fc\u00b7der\u00b7lich", "ihm", "nicht", "die", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Teilst seine Freuden nicht;", "tokens": ["Teilst", "sei\u00b7ne", "Freu\u00b7den", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Fliehst \u00e4ngstlich vor dem sanften Band,", "tokens": ["Fliehst", "\u00e4ngst\u00b7lich", "vor", "dem", "sanf\u00b7ten", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Liebe dir umflicht.", "tokens": ["Das", "Lie\u00b7be", "dir", "um\u00b7flicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wi\u00df! Lieb' ist Gottesgab' und scheucht", "tokens": ["Wi\u00df", "!", "Lieb'", "ist", "Got\u00b7tes\u00b7gab'", "und", "scheucht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "NN", "VAFIN", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sorgen vor sich hin;", "tokens": ["Die", "Sor\u00b7gen", "vor", "sich", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer willig seine Hand ihr reicht,", "tokens": ["Wer", "wil\u00b7lig", "sei\u00b7ne", "Hand", "ihr", "reicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wei\u00df nichts vom tr\u00fcben Sinn.", "tokens": ["Wei\u00df", "nichts", "vom", "tr\u00fc\u00b7ben", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Blick auf, o Freund, sie l\u00e4chelt dir", "tokens": ["Blick", "auf", ",", "o", "Freund", ",", "sie", "l\u00e4\u00b7chelt", "dir"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "FM", "NN", "$,", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus Daphnens holdem Blick;", "tokens": ["Aus", "Daph\u00b7nens", "hol\u00b7dem", "Blick", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf! Wandl' ins Paradies mit ihr,", "tokens": ["Auf", "!", "Wandl'", "ins", "Pa\u00b7ra\u00b7dies", "mit", "ihr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "NN", "APPRART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und la\u00df den Gram zur\u00fcck!", "tokens": ["Und", "la\u00df", "den", "Gram", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}