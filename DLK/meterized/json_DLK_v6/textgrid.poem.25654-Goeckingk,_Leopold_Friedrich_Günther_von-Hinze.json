{"textgrid.poem.25654": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "Hinze", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wem gilt doch dieses Spottgedicht?", "tokens": ["Wem", "gilt", "doch", "die\u00b7ses", "Spott\u00b7ge\u00b7dicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fing Hinze l\u00e4chelnd an zu fragen.", "tokens": ["Fing", "Hin\u00b7ze", "l\u00e4\u00b7chelnd", "an", "zu", "fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJD", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich k\u00f6nnt' es allen Menschen sagen,", "tokens": ["Ich", "k\u00f6nnt'", "es", "al\u00b7len", "Men\u00b7schen", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gerade nur dem Frager nicht.", "tokens": ["Ge\u00b7ra\u00b7de", "nur", "dem", "Fra\u00b7ger", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}