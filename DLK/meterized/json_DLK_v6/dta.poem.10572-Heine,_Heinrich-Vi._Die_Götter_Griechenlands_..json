token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Und wenn ich bedenke, wie feig und windig", "tokens": ["Und", "wenn", "ich", "be\u00b7den\u00b7ke", ",", "wie", "feig", "und", "win\u00b7dig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "PWAV", "ADJD", "KON", "ADJD"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.8": {"text": "Die G\u00f6tter sind, die Euch besiegten,", "tokens": ["Die", "G\u00f6t\u00b7ter", "sind", ",", "die", "Euch", "be\u00b7sieg\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die neuen, herrschenden, tristen G\u00f6tter.", "tokens": ["Die", "neu\u00b7en", ",", "herr\u00b7schen\u00b7den", ",", "tris\u00b7ten", "G\u00f6t\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVPP", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Die Schadenfrohen im Schafspelz der Demuth \u2014", "tokens": ["Die", "Scha\u00b7den\u00b7fro\u00b7hen", "im", "Schaf\u00b7spelz", "der", "De\u00b7muth"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ART", "NN", "$("], "meter": "-+-+--+---+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "O da fa\u00dft mich ein d\u00fcsterer Groll,", "tokens": ["O", "da", "fa\u00dft", "mich", "ein", "d\u00fcs\u00b7te\u00b7rer", "Groll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.12": {"text": "Und brechen m\u00f6cht' ich die neuen Tempel,", "tokens": ["Und", "bre\u00b7chen", "m\u00f6cht'", "ich", "die", "neu\u00b7en", "Tem\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Und k\u00e4mpfen f\u00fcr Euch, Ihr alten G\u00f6tter,", "tokens": ["Und", "k\u00e4mp\u00b7fen", "f\u00fcr", "Euch", ",", "Ihr", "al\u00b7ten", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "F\u00fcr Euch und Eu'r gutes, ambrosisches Recht,", "tokens": ["F\u00fcr", "Euch", "und", "Eu'r", "gu\u00b7tes", ",", "am\u00b7bro\u00b7si\u00b7sches", "Recht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "NE", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.15": {"text": "Und vor Euren hohen Alt\u00e4ren,", "tokens": ["Und", "vor", "Eu\u00b7ren", "ho\u00b7hen", "Al\u00b7t\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.16": {"text": "Den wiedergebauten, den opferdampfenden", "tokens": ["Den", "wie\u00b7der\u00b7ge\u00b7bau\u00b7ten", ",", "den", "op\u00b7fer\u00b7damp\u00b7fen\u00b7den"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ART", "NN"], "meter": "-+--+--+-+--", "measure": "amphibrach.tri.plus"}, "line.17": {"text": "M\u00f6cht' ich selber knien und beten,", "tokens": ["M\u0