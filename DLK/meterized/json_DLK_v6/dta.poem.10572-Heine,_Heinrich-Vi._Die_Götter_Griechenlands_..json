{"dta.poem.10572": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vi.  \n  Die G\u00f6tter Griechenlands .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Staunend, und seltsam geblendet, betracht' ich", "tokens": ["Stau\u00b7nend", ",", "und", "selt\u00b7sam", "ge\u00b7blen\u00b7det", ",", "be\u00b7tracht'", "ich"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "$,", "KON", "ADJD", "VVPP", "$,", "VVFIN", "PPER"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Das luftige Pantheon,", "tokens": ["Das", "luf\u00b7ti\u00b7ge", "Pan\u00b7the\u00b7on", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die feierlich stummen, grau'nhaft bewegten", "tokens": ["Die", "fei\u00b7er\u00b7lich", "stum\u00b7men", ",", "grau'\u00b7nhaft", "be\u00b7weg\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJD", "VVINF", "$,", "ADJD", "ADJA"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Riesengestalten.", "tokens": ["Rie\u00b7sen\u00b7ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Der dort ist Kronion, der Himmelsk\u00f6nig,", "tokens": ["Der", "dort", "ist", "Kro\u00b7ni\u00b7on", ",", "der", "Him\u00b7mels\u00b7k\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "VAFIN", "NE", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Schneewei\u00df sind die Locken des Haupts,", "tokens": ["Schnee\u00b7wei\u00df", "sind", "die", "Lo\u00b7cken", "des", "Haupts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Die ber\u00fchmten, olymposersch\u00fctternden Locken.", "tokens": ["Die", "be\u00b7r\u00fchm\u00b7ten", ",", "o\u00b7lym\u00b7po\u00b7ser\u00b7sch\u00fct\u00b7tern\u00b7den", "Lo\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Er h\u00e4lt in der Hand den erloschenen Blitz,", "tokens": ["Er", "h\u00e4lt", "in", "der", "Hand", "den", "er\u00b7lo\u00b7sche\u00b7nen", "Blitz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.9": {"text": "In seinem Gesichte liegt Ungl\u00fcck und Gram,", "tokens": ["In", "sei\u00b7nem", "Ge\u00b7sich\u00b7te", "liegt", "Un\u00b7gl\u00fcck", "und", "Gram", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.10": {"text": "Und doch noch immer der alte Stolz.", "tokens": ["Und", "doch", "noch", "im\u00b7mer", "der", "al\u00b7te", "Stolz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.11": {"text": "Das waren bessere Zeiten, o Zeus,", "tokens": ["Das", "wa\u00b7ren", "bes\u00b7se\u00b7re", "Zei\u00b7ten", ",", "o", "Zeus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJA", "NN", "$,", "FM", "NE", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Als du dich himmlisch erg\u00f6tztest", "tokens": ["Als", "du", "dich", "himm\u00b7lisch", "er\u00b7g\u00f6tz\u00b7test"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "An Knaben und Nymphen und Hekatomben!", "tokens": ["An", "Kna\u00b7ben", "und", "Nym\u00b7phen", "und", "He\u00b7ka\u00b7tom\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NE", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "Doch auch die G\u00f6tter regieren nicht ewig,", "tokens": ["Doch", "auch", "die", "G\u00f6t\u00b7ter", "re\u00b7gie\u00b7ren", "nicht", "e\u00b7wig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Die jungen verdr\u00e4ngen die alten,", "tokens": ["Die", "jun\u00b7gen", "ver\u00b7dr\u00e4n\u00b7gen", "die", "al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.16": {"text": "Wie du einst selber den greisen Vater", "tokens": ["Wie", "du", "einst", "sel\u00b7ber", "den", "grei\u00b7sen", "Va\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Und deine Titanen-Oehme verdr\u00e4ngt,", "tokens": ["Und", "dei\u00b7ne", "Ti\u00b7ta\u00b7nen\u00b7Oeh\u00b7me", "ver\u00b7dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Jupiter Parricida!", "tokens": ["Ju\u00b7pi\u00b7ter", "Par\u00b7ri\u00b7ci\u00b7da", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.19": {"text": "Auch dich erkenn' ich, stolze Here!", "tokens": ["Auch", "dich", "er\u00b7kenn'", "ich", ",", "stol\u00b7ze", "He\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Trotz all deiner eifers\u00fcchtigen Angst,", "tokens": ["Trotz", "all", "dei\u00b7ner", "ei\u00b7fer\u00b7s\u00fcch\u00b7ti\u00b7gen", "Angst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.21": {"text": "Hat doch eine andre das Zepter gewonnen,", "tokens": ["Hat", "doch", "ei\u00b7ne", "and\u00b7re", "das", "Zep\u00b7ter", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "ART", "NN", "VVPP", "$,"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Und du bist nicht mehr die Himmelsk\u00f6n'gin,", "tokens": ["Und", "du", "bist", "nicht", "mehr", "die", "Him\u00b7mels\u00b7k\u00f6n'\u00b7gin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "ADV", "ART", "NN", "$,"], "meter": "----+-+--+", "measure": "iambic.tri.chol"}, "line.23": {"text": "Und dein gro\u00dfes Aug' ist erstarrt,", "tokens": ["Und", "dein", "gro\u00b7\u00dfes", "Aug'", "ist", "er\u00b7starrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.24": {"text": "Und deine Lilienarme sind kraftlos,", "tokens": ["Und", "dei\u00b7ne", "Li\u00b7li\u00b7en\u00b7ar\u00b7me", "sind", "kraft\u00b7los", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Und nimmermehr trifft deine Rache", "tokens": ["Und", "nim\u00b7mer\u00b7mehr", "trifft", "dei\u00b7ne", "Ra\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Die gottbefruchtete Jungfrau", "tokens": ["Die", "gott\u00b7be\u00b7fruch\u00b7te\u00b7te", "Jung\u00b7frau"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Und den wunderth\u00e4tigen Gottessohn.", "tokens": ["Und", "den", "wun\u00b7der\u00b7th\u00e4\u00b7ti\u00b7gen", "Got\u00b7tes\u00b7sohn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Auch dich erkenn' ich, Pallas Athene!", "tokens": ["Auch", "dich", "er\u00b7kenn'", "ich", ",", "Pal\u00b7las", "A\u00b7the\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "$,", "NE", "NE", "$."], "meter": "++-+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Mit Schild und Weisheit konntest du nicht", "tokens": ["Mit", "Schild", "und", "Weis\u00b7heit", "konn\u00b7test", "du", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VMFIN", "PPER", "PTKNEG"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Abwehren das G\u00f6tterverderben?", "tokens": ["Ab\u00b7weh\u00b7ren", "das", "G\u00f6t\u00b7ter\u00b7ver\u00b7der\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Auch dich erkenn' ich, auch dich, Aphrodite,", "tokens": ["Auch", "dich", "er\u00b7kenn'", "ich", ",", "auch", "dich", ",", "A\u00b7phro\u00b7di\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "$,", "ADV", "PPER", "$,", "NE", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Einst die goldene! jetzt die silberne!", "tokens": ["Einst", "die", "gol\u00b7de\u00b7ne", "!", "jetzt", "die", "sil\u00b7ber\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "$.", "ADV", "ART", "NN", "$."], "meter": "--+--+-+--", "measure": "anapaest.di.plus"}, "line.6": {"text": "Zwar schm\u00fcckt dich noch immer des G\u00fcrtels Liebreiz;", "tokens": ["Zwar", "schm\u00fcckt", "dich", "noch", "im\u00b7mer", "des", "G\u00fcr\u00b7tels", "Lieb\u00b7reiz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Doch graut mir heimlich vor deiner Sch\u00f6nheit,", "tokens": ["Doch", "graut", "mir", "heim\u00b7lich", "vor", "dei\u00b7ner", "Sch\u00f6n\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und wollt' mich begl\u00fccken dein g\u00fctiger Leib,", "tokens": ["Und", "wollt'", "mich", "be\u00b7gl\u00fc\u00b7cken", "dein", "g\u00fc\u00b7ti\u00b7ger", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.9": {"text": "Wie andre Helden, ich st\u00fcrbe vor Angst;", "tokens": ["Wie", "and\u00b7re", "Hel\u00b7den", ",", "ich", "st\u00fcr\u00b7be", "vor", "Angst", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Als Leicheng\u00f6ttin erscheinst du mir,", "tokens": ["Als", "Lei\u00b7chen\u00b7g\u00f6t\u00b7tin", "er\u00b7scheinst", "du", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Venus Libitina!", "tokens": ["Ve\u00b7nus", "Li\u00b7bi\u00b7ti\u00b7na", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "OOO---", "measure": "unknown.measure.zero"}, "line.12": {"text": "Nicht mehr mit Liebe schaut nach dir,", "tokens": ["Nicht", "mehr", "mit", "Lie\u00b7be", "schaut", "nach", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Dort, der schreckliche Ares.", "tokens": ["Dort", ",", "der", "schreck\u00b7li\u00b7che", "A\u00b7res", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.14": {"text": "Es schaut so traurig Ph\u00f6bos Apollo,", "tokens": ["Es", "schaut", "so", "trau\u00b7rig", "Ph\u00f6\u00b7bos", "A\u00b7pol\u00b7lo", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "NE", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Der J\u00fcngling. Es schweigt seine Lei'r,", "tokens": ["Der", "J\u00fcng\u00b7ling", ".", "Es", "schweigt", "sei\u00b7ne", "Lei'r", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.16": {"text": "Die so freudig erklungen beim G\u00f6ttermahl.", "tokens": ["Die", "so", "freu\u00b7dig", "er\u00b7klun\u00b7gen", "beim", "G\u00f6t\u00b7ter\u00b7mahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVPP", "APPRART", "NN", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "Noch trauriger schaut Hephaistos,", "tokens": ["Noch", "trau\u00b7ri\u00b7ger", "schaut", "He\u00b7phais\u00b7tos", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "NE", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.18": {"text": "Und wahrlich, der Hinkende! nimmermehr", "tokens": ["Und", "wahr\u00b7lich", ",", "der", "Hin\u00b7ken\u00b7de", "!", "nim\u00b7mer\u00b7mehr"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "$,", "ART", "NN", "$.", "ADV"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.19": {"text": "F\u00e4llt er Hebe'n in's Amt,", "tokens": ["F\u00e4llt", "er", "He\u00b7be'n", "in's", "Amt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "APPRART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.20": {"text": "Und schenkt gesch\u00e4ftig, in der Versammlung,", "tokens": ["Und", "schenkt", "ge\u00b7sch\u00e4f\u00b7tig", ",", "in", "der", "Ver\u00b7samm\u00b7lung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Den lieblichen Nektar \u2014 Und l\u00e4ngst ist erloschen", "tokens": ["Den", "lieb\u00b7li\u00b7chen", "Nek\u00b7tar", "Und", "l\u00e4ngst", "ist", "er\u00b7lo\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "KON", "ADV", "VAFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Das unausl\u00f6schliche G\u00f6ttergel\u00e4chter.", "tokens": ["Das", "un\u00b7aus\u00b7l\u00f6schli\u00b7che", "G\u00f6t\u00b7ter\u00b7ge\u00b7l\u00e4ch\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich hab' Euch niemals geliebt, Ihr G\u00f6tter!", "tokens": ["Ich", "hab'", "Euch", "nie\u00b7mals", "ge\u00b7liebt", ",", "Ihr", "G\u00f6t\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Denn widerw\u00e4rtig sind mir die Griechen,", "tokens": ["Denn", "wi\u00b7der\u00b7w\u00e4r\u00b7tig", "sind", "mir", "die", "Grie\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und gar di