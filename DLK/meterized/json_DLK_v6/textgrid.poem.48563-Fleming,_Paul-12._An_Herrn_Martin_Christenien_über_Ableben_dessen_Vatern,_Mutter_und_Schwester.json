{"textgrid.poem.48563": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "12. An Herrn Martin Christenien \u00fcber Ableben dessen Vatern, Mutter und Schwester", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebster nach dem Liebsten du,", "tokens": ["Liebs\u00b7ter", "nach", "dem", "Liebs\u00b7ten", "du", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "o du meiner Freunde Seele,", "tokens": ["o", "du", "mei\u00b7ner", "Freun\u00b7de", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "gieb doch, Bruder, gieb doch zu,", "tokens": ["gieb", "doch", ",", "Bru\u00b7der", ",", "gieb", "doch", "zu", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "NN", "$,", "VVIMP", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "da\u00df ich mich mit dir auch qu\u00e4le,", "tokens": ["da\u00df", "ich", "mich", "mit", "dir", "auch", "qu\u00e4\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "der du in viel Toden stirbst", "tokens": ["der", "du", "in", "viel", "To\u00b7den", "stirbst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PIAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "und in keinem doch verdirbst!", "tokens": ["und", "in", "kei\u00b7nem", "doch", "ver\u00b7dirbst", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach! was ist das Leben doch,", "tokens": ["Ach", "!", "was", "ist", "das", "Le\u00b7ben", "doch", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWS", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "in dem nichts als Sterben lebet?", "tokens": ["in", "dem", "nichts", "als", "Ster\u00b7ben", "le\u00b7bet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIS", "KOKOM", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00c4rger ist es, \u00e4rger noch", "tokens": ["\u00c4r\u00b7ger", "ist", "es", ",", "\u00e4r\u00b7ger", "noch"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "$,", "ADJD", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "als der Tod, der vor uns schwebet,", "tokens": ["als", "der", "Tod", ",", "der", "vor", "uns", "schwe\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "der, wie sehr man nach ihm greift,", "tokens": ["der", ",", "wie", "sehr", "man", "nach", "ihm", "greift", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "ADV", "PIS", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "stetigs weicht und von uns l\u00e4uft.", "tokens": ["ste\u00b7tigs", "weicht", "und", "von", "uns", "l\u00e4uft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00dcber das so gro\u00dfe Leid,", "tokens": ["\u00dc\u00b7ber", "das", "so", "gro\u00b7\u00dfe", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "das der ungest\u00fcme W\u00fcrger", "tokens": ["das", "der", "un\u00b7ge\u00b7st\u00fc\u00b7me", "W\u00fcr\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "nun so eine lange Zeit", "tokens": ["nun", "so", "ei\u00b7ne", "lan\u00b7ge", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "gegen dich und seine B\u00fcrger", "tokens": ["ge\u00b7gen", "dich", "und", "sei\u00b7ne", "B\u00fcr\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "ausge\u00fcbt und noch h\u00e4lt an,", "tokens": ["aus\u00b7ge\u00b7\u00fcbt", "und", "noch", "h\u00e4lt", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "war dein gr\u00f6\u00dftes nicht getan.", "tokens": ["war", "dein", "gr\u00f6\u00df\u00b7tes", "nicht", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dein Raub aus so mancher Not,", "tokens": ["Dein", "Raub", "aus", "so", "man\u00b7cher", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADV", "PIAT", "NN", "$,"], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "dein Trost, deiner Jugend Freude,", "tokens": ["dein", "Trost", ",", "dei\u00b7ner", "Ju\u00b7gend", "Freu\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Alles ist auf einmal tot,", "tokens": ["Al\u00b7les", "ist", "auf", "ein\u00b7mal", "tot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "ihm zur Ruh' und dir zu Leide.", "tokens": ["ihm", "zur", "Ruh'", "und", "dir", "zu", "Lei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "KON", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Deine Liebsten von der Welt", "tokens": ["Dei\u00b7ne", "Liebs\u00b7ten", "von", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "hat das strenge Recht gef\u00e4llt.", "tokens": ["hat", "das", "stren\u00b7ge", "Recht", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Drei auf einmal ist zu viel:", "tokens": ["Drei", "auf", "ein\u00b7mal", "ist", "zu", "viel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "APPR", "ADV", "VAFIN", "PTKA", "PIS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vater, Mutter, Schwester fallen.", "tokens": ["Va\u00b7ter", ",", "Mut\u00b7ter", ",", "Schwes\u00b7ter", "fal\u00b7len", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was ist deiner Hoffnung Ziel,", "tokens": ["Was", "ist", "dei\u00b7ner", "Hoff\u00b7nung", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "du Betr\u00fcbtster unter Allen?", "tokens": ["du", "Be\u00b7tr\u00b7\u00fcbts\u00b7ter", "un\u00b7ter", "Al\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Eine Gruft hat sie und dich", "tokens": ["Ei\u00b7ne", "Gruft", "hat", "sie", "und", "dich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "KON", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "und den andern Dich auch, mich.", "tokens": ["und", "den", "an\u00b7dern", "Dich", "auch", ",", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "ADJA", "PPER", "ADV", "$,", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wie ist aber ihm zu tun?", "tokens": ["Wie", "ist", "a\u00b7ber", "ihm", "zu", "tun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was so hin ist, k\u00f6mmt nicht wieder.", "tokens": ["Was", "so", "hin", "ist", ",", "k\u00f6mmt", "nicht", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "VAFIN", "$,", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wol dem, der in sich kan ruhn,", "tokens": ["Wol", "dem", ",", "der", "in", "sich", "kan", "ruhn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "PRELS", "APPR", "PRF", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "der l\u00e4\u00dft seine Segel nieder,", "tokens": ["der", "l\u00e4\u00dft", "sei\u00b7ne", "Se\u00b7gel", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "wenn das Wetter hat sein Spiel", "tokens": ["wenn", "das", "Wet\u00b7ter", "hat", "sein", "Spiel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "und der Wind nicht fugen wil.", "tokens": ["und", "der", "Wind", "nicht", "fu\u00b7gen", "wil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "La\u00df den Zeiten ihren Lauf!", "tokens": ["La\u00df", "den", "Zei\u00b7ten", "ih\u00b7ren", "Lauf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was der Himmel hei\u00dft geschehen,", "tokens": ["Was", "der", "Him\u00b7mel", "hei\u00dft", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "das h\u00e4lt man vergebens auf.", "tokens": ["das", "h\u00e4lt", "man", "ver\u00b7ge\u00b7bens", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "ADV", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Auf den H\u00f6chsten mu\u00df man sehen,", "tokens": ["Auf", "den", "H\u00f6chs\u00b7ten", "mu\u00df", "man", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "der uns dreifach oft betr\u00fcbt,", "tokens": ["der", "uns", "drei\u00b7fach", "oft", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "weil er uns auch dreifach liebt.", "tokens": ["weil", "er", "uns", "auch", "drei\u00b7fach", "liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "PAV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Tr\u00f6ste dich und schau auf mich!", "tokens": ["Tr\u00f6s\u00b7te", "dich", "und", "schau", "auf", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "ADJD", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich verzeihe mich der Meinen.", "tokens": ["Ich", "ver\u00b7zei\u00b7he", "mich", "der", "Mei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Heute trifft das Elend dich;", "tokens": ["Heu\u00b7te", "trifft", "das", "E\u00b7lend", "dich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "ich vielleicht mu\u00df morgen weinen,", "tokens": ["ich", "viel\u00b7leicht", "mu\u00df", "mor\u00b7gen", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "bevoraus weil ich forthin", "tokens": ["be\u00b7vo\u00b7raus", "weil", "ich", "for\u00b7thin"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "weit von euch, ihr Lieben, bin.", "tokens": ["weit", "von", "euch", ",", "ihr", "Lie\u00b7ben", ",", "bin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "$,", "PPOSAT", "ADJA", "$,", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Mutter ", "tokens": ["Mut\u00b7ter"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Vater, Mutter, Schwester, Freunde,", "tokens": ["Va\u00b7ter", ",", "Mut\u00b7ter", ",", "Schwes\u00b7ter", ",", "Freun\u00b7de", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "mein! erlaubet di\u00df doch mir,", "tokens": ["mein", "!", "er\u00b7lau\u00b7bet", "di\u00df", "doch", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$.", "VVFIN", "PDS", "ADV", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "das ihr mehr w\u00fcndscht eurem Feinde,", "tokens": ["das", "ihr", "mehr", "w\u00fcnd\u00b7scht", "eu\u00b7rem", "Fein\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "da\u00df ich ferner L\u00e4nder Zier", "tokens": ["da\u00df", "ich", "fer\u00b7ner", "L\u00e4n\u00b7der", "Zier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "unserm ", "tokens": ["un\u00b7serm"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.10": {"line.1": {"text": "Ist mir Gott und Gl\u00fccke gut,", "tokens": ["Ist", "mir", "Gott", "und", "Gl\u00fc\u00b7cke", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "KON", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "da\u00df ich mit gelehrten K\u00fcssen,", "tokens": ["da\u00df", "ich", "mit", "ge\u00b7lehr\u00b7ten", "K\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "wie mein ", "tokens": ["wie", "mein"], "token_info": ["word", "word"], "pos": ["KOKOM", "PPOSAT"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "euch hinwieder soll begr\u00fc\u00dfen,", "tokens": ["euch", "hin\u00b7wie\u00b7der", "soll", "be\u00b7gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "denn soll meiner Verse Lust", "tokens": ["denn", "soll", "mei\u00b7ner", "Ver\u00b7se", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "auch bei Fremden sein bewu\u00dft.", "tokens": ["auch", "bei", "Frem\u00b7den", "sein", "be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PPOSAT", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Meint nicht, wie der P\u00f6fel spricht,", "tokens": ["Meint", "nicht", ",", "wie", "der", "P\u00f6\u00b7fel", "spricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mitternacht sei ganz ohn' Ehren,", "tokens": ["Mit\u00b7ter\u00b7nacht", "sei", "ganz", "ohn'", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Persien, das habe nicht,", "tokens": ["Per\u00b7si\u00b7en", ",", "das", "ha\u00b7be", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PDS", "VAFIN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "was uns k\u00f6nne Weisheit lehren!", "tokens": ["was", "uns", "k\u00f6n\u00b7ne", "Weis\u00b7heit", "leh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VMFIN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denkt, da\u00df in der Barbarei", "tokens": ["Denkt", ",", "da\u00df", 