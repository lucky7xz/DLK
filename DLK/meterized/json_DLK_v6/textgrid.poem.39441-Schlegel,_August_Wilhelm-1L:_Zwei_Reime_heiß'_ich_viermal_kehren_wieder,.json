{"textgrid.poem.39441": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zwei Reime hei\u00df' ich viermal kehren wieder,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei Reime hei\u00df' ich viermal kehren wieder,", "tokens": ["Zwei", "Rei\u00b7me", "hei\u00df'", "ich", "vier\u00b7mal", "keh\u00b7ren", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und stelle sie, getheilt, in gleiche Reihen,", "tokens": ["Und", "stel\u00b7le", "sie", ",", "ge\u00b7theilt", ",", "in", "glei\u00b7che", "Rei\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "VVPP", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df hier und dort zwei eingefa\u00dft von zweien", "tokens": ["Da\u00df", "hier", "und", "dort", "zwei", "ein\u00b7ge\u00b7fa\u00dft", "von", "zwei\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "KON", "ADV", "CARD", "VVPP", "APPR", "CARD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Doppelchore schweben auf und nieder.", "tokens": ["Im", "Dop\u00b7pel\u00b7cho\u00b7re", "schwe\u00b7ben", "auf", "und", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dann schlingt des Gleichlauts Kette durch zwei Glieder", "tokens": ["Dann", "schlingt", "des", "Gleich\u00b7lauts", "Ket\u00b7te", "durch", "zwei", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich freier wechselnd, jegliches von dreien.", "tokens": ["Sich", "frei\u00b7er", "wech\u00b7selnd", ",", "jeg\u00b7li\u00b7ches", "von", "drei\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ADJD", "$,", "ADJA", "APPR", "CARD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In solcher Ordnung, solcher Zahl gedeihen", "tokens": ["In", "sol\u00b7cher", "Ord\u00b7nung", ",", "sol\u00b7cher", "Zahl", "ge\u00b7dei\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die zartesten und stolzesten der Lieder.", "tokens": ["Die", "zar\u00b7tes\u00b7ten", "und", "stol\u00b7zes\u00b7ten", "der", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}}, "stanza.3": {"line.1": {"text": "Den werd' ich nie mit meinen Zeilen kr\u00e4nzen,", "tokens": ["Den", "werd'", "ich", "nie", "mit", "mei\u00b7nen", "Zei\u00b7len", "kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem eitle Spielerei mein Wesen d\u00fcnket,", "tokens": ["Dem", "eit\u00b7le", "Spie\u00b7le\u00b7rei", "mein", "We\u00b7sen", "d\u00fcn\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Eigensinn die k\u00fcnstlichen Gesetze.", "tokens": ["Und", "Ei\u00b7gen\u00b7sinn", "die", "k\u00fcnst\u00b7li\u00b7chen", "Ge\u00b7set\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch, wem in mir geheimer Zauber winket,", "tokens": ["Doch", ",", "wem", "in", "mir", "ge\u00b7hei\u00b7mer", "Zau\u00b7ber", "win\u00b7ket", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWS", "APPR", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem leih' ich Hoheit, F\u00fcll' in engen Gr\u00e4nzen.", "tokens": ["Dem", "leih'", "ich", "Ho\u00b7heit", ",", "F\u00fcll'", "in", "en\u00b7gen", "Gr\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "NN", "$,", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und reines Ebenma\u00df der Gegens\u00e4tze.", "tokens": ["Und", "rei\u00b7nes", "E\u00b7ben\u00b7ma\u00df", "der", "Ge\u00b7gen\u00b7s\u00e4t\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}