{"textgrid.poem.40946": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Die schw\u00e4bische Dichterschule", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwohin soll den Fu\u00df ich lenken, ich, ein fremder Wandersmann,", "tokens": ["\u00bb", "wo\u00b7hin", "soll", "den", "Fu\u00df", "ich", "len\u00b7ken", ",", "ich", ",", "ein", "frem\u00b7der", "Wan\u00b7ders\u00b7mann", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VMFIN", "ART", "NN", "PPER", "VVINF", "$,", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Da\u00df ich eure Dichterschule, gute Schwaben, finden kann?\u00ab", "tokens": ["Da\u00df", "ich", "eu\u00b7re", "Dich\u00b7ter\u00b7schu\u00b7le", ",", "gu\u00b7te", "Schwa\u00b7ben", ",", "fin\u00b7den", "kann", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "$,", "ADJA", "NN", "$,", "VVINF", "VMFIN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Fremder Wanderer! o gerne will ich solches sagen dir:", "tokens": ["Frem\u00b7der", "Wan\u00b7de\u00b7rer", "!", "o", "ger\u00b7ne", "will", "ich", "sol\u00b7ches", "sa\u00b7gen", "dir", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "FM", "ADV", "VMFIN", "PPER", "PIS", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Geh durch diese lichte Matten in das dunkle Waldrevier,", "tokens": ["Geh", "durch", "die\u00b7se", "lich\u00b7te", "Mat\u00b7ten", "in", "das", "dunk\u00b7le", "Wald\u00b7re\u00b7vier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PDAT", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wo die Tanne steht, die hohe, die als Mast einst schifft durchs Meer;", "tokens": ["Wo", "die", "Tan\u00b7ne", "steht", ",", "die", "ho\u00b7he", ",", "die", "als", "Mast", "einst", "schifft", "durchs", "Meer", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "ART", "ADJA", "$,", "PRELS", "KOUS", "NN", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Wo von Zweig zu Zweig sich schwinget singend lust'ger V\u00f6gel Heer;", "tokens": ["Wo", "von", "Zweig", "zu", "Zweig", "sich", "schwin\u00b7get", "sin\u00b7gend", "lust'\u00b7ger", "V\u00f6\u00b7gel", "Heer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "APPR", "NN", "PRF", "VVFIN", "ADJD", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Wo das Reh mit klaren Augen aus dem dunkeln Dickicht sieht", "tokens": ["Wo", "das", "Reh", "mit", "kla\u00b7ren", "Au\u00b7gen", "aus", "dem", "dun\u00b7keln", "Di\u00b7ckicht", "sieht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Und der Hirsch, der schlanke, setzet \u00fcber Felsen von Granit;", "tokens": ["Und", "der", "Hirsch", ",", "der", "schlan\u00b7ke", ",", "set\u00b7zet", "\u00fc\u00b7ber", "Fel\u00b7sen", "von", "Gra\u00b7nit", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "VVFIN", "$,", "VVFIN", "APPR", "NN", "APPR", "NE", "$."], "meter": "--+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Trete dann aus Waldes Dunkel, wo im goldnen Sonnenstrahl", "tokens": ["Tre\u00b7te", "dann", "aus", "Wal\u00b7des", "Dun\u00b7kel", ",", "wo", "im", "gold\u00b7nen", "Son\u00b7nen\u00b7strahl"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "NN", "NN", "$,", "PWAV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Gr\u00fc\u00dfen Berge dich voll Reben, Neckars Blau im tiefen Tal;", "tokens": ["Gr\u00fc\u00b7\u00dfen", "Ber\u00b7ge", "dich", "voll", "Re\u00b7ben", ",", "Ne\u00b7ckars", "Blau", "im", "tie\u00b7fen", "Tal", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "ADJD", "NN", "$,", "NN", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Wo ein goldnes Meer von \u00c4hren durch die Ebnen wogt und wallt,", "tokens": ["Wo", "ein", "gold\u00b7nes", "Meer", "von", "\u00c4h\u00b7ren", "durch", "die", "Eb\u00b7nen", "wogt", "und", "wallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "NN", "APPR", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Dr\u00fcber in den blauen L\u00fcften Jubelruf der Lerche schallt;", "tokens": ["Dr\u00fc\u00b7ber", "in", "den", "blau\u00b7en", "L\u00fcf\u00b7ten", "Ju\u00b7bel\u00b7ruf", "der", "Ler\u00b7che", "schallt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Wo der Winzer, wo der Schnitter singt ein Lied durch Berg und Flur:", "tokens": ["Wo", "der", "Win\u00b7zer", ",", "wo", "der", "Schnit\u00b7ter", "singt", "ein", "Lied", "durch", "Berg", "und", "Flur", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Da ist schw\u00e4b'scher Dichter Schule, und ihr Meister hei\u00dft \u2013 Natur!", "tokens": ["Da", "ist", "schw\u00e4b'\u00b7scher", "Dich\u00b7ter", "Schu\u00b7le", ",", "und", "ihr", "Meis\u00b7ter", "hei\u00dft", "\u2013", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJA", "NN", "NN", "$,", "KON", "PPOSAT", "NN", "VVFIN", "$(", "NN", "$."], "meter": "----+-+-+-+-+-+", "measure": "unknown.measure.hexa"}}}}}