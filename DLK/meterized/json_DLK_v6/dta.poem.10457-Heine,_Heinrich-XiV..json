{"dta.poem.10457": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XiV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Das Meer ergl\u00e4nzte weit hinaus,", "tokens": ["Das", "Meer", "er\u00b7gl\u00e4nz\u00b7te", "weit", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im letzten Abendscheine;", "tokens": ["Im", "letz\u00b7ten", "A\u00b7bend\u00b7schei\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir sa\u00dfen am einsamen Fischerhaus,", "tokens": ["Wir", "sa\u00b7\u00dfen", "am", "ein\u00b7sa\u00b7men", "Fi\u00b7scher\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wir sa\u00dfen stumm und alleine.", "tokens": ["Wir", "sa\u00b7\u00dfen", "stumm", "und", "al\u00b7lei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADV", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Nebel stieg, das Wasser schwoll,", "tokens": ["Der", "Ne\u00b7bel", "stieg", ",", "das", "Was\u00b7ser", "schwoll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die M\u00f6ve flog hin und wieder;", "tokens": ["Die", "M\u00f6\u00b7ve", "flog", "hin", "und", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "ADV", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Aus deinen Augen, liebevoll,", "tokens": ["Aus", "dei\u00b7nen", "Au\u00b7gen", ",", "lie\u00b7be\u00b7voll", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fielen die Thr\u00e4nen nieder.", "tokens": ["Fie\u00b7len", "die", "Thr\u00e4\u00b7nen", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Ich sah sie fallen auf deine Hand,", "tokens": ["Ich", "sah", "sie", "fal\u00b7len", "auf", "dei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und bin auf's Knie gesunken;", "tokens": ["Und", "bin", "auf's", "Knie", "ge\u00b7sun\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich hab' von deiner wei\u00dfen Hand", "tokens": ["Ich", "hab'", "von", "dei\u00b7ner", "wei\u00b7\u00dfen", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Thr\u00e4nen fortgetrunken.", "tokens": ["Die", "Thr\u00e4\u00b7nen", "fort\u00b7ge\u00b7trun\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Seit jener Stunde verzehrt sich mein Leib,", "tokens": ["Seit", "je\u00b7ner", "Stun\u00b7de", "ver\u00b7zehrt", "sich", "mein", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Seele stirbt vor Sehnen; \u2014", "tokens": ["Die", "See\u00b7le", "stirbt", "vor", "Seh\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mich hat das ungl\u00fcckseel'ge Weib", "tokens": ["Mich", "hat", "das", "un\u00b7gl\u00fcck\u00b7seel'\u00b7ge", "Weib"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vergiftet mit ihren Thr\u00e4nen.", "tokens": ["Ver\u00b7gif\u00b7tet", "mit", "ih\u00b7ren", "Thr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}