{"dta.poem.8421": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Nonne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Ein sch\u00f6ner junger Ritter", "tokens": ["Ein", "sch\u00f6\u00b7ner", "jun\u00b7ger", "Rit\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein M\u00e4dchen, das der Welt entfloh,", "tokens": ["Ein", "M\u00e4d\u00b7chen", ",", "das", "der", "Welt", "ent\u00b7floh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Troz Klosterthor und Gitter;", "tokens": ["Troz", "Klos\u00b7ter\u00b7thor", "und", "Git\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sprach viel von seiner Liebespein,", "tokens": ["Sprach", "viel", "von", "sei\u00b7ner", "Lie\u00b7be\u00b7spein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und schwur auf seinen Knieen,", "tokens": ["Und", "schwur", "auf", "sei\u00b7nen", "Kni\u00b7e\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.6": {"text": "Sie aus dem Kerker zu befrein,", "tokens": ["Sie", "aus", "dem", "Ker\u00b7ker", "zu", "be\u00b7fr\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und stets f\u00fcr sie zu gl\u00fchen.", "tokens": ["Und", "stets", "f\u00fcr", "sie", "zu", "gl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Bei diesem Muttergottesbild,", "tokens": ["Bei", "die\u00b7sem", "Mut\u00b7ter\u00b7got\u00b7tes\u00b7bild", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei diesem Jesuskinde,", "tokens": ["Bei", "die\u00b7sem", "Je\u00b7sus\u00b7kin\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das ihre Mutterarme f\u00fcllt,", "tokens": ["Das", "ih\u00b7re", "Mut\u00b7ter\u00b7ar\u00b7me", "f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schw\u00f6r' ichs dir, o Belinde!", "tokens": ["Schw\u00f6r'", "ichs", "dir", ",", "o", "Be\u00b7lin\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "$,", "FM", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Dir ist mein ganzes Herz geweiht,", "tokens": ["Dir", "ist", "mein", "gan\u00b7zes", "Herz", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So lang' ich Odem habe!", "tokens": ["So", "lang'", "ich", "O\u00b7dem", "ha\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "NN", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Bei meiner Seelen Seligkeit.", "tokens": ["Bei", "mei\u00b7ner", "See\u00b7len", "Se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dich lieb' ich bis zum Grabe!", "tokens": ["Dich", "lieb'", "ich", "bis", "zum", "Gra\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was glaubt ein armes M\u00e4dchen nicht,", "tokens": ["Was", "glaubt", "ein", "ar\u00b7mes", "M\u00e4d\u00b7chen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zumal in einer Zelle?", "tokens": ["Zu\u00b7mal", "in", "ei\u00b7ner", "Zel\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ach! sie vergass der Nonnenpflicht,", "tokens": ["Ach", "!", "sie", "ver\u00b7gass", "der", "Non\u00b7nen\u00b7pflicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Himmels und der H\u00f6lle.", "tokens": ["Des", "Him\u00b7mels", "und", "der", "H\u00f6l\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die, von den Engeln angeschaut,", "tokens": ["Die", ",", "von", "den", "En\u00b7geln", "an\u00b7ge\u00b7schaut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sich ihrem Jesu weihte,", "tokens": ["Sich", "ih\u00b7rem", "Je\u00b7su", "weih\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NE", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die reine sch\u00f6ne Gottesbraut", "tokens": ["Die", "rei\u00b7ne", "sch\u00f6\u00b7ne", "Got\u00b7tes\u00b7braut"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ward eines Frevlers Beute.", "tokens": ["Ward", "ei\u00b7nes", "Frev\u00b7lers", "Beu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Drauf wurde, wie die M\u00e4nner sind,", "tokens": ["Drauf", "wur\u00b7de", ",", "wie", "die", "M\u00e4n\u00b7ner", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "$,", "PWAV", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Herz von Stund' an lauer;", "tokens": ["Sein", "Herz", "von", "Stund'", "an", "lau\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er \u00fcberliess das arme Kind", "tokens": ["Er", "\u00fc\u00b7berl\u00b7iess", "das", "ar\u00b7me", "Kind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf ewig ihrer Trauer,", "tokens": ["Auf", "e\u00b7wig", "ih\u00b7rer", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vergass der alten Z\u00e4rtlichkeit", "tokens": ["Ver\u00b7gass", "der", "al\u00b7ten", "Z\u00e4rt\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und aller seiner Eide,", "tokens": ["Und", "al\u00b7ler", "sei\u00b7ner", "Ei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und flog im bunten Gallakleid", "tokens": ["Und", "flog", "im", "bun\u00b7ten", "Gal\u00b7lak\u00b7leid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nach neuer Augenweide;", "tokens": ["Nach", "neu\u00b7er", "Au\u00b7gen\u00b7wei\u00b7de", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Begann mit andern Weibern Reihn", "tokens": ["Be\u00b7gann", "mit", "an\u00b7dern", "Wei\u00b7bern", "Reihn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im kerzenhellen Saale,", "tokens": ["Im", "ker\u00b7zen\u00b7hel\u00b7len", "Saa\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gab andern Weibern Schmeichelein", "tokens": ["Gab", "an\u00b7dern", "Wei\u00b7bern", "Schmei\u00b7che\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Beim lauten Traubenmahle,", "tokens": ["Beim", "lau\u00b7ten", "Trau\u00b7ben\u00b7mah\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und r\u00fchmte sich des Minnegl\u00fccks", "tokens": ["Und", "r\u00fchm\u00b7te", "sich", "des", "Min\u00b7ne\u00b7gl\u00fccks"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bei seiner sch\u00f6nen Nonne,", "tokens": ["Bei", "sei\u00b7ner", "sch\u00f6\u00b7nen", "Non\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und jedes Kusses, jedes Blicks,", "tokens": ["Und", "je\u00b7des", "Kus\u00b7ses", ",", "je\u00b7des", "Blicks", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und jeder andern Wonne.", "tokens": ["Und", "je\u00b7der", "an\u00b7dern", "Won\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Nonne, voll von welscher Wut,", "tokens": ["Die", "Non\u00b7ne", ",", "voll", "von", "wel\u00b7scher", "Wut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "APPR", "PWAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Entgl\u00fcht' in ihrem Mute,", "tokens": ["Ent\u00b7gl\u00fcht'", "in", "ih\u00b7rem", "Mu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und sann auf nichts als Dolch und Blut,", "tokens": ["Und", "sann", "auf", "nichts", "als", "Dolch", "und", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIS", "KOKOM", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und tr\u00e4umte nur von Blute.", "tokens": ["Und", "tr\u00e4um\u00b7te", "nur", "von", "Blu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie dingte pl\u00f6zlich eine Schaar", "tokens": ["Sie", "ding\u00b7te", "pl\u00f6z\u00b7lich", "ei\u00b7ne", "Schaar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von wilden Meuchelm\u00f6rdern,", "tokens": ["Von", "wil\u00b7den", "Meu\u00b7chel\u00b7m\u00f6r\u00b7dern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Den Mann, der treulos worden war,", "tokens": ["Den", "Mann", ",", "der", "treu\u00b7los", "wor\u00b7den", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VAPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ins Todtenreich zu f\u00f6rdern.", "tokens": ["Ins", "Tod\u00b7ten\u00b7reich", "zu", "f\u00f6r\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die boren manches M\u00f6rderschwert", "tokens": ["Die", "bo\u00b7ren", "man\u00b7ches", "M\u00f6r\u00b7der\u00b7schwert"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In seine schwarze Seele:", "tokens": ["In", "sei\u00b7ne", "schwar\u00b7ze", "See\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sein schwarzer falscher Geist entf\u00e4hrt,", "tokens": ["Sein", "schwar\u00b7zer", "fal\u00b7scher", "Geist", "ent\u00b7f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Schwefeldampf der H\u00f6hle.", "tokens": ["Wie", "Schwe\u00b7fel\u00b7dampf", "der", "H\u00f6h\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er wimmert durch die Luft, wo sein", "tokens": ["Er", "wim\u00b7mert", "durch", "die", "Luft", ",", "wo", "sein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "PWAV", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Krallenteufel harret;", "tokens": ["Ein", "Kral\u00b7len\u00b7teu\u00b7fel", "har\u00b7ret", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Drauf ward sein blutendes Gebein", "tokens": ["Drauf", "ward", "sein", "blu\u00b7ten\u00b7des", "Ge\u00b7bein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In eine Gruft verscharret.", "tokens": ["In", "ei\u00b7ne", "Gruft", "ver\u00b7schar\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die Nonne flog, wie Nacht begann,", "tokens": ["Die", "Non\u00b7ne", "flog", ",", "wie", "Nacht", "be\u00b7gann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur kleinen Dorfkapelle,", "tokens": ["Zur", "klei\u00b7nen", "Dorf\u00b7ka\u00b7pel\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und riss den wunden Rittersmann", "tokens": ["Und", "riss", "den", "wun\u00b7den", "Rit\u00b7ters\u00b7mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus seiner Ruhestelle,", "tokens": ["Aus", "sei\u00b7ner", "Ru\u00b7he\u00b7stel\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Riss ihm das Bubenherz heraus,", "tokens": ["Riss", "ihm", "das", "Bu\u00b7ben\u00b7herz", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Recht ihren Zorn zu b\u00fcssen,", "tokens": ["Recht", "ih\u