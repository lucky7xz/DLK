{"textgrid.poem.43315": {"metadata": {"author": {"name": "Gerstenberg, Heinrich Wilhelm von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Jupiter liebte unter den Halbg\u00f6ttinnen keine so z\u00e4rtlich, als die Nymphe Parthenope.", "genre": "verse", "period": "N.A.", "pub_year": 1780, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jupiter liebte unter den Halbg\u00f6ttinnen keine so z\u00e4rtlich, als die Nymphe Parthenope.", "tokens": ["Ju\u00b7pi\u00b7ter", "lieb\u00b7te", "un\u00b7ter", "den", "Halb\u00b7g\u00f6t\u00b7tin\u00b7nen", "kei\u00b7ne", "so", "z\u00e4rt\u00b7lich", ",", "als", "die", "Nym\u00b7phe", "Par\u00b7the\u00b7no\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "PIAT", "ADV", "ADJD", "$,", "KOUS", "ART", "NN", "NE", "$."], "meter": "+--+-+--++--+--+-+-+-+--+", "measure": "iambic.octa.plus.invert"}}, "stanza.2": {"line.1": {"text": "Schon mancher goldne Tag war ihrem Scherz verflossen;", "tokens": ["Schon", "man\u00b7cher", "gold\u00b7ne", "Tag", "war", "ih\u00b7rem", "Scherz", "ver\u00b7flos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr Herz war keiner Lust, die Venus schenkt, verschlossen;", "tokens": ["Ihr", "Herz", "war", "kei\u00b7ner", "Lust", ",", "die", "Ve\u00b7nus", "schenkt", ",", "ver\u00b7schlos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIAT", "NN", "$,", "ART", "NN", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In ihrer K\u00fcsse Harmonie", "tokens": ["In", "ih\u00b7rer", "K\u00fcs\u00b7se", "Har\u00b7mo\u00b7nie"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Scholl noch ein Seufzer nie.", "tokens": ["Scholl", "noch", "ein", "Seuf\u00b7zer", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da erst verga\u00df der Gott zufrieden seinen Himmel,", "tokens": ["Da", "erst", "ver\u00b7ga\u00df", "der", "Gott", "zu\u00b7frie\u00b7den", "sei\u00b7nen", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Floh in den stillen Hayn vom pr\u00e4chtigen Get\u00fcmmel,", "tokens": ["Floh", "in", "den", "stil\u00b7len", "Hayn", "vom", "pr\u00e4ch\u00b7ti\u00b7gen", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sah seine Sch\u00f6n, und k\u00fc\u00dfte sie.", "tokens": ["Sah", "sei\u00b7ne", "Sch\u00f6n", ",", "und", "k\u00fc\u00df\u00b7te", "sie", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie war dem Gott, als er die\u00df sah;", "tokens": ["Wie", "war", "dem", "Gott", ",", "als", "er", "die\u00df", "sah", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "KOUS", "PPER", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bang und bet\u00e4ubt folgt er dem Faunen;", "tokens": ["Bang", "und", "be\u00b7t\u00e4ubt", "folgt", "er", "dem", "Fau\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und \u2013 falsche Nymphe! welch Erstaunen! \u2013", "tokens": ["Und", "\u2013", "fal\u00b7sche", "Nym\u00b7phe", "!", "welch", "Er\u00b7stau\u00b7nen", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "$(", "ADJA", "NN", "$.", "PWAT", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und was er f\u00fcrchtete, geschah.", "tokens": ["Und", "was", "er", "f\u00fcrch\u00b7te\u00b7te", ",", "ge\u00b7schah", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es lispelt Ku\u00df um Ku\u00df ins laute Spiel der Bl\u00e4tter,", "tokens": ["Es", "lis\u00b7pelt", "Ku\u00df", "um", "Ku\u00df", "ins", "lau\u00b7te", "Spiel", "der", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es lacht \u2013 Verruchteste! bebt vor dem Gott der G\u00f6tter!", "tokens": ["Es", "lacht", "\u2013", "Ver\u00b7ruch\u00b7tes\u00b7te", "!", "bebt", "vor", "dem", "Gott", "der", "G\u00f6t\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "NN", "$.", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Lacht nicht \u2013 Entsetzen, Schaam, langsame zitternde Wuth", "tokens": ["Lacht", "nicht", "\u2013", "Ent\u00b7set\u00b7zen", ",", "Schaam", ",", "lang\u00b7sa\u00b7me", "zit\u00b7tern\u00b7de", "Wuth"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "$(", "NN", "$,", "NN", "$,", "ADJA", "ADJA", "NN"], "meter": "---+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Pre\u00dft ihm das Aug hervor, schw\u00e4rzt sein Gesicht mit Blut.", "tokens": ["Pre\u00dft", "ihm", "das", "Aug", "her\u00b7vor", ",", "schw\u00e4rzt", "sein", "Ge\u00b7sicht", "mit", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da starrt der Gott \u2013 und jetzund flammt die Glut \u2013", "tokens": ["Da", "starrt", "der", "Gott", "\u2013", "und", "je\u00b7tzund", "flammt", "die", "Glut", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$(", "KON", "ADV", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und itzt steht er vor den Verr\u00e4thern;", "tokens": ["Und", "itzt", "steht", "er", "vor", "den", "Ver\u00b7r\u00e4\u00b7thern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Und beyde fliehn gleich scheuen Misseth\u00e4tern:", "tokens": ["Und", "bey\u00b7de", "fliehn", "gleich", "scheu\u00b7en", "Mis\u00b7set\u00b7h\u00e4\u00b7tern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Doch Jupitern entflieht man nicht!", "tokens": ["Doch", "Ju\u00b7pi\u00b7tern", "ent\u00b7flieht", "man", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PIS", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Er will \u2013 fest stehn sie da er spricht:", "tokens": ["Er", "will", "\u2013", "fest", "stehn", "sie", "da", "er", "spricht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$(", "ADJD", "VVFIN", "PPER", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "\u00bbdu Falsche, wagsts, beschimpfest mein Vertrauen \u2013", "tokens": ["\u00bb", "du", "Fal\u00b7sche", ",", "wagsts", ",", "be\u00b7schimp\u00b7fest", "mein", "Ver\u00b7trau\u00b7en", "\u2013"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "NN", "$,", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Geh! \u2013 Niedertr\u00e4chtige \u2013 buhl, und erweck nur Grauen,", "tokens": ["Geh", "!", "\u2013", "Nie\u00b7der\u00b7tr\u00e4ch\u00b7ti\u00b7ge", "\u2013", "buhl", ",", "und", "er\u00b7weck", "nur", "Grau\u00b7en", ","], "token_info": ["word", "punct", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "$(", "PIS", "$(", "NE", "$,", "KON", "ADJD", "ADV", "NN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Du Faun, geh hin; dich straf ich nicht:\u00ab", "tokens": ["Du", "Faun", ",", "geh", "hin", ";", "dich", "straf", "ich", "nicht", ":", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "NN", "$,", "VVFIN", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und eine Thr\u00e4ne, da er spricht,", "tokens": ["Und", "ei\u00b7ne", "Thr\u00e4\u00b7ne", ",", "da", "er", "spricht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Benetzt sein weggewandt Gesicht.", "tokens": ["Be\u00b7netzt", "sein", "weg\u00b7ge\u00b7wandt", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAINF", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Selbst im Wasser noch der Liebe Flammen leidet,", "tokens": ["Die", "Selbst", "im", "Was\u00b7ser", "noch", "der", "Lie\u00b7be", "Flam\u00b7men", "lei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ADV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Manch buhlerisches Lied umsonst gen Himmel schickt,", "tokens": ["Manch", "buh\u00b7le\u00b7ri\u00b7sches", "Lied", "um\u00b7sonst", "gen", "Him\u00b7mel", "schickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil jeder schauervoll sie meidet,", "tokens": ["Weil", "je\u00b7der", "schau\u00b7er\u00b7voll", "sie", "mei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die, wenn sie einen ja ber\u00fcckt,", "tokens": ["Die", ",", "wenn", "sie", "ei\u00b7nen", "ja", "be\u00b7r\u00fcckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "ART", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und nun erkennt, da\u00df sie ihn \u00e4ngstigt, nicht entz\u00fcckt,", "tokens": ["Und", "nun", "er\u00b7kennt", ",", "da\u00df", "sie", "ihn", "\u00e4ngs\u00b7tigt", ",", "nicht", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den ganzen Erebus in ihrer Seele leidet?", "tokens": ["Den", "gan\u00b7zen", "E\u00b7re\u00b7bus", "in", "ih\u00b7rer", "See\u00b7le", "lei\u00b7det", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Hast du die Elende gesehen? Es ist Parthenope; so hat Jupiter sie gestraft!", "tokens": ["Hast", "du", "die", "E\u00b7len\u00b7de", "ge\u00b7se\u00b7hen", "?", "Es", "ist", "Par\u00b7the\u00b7no\u00b7pe", ";", "so", "hat", "Ju\u00b7pi\u00b7ter", "sie", "ge\u00b7straft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$.", "PPER", "VAFIN", "NE", "$.", "ADV", "VAFIN", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+--+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}}}}