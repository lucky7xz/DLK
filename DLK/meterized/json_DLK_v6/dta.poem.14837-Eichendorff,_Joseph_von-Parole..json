{"dta.poem.14837": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Parole.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.71", "af:0.28"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Da sprangen vom Fels die Quellen,", "tokens": ["Da", "spran\u00b7gen", "vom", "Fels", "die", "Quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da flogen die V\u00f6glein in's Thal.", "tokens": ["Da", "flo\u00b7gen", "die", "V\u00f6\u00b7glein", "in's", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "\u201eund wo ihr ihn trefft, ihr Gesellen,", "tokens": ["\u201e", "und", "wo", "ihr", "ihn", "trefft", ",", "ihr", "Ge\u00b7sel\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "KON", "PWAV", "PPER", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Gr\u00fc\u00dft mir ihn tausendmal!\u201c", "tokens": ["Gr\u00fc\u00dft", "mir", "ihn", "tau\u00b7send\u00b7mal", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "$.", "$("], "meter": "++-+-+", "measure": "iambic.tri"}}}}}