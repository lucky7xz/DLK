{"textgrid.poem.49631": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Verehrter F\u00fcrst, mit Ihrer L\u00e4chlermiene", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verehrter F\u00fcrst, mit Ihrer L\u00e4chlermiene", "tokens": ["Ver\u00b7ehr\u00b7ter", "F\u00fcrst", ",", "mit", "Ih\u00b7rer", "L\u00e4ch\u00b7ler\u00b7mie\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wohin nur neigen Sie? Nach rechts? Nach links?", "tokens": ["Wo\u00b7hin", "nur", "nei\u00b7gen", "Sie", "?", "Nach", "rechts", "?", "Nach", "links", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "$.", "APPR", "ADV", "$.", "APPR", "ADV", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ach, wenn Ihr Kopf nicht so frisiert erschiene,", "tokens": ["Ach", ",", "wenn", "Ihr", "Kopf", "nicht", "so", "fri\u00b7siert", "er\u00b7schie\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPOSAT", "NN", "PTKNEG", "ADV", "ADJD", "ADJA", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Verglich' ich ihn mit jenem Haupt der Sphinx!", "tokens": ["Ver\u00b7glich'", "ich", "ihn", "mit", "je\u00b7nem", "Haupt", "der", "Sphinx", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "APPR", "PDAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Vergn\u00fcgter Pl\u00e4tscherer in Meereswogen,", "tokens": ["Ver\u00b7gn\u00fcg\u00b7ter", "Pl\u00e4t\u00b7sche\u00b7rer", "in", "Mee\u00b7res\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was ist an Ihnen eigentlich stabil?", "tokens": ["Was", "ist", "an", "Ih\u00b7nen", "ei\u00b7gent\u00b7lich", "sta\u00b7bil", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie sind ein Mensch gewordner Regenbogen", "tokens": ["Sie", "sind", "ein", "Mensch", "ge\u00b7word\u00b7ner", "Re\u00b7gen\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit Ihrem wechselvollen Farbenspiel.", "tokens": ["Mit", "Ih\u00b7rem", "wech\u00b7sel\u00b7vol\u00b7len", "Far\u00b7ben\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und bald in jenen, bald in den Kul\u00f6ren", "tokens": ["Und", "bald", "in", "je\u00b7nen", ",", "bald", "in", "den", "Ku\u00b7l\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PDAT", "$,", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und immer angepa\u00dft beweisen Sie", "tokens": ["Und", "im\u00b7mer", "an\u00b7ge\u00b7pa\u00dft", "be\u00b7wei\u00b7sen", "Sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVPP", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "An Dummen, die auf Sie noch gl\u00e4ubig h\u00f6ren:", "tokens": ["An", "Dum\u00b7men", ",", "die", "auf", "Sie", "noch", "gl\u00e4u\u00b7big", "h\u00f6\u00b7ren", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "APPR", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Vorteil der Gesinnungs-Mimikry.", "tokens": ["Den", "Vor\u00b7teil", "der", "Ge\u00b7sin\u00b7nungs\u00b7Mi\u00b7mi\u00b7kry", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wo ist noch Honig, liebenswerter Falter,", "tokens": ["Wo", "ist", "noch", "Ho\u00b7nig", ",", "lie\u00b7bens\u00b7wer\u00b7ter", "Fal\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr Sie zu naschen, heitres G\u00f6tterkind?", "tokens": ["F\u00fcr", "Sie", "zu", "na\u00b7schen", ",", "heit\u00b7res", "G\u00f6t\u00b7ter\u00b7kind", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wie \u2013 verst\u00e4ndnisvoller Gunsterhalter,", "tokens": ["Und", "wie", "\u2013", "ver\u00b7st\u00e4nd\u00b7nis\u00b7vol\u00b7ler", "Guns\u00b7ter\u00b7hal\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWAV", "$(", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie geht im Kaiserschlosse jetzt der Wind?", "tokens": ["Wie", "geht", "im", "Kai\u00b7ser\u00b7schlos\u00b7se", "jetzt", "der", "Wind", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPRART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wer holt sich Antwort von dem R\u00e4tselreichen?", "tokens": ["Wer", "holt", "sich", "Ant\u00b7wort", "von", "dem", "R\u00e4t\u00b7sel\u00b7rei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von diesem Sonderbaren an der See?", "tokens": ["Von", "die\u00b7sem", "Son\u00b7der\u00b7ba\u00b7ren", "an", "der", "See", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich m\u00f6chte ihn mit einer Sphinx vergleichen,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "ihn", "mit", "ei\u00b7ner", "Sphinx", "ver\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch immer st\u00f6rt mich seine Lausallee.", "tokens": ["Doch", "im\u00b7mer", "st\u00f6rt", "mich", "sei\u00b7ne", "Lau\u00b7sal\u00b7lee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}