{"textgrid.poem.42023": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Sehnsucht schwand mit ihren weichen Tr\u00e4umen,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sehnsucht schwand mit ihren weichen Tr\u00e4umen,", "tokens": ["Die", "Sehn\u00b7sucht", "schwand", "mit", "ih\u00b7ren", "wei\u00b7chen", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schon ist des Lenzes fr\u00fchster Ku\u00df vergl\u00fcht,", "tokens": ["Schon", "ist", "des", "Len\u00b7zes", "fr\u00fchs\u00b7ter", "Ku\u00df", "ver\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein kr\u00e4ftiger Geschlecht beginnt zu keimen,", "tokens": ["Ein", "kr\u00e4f\u00b7ti\u00b7ger", "Ge\u00b7schlecht", "be\u00b7ginnt", "zu", "kei\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der zarten Kindheit frisches Bild verbl\u00fcht.", "tokens": ["Der", "zar\u00b7ten", "Kind\u00b7heit", "fri\u00b7sches", "Bild", "ver\u00b7bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nur wenn erwacht in's jugendliche Leben", "tokens": ["Nur", "wenn", "er\u00b7wacht", "in's", "ju\u00b7gend\u00b7li\u00b7che", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "VVPP", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die duft'ge Welt den bl\u00fchnden Busen taucht,", "tokens": ["Die", "duft'\u00b7ge", "Welt", "den", "bl\u00fchn\u00b7den", "Bu\u00b7sen", "taucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Kann ahnungsvoll der reine Kelch sich heben,", "tokens": ["Kann", "ah\u00b7nungs\u00b7voll", "der", "rei\u00b7ne", "Kelch", "sich", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "ART", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Woraus das Wehn der geist'gen Liebe haucht.", "tokens": ["Wo\u00b7raus", "das", "Wehn", "der", "geist'\u00b7gen", "Lie\u00b7be", "haucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Drum wollten wir noch einmal dich begr\u00fc\u00dfen,", "tokens": ["Drum", "woll\u00b7ten", "wir", "noch", "ein\u00b7mal", "dich", "be\u00b7gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Eh schw\u00fcle Gluth die Silberwang' uns tr\u00fcbt;", "tokens": ["Eh", "schw\u00fc\u00b7le", "Gluth", "die", "Sil\u00b7ber\u00b7wang'", "uns", "tr\u00fcbt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O denke du, wenn andre Blumen sprie\u00dfen,", "tokens": ["O", "den\u00b7ke", "du", ",", "wenn", "and\u00b7re", "Blu\u00b7men", "sprie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "KOUS", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch oft an uns, die z\u00e4rtlich dich geliebt.", "tokens": ["Noch", "oft", "an", "uns", ",", "die", "z\u00e4rt\u00b7lich", "dich", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "$,", "PRELS", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Noch lebt's und keimt's auf \u00fcppigem Gefilde,", "tokens": ["Noch", "lebt's", "und", "keimt's", "auf", "\u00fcp\u00b7pi\u00b7gem", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "PIS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und Neues beut dir stets der Augenblick:", "tokens": ["Und", "Neu\u00b7es", "beut", "dir", "stets", "der", "Au\u00b7gen\u00b7blick", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Doch fruchtlos ruft die fr\u00fch'sten Traumgebilde", "tokens": ["Doch", "frucht\u00b7los", "ruft", "die", "fr\u00fch'\u00b7sten", "Traum\u00b7ge\u00b7bil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Das weiche Herz verlangend sich zur\u00fcck.", "tokens": ["Das", "wei\u00b7che", "Herz", "ver\u00b7lan\u00b7gend", "sich", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Gern w\u00e4ren wir mit heiterm Wort gekommen", "tokens": ["Gern", "w\u00e4\u00b7ren", "wir", "mit", "hei\u00b7term", "Wort", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und h\u00e4tten freundlich gern mit dir gekos't:", "tokens": ["Und", "h\u00e4t\u00b7ten", "freund\u00b7lich", "gern", "mit", "dir", "ge\u00b7kos't", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch ach, der Schein kann nie dem Herzen frommen,", "tokens": ["Doch", "ach", ",", "der", "Schein", "kann", "nie", "dem", "Her\u00b7zen", "from\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "ART", "NN", "VMFIN", "ADV", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Schmerz nur ist des Schmerzes einz'ger Trost.", "tokens": ["Und", "Schmerz", "nur", "ist", "des", "Schmer\u00b7zes", "einz'\u00b7ger", "Trost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch wirst du einst nicht selber zu uns sagen,", "tokens": ["Doch", "wirst", "du", "einst", "nicht", "sel\u00b7ber", "zu", "uns", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PTKNEG", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wenn unserm Kelch der letzte Duft entflieht:", "tokens": ["Wenn", "un\u00b7serm", "Kelch", "der", "letz\u00b7te", "Duft", "ent\u00b7flieht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Welkt, Bl\u00fcmlein, welkt, ich will euch nicht beklagen,", "tokens": ["Welkt", ",", "Bl\u00fcm\u00b7lein", ",", "welkt", ",", "ich", "will", "euch", "nicht", "be\u00b7kla\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ihr habt ja sch\u00f6n, wenn auch nur kurz, gebl\u00fcht.", "tokens": ["Ihr", "habt", "ja", "sch\u00f6n", ",", "wenn", "auch", "nur", "kurz", ",", "ge\u00b7bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "KOUS", "ADV", "ADV", "ADJD", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}