{"textgrid.poem.43115": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00fcsse mich, denn, ach! sie bluten", "tokens": ["K\u00fcs\u00b7se", "mich", ",", "denn", ",", "ach", "!", "sie", "blu\u00b7ten"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "$,", "KON", "$,", "ITJ", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle noch die alten Wunden,", "tokens": ["Al\u00b7le", "noch", "die", "al\u00b7ten", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00fcsse mich, da\u00df ich vergesse", "tokens": ["K\u00fcs\u00b7se", "mich", ",", "da\u00df", "ich", "ver\u00b7ges\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "KOUS", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle die verfluchten Stunden!", "tokens": ["Al\u00b7le", "die", "ver\u00b7fluch\u00b7ten", "Stun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00df mich von den s\u00fc\u00dfen Lippen", "tokens": ["La\u00df", "mich", "von", "den", "s\u00fc\u00b7\u00dfen", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wieder Gl\u00fcck und Liebe saugen,", "tokens": ["Wie\u00b7der", "Gl\u00fcck", "und", "Lie\u00b7be", "sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df mich sterben, \u00fcberstrahlet", "tokens": ["La\u00df", "mich", "ster\u00b7ben", ",", "\u00fc\u00b7bers\u00b7trah\u00b7let"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von dem Himmel deiner Augen!", "tokens": ["Von", "dem", "Him\u00b7mel", "dei\u00b7ner", "Au\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}