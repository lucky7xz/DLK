{"textgrid.poem.35855": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: O, so ruhe nun im Frieden", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.71", "da:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O, so ruhe nun im Frieden", "tokens": ["O", ",", "so", "ru\u00b7he", "nun", "im", "Frie\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "VVFIN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der stillen Felsenkluft! \u2013", "tokens": ["In", "der", "stil\u00b7len", "Fel\u00b7sen\u00b7kluft", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich bin auch mit Dir geschieden,", "tokens": ["Ich", "bin", "auch", "mit", "Dir", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine Gruft ist meine Gruft.", "tokens": ["Dei\u00b7ne", "Gruft", "ist", "mei\u00b7ne", "Gruft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Drau\u00dfen kalte Winde wehen,", "tokens": ["Drau\u00b7\u00dfen", "kal\u00b7te", "Win\u00b7de", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Welt ist leer und arm.", "tokens": ["Und", "die", "Welt", "ist", "leer", "und", "arm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Hei\u00df' mich nie von hinnen gehen,", "tokens": ["Hei\u00df'", "mich", "nie", "von", "hin\u00b7nen", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hier nur ist es lind und warm.", "tokens": ["Hier", "nur", "ist", "es", "lind", "und", "warm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Drau\u00dfen m\u00f6gen Menschen wandeln,", "tokens": ["Drau\u00b7\u00dfen", "m\u00f6\u00b7gen", "Men\u00b7schen", "wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenig k\u00fcmmert mich ihr Thun,", "tokens": ["We\u00b7nig", "k\u00fcm\u00b7mert", "mich", "ihr", "Thun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob sie reden, ob sie handeln;", "tokens": ["Ob", "sie", "re\u00b7den", ",", "ob", "sie", "han\u00b7deln", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "$,", "KOUS", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df in Deinem Grab mich ruhn.", "tokens": ["La\u00df", "in", "Dei\u00b7nem", "Grab", "mich", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Geht vor\u00fcber! Geht vor\u00fcber!", "tokens": ["Geht", "vor\u00b7\u00fc\u00b7ber", "!", "Geht", "vor\u00b7\u00fc\u00b7ber", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00e4lzt den Stein nicht von der Th\u00fcr;", "tokens": ["W\u00e4lzt", "den", "Stein", "nicht", "von", "der", "Th\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der hier ruht, Er ist mir lieber,", "tokens": ["Der", "hier", "ruht", ",", "Er", "ist", "mir", "lie\u00b7ber", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als der Himmel Pracht und Zier.", "tokens": ["Als", "der", "Him\u00b7mel", "Pracht", "und", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Gebt nur Acht, ihr r\u00fcst'gen W\u00e4chter,", "tokens": ["Gebt", "nur", "Acht", ",", "ihr", "r\u00fcst'\u00b7gen", "W\u00e4ch\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "CARD", "$,", "PPER", "VVFIN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seht mit scharfem Auge zu,", "tokens": ["Seht", "mit", "schar\u00b7fem", "Au\u00b7ge", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df nicht Zion's muntre T\u00f6chter", "tokens": ["Da\u00df", "nicht", "Zion's", "mun\u00b7tre", "T\u00f6ch\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "NE", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "St\u00f6ren meines Liebsten Ruh'.", "tokens": ["St\u00f6\u00b7ren", "mei\u00b7nes", "Liebs\u00b7ten", "Ruh'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Denn ich hab' Ihn nun gefunden,", "tokens": ["Denn", "ich", "hab'", "Ihn", "nun", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den ich liebe, meinen Freund,", "tokens": ["Den", "ich", "lie\u00b7be", ",", "mei\u00b7nen", "Freund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und im Kusse Seiner Wunden", "tokens": ["Und", "im", "Kus\u00b7se", "Sei\u00b7ner", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bin ich selig Ihm vereint.", "tokens": ["Bin", "ich", "se\u00b7lig", "Ihm", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und nun mu\u00df ich Ihn umfassen,", "tokens": ["Und", "nun", "mu\u00df", "ich", "Ihn", "um\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bin Ihm ewig nun gesellt;", "tokens": ["Bin", "Ihm", "e\u00b7wig", "nun", "ge\u00b7sellt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimmer kann ich von Ihm lassen,", "tokens": ["Nim\u00b7mer", "kann", "ich", "von", "Ihm", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ob auch f\u00e4llt und bricht die Welt. \u2013", "tokens": ["Ob", "auch", "f\u00e4llt", "und", "bricht", "die", "Welt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "KON", "VVFIN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Lehre mich, o Magdalena,", "tokens": ["Leh\u00b7re", "mich", ",", "o", "Mag\u00b7da\u00b7le\u00b7na", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "FM", "FM", "$,"], "meter": "+--+---+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Lieben Ihn, wie Du geliebt;", "tokens": ["Lie\u00b7ben", "Ihn", ",", "wie", "Du", "ge\u00b7liebt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "PPER", "$,", "PWAV", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob ich so Sein Herz vers\u00f6hne,", "tokens": ["Ob", "ich", "so", "Sein", "Herz", "ver\u00b7s\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das mein Leichtsinn einst betr\u00fcbt.", "tokens": ["Das", "mein", "Leicht\u00b7sinn", "einst", "be\u00b7tr\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Deine Demuth, Deine Treue,", "tokens": ["Dei\u00b7ne", "De\u00b7muth", ",", "Dei\u00b7ne", "Treu\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Deinen Schmerz erflehe ich.", "tokens": ["Dei\u00b7nen", "Schmerz", "er\u00b7fle\u00b7he", "ich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ganz in Wehmuth, ganz in Reue,", "tokens": ["Ganz", "in", "Weh\u00b7muth", ",", "ganz", "in", "Reu\u00b7e", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ganz in Liebe l\u00f6se mich.", "tokens": ["Ganz", "in", "Lie\u00b7be", "l\u00f6\u00b7se", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Dein Verlangen, Suchen, Sehnen,", "tokens": ["Dein", "Ver\u00b7lan\u00b7gen", ",", "Su\u00b7chen", ",", "Seh\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "All Dein B\u00fc\u00dfen, Deine Pein,", "tokens": ["All", "Dein", "B\u00fc\u00b7\u00dfen", ",", "Dei\u00b7ne", "Pein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine Seufzer, Deine Thr\u00e4nen", "tokens": ["Dei\u00b7ne", "Seuf\u00b7zer", ",", "Dei\u00b7ne", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sollen fort auch meine sein.", "tokens": ["Sol\u00b7len", "fort", "auch", "mei\u00b7ne", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKVZ", "ADV", "PPOSAT", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Ob ich so zu Ihm mag dringen,", "tokens": ["Ob", "ich", "so", "zu", "Ihm", "mag", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Neben Seinem Thron einst stehn,", "tokens": ["Ne\u00b7ben", "Sei\u00b7nem", "Thron", "einst", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob ich so Ihn mag erringen", "tokens": ["Ob", "ich", "so", "Ihn", "mag", "er\u00b7rin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sein g\u00f6ttlich Antlitz sehn!", "tokens": ["Und", "sein", "g\u00f6tt\u00b7lich", "Ant\u00b7litz", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}