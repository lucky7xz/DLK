mer", ",", "den", "F\u00fcrs\u00b7ten", "ein", "Bei\u00b7spiel", "zu", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ART", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Von allen gefolgt, von keinem erreicht,", "tokens": ["Von", "al\u00b7len", "ge\u00b7folgt", ",", "von", "kei\u00b7nem", "er\u00b7reicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVPP", "$,", "APPR", "PIS", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Unsterbliche Thaten zu thun.", "tokens": ["U\u00b7nsterb\u00b7li\u00b7che", "Tha\u00b7ten", "zu", "thun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+---+--+", "measure": "iambic.tri.chol"}}, "stanza.16": {"line.1": {"text": "Ha, lockender Ruf! Er folgt dir, er folgt!", "tokens": ["Ha", ",", "lo\u00b7cken\u00b7der", "Ruf", "!", "Er", "folgt", "dir", ",", "er", "folgt", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "NN", "$.", "PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ha, Geister der Ahnen! Er folgt euch, er folgt!", "tokens": ["Ha", ",", "Geis\u00b7ter", "der", "Ah\u00b7nen", "!", "Er", "folgt", "euch", ",", "er", "folgt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "ART", "NN", "$.", "PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Dort tr\u00e4gt ihn sein Ro\u00df in Wolken von Staub", "tokens": ["Dort", "tr\