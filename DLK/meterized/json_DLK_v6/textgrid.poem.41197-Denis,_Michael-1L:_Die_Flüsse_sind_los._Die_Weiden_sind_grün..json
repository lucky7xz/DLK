{"textgrid.poem.41197": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Fl\u00fcsse sind los. Die Weiden sind gr\u00fcn.", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Fl\u00fcsse sind los. Die Weiden sind gr\u00fcn.", "tokens": ["Die", "Fl\u00fcs\u00b7se", "sind", "los", ".", "Die", "Wei\u00b7den", "sind", "gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKVZ", "$.", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Nachtigall rufet den Gatten ins Laub.", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "ru\u00b7fet", "den", "Gat\u00b7ten", "ins", "Laub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Auf, Krieger! auf, auf! Und h\u00f6rest du nicht", "tokens": ["Auf", ",", "Krie\u00b7ger", "!", "auf", ",", "auf", "!", "Und", "h\u00f6\u00b7rest", "du", "nicht"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "$,", "NN", "$.", "PTKVZ", "$,", "PTKVZ", "$.", "KON", "VVFIN", "PPER", "PTKNEG"], "meter": "-+-++-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Den ehernen Ruf in's Gefild?", "tokens": ["Den", "e\u00b7her\u00b7nen", "Ruf", "in's", "Ge\u00b7fild", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Sch\u00f6n wirbelt der Ruf. Ihn h\u00f6ret dein Ro\u00df,", "tokens": ["Sch\u00f6n", "wir\u00b7belt", "der", "Ruf", ".", "Ihn", "h\u00f6\u00b7ret", "dein", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Und wiehert und stampfet. Auf, Krieger! entheb'", "tokens": ["Und", "wie\u00b7hert", "und", "stamp\u00b7fet", ".", "Auf", ",", "Krie\u00b7ger", "!", "ent\u00b7heb'"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$.", "APPR", "$,", "NN", "$.", "VVFIN"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dein Heldengeschmeid' der ruhigen Wand!", "tokens": ["Dein", "Hel\u00b7den\u00b7ge\u00b7schmeid'", "der", "ru\u00b7hi\u00b7gen", "Wand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Auf, schm\u00fccke dich, z\u00e4ume dein Ro\u00df!", "tokens": ["Auf", ",", "schm\u00fc\u00b7cke", "dich", ",", "z\u00e4u\u00b7me", "dein", "Ro\u00df", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Hinan in's Gefild! hinan in's Gefild!", "tokens": ["Hi\u00b7nan", "in's", "Ge\u00b7fild", "!", "hi\u00b7nan", "in's", "Ge\u00b7fild", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$.", "ADV", "APPRART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dort warten sie deiner die Lenker der Schlacht.", "tokens": ["Dort", "war\u00b7ten", "sie", "dei\u00b7ner", "die", "Len\u00b7ker", "der", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Du kennest sie l\u00e4ngst. Sie kennen auch dich.", "tokens": ["Du", "ken\u00b7nest", "sie", "l\u00e4ngst", ".", "Sie", "ken\u00b7nen", "auch", "dich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$.", "PPER", "VVFIN", "ADV", "PPER", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ihr saht euch in Feuer und Staub.", "tokens": ["Ihr", "saht", "euch", "in", "Feu\u00b7er", "und", "Staub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Hinan in's Gefild! hinan in's Gefild!", "tokens": ["Hi\u00b7nan", "in's", "Ge\u00b7fild", "!", "hi\u00b7nan", "in's", "Ge\u00b7fild", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$.", "ADV", "APPRART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Waffen und Heldenbem\u00fchungen Freund,", "tokens": ["Der", "Waf\u00b7fen", "und", "Hel\u00b7den\u00b7be\u00b7m\u00fc\u00b7hun\u00b7gen", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "NN", "$,"], "meter": "-+--+--++-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Liebling des Ruhm's, dein Vater und Haupt,", "tokens": ["Der", "Lieb\u00b7ling", "des", "Ruhm's", ",", "dein", "Va\u00b7ter", "und", "Haupt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "O Krieger! auch ", "tokens": ["O", "Krie\u00b7ger", "!", "auch"], "token_info": ["word", "word", "punct", "word"], "pos": ["NE", "NN", "$.", "ADV"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Im th\u00fcrmenden Wien, da sa\u00df er und schalt", "tokens": ["Im", "th\u00fcr\u00b7men\u00b7den", "Wi\u00b7en", ",", "da", "sa\u00df", "er", "und", "schalt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "KON", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Den z\u00f6gernden Winter, und w\u00fcnschte den Lenz,", "tokens": ["Den", "z\u00f6\u00b7gern\u00b7den", "Win\u00b7ter", ",", "und", "w\u00fcnschte", "den", "Lenz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und seufzte, sobald im Saale sein Aug'", "tokens": ["Und", "seufz\u00b7te", ",", "so\u00b7bald", "im", "Saa\u00b7le", "sein", "Aug'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ADV", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die V\u00e4ter im Panzer ersah.", "tokens": ["Die", "V\u00e4\u00b7ter", "im", "Pan\u00b7zer", "er\u00b7sah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Und schlief er, da war's, als kl\u00e4ng' ihm das Erz,", "tokens": ["Und", "schlief", "er", ",", "da", "wa\u00b7r's", ",", "als", "kl\u00e4ng'", "ihm", "das", "Erz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "ADV", "VAFIN", "$,", "KOUS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Als wehten die Fahnen, und blitzte der Stahl,", "tokens": ["Als", "weh\u00b7ten", "die", "Fah\u00b7nen", ",", "und", "blitz\u00b7te", "der", "Stahl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Als krachten um ihn die Donner der Schlacht.", "tokens": ["Als", "krach\u00b7ten", "um", "ihn", "die", "Don\u00b7ner", "der", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "O Krieger! er tr\u00e4umte von dir.", "tokens": ["O", "Krie\u00b7ger", "!", "er", "tr\u00e4um\u00b7te", "von", "dir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Nun kehrte der Lenz. Noch gab er den Ku\u00df", "tokens": ["Nun", "kehr\u00b7te", "der", "Lenz", ".", "Noch", "gab", "er", "den", "Ku\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der scheidenden Schwester", "tokens": ["Der", "schei\u00b7den\u00b7den", "Schwes\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und wandte sich weg, und eilte zu dir,", "tokens": ["Und", "wand\u00b7te", "sich", "weg", ",", "und", "eil\u00b7te", "zu", "dir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "O Krieger! in's Eisengefild.", "tokens": ["O", "Krie\u00b7ger", "!", "in's", "Ei\u00b7sen\u00b7ge\u00b7fild", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.8": {"line.1": {"text": "Hat etwa sein Wien nicht Reize genug,", "tokens": ["Hat", "et\u00b7wa", "sein", "Wi\u00b7en", "nicht", "Rei\u00b7ze", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "PTKNEG", "NN", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Nicht gold'ne Gerichte, nicht G\u00f6ttergetr\u00e4nk,", "tokens": ["Nicht", "gold'\u00b7ne", "Ge\u00b7rich\u00b7te", ",", "nicht", "G\u00f6t\u00b7ter\u00b7ge\u00b7tr\u00e4nk", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "$,", "PTKNEG", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Nicht Hallen, geschm\u00fcckt mit jeglicher Kunst,", "tokens": ["Nicht", "Hal\u00b7len", ",", "ge\u00b7schm\u00fcckt", "mit", "jeg\u00b7li\u00b7cher", "Kunst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "VVPP", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nicht T\u00e4nze, nicht Saitenget\u00f6n?", "tokens": ["Nicht", "T\u00e4n\u00b7ze", ",", "nicht", "Sai\u00b7ten\u00b7ge\u00b7t\u00f6n", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PTKNEG", "NE", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.9": {"line.1": {"text": "Und brannte nicht erst im feiernden Wien", "tokens": ["Und", "brann\u00b7te", "nicht", "erst", "im", "fei\u00b7ern\u00b7den", "Wi\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auf Joseph's erhabener Mutter Gebot", "tokens": ["Auf", "Jo\u00b7se\u00b7ph's", "er\u00b7ha\u00b7be\u00b7ner", "Mut\u00b7ter", "Ge\u00b7bot"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zur neidischen Nacht ein sterniger Bau,", "tokens": ["Zur", "nei\u00b7di\u00b7schen", "Nacht", "ein", "ster\u00b7ni\u00b7ger", "Bau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein sterniges Blumengefield'", "tokens": ["Ein", "ster\u00b7ni\u00b7ges", "Blu\u00b7men\u00b7ge\u00b7field'"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.10": {"line.1": {"text": "Und locket der Tag, hat Joseph nicht Lenz", "tokens": ["Und", "lo\u00b7cket", "der", "Tag", ",", "hat", "Jo\u00b7se\u00b7ph", "nicht", "Lenz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "VAFIN", "NE", "PTKNEG", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In deinem Gebiete, du feierndes Wien!", "tokens": ["In", "dei\u00b7nem", "Ge\u00b7bie\u00b7te", ",", "du", "fei\u00b7ern\u00b7des", "Wi\u00b7en", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Nicht V\u00f6gelgesang, und Fluthenger\u00e4usch,", "tokens": ["Nicht", "V\u00f6\u00b7gel\u00b7ge\u00b7sang", ",", "und", "Flut\u00b7hen\u00b7ge\u00b7r\u00e4usch", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "KON", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nicht Blumen, und Schattengew\u00f6lb'?", "tokens": ["Nicht", "Blu\u00b7men", ",", "und", "Schat\u00b7ten\u00b7ge\u00b7w\u00f6lb'", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "KON", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.11": {"line.1": {"text": "Und will er nicht ruh'n, und suchet er Schwei\u00df,", "tokens": ["Und", "will", "er", "nicht", "ruh'n", ",", "und", "su\u00b7chet", "er", "Schwei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,", "KON", "VVFIN", "PPER", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie? schallet kein Jagdruf? erhitzt ihn kein Hirsch?", "tokens": ["Wie", "?", "schal\u00b7let", "kein", "Jag\u00b7druf", "?", "er\u00b7hitzt", "ihn", "kein", "Hirsch", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "PIAT", "NN", "$.", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und fasset er nicht im Haine den Spie\u00df,", "tokens": ["Und", "fas\u00b7set", "er", "nicht", "im", "Hai\u00b7ne", "den", "Spie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und st\u00f6\u00dft ihn dem Eber in's Herz?", "tokens": ["Und", "st\u00f6\u00dft", "ihn", "dem", "E\u00b7ber", "in's", "Herz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.12": {"line.1": {"text": "Und ehrte sein Volk ihn minder darum?", "tokens": ["Und", "ehr\u00b7te", "sein", "Volk", "ihn", "min\u00b7der", "da\u00b7rum", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PPER", "ADV", "PAV", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und blieb' er nicht Herrscher der Deutschen auch dann?", "tokens": ["Und", "blieb'", "er", "nicht", "Herr\u00b7scher", "der", "Deut\u00b7schen", "auch", "dann", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "NN", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und freu'ten vor ihm sich F\u00fcrsten nicht so?", "tokens": ["Und", "freu'\u00b7ten", "vor", "ihm", "sich", "F\u00fcrs\u00b7ten", "nicht", "so", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "PRF", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und freuen sich F\u00fcrsten nicht so?", "tokens": ["Und", "freu\u00b7en", "sich", "F\u00fcrs\u00b7ten", "nicht", "so", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "NN", "PTKNEG", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.13": {"line.1": {"text": "Doch kann er? \u2013 Und wie, wie trieb' er von sich", "tokens": ["Doch", "kann", "er", "?", "\u2013", "Und", "wie", ",", "wie", "trieb'", "er", "von", "sich"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "$.", "$(", "KON", "PWAV", "$,", "PWAV", "VVFIN", "PPER", "APPR", "PRF"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Geister der Ahnen, sein helles Geleit?", "tokens": ["Die", "Geis\u00b7ter", "der", "Ah\u00b7nen", ",", "sein", "hel\u00b7les", "Ge\u00b7leit", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Sie schweben um ihn, und winken. Er schaut,", "tokens": ["Sie", "schwe\u00b7ben", "um", "ihn", ",", "und", "win\u00b7ken", ".", "Er", "schaut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "KON", "VVFIN", "$.", "PPER", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sieht Arbeit und Ehre vor sich.", "tokens": ["Sieht", "Ar\u00b7beit", "und", "Eh\u00b7re", "vor", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "APPR", "PRF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.14": {"line.1": {"text": "Wie zw\u00e4ng' er den Ruf, den m\u00e4chtigen Ruf,", "tokens": ["Wie", "zw\u00e4ng'",