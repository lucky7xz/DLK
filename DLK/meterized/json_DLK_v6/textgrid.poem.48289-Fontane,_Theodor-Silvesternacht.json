{"textgrid.poem.48289": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Silvesternacht", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Dorf ist still, still ist die Nacht,", "tokens": ["Das", "Dorf", "ist", "still", ",", "still", "ist", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Mutter schl\u00e4ft, die Tochter wacht,", "tokens": ["Die", "Mut\u00b7ter", "schl\u00e4ft", ",", "die", "Toch\u00b7ter", "wacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie deckt den Tisch, sie deckt f\u00fcr zwei,", "tokens": ["Sie", "deckt", "den", "Tisch", ",", "sie", "deckt", "f\u00fcr", "zwei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "APPR", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sehnt die Mitternacht herbei.", "tokens": ["Und", "sehnt", "die", "Mit\u00b7ter\u00b7nacht", "her\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wem gilt die Unruh? wem die Hast?", "tokens": ["Wem", "gilt", "die", "Un\u00b7ruh", "?", "wem", "die", "Hast", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$.", "PWS", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer ist der mittern\u00e4cht'ge Gast?", "tokens": ["Wer", "ist", "der", "mit\u00b7ter\u00b7n\u00e4cht'\u00b7ge", "Gast", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ob ihr sie fragt, sie kennt ihn nicht,", "tokens": ["Ob", "ihr", "sie", "fragt", ",", "sie", "kennt", "ihn", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie wei\u00df nur, was die Sage spricht.", "tokens": ["Sie", "wei\u00df", "nur", ",", "was", "die", "Sa\u00b7ge", "spricht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die spricht: Wenn wo ein M\u00e4dchen wacht", "tokens": ["Die", "spricht", ":", "Wenn", "wo", "ein", "M\u00e4d\u00b7chen", "wacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$.", "KOUS", "PWAV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um zw\u00f6lf in der Silvesternacht,", "tokens": ["Um", "zw\u00f6lf", "in", "der", "Sil\u00b7ves\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "APPR", "ART", "NN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Und wenn sie deckt den Tisch f\u00fcr ", "tokens": ["Und", "wenn", "sie", "deckt", "den", "Tisch", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ART", "NN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gewahrt sie, wer ihr K\u00fcnft'ger sei.", "tokens": ["Ge\u00b7wahrt", "sie", ",", "wer", "ihr", "K\u00fcnft'\u00b7ger", "sei", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWS", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und h\u00e4tt' ihn nie gesehn die Maid,", "tokens": ["Und", "h\u00e4tt'", "ihn", "nie", "ge\u00b7sehn", "die", "Maid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und w\u00e4r' er hundert Meilen weit,", "tokens": ["Und", "w\u00e4r'", "er", "hun\u00b7dert", "Mei\u00b7len", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "CARD", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er tritt herein und schickt sich an,", "tokens": ["Er", "tritt", "her\u00b7ein", "und", "schickt", "sich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und i\u00dft und trinkt, und scheidet dann. \u2013", "tokens": ["Und", "i\u00dft", "und", "trinkt", ",", "und", "schei\u00b7det", "dann", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$,", "KON", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Zw\u00f6lf schl\u00e4gt die Uhr, sie horcht erschreckt,", "tokens": ["Zw\u00f6lf", "schl\u00e4gt", "die", "Uhr", ",", "sie", "horcht", "er\u00b7schreckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie wollt', ihr Tisch w\u00e4r' ungedeckt,", "tokens": ["Sie", "wollt'", ",", "ihr", "Tisch", "w\u00e4r'", "un\u00b7ge\u00b7deckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es \u00fcberf\u00e4llt sie Angst und Graun,", "tokens": ["Es", "\u00fc\u00b7berf\u00b7\u00e4llt", "sie", "Angst", "und", "Graun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie will den Br\u00e4utigam nicht schaun.", "tokens": ["Sie", "will", "den", "Br\u00e4u\u00b7ti\u00b7gam", "nicht", "schaun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NE", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Fort setzt der Zeiger seinen Lauf,", "tokens": ["Fort", "setzt", "der", "Zei\u00b7ger", "sei\u00b7nen", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Niemand tritt ein, sie atmet auf,", "tokens": ["Nie\u00b7mand", "tritt", "ein", ",", "sie", "at\u00b7met", "auf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Sie starrt nicht l\u00e4nger auf die T\u00fcr \u2013", "tokens": ["Sie", "starrt", "nicht", "l\u00e4n\u00b7ger", "auf", "die", "T\u00fcr", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJD", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Herr Gott, da sitzt er neben ihr.", "tokens": ["Herr", "Gott", ",", "da", "sitzt", "er", "ne\u00b7ben", "ihr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Sein Aug' ist gl\u00fch, bla\u00df sein Gesicht,", "tokens": ["Sein", "Aug'", "ist", "gl\u00fch", ",", "bla\u00df", "sein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie sah ihn all' ihr Lebtag nicht,", "tokens": ["Sie", "sah", "ihn", "all'", "ihr", "Leb\u00b7tag", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er blitzt sie an und schenket ein", "tokens": ["Er", "blitzt", "sie", "an", "und", "schen\u00b7ket", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und spricht: \u00bbHeut Nacht noch bist du mein.", "tokens": ["Und", "spricht", ":", "\u00bb", "Heut", "Nacht", "noch", "bist", "du", "mein", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "ADV", "NN", "ADV", "VAFIN", "PPER", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ich bin ein st\u00fcrmischer Gesell',", "tokens": ["Ich", "bin", "ein", "st\u00fcr\u00b7mi\u00b7scher", "Ge\u00b7sell'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich w\u00e4hle rasch und freie schnell,", "tokens": ["Ich", "w\u00e4h\u00b7le", "rasch", "und", "frei\u00b7e", "schnell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich bin der Br\u00e4ut'gam, du die Braut,", "tokens": ["Ich", "bin", "der", "Br\u00e4ut'\u00b7gam", ",", "du", "die", "Braut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NE", "$,", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und bin der Priester, der uns traut.\u00ab", "tokens": ["Und", "bin", "der", "Pries\u00b7ter", ",", "der", "uns", "traut", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Er fa\u00dft sie um \u2013 ein einz'ger Schrei,", "tokens": ["Er", "fa\u00dft", "sie", "um", "\u2013", "ein", "einz'\u00b7ger", "Schrei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "$(", "ART", "ADJA", "NN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Die Mutter h\u00f6rt's und kommt herbei;", "tokens": ["Die", "Mut\u00b7ter", "h\u00f6rt's", "und", "kommt", "her\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu sp\u00e4t, versch\u00fcttet liegt der Wein,", "tokens": ["Zu", "sp\u00e4t", ",", "ver\u00b7sch\u00fct\u00b7tet", "liegt", "der", "Wein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "$,", "VVPP", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Tot ist die Tochter und \u2013 allein.", "tokens": ["Tot", "ist", "die", "Toch\u00b7ter", "und", "\u2013", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "KON", "$(", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}