{"dta.poem.1369": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Himmel-Schl\u00fcssel  \n oder  \n Geistliche Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Himmel/ der mit so viel Augen ob uns unerm\u00fcdet wacht/", "tokens": ["Him\u00b7mel", "/", "der", "mit", "so", "viel", "Au\u00b7gen", "ob", "uns", "un\u00b7er\u00b7m\u00fc\u00b7det", "wacht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ART", "APPR", "ADV", "PIAT", "NN", "KOUS", "PPER", "ADJD", "VVFIN", "$("], "meter": "+--+--+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Als du hell-begl\u00e4nzte Lichter steckest aus bey brauner", "tokens": ["Als", "du", "hell\u00b7be\u00b7gl\u00e4nz\u00b7te", "Lich\u00b7ter", "ste\u00b7ckest", "aus", "bey", "brau\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "APPR", "APPR", "ADJA"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "Nacht/", "tokens": ["Nacht", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Warum regnet auff die B\u00f6sen deines Segens Uberflu\u00df/", "tokens": ["Wa\u00b7rum", "reg\u00b7net", "auff", "die", "B\u00f6\u00b7sen", "dei\u00b7nes", "Se\u00b7gens", "U\u00b7ber\u00b7flu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Wenn die Unschuld d\u00fcrren Mangel/ harte Sorge tragen", "tokens": ["Wenn", "die", "Un\u00b7schuld", "d\u00fcr\u00b7ren", "Man\u00b7gel", "/", "har\u00b7te", "Sor\u00b7ge", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "$(", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "mu\u00df.", "tokens": ["mu\u00df", "."], "token_info": ["word", "punct"], "pos": ["VMFIN", "$."], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Aber was will ich ergr\u00fcnden deinen unersch\u00f6pfften Sinn/", "tokens": ["A\u00b7ber", "was", "will", "ich", "er\u00b7gr\u00fcn\u00b7den", "dei\u00b7nen", "un\u00b7er\u00b7sch\u00f6pff\u00b7ten", "Sinn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VMFIN", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Der ich nichts als Staub der Erden/ nichts als Rauch und", "tokens": ["Der", "ich", "nichts", "als", "Staub", "der", "Er\u00b7den", "/", "nichts", "als", "Rauch", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PIS", "KOKOM", "NN", "ART", "NN", "$(", "PIS", "KOKOM", "NN", "KON"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.9": {"text": "Schatten bin?", "tokens": ["Schat\u00b7ten", "bin", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "Kan nicht di\u00df/ was Freude scheinet/ andern Plag und Unge-", "tokens": ["Kan", "nicht", "di\u00df", "/", "was", "Freu\u00b7de", "schei\u00b7net", "/", "an\u00b7dern", "Plag", "und", "Un\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PTKNEG", "PDS", "$(", "PWS", "NN", "VVFIN", "$(", "ADJA", "NN", "KON", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.11": {"text": "deyn/", "tokens": ["deyn", "/"], "token_info": ["word", "punct"], "pos": ["PPOSAT", "$("], "meter": "+", "measure": "single.up"}, "line.12": {"text": "Was vor Ungl\u00fcck wird gehalten/ unser Nutz und Bestes", "tokens": ["Was", "vor", "Un\u00b7gl\u00fcck", "wird", "ge\u00b7hal\u00b7ten", "/", "un\u00b7ser", "Nutz", "und", "Bes\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "NN", "VAFIN", "VVPP", "$(", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.13": {"text": "seyn?", "tokens": ["seyn", "?"], "token_info": ["word", "punct"], "pos": ["VAINF", "$."], "meter": "+", "measure": "single.up"}, "line.14": {"text": "Ich bem\u00fche mich vergebens zu ergr\u00fcnden deinen Sinn/", "tokens": ["Ich", "be\u00b7m\u00fc\u00b7he", "mich", "ver\u00b7ge\u00b7bens", "zu", "er\u00b7gr\u00fcn\u00b7den", "dei\u00b7nen", "Sinn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.15": {"text": "Der ich nichts als Staub der Erden/ nichts als Rauch und", "tokens": ["Der", "ich", "nichts", "als", "Staub", "der", "Er\u00b7den", "/", "nichts", "als", "Rauch", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PIS", "KOKOM", "NN", "ART", "NN", "$(", "PIS", "KOKOM", "NN", "KON"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.16": {"text": "Schatten bin.", "tokens": ["Schat\u00b7ten", "bin", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}