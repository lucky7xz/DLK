{"textgrid.poem.47352": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Wie die Ceder", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie die Ceder will ich erheben", "tokens": ["Wie", "die", "Ce\u00b7der", "will", "ich", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "--+-++-+-", "measure": "anapaest.init"}, "line.2": {"text": "\u00dcber die Wolken hoch mein Haupt,", "tokens": ["\u00dc\u00b7ber", "die", "Wol\u00b7ken", "hoch", "mein", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Still in L\u00fcften des Himmels schweben,", "tokens": ["Still", "in", "L\u00fcf\u00b7ten", "des", "Him\u00b7mels", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Von Erdsorgen unangestaubt;", "tokens": ["Von", "Erd\u00b7sor\u00b7gen", "un\u00b7an\u00b7ge\u00b7staubt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Wenn dereinst mir das Gl\u00fcck erlaubt,", "tokens": ["Wenn", "de\u00b7reinst", "mir", "das", "Gl\u00fcck", "er\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Mich zur Einsamkeit zu begeben,", "tokens": ["Mich", "zur", "Ein\u00b7sam\u00b7keit", "zu", "be\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Zu entsagen dem Menschenleben,", "tokens": ["Zu", "ent\u00b7sa\u00b7gen", "dem", "Men\u00b7schen\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Das den Frieden der Seele raubt.", "tokens": ["Das", "den", "Frie\u00b7den", "der", "See\u00b7le", "raubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}