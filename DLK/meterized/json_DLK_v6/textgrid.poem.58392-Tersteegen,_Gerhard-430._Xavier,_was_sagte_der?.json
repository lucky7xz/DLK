{"textgrid.poem.58392": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "430. Xavier, was sagte der?", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbnoch mehr!\u00ab rief Xavier, wenn Gott ihm Leiden schickte;", "tokens": ["\u00bb", "noch", "mehr", "!", "\u00ab", "rief", "Xa\u00b7vier", ",", "wenn", "Gott", "ihm", "Lei\u00b7den", "schick\u00b7te", ";"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "$.", "$(", "VVFIN", "NE", "$,", "KOUS", "NN", "PPER", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "\u00bbes ist genug!\u00ab sprach er, wenn ihn die Gnad' erquickte.", "tokens": ["\u00bb", "es", "ist", "ge\u00b7nug", "!", "\u00ab", "sprach", "er", ",", "wenn", "ihn", "die", "Gnad'", "er\u00b7quick\u00b7te", "."], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "$.", "$(", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So k\u00fchne bin ich nicht, weil meine Liebe klein,", "tokens": ["So", "k\u00fch\u00b7ne", "bin", "ich", "nicht", ",", "weil", "mei\u00b7ne", "Lie\u00b7be", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VAFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch, schickt Gott gr\u00f6\u00dfre Kraft, so schick' er gr\u00f6\u00dfre Pein!", "tokens": ["Doch", ",", "schickt", "Gott", "gr\u00f6\u00df\u00b7re", "Kraft", ",", "so", "schick'", "er", "gr\u00f6\u00df\u00b7re", "Pein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "NN", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}