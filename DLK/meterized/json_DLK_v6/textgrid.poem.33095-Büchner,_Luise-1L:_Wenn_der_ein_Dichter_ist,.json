{"textgrid.poem.33095": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn der ein Dichter ist,", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn der ein Dichter ist,", "tokens": ["Wenn", "der", "ein", "Dich\u00b7ter", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dem, wenn der Mai erbl\u00fchet,", "tokens": ["Dem", ",", "wenn", "der", "Mai", "er\u00b7bl\u00fc\u00b7het", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Seele in der Brust", "tokens": ["Die", "See\u00b7le", "in", "der", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In Sehnsucht fast vergl\u00fchet,", "tokens": ["In", "Sehn\u00b7sucht", "fast", "ver\u00b7gl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der seine holde Pracht,", "tokens": ["Der", "sei\u00b7ne", "hol\u00b7de", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Den Jubel in den Hainen", "tokens": ["Den", "Ju\u00b7bel", "in", "den", "Hai\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Nur leis erwiedern kann", "tokens": ["Nur", "leis", "er\u00b7wie\u00b7dern", "kann"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVINF", "VMFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Mit schmerzlich s\u00fc\u00dfem Weinen;", "tokens": ["Mit", "schmerz\u00b7lich", "s\u00fc\u00b7\u00dfem", "Wei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn der ein Dichter ist,", "tokens": ["Wenn", "der", "ein", "Dich\u00b7ter", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Den Ehrfurcht tief durchbebet,", "tokens": ["Den", "Ehr\u00b7furcht", "tief", "durch\u00b7be\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo schwindelnd gro\u00df vor ihm", "tokens": ["Wo", "schwin\u00b7delnd", "gro\u00df", "vor", "ihm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADJD", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sich die Natur erhebet;", "tokens": ["Sich", "die", "Na\u00b7tur", "er\u00b7he\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dem fast der Athem stockt", "tokens": ["Dem", "fast", "der", "A\u00b7them", "stockt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und wankt des Fu\u00dfes St\u00e4rke,", "tokens": ["Und", "wankt", "des", "Fu\u00b7\u00dfes", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Vor eines Genius", "tokens": ["Vor", "ei\u00b7nes", "Ge\u00b7nius"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Erhab'nem Sch\u00f6pferwerke;", "tokens": ["Er\u00b7hab'\u00b7nem", "Sch\u00f6p\u00b7fer\u00b7wer\u00b7ke", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wenn der ein Dichter ist,", "tokens": ["Wenn", "der", "ein", "Dich\u00b7ter", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dess' Herz in Flammen lodert,", "tokens": ["Dess'", "Herz", "in", "Flam\u00b7men", "lo\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo Unterdr\u00fcckung herrscht", "tokens": ["Wo", "Un\u00b7ter\u00b7dr\u00fc\u00b7ckung", "herrscht"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und Unbill Rechte fodert,", "tokens": ["Und", "Un\u00b7bill", "Rech\u00b7te", "fo\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dem nach der Feder zuckt", "tokens": ["Dem", "nach", "der", "Fe\u00b7der", "zuckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die Hand, wie nach dem Schwerte,", "tokens": ["Die", "Hand", ",", "wie", "nach", "dem", "Schwer\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df das Gemeine tief", "tokens": ["Da\u00df", "das", "Ge\u00b7mei\u00b7ne", "tief"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Von ihm gez\u00fcchtigt werde;", "tokens": ["Von", "ihm", "ge\u00b7z\u00fcch\u00b7tigt", "wer\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wenn der ein Dichter ist,", "tokens": ["Wenn", "der", "ein", "Dich\u00b7ter", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Den jede Menschenklage,", "tokens": ["Den", "je\u00b7de", "Men\u00b7schen\u00b7kla\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den jedes fremde Leid", "tokens": ["Den", "je\u00b7des", "frem\u00b7de", "Leid"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Trifft wie mit eignem Schlage,", "tokens": ["Trifft", "wie", "mit", "eig\u00b7nem", "Schla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der keine Thr\u00e4ne sieht,", "tokens": ["Der", "kei\u00b7ne", "Thr\u00e4\u00b7ne", "sieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die er nicht mit mu\u00df weinen,", "tokens": ["Die", "er", "nicht", "mit", "mu\u00df", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "APPR", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und dem der eigne Schmerz", "tokens": ["Und", "dem", "der", "eig\u00b7ne", "Schmerz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Stets doppelt wird erscheinen;", "tokens": ["Stets", "dop\u00b7pelt", "wird", "er\u00b7schei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wenn der ein Dichter ist,", "tokens": ["Wenn", "der", "ein", "Dich\u00b7ter", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dem hei\u00df die Wange brennet,", "tokens": ["Dem", "hei\u00df", "die", "Wan\u00b7ge", "bren\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn man des Vaterlands", "tokens": ["Wenn", "man", "des", "Va\u00b7ter\u00b7lands"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Geliebten Namen nennet,", "tokens": ["Ge\u00b7lieb\u00b7ten", "Na\u00b7men", "nen\u00b7net", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.5": {"text": "Dem das entz\u00fcckte Herz", "tokens": ["Dem", "das", "ent\u00b7z\u00fcck\u00b7te", "Herz"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "In Wonne wollt' vergehen,", "tokens": ["In", "Won\u00b7ne", "wollt'", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wenn einmal k\u00f6nnte noch", "tokens": ["Wenn", "ein\u00b7mal", "k\u00f6nn\u00b7te", "noch"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "VMFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Er frei und gro\u00df es sehen!", "tokens": ["Er", "frei", "und", "gro\u00df", "es", "se\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O, Gott \u2013 nicht kann ich sp\u00fcren,", "tokens": ["O", ",", "Gott", "\u2013", "nicht", "kann", "ich", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$(", "PTKNEG", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ob ich in edler Form", "tokens": ["Ob", "ich", "in", "ed\u00b7ler", "Form"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wei\u00df fremdes Herz zu r\u00fchren,", "tokens": ["Wei\u00df", "frem\u00b7des", "Herz", "zu", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ob Geister m\u00e4cht'gen Schwungs", "tokens": ["Ob", "Geis\u00b7ter", "m\u00e4cht'\u00b7gen", "Schwungs"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein Geist empor kann raffen \u2013", "tokens": ["Mein", "Geist", "em\u00b7por", "kann", "raf\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Doch meine Seele hast", "tokens": ["Doch", "mei\u00b7ne", "See\u00b7le", "hast"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Zum Dichter du geschaffen!", "tokens": ["Zum", "Dich\u00b7ter", "du", "ge\u00b7schaf\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}