{"textgrid.poem.39164": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wohl t\u00fcrmen andre nach durchfochtnen Kriegen", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl t\u00fcrmen andre nach durchfochtnen Kriegen", "tokens": ["Wohl", "t\u00fcr\u00b7men", "and\u00b7re", "nach", "durch\u00b7focht\u00b7nen", "Krie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zur Ehrens\u00e4ule stolze Architraven:", "tokens": ["Zur", "Eh\u00b7ren\u00b7s\u00e4u\u00b7le", "stol\u00b7ze", "Ar\u00b7chit\u00b7ra\u00b7ven", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ich w\u00f6lbe jauchzend meinem Unterliegen", "tokens": ["Ich", "w\u00f6l\u00b7be", "jauch\u00b7zend", "mei\u00b7nem", "Un\u00b7ter\u00b7lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Siegesbogen jubelnder Oktaven;", "tokens": ["Den", "Sie\u00b7ges\u00b7bo\u00b7gen", "ju\u00b7beln\u00b7der", "Ok\u00b7ta\u00b7ven", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wohl singen andre von der Freiheit Siegen:", "tokens": ["Wohl", "sin\u00b7gen", "and\u00b7re", "von", "der", "Frei\u00b7heit", "Sie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ich spr\u00fche Hymnen, weil ich ward zum Sklaven,", "tokens": ["Ich", "spr\u00fc\u00b7he", "Hym\u00b7nen", ",", "weil", "ich", "ward", "zum", "Skla\u00b7ven", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "KOUS", "PPER", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ausflutend in melodischer Entz\u00fcckung", "tokens": ["Aus\u00b7flu\u00b7tend", "in", "me\u00b7lo\u00b7di\u00b7scher", "Ent\u00b7z\u00fc\u00b7ckung"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den ganzen Wonnesturm der Herzbedr\u00fcckung.", "tokens": ["Den", "gan\u00b7zen", "Won\u00b7nes\u00b7turm", "der", "Herz\u00b7be\u00b7dr\u00fc\u00b7ckung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ja, Deinen Siegestempel will ich bauen", "tokens": ["Ja", ",", "Dei\u00b7nen", "Sie\u00b7ge\u00b7stem\u00b7pel", "will", "ich", "bau\u00b7en"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In k\u00fchner S\u00e4ulenordnung der Terzine,", "tokens": ["In", "k\u00fch\u00b7ner", "S\u00e4u\u00b7le\u00b7nord\u00b7nung", "der", "Ter\u00b7zi\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Will in Sonetten meine Tr\u00e4nen tauen,", "tokens": ["Will", "in", "So\u00b7net\u00b7ten", "mei\u00b7ne", "Tr\u00e4\u00b7nen", "tau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Lieblingsmundart treuer Paladine;", "tokens": ["Der", "Lieb\u00b7lings\u00b7mun\u00b7dart", "treu\u00b7er", "Pa\u00b7la\u00b7di\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Entrei\u00dfen will ich diesen Erdenauen", "tokens": ["Ent\u00b7rei\u00b7\u00dfen", "will", "ich", "die\u00b7sen", "Er\u00b7de\u00b7nau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Den Abglanz Deiner Gottheit, Flordespine!", "tokens": ["Den", "Ab\u00b7glanz", "Dei\u00b7ner", "Got\u00b7theit", ",", "Flor\u00b7des\u00b7pi\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Dich rollen auf Akkorden von Toskana", "tokens": ["Dich", "rol\u00b7len", "auf", "Ak\u00b7kor\u00b7den", "von", "Tos\u00b7ka\u00b7na"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "NE"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Ins bunte Wogenschlo\u00df der Fee Morgana.", "tokens": ["Ins", "bun\u00b7te", "Wo\u00b7gen\u00b7schlo\u00df", "der", "Fee", "Mor\u00b7ga\u00b7na", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "NE", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Schon liegt am Strand der Kahn, der Dich empfange.", "tokens": ["Schon", "liegt", "am", "Strand", "der", "Kahn", ",", "der", "Dich", "emp\u00b7fan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Drum kost das Meer im zauberhaften Tone,", "tokens": ["Drum", "kost", "das", "Meer", "im", "zau\u00b7ber\u00b7haf\u00b7ten", "To\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Segel bl\u00e4ht sich unterm Windesdrange,", "tokens": ["Das", "Se\u00b7gel", "bl\u00e4ht", "sich", "un\u00b7term", "Win\u00b7des\u00b7dran\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Wimpel flattert von des Mastes Krone;", "tokens": ["Das", "Wim\u00b7pel", "flat\u00b7tert", "von", "des", "Mas\u00b7tes", "Kro\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das Ruder schl\u00e4gt die Flut im Wechselsange", "tokens": ["Das", "Ru\u00b7der", "schl\u00e4gt", "die", "Flut", "im", "Wech\u00b7sel\u00b7san\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und schwatzt Geschichten vom Dekamerone,", "tokens": ["Und", "schwatzt", "Ge\u00b7schich\u00b7ten", "vom", "De\u00b7ka\u00b7me\u00b7ro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Das Reich der Wasser glimmt in Rosenflammen,", "tokens": ["Das", "Reich", "der", "Was\u00b7ser", "glimmt", "in", "Ro\u00b7sen\u00b7flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und Meer und \u00c4ther schwimmt im Klang zusammen.", "tokens": ["Und", "Meer", "und", "\u00c4\u00b7ther", "schwimmt", "im", "Klang", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Du aber selbst! Du bist die Fee Morgane,", "tokens": ["Du", "a\u00b7ber", "selbst", "!", "Du", "bist", "die", "Fee", "Mor\u00b7ga\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "$.", "PPER", "VAFIN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die dieses Feuer purpurfarb entz\u00fcndet,", "tokens": ["Die", "die\u00b7ses", "Feu\u00b7er", "pur\u00b7purf\u00b7arb", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Hauch der Luft, den Wogenschlag am Kahne", "tokens": ["Den", "Hauch", "der", "Luft", ",", "den", "Wo\u00b7gen\u00b7schlag", "am", "Kah\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In Wort und Reim zur Harmonie gegr\u00fcndet;", "tokens": ["In", "Wort", "und", "Reim", "zur", "Har\u00b7mo\u00b7nie", "ge\u00b7gr\u00fcn\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du sch\u00f6ne Herrscherin der Ozeane,", "tokens": ["Du", "sch\u00f6\u00b7ne", "Herr\u00b7sche\u00b7rin", "der", "O\u00b7zea\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die Meere z\u00e4hmt und St\u00fcrmen Rhythmus k\u00fcndet,", "tokens": ["Die", "Mee\u00b7re", "z\u00e4hmt", "und", "St\u00fcr\u00b7men", "Rhyth\u00b7mus", "k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Du segle fliehend aus dem Reich der Prose", "tokens": ["Du", "seg\u00b7le", "flie\u00b7hend", "aus", "dem", "Reich", "der", "Pro\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mit mir zum Blumenstrand der S\u00fcdlandsrose.", "tokens": ["Mit", "mir", "zum", "Blu\u00b7men\u00b7strand", "der", "S\u00fcd\u00b7lands\u00b7ro\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}