{"textgrid.poem.34237": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Text und Commentar", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht verbinde das Maul dem Ochsen, wenn er dir drischet!", "tokens": ["Nicht", "ver\u00b7bin\u00b7de", "das", "Maul", "dem", "Och\u00b7sen", ",", "wenn", "er", "dir", "dri\u00b7schet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ART", "NN", "ART", "NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Also sagte der Herr, da er auf Sinai stand.", "tokens": ["Al\u00b7so", "sag\u00b7te", "der", "Herr", ",", "da", "er", "auf", "Si\u00b7nai", "stand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "APPR", "NE", "VVFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Aber, mi\u00dfbraucht er die Freiheit, erfrecht er sich, Aehren zu fressen,", "tokens": ["A\u00b7ber", ",", "mi\u00df\u00b7braucht", "er", "die", "Frei\u00b7heit", ",", "er\u00b7frecht", "er", "sich", ",", "A\u00b7eh\u00b7ren", "zu", "fres\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "ART", "NN", "$,", "VVFIN", "PPER", "PRF", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+--+--+-+-+--+-", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Gieb ihm einen darauf! Also erl\u00e4utert's der Mensch.", "tokens": ["Gieb", "ihm", "ei\u00b7nen", "da\u00b7rauf", "!", "Al\u00b7so", "er\u00b7l\u00e4u\u00b7tert's", "der", "Mensch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "PAV", "$.", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}}}}}