{"textgrid.poem.53076": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Officiosus Amor", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nimfe, gieb mir selbst den Mund,", "tokens": ["Nim\u00b7fe", ",", "gieb", "mir", "selbst", "den", "Mund", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVIMP", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "So wird mir dein Hertze kundt,", "tokens": ["So", "wird", "mir", "dein", "Hert\u00b7ze", "kundt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Reich mir deiner Armen Band,", "tokens": ["Reich", "mir", "dei\u00b7ner", "Ar\u00b7men", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der gew\u00fcnschten Liebe Pfand!", "tokens": ["Der", "ge\u00b7w\u00fcnschten", "Lie\u00b7be", "Pfand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}}, "stanza.2": {"line.1": {"text": "Denn, so lange du noch nicht", "tokens": ["Denn", ",", "so", "lan\u00b7ge", "du", "noch", "nicht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "ADV", "ADV", "PPER", "ADV", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir gehorchen wirst, mein Liecht,", "tokens": ["Mir", "ge\u00b7hor\u00b7chen", "wirst", ",", "mein", "Liecht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VAFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird dein Lieben nur ein Schein", "tokens": ["Wird", "dein", "Lie\u00b7ben", "nur", "ein", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "ADJA", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vnd vor nichts zu achten seyn.", "tokens": ["Vnd", "vor", "nichts", "zu", "ach\u00b7ten", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "PTKZU", "VVINF", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Trewe Lieb' ist jederzeit", "tokens": ["Tre\u00b7we", "Lieb'", "ist", "je\u00b7der\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VAFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu gehorsamen bereit,", "tokens": ["Zu", "ge\u00b7hor\u00b7sa\u00b7men", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat Ihr Thun gerichtet hinn", "tokens": ["Hat", "Ihr", "Thun", "ge\u00b7rich\u00b7tet", "hinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auff des Liebsten Hertz vnd Sinn.", "tokens": ["Auff", "des", "Liebs\u00b7ten", "Hertz", "vnd", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Glut bricht von sich selbst hervor", "tokens": ["Glut", "bricht", "von", "sich", "selbst", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PRF", "ADV", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vnd st\u00f6sst jhre Flamm empor,", "tokens": ["Vnd", "st\u00f6sst", "jhre", "Flamm", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo sich Rauch vnd Dampff nur findt,", "tokens": ["Wo", "sich", "Rauch", "vnd", "Dampff", "nur", "findt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NN", "KON", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00df vergehn durch Lufft vnd Wind.", "tokens": ["Mu\u00df", "ver\u00b7gehn", "durch", "Lufft", "vnd", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sch\u00e4mst du aber dich vor mir,", "tokens": ["Sch\u00e4mst", "du", "a\u00b7ber", "dich", "vor", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "So gedencke, meine Zier,", "tokens": ["So", "ge\u00b7den\u00b7cke", ",", "mei\u00b7ne", "Zier", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich das bin, was du bist,", "tokens": ["Da\u00df", "ich", "das", "bin", ",", "was", "du", "bist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VAFIN", "$,", "PWS", "PPER", "VAFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Vnd werd' jetzt nicht erst gek\u00fcsst!", "tokens": ["Vnd", "werd'", "jetzt", "nicht", "erst", "ge\u00b7k\u00fcsst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Wo ich mich, gleich wie du wol", "tokens": ["Wo", "ich", "mich", ",", "gleich", "wie", "du", "wol"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "$,", "ADV", "KOKOM", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch mit andern sch\u00e4men sol,", "tokens": ["Auch", "mit", "an\u00b7dern", "sch\u00e4\u00b7men", "sol", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIS", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00fcrde nicht die gantze Welt", "tokens": ["W\u00fcr\u00b7de", "nicht", "die", "gant\u00b7ze", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In gar kurtzer Zeit gef\u00e4llt?", "tokens": ["In", "gar", "kurt\u00b7zer", "Zeit", "ge\u00b7f\u00e4llt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Venus hat sich, wie bekannt,", "tokens": ["Ve\u00b7nus", "hat", "sich", ",", "wie", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PRF", "$,", "PWAV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zum Adonis selbst gewandt,", "tokens": ["Zum", "A\u00b7do\u00b7nis", "selbst", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Vnd mit jhm so manche Nacht", "tokens": ["Vnd", "mit", "jhm", "so", "man\u00b7che", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "ADV", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Liebe zugebracht.", "tokens": ["In", "der", "Lie\u00b7be", "zu\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Komm, der Mond am Firmament", "tokens": ["Komm", ",", "der", "Mond", "am", "Fir\u00b7ma\u00b7ment"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat sich schon zu vns gewendt,", "tokens": ["Hat", "sich", "schon", "zu", "vns", "ge\u00b7wendt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Komm, die Nacht k\u00f6mpt auch heran", "tokens": ["Komm", ",", "die", "Nacht", "k\u00f6mpt", "auch", "he\u00b7ran"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "ART", "NN", "VVFIN", "ADV", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da sich k\u00fcsset was nur kan!", "tokens": ["Da", "sich", "k\u00fcs\u00b7set", "was", "nur", "kan", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "VVFIN", "PWS", "ADV", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Morgen, h\u00f6r ich, wilst du fort", "tokens": ["Mor\u00b7gen", ",", "h\u00f6r", "ich", ",", "wilst", "du", "fort"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "VMFIN", "PPER", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Von vns an ein frembdes Orth,", "tokens": ["Von", "vns", "an", "ein", "fremb\u00b7des", "Orth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vnd wer wei\u00df auff welchen Tag", "tokens": ["Vnd", "wer", "wei\u00df", "auff", "wel\u00b7chen", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VVFIN", "APPR", "PWAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich dich wieder sprechen mag;", "tokens": ["Ich", "dich", "wie\u00b7der", "spre\u00b7chen", "mag", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Darumb Hertz mich ohne schew,", "tokens": ["Da\u00b7rumb", "Hertz", "mich", "oh\u00b7ne", "schew", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "PPER", "APPR", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich deiner Inndenck sey!", "tokens": ["Da\u00df", "ich", "dei\u00b7ner", "I\u00b7nndenck", "sey", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich bitt' einmal noch jetzund:", "tokens": ["Ich", "bitt'", "ein\u00b7mal", "noch", "je\u00b7tzund", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Nymfe, gib mir selbst den Mund!", "tokens": ["Nym\u00b7fe", ",", "gib", "mir", "selbst", "den", "Mund", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}