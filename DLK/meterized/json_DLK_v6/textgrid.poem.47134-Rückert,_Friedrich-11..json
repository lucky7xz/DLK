{"textgrid.poem.47134": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr! die Sch\u00f6nheit dieser Erde,", "tokens": ["Herr", "!", "die", "Sch\u00f6n\u00b7heit", "die\u00b7ser", "Er\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gib, da\u00df sie die Sehkraft wecke", "tokens": ["Gib", ",", "da\u00df", "sie", "die", "Seh\u00b7kraft", "we\u00b7cke"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Meines Auges, nicht ihm werde", "tokens": ["Mei\u00b7nes", "Au\u00b7ges", ",", "nicht", "ihm", "wer\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PTKNEG", "PPER", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine Blindheits-Zauberdecke.", "tokens": ["Ei\u00b7ne", "Blind\u00b7heits\u00b7Zau\u00b7ber\u00b7de\u00b7cke", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Jeden Blumenstrahl der Auen", "tokens": ["Je\u00b7den", "Blu\u00b7men\u00b7strahl", "der", "Au\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df der Seele dazu dienen,", "tokens": ["La\u00df", "der", "See\u00b7le", "da\u00b7zu", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PAV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Neu gekr\u00e4ftigt aufzuschauen", "tokens": ["Neu", "ge\u00b7kr\u00e4f\u00b7tigt", "auf\u00b7zu\u00b7schau\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "VVPP", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dorthin, wo die Sonn' erschienen.", "tokens": ["Dor\u00b7thin", ",", "wo", "die", "Sonn'", "er\u00b7schie\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}