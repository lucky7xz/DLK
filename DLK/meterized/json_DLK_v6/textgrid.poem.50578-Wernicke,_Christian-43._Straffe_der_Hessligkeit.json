{"textgrid.poem.50578": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "43. Straffe der Hessligkeit", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor ", "tokens": ["Vor"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Als nun der ", "tokens": ["Als", "nun", "der"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADV", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Voraus vor seinen Dienern kam,", "tokens": ["Vo\u00b7raus", "vor", "sei\u00b7nen", "Die\u00b7nern", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihn der ", "tokens": ["Und", "ihn", "der"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Tragt Holtz in das Gemach, sprach er, und eilt geschwinde,", "tokens": ["Tragt", "Holtz", "in", "das", "Ge\u00b7mach", ",", "sprach", "er", ",", "und", "eilt", "ge\u00b7schwin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "ART", "NN", "$,", "VVFIN", "PPER", "$,", "KON", "VVFIN", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dass, wenn er kommt, eur Herr ein warmes Zimmer finde.", "tokens": ["Dass", ",", "wenn", "er", "kommt", ",", "eur", "Herr", "ein", "war\u00b7mes", "Zim\u00b7mer", "fin\u00b7de", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "KOUS", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So kam sein Volck schon an. Es d\u00fcnckt euch wunderbar,", "tokens": ["So", "kam", "sein", "Volck", "schon", "an", ".", "Es", "d\u00fcnckt", "euch", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sagt' er, dass ihr mich itzt versp\u00fchret", "tokens": ["Sagt'", "er", ",", "dass", "ihr", "mich", "itzt", "ver\u00b7sp\u00fch\u00b7ret"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "ADV", "VVFIN"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.9": {"text": "In Etwas, ", "tokens": ["In", "Et\u00b7was", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "ADV", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Kennt' ihr mich nicht ihr gute Leut'?", "tokens": ["Kennt'", "ihr", "mich", "nicht", "ihr", "gu\u00b7te", "Leut'", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKNEG", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}