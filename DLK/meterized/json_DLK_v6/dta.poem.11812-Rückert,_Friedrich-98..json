{"dta.poem.11812": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "98.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Blick her, o Welt, was soll von dir die Nachwelt denken,", "tokens": ["Blick", "her", ",", "o", "Welt", ",", "was", "soll", "von", "dir", "die", "Nach\u00b7welt", "den\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "FM", "NN", "$,", "PWS", "VMFIN", "APPR", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn deine Maler ihr von dir dies Zerrbild schenken?", "tokens": ["Wenn", "dei\u00b7ne", "Ma\u00b7ler", "ihr", "von", "dir", "dies", "Zerr\u00b7bild", "schen\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "APPR", "PPER", "PDS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "In jedem Zuge Streit und Unzufriedenheit,", "tokens": ["In", "je\u00b7dem", "Zu\u00b7ge", "Streit", "und", "Un\u00b7zu\u00b7frie\u00b7den\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Krampf, Spannung, Unnatur und Uebertriebenheit!", "tokens": ["Krampf", ",", "Span\u00b7nung", ",", "Un\u00b7na\u00b7tur", "und", "Ue\u00b7ber\u00b7trie\u00b7ben\u00b7heit", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und willst du Beifall wol daf\u00fcr den Pfuschern schenken,", "tokens": ["Und", "willst", "du", "Bei\u00b7fall", "wol", "da\u00b7f\u00fcr", "den", "Pfu\u00b7schern", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "NN", "ADV", "PAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die die Geberden dir verzerren und verrenken?", "tokens": ["Die", "die", "Ge\u00b7ber\u00b7den", "dir", "ver\u00b7zer\u00b7ren", "und", "ver\u00b7ren\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Dir selbst gefallen gar in den entstellten Mienen,", "tokens": ["Dir", "selbst", "ge\u00b7fal\u00b7len", "gar", "in", "den", "ent\u00b7stell\u00b7ten", "Mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und werden gleich dem Bild, in dem du dir erschienen?", "tokens": ["Und", "wer\u00b7den", "gleich", "dem", "Bild", ",", "in", "dem", "du", "dir", "er\u00b7schie\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Blick her, o Welt, ich will ein sch\u00f6nres Bild dir zeigen,", "tokens": ["Blick", "her", ",", "o", "Welt", ",", "ich", "will", "ein", "sch\u00f6n\u00b7res", "Bild", "dir", "zei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "FM", "NN", "$,", "PPER", "VMFIN", "ART", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und bist du selbst es nicht, so mach' es dir zu eigen.", "tokens": ["Und", "bist", "du", "selbst", "es", "nicht", ",", "so", "mach'", "es", "dir", "zu", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PPER", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "PPER", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Sieh, da\u00df du heiter seyn, da\u00df du auch l\u00e4cheln kannst!", "tokens": ["Sieh", ",", "da\u00df", "du", "hei\u00b7ter", "seyn", ",", "da\u00df", "du", "auch", "l\u00e4\u00b7cheln", "kannst", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "ADJD", "VAINF", "$,", "KOUS", "PPER", "ADV", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und habe lieb das Bild, bis du dich lieb gewannst.", "tokens": ["Und", "ha\u00b7be", "lieb", "das", "Bild", ",", "bis", "du", "dich", "lieb", "ge\u00b7wannst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "ART", "NN", "$,", "KOUS", "PPER", "PRF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wir wollen dieses Bild von dir der Nachwelt schenken,", "tokens": ["Wir", "wol\u00b7len", "die\u00b7ses", "Bild", "von", "dir", "der", "Nach\u00b7welt", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PDAT", "NN", "APPR", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und in Vergessenheit die Schreckzerrbilder senken.", "tokens": ["Und", "in", "Ver\u00b7ges\u00b7sen\u00b7heit", "die", "Schreck\u00b7zerr\u00b7bil\u00b7der", "sen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Wir wollen dieses Bild von dir der Nachwelt schenken,", "tokens": ["Wir", "wol\u00b7len", "die\u00b7ses", "Bild", "von", "dir", "der", "Nach\u00b7welt", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PDAT", "NN", "APPR", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ohne Schaudern sie m\u00f6g' ihrer Ahnfrau denken.", "tokens": ["Da\u00df", "oh\u00b7ne", "Schau\u00b7dern", "sie", "m\u00f6g'", "ih\u00b7rer", "Ahn\u00b7frau", "den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}}}}