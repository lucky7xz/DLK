{"textgrid.poem.60191": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mischt immer eure Bl\u00e4tter, spielet", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mischt immer eure Bl\u00e4tter, spielet", "tokens": ["Mischt", "im\u00b7mer", "eu\u00b7re", "Bl\u00e4t\u00b7ter", ",", "spie\u00b7let"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "ADV", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gedankenvoll, und hoffend f\u00fchlet", "tokens": ["Ge\u00b7dan\u00b7ken\u00b7voll", ",", "und", "hof\u00b7fend", "f\u00fch\u00b7let"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Freuden des Gewinnes ganz;", "tokens": ["Die", "Freu\u00b7den", "des", "Ge\u00b7win\u00b7nes", "ganz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Geist, zu stoisch und zu trocken,", "tokens": ["Mein", "Geist", ",", "zu", "sto\u00b7isch", "und", "zu", "tro\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PTKA", "ADJD", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Lie\u00df nie die Charten sich verlocken,", "tokens": ["Lie\u00df", "nie", "die", "Char\u00b7ten", "sich", "ver\u00b7lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und h\u00fcpfte nie zu einem Tanz!", "tokens": ["Und", "h\u00fcpf\u00b7te", "nie", "zu", "ei\u00b7nem", "Tanz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zu steif den Fu\u00df im Tact zu lenken,", "tokens": ["Zu", "steif", "den", "Fu\u00df", "im", "Tact", "zu", "len\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu roh, beym Spiele was zu denken,", "tokens": ["Zu", "roh", ",", "beym", "Spie\u00b7le", "was", "zu", "den\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "$,", "APPRART", "NN", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Blieb ich in beyden ungelehrt;", "tokens": ["Blieb", "ich", "in", "bey\u00b7den", "un\u00b7ge\u00b7lehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich kenne nicht der Bl\u00e4tter Nahmen,", "tokens": ["Ich", "ken\u00b7ne", "nicht", "der", "Bl\u00e4t\u00b7ter", "Nah\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wei\u00df nicht, was Buben sind und Damen,", "tokens": ["Wei\u00df", "nicht", ",", "was", "Bu\u00b7ben", "sind", "und", "Da\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PWS", "NN", "VAFIN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wei\u00df nichts vom Blatt, dem Sieg geh\u00f6rt.", "tokens": ["Wei\u00df", "nichts", "vom", "Blatt", ",", "dem", "Sieg", "ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPRART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nur B\u00fccher hab ich liebgewonnen,", "tokens": ["Nur", "B\u00fc\u00b7cher", "hab", "ich", "lieb\u00b7ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Darinn gelesen, nachgesonnen,", "tokens": ["Da\u00b7rinn", "ge\u00b7le\u00b7sen", ",", "nach\u00b7ge\u00b7son\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PAV", "VVPP", "$,", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Selbst eins gemacht, so schlecht es war!", "tokens": ["Selbst", "eins", "ge\u00b7macht", ",", "so", "schlecht", "es", "war", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVPP", "$,", "ADV", "VVFIN", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nichts fragt ich da nach Spiel und T\u00e4nzen,", "tokens": ["Nichts", "fragt", "ich", "da", "nach", "Spiel", "und", "T\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich las, wodurch sich Helden cr\u00e4nzen,", "tokens": ["Ich", "las", ",", "wo\u00b7durch", "sich", "Hel\u00b7den", "cr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PRF", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und tr\u00e4umte Schlachten und Gefahr!", "tokens": ["Und", "tr\u00e4um\u00b7te", "Schlach\u00b7ten", "und", "Ge\u00b7fahr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich ging, auf selbst gebauten W\u00e4llen,", "tokens": ["Ich", "ging", ",", "auf", "selbst", "ge\u00b7bau\u00b7ten", "W\u00e4l\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Lie\u00df sich mein Volk in Ordnung stellen", "tokens": ["Lie\u00df", "sich", "mein", "Volk", "in", "Ord\u00b7nung", "stel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und that, als wie ein General;", "tokens": ["Und", "that", ",", "als", "wie", "ein", "Ge\u00b7ne\u00b7ral", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Warf Schanzen auf, scho\u00df Ziegelst\u00fccke,", "tokens": ["Warf", "Schan\u00b7zen", "auf", ",", "scho\u00df", "Zie\u00b7gel\u00b7st\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "PTKVZ", "$,", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zog schlechterdings mich nicht zur\u00fccke,", "tokens": ["Zog", "schlech\u00b7ter\u00b7dings", "mich", "nicht", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sprach laut wenn ich den Sturm befahl!", "tokens": ["Sprach", "laut", "wenn", "ich", "den", "Sturm", "be\u00b7fahl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "War eine Vestung eingenommen,", "tokens": ["War", "ei\u00b7ne", "Ves\u00b7tung", "ein\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dann lie\u00df ich meine V\u00f6lker kommen", "tokens": ["Dann", "lie\u00df", "ich", "mei\u00b7ne", "V\u00f6l\u00b7ker", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Drang tiefer ein in Feindes Land,", "tokens": ["Drang", "tie\u00b7fer", "ein", "in", "Fein\u00b7des", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Marschirte listig hin und wieder", "tokens": ["Mar\u00b7schir\u00b7te", "lis\u00b7tig", "hin", "und", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "PTKVZ", "KON", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hieb viele tausend Feinde nieder,", "tokens": ["Hieb", "vie\u00b7le", "tau\u00b7send", "Fein\u00b7de", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "CARD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In allen Nesseln die ich fand.", "tokens": ["In", "al\u00b7len", "Nes\u00b7seln", "die", "ich", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Da lagen dann die kleinen Leichen,", "tokens": ["Da", "la\u00b7gen", "dann", "die", "klei\u00b7nen", "Lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gef\u00e4llt von meinen starken Streichen,", "tokens": ["Ge\u00b7f\u00e4llt", "von", "mei\u00b7nen", "star\u00b7ken", "Strei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bey tausenden gestreckt vor mir;", "tokens": ["Bey", "tau\u00b7sen\u00b7den", "ge\u00b7streckt", "vor", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVPP", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Stolz dacht ich mich als Ueberwinder", "tokens": ["Stolz", "dacht", "ich", "mich", "als", "Ue\u00b7ber\u00b7win\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PRF", "KOUS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich war ein Kind, und wie die Kinder", "tokens": ["Ich", "war", "ein", "Kind", ",", "und", "wie", "die", "Kin\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "KON", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Thun gar zu oft im Alter wir!", "tokens": ["Thun", "gar", "zu", "oft", "im", "Al\u00b7ter", "wir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKA", "ADV", "APPRART", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "O meine Phantasie ist heftig,", "tokens": ["O", "mei\u00b7ne", "Phan\u00b7ta\u00b7sie", "ist", "hef\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schon dazumahl war sie gesch\u00e4ftig,", "tokens": ["Schon", "da\u00b7zu\u00b7mahl", "war", "sie", "ge\u00b7sch\u00e4f\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als ich noch meine Heerde trieb;", "tokens": ["Als", "ich", "noch", "mei\u00b7ne", "Heer\u00b7de", "trieb", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Itzt aber sieht sie andre Schlachten", "tokens": ["Itzt", "a\u00b7ber", "sieht", "sie", "and\u00b7re", "Schlach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Denkt die, die sich unsterblich machten,", "tokens": ["Denkt", "die", ",", "die", "sich", "uns\u00b7terb\u00b7lich", "mach\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "$,", "PRELS", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und den, der sich unsterblich schrieb!", "tokens": ["Und", "den", ",", "der", "sich", "uns\u00b7terb\u00b7lich", "schrieb", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "PRELS", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}