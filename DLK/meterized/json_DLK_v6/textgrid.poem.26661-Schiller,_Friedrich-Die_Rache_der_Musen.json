{"textgrid.poem.26661": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Rache der Musen", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weinend kamen einst die Neune", "tokens": ["Wei\u00b7nend", "ka\u00b7men", "einst", "die", "Neu\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu dem Liedergott.", "tokens": ["Zu", "dem", "Lie\u00b7der\u00b7gott", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "\u00bbh\u00f6r, Papachen\u00ab, rief die Kleine,", "tokens": ["\u00bb", "h\u00f6r", ",", "Pa\u00b7pa\u00b7chen", "\u00ab", ",", "rief", "die", "Klei\u00b7ne", ","], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "NN", "$(", "$,", "VVFIN", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbwie man uns bedroht!", "tokens": ["\u00bb", "wie", "man", "uns", "be\u00b7droht", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PIS", "PPER", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Junge Dintenlecker schw\u00e4rmen", "tokens": ["Jun\u00b7ge", "Din\u00b7ten\u00b7le\u00b7cker", "schw\u00e4r\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um den Helikon,", "tokens": ["Um", "den", "He\u00b7li\u00b7kon", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Raufen sich, hantieren, l\u00e4rmen", "tokens": ["Rau\u00b7fen", "sich", ",", "han\u00b7tie\u00b7ren", ",", "l\u00e4r\u00b7men"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["VVFIN", "PRF", "$,", "VVINF", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis zu deinem Thron.", "tokens": ["Bis", "zu", "dei\u00b7nem", "Thron", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Galoppieren auf dem Springer,", "tokens": ["Ga\u00b7lop\u00b7pie\u00b7ren", "auf", "dem", "Sprin\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Reiten ihn zur Tr\u00e4nk,", "tokens": ["Rei\u00b7ten", "ihn", "zur", "Tr\u00e4nk", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Nennen sich gar hohe S\u00e4nger,", "tokens": ["Nen\u00b7nen", "sich", "gar", "ho\u00b7he", "S\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Barden einge, denk!", "tokens": ["Bar\u00b7den", "ein\u00b7ge", ",", "denk", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Wollen uns \u2013 wie garstig! \u2013 n\u00f6ten,", "tokens": ["Wol\u00b7len", "uns", "\u2013", "wie", "gars\u00b7tig", "!", "\u2013", "n\u00f6\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "PWAV", "ADJD", "$.", "$(", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ei! die Grobian!", "tokens": ["Ei", "!", "die", "Gro\u00b7bi\u00b7an", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Was ich, ohne Schamerr\u00f6ten,", "tokens": ["Was", "ich", ",", "oh\u00b7ne", "Scha\u00b7mer\u00b7r\u00f6\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "$,", "KOUI", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht erz\u00e4hlen kann;", "tokens": ["Nicht", "er\u00b7z\u00e4h\u00b7len", "kann", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Einer br\u00fcllt heraus vor allen,", "tokens": ["Ei\u00b7ner", "br\u00fcllt", "he\u00b7raus", "vor", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schreit: ", "tokens": ["Schreit", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Schl\u00e4gt mit beiden F\u00e4ust und Ballen", "tokens": ["Schl\u00e4gt", "mit", "bei\u00b7den", "F\u00e4ust", "und", "Bal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um sich wie ein B\u00e4r.", "tokens": ["Um", "sich", "wie", "ein", "B\u00e4r", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Pfeift wohl gar \u2013 wie ungeschliffen! \u2013", "tokens": ["Pfeift", "wohl", "gar", "\u2013", "wie", "un\u00b7ge\u00b7schlif\u00b7fen", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "ADV", "$(", "PWAV", "ADJD", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Andre Schl\u00e4fer wach.", "tokens": ["And\u00b7re", "Schl\u00e4\u00b7fer", "wach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Doch kommt keiner nach.", "tokens": ["Doch", "kommt", "kei\u00b7ner", "nach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Droht, er komm noch \u00f6fter wieder;", "tokens": ["Droht", ",", "er", "komm", "noch", "\u00f6f\u00b7ter", "wie\u00b7der", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ADV", "ADV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da sei Zeus daf\u00fcr!", "tokens": ["Da", "sei", "Zeus", "da\u00b7f\u00fcr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "PAV", "$."], "meter": "--+-+", "measure": "anapaest.init"}, "line.3": {"text": "Vater, liebst du Sang und Lieder,", "tokens": ["Va\u00b7ter", ",", "liebst", "du", "Sang", "und", "Lie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weis ihm doch die T\u00fcr!\u00ab", "tokens": ["Weis", "ihm", "doch", "die", "T\u00fcr", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Vater Ph\u00f6bus h\u00f6rt mit Lachen", "tokens": ["Va\u00b7ter", "Ph\u00f6\u00b7bus", "h\u00f6rt", "mit", "La\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NE", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihren Klagbericht:", "tokens": ["Ih\u00b7ren", "Klag\u00b7be\u00b7richt", ":"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "\u00bbwollens kurz mit ihnen machen,", "tokens": ["\u00bb", "wol\u00b7lens", "kurz", "mit", "ih\u00b7nen", "ma\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kinder, zittert nicht!", "tokens": ["Kin\u00b7der", ",", "zit\u00b7tert", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Eine mu\u00df ins h\u00f6llsche Feuer,", "tokens": ["Ei\u00b7ne", "mu\u00df", "ins", "h\u00f6ll\u00b7sche", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Geh, Melpomene!", "tokens": ["Geh", ",", "Mel\u00b7po\u00b7me\u00b7ne", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Leihe Kleider, Noten, Leier", "tokens": ["Lei\u00b7he", "Klei\u00b7der", ",", "No\u00b7ten", ",", "Lei\u00b7er"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einer ", "tokens": ["Ei\u00b7ner"], "token_info": ["word"], "pos": ["PIS"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.10": {"line.1": {"text": "Sie begegn' in dem Gewande,", "tokens": ["Sie", "be\u00b7gegn'", "in", "dem", "Ge\u00b7wan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als w\u00e4r sie verirrt,", "tokens": ["Als", "w\u00e4r", "sie", "ver\u00b7irrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Einem dieser Jaunerbande,", "tokens": ["Ei\u00b7nem", "die\u00b7ser", "Jau\u00b7ner\u00b7ban\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn es dunkel wird.", "tokens": ["Wenn", "es", "dun\u00b7kel", "wird", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.11": {"line.1": {"text": "M\u00f6gen dann in finstern K\u00fcssen", "tokens": ["M\u00f6\u00b7gen", "dann", "in", "fins\u00b7tern", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An dem artgen Kind", "tokens": ["An", "dem", "art\u00b7gen", "Kind"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+-+", "measure": "anapaest.init"}, "line.3": {"text": "Ihre wilden L\u00fcste b\u00fc\u00dfen,", "tokens": ["Ih\u00b7re", "wil\u00b7den", "L\u00fcs\u00b7te", "b\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie sie w\u00fcrdig sind.\u00ab", "tokens": ["Wie", "sie", "w\u00fcr\u00b7dig", "sind", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VAFIN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.12": {"line.1": {"text": "Red' und Tat! \u2013 Die H\u00f6lleng\u00f6ttin", "tokens": ["Red'", "und", "Tat", "!", "\u2013", "Die", "H\u00f6l\u00b7len\u00b7g\u00f6t\u00b7tin"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["NN", "KON", "NN", "$.", "$(", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War schon aufgeschm\u00fcckt;", "tokens": ["War", "schon", "auf\u00b7ge\u00b7schm\u00fcckt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Man erz\u00e4hlt, die Herren h\u00e4tten", "tokens": ["Man", "er\u00b7z\u00e4hlt", ",", "die", "Her\u00b7ren", "h\u00e4t\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "ART", "NN", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kaum den Raub erblickt,", "tokens": ["Kaum", "den", "Raub", "er\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.13": {"line.1": {"text": "W\u00e4ren, wie die Geir auf Tauben,", "tokens": ["W\u00e4\u00b7ren", ",", "wie", "die", "Geir", "auf", "Tau\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWAV", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Losgest\u00fcrzt auf sie \u2013", "tokens": ["Los\u00b7ge\u00b7st\u00fcrzt", "auf", "sie", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Etwas will ich daran glauben,", "tokens": ["Et\u00b7was", "will", "ich", "da\u00b7ran", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PAV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles glaub ich nie.", "tokens": ["Al\u00b7les", "glaub", "ich", "nie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.14": {"line.1": {"text": "Waren h\u00fcbsche Jungens drunter,", "tokens": ["Wa\u00b7ren", "h\u00fcb\u00b7sche", "Jun\u00b7gens", "drun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "PAV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie gerieten sie,", "tokens": ["Wie", "ge\u00b7rie\u00b7ten", "sie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Dieses, Br\u00fcder, nimmt mich wunder,", "tokens": ["Die\u00b7ses", ",", "Br\u00fc\u00b7der", ",", "nimmt", "mich", "wun\u00b7der", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "NN", "$,", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In die Kompanie?", "tokens": ["In", "die", "Kom\u00b7pa\u00b7nie", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.15": {"line.1": {"text": "Die G\u00f6ttin abortiert hernach:", "tokens": ["Die", "G\u00f6t\u00b7tin", "ab\u00b7or\u00b7tiert", "her\u00b7nach", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kam raus ein neuer \u2013 Almanach.", "tokens": ["Kam", "raus", "ein", "neu\u00b7er", "\u2013", "Al\u00b7ma\u00b7nach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ADV", "ART", "ADJA", "$(", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}