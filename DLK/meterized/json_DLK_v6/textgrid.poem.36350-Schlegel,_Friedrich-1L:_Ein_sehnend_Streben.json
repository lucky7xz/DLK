{"textgrid.poem.36350": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein sehnend Streben", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein sehnend Streben", "tokens": ["Ein", "seh\u00b7nend", "Stre\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Teilt mir das Herz,", "tokens": ["Teilt", "mir", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Bis alles Leben", "tokens": ["Bis", "al\u00b7les", "Le\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Sich l\u00f6st in Schmerz.", "tokens": ["Sich", "l\u00f6st", "in", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "In Leid erwachte", "tokens": ["In", "Leid", "er\u00b7wach\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Der junge Sinn,", "tokens": ["Der", "jun\u00b7ge", "Sinn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und Liebe brachte", "tokens": ["Und", "Lie\u00b7be", "brach\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Zum Ziel mich hin.", "tokens": ["Zum", "Ziel", "mich", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ihr edle Flammen", "tokens": ["Ihr", "ed\u00b7le", "Flam\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Wecktet mich auf;", "tokens": ["Weck\u00b7tet", "mich", "auf", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Es ging mitsammen", "tokens": ["Es", "ging", "mit\u00b7sam\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Zu Gott der Lauf.", "tokens": ["Zu", "Gott", "der", "Lauf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Ein Feuer war es,", "tokens": ["Ein", "Feu\u00b7er", "war", "es", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Das alles treibt;", "tokens": ["Das", "al\u00b7les", "treibt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ein starkes, klares,", "tokens": ["Ein", "star\u00b7kes", ",", "kla\u00b7res", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Was ewig bleibt.", "tokens": ["Was", "e\u00b7wig", "bleibt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Was wir anstrebten,", "tokens": ["Was", "wir", "an\u00b7streb\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "War treu gemeint;", "tokens": ["War", "treu", "ge\u00b7meint", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Was wir durchlebten", "tokens": ["Was", "wir", "durch\u00b7leb\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Bleibt tief vereint.", "tokens": ["Bleibt", "tief", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Da trat ein Scheiden", "tokens": ["Da", "trat", "ein", "Schei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Mir in die Brust;", "tokens": ["Mir", "in", "die", "Brust", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$."], "meter": "++-+", "measure": "iambic.di"}, "line.3": {"text": "Das tiefe Leiden", "tokens": ["Das", "tie\u00b7fe", "Lei\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Der Liebeslust.", "tokens": ["Der", "Lie\u00b7bes\u00b7lust", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Im Seelengrunde", "tokens": ["Im", "See\u00b7len\u00b7grun\u00b7de"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Wohnt mir Ein Bild;", "tokens": ["Wohnt", "mir", "Ein", "Bild", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die Todeswunde", "tokens": ["Die", "To\u00b7des\u00b7wun\u00b7de"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ward nie gestillt.", "tokens": ["Ward", "nie", "ge\u00b7stillt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Viel tausend Tr\u00e4nen", "tokens": ["Viel", "tau\u00b7send", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "CARD", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Flossen hinab;", "tokens": ["Flos\u00b7sen", "hin\u00b7ab", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ein ewig Sehnen", "tokens": ["Ein", "e\u00b7wig", "Seh\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Zu ihr ins Grab.", "tokens": ["Zu", "ihr", "ins", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "In Liebeswogen", "tokens": ["In", "Lie\u00b7bes\u00b7wo\u00b7gen"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Wallet der Geist,", "tokens": ["Wal\u00b7let", "der", "Geist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Bis fortgezogen", "tokens": ["Bis", "fort\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Die Brust zerrei\u00dft.", "tokens": ["Die", "Brust", "zer\u00b7rei\u00dft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "Ein Stern erschien mir", "tokens": ["Ein", "Stern", "er\u00b7schien", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Vom Paradies;", "tokens": ["Vom", "Pa\u00b7ra\u00b7dies", ";"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und dahin fliehn wir", "tokens": ["Und", "da\u00b7hin", "fliehn", "wir"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PAV", "VVFIN", "PPER"], "meter": "--+-+", "measure": "anapaest.init"}, "line.4": {"text": "Vereint gewi\u00df.", "tokens": ["Ver\u00b7eint", "ge\u00b7wi\u00df", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.11": {"line.1": {"text": "Hier noch befeuchtet", "tokens": ["Hier", "noch", "be\u00b7feuch\u00b7tet"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Der Blick sich lind,", "tokens": ["Der", "Blick", "sich", "lind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wenn mich umleuchtet", "tokens": ["Wenn", "mich", "um\u00b7leuch\u00b7tet"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Dies Himmelskind.", "tokens": ["Dies", "Him\u00b7mel\u00b7skind", "."], "token_info": ["word", "word", "punct"], "pos": ["PDS", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.12": {"line.1": {"text": "Ein Zauber waltet", "tokens": ["Ein", "Zau\u00b7ber", "wal\u00b7tet"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Jetzt \u00fcber mich,", "tokens": ["Jetzt", "\u00fc\u00b7ber", "mich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und der gestaltet", "tokens": ["Und", "der", "ge\u00b7stal\u00b7tet"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Dies all nach sich.", "tokens": ["Dies", "all", "nach", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PIAT", "APPR", "PRF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.13": {"line.1": {"text": "Als ob uns verm\u00e4hle", "tokens": ["Als", "ob", "uns", "ver\u00b7m\u00e4h\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Geistesgewalt,", "tokens": ["Geis\u00b7tes\u00b7ge\u00b7walt", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wo Seele in Seele", "tokens": ["Wo", "See\u00b7le", "in", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Hin\u00fcberwallt.", "tokens": ["Hin\u00b7\u00fc\u00b7berw\u00b7allt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.14": {"line.1": {"text": "Ob auch zerspalten", "tokens": ["Ob", "auch", "zer\u00b7spal\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADV", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Mir ist das Herz;", "tokens": ["Mir", "ist", "das", "Herz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Selig doch halten", "tokens": ["Se\u00b7lig", "doch", "hal\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADV", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Will ich den Schmerz.", "tokens": ["Will", "ich", "den", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}