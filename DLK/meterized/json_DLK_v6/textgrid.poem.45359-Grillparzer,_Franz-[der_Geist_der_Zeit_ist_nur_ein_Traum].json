{"textgrid.poem.45359": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[der Geist der Zeit ist nur ein Traum]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Geist der Zeit ist nur ein Traum,", "tokens": ["Der", "Geist", "der", "Zeit", "ist", "nur", "ein", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Oft ist nur Mode das Bewunderte,", "tokens": ["Oft", "ist", "nur", "Mo\u00b7de", "das", "Be\u00b7wun\u00b7der\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Der Geist, der stille, der Jahrhunderte.", "tokens": ["Der", "Geist", ",", "der", "stil\u00b7le", ",", "der", "Jahr\u00b7hun\u00b7der\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was klein um klein und Griff um Griff", "tokens": ["Was", "klein", "um", "klein", "und", "Griff", "um", "Griff"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "APPR", "ADJD", "KON", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Polypenartig sich erweitert,", "tokens": ["Po\u00b7ly\u00b7pen\u00b7ar\u00b7tig", "sich", "er\u00b7wei\u00b7tert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wird endlich zum Korallenriff,", "tokens": ["Wird", "end\u00b7lich", "zum", "Ko\u00b7ral\u00b7len\u00b7riff", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An dem dein hohles Staatsschiff scheitert.", "tokens": ["An", "dem", "dein", "hoh\u00b7les", "Staats\u00b7schiff", "schei\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}