{"textgrid.poem.49451": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "Ein Witwer spricht:", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich seh eine Gruppe auf dem Meer,", "tokens": ["Ich", "seh", "ei\u00b7ne", "Grup\u00b7pe", "auf", "dem", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Welch Meer? Dem Meer meiner Tr\u00e4nen!", "tokens": ["Welch", "Meer", "?", "Dem", "Meer", "mei\u00b7ner", "Tr\u00e4\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Meine Augen, vom Salzwind feucht und schwer", "tokens": ["Mei\u00b7ne", "Au\u00b7gen", ",", "vom", "Sal\u00b7zwind", "feucht", "und", "schwer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "APPRART", "NN", "ADJD", "KON", "ADJD"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "In der Nacht voll Sturm und Sehnen,", "tokens": ["In", "der", "Nacht", "voll", "Sturm", "und", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sind zwei Sterne \u00fcber dem Meer!", "tokens": ["Sind", "zwei", "Ster\u00b7ne", "\u00fc\u00b7ber", "dem", "Meer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "CARD", "NN", "APPR", "ART", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Eine junge Frau seh ich nahen", "tokens": ["Ei\u00b7ne", "jun\u00b7ge", "Frau", "seh", "ich", "na\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADJA"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Mit ihrem Kind, das schon gross,", "tokens": ["Mit", "ih\u00b7rem", "Kind", ",", "das", "schon", "gross", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "In einem Schiff ohne Rahen,", "tokens": ["In", "ei\u00b7nem", "Schiff", "oh\u00b7ne", "Ra\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ohne Ruder und Segel im Flutgetos',", "tokens": ["Oh\u00b7ne", "Ru\u00b7der", "und", "Se\u00b7gel", "im", "Flut\u00b7ge\u00b7to\u00b7s'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPRART", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.5": {"text": "Ein Kind, eine Frau seh ich nahen.", "tokens": ["Ein", "Kind", ",", "ei\u00b7ne", "Frau", "seh", "ich", "na\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "PPER", "ADJA", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Im Flutentosen, im Sturmgebraus", "tokens": ["Im", "Flu\u00b7ten\u00b7to\u00b7sen", ",", "im", "Sturm\u00b7ge\u00b7braus"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Umklammert die Mutter der Kleine,", "tokens": ["Um\u00b7klam\u00b7mert", "die", "Mut\u00b7ter", "der", "Klei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die nicht mehr weiss wo ein noch aus", "tokens": ["Die", "nicht", "mehr", "weiss", "wo", "ein", "noch", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PTKNEG", "ADV", "VVFIN", "PWAV", "ART", "ADV", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und doch hofft, dass Rettung erscheine", "tokens": ["Und", "doch", "hofft", ",", "dass", "Ret\u00b7tung", "er\u00b7schei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "$,", "KOUS", "NN", "VVFIN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Im Flutentosen, im Sturmgebraus.", "tokens": ["Im", "Flu\u00b7ten\u00b7to\u00b7sen", ",", "im", "Sturm\u00b7ge\u00b7braus", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Ja, hoff auf Gott, arme T\u00f6rin,", "tokens": ["Ja", ",", "hoff", "auf", "Gott", ",", "ar\u00b7me", "T\u00f6\u00b7rin", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "APPR", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zu dem Vater flehe, Kind,", "tokens": ["Zu", "dem", "Va\u00b7ter", "fle\u00b7he", ",", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rast auch der Sturm \u00fcbers Meer hin,", "tokens": ["Rast", "auch", "der", "Sturm", "\u00fc\u00b7bers", "Meer", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Mein Herz weissagt euch, dass der Wind", "tokens": ["Mein", "Herz", "weis\u00b7sagt", "euch", ",", "dass", "der", "Wind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bald schweigt, o Kind, arme T\u00f6rin.", "tokens": ["Bald", "schweigt", ",", "o", "Kind", ",", "ar\u00b7me", "T\u00f6\u00b7rin", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "FM", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}}, "stanza.5": {"line.1": {"text": "Und still die Gruppe auf dem Meer,", "tokens": ["Und", "still", "die", "Grup\u00b7pe", "auf", "dem", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Diesem Meer der guten Tr\u00e4nen.", "tokens": ["Die\u00b7sem", "Meer", "der", "gu\u00b7ten", "Tr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Augen im Himmel, der wolkenleer", "tokens": ["Mei\u00b7ne", "Au\u00b7gen", "im", "Him\u00b7mel", ",", "der", "wol\u00b7ken\u00b7leer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,", "ART", "NN"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "In der Nacht ohne Sturm und Sehnen,", "tokens": ["In", "der", "Nacht", "oh\u00b7ne", "Sturm", "und", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Sind zwei Engel \u00fcber dem Meer.", "tokens": ["Sind", "zwei", "En\u00b7gel", "\u00fc\u00b7ber", "dem", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "CARD", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}