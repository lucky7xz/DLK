{"textgrid.poem.50922": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Der Gemsj\u00e4ger", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er kam, ein alter J\u00e4gersmann,", "tokens": ["Er", "kam", ",", "ein", "al\u00b7ter", "J\u00e4\u00b7gers\u00b7mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herab an unsrer Str\u00f6me Flut,", "tokens": ["Her\u00b7ab", "an", "uns\u00b7rer", "Str\u00f6\u00b7me", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er hatte kurze Hosen an", "tokens": ["Er", "hat\u00b7te", "kur\u00b7ze", "Ho\u00b7sen", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und trug 'nen spitzen J\u00e4gerhut.", "tokens": ["Und", "trug", "'nen", "spit\u00b7zen", "J\u00e4\u00b7ger\u00b7hut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er ging so ernst, er sah so schlicht,", "tokens": ["Er", "ging", "so", "ernst", ",", "er", "sah", "so", "schlicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "PPER", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie seiner Joppe graues Tuch,", "tokens": ["Wie", "sei\u00b7ner", "Jop\u00b7pe", "grau\u00b7es", "Tuch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus seinem Mund ging das Ger\u00fccht", "tokens": ["Aus", "sei\u00b7nem", "Mund", "ging", "das", "Ge\u00b7r\u00fccht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von manchem guten Weidmannsspruch.", "tokens": ["Von", "man\u00b7chem", "gu\u00b7ten", "Weid\u00b7manns\u00b7spruch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "In seiner Tasche, dachten wir,", "tokens": ["In", "sei\u00b7ner", "Ta\u00b7sche", ",", "dach\u00b7ten", "wir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Birgt er gewi\u00df aus Alpenkraut", "tokens": ["Birgt", "er", "ge\u00b7wi\u00df", "aus", "Al\u00b7pen\u00b7kraut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "F\u00fcr altes Weh manch Elixir,", "tokens": ["F\u00fcr", "al\u00b7tes", "Weh", "manch", "E\u00b7li\u00b7xir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In hoher Einsamkeit gebraut.", "tokens": ["In", "ho\u00b7her", "Ein\u00b7sam\u00b7keit", "ge\u00b7braut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und wachsam, recht nach J\u00e4gerart,", "tokens": ["Und", "wach\u00b7sam", ",", "recht", "nach", "J\u00e4\u00b7ger\u00b7art", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sp\u00e4ht rings sein scharfes Aug herum,", "tokens": ["Sp\u00e4ht", "rings", "sein", "schar\u00b7fes", "Aug", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und seine sichre Kugel wahrt", "tokens": ["Und", "sei\u00b7ne", "sich\u00b7re", "Ku\u00b7gel", "wahrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor Feinden unser Heiligtum!", "tokens": ["Vor", "Fein\u00b7den", "un\u00b7ser", "Hei\u00b7lig\u00b7tum", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wir holten ihn mit Kr\u00e4nzen ein", "tokens": ["Wir", "hol\u00b7ten", "ihn", "mit", "Kr\u00e4n\u00b7zen", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und f\u00fchrten ihn mit frohem Mut", "tokens": ["Und", "f\u00fchr\u00b7ten", "ihn", "mit", "fro\u00b7hem", "Mut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In unser neues Haus am Main,", "tokens": ["In", "un\u00b7ser", "neu\u00b7es", "Haus", "am", "Main", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ernsthaft zog er seinen Hut. \u2013", "tokens": ["Und", "ernst\u00b7haft", "zog", "er", "sei\u00b7nen", "Hut", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und heut noch sitzt er da und spricht", "tokens": ["Und", "heut", "noch", "sitzt", "er", "da", "und", "spricht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPER", "ADV", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Spr\u00fcchlein von der bessern Zeit.", "tokens": ["Sein", "Spr\u00fcch\u00b7lein", "von", "der", "bes\u00b7sern", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch immer macht er sein Gesicht", "tokens": ["Noch", "im\u00b7mer", "macht", "er", "sein", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Voll Einfalt und voll Ehrlichkeit.", "tokens": ["Voll", "Ein\u00b7falt", "und", "voll", "Ehr\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch wenn die Nacht auf Erden graut,", "tokens": ["Doch", "wenn", "die", "Nacht", "auf", "Er\u00b7den", "graut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann schleicht aus Kluft und Spalt hervor", "tokens": ["Dann", "schleicht", "aus", "Kluft", "und", "Spalt", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die schlimme Sippschaft, wohlvertraut;", "tokens": ["Die", "schlim\u00b7me", "Sipp\u00b7schaft", ",", "wohl\u00b7ver\u00b7traut", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er aber \u00f6ffnet still das Tor.", "tokens": ["Er", "a\u00b7ber", "\u00f6ff\u00b7net", "still", "das", "Tor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Wohl h\u00e4lt er stets den Hahn gespannt:", "tokens": ["Wohl", "h\u00e4lt", "er", "stets", "den", "Hahn", "ge\u00b7spannt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Kugel ist f\u00fcr unser Herz;", "tokens": ["Die", "Ku\u00b7gel", "ist", "f\u00fcr", "un\u00b7ser", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und unsre Kinder schl\u00e4gt die Hand,", "tokens": ["Und", "uns\u00b7re", "Kin\u00b7der", "schl\u00e4gt", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die lindern sollte unsern Schmerz.", "tokens": ["Die", "lin\u00b7dern", "soll\u00b7te", "un\u00b7sern", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wir sind versto\u00dfen, der Spa\u00df ist aus!", "tokens": ["Wir", "sind", "ver\u00b7sto\u00b7\u00dfen", ",", "der", "Spa\u00df", "ist", "aus", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "ART", "NN", "VAFIN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Verriegelt ist die neue T\u00fcr,", "tokens": ["Ver\u00b7rie\u00b7gelt", "ist", "die", "neu\u00b7e", "T\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und aus dem totenstillen Haus", "tokens": ["Und", "aus", "dem", "to\u00b7tens\u00b7til\u00b7len", "Haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Blinzt nur des J\u00e4gers Rohr herf\u00fcr!", "tokens": ["Blinzt", "nur", "des", "J\u00e4\u00b7gers", "Rohr", "her\u00b7f\u00fcr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}