{"textgrid.poem.53115": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Klugheit (wie man spricht) k\u00f6mpt vor Jahren nicht", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Lieb wil nichts nach Liebe fragen,", "tokens": ["Mein", "Lieb", "wil", "nichts", "nach", "Lie\u00b7be", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PIS", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist solcher Wollust Spinnen-feind,", "tokens": ["Ist", "sol\u00b7cher", "Wol\u00b7lust", "Spin\u00b7nen\u00b7feind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer Ihr vom Breutgam was wil sagen,", "tokens": ["Wer", "Ihr", "vom", "Breut\u00b7gam", "was", "wil", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NE", "PWS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Macht, da\u00df Sie sehr dar\u00fcber weint.", "tokens": ["Macht", ",", "da\u00df", "Sie", "sehr", "da\u00b7r\u00fc\u00b7ber", "weint", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADV", "PAV", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Gott la\u00df es ja mich nicht erleben,", "tokens": ["Gott", "la\u00df", "es", "ja", "mich", "nicht", "er\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "(so spricht Sie weh-vnd vnmuts-voll)", "tokens": ["(", "so", "spricht", "Sie", "weh\u00b7\u00b7vnd", "vn\u00b7muts\u00b7voll", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADJD", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich das Ja-wort von mir geben", "tokens": ["Da\u00df", "ich", "das", "Ja\u00b7wort", "von", "mir", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd wem versaget werden soll!", "tokens": ["Vnd", "wem", "ver\u00b7sa\u00b7get", "wer\u00b7den", "soll", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wil sie von diesem Sinn nicht weichen,", "tokens": ["Wil", "sie", "von", "die\u00b7sem", "Sinn", "nicht", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PDAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Stimmt mit der Red' ihr Hertz auch ein,", "tokens": ["Stimmt", "mit", "der", "Red'", "ihr", "Hertz", "auch", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "So sag ich, warlich Ihres gleichen", "tokens": ["So", "sag", "ich", ",", "war\u00b7lich", "Ih\u00b7res", "glei\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wird leichtlich nicht zu finden seyn.", "tokens": ["Wird", "leicht\u00b7lich", "nicht", "zu", "fin\u00b7den", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PTKNEG", "PTKZU", "VVINF", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ihr zu gefallen wil ichs gla\u00fcben,", "tokens": ["Ihr", "zu", "ge\u00b7fal\u00b7len", "wil", "ichs", "gla\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Seh' ihre zarte Jugend an,", "tokens": ["Seh'", "ih\u00b7re", "zar\u00b7te", "Ju\u00b7gend", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der ich die Einfalt zu-mu\u00df schreiben,", "tokens": ["Der", "ich", "die", "Ein\u00b7falt", "zu\u00b7mu\u00df", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die nichts von Liebe wissen kan.", "tokens": ["Die", "nichts", "von", "Lie\u00b7be", "wis\u00b7sen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch nehm' ich es mit ihren Wangen,", "tokens": ["Doch", "nehm'", "ich", "es", "mit", "ih\u00b7ren", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit ihrer Augen Glantz bezeugt,", "tokens": ["Mit", "ih\u00b7rer", "Au\u00b7gen", "Glantz", "be\u00b7zeugt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit allem dem, was mich gefangen,", "tokens": ["Mit", "al\u00b7lem", "dem", ",", "was", "mich", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ART", "$,", "PWS", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd sch\u00f6nes sich an Ihr era\u00fcgt,", "tokens": ["Vnd", "sch\u00f6\u00b7nes", "sich", "an", "Ihr", "er\u00b7a\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Mit Ihren sitsamen Geberden,", "tokens": ["Mit", "Ih\u00b7ren", "sit\u00b7sa\u00b7men", "Ge\u00b7ber\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit des geraden Leibes Pracht,", "tokens": ["Mit", "des", "ge\u00b7ra\u00b7den", "Lei\u00b7bes", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der noch, ob Gott wil, mein sol werden,", "tokens": ["Der", "noch", ",", "ob", "Gott", "wil", ",", "mein", "sol", "wer\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "KOUS", "NN", "VMFIN", "$,", "PPOSAT", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie ist dem Kloster nicht gemacht.", "tokens": ["Sie", "ist", "dem", "Klos\u00b7ter", "nicht", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}