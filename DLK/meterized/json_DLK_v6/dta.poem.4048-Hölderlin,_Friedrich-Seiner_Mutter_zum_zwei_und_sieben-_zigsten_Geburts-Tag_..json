{"dta.poem.4048": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Seiner Mutter zum zwei und sieben-  \n zigsten Geburts-Tag .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Vieles hast Du erlebt, Du theure Mutter! und", "tokens": ["Vie\u00b7les", "hast", "Du", "er\u00b7lebt", ",", "Du", "theu\u00b7re", "Mut\u00b7ter", "!", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PIS", "VAFIN", "PPER", "VVPP", "$,", "PPER", "ADJA", "NN", "$.", "KON"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "ruhst nun", "tokens": ["ruhst", "nun"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Gl\u00fccklich, von Fernen und Nah'n liebend beim", "tokens": ["Gl\u00fcck\u00b7lich", ",", "von", "Fer\u00b7nen", "und", "Nah'n", "lie\u00b7bend", "beim"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "APPR", "ADJA", "KON", "NN", "ADJD", "APPRART"], "meter": "+--+--++-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Namen genannt,", "tokens": ["Na\u00b7men", "ge\u00b7nannt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Mir auch herzlich geehrt in des Alters silberner", "tokens": ["Mir", "auch", "herz\u00b7lich", "ge\u00b7ehrt", "in", "des", "Al\u00b7ters", "sil\u00b7ber\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADJD", "VVPP", "APPR", "ART", "NN", "ADJA"], "meter": "+-+--+--+-+--", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Krone,", "tokens": ["Kro\u00b7ne", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Unter den Kindern, die Dir reifen und wachsen", "tokens": ["Un\u00b7ter", "den", "Kin\u00b7dern", ",", "die", "Dir", "rei\u00b7fen", "und", "wach\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "KON", "VVINF"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.8": {"text": "und bl\u00fch'n.", "tokens": ["und", "bl\u00fch'", "n."], "token_info": ["word", "word", "abbreviation"], "pos": ["KON", "XY", "XY"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Langes Leben hat Dir die sanfte Seele gewonnen,", "tokens": ["Lan\u00b7ges", "Le\u00b7ben", "hat", "Dir", "die", "sanf\u00b7te", "See\u00b7le", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Und die Hoffnung, die Dich freundlich im Leiden", "tokens": ["Und", "die", "Hoff\u00b7nung", ",", "die", "Dich", "freund\u00b7lich", "im", "Lei\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "APPRART", "NN"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "gef\u00fchrt.", "tokens": ["ge\u00b7f\u00fchrt", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.12": {"text": "Denn zufrieden bist Du und fromm, wie die", "tokens": ["Denn", "zu\u00b7frie\u00b7den", "bist", "Du", "und", "fromm", ",", "wie", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "KON", "ADJD", "$,", "PWAV", "ART"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Mutter, die einst den", "tokens": ["Mut\u00b7ter", ",", "die", "einst", "den"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ADV", "ART"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Besten der Menschen, den Freund unserer Erde,", "tokens": ["Bes\u00b7ten", "der", "Men\u00b7schen", ",", "den", "Freund", "un\u00b7se\u00b7rer", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+--+--+---+-", "measure": "dactylic.tri.plus"}, "line.15": {"text": "gebar.", "tokens": ["ge\u00b7bar", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "-+", "measure": "iambic.single"}, "line.16": {"text": "Ach! sie wissen es nicht, wie der Hohe wandelt'", "tokens": ["Ach", "!", "sie", "wis\u00b7sen", "es", "nicht", ",", "wie", "der", "Ho\u00b7he", "wan\u00b7delt'"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "+-+--++-+--+", "measure": "trochaic.hexa.relaxed"}, "line.17": {"text": "im Volke,", "tokens": ["im", "Vol\u00b7ke", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.18": {"text": "Und vergessen ist fast, was der Lebendige war.", "tokens": ["Und", "ver\u00b7ges\u00b7sen", "ist", "fast", ",", "was", "der", "Le\u00b7ben\u00b7di\u00b7ge", "war", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "ADV", "$,", "PRELS", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.19": {"text": "Wenige kennen ihn doch, und oft erscheint erheiternd", "tokens": ["We\u00b7ni\u00b7ge", "ken\u00b7nen", "ihn", "doch", ",", "und", "oft", "er\u00b7scheint", "er\u00b7hei\u00b7ternd"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "VVFIN", "PPER", "ADV", "$,", "KON", "ADV", "VVFIN", "VVPP"], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}, "line.20": {"text": "Mitten in st\u00fcrmischer Zeit ihnen das himmlische", "tokens": ["Mit\u00b7ten", "in", "st\u00fcr\u00b7mi\u00b7scher", "Zeit", "ih\u00b7nen", "das", "himm\u00b7li\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "PPER", "ART", "ADJA"], "meter": "+--+--+---+-+", "measure": "dactylic.tri.plus"}, "line.21": {"text": "Bild.", "tokens": ["Bild", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.22": {"text": "Allvers\u00f6hnend und still, mit armen Sterblichen", "tokens": ["All\u00b7ver\u00b7s\u00f6h\u00b7nend", "und", "still", ",", "mit", "ar\u00b7men", "Sterb\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "$,", "APPR", "ADJA", "NN"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.23": {"text": "gieng er,", "tokens": ["gieng", "er", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.24": {"text": "Dieser einzige Mann, g\u00f6ttlich im Geiste, dahin.", "tokens": ["Die\u00b7ser", "ein\u00b7zi\u00b7ge", "Mann", ",", "g\u00f6tt\u00b7lich", "im", "Geis\u00b7te", ",", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,", "ADJD", "APPRART", "NN", "$,", "PAV", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.25": {"text": "Keins der Lebenden war aus seiner Seele geschlossen,", "tokens": ["Keins", "der", "Le\u00b7ben\u00b7den", "war", "aus", "sei\u00b7ner", "See\u00b7le", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.26": {"text": "Und die Leiden der Welt trug er an liebender", "tokens": ["Und", "die", "Lei\u00b7den", "der", "Welt", "trug", "er", "an", "lie\u00b7ben\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "PPER", "APPR", "ADJA"], "meter": "--+--++--+--", "measure": "anapaest.di.plus"}, "line.27": {"text": "Brust.", "tokens": ["Brust", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.28": {"text": "Mit dem Tode befreundet' er sich, im Namen der", "tokens": ["Mit", "dem", "To\u00b7de", "be\u00b7freun\u00b7det'", "er", "sich", ",", "im", "Na\u00b7men", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PRF", "$,", "APPRART", "NN", "ART"], "meter": "--+--++-+-+--", "measure": "anapaest.di.plus"}, "line.29": {"text": "Andern", "tokens": ["An\u00b7dern"], "token_info": ["word"], "pos": ["ADJA"], "meter": "+-", "measure": "trochaic.single"}, "line.30": {"text": "Gieng er aus Schmerzen und M\u00fch'n, siegend,", "tokens": ["Gieng", "er", "aus", "Schmer\u00b7zen", "und", "M\u00fch'n", ",", "sie\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,", "VVPP", "$,"], "meter": "++-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.31": {"text": "zum Vater zur\u00fcck.", "tokens": ["zum", "Va\u00b7ter", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.32": {"text": "Und Du kennest ihn auch, Du theuere Mutter,", "tokens": ["Und", "Du", "ken\u00b7nest", "ihn", "auch", ",", "Du", "theu\u00b7e\u00b7re", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "--+-++-+--+-", "measure": "iambic.penta.relaxed"}, "line.33": {"text": "und wandelst", "tokens": ["und", "wan\u00b7delst"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.34": {"text": "Glaubend und duldend und still ihm dem Er-", "tokens": ["Glau\u00b7bend", "und", "dul\u00b7dend", "und", "still", "ihm", "dem", "Er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "ADJD", "KON", "ADJD", "PPER", "ART", "TRUNC"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.35": {"text": "habenen nach.", "tokens": ["ha\u00b7be\u00b7nen", "nach", "."], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "APPR", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.36": {"text": "Sieh! es haben mich selbst verj\u00fcngt die kindlichen", "tokens": ["Sieh", "!", "es", "ha\u00b7ben", "mich", "selbst", "ver\u00b7j\u00fcngt", "die", "kind\u00b7li\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "PPER", "VAFIN", "PPER", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.37": {"text": "Worte,", "tokens": ["Wor\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.38": {"text": "Und es rinnen, wie einst, Thr\u00e4nen vom Auge", "tokens": ["Und", "es", "rin\u00b7nen", ",", "wie", "einst", ",", "Thr\u00e4\u00b7nen", "vom", "Au\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVINF", "$,", "PWAV", "ADV", "$,", "NN", "APPRART", "NN"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.39": {"text": "mir noch;", "tokens": ["mir", "noch", ";"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "ADV", "$."], "meter": "-+", "measure": "iambic.single"}, "line.40": {"text": "Und ich denke zur\u00fcck an l\u00e4ngst vergangene Tage,", "tokens": ["Und", "ich", "den\u00b7ke", "zu\u00b7r\u00fcck", "an", "l\u00e4ngst", "ver\u00b7gan\u00b7ge\u00b7ne", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.41": {"text": "Und die Heimath erfreut wieder mein einsam", "tokens": ["Und", "die", "Hei\u00b7math", "er\u00b7freut", "wie\u00b7der", "mein", "ein\u00b7sam"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "PPOSAT", "ADJD"], "meter": "--+--++--+-", "measure": "anapaest.di.plus"}, "line.42": {"text": "Gem\u00fcth,", "tokens": ["Ge\u00b7m\u00fcth", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.43": {"text": "Und das Haus, wo ich einst bei deinen Segnungen", "tokens": ["Und", "das", "Haus", ",", "wo", "ich", "einst", "bei", "dei\u00b7nen", "Seg\u00b7nun\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PWAV", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "--+--+-+-++-", "measure": "anapaest.di.plus"}, "line.44": {"text": "aufwuchs,", "tokens": ["auf\u00b7wuchs", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.45": {"text": "Wo, von Liebe gen\u00e4hrt, schneller der Knabe gedieh.", "tokens": ["Wo", ",", "von", "Lie\u00b7be", "ge\u00b7n\u00e4hrt", ",", "schnel\u00b7ler", "der", "Kna\u00b7be", "ge\u00b7dieh", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "NN", "VVPP", "$,", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.46": {"text": "Ach! wie dacht' ich dann oft, Du solltest meiner", "tokens": ["Ach", "!", "wie", "dacht'", "ich", "dann", "oft", ",", "Du", "soll\u00b7test", "mei\u00b7ner"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "PWAV", "VVFIN", "PPER", "ADV", "ADV", "$,", "PPER", "VMFIN", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.47": {"text": "Dich freuen,", "tokens": ["Dich", "freu\u00b7en", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.48": {"text": "Wenn ich ferne mich sah wirkend in offener Welt.", "tokens": ["Wenn", "ich", "fer\u00b7ne", "mich", "sah", "wir\u00b7kend", "in", "of\u00b7fe\u00b7ner", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.49": {"text": "Manches hab' ich versucht und getr\u00e4umt und habe", "tokens": ["Man\u00b7ches", "hab'", "ich", "ver\u00b7sucht", "und", "ge\u00b7tr\u00e4umt", "und", "ha\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "PPER", "VVPP", "KON", "VVPP", "KON", "VAFIN"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.50": {"text": "die Brust mir", "tokens": ["die", "Brust", "mir"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.51": {"text": "Wund gerungen inde\u00df, aber ihr heilet sie mir", "tokens": ["Wund", "ge\u00b7run\u00b7gen", "in\u00b7de\u00df", ",", "a\u00b7ber", "ihr", "hei\u00b7let", "sie", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "ADV", "$,", "KON", "PPER", "VVFIN", "PPER", "PPER"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.52": {"text": "O ihr Lieben; und lange, wie Du, o Mutter!", "tokens": ["O", "ihr", "Lie\u00b7ben", ";", "und", "lan\u00b7ge", ",", "wie", "Du", ",", "o", "Mut\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "$.", "KON", "ADV", "$,", "PWAV", "PPER", "$,", "FM", "NN", "$."], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.53": {"text": "zu leben,", "tokens": ["zu", "le\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.54": {"text": "Will ich lernen; es ist ruhig das Alter und", "tokens": ["Will", "ich", "ler\u00b7nen", ";", "es", "ist", "ru\u00b7hig", "das", "Al\u00b7ter", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF", "$.", "PPER", "VAFIN", "ADJD", "ART", "NN", "KON"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.55": {"text": "fromm.", "tokens": ["fromm", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+", "measure": "single.up"}, "line.56": {"text": "Kommen will ich zu Dir, dann segne den Enkel", "tokens": ["Kom\u00b7men", "will", "ich", "zu", "Dir", ",", "dann", "seg\u00b7ne", "den", "En\u00b7kel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "APPR", "PPER", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.57": {"text": "noch einmal,", "tokens": ["noch", "ein\u00b7mal", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.58": {"text": "Da\u00df Dir halte der Mann, was er, als Knabe,", "tokens": ["Da\u00df", "Dir", "hal\u00b7te", "der", "Mann", ",", "was", "er", ",", "als", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "$,", "PWS", "PPER", "$,", "KOUS", "NN", "$,"], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.59": {"text": "gelobt.", "tokens": ["ge\u00b7lobt", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}}}}}