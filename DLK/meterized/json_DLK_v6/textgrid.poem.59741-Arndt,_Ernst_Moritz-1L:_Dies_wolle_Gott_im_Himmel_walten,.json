{"textgrid.poem.59741": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dies wolle Gott im Himmel walten,", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dies wolle Gott im Himmel walten,", "tokens": ["Dies", "wol\u00b7le", "Gott", "im", "Him\u00b7mel", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der jedes gute Werk regiert!", "tokens": ["Der", "je\u00b7des", "gu\u00b7te", "Werk", "re\u00b7giert", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hier stehn wir halbzerri\u00dfnen Alten", "tokens": ["Hier", "stehn", "wir", "halb\u00b7zer\u00b7ri\u00df\u00b7nen", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In frischer Reihe aufmarschiert;", "tokens": ["In", "fri\u00b7scher", "Rei\u00b7he", "auf\u00b7mar\u00b7schiert", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gebete gehn zur Himmelsbl\u00e4ue,", "tokens": ["Ge\u00b7be\u00b7te", "gehn", "zur", "Him\u00b7mels\u00b7bl\u00e4u\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wir feiern heut ein gro\u00dfes Fest,", "tokens": ["Wir", "fei\u00b7ern", "heut", "ein", "gro\u00b7\u00dfes", "Fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein sch\u00f6nstes Fest, ein Fest der Treue:", "tokens": ["Ein", "sch\u00f6ns\u00b7tes", "Fest", ",", "ein", "Fest", "der", "Treu\u00b7e", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wir nageln unsre Fahne fest.", "tokens": ["Wir", "na\u00b7geln", "uns\u00b7re", "Fah\u00b7ne", "fest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dies meint nicht ", "tokens": ["Dies", "meint", "nicht"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VVFIN", "PTKNEG"], "meter": "+--", "measure": "dactylic.init"}, "line.2": {"text": "Die mu\u00df durch Gott gefestet sein,", "tokens": ["Die", "mu\u00df", "durch", "Gott", "ge\u00b7fes\u00b7tet", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "NN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df, wann die Schlachtenwetter hageln", "tokens": ["Da\u00df", ",", "wann", "die", "Schlach\u00b7ten\u00b7wet\u00b7ter", "ha\u00b7geln"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "PWAV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Blei und Eisen niederspein,", "tokens": ["Und", "Blei", "und", "Ei\u00b7sen", "nie\u00b7der\u00b7spein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Fahne fliege als ein Zeichen,", "tokens": ["Die", "Fah\u00b7ne", "flie\u00b7ge", "als", "ein", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Ehre Pfand, der Treue Pfand,", "tokens": ["Der", "Eh\u00b7re", "Pfand", ",", "der", "Treu\u00b7e", "Pfand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df in dem Kampf kein Mann will weichen", "tokens": ["Da\u00df", "in", "dem", "Kampf", "kein", "Mann", "will", "wei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "PIAT", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "F\u00fcr K\u00f6nig, Gott und Vaterland.", "tokens": ["F\u00fcr", "K\u00f6\u00b7nig", ",", "Gott", "und", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So stehen wir, die Veteranen,", "tokens": ["So", "ste\u00b7hen", "wir", ",", "die", "Ve\u00b7te\u00b7ra\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie uns die Treue hergebot,", "tokens": ["Wie", "uns", "die", "Treu\u00b7e", "her\u00b7ge\u00b7bot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und denken an zerscho\u00dfne Fahnen", "tokens": ["Und", "den\u00b7ken", "an", "zer\u00b7scho\u00df\u00b7ne", "Fah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und tapfrer Kameraden Tod,", "tokens": ["Und", "tapf\u00b7rer", "Ka\u00b7me\u00b7ra\u00b7den", "Tod", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "An hei\u00dfe Tage, schwere Wunden,", "tokens": ["An", "hei\u00b7\u00dfe", "Ta\u00b7ge", ",", "schwe\u00b7re", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wo Schlachtendunkel uns umzog,", "tokens": ["Wo", "Schlach\u00b7ten\u00b7dun\u00b7kel", "uns", "um\u00b7zog", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch auch an manche Freudenstunden,", "tokens": ["Doch", "auch", "an", "man\u00b7che", "Freu\u00b7den\u00b7stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wo Preu\u00dfens Adler oben flog.", "tokens": ["Wo", "Preu\u00b7\u00dfens", "Ad\u00b7ler", "o\u00b7ben", "flog", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So stehn wir hier, die Veteranen,", "tokens": ["So", "stehn", "wir", "hier", ",", "die", "Ve\u00b7te\u00b7ra\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als rief es: ", "tokens": ["Als", "rief", "es", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Vor allen denkt man heut der Ahnen,", "tokens": ["Vor", "al\u00b7len", "denkt", "man", "heut", "der", "Ah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "PIS", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Heldengeister heut im Heer,", "tokens": ["Der", "Hel\u00b7den\u00b7geis\u00b7ter", "heut", "im", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der h\u00f6chsten, hellsten Siegesblitze \u2013", "tokens": ["Der", "h\u00f6chs\u00b7ten", ",", "hells\u00b7ten", "Sie\u00b7ges\u00b7blit\u00b7ze", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr Name klingt Unsterblichkeit \u2013", "tokens": ["Ihr", "Na\u00b7me", "klingt", "U\u00b7nsterb\u00b7lich\u00b7keit", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Der Friedrich Wilhelme, der Fritze;", "tokens": ["Der", "Fried\u00b7rich", "Wil\u00b7hel\u00b7me", ",", "der", "Frit\u00b7ze", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "$,", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Durch sie sei unser Tuch geweiht!", "tokens": ["Durch", "sie", "sei", "un\u00b7ser", "Tuch", "ge\u00b7weiht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "So stehn wir hier, die Veteranen,", "tokens": ["So", "stehn", "wir", "hier", ",", "die", "Ve\u00b7te\u00b7ra\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und viele fallen uns noch ein,", "tokens": ["Und", "vie\u00b7le", "fal\u00b7len", "uns", "noch", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die leuchten auch als Preu\u00dfens Ahnen \u2013", "tokens": ["Die", "leuch\u00b7ten", "auch", "als", "Preu\u00b7\u00dfens", "Ah\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "KOUS", "NE", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie schaun auf unser Fest mit ein \u2013", "tokens": ["Sie", "schaun", "auf", "un\u00b7ser", "Fest", "mit", "ein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "APPR", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schwerin und Seidlitz, Zieten, Bl\u00fccher \u2013", "tokens": ["Schwe\u00b7rin", "und", "Seid\u00b7litz", ",", "Zie\u00b7ten", ",", "Bl\u00fc\u00b7cher", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "KON", "NE", "$,", "NE", "$,", "NE", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wer z\u00e4hlte alle Helden her,", "tokens": ["Wer", "z\u00e4hl\u00b7te", "al\u00b7le", "Hel\u00b7den", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die f\u00fcllen die Geschichtenb\u00fccher", "tokens": ["Die", "f\u00fcl\u00b7len", "die", "Ge\u00b7schich\u00b7ten\u00b7b\u00fc\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit sch\u00f6nster deutscher Siegesm\u00e4r?", "tokens": ["Mit", "sch\u00f6ns\u00b7ter", "deut\u00b7scher", "Sie\u00b7ges\u00b7m\u00e4r", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und nun das h\u00f6chste Hoch der Alten!", "tokens": ["Und", "nun", "das", "h\u00f6chs\u00b7te", "Hoch", "der", "Al\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Himmel steige das Gebet!", "tokens": ["Zum", "Him\u00b7mel", "stei\u00b7ge", "das", "Ge\u00b7bet", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wir wollen feste Treue halten,", "tokens": ["Wir", "wol\u00b7len", "fes\u00b7te", "Treu\u00b7e", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo diese Fahne vor uns weht;", "tokens": ["Wo", "die\u00b7se", "Fah\u00b7ne", "vor", "uns", "weht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und mu\u00df sie einst im Felde fliegen", "tokens": ["Und", "mu\u00df", "sie", "einst", "im", "Fel\u00b7de", "flie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den stolzen Preu\u00dfenadlerflug,", "tokens": ["Den", "stol\u00b7zen", "Preu\u00b7\u00dfen\u00b7ad\u00b7ler\u00b7flug", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So bleibe ", "tokens": ["So", "blei\u00b7be"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Der Veteranen Ehrenspruch.", "tokens": ["Der", "Ve\u00b7te\u00b7ra\u00b7nen", "Eh\u00b7ren\u00b7spruch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}