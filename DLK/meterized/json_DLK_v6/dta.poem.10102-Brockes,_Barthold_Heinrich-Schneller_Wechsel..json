{"dta.poem.10102": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Schneller Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich stand, an einem heitern Morgen,", "tokens": ["Ich", "stand", ",", "an", "ei\u00b7nem", "hei\u00b7tern", "Mor\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nachdem das falbe Heer der Schatten", "tokens": ["Nach\u00b7dem", "das", "fal\u00b7be", "Heer", "der", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sich mit der k\u00fchlen Nacht verborgen,", "tokens": ["Sich", "mit", "der", "k\u00fch\u00b7len", "Nacht", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bey meines Brunnens Wasser-Strahl\u2019;", "tokens": ["Bey", "mei\u00b7nes", "Brun\u00b7nens", "Was\u00b7ser\u00b7Strahl'", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und sahe, mit vergn\u00fcgten Blicken,", "tokens": ["Und", "sa\u00b7he", ",", "mit", "ver\u00b7gn\u00fcg\u00b7ten", "Bli\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie, von dem Morgen-Licht, sich alle Tropfen schm\u00fccken.", "tokens": ["Wie", ",", "von", "dem", "Mor\u00b7gen\u00b7Licht", ",", "sich", "al\u00b7le", "Trop\u00b7fen", "schm\u00fc\u00b7cken", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "ART", "NN", "$,", "PRF", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Wolcken, die bisher die Lufft verh\u00fcllet hatten,", "tokens": ["Die", "Wol\u00b7cken", ",", "die", "bis\u00b7her", "die", "Lufft", "ver\u00b7h\u00fcl\u00b7let", "hat\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zertheilten sich, und liessen Wunder-sch\u00f6n,", "tokens": ["Zer\u00b7theil\u00b7ten", "sich", ",", "und", "lies\u00b7sen", "Wun\u00b7der\u00b7sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In hellen Silber-weissen Grentzen,", "tokens": ["In", "hel\u00b7len", "Sil\u00b7ber\u00b7weis\u00b7sen", "Grent\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die holen Tieffen, wie Sapphir,", "tokens": ["Die", "ho\u00b7len", "Tief\u00b7fen", ",", "wie", "Sap\u00b7phir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ja aller Creaturen Zier,", "tokens": ["Ja", "al\u00b7ler", "Crea\u00b7tu\u00b7ren", "Zier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PIAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Das Gold der Sonnen selber, sehn.", "tokens": ["Das", "Gold", "der", "Son\u00b7nen", "sel\u00b7ber", ",", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "$,", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der helle Glantz, der Welt und Himmel f\u00fcllet,", "tokens": ["Der", "hel\u00b7le", "Glantz", ",", "der", "Welt", "und", "Him\u00b7mel", "f\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ward an dem Ort, woselbst das Wasser quillet,", "tokens": ["Ward", "an", "dem", "Ort", ",", "wo\u00b7selbst", "das", "Was\u00b7ser", "quil\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wo es wieder nieder f\u00e4llt,", "tokens": ["Und", "wo", "es", "wie\u00b7der", "nie\u00b7der", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In einem Schimmer vorgestellt,", "tokens": ["In", "ei\u00b7nem", "Schim\u00b7mer", "vor\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der unbeschreiblich ist. Die regen Wellen wallen", "tokens": ["Der", "un\u00b7be\u00b7schreib\u00b7lich", "ist", ".", "Die", "re\u00b7gen", "Wel\u00b7len", "wal\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VAFIN", "$.", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und bilden recht des Himmels Licht,", "tokens": ["Und", "bil\u00b7den", "recht", "des", "Him\u00b7mels", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Als wie in Spiegeln von Crystallen.", "tokens": ["Als", "wie", "in", "Spie\u00b7geln", "von", "Crys\u00b7tal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Jhr unaufh\u00f6rliches bewegen", "tokens": ["Ihr", "un\u00b7auf\u00b7h\u00f6r\u00b7li\u00b7ches", "be\u00b7we\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Macht, da\u00df die Strahlen r\u00fcckw\u00e4rts prallen,", "tokens": ["Macht", ",", "da\u00df", "die", "Strah\u00b7len", "r\u00fcck\u00b7w\u00e4rts", "pral\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Uns durch das Aug\u2019 ins Hertze fallen,", "tokens": ["Uns", "durch", "das", "Aug'", "ins", "Hert\u00b7ze", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "APPRART", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und eine Seelen-Freud\u2019 erregen", "tokens": ["Und", "ei\u00b7ne", "See\u00b7len\u00b7Freud'", "er\u00b7re\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Demjenigen, der an der Creaturen Sch\u00e4tzen", "tokens": ["Dem\u00b7je\u00b7ni\u00b7gen", ",", "der", "an", "der", "Crea\u00b7tu\u00b7ren", "Sch\u00e4t\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "$,", "PRELS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Sich dann und wann gewehnt hat zu ergetzen.", "tokens": ["Sich", "dann", "und", "wann", "ge\u00b7wehnt", "hat", "zu", "er\u00b7get\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "KON", "PWAV", "VVPP", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Es  blitzt, es gl\u00e4ntzt, es funckelt auf der Fluth,", "tokens": ["Es", "blitzt", ",", "es", "gl\u00e4ntzt", ",", "es", "fun\u00b7ckelt", "auf", "der", "Fluth", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Jm hellen Wiederschein, der Sonnen Wunder-Gluth,", "tokens": ["Jm", "hel\u00b7len", "Wie\u00b7der\u00b7schein", ",", "der", "Son\u00b7nen", "Wun\u00b7der\u00b7Gluth", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die aller Farben Sch\u00f6nheits-Quelle.", "tokens": ["Die", "al\u00b7ler", "Far\u00b7ben", "Sch\u00f6n\u00b7heits\u00b7Quel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es blinckt auf einer glatten Stelle,", "tokens": ["Es", "blinckt", "auf", "ei\u00b7ner", "glat\u00b7ten", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es blinckt auf einer ieden Welle,", "tokens": ["Es", "blinckt", "auf", "ei\u00b7ner", "ie\u00b7den", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die sich in Circkelchen formirt,", "tokens": ["Die", "sich", "in", "Cir\u00b7ckel\u00b7chen", "for\u00b7mirt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vom Sonnen-Licht gef\u00e4rbet, und geziert.", "tokens": ["Vom", "Son\u00b7nen\u00b7Licht", "ge\u00b7f\u00e4r\u00b7bet", ",", "und", "ge\u00b7ziert", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Man siehet Licht und Glantz auf regen Kreisen schwimmen;", "tokens": ["Man", "sie\u00b7het", "Licht", "und", "Glantz", "auf", "re\u00b7gen", "Krei\u00b7sen", "schwim\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "KON", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Man sieht, als wie ein Feur, das rege Wasser glimmen.", "tokens": ["Man", "sieht", ",", "als", "wie", "ein", "Feur", ",", "das", "re\u00b7ge", "Was\u00b7ser", "glim\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "KOKOM", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Zumahl", "tokens": ["Zu\u00b7mahl"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Da, wo der Wasser-Strahl", "tokens": ["Da", ",", "wo", "der", "Was\u00b7ser\u00b7Strahl"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Das untre Wasser trifft und bricht,", "tokens": ["Das", "un\u00b7tre", "Was\u00b7ser", "trifft", "und", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Indem daselbst, als wenn sie leben,", "tokens": ["In\u00b7dem", "da\u00b7selbst", ",", "als", "wenn", "sie", "le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PAV", "$,", "KOKOM", "KOUS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Die Tropfen sprudelnd sich erheben,", "tokens": ["Die", "Trop\u00b7fen", "spru\u00b7delnd", "sich", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Und sch\u00e4umend kochen, h\u00fcpfen springen:", "tokens": ["Und", "sch\u00e4u\u00b7mend", "ko\u00b7chen", ",", "h\u00fcp\u00b7fen", "sprin\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$,", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Wodurch sie ein best\u00e4ndigs Licht,", "tokens": ["Wo\u00b7durch", "sie", "ein", "be\u00b7st\u00e4n\u00b7digs", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Als dessen Theilchen, an und in sie dringen,", "tokens": ["Als", "des\u00b7sen", "Theil\u00b7chen", ",", "an", "und", "in", "sie", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRELAT", "NN", "$,", "PTKVZ", "KON", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Dem sie beschauenden Gesicht", "tokens": ["Dem", "sie", "be\u00b7schau\u00b7en\u00b7den", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Mit tausend Lust, im r\u00fcckw\u00e4rts prallen, bringen.", "tokens": ["Mit", "tau\u00b7send", "Lust", ",", "im", "r\u00fcck\u00b7w\u00e4rts", "pral\u00b7len", ",", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$,", "APPRART", "ADV", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Indem ich nun vergn\u00fcget stehe,", "tokens": ["In\u00b7dem", "ich", "nun", "ver\u00b7gn\u00fc\u00b7get", "ste\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und diesen weissen Glantz besehe,", "tokens": ["Und", "die\u00b7sen", "weis\u00b7sen", "Glantz", "be\u00b7se\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bedeckt ein falber Wolcken-Schleyer", "tokens": ["Be\u00b7deckt", "ein", "fal\u00b7ber", "Wol\u00b7cken\u00b7Schley\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von ungefehr des Himmels Feuer.", "tokens": ["Von", "un\u00b7ge\u00b7fehr", "des", "Him\u00b7mels", "Feu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht zu beschreiben ist, was auf der Fluth", "tokens": ["Nicht", "zu", "be\u00b7schrei\u00b7ben", "ist", ",", "was", "auf", "der", "Fluth"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "PTKZU", "VVINF", "VAFIN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der Umstand gleich f\u00fcr Wirckung thut.", "tokens": ["Der", "Um\u00b7stand", "gleich", "f\u00fcr", "Wir\u00b7ckung", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Das Licht verschwand, der Silber-Glantz", "tokens": ["Das", "Licht", "ver\u00b7schwand", ",", "der", "Sil\u00b7ber\u00b7Glantz"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, 