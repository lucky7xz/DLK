{"textgrid.poem.43434": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Freut euch alle, freut euch alle,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freut euch alle, freut euch alle,", "tokens": ["Freut", "euch", "al\u00b7le", ",", "freut", "euch", "al\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "$,", "VVFIN", "PPER", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lobet Gott mit Jubelschalle,", "tokens": ["Lo\u00b7bet", "Gott", "mit", "Ju\u00b7bel\u00b7schal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der noch immer Wunder thut.", "tokens": ["Der", "noch", "im\u00b7mer", "Wun\u00b7der", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Das Censuredict das neue", "tokens": ["Das", "Cen\u00b7su\u00b7re\u00b7dict", "das", "neu\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Will, da\u00df alle Welt sich freue \u2013", "tokens": ["Will", ",", "da\u00df", "al\u00b7le", "Welt", "sich", "freu\u00b7e", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "KOUS", "PIAT", "NN", "PRF", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ach, wie ist es mild und gut!", "tokens": ["Ach", ",", "wie", "ist", "es", "mild", "und", "gut", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie ein Stern aus finstrer Wolke", "tokens": ["Wie", "ein", "Stern", "aus", "finst\u00b7rer", "Wol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kam es her zu unserm Volke", "tokens": ["Kam", "es", "her", "zu", "un\u00b7serm", "Vol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und erschien als heil'ger Christ.", "tokens": ["Und", "er\u00b7schien", "als", "heil'\u00b7ger", "Christ", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Freut euch, Kinder, Frau'n und Greise!", "tokens": ["Freut", "euch", ",", "Kin\u00b7der", ",", "Frau'n", "und", "Grei\u00b7se", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Freut euch, Fromme, Klug' und Weise!", "tokens": ["Freut", "euch", ",", "From\u00b7me", ",", "Klug'", "und", "Wei\u00b7se", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seht, wie gut und mild es ist.", "tokens": ["Seht", ",", "wie", "gut", "und", "mild", "es", "ist", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ADJD", "KON", "ADJD", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wollt ihr ferner euch beschweren?", "tokens": ["Wollt", "ihr", "fer\u00b7ner", "euch", "be\u00b7schwe\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00f6nnet ihr noch mehr begehren?", "tokens": ["K\u00f6n\u00b7net", "ihr", "noch", "mehr", "be\u00b7geh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Querulanten, schweiget still!", "tokens": ["Que\u00b7ru\u00b7lan\u00b7ten", ",", "schwei\u00b7get", "still", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ja, wir d\u00fcrfen Alles sagen,", "tokens": ["Ja", ",", "wir", "d\u00fcr\u00b7fen", "Al\u00b7les", "sa\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles w\u00fcnschen, hoffen, klagen,", "tokens": ["Al\u00b7les", "w\u00fcn\u00b7schen", ",", "hof\u00b7fen", ",", "kla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PIS", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alles \u2013 wenn's der Censor will.", "tokens": ["Al\u00b7les", "\u2013", "wenn's", "der", "Cen\u00b7sor", "will", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$(", "KOUS", "ART", "NN", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}