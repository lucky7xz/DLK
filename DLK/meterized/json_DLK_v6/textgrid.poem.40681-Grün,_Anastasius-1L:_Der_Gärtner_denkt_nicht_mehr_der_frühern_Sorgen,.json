{"textgrid.poem.40681": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der G\u00e4rtner denkt nicht mehr der fr\u00fchern Sorgen,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der G\u00e4rtner denkt nicht mehr der fr\u00fchern Sorgen,", "tokens": ["Der", "G\u00e4rt\u00b7ner", "denkt", "nicht", "mehr", "der", "fr\u00fc\u00b7hern", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn seine Blumen bl\u00fchen;", "tokens": ["Wenn", "sei\u00b7ne", "Blu\u00b7men", "bl\u00fc\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Pfl\u00fcger auch verga\u00df der alten M\u00fchen,", "tokens": ["Der", "Pfl\u00fc\u00b7ger", "auch", "ver\u00b7ga\u00df", "der", "al\u00b7ten", "M\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn er sein Korn geborgen;", "tokens": ["Wenn", "er", "sein", "Korn", "ge\u00b7bor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Des Tages Last vers\u00fc\u00dft es dem Gem\u00fcthe,", "tokens": ["Des", "Ta\u00b7ges", "Last", "ver\u00b7s\u00fc\u00dft", "es", "dem", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das klug von jenen lernte,", "tokens": ["Das", "klug", "von", "je\u00b7nen", "lern\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "APPR", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Beim Pflanzen schon zu denken an die Bl\u00fcthe,", "tokens": ["Beim", "Pflan\u00b7zen", "schon", "zu", "den\u00b7ken", "an", "die", "Bl\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "PTKZU", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Beim S\u00e4en an die Ernte.", "tokens": ["Beim", "S\u00e4\u00b7en", "an", "die", "Ern\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}