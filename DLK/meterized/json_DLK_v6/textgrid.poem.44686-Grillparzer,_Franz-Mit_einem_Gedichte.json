{"textgrid.poem.44686": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Mit einem Gedichte", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn der Dichter sonst wohl adelt,", "tokens": ["Wenn", "der", "Dich\u00b7ter", "sonst", "wohl", "a\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was er klein und niedrig fand;", "tokens": ["Was", "er", "klein", "und", "nied\u00b7rig", "fand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lobt hier, wer mein Lied auch tadelt,", "tokens": ["Lobt", "hier", ",", "wer", "mein", "Lied", "auch", "ta\u00b7delt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PWS", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenigstens den Gegenstand.", "tokens": ["We\u00b7nigs\u00b7tens", "den", "Ge\u00b7gen\u00b7stand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}