{"dta.poem.6038": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es l\u00e4ss'ts, wenn wir zu Schiff, uns die Erfahrung lernen;", "tokens": ["Es", "l\u00e4ss'ts", ",", "wenn", "wir", "zu", "Schiff", ",", "uns", "die", "Er\u00b7fah\u00b7rung", "ler\u00b7nen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "APPR", "NN", "$,", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Indem wir ja so dann nicht anders meinen;", "tokens": ["In\u00b7dem", "wir", "ja", "so", "dann", "nicht", "an\u00b7ders", "mei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als da\u00df so Th\u00fcrm\u2019, als Felsen, sich entfernen,", "tokens": ["Als", "da\u00df", "so", "Th\u00fcrm'", ",", "als", "Fel\u00b7sen", ",", "sich", "ent\u00b7fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ADV", "NN", "$,", "KOUS", "NN", "$,", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und da\u00df die Ufer stets zu fliehen scheinen.", "tokens": ["Und", "da\u00df", "die", "U\u00b7fer", "stets", "zu", "flie\u00b7hen", "schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}