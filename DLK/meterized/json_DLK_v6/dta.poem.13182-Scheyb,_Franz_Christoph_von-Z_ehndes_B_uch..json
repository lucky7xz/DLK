{"dta.poem.13182": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z ehndes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nachdem Theresia zum Thron gekommen war,", "tokens": ["Nach\u00b7dem", "The\u00b7re\u00b7sia", "zum", "Thron", "ge\u00b7kom\u00b7men", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPRART", "NN", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "So stellte sich der Saal in solcher Ordnung dar,", "tokens": ["So", "stell\u00b7te", "sich", "der", "Saal", "in", "sol\u00b7cher", "Ord\u00b7nung", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df er dieselbige mit einem Krei\u00df umrunge,", "tokens": ["Da\u00df", "er", "die\u00b7sel\u00b7bi\u00b7ge", "mit", "ei\u00b7nem", "Krei\u00df", "um\u00b7run\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In welchen, wo man konnt, sich jede Tugend", "tokens": ["In", "wel\u00b7chen", ",", "wo", "man", "konnt", ",", "sich", "je\u00b7de", "Tu\u00b7gend"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PWAT", "$,", "PWAV", "PIS", "VMFIN", "$,", "PRF", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "drunge.", "tokens": ["drun\u00b7ge", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}