{"dta.poem.4020": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Ehmals und Jetzt .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "In j\u00fcngeren Tagen war ich des Morgens froh,", "tokens": ["In", "j\u00fcn\u00b7ge\u00b7ren", "Ta\u00b7gen", "war", "ich", "des", "Mor\u00b7gens", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPER", "ART", "ADV", "ADJD", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Des Abends weint' ich; jetzt, da ich \u00e4lter bin,", "tokens": ["Des", "A\u00b7bends", "weint'", "ich", ";", "jetzt", ",", "da", "ich", "\u00e4l\u00b7ter", "bin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$.", "ADV", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Beginn' ich zweifelnd meinen Tag, doch", "tokens": ["Be\u00b7ginn'", "ich", "zwei\u00b7felnd", "mei\u00b7nen", "Tag", ",", "doch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVIMP", "PPER", "ADJD", "PPOSAT", "NN", "$,", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Heilig und heiter ist mir sein Ende.", "tokens": ["Hei\u00b7lig", "und", "hei\u00b7ter", "ist", "mir", "sein", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "KON", "ADJD", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}}}}