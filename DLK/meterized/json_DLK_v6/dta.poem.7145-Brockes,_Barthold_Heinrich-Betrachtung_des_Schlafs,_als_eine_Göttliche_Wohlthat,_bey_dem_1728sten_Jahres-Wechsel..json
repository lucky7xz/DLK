{"dta.poem.7145": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer in so naher Noth von ohngefehr erweckt,", "tokens": ["Wer", "in", "so", "na\u00b7her", "Noth", "von", "ohn\u00b7ge\u00b7fehr", "er\u00b7weckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADV", "ADJA", "NN", "APPR", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und schnell ermuntert wird, scheint von des Todes Schwelle,", "tokens": ["Und", "schnell", "er\u00b7mun\u00b7tert", "wird", ",", "scheint", "von", "des", "To\u00b7des", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "VAFIN", "$,", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ja gleichsam selbsten aus der H\u00f6lle,", "tokens": ["Ja", "gleich\u00b7sam", "selbs\u00b7ten", "aus", "der", "H\u00f6l\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "ADJA", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Entrissen und befreyt.", "tokens": ["Ent\u00b7ris\u00b7sen", "und", "be\u00b7freyt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}