{"dta.poem.13164": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jn diesem Augenblick erhob sich das Geschrey:", "tokens": ["In", "die\u00b7sem", "Au\u00b7gen\u00b7blick", "er\u00b7hob", "sich", "das", "Ge\u00b7schrey", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df auch ", "tokens": ["Da\u00df", "auch"], "token_info": ["word", "word"], "pos": ["KOUS", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "295Sie komme selbst zum Rath. So durfft man nicht verweilen:", "tokens": ["kom\u00b7me", "selbst", "zum", "Rath", ".", "So", "durfft", "man", "nicht", "ver\u00b7wei\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "$.", "ADV", "VVFIN", "PIS", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Gleich fieng man an den Krei\u00df in Reihen auszutheilen.", "tokens": ["Gleich", "fi\u00b7eng", "man", "an", "den", "Krei\u00df", "in", "Rei\u00b7hen", "aus\u00b7zu\u00b7thei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "APPR", "NN", "VVIZU", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.5": {"text": "Es wurde Plaz gemacht. Man drang sich \u00fcberall,", "tokens": ["Es", "wur\u00b7de", "Plaz", "ge\u00b7macht", ".", "Man", "drang", "sich", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "VVPP", "$.", "PIS", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df die Versammlung sich gleich einem Wellen-Schwall", "tokens": ["Da\u00df", "die", "Ver\u00b7samm\u00b7lung", "sich", "gleich", "ei\u00b7nem", "Wel\u00b7len\u00b7Schwall"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PRF", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "An manchem Ort bewog und durch einander wunde;", "tokens": ["An", "man\u00b7chem", "Ort", "be\u00b7wog", "und", "durch", "ein\u00b7an\u00b7der", "wun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "KON", "APPR", "PRF", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "300Wodurch ein solch Ger\u00e4usch, ein solch Get\u00f6\u00df entstunde,", "tokens": ["ein", "solch", "Ge\u00b7r\u00e4usch", ",", "ein", "solch", "Ge\u00b7t\u00f6\u00df", "ent\u00b7stun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wie es in dem Geb\u00fcsch und in den B\u00e4umen braust,", "tokens": ["Wie", "es", "in", "dem", "Ge\u00b7b\u00fcsch", "und", "in", "den", "B\u00e4u\u00b7men", "braust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.10": {"text": "Wann ein erz\u00fcrnter Wind durch Aest\u2019 und Bl\u00e4tter saust.", "tokens": ["Wann", "ein", "er\u00b7z\u00fcrn\u00b7ter", "Wind", "durch", "A\u00b7est'", "und", "Bl\u00e4t\u00b7ter", "saust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Man wu\u00dfte nicht, wo man sich hin begeben sollte,", "tokens": ["Man", "wu\u00df\u00b7te", "nicht", ",", "wo", "man", "sich", "hin", "be\u00b7ge\u00b7ben", "soll\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "$,", "PWAV", "PIS", "PRF", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wann man die K\u00f6niginn bequemlich sehen wollte.", "tokens": ["Wann", "man", "die", "K\u00f6\u00b7ni\u00b7ginn", "be\u00b7quem\u00b7lich", "se\u00b7hen", "woll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "305Nichts halff als die Geduld. Bi\u00df jemand mit der Hand", "tokens": ["halff", "als", "die", "Ge\u00b7duld", ".", "Bi\u00df", "je\u00b7mand", "mit", "der", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "$.", "APPR", "PIS", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.14": {"text": "Das Zeichen gab und wies, da\u00df, wo man sich befand", "tokens": ["Das", "Zei\u00b7chen", "gab", "und", "wies", ",", "da\u00df", ",", "wo", "man", "sich", "be\u00b7fand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,", "KOUS", "$,", "PWAV", "PIS", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Man m\u00f6chte ruhig seyn, und jenen Raum verlassen", "tokens": ["Man", "m\u00f6ch\u00b7te", "ru\u00b7hig", "seyn", ",", "und", "je\u00b7nen", "Raum", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ADJD", "VAINF", "$,", "KON", "PDAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Auf dem die Tugenden vorher im Krei\u00dfe sassen.", "tokens": ["Auf", "dem", "die", "Tu\u00b7gen\u00b7den", "vor\u00b7her", "im", "Krei\u00b7\u00dfe", "sas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}