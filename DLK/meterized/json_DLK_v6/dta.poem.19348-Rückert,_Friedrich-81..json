{"dta.poem.19348": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "81.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die S\u00fcnd' ist innerlich; und innerlich f\u00fcr sich", "tokens": ["Die", "S\u00fcnd'", "ist", "in\u00b7ner\u00b7lich", ";", "und", "in\u00b7ner\u00b7lich", "f\u00fcr", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "KON", "ADJD", "APPR", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Seyn wollen, eben das ist S\u00fcnde wesentlich.", "tokens": ["Seyn", "wol\u00b7len", ",", "e\u00b7ben", "das", "ist", "S\u00fcn\u00b7de", "we\u00b7sent\u00b7lich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VMFIN", "$,", "ADV", "PDS", "VAFIN", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die S\u00fcnde kann gar aus dem Innern nicht heraus,", "tokens": ["Die", "S\u00fcn\u00b7de", "kann", "gar", "aus", "dem", "In\u00b7nern", "nicht", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "APPR", "ART", "NN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wie heraus sie tritt, wird Anderes daraus.", "tokens": ["Und", "wie", "he\u00b7raus", "sie", "tritt", ",", "wird", "An\u00b7de\u00b7res", "da\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "PPER", "VVFIN", "$,", "VAFIN", "PIS", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Daraus wird eine That, die in die Reihe trat", "tokens": ["Da\u00b7raus", "wird", "ei\u00b7ne", "That", ",", "die", "in", "die", "Rei\u00b7he", "trat"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der andern Thaten, die dort tragen gute Saat.", "tokens": ["Der", "an\u00b7dern", "Tha\u00b7ten", ",", "die", "dort", "tra\u00b7gen", "gu\u00b7te", "Saat", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "VVINF", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "In Gottes Acker ist von Nutzen auch der Mist;", "tokens": ["In", "Got\u00b7tes", "A\u00b7cker", "ist", "von", "Nut\u00b7zen", "auch", "der", "Mist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VAFIN", "APPR", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Pfui aber \u00fcber dich, wenn du nichts bessers bist.", "tokens": ["Pfui", "a\u00b7ber", "\u00fc\u00b7ber", "dich", ",", "wenn", "du", "nichts", "bes\u00b7sers", "bist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPER", "$,", "KOUS", "PPER", "PIS", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}