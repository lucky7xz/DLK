{"textgrid.poem.47331": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Einladung", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wie soll der Nachtigallen", "tokens": ["O", "wie", "soll", "der", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PWAV", "VMFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seele denn ins Ohr dir fallen,", "tokens": ["See\u00b7le", "denn", "ins", "Ohr", "dir", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn dir immer noch vor Ohren", "tokens": ["Wenn", "dir", "im\u00b7mer", "noch", "vor", "Oh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Summet das Geschw\u00e4tz von Thoren.", "tokens": ["Sum\u00b7met", "das", "Ge\u00b7schw\u00e4tz", "von", "Tho\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und wie soll dir Rosenbl\u00fcte", "tokens": ["Und", "wie", "soll", "dir", "Ro\u00b7sen\u00b7bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "VMFIN", "PPER", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wirklich bl\u00fchen ins Gem\u00fcte,", "tokens": ["Wirk\u00b7lich", "bl\u00fc\u00b7hen", "ins", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Willst du noch nach Schimmer gaffen,", "tokens": ["Willst", "du", "noch", "nach", "Schim\u00b7mer", "gaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den nicht die Natur erschaffen.", "tokens": ["Den", "nicht", "die", "Na\u00b7tur", "er\u00b7schaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Willst du aufgenommen werden", "tokens": ["Willst", "du", "auf\u00b7ge\u00b7nom\u00b7men", "wer\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVPP", "VAINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus dem Irrgewirr auf Erden", "tokens": ["Aus", "dem", "Irr\u00b7ge\u00b7wirr", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In des Fr\u00fchlings heitre Ch\u00f6re,", "tokens": ["In", "des", "Fr\u00fch\u00b7lings", "heit\u00b7re", "Ch\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So nichts andres sieh und h\u00f6re.", "tokens": ["So", "nichts", "and\u00b7res", "sieh", "und", "h\u00f6\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "PIS", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Suche bei uns nicht Zerstreuung,", "tokens": ["Su\u00b7che", "bei", "uns", "nicht", "Zer\u00b7streu\u00b7ung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PTKNEG", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sondern ewige Erfreuung.", "tokens": ["Son\u00b7dern", "e\u00b7wi\u00b7ge", "Er\u00b7freu\u00b7ung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Komm und trinke ganzer Seele", "tokens": ["Komm", "und", "trin\u00b7ke", "gan\u00b7zer", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rosenduft und Philomele!", "tokens": ["Ro\u00b7sen\u00b7duft", "und", "Phi\u00b7lo\u00b7me\u00b7le", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}