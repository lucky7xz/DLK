{"textgrid.poem.67685": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Donnervogel z\u00fcrnte: \u00bbMeinst Du gar,", "tokens": ["Der", "Don\u00b7ner\u00b7vo\u00b7gel", "z\u00fcrn\u00b7te", ":", "\u00bb", "Meinst", "Du", "gar", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du Kriecher, mich den Weg zur H\u00f6h zu lehren?\u00ab", "tokens": ["Du", "Krie\u00b7cher", ",", "mich", "den", "Weg", "zur", "H\u00f6h", "zu", "leh\u00b7ren", "?", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "NN", "$,", "PRF", "ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und greift, ihn zu zerrei\u00dfen. \u00bbNein, f\u00fcrwahr,", "tokens": ["Und", "greift", ",", "ihn", "zu", "zer\u00b7rei\u00b7\u00dfen", ".", "\u00bb", "Nein", ",", "f\u00fcr\u00b7wahr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$.", "$(", "PTKANT", "$,", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcrwahr, wer wollte das begehren?", "tokens": ["F\u00fcr\u00b7wahr", ",", "wer", "woll\u00b7te", "das", "be\u00b7geh\u00b7ren", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VMFIN", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Allein, ich dachte nur, ein hoher Adler fleugt,", "tokens": ["Al\u00b7lein", ",", "ich", "dach\u00b7te", "nur", ",", "ein", "ho\u00b7her", "Ad\u00b7ler", "fleugt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "ADV", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Allein \u2013 ein armer Wurm \u2013 was soll er thun? \u2013 er kreucht.\u00ab", "tokens": ["Al\u00b7lein", "\u2013", "ein", "ar\u00b7mer", "Wurm", "\u2013", "was", "soll", "er", "thun", "?", "\u2013", "er", "kreucht", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "$(", "ART", "ADJA", "NN", "$(", "PWS", "VMFIN", "PPER", "VVINF", "$.", "$(", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Adler flog gro\u00dfm\u00fcthig fort", "tokens": ["Der", "Ad\u00b7ler", "flog", "gro\u00df\u00b7m\u00fct\u00b7hig", "fort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und lie\u00df dem Wurm den Ort.", "tokens": ["Und", "lie\u00df", "dem", "Wurm", "den", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}