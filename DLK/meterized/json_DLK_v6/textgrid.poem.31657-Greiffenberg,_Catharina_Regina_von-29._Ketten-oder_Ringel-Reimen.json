{"textgrid.poem.31657": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "29. Ketten-oder Ringel-Reimen", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00fcssen wir schon hier viel leiden: Gottes Gnad kan alls vers\u00fcssen.", "tokens": ["M\u00fcs\u00b7sen", "wir", "schon", "hier", "viel", "lei\u00b7den", ":", "Got\u00b7tes", "Gnad", "kan", "alls", "ver\u00b7s\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "ADV", "VVINF", "$.", "NN", "NN", "VMFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "streben nach den Himmels-Freuden / ist ger\u00fcster Christen Leben:", "tokens": ["stre\u00b7ben", "nach", "den", "Him\u00b7mels\u00b7Freu\u00b7den", "/", "ist", "ge\u00b7r\u00fcs\u00b7ter", "Chris\u00b7ten", "Le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$(", "VAFIN", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "achten weder Hitz noch Blitz / herzhafft zu dem H\u00f6chsten trachten;", "tokens": ["ach\u00b7ten", "we\u00b7der", "Hitz", "noch", "Blitz", "/", "herz\u00b7hafft", "zu", "dem", "H\u00f6chs\u00b7ten", "trach\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "KON", "NN", "ADV", "NN", "$(", "VVFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "bringet es schon Tr\u00fcbsal- Hitz / letzlich doch mit Palm umringet.", "tokens": ["brin\u00b7get", "es", "schon", "Tr\u00fcb\u00b7sa\u00b7l", "Hitz", "/", "letz\u00b7lich", "doch", "mit", "Palm", "um\u00b7rin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "TRUNC", "NN", "$(", "ADV", "ADV", "APPR", "NE", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Lasset den beleibten Schatten / diese Welt: den Himmel fasset.", "tokens": ["Las\u00b7set", "den", "be\u00b7leib\u00b7ten", "Schat\u00b7ten", "/", "die\u00b7se", "Welt", ":", "den", "Him\u00b7mel", "fas\u00b7set", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$(", "PDAT", "NN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "----+-+-+-+-+-+-", "measure": "unknown.measure.hexa"}, "line.6": {"text": "Fliehet / euch mit dem zu gatten / was verderben nach sich ziehet.", "tokens": ["Flie\u00b7het", "/", "euch", "mit", "dem", "zu", "gat\u00b7ten", "/", "was", "ver\u00b7der\u00b7ben", "nach", "sich", "zie\u00b7het", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "PPER", "APPR", "ART", "PTKZU", "VVINF", "$(", "PWS", "VVFIN", "APPR", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.7": {"text": "weiset / da\u00df ein edles Herz seine St\u00e4rk' im Vngl\u00fcck preiset /", "tokens": ["wei\u00b7set", "/", "da\u00df", "ein", "ed\u00b7les", "Herz", "sei\u00b7ne", "St\u00e4rk'", "im", "Vn\u00b7gl\u00fcck", "prei\u00b7set", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "KOUS", "ART", "ADJA", "NN", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "gl\u00e4nzet in den gr\u00f6sten Schmerz / in der Pein den Schein erg\u00e4nzet.", "tokens": ["gl\u00e4n\u00b7zet", "in", "den", "gr\u00f6s\u00b7ten", "Schmerz", "/", "in", "der", "Pein", "den", "Schein", "er\u00b7g\u00e4n\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$(", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Alle Welt / mag w\u00fcten / toben: wann uns nur des Geistes Strahle", "tokens": ["Al\u00b7le", "Welt", "/", "mag", "w\u00fc\u00b7ten", "/", "to\u00b7ben", ":", "wann", "uns", "nur", "des", "Geis\u00b7tes", "Strah\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$(", "VMFIN", "VVINF", "$(", "VVFIN", "$.", "PWAV", "PPER", "ADV", "ART", "NN", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.10": {"text": "giebet einen Glanz von oben; und das untre nicht betr\u00fcbet.", "tokens": ["gie\u00b7bet", "ei\u00b7nen", "Glanz", "von", "o\u00b7ben", ";", "und", "das", "un\u00b7tre", "nicht", "be\u00b7tr\u00fc\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADV", "$.", "KON", "PDS", "VVFIN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Streitet mit dem Laster-Hauff; dapffern Siegern ist bereitet", "tokens": ["Strei\u00b7tet", "mit", "dem", "Las\u00b7ter\u00b7Hauff", ";", "dapf\u00b7fern", "Sie\u00b7gern", "ist", "be\u00b7rei\u00b7tet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "$.", "ADJA", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "Ehre nach vollendtem Lauff / die kein Vngl\u00fcck mehr verst\u00f6hre.", "tokens": ["Eh\u00b7re", "nach", "vol\u00b7lend\u00b7tem", "Lauff", "/", "die", "kein", "Vn\u00b7gl\u00fcck", "mehr", "ver\u00b7st\u00f6h\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$(", "ART", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "+--+--+--+-+-+-", "measure": "elegiambus"}}}}}