{"textgrid.poem.26296": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Die B\u00e4ume, die lieben", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, die Stunden, die langen!", "tokens": ["Ach", ",", "die", "Stun\u00b7den", ",", "die", "lan\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "$,", "ART", "ADJA", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Die Sonne ist untergegangen", "tokens": ["Die", "Son\u00b7ne", "ist", "un\u00b7ter\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die dunklen B\u00e4ume, die lieben,", "tokens": ["Die", "dunk\u00b7len", "B\u00e4u\u00b7me", ",", "die", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die sind stehen geblieben.", "tokens": ["Die", "sind", "ste\u00b7hen", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVFIN", "VVPP", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Sie wiegen sich bei mir die ganze Nacht,", "tokens": ["Sie", "wie\u00b7gen", "sich", "bei", "mir", "die", "gan\u00b7ze", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wir fliegen dem Mond zu, der sich aufgemacht.", "tokens": ["Wir", "flie\u00b7gen", "dem", "Mond", "zu", ",", "der", "sich", "auf\u00b7ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "PRELS", "PRF", "VVPP", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Ach, die B\u00e4ume, die lieben,", "tokens": ["Ach", ",", "die", "B\u00e4u\u00b7me", ",", "die", "lie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Wenn sie rauschend die Bl\u00e4tter verschieben,", "tokens": ["Wenn", "sie", "rau\u00b7schend", "die", "Bl\u00e4t\u00b7ter", "ver\u00b7schie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Rufen sie Gedachtes in die R\u00e4ume.", "tokens": ["Ru\u00b7fen", "sie", "Ge\u00b7dach\u00b7tes", "in", "die", "R\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Aber ihre Schattenspiele sind ihre Tr\u00e4ume.", "tokens": ["A\u00b7ber", "ih\u00b7re", "Schat\u00b7ten\u00b7spie\u00b7le", "sind", "ih\u00b7re", "Tr\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.5": {"text": "Sie sind nicht wie die andern.", "tokens": ["Sie", "sind", "nicht", "wie", "die", "an\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "KOKOM", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im Stillstehen k\u00f6nnen sie wandern.", "tokens": ["Im", "Still\u00b7ste\u00b7hen", "k\u00f6n\u00b7nen", "sie", "wan\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "VVFIN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Aber da wir einer Erde Kinder sind,", "tokens": ["A\u00b7ber", "da", "wir", "ei\u00b7ner", "Er\u00b7de", "Kin\u00b7der", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "Sprechen die B\u00e4ume, die lieben,", "tokens": ["Spre\u00b7chen", "die", "B\u00e4u\u00b7me", ",", "die", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.9": {"text": "Nicht nur in den Wind.", "tokens": ["Nicht", "nur", "in", "den", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Es ist nicht \u00fcbertrieben:", "tokens": ["Es", "ist", "nicht", "\u00fc\u00b7bert\u00b7rie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Ihre Worte sind oft in mir h\u00e4ngen geblieben.", "tokens": ["Ih\u00b7re", "Wor\u00b7te", "sind", "oft", "in", "mir", "h\u00e4n\u00b7gen", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "APPR", "PPER", "VVFIN", "VVPP", "$."], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}}}}}