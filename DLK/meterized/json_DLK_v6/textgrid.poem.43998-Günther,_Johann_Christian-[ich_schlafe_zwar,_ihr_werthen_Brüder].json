{"textgrid.poem.43998": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[ich schlafe zwar, ihr werthen Br\u00fcder]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich schlafe zwar, ihr werthen Br\u00fcder,", "tokens": ["Ich", "schla\u00b7fe", "zwar", ",", "ihr", "wert\u00b7hen", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich schlafe zwar, so denckt ihr noch;", "tokens": ["Ich", "schla\u00b7fe", "zwar", ",", "so", "denckt", "ihr", "noch", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr haltet meine schlechten Lieder", "tokens": ["Ihr", "hal\u00b7tet", "mei\u00b7ne", "schlech\u00b7ten", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor ein vor mich gezwungnes Joch.", "tokens": ["Vor", "ein", "vor", "mich", "ge\u00b7zwung\u00b7nes", "Joch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch aber sollt ihr dieses wi\u00dfen:", "tokens": ["Doch", "a\u00b7ber", "sollt", "ihr", "die\u00b7ses", "wi\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich handle nicht mit Joabs K\u00fc\u00dfen.", "tokens": ["Ich", "hand\u00b7le", "nicht", "mit", "Joabs", "K\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NE", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich glaube so, so wie ich's meine,", "tokens": ["Ich", "glau\u00b7be", "so", ",", "so", "wie", "ich's", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ADV", "KOKOM", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein andrer glaube, wie er will.", "tokens": ["Ein", "an\u00b7drer", "glau\u00b7be", ",", "wie", "er", "will", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$,", "PWAV", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Kirschen sind zwar etwas kleine,", "tokens": ["Die", "Kir\u00b7schen", "sind", "zwar", "et\u00b7was", "klei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch jede Frucht w\u00e4chst in der Still;", "tokens": ["Doch", "je\u00b7de", "Frucht", "w\u00e4chst", "in", "der", "Still", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und h\u00e4lt man sie vor Stachelbeeren,", "tokens": ["Und", "h\u00e4lt", "man", "sie", "vor", "Sta\u00b7chel\u00b7bee\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was thut's? Man mag sie auch verzehren.", "tokens": ["Was", "thut's", "?", "Man", "mag", "sie", "auch", "ver\u00b7zeh\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$.", "PIS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das weis ich, da\u00df mein treu Gem\u00fcthe", "tokens": ["Das", "weis", "ich", ",", "da\u00df", "mein", "treu", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "PTKVZ", "PPER", "$,", "KOUS", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem P\u00f6bel ein Gel\u00e4chter macht;", "tokens": ["Dem", "P\u00f6\u00b7bel", "ein", "Ge\u00b7l\u00e4ch\u00b7ter", "macht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich weis auch, da\u00df des Himmels G\u00fcte", "tokens": ["Ich", "weis", "auch", ",", "da\u00df", "des", "Him\u00b7mels", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PTKVZ", "ADV", "$,", "KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So gut vor mich als alle wacht,", "tokens": ["So", "gut", "vor", "mich", "als", "al\u00b7le", "wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPER", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und darum wird mir mein Gewi\u00dfen", "tokens": ["Und", "da\u00b7rum", "wird", "mir", "mein", "Ge\u00b7wi\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auch wider Narren zeugen m\u00fc\u00dfen.", "tokens": ["Auch", "wi\u00b7der", "Nar\u00b7ren", "zeu\u00b7gen", "m\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Welt ist jezo voller Narren,", "tokens": ["Die", "Welt", "ist", "je\u00b7zo", "vol\u00b7ler", "Nar\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und darum bin ich einer mit,", "tokens": ["Und", "da\u00b7rum", "bin", "ich", "ei\u00b7ner", "mit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VAFIN", "PPER", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und der hat wohl den gr\u00f6sten Sparren,", "tokens": ["Und", "der", "hat", "wohl", "den", "gr\u00f6s\u00b7ten", "Spar\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der aus gemeinem Gleise tritt;", "tokens": ["Der", "aus", "ge\u00b7mei\u00b7nem", "Glei\u00b7se", "tritt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Inde\u00dfen wird der Schlu\u00df nicht triegen,", "tokens": ["In\u00b7de\u00b7\u00dfen", "wird", "der", "Schlu\u00df", "nicht", "trie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df mancher auch kan wacker l\u00fcgen.", "tokens": ["Da\u00df", "man\u00b7cher", "auch", "kan", "wa\u00b7cker", "l\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VMFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Man leugt bisweilen nach der Mode,", "tokens": ["Man", "leugt", "bis\u00b7wei\u00b7len", "nach", "der", "Mo\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und nach der Mode l\u00fcg auch ich;", "tokens": ["Und", "nach", "der", "Mo\u00b7de", "l\u00fcg", "auch", "ich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ADV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein jeder schwimmt im Narrensode,", "tokens": ["Ein", "je\u00b7der", "schwimmt", "im", "Nar\u00b7ren\u00b7so\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Narren, was verdenckt ihr mich?", "tokens": ["Ihr", "Nar\u00b7ren", ",", "was", "ver\u00b7denckt", "ihr", "mich", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWS", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr Narren l\u00fcgt, so will ich l\u00fcgen,", "tokens": ["Ihr", "Nar\u00b7ren", "l\u00fcgt", ",", "so", "will", "ich", "l\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "ADV", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bis da\u00df wir alle Kappen kriegen.", "tokens": ["Bis", "da\u00df", "wir", "al\u00b7le", "Kap\u00b7pen", "krie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Kappe, die ich jezo trage,", "tokens": ["Die", "Kap\u00b7pe", ",", "die", "ich", "je\u00b7zo", "tra\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Steht mir wie Eulenspiegeln an,", "tokens": ["Steht", "mir", "wie", "Eu\u00b7len\u00b7spie\u00b7geln", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch dies ist die gewi\u00dfe Plage,", "tokens": ["Doch", "dies", "ist", "die", "ge\u00b7wi\u00b7\u00dfe", "Pla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich sie nicht vertragen kan;", "tokens": ["Da\u00df", "ich", "sie", "nicht", "ver\u00b7tra\u00b7gen", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sonst w\u00fcrden auf den Pfafenm\u00fczen", "tokens": ["Sonst", "w\u00fcr\u00b7den", "auf", "den", "Pfa\u00b7fen\u00b7m\u00fc\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Schellen warlich be\u00dfer sizen.", "tokens": ["Die", "Schel\u00b7len", "war\u00b7lich", "be\u00b7\u00dfer", "si\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Was wollt ihr mehr? Der Streusand r\u00fcselt,", "tokens": ["Was", "wollt", "ihr", "mehr", "?", "Der", "Streu\u00b7sand", "r\u00fc\u00b7selt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und ob gleich bey dergleichen Art", "tokens": ["Und", "ob", "gleich", "bey", "derg\u00b7lei\u00b7chen", "Art"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "APPR", "PIS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein toller Schedel altklug gr\u00fcselt,", "tokens": ["Ein", "tol\u00b7ler", "Sche\u00b7del", "alt\u00b7klug", "gr\u00fc\u00b7selt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So bin ich doch wie er verwahrt,", "tokens": ["So", "bin", "ich", "doch", "wie", "er", "ver\u00b7wahrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "KOKOM", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weil, wenn mich stumme W\u00fcrmer rei\u00dfen,", "tokens": ["Weil", ",", "wenn", "mich", "stum\u00b7me", "W\u00fcr\u00b7mer", "rei\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auch ihn die stummen W\u00fcrmer bei\u00dfen.", "tokens": ["Auch", "ihn", "die", "stum\u00b7men", "W\u00fcr\u00b7mer", "bei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Die stummen W\u00fcrmer bei\u00dfen alle;", "tokens": ["Die", "stum\u00b7men", "W\u00fcr\u00b7mer", "bei\u00b7\u00dfen", "al\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIS", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Kl\u00fcgste scheint, wer sie verh\u00e4lt;", "tokens": ["Der", "Kl\u00fcgs\u00b7te", "scheint", ",", "wer", "sie", "ver\u00b7h\u00e4lt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wem die Frucht von Adams Falle", "tokens": ["Und", "wem", "die", "Frucht", "von", "A\u00b7dams", "Fal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ART", "NN", "APPR", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch jezo den Geschmack verg\u00e4llt,", "tokens": ["Noch", "je\u00b7zo", "den", "Ge\u00b7schmack", "ver\u00b7g\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Der wird vorwahr am besten schlie\u00dfen,", "tokens": ["Der", "wird", "vor\u00b7wahr", "am", "bes\u00b7ten", "schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PTKA", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn ihn die Stiche nicht verdrie\u00dfen.", "tokens": ["Wenn", "ihn", "die", "Sti\u00b7che", "nicht", "ver\u00b7drie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}