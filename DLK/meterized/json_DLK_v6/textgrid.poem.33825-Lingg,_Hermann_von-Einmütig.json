{"textgrid.poem.33825": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Einm\u00fctig", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn auch nur ", "tokens": ["Wenn", "auch", "nur"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADV", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Der nicht sich beugt,", "tokens": ["Der", "nicht", "sich", "beugt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "PRF", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mit uns f\u00fcr Wahrheit zeugt,", "tokens": ["Mit", "uns", "f\u00fcr", "Wahr\u00b7heit", "zeugt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie das erhebt!", "tokens": ["Wie", "das", "er\u00b7hebt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wenn nur ein einz'ger Mann", "tokens": ["Wenn", "nur", "ein", "einz'\u00b7ger", "Mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Noch mit uns klagt,", "tokens": ["Noch", "mit", "uns", "klagt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "++-+", "measure": "iambic.di"}, "line.3": {"text": "Mit uns des Schweigens Bann", "tokens": ["Mit", "uns", "des", "Schwei\u00b7gens", "Bann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zu brechen wagt!", "tokens": ["Zu", "bre\u00b7chen", "wagt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Wenn Ein Mensch bei Verstand", "tokens": ["Wenn", "Ein", "Mensch", "bei", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Noch mit uns flucht,", "tokens": ["Noch", "mit", "uns", "flucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Der Vorurteile Band", "tokens": ["Der", "Vor\u00b7ur\u00b7tei\u00b7le", "Band"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zu tilgen sucht!", "tokens": ["Zu", "til\u00b7gen", "sucht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Wenn auch nur ", "tokens": ["Wenn", "auch", "nur"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADV", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Die Hand uns dr\u00fcckt", "tokens": ["Die", "Hand", "uns", "dr\u00fcckt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und mit uns denkt und will,", "tokens": ["Und", "mit", "uns", "denkt", "und", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "KON", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie das begl\u00fcckt!", "tokens": ["Wie", "das", "be\u00b7gl\u00fcckt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}