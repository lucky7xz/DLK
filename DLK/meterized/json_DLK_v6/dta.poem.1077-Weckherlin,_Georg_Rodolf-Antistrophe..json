{"dta.poem.1077": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Antistrophe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Obwol der s\u00fc\u00df-spihlende Got ", "tokens": ["Ob\u00b7wol", "der", "s\u00fc\u00df\u00b7spihlen\u00b7de", "Got"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "(laubet", "tokens": ["(", "lau\u00b7bet"], "token_info": ["punct", "word"], "pos": ["$(", "VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Die zweig stehtsgr\u00fcn vnd frisch-be-", "tokens": ["Die", "zweig", "stehts\u00b7gr\u00fcn", "vnd", "frischbe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJD", "KON", "TRUNC"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Allen abzubrechen erlaubet", "tokens": ["Al\u00b7len", "ab\u00b7zu\u00b7bre\u00b7chen", "er\u00b7lau\u00b7bet"], "token_info": ["word", "word", "word"], "pos": ["PIS", "VVIZU", "VVFIN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Welche nichts fragen nach dem Tod:", "tokens": ["Wel\u00b7che", "nichts", "fra\u00b7gen", "nach", "dem", "Tod", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Kan doch eines jeden verstand", "tokens": ["Kan", "doch", "ei\u00b7nes", "je\u00b7den", "ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "PIAT", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Nicht das stehts-wehrende trumb finden/", "tokens": ["Nicht", "das", "stehts\u00b7weh\u00b7ren\u00b7de", "trumb", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Damit Er m\u00f6g mit wehrter Hand", "tokens": ["Da\u00b7mit", "Er", "m\u00f6g", "mit", "wehr\u00b7ter", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Den vnver w\u00e4lcklichen crantz binden;", "tokens": ["Den", "vn\u00b7ver", "w\u00e4lck\u00b7li\u00b7chen", "crantz", "bin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sondern sehr klein ist die anzahl/", "tokens": ["Son\u00b7dern", "sehr", "klein", "ist", "die", "an\u00b7zahl", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "(darunder Ich verhoff zubleiben)", "tokens": ["(", "da\u00b7run\u00b7der", "Ich", "ver\u00b7hoff", "zu\u00b7blei\u00b7ben", ")"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PAV", "PPER", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Welche in der Ewigkeit sahl", "tokens": ["Wel\u00b7che", "in", "der", "E\u00b7wig\u00b7keit", "sahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAT", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "K\u00f6nden einen namen aufschreiben.", "tokens": ["K\u00f6n\u00b7den", "ei\u00b7nen", "na\u00b7men", "auf\u00b7schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVIZU", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}}}}