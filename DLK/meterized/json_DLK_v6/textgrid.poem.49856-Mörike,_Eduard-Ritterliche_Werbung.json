{"textgrid.poem.49856": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Ritterliche Werbung", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo gehst du hin, du sch\u00f6nes Kind?", "tokens": ["Wo", "gehst", "du", "hin", ",", "du", "sch\u00f6\u00b7nes", "Kind", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu melken, Herr! \u2013 sprach Gotelind.", "tokens": ["Zu", "mel\u00b7ken", ",", "Herr", "!", "\u2013", "sprach", "Go\u00b7te\u00b7lind", "."], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "NN", "$.", "$(", "VVFIN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer ist dein Vater, du sch\u00f6nes Kind?", "tokens": ["Wer", "ist", "dein", "Va\u00b7ter", ",", "du", "sch\u00f6\u00b7nes", "Kind", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der M\u00fcller im Tal \u2013 sprach Gotelind.", "tokens": ["Der", "M\u00fcl\u00b7ler", "im", "Tal", "\u2013", "sprach", "Go\u00b7te\u00b7lind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NE", "APPRART", "NN", "$(", "VVFIN", "NE", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Wie, wenn ich dich freite, sch\u00f6nes Kind?", "tokens": ["Wie", ",", "wenn", "ich", "dich", "frei\u00b7te", ",", "sch\u00f6\u00b7nes", "Kind", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Zu viel der Ehre! \u2013 sprach Gotelind.", "tokens": ["Zu", "viel", "der", "Eh\u00b7re", "!", "\u2013", "sprach", "Go\u00b7te\u00b7lind", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["APPR", "PIS", "ART", "NN", "$.", "$(", "VVFIN", "NE", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Was hast du zur Mitgift, sch\u00f6nes Kind?", "tokens": ["Was", "hast", "du", "zur", "Mit\u00b7gift", ",", "sch\u00f6\u00b7nes", "Kind", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPRART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Herr, mein Gesicht! \u2013 sprach Gotelind.", "tokens": ["Herr", ",", "mein", "Ge\u00b7sicht", "!", "\u2013", "sprach", "Go\u00b7te\u00b7lind", "."], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$.", "$(", "VVFIN", "NE", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "So kann ich dich nicht wohl frein, mein Kind.", "tokens": ["So", "kann", "ich", "dich", "nicht", "wohl", "frein", ",", "mein", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "PTKNEG", "ADV", "PTKVZ", "$,", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wer hat's Euch gehei\u00dfen? \u2013 sprach Gotelind.", "tokens": ["Wer", "hat's", "Euch", "ge\u00b7hei\u00b7\u00dfen", "?", "\u2013", "sprach", "Go\u00b7te\u00b7lind", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "VVPP", "$.", "$(", "VVFIN", "NE", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}