{"dta.poem.10782": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Epigramma Ejusdem  \n  Von zwey newen Buckeligen Eheleuten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Als dieser Tagen erst der Buckel Hans genommen", "tokens": ["Als", "die\u00b7ser", "Ta\u00b7gen", "erst", "der", "Bu\u00b7ckel", "Hans", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "NN", "ADV", "ART", "NN", "NE", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zur Ehe die Hoffer Ann: da fragte m\u00e4nniglich/", "tokens": ["Zur", "E\u00b7he", "die", "Hof\u00b7fer", "Ann", ":", "da", "frag\u00b7te", "m\u00e4n\u00b7nig\u00b7lich", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "NE", "$.", "ADV", "VVFIN", "ADJD", "$("], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wie wirdt das Sprichwort nun hiehero reumen sich/", "tokens": ["Wie", "wirdt", "das", "Sprich\u00b7wort", "nun", "hie\u00b7he\u00b7ro", "reu\u00b7men", "sich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "PAV", "VVFIN", "PRF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df nimmermehr zween Berg zusammen k\u00f6nnen kommen?", "tokens": ["Da\u00df", "nim\u00b7mer\u00b7mehr", "zween", "Berg", "zu\u00b7sam\u00b7men", "k\u00f6n\u00b7nen", "kom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos":