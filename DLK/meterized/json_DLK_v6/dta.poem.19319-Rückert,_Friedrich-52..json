{"dta.poem.19319": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "52.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Unleidlicher ist nichts, geeigneter zu Kr\u00e4mpfen,", "tokens": ["Un\u00b7leid\u00b7li\u00b7cher", "ist", "nichts", ",", "ge\u00b7eig\u00b7ne\u00b7ter", "zu", "Kr\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PIS", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Als zwei Systeme, die als solche sich bek\u00e4mpfen.", "tokens": ["Als", "zwei", "Sys\u00b7te\u00b7me", ",", "die", "als", "sol\u00b7che", "sich", "be\u00b7k\u00e4mp\u00b7fen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "CARD", "NN", "$,", "PRELS", "KOKOM", "PIAT", "PRF", "VVINF", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Dis klappert hier, das dort, mit eigner Formeln Knarren,", "tokens": ["Dis", "klap\u00b7pert", "hier", ",", "das", "dort", ",", "mit", "eig\u00b7ner", "For\u00b7meln", "Knar\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "$,", "PRELS", "ADV", "$,", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wer dazwischen steht und h\u00f6rt es, wird zum Narren.", "tokens": ["Und", "wer", "da\u00b7zwi\u00b7schen", "steht", "und", "h\u00f6rt", "es", ",", "wird", "zum", "Nar\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PAV", "VVFIN", "KON", "VVFIN", "PPER", "$,", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Zwei Instrumenten gleich, in zwei verschiednen T\u00f6nen", "tokens": ["Zwei", "Inst\u00b7ru\u00b7men\u00b7ten", "gleich", ",", "in", "zwei", "ver\u00b7schied\u00b7nen", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["CARD", "NN", "ADV", "$,", "APPR", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gestimmt, wo eines will das andre niederdr\u00f6hnen.", "tokens": ["Ge\u00b7stimmt", ",", "wo", "ei\u00b7nes", "will", "das", "and\u00b7re", "nie\u00b7der\u00b7dr\u00f6h\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "PIS", "VMFIN", "ART", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Jedwedes w\u00e4r', allein geh\u00f6rt, villeicht erfreulich;", "tokens": ["Jed\u00b7we\u00b7des", "w\u00e4r'", ",", "al\u00b7lein", "ge\u00b7h\u00f6rt", ",", "vil\u00b7leicht", "er\u00b7freu\u00b7lich", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "$,", "ADV", "VVFIN", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr Durcheinanderschrei'n ist ganz und gar abscheulich.", "tokens": ["Ihr", "Durch\u00b7ein\u00b7an\u00b7der\u00b7schrei'n", "ist", "ganz", "und", "gar", "ab\u00b7scheu\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}