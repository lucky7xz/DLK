{"textgrid.poem.56665": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Der Herbst", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So komm, du wilder West,", "tokens": ["So", "komm", ",", "du", "wil\u00b7der", "West", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "und sing geheimnisvoll und runenkundig", "tokens": ["und", "sing", "ge\u00b7heim\u00b7nis\u00b7voll", "und", "ru\u00b7nen\u00b7kun\u00b7dig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "in meinen Kiefern und Wacholderb\u00fcschen", "tokens": ["in", "mei\u00b7nen", "Kie\u00b7fern", "und", "Wa\u00b7chol\u00b7der\u00b7b\u00fc\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "das uralt d\u00fcstere Jahreslied des Todes!", "tokens": ["das", "ur\u00b7alt", "d\u00fcs\u00b7te\u00b7re", "Jah\u00b7res\u00b7lied", "des", "To\u00b7des", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Und rei\u00df aus meinem Herz des Sommers Freuden,", "tokens": ["Und", "rei\u00df", "aus", "mei\u00b7nem", "Herz", "des", "Som\u00b7mers", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "rei\u00df sie gleich m\u00fcd gewordenen Bl\u00e4ttern ab,", "tokens": ["rei\u00df", "sie", "gleich", "m\u00fcd", "ge\u00b7wor\u00b7de\u00b7nen", "Bl\u00e4t\u00b7tern", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "auf da\u00df mein Fu\u00df sie raschelnd von sich sto\u00dfe.", "tokens": ["auf", "da\u00df", "mein", "Fu\u00df", "sie", "ra\u00b7schelnd", "von", "sich", "sto\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPOSAT", "NN", "PPER", "VVPP", "APPR", "PRF", "VVFIN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.8": {"text": "So wie von jenem Ahorn taumelnd dort", "tokens": ["So", "wie", "von", "je\u00b7nem", "A\u00b7horn", "tau\u00b7melnd", "dort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "APPR", "PDAT", "NN", "ADJD", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "die schwarzgefleckten Bl\u00e4tter landw\u00e4rts wirbeln,", "tokens": ["die", "schwarz\u00b7ge\u00b7fleck\u00b7ten", "Bl\u00e4t\u00b7ter", "land\u00b7w\u00e4rts", "wir\u00b7beln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "la\u00df all des Sommers gaukelnde Gestalten", "tokens": ["la\u00df", "all", "des", "Som\u00b7mers", "gau\u00b7keln\u00b7de", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PIAT", "ART", "NN", "ADJA", "NN"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.11": {"text": "zu krausen Scharen windgewiegt", "tokens": ["zu", "krau\u00b7sen", "Scha\u00b7ren", "wind\u00b7ge\u00b7wiegt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "ins graue Land Vergessenheit hinflattern!", "tokens": ["ins", "grau\u00b7e", "Land", "Ver\u00b7ges\u00b7sen\u00b7heit", "hin\u00b7flat\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und dann, oh West, oh wilder West,", "tokens": ["Und", "dann", ",", "oh", "West", ",", "oh", "wil\u00b7der", "West", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "FM", "FM", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "saug aus des Weltmeers weitgeebbten Br\u00fcsten", "tokens": ["saug", "aus", "des", "Welt\u00b7meers", "weit\u00b7geebb\u00b7ten", "Br\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.15": {"text": "dir Sturmeskr\u00e4fte hoch und schleudere mich", "tokens": ["dir", "Stur\u00b7mes\u00b7kr\u00e4f\u00b7te", "hoch", "und", "schleu\u00b7de\u00b7re", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "ADJD", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.16": {"text": "hohnlachend jenen Spukgestalten nach", "tokens": ["hohn\u00b7la\u00b7chend", "je\u00b7nen", "Spuk\u00b7ge\u00b7stal\u00b7ten", "nach"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "PDAT", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "und brause, laut aus vollen Lungen tobend,", "tokens": ["und", "brau\u00b7se", ",", "laut", "aus", "vol\u00b7len", "Lun\u00b7gen", "to\u00b7bend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADJD", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "\u00fcber das Sommergl\u00fcck, das du zerst\u00f6rt!", "tokens": ["\u00fc\u00b7ber", "das", "Som\u00b7mer\u00b7gl\u00fcck", ",", "das", "du", "zer\u00b7st\u00f6rt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}}}}