{"textgrid.poem.53672": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Der kleine Hund an der Ecke", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da stehst du wie ein kleines Gummischweinchen", "tokens": ["Da", "stehst", "du", "wie", "ein", "klei\u00b7nes", "Gum\u00b7mischwein\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "und gehst auf deinen dicken Batterbeinchen", "tokens": ["und", "gehst", "auf", "dei\u00b7nen", "di\u00b7cken", "Bat\u00b7ter\u00b7bein\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "recht zittrig um den gro\u00dfen Mann herum.", "tokens": ["recht", "zitt\u00b7rig", "um", "den", "gro\u00b7\u00dfen", "Mann", "he\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mama war Ziehhund. Doch du seist von Rasse,", "tokens": ["Ma\u00b7ma", "war", "Zieh\u00b7hund", ".", "Doch", "du", "seist", "von", "Ras\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$.", "KON", "PPER", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "sagt jener (er ist schlecht bei Kasse) \u2013", "tokens": ["sagt", "je\u00b7ner", "(", "er", "ist", "schlecht", "bei", "Kas\u00b7se", ")", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PDAT", "$(", "PPER", "VAFIN", "ADJD", "APPR", "NN", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "du bist so klein . . . man gab dir so viel Rum . . .", "tokens": ["du", "bist", "so", "klein", ".", ".", ".", "man", "gab", "dir", "so", "viel", "Rum", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$.", "$.", "$.", "PIS", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und manchmal nimmt er dich an sein Jackett.", "tokens": ["Und", "manch\u00b7mal", "nimmt", "er", "dich", "an", "sein", "Ja\u00b7ckett", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Nachts liegst du ganz bescheiden unterm Bett.", "tokens": ["Nachts", "liegst", "du", "ganz", "be\u00b7schei\u00b7den", "un\u00b7term", "Bett", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du kommst, wenns gut geht, zu der feinen Dame,", "tokens": ["Du", "kommst", ",", "wenns", "gut", "geht", ",", "zu", "der", "fei\u00b7nen", "Da\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADJD", "VVFIN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die so sch\u00f6n riecht; Frau Mimi ist ihr Name.", "tokens": ["die", "so", "sch\u00f6n", "riecht", ";", "Frau", "Mi\u00b7mi", "ist", "ihr", "Na\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVFIN", "$.", "NN", "NE", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Sie liebkost dich \u2013 besonders s\u00fc\u00df vor Herrn.", "tokens": ["Sie", "lieb\u00b7kost", "dich", "\u2013", "be\u00b7son\u00b7ders", "s\u00fc\u00df", "vor", "Herrn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "ADV", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und du h\u00f6rst alles, was sich da vollziehn wird,", "tokens": ["Und", "du", "h\u00f6rst", "al\u00b7les", ",", "was", "sich", "da", "voll\u00b7ziehn", "wird", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIS", "$,", "PRELS", "PRF", "ADV", "VVINF", "VAFIN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "und wie zum Hahn auf einmal ganz Berlin wird,", "tokens": ["und", "wie", "zum", "Hahn", "auf", "ein\u00b7mal", "ganz", "Ber\u00b7lin", "wird", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPRART", "NN", "APPR", "ADV", "ADV", "NE", "VAFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "denn ganz Berlin hat deine Herrin gern.", "tokens": ["denn", "ganz", "Ber\u00b7lin", "hat", "dei\u00b7ne", "Her\u00b7rin", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NE", "VAFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wie sie auch haucht, verl\u00f6schend, im Falsett . . .", "tokens": ["Wie", "sie", "auch", "haucht", ",", "ver\u00b7l\u00f6\u00b7schend", ",", "im", "Fal\u00b7sett", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$,", "VVPP", "$,", "APPRART", "NN", "$.", "$.", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Du liegst bescheiden unter ihrem Bett.", "tokens": ["Du", "liegst", "be\u00b7schei\u00b7den", "un\u00b7ter", "ih\u00b7rem", "Bett", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und du h\u00f6rst abends, wenn Madame zur Ruh geht,", "tokens": ["Und", "du", "h\u00f6rst", "a\u00b7bends", ",", "wenn", "Ma\u00b7da\u00b7me", "zur", "Ruh", "geht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,", "KOUS", "FM", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "von ihren Herrn, wie es im Leben zugeht \u2013:", "tokens": ["von", "ih\u00b7ren", "Herrn", ",", "wie", "es", "im", "Le\u00b7ben", "zu\u00b7geht", "\u2013", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PWAV", "PPER", "APPRART", "NN", "VVFIN", "$(", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "wie man dem Regisseur ein Ding gedreht.", "tokens": ["wie", "man", "dem", "Re\u00b7gis\u00b7seur", "ein", "Ding", "ge\u00b7dreht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du h\u00f6rst von d\u00fcstern Polizeimysterien,", "tokens": ["Du", "h\u00f6rst", "von", "d\u00fcs\u00b7tern", "Po\u00b7li\u00b7zei\u00b7mys\u00b7te\u00b7ri\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+----+-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "von allem Klatsch aus allen Ministerien,", "tokens": ["von", "al\u00b7lem", "Klatsch", "aus", "al\u00b7len", "Mi\u00b7nis\u00b7te\u00b7ri\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "und wer zu Haniel konferieren geht.", "tokens": ["und", "wer", "zu", "Ha\u00b7niel", "kon\u00b7fe\u00b7rie\u00b7ren", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da staunste, Kleener? Siehste, so fluscht det . . . !", "tokens": ["Da", "stauns\u00b7te", ",", "Klee\u00b7ner", "?", "Siehs\u00b7te", ",", "so", "fluscht", "det", ".", ".", ".", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["ADV", "VVFIN", "$,", "NN", "$.", "NN", "$,", "ADV", "VVFIN", "PDS", "$.", "$.", "$.", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und du liegst ganz bescheiden unterm Bett.", "tokens": ["Und", "du", "liegst", "ganz", "be\u00b7schei\u00b7den", "un\u00b7term", "Bett", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Merks dir genau! Spitz deine kleinen Ohren!", "tokens": ["Merks", "dir", "ge\u00b7nau", "!", "Spitz", "dei\u00b7ne", "klei\u00b7nen", "Oh\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJD", "$.", "NE", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Sonst geht dir noch der letzte Tip verloren \u2013", "tokens": ["Sonst", "geht", "dir", "noch", "der", "letz\u00b7te", "Tip", "ver\u00b7lo\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "hoch auf dem Nachttisch raschelt ein Papier . . .", "tokens": ["hoch", "auf", "dem", "Nacht\u00b7tisch", "ra\u00b7schelt", "ein", "Pa\u00b7pier", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$.", "$.", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Der Reichstag und die Presse? Liebes Kleinchen,", "tokens": ["Der", "Reichs\u00b7tag", "und", "die", "Pres\u00b7se", "?", "Lie\u00b7bes", "Klein\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$.", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "guck sie ver\u00e4chtlich an und heb ein Beinchen!", "tokens": ["guck", "sie", "ver\u00b7\u00e4cht\u00b7lich", "an", "und", "heb", "ein", "Be\u00b7in\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "PTKVZ", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Das wahre Leben hast du nur bei ihr.", "tokens": ["Das", "wah\u00b7re", "Le\u00b7ben", "hast", "du", "nur", "bei", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Fassaden sind nur Falle und nie wichtig.", "tokens": ["Fas\u00b7sa\u00b7den", "sind", "nur", "Fal\u00b7le", "und", "nie", "wich\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "NN", "KON", "ADV", "ADJD", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.8": {"text": "Mit einer klugen Frau spricht jeder richtig.", "tokens": ["Mit", "ei\u00b7ner", "klu\u00b7gen", "Frau", "spricht", "je\u00b7der", "rich\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Du h\u00f6rst im Dunkel leis die beiden Stimmen . . .", "tokens": ["Du", "h\u00f6rst", "im", "Dun\u00b7kel", "leis", "die", "bei\u00b7den", "Stim\u00b7men", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADJD", "ART", "PIAT", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Du witterst, da\u00df da Zigaretten glimmen . . .", "tokens": ["Du", "wit\u00b7terst", ",", "da\u00df", "da", "Zi\u00b7ga\u00b7ret\u00b7ten", "glim\u00b7men", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADV", "NN", "VVINF", "$.", "$.", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Da, da l\u00fcgt keiner. Da ist jeder ganz honett.", "tokens": ["Da", ",", "da", "l\u00fcgt", "kei\u00b7ner", ".", "Da", "ist", "je\u00b7der", "ganz", "ho\u00b7nett", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PIS", "$.", "ADV", "VAFIN", "PIS", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Das Ding an sich: du h\u00f6rst es.", "tokens": ["Das", "Ding", "an", "sich", ":", "du", "h\u00f6rst", "es", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Unterm Bett.", "tokens": ["Un\u00b7term", "Bett", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}