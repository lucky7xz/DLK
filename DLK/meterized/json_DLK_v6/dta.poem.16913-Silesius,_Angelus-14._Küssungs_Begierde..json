{"dta.poem.16913": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "14.  K\u00fcssungs Begierde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Ach la\u00df mich doch mein Kind mein Gott an deinen F\u00fcss\u00e9/", "tokens": ["Ach", "la\u00df", "mich", "doch", "mein", "Kind", "mein", "Gott", "an", "dei\u00b7nen", "F\u00fcss\u00e9", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVIMP", "PPER", "ADV", "PPOSAT", "NN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur einen Augenblik das minste Br\u00fcnklein k\u00fcssen.", "tokens": ["Nur", "ei\u00b7nen", "Au\u00b7gen\u00b7blik", "das", "mins\u00b7te", "Br\u00fcn\u00b7klein", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich wei\u00df werd\u2019 ich von Dir nur blo\u00df ber\u00fchret seyn/", "tokens": ["Ich", "wei\u00df", "werd'", "ich", "von", "Dir", "nur", "blo\u00df", "be\u00b7r\u00fch\u00b7ret", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VAFIN", "PPER", "APPR", "PPER", "ADV", "ADV", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df straks verschwinden wird/ mein\u2019/ und auch deine", "tokens": ["Da\u00df", "straks", "ver\u00b7schwin\u00b7den", "wird", "/", "mein'", "/", "und", "auch", "dei\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADV", "VVPP", "VAFIN", "$(", "PPOSAT", "$(", "KON", "ADV", "PPOSAT"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Pein.", "tokens": ["Pein", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}}}}}