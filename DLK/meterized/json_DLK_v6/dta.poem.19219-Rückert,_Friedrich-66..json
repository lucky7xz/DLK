{"dta.poem.19219": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "66.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Aberglaube sagt: Wirst du beim Wandern sp\u00fcren,", "tokens": ["Der", "A\u00b7berg\u00b7lau\u00b7be", "sagt", ":", "Wirst", "du", "beim", "Wan\u00b7dern", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "VAFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df dich ein irrer Geist bei Nacht will irre f\u00fchren;", "tokens": ["Da\u00df", "dich", "ein", "ir\u00b7rer", "Geist", "bei", "Nacht", "will", "ir\u00b7re", "f\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "NN", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So halte dich nur mit dem rechten Fu\u00df im Gleise", "tokens": ["So", "hal\u00b7te", "dich", "nur", "mit", "dem", "rech\u00b7ten", "Fu\u00df", "im", "Glei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Fahrwegs fein, und ungeirrt geht deine Reise.", "tokens": ["Des", "Fahr\u00b7wegs", "fein", ",", "und", "un\u00b7ge\u00b7irrt", "geht", "dei\u00b7ne", "Rei\u00b7se", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "KON", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Denn auf dem Gehweg nur, nicht auf dem Fahrweg kann", "tokens": ["Denn", "auf", "dem", "Geh\u00b7weg", "nur", ",", "nicht", "auf", "dem", "Fahr\u00b7weg", "kann"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "$,", "PTKNEG", "APPR", "ART", "NN", "VMFIN"], "meter": "-+-++-++-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Es Schaden thun, und hat Macht \u00fcbern Wandersmann.", "tokens": ["Es", "Scha\u00b7den", "thun", ",", "und", "hat", "Macht", "\u00fc\u00b7bern", "Wan\u00b7ders\u00b7mann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVINF", "$,", "KON", "VAFIN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "In Zweifelsf\u00e4llen ists und bei Bedenklichkeiten", "tokens": ["In", "Zwei\u00b7fels\u00b7f\u00e4l\u00b7len", "ists", "und", "bei", "Be\u00b7denk\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Heerweg besser als auf eignen Pfaden schreiten.", "tokens": ["Im", "Heer\u00b7weg", "bes\u00b7ser", "als", "auf", "eig\u00b7nen", "Pfa\u00b7den", "schrei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "KOKOM", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}