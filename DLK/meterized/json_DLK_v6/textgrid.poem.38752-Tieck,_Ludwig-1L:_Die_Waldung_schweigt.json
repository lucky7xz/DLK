{"textgrid.poem.38752": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Waldung schweigt", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Waldung schweigt", "tokens": ["Die", "Wal\u00b7dung", "schweigt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und Nebel schleichen,", "tokens": ["Und", "Ne\u00b7bel", "schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Die Sonne steigt,", "tokens": ["Die", "Son\u00b7ne", "steigt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Gl\u00e4nzt durch die Eichen:", "tokens": ["Gl\u00e4nzt", "durch", "die", "Ei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Um nasses Moos", "tokens": ["Um", "nas\u00b7ses", "Moos"], "token_info": ["word", "word", "word"], "pos": ["KOUI", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Erheben Funken,", "tokens": ["Er\u00b7he\u00b7ben", "Fun\u00b7ken", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Der Erde Schoos", "tokens": ["Der", "Er\u00b7de", "Schoos"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Bl\u00fcht auf und die V\u00f6gel sind trunken.", "tokens": ["Bl\u00fcht", "auf", "und", "die", "V\u00f6\u00b7gel", "sind", "trun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Die Lerche singt", "tokens": ["Die", "Ler\u00b7che", "singt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "In hohen L\u00fcften,", "tokens": ["In", "ho\u00b7hen", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Der Nachhall klingt", "tokens": ["Der", "Nach\u00b7hall", "klingt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "In allen Kl\u00fcften. \u2013", "tokens": ["In", "al\u00b7len", "Kl\u00fcf\u00b7ten", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIAT", "NN", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Durch Nebelzug", "tokens": ["Durch", "Ne\u00b7bel\u00b7zug"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Nun rastlos weiter", "tokens": ["Nun", "rast\u00b7los", "wei\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Im schnellen Flug!", "tokens": ["Im", "schnel\u00b7len", "Flug", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Schon gl\u00e4nzet die Sonne so heiter!", "tokens": ["Schon", "gl\u00e4n\u00b7zet", "die", "Son\u00b7ne", "so", "hei\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Die Sch\u00f6pfung regt", "tokens": ["Die", "Sch\u00f6p\u00b7fung", "regt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Die muntern Glieder.", "tokens": ["Die", "mun\u00b7tern", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Das Herz mir schl\u00e4gt, \u2013", "tokens": ["Das", "Herz", "mir", "schl\u00e4gt", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Ich seh' sie wieder!", "tokens": ["Ich", "seh'", "sie", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Durch niedre Luft", "tokens": ["Durch", "nied\u00b7re", "Luft"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Mit schwerem Segen", "tokens": ["Mit", "schwe\u00b7rem", "Se\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Zieht Nebelduft,", "tokens": ["Zieht", "Ne\u00b7bel\u00b7duft", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Ihm nicken die Saaten entgegen.", "tokens": ["Ihm", "ni\u00b7cken", "die", "Saa\u00b7ten", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Wo find ich sie?", "tokens": ["Wo", "find", "ich", "sie", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wo mag sie weilen?", "tokens": ["Wo", "mag", "sie", "wei\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Vergi\u00dft mich nie", "tokens": ["Ver\u00b7gi\u00dft", "mich", "nie"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und w\u00fcnscht mein Eilen?", "tokens": ["Und", "w\u00fcnscht", "mein", "Ei\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Ha, jeder Sinn,", "tokens": ["Ha", ",", "je\u00b7der", "Sinn", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIAT", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Vom Bilde trunken,", "tokens": ["Vom", "Bil\u00b7de", "trun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Strebt zu ihr hin,", "tokens": ["Strebt", "zu", "ihr", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Die Sch\u00f6pfung ist ringsum versunken.", "tokens": ["Die", "Sch\u00f6p\u00b7fung", "ist", "ring\u00b7sum", "ver\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Ihr holder Blick,", "tokens": ["Ihr", "hol\u00b7der", "Blick", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der auf mich sinket,", "tokens": ["Der", "auf", "mich", "sin\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ist schon mein Gl\u00fcck.", "tokens": ["Ist", "schon", "mein", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Wie freundlich winket", "tokens": ["Wie", "freund\u00b7lich", "win\u00b7ket"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Der zarte Mund", "tokens": ["Der", "zar\u00b7te", "Mund"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Mir Zauberk\u00fcssen!", "tokens": ["Mir", "Zau\u00b7ber\u00b7k\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "O holder Bund!", "tokens": ["O", "hol\u00b7der", "Bund", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "O Gl\u00fcck! mich so nahe zu wissen!", "tokens": ["O", "Gl\u00fcck", "!", "mich", "so", "na\u00b7he", "zu", "wis\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPER", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.6": {"line.1": {"text": "Auf, eilt euch, schnell!", "tokens": ["Auf", ",", "eilt", "euch", ",", "schnell", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "$,", "VVFIN", "PPER", "$,", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ihr Rosse munter!", "tokens": ["Ihr", "Ros\u00b7se", "mun\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Der Tag wird hell,", "tokens": ["Der", "Tag", "wird", "hell", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "So fliegt hinunter!", "tokens": ["So", "fliegt", "hin\u00b7un\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Da\u00df auch mein Herz", "tokens": ["Da\u00df", "auch", "mein", "Herz"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Den Segen finde,", "tokens": ["Den", "Se\u00b7gen", "fin\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Und jeder Schmerz", "tokens": ["Und", "je\u00b7der", "Schmerz"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Im himmlischen Taumel verschwinde!", "tokens": ["Im", "himm\u00b7li\u00b7schen", "Tau\u00b7mel", "ver\u00b7schwin\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}