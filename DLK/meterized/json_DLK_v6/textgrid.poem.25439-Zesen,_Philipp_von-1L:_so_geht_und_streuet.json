{"textgrid.poem.25439": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "1L: so geht und streuet", "genre": "verse", "period": "N.A.", "pub_year": 1654, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "so geht und streuet", "tokens": ["so", "geht", "und", "streu\u00b7et"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "der Rosen bl\u00fcht/", "tokens": ["der", "Ro\u00b7sen", "bl\u00fcht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "seid stets bem\u00fcht", "tokens": ["seid", "stets", "be\u00b7m\u00fcht"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADV", "VVFIN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "auf ihren preis/", "tokens": ["auf", "ih\u00b7ren", "preis", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "der niemahls weis", "tokens": ["der", "nie\u00b7mahls", "weis"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADV", "PTKVZ"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "sein end' und ziel:", "tokens": ["sein", "end'", "und", "ziel", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "r\u00fchmt oft und viel", "tokens": ["r\u00fchmt", "oft", "und", "viel"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "KON", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "ihr tugend-licht/", "tokens": ["ihr", "tu\u00b7gen\u00b7dlicht", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "das nie gebricht/", "tokens": ["das", "nie", "ge\u00b7bricht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "das nie verbleicht/", "tokens": ["das", "nie", "ver\u00b7bleicht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVPP", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "das nicht entweicht/", "tokens": ["das", "nicht", "ent\u00b7weicht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "das nicht vergeht/", "tokens": ["das", "nicht", "ver\u00b7geht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "so lang als steht", "tokens": ["so", "lang", "als", "steht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "ein rosen-garten", "tokens": ["ein", "ro\u00b7sen\u00b7gar\u00b7ten"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}}}}}