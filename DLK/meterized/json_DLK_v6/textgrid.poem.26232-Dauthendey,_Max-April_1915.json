{"textgrid.poem.26232": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "April 1915", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mutter Erde, deutsche Erde,", "tokens": ["Mut\u00b7ter", "Er\u00b7de", ",", "deut\u00b7sche", "Er\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gibst jetzt deinen W\u00e4ldern Kraft,", "tokens": ["Gibst", "jetzt", "dei\u00b7nen", "W\u00e4l\u00b7dern", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Machst, da\u00df es jetzt Fr\u00fchling werde.", "tokens": ["Machst", ",", "da\u00df", "es", "jetzt", "Fr\u00fch\u00b7ling", "wer\u00b7de", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "ADV", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von den Birken tropft der Saft,", "tokens": ["Von", "den", "Bir\u00b7ken", "tropft", "der", "Saft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alten Eichen springt die Rinde,", "tokens": ["Al\u00b7ten", "Ei\u00b7chen", "springt", "die", "Rin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und es blinkt der junge Trieb.", "tokens": ["Und", "es", "blinkt", "der", "jun\u00b7ge", "Trieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Unsere Feinde \u00fcberwinde,", "tokens": ["Un\u00b7se\u00b7re", "Fein\u00b7de", "\u00fc\u00b7berw\u00b7in\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Deutscher Faust gib Trotz zum Hieb!", "tokens": ["Deut\u00b7scher", "Faust", "gib", "Trotz", "zum", "Hieb", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVIMP", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}