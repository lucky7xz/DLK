{"textgrid.poem.40243": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Drei Ausrufungzeichen und \u00bbLieber Freund\u00ab", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drei Ausrufungzeichen und \u00bbLieber Freund\u00ab", "tokens": ["Drei", "Aus\u00b7ru\u00b7fung\u00b7zei\u00b7chen", "und", "\u00bb", "Lie\u00b7ber", "Freund", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "NN", "KON", "$(", "ADJD", "NN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "hat er das blos so hingemeint?", "tokens": ["hat", "er", "das", "blos", "so", "hin\u00b7ge\u00b7meint", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PDS", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er ist doch sonst nicht von der Sorte,", "tokens": ["Er", "ist", "doch", "sonst", "nicht", "von", "der", "Sor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "die Jedem gleich alle zehn Finger reichen;", "tokens": ["die", "Je\u00b7dem", "gleich", "al\u00b7le", "zehn", "Fin\u00b7ger", "rei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADV", "PIAT", "CARD", "NN", "VVINF", "$."], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "und ich wei\u00df, er w\u00e4gt seine schriftlichen Worte", "tokens": ["und", "ich", "wei\u00df", ",", "er", "w\u00e4gt", "sei\u00b7ne", "schrift\u00b7li\u00b7chen", "Wor\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "und verschleudert keine Ausrufungzeichen.", "tokens": ["und", "ver\u00b7schleu\u00b7dert", "kei\u00b7ne", "Aus\u00b7ru\u00b7fung\u00b7zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Hm; nimmt er mich also f\u00fcr seines Gleichen?", "tokens": ["Hm", ";", "nimmt", "er", "mich", "al\u00b7so", "f\u00fcr", "sei\u00b7nes", "Glei\u00b7chen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "PRF", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Er wei\u00df doch: in Dem, was ist und scheint,", "tokens": ["Er", "wei\u00df", "doch", ":", "in", "Dem", ",", "was", "ist", "und", "scheint", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "APPR", "PDS", "$,", "PWS", "VAFIN", "KON", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "sind unsre Gehirne sich ziemlich feind!", "tokens": ["sind", "uns\u00b7re", "Ge\u00b7hir\u00b7ne", "sich", "ziem\u00b7lich", "feind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PRF", "ADV", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Also wahrhaft gemeint? \u2013 Ja, lieber Freund? \u2013", "tokens": ["Al\u00b7so", "wahr\u00b7haft", "ge\u00b7meint", "?", "\u2013", "Ja", ",", "lie\u00b7ber", "Freund", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "VVPP", "$.", "$(", "PTKANT", "$,", "ADV", "NN", "$.", "$("], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Dann, hier, beide H\u00e4nde \u2013 ", "tokens": ["Dann", ",", "hier", ",", "bei\u00b7de", "H\u00e4n\u00b7de", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$,", "PIAT", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}