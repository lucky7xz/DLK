{"textgrid.poem.36174": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schoo\u00dfkind des Himmels, vielgew\u00fcnschter Friede,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schoo\u00dfkind des Himmels, vielgew\u00fcnschter Friede,", "tokens": ["Schoo\u00df\u00b7kind", "des", "Him\u00b7mels", ",", "viel\u00b7ge\u00b7w\u00fcnschter", "Frie\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Bist du auf ewig unserm Stern' entflohn?", "tokens": ["Bist", "du", "auf", "e\u00b7wig", "un\u00b7serm", "Stern'", "ent\u00b7flohn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJD", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ver\u00f6det ist die Welt, die Menschheit m\u00fcde;", "tokens": ["Ver\u00b7\u00f6\u00b7det", "ist", "die", "Welt", ",", "die", "Menschheit", "m\u00fc\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Rechts br\u00fcllt und links des Schlachtrufs Donnerton.", "tokens": ["Rechts", "br\u00fcllt", "und", "links", "des", "Schlach\u00b7trufs", "Don\u00b7ner\u00b7ton", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "ADV", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es wetterleuchten S\u00fcd und Norden,", "tokens": ["Es", "wet\u00b7ter\u00b7leuch\u00b7ten", "S\u00fcd", "und", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Himmel f\u00e4rbt sich flammenroth.", "tokens": ["Der", "Him\u00b7mel", "f\u00e4rbt", "sich", "flam\u00b7men\u00b7roth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nur Eine Kunst gilt: Menschen morden!", "tokens": ["Nur", "Ei\u00b7ne", "Kunst", "gilt", ":", "Men\u00b7schen", "mor\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$.", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nur Eine Zuflucht bleibt: Der Tod!", "tokens": ["Nur", "Ei\u00b7ne", "Zu\u00b7flucht", "bleibt", ":", "Der", "Tod", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}