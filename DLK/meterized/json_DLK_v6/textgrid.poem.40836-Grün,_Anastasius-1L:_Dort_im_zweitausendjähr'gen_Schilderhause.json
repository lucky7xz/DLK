{"textgrid.poem.40836": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dort im zweitausendj\u00e4hr'gen Schilderhause", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dort im zweitausendj\u00e4hr'gen Schilderhause", "tokens": ["Dort", "im", "zwei\u00b7tau\u00b7send\u00b7j\u00e4hr'\u00b7gen", "Schil\u00b7der\u00b7hau\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vor'm Thor Pompeji's lehnt ein morsch Gerippe;", "tokens": ["Vor'm", "Thor", "Pom\u00b7pe\u00b7ji's", "lehnt", "ein", "morsch", "Ge\u00b7rip\u00b7pe", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Speer h\u00e4lt noch die Knochenfaust! Welch grause,", "tokens": ["Den", "Speer", "h\u00e4lt", "noch", "die", "Kno\u00b7chen\u00b7faust", "!", "Welch", "grau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "$.", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mi\u00dflungne Posse auf des Todes Lippe!", "tokens": ["Mi\u00df\u00b7lung\u00b7ne", "Pos\u00b7se", "auf", "des", "To\u00b7des", "Lip\u00b7pe", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "In der Livrey bourbon'scher Lilien schreitet", "tokens": ["In", "der", "Liv\u00b7rey", "bour\u00b7bon'\u00b7scher", "Li\u00b7li\u00b7en", "schrei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dabei ein neuer W\u00e4chter auf und nieder;", "tokens": ["Da\u00b7bei", "ein", "neu\u00b7er", "W\u00e4ch\u00b7ter", "auf", "und", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des R\u00f6mers Sanduhr, den er abl\u00f6st, gleitet", "tokens": ["Des", "R\u00f6\u00b7mers", "San\u00b7duhr", ",", "den", "er", "ab\u00b7l\u00f6st", ",", "glei\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auch ihm und mi\u00dft des tr\u00e4gen Tages Glieder.", "tokens": ["Auch", "ihm", "und", "mi\u00dft", "des", "tr\u00e4\u00b7gen", "Ta\u00b7ges", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "KON", "VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und zu dem kn\u00f6chernen Kam'raden spricht er:", "tokens": ["Und", "zu", "dem", "kn\u00f6\u00b7cher\u00b7nen", "Kam'\u00b7ra\u00b7den", "spricht", "er", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "---+--+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "\u00bbob sie dich All' auch Bild der Treue nennen,", "tokens": ["\u00bb", "ob", "sie", "dich", "All'", "auch", "Bild", "der", "Treu\u00b7e", "nen\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "PRF", "PIS", "ADV", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Ich kann in dir, du Armer, den Berichter", "tokens": ["Ich", "kann", "in", "dir", ",", "du", "Ar\u00b7mer", ",", "den", "Be\u00b7rich\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "$,", "PPER", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von tausendj\u00e4hr'gem Narrenthum nur kennen!", "tokens": ["Von", "tau\u00b7send\u00b7j\u00e4hr'\u00b7gem", "Nar\u00b7ren\u00b7thum", "nur", "ken\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ei, meintest du die Vaterstadt zu schirmen?", "tokens": ["Ei", ",", "mein\u00b7test", "du", "die", "Va\u00b7ter\u00b7stadt", "zu", "schir\u00b7men", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Katapulte des Vesuvs zu hemmen?", "tokens": ["Die", "Ka\u00b7ta\u00b7pul\u00b7te", "des", "Ve\u00b7suvs", "zu", "hem\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Gluthgeschwader, die, den Wall zu st\u00fcrmen,", "tokens": ["Die", "Gluth\u00b7ge\u00b7schwa\u00b7der", ",", "die", ",", "den", "Wall", "zu", "st\u00fcr\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er niederbrausen lie\u00df, zur\u00fcck zu d\u00e4mmen?", "tokens": ["Er", "nie\u00b7der\u00b7brau\u00b7sen", "lie\u00df", ",", "zu\u00b7r\u00fcck", "zu", "d\u00e4m\u00b7men", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VVFIN", "$,", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Auch ich bin einst in Waffen schon gestanden,", "tokens": ["Auch", "ich", "bin", "einst", "in", "Waf\u00b7fen", "schon", "ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Freiheit Banner rauschte auf mich nieder!", "tokens": ["Der", "Frei\u00b7heit", "Ban\u00b7ner", "rauschte", "auf", "mich", "nie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Durch der Abruzzen gr\u00fcne Thale wanden", "tokens": ["Durch", "der", "Ab\u00b7ruz\u00b7zen", "gr\u00fc\u00b7ne", "Tha\u00b7le", "wan\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Wie wei\u00dfe Mauern sich der Deutschen Glieder.", "tokens": ["Wie", "wei\u00b7\u00dfe", "Mau\u00b7ern", "sich", "der", "Deut\u00b7schen", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Als Wall des Vaterlands den Kugeln allen", "tokens": ["Als", "Wall", "des", "Va\u00b7ter\u00b7lands", "den", "Ku\u00b7geln", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "ART", "NN", "ART", "NN", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wollt' ich die freie Brust entgegentragen,", "tokens": ["Wollt'", "ich", "die", "frei\u00b7e", "Brust", "ent\u00b7ge\u00b7gen\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ei, h\u00e4tte nur in nahen Waldeshallen", "tokens": ["Ei", ",", "h\u00e4t\u00b7te", "nur", "in", "na\u00b7hen", "Wal\u00b7des\u00b7hal\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VAFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht eine Nachtigall so sch\u00f6n geschlagen!", "tokens": ["Nicht", "ei\u00b7ne", "Nach\u00b7ti\u00b7gall", "so", "sch\u00f6n", "ge\u00b7schla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "In ihre Reihn, hoch in der Faust den Degen,", "tokens": ["In", "ih\u00b7re", "Reihn", ",", "hoch", "in", "der", "Faust", "den", "De\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJD", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "W\u00e4r' ich gest\u00fcrzt, von Todesmuth entgl\u00fchet,", "tokens": ["W\u00e4r'", "ich", "ge\u00b7st\u00fcrzt", ",", "von", "To\u00b7des\u00b7muth", "ent\u00b7gl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ei, h\u00e4tte nur hart neben meinen Wegen", "tokens": ["Ei", ",", "h\u00e4t\u00b7te", "nur", "hart", "ne\u00b7ben", "mei\u00b7nen", "We\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VAFIN", "ADV", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Nicht eine Rose gar so sch\u00f6n gebl\u00fchet!", "tokens": ["Nicht", "ei\u00b7ne", "Ro\u00b7se", "gar", "so", "sch\u00f6n", "ge\u00b7bl\u00fc\u00b7het", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Die Trommeln wirbeln, und die Fahnen wehen;", "tokens": ["Die", "Trom\u00b7meln", "wir\u00b7beln", ",", "und", "die", "Fah\u00b7nen", "we\u00b7hen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ja herrlich ist's, im Feld des Ruhms zu sinken!", "tokens": ["Ja", "herr\u00b7lich", "ist's", ",", "im", "Feld", "des", "Ruhms", "zu", "sin\u00b7ken", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "VAFIN", "$,", "APPRART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ei, h\u00e4tt' ich nur die Traube nicht gesehen", "tokens": ["Ei", ",", "h\u00e4tt'", "ich", "nur", "die", "Trau\u00b7be", "nicht", "ge\u00b7se\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VAFIN", "PPER", "ADV", "ART", "NN", "PTKNEG", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So sch\u00f6n und voll an gr\u00fcner Hecke winken!", "tokens": ["So", "sch\u00f6n", "und", "voll", "an", "gr\u00fc\u00b7ner", "He\u00b7cke", "win\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Das Leben ist das Sch\u00f6nste doch im Leben!", "tokens": ["Das", "Le\u00b7ben", "ist", "das", "Sch\u00f6ns\u00b7te", "doch", "im", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Drum rett' ich dir, Italia, das meine!", "tokens": ["Drum", "rett'", "ich", "dir", ",", "I\u00b7ta\u00b7lia", ",", "das", "mei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "$,", "NE", "$,", "PRELS", "PPOSAT", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und sieh, auch dankbar sind die lieben Reben,", "tokens": ["Und", "sieh", ",", "auch", "dank\u00b7bar", "sind", "die", "lie\u00b7ben", "Re\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "ADJD", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Nachtigallen und die Rosenhaine!\u00ab", "tokens": ["Die", "Nach\u00b7ti\u00b7gal\u00b7len", "und", "die", "Ro\u00b7sen\u00b7hai\u00b7ne", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Er sprach's, doch h\u00e4lt den Speer noch ohne Wanken", "tokens": ["Er", "sprach's", ",", "doch", "h\u00e4lt", "den", "Speer", "noch", "oh\u00b7ne", "Wan\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der tausendj\u00e4hr'ge W\u00e4chter ihm entgegen!", "tokens": ["Der", "tau\u00b7send\u00b7j\u00e4hr'\u00b7ge", "W\u00e4ch\u00b7ter", "ihm", "ent\u00b7ge\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So ein Geripp' mag eigene Gedanken", "tokens": ["So", "ein", "Ge\u00b7ripp'", "mag", "ei\u00b7ge\u00b7ne", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VMFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Reben, Rosen, Nachtigallen hegen.", "tokens": ["Von", "Re\u00b7ben", ",", "Ro\u00b7sen", ",", "Nach\u00b7ti\u00b7gal\u00b7len", "he\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}