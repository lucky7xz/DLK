{"textgrid.poem.49470": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "[du, Kind, glaubst an den Kaffeegrund]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, Kind, glaubst an den Kaffeegrund,", "tokens": ["Du", ",", "Kind", ",", "glaubst", "an", "den", "Kaf\u00b7fee\u00b7grund", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aufs Lottospiel verl\u00e4sst du dich:", "tokens": ["Aufs", "Lot\u00b7to\u00b7spiel", "ver\u00b7l\u00e4sst", "du", "dich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An deine Augen glaube ich.", "tokens": ["An", "dei\u00b7ne", "Au\u00b7gen", "glau\u00b7be", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "An Ungl\u00fcckstage, M\u00e4rchen und", "tokens": ["An", "Un\u00b7gl\u00fccks\u00b7ta\u00b7ge", ",", "M\u00e4r\u00b7chen", "und"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An Tr\u00e4ume glaubst du, die nicht tr\u00fcgen,", "tokens": ["An", "Tr\u00e4u\u00b7me", "glaubst", "du", ",", "die", "nicht", "tr\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$,", "PRELS", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich glaub allein an deine L\u00fcgen.", "tokens": ["Ich", "glaub", "al\u00b7lein", "an", "dei\u00b7ne", "L\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "An Gott glaubst du ganz wesenlos,", "tokens": ["An", "Gott", "glaubst", "du", "ganz", "we\u00b7sen\u00b7los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du weisst, dass man zu Heil'gen fleht,", "tokens": ["Du", "weisst", ",", "dass", "man", "zu", "Heil'\u00b7gen", "fleht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PIS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr jeden Kummer ein Gebet.", "tokens": ["F\u00fcr", "je\u00b7den", "Kum\u00b7mer", "ein", "Ge\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Ich glaube an die Stunden bloss,", "tokens": ["Ich", "glau\u00b7be", "an", "die", "Stun\u00b7den", "bloss", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die blau und rosig mir erbl\u00fchen", "tokens": ["Die", "blau", "und", "ro\u00b7sig", "mir", "er\u00b7bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "KON", "ADJD", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In unsrer blassen N\u00e4chte Gl\u00fchen.", "tokens": ["In", "uns\u00b7rer", "blas\u00b7sen", "N\u00e4ch\u00b7te", "Gl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und alles dieses glaube ich", "tokens": ["Und", "al\u00b7les", "die\u00b7ses", "glau\u00b7be", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "PDAT", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So fest und unersch\u00fctterlich,", "tokens": ["So", "fest", "und", "un\u00b7er\u00b7sch\u00fct\u00b7ter\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dass ich nur lebe noch f\u00fcr dich.", "tokens": ["Dass", "ich", "nur", "le\u00b7be", "noch", "f\u00fcr", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}