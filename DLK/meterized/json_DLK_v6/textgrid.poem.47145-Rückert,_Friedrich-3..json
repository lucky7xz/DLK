{"textgrid.poem.47145": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist alle Phantasie", "tokens": ["Was", "ist", "al\u00b7le", "Phan\u00b7ta\u00b7sie"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gegen Liebeswirklichkeit?", "tokens": ["Ge\u00b7gen", "Lie\u00b7bes\u00b7wirk\u00b7lich\u00b7keit", "?"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was sind alle Lieder, die", "tokens": ["Was", "sind", "al\u00b7le", "Lie\u00b7der", ",", "die"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "VAFIN", "PIAT", "NN", "$,", "PRELS"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich gesungen vor der Zeit?", "tokens": ["Ich", "ge\u00b7sun\u00b7gen", "vor", "der", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ein verlornes halbes Streben,", "tokens": ["Ein", "ver\u00b7lor\u00b7nes", "hal\u00b7bes", "Stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was nicht lebte, zu beleben;", "tokens": ["Was", "nicht", "leb\u00b7te", ",", "zu", "be\u00b7le\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Diese Lieder leben nur,", "tokens": ["Die\u00b7se", "Lie\u00b7der", "le\u00b7ben", "nur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Weil ich sie an mir erfuhr.", "tokens": ["Weil", "ich", "sie", "an", "mir", "er\u00b7fuhr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Nicht in ferne Himmelsr\u00e4ume", "tokens": ["Nicht", "in", "fer\u00b7ne", "Him\u00b7mels\u00b7r\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Braucht' ich dichtend auszufliegen,", "tokens": ["Braucht'", "ich", "dich\u00b7tend", "aus\u00b7zu\u00b7flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht in wesenlose Tr\u00e4ume", "tokens": ["Nicht", "in", "we\u00b7sen\u00b7lo\u00b7se", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eigensinnig mich zu wiegen.", "tokens": ["Ei\u00b7gen\u00b7sin\u00b7nig", "mich", "zu", "wie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Still daheim, in Liebe wach,", "tokens": ["Still", "da\u00b7heim", ",", "in", "Lie\u00b7be", "wach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$,", "APPR", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Unter meines Liebchens Dach", "tokens": ["Un\u00b7ter", "mei\u00b7nes", "Lieb\u00b7chens", "Dach"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schrieb ich unbem\u00fcht nur nach,", "tokens": ["Schrieb", "ich", "un\u00b7be\u00b7m\u00fcht", "nur", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Was mein Herz mit ihrem sprach.", "tokens": ["Was", "mein", "Herz", "mit", "ih\u00b7rem", "sprach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "APPR", "PPOSAT", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}