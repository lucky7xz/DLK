{"dta.poem.15756": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die K\u00fcrze lieb' ich sehr, der Rede B\u00fcndigkeit,", "tokens": ["Die", "K\u00fcr\u00b7ze", "lieb'", "ich", "sehr", ",", "der", "Re\u00b7de", "B\u00fcn\u00b7dig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wodurch ein Dichtermund zeigt seine M\u00fcndigkeit.", "tokens": ["Wo\u00b7durch", "ein", "Dich\u00b7ter\u00b7mund", "zeigt", "sei\u00b7ne", "M\u00fcn\u00b7dig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Vielwortigkeit ists die den Sch\u00fcler nur verklagt,", "tokens": ["Viel\u00b7wor\u00b7tig\u00b7keit", "ists", "die", "den", "Sch\u00fc\u00b7ler", "nur", "ver\u00b7klagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df er das eine Wort nicht traf, das Alles sagt.", "tokens": ["Da\u00df", "er", "das", "ei\u00b7ne", "Wort", "nicht", "traf", ",", "das", "Al\u00b7les", "sagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "ART", "NN", "PTKNEG", "VVFIN", "$,", "PRELS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch wo der ringende Gedank' ist \u00fcberschw\u00e4nglich,", "tokens": ["Doch", "wo", "der", "rin\u00b7gen\u00b7de", "Ge\u00b7dank'", "ist", "\u00fc\u00b7bersc\u00b7hw\u00e4ng\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist Widerholung auch dem Meister unumg\u00e4nglich.", "tokens": ["Ist", "Wi\u00b7der\u00b7ho\u00b7lung", "auch", "dem", "Meis\u00b7ter", "un\u00b7um\u00b7g\u00e4ng\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wo du das Thema nicht vermagst hervorzutonen,", "tokens": ["Wo", "du", "das", "The\u00b7ma", "nicht", "ver\u00b7magst", "her\u00b7vor\u00b7zu\u00b7to\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ersch\u00f6pfen mu\u00dft du es in Variazionen.", "tokens": ["Er\u00b7sch\u00f6p\u00b7fen", "mu\u00dft", "du", "es", "in", "Va\u00b7ria\u00b7zi\u00b7o\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "APPR", "NE", "$."], "meter": "-+-----+--+-", "measure": "iambic.tri.relaxed"}}}}}