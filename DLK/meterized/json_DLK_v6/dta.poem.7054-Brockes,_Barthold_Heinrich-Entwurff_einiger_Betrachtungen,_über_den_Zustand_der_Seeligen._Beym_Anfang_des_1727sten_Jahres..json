{"dta.poem.7054": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit welcher Simmetrie, die Geist und Seel entz\u00fcckt,", "tokens": ["Mit", "wel\u00b7cher", "Sim\u00b7me\u00b7trie", ",", "die", "Geist", "und", "Seel", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "$,", "ART", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mu\u00df diese Wohnung ausgeschm\u00fcckt", "tokens": ["Mu\u00df", "die\u00b7se", "Woh\u00b7nung", "aus\u00b7ge\u00b7schm\u00fcckt"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PDAT", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und aufgef\u00fchret seyn; Die, Dessen Allmachts-Hand,", "tokens": ["Und", "auf\u00b7ge\u00b7f\u00fch\u00b7ret", "seyn", ";", "Die", ",", "Des\u00b7sen", "All\u00b7machts\u00b7Hand", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAINF", "$.", "ART", "$,", "PRELAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Hiram angef\u00fcllt, Bezaleeln begl\u00fcckt", "tokens": ["Der", "Hi\u00b7ram", "an\u00b7ge\u00b7f\u00fcllt", ",", "Be\u00b7za\u00b7le\u00b7eln", "be\u00b7gl\u00fcckt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mit Weisheit und Verstand,", "tokens": ["Mit", "Weis\u00b7heit", "und", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Zu Seiner Ehren selbst geschm\u00fcckt und aufgef\u00fchret!", "tokens": ["Zu", "Sei\u00b7ner", "Eh\u00b7ren", "selbst", "ge\u00b7schm\u00fcckt", "und", "auf\u00b7ge\u00b7f\u00fch\u00b7ret", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Welch\u2019 eine Majest\u00e4t mu\u00df, sonder Maa\u00df und Grentzen,", "tokens": ["Welch'", "ei\u00b7ne", "Ma\u00b7jes\u00b7t\u00e4t", "mu\u00df", ",", "son\u00b7der", "Maa\u00df", "und", "Grent\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "VMFIN", "$,", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So \u00fcm, als in demselben Orte gl\u00e4ntzen,", "tokens": ["So", "\u00fcm", ",", "als", "in", "dem\u00b7sel\u00b7ben", "Or\u00b7te", "gl\u00e4nt\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "KOUS", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Den GOTT gebauet, GOTT gezieret,", "tokens": ["Den", "GoTT", "ge\u00b7bau\u00b7et", ",", "GoTT", "ge\u00b7zie\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Um Seine Majest\u00e4t, Huld, Allmacht, Herrlichkeit", "tokens": ["Um", "Sei\u00b7ne", "Ma\u00b7jes\u00b7t\u00e4t", ",", "Huld", ",", "All\u00b7macht", ",", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["KOUI", "PPOSAT", "NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "In Seiner Herrlichkeit zu zeigen!", "tokens": ["In", "Sei\u00b7ner", "Herr\u00b7lich\u00b7keit", "zu", "zei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Wie mu\u00df die Vollenkommenheit", "tokens": ["Wie", "mu\u00df", "die", "Vol\u00b7len\u00b7kom\u00b7men\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Derselben Anmuth, Zier und Pracht", "tokens": ["Der\u00b7sel\u00b7ben", "An\u00b7muth", ",", "Zier", "und", "Pracht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "All\u2019 andre Zierde, Pracht und Anmuth, \u00fcbersteigen,", "tokens": ["All'", "and\u00b7re", "Zier\u00b7de", ",", "Pracht", "und", "An\u00b7muth", ",", "\u00fc\u00b7bers\u00b7tei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "NN", "KON", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die Menschen-Witz, die Engel-Weisheit fassen,", "tokens": ["Die", "Men\u00b7schen\u00b7Witz", ",", "die", "En\u00b7gel\u00b7Weis\u00b7heit", "fas\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Und alle Creatur ergr\u00fcnden kan!", "tokens": ["Und", "al\u00b7le", "Crea\u00b7tur", "er\u00b7gr\u00fcn\u00b7den", "kan", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "O Himmel! welch ein Bau, den selbst die ew\u2019ge Macht,", "tokens": ["O", "Him\u00b7mel", "!", "welch", "ein", "Bau", ",", "den", "selbst", "die", "ew'\u00b7ge", "Macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWAT", "ART", "NN", "$,", "PRELS", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Von ew\u2019ger Weisheit unterst\u00fctzet,", "tokens": ["Von", "ew'\u00b7ger", "Weis\u00b7heit", "un\u00b7ter\u00b7st\u00fct\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Zu Seiner Ehre werden lassen!", "tokens": ["Zu", "Sei\u00b7ner", "Eh\u00b7re", "wer\u00b7den", "las\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Wie unaussprechlich licht, wie unbegreifflich helle", "tokens": ["Wie", "un\u00b7aus\u00b7sprech\u00b7lich", "licht", ",", "wie", "un\u00b7be\u00b7greif\u00b7flich", "hel\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "NN", "$,", "PWAV", "ADJD", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Mu\u00df jeder Punct, mu\u00df jede Stelle,", "tokens": ["Mu\u00df", "je\u00b7der", "Punct", ",", "mu\u00df", "je\u00b7de", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "$,", "VMFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Mu\u00df jeder Theil, wie sch\u00f6n das ", "tokens": ["Mu\u00df", "je\u00b7der", "Theil", ",", "wie", "sch\u00f6n", "das"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PIAT", "NN", "$,", "PWAV", "ADJD", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Welch Anmuth mu\u00df aus jedem Orte quillen!", "tokens": ["Welch", "An\u00b7muth", "mu\u00df", "aus", "je\u00b7dem", "Or\u00b7te", "quil\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Welch holder Freuden-Glantz mu\u00df alle Pl\u00e4tze f\u00fcllen!", "tokens": ["Welch", "hol\u00b7der", "Freu\u00b7den\u00b7Glantz", "mu\u00df", "al\u00b7le", "Pl\u00e4t\u00b7ze", "f\u00fcl\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VMFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Mu\u00df nicht ein seel\u2019ger Wieder-Schein,", "tokens": ["Mu\u00df", "nicht", "ein", "seel'\u00b7ger", "Wie\u00b7der\u00b7Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Da GOTT, der aller Sonnen Sonne,", "tokens": ["Da", "GoTT", ",", "der", "al\u00b7ler", "Son\u00b7nen", "Son\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "PRELS", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Unendlich an sie strahlt, von allen Orten prallen,", "tokens": ["Un\u00b7end\u00b7lich", "an", "sie", "strahlt", ",", "von", "al\u00b7len", "Or\u00b7ten", "pral\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VVFIN", "$,", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und allen Seeligen in ihre Seelen fallen,", "tokens": ["Und", "al\u00b7len", "See\u00b7li\u00b7gen", "in", "ih\u00b7re", "See\u00b7len", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Jhr Innerstes ergetzen und erquicken,", "tokens": ["Ihr", "In\u00b7ners\u00b7tes", "er\u00b7get\u00b7zen", "und", "er\u00b7qui\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Sie stets durchdringen und entz\u00fccken!", "tokens": ["Sie", "stets", "durch\u00b7drin\u00b7gen", "und", "ent\u00b7z\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}