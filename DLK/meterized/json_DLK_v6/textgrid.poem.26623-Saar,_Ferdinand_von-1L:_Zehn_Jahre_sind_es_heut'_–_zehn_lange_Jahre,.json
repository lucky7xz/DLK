{"textgrid.poem.26623": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zehn Jahre sind es heut' \u2013 zehn lange Jahre,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zehn Jahre sind es heut' \u2013 zehn lange Jahre,", "tokens": ["Zehn", "Jah\u00b7re", "sind", "es", "heut'", "\u2013", "zehn", "lan\u00b7ge", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "ADV", "$(", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da lagst du hingestreckt auf dunkler Bahre,", "tokens": ["Da", "lagst", "du", "hin\u00b7ge\u00b7streckt", "auf", "dunk\u00b7ler", "Bah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Antlitz fahl, das helle Aug' geschlossen,", "tokens": ["Das", "Ant\u00b7litz", "fahl", ",", "das", "hel\u00b7le", "Aug'", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "D'raus mir der Liebe reinster Strahl geflossen.", "tokens": ["D'raus", "mir", "der", "Lie\u00b7be", "reins\u00b7ter", "Strahl", "ge\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du wiesest nicht den sanften Glanz der Ruhe,", "tokens": ["Du", "wie\u00b7sest", "nicht", "den", "sanf\u00b7ten", "Glanz", "der", "Ru\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Todte oft verkl\u00e4rt in ihrer Truhe;", "tokens": ["Der", "Tod\u00b7te", "oft", "ver\u00b7kl\u00e4rt", "in", "ih\u00b7rer", "Tru\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein herber Ernst versch\u00e4rfte deine Z\u00fcge", "tokens": ["Ein", "her\u00b7ber", "Ernst", "ver\u00b7sch\u00e4rf\u00b7te", "dei\u00b7ne", "Z\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und deiner Lippen marmor'nes Gef\u00fcge.", "tokens": ["Und", "dei\u00b7ner", "Lip\u00b7pen", "mar\u00b7mor'\u00b7nes", "Ge\u00b7f\u00fc\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Ernst des Schmerzes war's, den du getragen", "tokens": ["Der", "Ernst", "des", "Schmer\u00b7zes", "wa\u00b7r's", ",", "den", "du", "ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "$,", "PRELS", "PPER", "VVPP"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In deiner Seele bis zu alten Tagen \u2013", "tokens": ["In", "dei\u00b7ner", "See\u00b7le", "bis", "zu", "al\u00b7ten", "Ta\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Duldens Ernst, den du, mich nicht zu schrecken,", "tokens": ["Des", "Dul\u00b7dens", "Ernst", ",", "den", "du", ",", "mich", "nicht", "zu", "schre\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "PRELS", "PPER", "$,", "PPER", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Stets in ein L\u00e4cheln wu\u00dftest zu verstecken.", "tokens": ["Stets", "in", "ein", "L\u00e4\u00b7cheln", "wu\u00df\u00b7test", "zu", "ver\u00b7ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mir aber blieb nicht fremd, was du gelitten,", "tokens": ["Mir", "a\u00b7ber", "blieb", "nicht", "fremd", ",", "was", "du", "ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PTKNEG", "ADJD", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich sah es klar, wie schweigend du gestritten \u2013", "tokens": ["Ich", "sah", "es", "klar", ",", "wie", "schwei\u00b7gend", "du", "ge\u00b7strit\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "PWAV", "ADJD", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mu\u00dft' ich doch selbst in deinen Lebenszeiten", "tokens": ["Mu\u00dft'", "ich", "doch", "selbst", "in", "dei\u00b7nen", "Le\u00b7bens\u00b7zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dir stets das allertiefste Weh' bereiten.", "tokens": ["Dir", "stets", "das", "al\u00b7ler\u00b7tiefs\u00b7te", "Weh'", "be\u00b7rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Hingehen sahst du mich auf steilen Bahnen;", "tokens": ["Hin\u00b7ge\u00b7hen", "sahst", "du", "mich", "auf", "stei\u00b7len", "Bah\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Wohin sie f\u00fchrten, konntest du wohl ahnen,", "tokens": ["Wo\u00b7hin", "sie", "f\u00fchr\u00b7ten", ",", "konn\u00b7test", "du", "wohl", "ah\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es blieb dein Geist dem meinen nicht verschlossen \u2013", "tokens": ["Es", "blieb", "dein", "Geist", "dem", "mei\u00b7nen", "nicht", "ver\u00b7schlos\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "VVFIN", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie w\u00e4r' ich sonst auch deinem Schoo\u00df entsprossen?", "tokens": ["Wie", "w\u00e4r'", "ich", "sonst", "auch", "dei\u00b7nem", "Schoo\u00df", "ent\u00b7spros\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Doch wu\u00dfte diese Welt an bangen Zweifeln", "tokens": ["Doch", "wu\u00df\u00b7te", "die\u00b7se", "Welt", "an", "ban\u00b7gen", "Zwei\u00b7feln"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PDAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gar viele dir in Herz und Sinn zu tr\u00e4ufeln;", "tokens": ["Gar", "vie\u00b7le", "dir", "in", "Herz", "und", "Sinn", "zu", "tr\u00e4u\u00b7feln", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "PPER", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie wu\u00dfte deinen Stolz dir zu verbittern", "tokens": ["Sie", "wu\u00df\u00b7te", "dei\u00b7nen", "Stolz", "dir", "zu", "ver\u00b7bit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und lie\u00df dich f\u00fcr das Heil des Sohnes zittern.", "tokens": ["Und", "lie\u00df", "dich", "f\u00fcr", "das", "Heil", "des", "Soh\u00b7nes", "zit\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wer wei\u00df es nicht, wie schlaue Tr\u00f6pfe malen", "tokens": ["Wer", "wei\u00df", "es", "nicht", ",", "wie", "schlau\u00b7e", "Tr\u00f6p\u00b7fe", "ma\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Nichtigkeit von allen Idealen \u2013", "tokens": ["Die", "Nich\u00b7tig\u00b7keit", "von", "al\u00b7len", "I\u00b7dea\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie hoch dem Neid stets ", "tokens": ["Wie", "hoch", "dem", "Neid", "stets"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Um die ", "tokens": ["Um", "die"], "token_info": ["word", "word"], "pos": ["KOUI", "ART"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.8": {"line.1": {"text": "Wer kennt sie nicht, des falschen Mitleids W\u00e4rme,", "tokens": ["Wer", "kennt", "sie", "nicht", ",", "des", "fal\u00b7schen", "Mit\u00b7leids", "W\u00e4r\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKNEG", "$,", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df der Getroffene sich tiefer h\u00e4rme \u2013", "tokens": ["Da\u00df", "der", "Ge\u00b7trof\u00b7fe\u00b7ne", "sich", "tie\u00b7fer", "h\u00e4r\u00b7me", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "ADJD", "VVFIN", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Nicht jenen frechen Muth, der unerschrocken", "tokens": ["Nicht", "je\u00b7nen", "fre\u00b7chen", "Muth", ",", "der", "un\u00b7er\u00b7schro\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "PDAT", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem Guten Schlechtes vorzieht mit Frohlocken.", "tokens": ["Dem", "Gu\u00b7ten", "Schlech\u00b7tes", "vor\u00b7zieht", "mit", "Froh\u00b7lo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+++-", "measure": "unknown.measure.hexa"}}, "stanza.9": {"line.1": {"text": "Wer kennt es nicht, dies stumme Achselzucken,", "tokens": ["Wer", "kennt", "es", "nicht", ",", "dies", "stum\u00b7me", "Ach\u00b7sel\u00b7zu\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKNEG", "$,", "PDS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dies niedertr\u00e4chtig harte Lobverschlucken,", "tokens": ["Dies", "nie\u00b7der\u00b7tr\u00e4ch\u00b7tig", "har\u00b7te", "Lob\u00b7ver\u00b7schlu\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dies Schweigen, wenn man nicht mehr kann verneinen,", "tokens": ["Dies", "Schwei\u00b7gen", ",", "wenn", "man", "nicht", "mehr", "kann", "ver\u00b7nei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "$,", "KOUS", "PIS", "PTKNEG", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf's neue stets bereit zum Wurf mit Steinen.", "tokens": ["Auf's", "neu\u00b7e", "stets", "be\u00b7reit", "zum", "Wurf", "mit", "Stei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADV", "ADJD", "APPRART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Dies Alles mu\u00dftest du gleich mir ertragen,", "tokens": ["Dies", "Al\u00b7les", "mu\u00df\u00b7test", "du", "gleich", "mir", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VMFIN", "PPER", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und oft im Inn'ren wolltest du verzagen;", "tokens": ["Und", "oft", "im", "Inn'\u00b7ren", "woll\u00b7test", "du", "ver\u00b7za\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn leicht nicht konnt' ich meine Kraft entfalten,", "tokens": ["Denn", "leicht", "nicht", "konnt'", "ich", "mei\u00b7ne", "Kraft", "ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKNEG", "VMFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und fast schon schien's: sie w\u00fcrden Recht behalten.", "tokens": ["Und", "fast", "schon", "schien's", ":", "sie", "w\u00fcr\u00b7den", "Recht", "be\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJA", "$.", "PPER", "VAFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Und das auch war's, was dir in letzter Stunde", "tokens": ["Und", "das", "auch", "wa\u00b7r's", ",", "was", "dir", "in", "letz\u00b7ter", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "ADV", "VAFIN", "$,", "PWS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Als scheuer Seufzer klang aus bleichem Munde \u2013", "tokens": ["Als", "scheu\u00b7er", "Seuf\u00b7zer", "klang", "aus", "blei\u00b7chem", "Mun\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das war es, was versch\u00e4rfte deine Z\u00fcge", "tokens": ["Das", "war", "es", ",", "was", "ver\u00b7sch\u00e4rf\u00b7te", "dei\u00b7ne", "Z\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "$,", "PWS", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und deiner Lippen marmor'nes Gef\u00fcge.", "tokens": ["Und", "dei\u00b7ner", "Lip\u00b7pen", "mar\u00b7mor'\u00b7nes", "Ge\u00b7f\u00fc\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.12": {"line.1": {"text": "Das war es, was mit schmerzlichem Erkennen", "tokens": ["Das", "war", "es", ",", "was", "mit", "schmerz\u00b7li\u00b7chem", "Er\u00b7ken\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich selber f\u00fchlte in der Brust mir brennen,", "tokens": ["Ich", "sel\u00b7ber", "f\u00fchl\u00b7te", "in", "der", "Brust", "mir", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {