{"textgrid.poem.52796": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Auf den 80sten Geburtstag des Herrn Altb\u00fcrgermeisters Martin", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlagt den Zapfen aus dem Fasse,", "tokens": ["Schlagt", "den", "Zap\u00b7fen", "aus", "dem", "Fas\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das der Vierunddrei\u00df'ger f\u00fcllt", "tokens": ["Das", "der", "Vierund\u00b7drei\u00df'\u00b7ger", "f\u00fcllt"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ART", "NN", "VVFIN"], "meter": "--++-+", "measure": "anapaest.init"}, "line.3": {"text": "Mit dem Nasse zu dem Spasse,", "tokens": ["Mit", "dem", "Nas\u00b7se", "zu", "dem", "Spas\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welcher unserm Martin gilt!", "tokens": ["Wel\u00b7cher", "un\u00b7serm", "Mar\u00b7tin", "gilt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Achtzig Jahr ist doch ein Alter,", "tokens": ["Acht\u00b7zig", "Jahr", "ist", "doch", "ein", "Al\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und ich glaub' er tanzt noch gar,", "tokens": ["Und", "ich", "glaub'", "er", "tanzt", "noch", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie der Herr Renteiverwalter,", "tokens": ["Wie", "der", "Herr", "Ren\u00b7tei\u00b7ver\u00b7wal\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der hoch in den Siebzig war!", "tokens": ["Der", "hoch", "in", "den", "Sieb\u00b7zig", "war", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NE", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Lustig Freunde und Bekannte,", "tokens": ["Lus\u00b7tig", "Freun\u00b7de", "und", "Be\u00b7kann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lustig liebe Schw\u00e4gerin,", "tokens": ["Lus\u00b7tig", "lie\u00b7be", "Schw\u00e4\u00b7ge\u00b7rin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lustig, lustig, Anverwandte,", "tokens": ["Lus\u00b7tig", ",", "lus\u00b7tig", ",", "An\u00b7ver\u00b7wand\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lustig ist nach meinem Sinn!", "tokens": ["Lus\u00b7tig", "ist", "nach", "mei\u00b7nem", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Leider, da\u00df beim heut'gen Feste", "tokens": ["Lei\u00b7der", ",", "da\u00df", "beim", "heut'\u00b7gen", "Fes\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seine Kinder sind zerstreut,", "tokens": ["Sei\u00b7ne", "Kin\u00b7der", "sind", "zer\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sich in der Zahl der G\u00e4ste", "tokens": ["Und", "sich", "in", "der", "Zahl", "der", "G\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur ein Einzig's mit uns freut.", "tokens": ["Nur", "ein", "Ein\u00b7zig's", "mit", "uns", "freut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach! die vielen Andern wohnen,", "tokens": ["Ach", "!", "die", "vie\u00b7len", "An\u00b7dern", "woh\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "PIAT", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In Paris, in Buffalo,", "tokens": ["In", "Pa\u00b7ris", ",", "in", "Buf\u00b7fa\u00b7lo", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "APPR", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Stuttgart, Sasbach \u2013 solchen Zonen,", "tokens": ["Stutt\u00b7gart", ",", "Sas\u00b7bach", "\u2013", "sol\u00b7chen", "Zo\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$(", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die wo ganz wo-anderswo!", "tokens": ["Die", "wo", "ganz", "wo\u00b7an\u00b7ders\u00b7wo", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PWAV", "ADV", "PWAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Lustig, Freunde und Bekannte,", "tokens": ["Lus\u00b7tig", ",", "Freun\u00b7de", "und", "Be\u00b7kann\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lustig, liebe Schw\u00e4gerin,", "tokens": ["Lus\u00b7tig", ",", "lie\u00b7be", "Schw\u00e4\u00b7ge\u00b7rin", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lustig, lustig, Anverwandte,", "tokens": ["Lus\u00b7tig", ",", "lus\u00b7tig", ",", "An\u00b7ver\u00b7wand\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lustig ist nach meinem Sinn!", "tokens": ["Lus\u00b7tig", "ist", "nach", "mei\u00b7nem", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ei, die Frau Altb\u00fcrgermeistrin", "tokens": ["Ei", ",", "die", "Frau", "Alt\u00b7b\u00fcr\u00b7ger\u00b7meis\u00b7trin"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ART", "NN", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Schenkt uns Einen, welcher packt,", "tokens": ["Schenkt", "uns", "Ei\u00b7nen", ",", "wel\u00b7cher", "packt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da ist Feuer, da ist Geist d'rin \u2013", "tokens": ["Da", "ist", "Feu\u00b7er", ",", "da", "ist", "Geist", "d'\u00b7rin", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$,", "ADV", "VAFIN", "NN", "NE", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und was sie f\u00fcr Kuchen backt!", "tokens": ["Und", "was", "sie", "f\u00fcr", "Ku\u00b7chen", "backt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00f6nnt sie doch den Kindern schicken", "tokens": ["K\u00f6nnt", "sie", "doch", "den", "Kin\u00b7dern", "schi\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Von der Bretzel auch ein St\u00fcck", "tokens": ["Von", "der", "Bret\u00b7zel", "auch", "ein", "St\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hinter Vater Martins R\u00fccken", "tokens": ["Hin\u00b7ter", "Va\u00b7ter", "Mar\u00b7tins", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "W\u00e4r' es n\u00f6thig nicht, zum Gl\u00fcck!", "tokens": ["W\u00e4r'", "es", "n\u00f6\u00b7thig", "nicht", ",", "zum", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "PTKNEG", "$,", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Lustig, Freunde und Bekannte,", "tokens": ["Lus\u00b7tig", ",", "Freun\u00b7de", "und", "Be\u00b7kann\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lustig, liebe Schw\u00e4gerin,", "tokens": ["Lus\u00b7tig", ",", "lie\u00b7be", "Schw\u00e4\u00b7ge\u00b7rin", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lustig, lustig, Anverwandte,", "tokens": ["Lus\u00b7tig", ",", "lus\u00b7tig", ",", "An\u00b7ver\u00b7wand\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lustig ist nach meinem Sinn!", "tokens": ["Lus\u00b7tig", "ist", "nach", "mei\u00b7nem", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Alle sind sie gut gerathen,", "tokens": ["Al\u00b7le", "sind", "sie", "gut", "ge\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie's von selber sich versteht,", "tokens": ["Wie's", "von", "sel\u00b7ber", "sich", "ver\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADV", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo mit lobenswerthen Thaten", "tokens": ["Wo", "mit", "lo\u00b7bens\u00b7wert\u00b7hen", "Tha\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stets voran der Vater geht,", "tokens": ["Stets", "vo\u00b7ran", "der", "Va\u00b7ter", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dem die Mutter geht zur Seite,", "tokens": ["Dem", "die", "Mut\u00b7ter", "geht", "zur", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dem die Freunde geh'n zur Hand,", "tokens": ["Dem", "die", "Freun\u00b7de", "geh'n", "zur", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der mit sich zu Rath geht heute,", "tokens": ["Der", "mit", "sich", "zu", "Rath", "geht", "heu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PRF", "APPR", "NN", "VVFIN", "ADV", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Und der morgen Rathsvorstand.", "tokens": ["Und", "der", "mor\u00b7gen", "Raths\u00b7vor\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.8": {"line.1": {"text": "Lustig, Freunde und Bekannte,", "tokens": ["Lus\u00b7tig", ",", "Freun\u00b7de", "und", "Be\u00b7kann\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lustig, liebe Schw\u00e4gerin,", "tokens": ["Lus\u00b7tig", ",", "lie\u00b7be", "Schw\u00e4\u00b7ge\u00b7rin", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lustig, lustig, Anverwandte,", "tokens": ["Lus\u00b7tig", ",", "lus\u00b7tig", ",", "An\u00b7ver\u00b7wand\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lustig ist nach meinem Sinn!", "tokens": ["Lus\u00b7tig", "ist", "nach", "mei\u00b7nem", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Aller Waisenkinder Vater,", "tokens": ["Al\u00b7ler", "Wai\u00b7sen\u00b7kin\u00b7der", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und den Wittwen zugethan,", "tokens": ["Und", "den", "Witt\u00b7wen", "zu\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nahm er sich sogar noch spater", "tokens": ["Nahm", "er", "sich", "so\u00b7gar", "noch", "spa\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Des entlass'nen Str\u00e4flings an.", "tokens": ["Des", "ent\u00b7lass'\u00b7nen", "Str\u00e4f\u00b7lings", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Philipp, der sein Knecht geworden,", "tokens": ["Phi\u00b7lipp", ",", "der", "sein", "Knecht", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPOSAT", "NN", "VAPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ist der br\u00e4vste Mensch nun fast,", "tokens": ["Ist", "der", "br\u00e4vs\u00b7te", "Mensch", "nun", "fast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Welcher sonst in S\u00fcd und Norden", "tokens": ["Wel\u00b7cher", "sonst", "in", "S\u00fcd", "und", "Nor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAT", "ADV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der Gemeinde fiel zur Last.", "tokens": ["Der", "Ge\u00b7mein\u00b7de", "fiel", "zur", "Last", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Lustig, Freunde und Bekannte,", "tokens": ["Lus\u00b7tig", ",", "Freun\u00b7de", "und", "Be\u00b7kann\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lustig, liebe Schw\u00e4gerin,", "tokens": ["Lus\u00b7tig", ",", "lie\u00b7be", "Schw\u00e4\u00b7ge\u00b7rin", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lustig, lustig, Anverwandte,", "tokens": ["Lus\u00b7tig", ",", "lus\u00