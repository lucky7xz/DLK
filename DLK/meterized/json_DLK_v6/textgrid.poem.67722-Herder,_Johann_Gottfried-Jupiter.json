{"textgrid.poem.67722": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Jupiter", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liegt bergunter! G\u00f6tter! wie der Hyder Geifer", "tokens": ["Liegt", "ber\u00b7gun\u00b7ter", "!", "G\u00f6t\u00b7ter", "!", "wie", "der", "Hy\u00b7der", "Gei\u00b7fer"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$.", "NN", "$.", "PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Durchbricht! Aeol, den Du kaum beugst!", "tokens": ["Durch\u00b7bricht", "!", "A\u00b7e\u00b7ol", ",", "den", "Du", "kaum", "beugst", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NE", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Seht, wie die H\u00f6lle seines Bauchs felsichte Fluthenfeuer", "tokens": ["Seht", ",", "wie", "die", "H\u00f6l\u00b7le", "sei\u00b7nes", "Bauchs", "fel\u00b7sich\u00b7te", "Flut\u00b7hen\u00b7feu\u00b7er"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Ausheult! (Jupiter sch\u00fcttelt.) Ach! nun \u00e4chzt er entathmend,", "tokens": ["Aus\u00b7heult", "!", "(", "Ju\u00b7pi\u00b7ter", "sch\u00fct\u00b7telt", ".", ")", "Ach", "!", "nun", "\u00e4chzt", "er", "en\u00b7tath\u00b7mend", ","], "token_info": ["word", "punct", "punct", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "NN", "VVFIN", "$.", "$(", "ITJ", "$.", "ADV", "VVFIN", "PPER", "VVPP", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.5": {"text": "Kr\u00fcmmt sich. \u2013 Bebendes Land! Dir donnr' Aetna's Rauch,", "tokens": ["Kr\u00fcmmt", "sich", ".", "\u2013", "Be\u00b7ben\u00b7des", "Land", "!", "Dir", "donn\u00b7r'", "A\u00b7et\u00b7na's", "Rauch", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$.", "$(", "NN", "NN", "$.", "PPER", "ADV", "NE", "NN", "$,"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Eh Du von Jupiter's rachsiegendem Blitz brennst!", "tokens": ["Eh", "Du", "von", "Ju\u00b7pi\u00b7ter's", "rach\u00b7sie\u00b7gen\u00b7dem", "Blitz", "brennst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NE", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}}}}}