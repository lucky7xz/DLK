{"dta.poem.7413": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Das freie Wort.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.85", "no:0.13"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sie sollen Alle singen", "tokens": ["Sie", "sol\u00b7len", "Al\u00b7le", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIS", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nach ihres Herzens Lust;", "tokens": ["Nach", "ih\u00b7res", "Her\u00b7zens", "Lust", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Ein Lied nur aus der Brust:", "tokens": ["Ein", "Lied", "nur", "aus", "der", "Brust", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Lied, um Dich zu preisen,", "tokens": ["Ein", "Lied", ",", "um", "Dich", "zu", "prei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Du Nibelungenhort,", "tokens": ["Du", "Ni\u00b7be\u00b7lun\u00b7gen\u00b7hort", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Du Brot und Stein der Weisen,", "tokens": ["Du", "Brot", "und", "Stein", "der", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Du freies Wort!", "tokens": ["Du", "frei\u00b7es", "Wort", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Habt Ihr es nicht gelesen:", "tokens": ["Habt", "Ihr", "es", "nicht", "ge\u00b7le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Wort war ", "tokens": ["Das", "Wort", "war"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VAFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Im Anfang ist's gewesen,", "tokens": ["Im", "An\u00b7fang", "ist's", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "VAPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und soll drum ewig sein.", "tokens": ["Und", "soll", "drum", "e\u00b7wig", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PAV", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und eh' Ihr Einen Schl\u00e4ger", "tokens": ["Und", "eh'", "Ihr", "Ei\u00b7nen", "Schl\u00e4\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Erhebt zum V\u00f6lkermord,", "tokens": ["Er\u00b7hebt", "zum", "V\u00f6l\u00b7ker\u00b7mord", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sucht unsern Pannertr\u00e4ger,", "tokens": ["Sucht", "un\u00b7sern", "Pan\u00b7ner\u00b7tr\u00e4\u00b7ger", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das freie Wort!", "tokens": ["Das", "frei\u00b7e", "Wort", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ihr habet zugeschworen", "tokens": ["Ihr", "ha\u00b7bet", "zu\u00b7ge\u00b7schwo\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So treu dem Vaterland,", "tokens": ["So", "treu", "dem", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch seid Ihr All' verloren", "tokens": ["Doch", "seid", "Ihr", "All'", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PIS", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und haltet nimmer Stand,", "tokens": ["Und", "hal\u00b7tet", "nim\u00b7mer", "Stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So lang in West und Osten,", "tokens": ["So", "lang", "in", "West", "und", "Os\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So lang in S\u00fcd und Nord", "tokens": ["So", "lang", "in", "S\u00fcd", "und", "Nord"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das beste Schwert mu\u00df rosten,", "tokens": ["Das", "bes\u00b7te", "Schwert", "mu\u00df", "ros\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das freie Wort!", "tokens": ["Das", "frei\u00b7e", "Wort", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Ach! es will finster werden,", "tokens": ["Ach", "!", "es", "will", "fins\u00b7ter", "wer\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl finster \u00fcberall,", "tokens": ["Wohl", "fins\u00b7ter", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch ist die Nacht auf Erden", "tokens": ["Doch", "ist", "die", "Nacht", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ja f\u00fcr die Nachtigall.", "tokens": ["Ja", "f\u00fcr", "die", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Heraus denn aus der Wolke,", "tokens": ["He\u00b7raus", "denn", "aus", "der", "Wol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Die, S\u00e4nger, Euch umflort;", "tokens": ["Die", ",", "S\u00e4n\u00b7ger", ",", "Euch", "um\u00b7flort", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "$,", "NN", "$,", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Erst predigt Eurem Volke", "tokens": ["Erst", "pre\u00b7digt", "Eu\u00b7rem", "Vol\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das freie Wort!", "tokens": ["Das", "frei\u00b7e", "Wort", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "La\u00dft Eure Adler fliegen,", "tokens": ["La\u00dft", "Eu\u00b7re", "Ad\u00b7ler", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr F\u00fcrsten, in die Welt,", "tokens": ["Ihr", "F\u00fcrs\u00b7ten", ",", "in", "die", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sie nicht m\u00fcssig liegen", "tokens": ["Und", "sie", "nicht", "m\u00fcs\u00b7sig", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "PTKNEG", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf Eurem Wappenfeld!", "tokens": ["Auf", "Eu\u00b7rem", "Wap\u00b7pen\u00b7feld", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O jagt einmal die Raben", "tokens": ["O", "jagt", "ein\u00b7mal", "die", "Ra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Aus unsern Landen fort,", "tokens": ["Aus", "un\u00b7sern", "Lan\u00b7den", "fort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und sprecht: Ihr sollt es haben,", "tokens": ["Und", "sprecht", ":", "Ihr", "sollt", "es", "ha\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VMFIN", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das freie Wort!", "tokens": ["Das", "frei\u00b7e", "Wort", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}