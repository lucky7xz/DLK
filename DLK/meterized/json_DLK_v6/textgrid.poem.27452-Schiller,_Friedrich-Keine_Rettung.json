{"textgrid.poem.27452": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Keine Rettung", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lobt ihn, er schmiert ein Buch, euch zu loben; verfolgt ihn, er schmiert eins,", "tokens": ["Lobt", "ihn", ",", "er", "schmiert", "ein", "Buch", ",", "euch", "zu", "lo\u00b7ben", ";", "ver\u00b7folgt", "ihn", ",", "er", "schmiert", "eins", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VVFIN", "ART", "NN", "$,", "PPER", "PTKZU", "VVINF", "$.", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PIS", "$,"], "meter": "-+-+-+--+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Euch zu schelten; er schmiert, was ihr auch treibet, ein Buch.", "tokens": ["Euch", "zu", "schel\u00b7ten", ";", "er", "schmiert", ",", "was", "ihr", "auch", "trei\u00b7bet", ",", "ein", "Buch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$.", "PPER", "VVFIN", "$,", "PWS", "PPER", "ADV", "VVFIN", "$,", "ART", "NN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Nahe warst du dem Edeln und bliebst doch der Alberne? N\u00e4her", "tokens": ["Na\u00b7he", "warst", "du", "dem", "E\u00b7deln", "und", "bliebst", "doch", "der", "Al\u00b7ber\u00b7ne", "?", "N\u00e4\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "VAFIN", "PPER", "ART", "NN", "KON", "VVFIN", "ADV", "ART", "NN", "$.", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "War ihm der Stuhl, wo er sa\u00df, aber er blieb nur ein Stuhl.", "tokens": ["War", "ihm", "der", "Stuhl", ",", "wo", "er", "sa\u00df", ",", "a\u00b7ber", "er", "blieb", "nur", "ein", "Stuhl", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+--++-+-+-+", "measure": "iambic.septa.relaxed"}}}}}