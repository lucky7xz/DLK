{"dta.poem.11158": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrifft eines cantors.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da ich am besten sang, so macht der tod den schlu\u00df,", "tokens": ["Da", "ich", "am", "bes\u00b7ten", "sang", ",", "so", "macht", "der", "tod", "den", "schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKA", "ADJD", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und giebt den tact darein, da\u00df ich pausiren mu\u00df.", "tokens": ["Und", "giebt", "den", "tact", "da\u00b7rein", ",", "da\u00df", "ich", "pau\u00b7si\u00b7ren", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PAV", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Piano klingt es nun an diesem stillen orte:", "tokens": ["Pi\u00b7a\u00b7no", "klingt", "es", "nun", "an", "die\u00b7sem", "stil\u00b7len", "or\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich starb als wie ein schall von dem vergangnen worte.", "tokens": ["Ich", "starb", "als", "wie", "ein", "schall", "von", "dem", "ver\u00b7gang\u00b7nen", "wor\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["