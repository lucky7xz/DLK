{"dta.poem.2556": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcckwunsch auf die W. und G. Hochzeit  \n den 17. Febr. 1670.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Apollo kam zu mir und wolte mich besprechen/", "tokens": ["A\u00b7pol\u00b7lo", "kam", "zu", "mir", "und", "wol\u00b7te", "mich", "be\u00b7spre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPER", "KON", "VMFIN", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er hatte sein Gesicht in schwartzen Zorn verh\u00fcllt:", "tokens": ["Er", "hat\u00b7te", "sein", "Ge\u00b7sicht", "in", "schwart\u00b7zen", "Zorn", "ver\u00b7h\u00fcllt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er schwur; Ich und mein Chor mu\u00df diesen Frevel r\u00e4chen/", "tokens": ["Er", "schwur", ";", "Ich", "und", "mein", "Chor", "mu\u00df", "die\u00b7sen", "Fre\u00b7vel", "r\u00e4\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "KON", "PPOSAT", "NN", "VMFIN", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil keine Pflicht bey dir der alten Freundschafft gilt:", "tokens": ["Weil", "kei\u00b7ne", "Pflicht", "bey", "dir", "der", "al\u00b7ten", "Freund\u00b7schafft", "gilt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "APPR", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Versagst du dem ein Lied/ der an der Pallas Br\u00fcsten", "tokens": ["Ver\u00b7sagst", "du", "dem", "ein", "Lied", "/", "der", "an", "der", "Pal\u00b7las", "Br\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ART", "NN", "$(", "ART", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Kost der Wissenschafft genossen als wie du?", "tokens": ["Die", "Kost", "der", "Wis\u00b7sen\u00b7schafft", "ge\u00b7nos\u00b7sen", "als", "wie", "du", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "KON", "PWAV", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wirst du bey diesem Fest den Pegasus nicht r\u00fcsten/", "tokens": ["Wirst", "du", "bey", "die\u00b7sem", "Fest", "den", "Pe\u00b7ga\u00b7sus", "nicht", "r\u00fcs\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PDAT", "NN", "ART", "NE", "PTKNEG", "VVFIN", "$("], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.8": {"text": "Da\u00df er der Schuldigkeit ein voll Gen\u00fcgen thu?", "tokens": ["Da\u00df", "er", "der", "Schul\u00b7dig\u00b7keit", "ein", "voll", "Ge\u00b7n\u00fc\u00b7gen", "thu", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich sprach: Gekr\u00f6nter F\u00fcrst/ f\u00fcr dem der Pindus zittert/", "tokens": ["Ich", "sprach", ":", "Ge\u00b7kr\u00f6n\u00b7ter", "F\u00fcrst", "/", "f\u00fcr", "dem", "der", "Pin\u00b7dus", "zit\u00b7tert", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADJA", "NN", "$(", "APPR", "PRELS", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und den der Musen Schaar als ihren K\u00f6nig ehrt/", "tokens": ["Und", "den", "der", "Mu\u00b7sen", "Schaar", "als", "ih\u00b7ren", "K\u00f6\u00b7nig", "ehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "NN", "KOKOM", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Seynd eure Majest\u00e4t auff mich so hoch erbittert?", "tokens": ["Seynd", "eu\u00b7re", "Ma\u00b7jes\u00b7t\u00e4t", "auff", "mich", "so", "hoch", "er\u00b7bit\u00b7tert", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ach ihr fu\u00dff\u00e4llig Knecht werd auff ein Wort geh\u00f6rt!", "tokens": ["Ach", "ihr", "fu\u00df\u00b7f\u00e4l\u00b7lig", "Knecht", "werd", "auff", "ein", "Wort", "ge\u00b7h\u00f6rt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "ADJD", "NN", "VAFIN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Den Freunden hab ich nie der Freundschaffts-Pflicht entzogen/", "tokens": ["Den", "Freun\u00b7den", "hab", "ich", "nie", "der", "Freund\u00b7schaffts\u00b7Pflicht", "ent\u00b7zo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und der Vertrauten Fest ist mir nicht unbekand/", "tokens": ["Und", "der", "Ver\u00b7trau\u00b7ten", "Fest", "ist", "mir", "nicht", "un\u00b7be\u00b7kand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "PPER", "PTKNEG", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Es w\u00e4re l\u00e4ngst mein Geist in Helicon geflogen/", "tokens": ["Es", "w\u00e4\u00b7re", "l\u00e4ngst", "mein", "Geist", "in", "He\u00b7li\u00b7con", "ge\u00b7flo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "APPR", "NE", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und h\u00e4tte zu dem Quell Pirenens sich gewand.", "tokens": ["Und", "h\u00e4t\u00b7te", "zu", "dem", "Quell", "Pi\u00b7re\u00b7nens", "sich", "ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ART", "NN", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Allein was bringt man f\u00fcr? von Lieb und Lieben fingen/", "tokens": ["Al\u00b7lein", "was", "bringt", "man", "f\u00fcr", "?", "von", "Lieb", "und", "Lie\u00b7ben", "fin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "PIS", "APPR", "$.", "APPR", "NN", "KON", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Von Schertzen/ Freud und Lust/ das ist ein altes Spiel/", "tokens": ["Von", "Schert\u00b7zen", "/", "Freud", "und", "Lust", "/", "das", "ist", "ein", "al\u00b7tes", "Spiel", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "NN", "KON", "NN", "$(", "PDS", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und \u00fcber Hals und Kopff die Reim auff W\u00fcnsche zwingen", "tokens": ["Und", "\u00fc\u00b7ber", "Hals", "und", "Kopff", "die", "Reim", "auff", "W\u00fcn\u00b7sche", "zwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN", "ART", "NN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ist ein gemeines Werck/ und gilt numehr nicht viel:", "tokens": ["Ist", "ein", "ge\u00b7mei\u00b7nes", "Werck", "/", "und", "gilt", "nu\u00b7mehr", "nicht", "viel", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$(", "KON", "VVFIN", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Des Frauen-Zimmers Pracht und Sch\u00f6nheit zu beschreiben", "tokens": ["Des", "Frau\u00b7en\u00b7Zim\u00b7mers", "Pracht", "und", "Sch\u00f6n\u00b7heit", "zu", "be\u00b7schrei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Mu\u00df ein Narcissus seyn der selbst voll Flammen brennt/", "tokens": ["Mu\u00df", "ein", "Nar\u00b7cis\u00b7sus", "seyn", "der", "selbst", "voll", "Flam\u00b7men", "brennt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPOSAT", "ART", "ADV", "ADJD", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Denn/ wem der Liebreitz nicht die Geister pflegt zu treiben/", "tokens": ["Denn", "/", "wem", "der", "Lieb\u00b7reitz", "nicht", "die", "Geis\u00b7ter", "pflegt", "zu", "trei\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "PWS", "ART", "NN", "PTKNEG", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Macht ungereimtes Ding/ das keine Zierrath kennt.", "tokens": ["Macht", "un\u00b7ge\u00b7reim\u00b7tes", "Ding", "/", "das", "kei\u00b7ne", "Zier\u00b7rath", "kennt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$(", "ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und du/ o Delius/ kanst selber nicht wol leiden/", "tokens": ["Und", "du", "/", "o", "De\u00b7lius", "/", "kanst", "sel\u00b7ber", "nicht", "wol", "lei\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "FM", "NE", "$(", "VMFIN", "ADV", "PTKNEG", "ADV", "VVINF", "$("], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.26": {"text": "Wenn man ver\u00e4chtlich Zeug in deinen Tempel bringt:", "tokens": ["Wenn", "man", "ver\u00b7\u00e4cht\u00b7lich", "Zeug", "in", "dei\u00b7nen", "Tem\u00b7pel", "bringt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "We\u00df aber soll ich mich auff solchen Fall bescheiden/", "tokens": ["We\u00df", "a\u00b7ber", "soll", "ich", "mich", "auff", "sol\u00b7chen", "Fall", "be\u00b7schei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VMFIN", "PPER", "PRF", "APPR", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wo nehm ich ein solch Lied/ das dir und allen klingt?", "tokens": ["Wo", "nehm", "ich", "ein", "solch", "Lied", "/", "das", "dir", "und", "al\u00b7len", "klingt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "PIAT", "NN", "$(", "PRELS", "PPER", "KON", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ich mag nicht den Gebrauch der V\u00f6lcker hier erzehlen/", "tokens": ["Ich", "mag", "nicht", "den", "Ge\u00b7brauch", "der", "V\u00f6l\u00b7cker", "hier", "er\u00b7zeh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ART", "NN", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und wie das Morgenland sein Hochzeit-Fest gemacht/", "tokens": ["Und", "wie", "das", "Mor\u00b7gen\u00b7land", "sein", "Hoch\u00b7zeit\u00b7Fest", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Den Deutschen wil ich nur zum Beyspiel hier erwehlen/", "tokens": ["Den", "Deut\u00b7schen", "wil", "ich", "nur", "zum", "Bey\u00b7spiel", "hier", "er\u00b7weh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "APPRART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Der einen Kopff der Braut gab vor die erste Nacht.", "tokens": ["Der", "ei\u00b7nen", "Kopff", "der", "Braut", "gab", "vor", "die", "ers\u00b7te", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Er muste sich zuvor wol f\u00fcr dem Feinde halten/", "tokens": ["Er", "mus\u00b7te", "sich", "zu\u00b7vor", "wol", "f\u00fcr", "dem", "Fein\u00b7de", "hal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Eh als der Hymen ihn zu Bette hat gef\u00fchrt:", "tokens": ["Eh", "als", "der", "Hy\u00b7men", "ihn", "zu", "Bet\u00b7te", "hat", "ge\u00b7f\u00fchrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "PPER", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.35": {"text": "Ob zwar nun die Gesetz auch mit der Zeit veralten/", "tokens": ["Ob", "zwar", "nun", "die", "Ge\u00b7setz", "auch", "mit", "der", "Zeit", "ver\u00b7al\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und di\u00df was gestern galt uns heute nicht geb\u00fchrt/", "tokens": ["Und", "di\u00df", "was", "ge\u00b7stern", "galt", "uns", "heu\u00b7te", "nicht", "ge\u00b7b\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "PRELS", "ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "VVPP", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.37": {"text": "Nichts destoweniger w\u00fcrd aus der Venus Munde", "tokens": ["Nichts", "de\u00b7sto\u00b7we\u00b7ni\u00b7ger", "w\u00fcrd", "aus", "der", "Ve\u00b7nus", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "PAV", "VAFIN", "APPR", "ART", "NN", "NN"], "meter": "+-+---+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.38": {"text": "Kein ander Urtheil gehn/ als: Welcher sich verfreyt", "tokens": ["Kein", "an\u00b7der", "Ur\u00b7theil", "gehn", "/", "als", ":", "Wel\u00b7cher", "sich", "ver\u00b7freyt"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVINF", "$(", "KOUS", "$.", "PWAT", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ist schuldig/ da\u00df er auch zum festen Liebes-Bunde", "tokens": ["Ist", "schul\u00b7dig", "/", "da\u00df", "er", "auch", "zum", "fes\u00b7ten", "Lie\u00b7bes\u00b7Bun\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "$(", "KOUS", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Mit einem Kopff die Braut beschenckt die Lebens-Zeit.", "tokens": ["Mit", "ei\u00b7nem", "Kopff", "die", "Braut", "be\u00b7schenckt", "die", "Le\u00b7bens\u00b7Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-