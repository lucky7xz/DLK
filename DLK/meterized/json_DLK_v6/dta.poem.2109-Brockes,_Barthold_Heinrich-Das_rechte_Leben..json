{"dta.poem.2109": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das rechte Leben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich leb' und f\u00fchle da\u00df ich lebe. Indem mich Gottes", "tokens": ["Ich", "leb'", "und", "f\u00fch\u00b7le", "da\u00df", "ich", "le\u00b7be", ".", "In\u00b7dem", "mich", "Got\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KOUS", "PPER", "VVFIN", "$.", "KOUS", "PPER", "NN"], "meter": "-+-+-+++-+--+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Werke r\u00fchren,", "tokens": ["Wer\u00b7ke", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Und meine Seel\u2019, in dem Genu\u00df, geschickt sich selbst zu", "tokens": ["Und", "mei\u00b7ne", "Seel'", ",", "in", "dem", "Ge\u00b7nu\u00df", ",", "ge\u00b7schickt", "sich", "selbst", "zu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "APPR", "ART", "NN", "$,", "VVFIN", "PRF", "ADV", "APPR"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "\u00fcberf\u00fchren,", "tokens": ["\u00fc\u00b7berf\u00b7\u00fch\u00b7ren", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Da\u00df Gott, so sie als mich gemacht,", "tokens": ["Da\u00df", "Gott", ",", "so", "sie", "als", "mich", "ge\u00b7macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "ADV", "PPER", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auch, da ich f\u00fcr sie sinnlich bin, da\u00df sie f\u00fcr mich her-", "tokens": ["Auch", ",", "da", "ich", "f\u00fcr", "sie", "sinn\u00b7lich", "bin", ",", "da\u00df", "sie", "f\u00fcr", "mich", "her"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "APPR", "PPER", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "APPR", "PPER", "TRUNC"], "meter": "-----+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "vorgebracht;", "tokens": ["vor\u00b7ge\u00b7bracht", ";"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Kann ich in der erwognen Lust, zugleich, sie, mich, und", "tokens": ["Kann", "ich", "in", "der", "er\u00b7wog\u00b7nen", "Lust", ",", "zu\u00b7gleich", ",", "sie", ",", "mich", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["VMFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,", "ADV", "$,", "PPER", "$,", "PPER", "$,", "KON"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Gott versp\u00fcren.", "tokens": ["Gott", "ver\u00b7sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Die\u00df \u00fcberlegen, die\u00df empfinden und die\u00df bewundern,", "tokens": ["Die\u00df", "\u00fc\u00b7berl\u00b7e\u00b7gen", ",", "die\u00df", "emp\u00b7fin\u00b7den", "und", "die\u00df", "be\u00b7wun\u00b7dern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "$,", "PDS", "VVINF", "KON", "PDS", "VVINF", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "die\u00df ist eben,", "tokens": ["die\u00df", "ist", "e\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "Und zwar allein, ein wahres Leben.", "tokens": ["Und", "zwar", "al\u00b7lein", ",", "ein", "wah\u00b7res", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}