{"textgrid.poem.67636": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "58.", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als einst der Hahn noch Ph\u00f6busstimme sang,", "tokens": ["Als", "einst", "der", "Hahn", "noch", "Ph\u00f6\u00b7buss\u00b7tim\u00b7me", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "H\u00f6rt, wie es ihm durch seiner Lieder Klang", "tokens": ["H\u00f6rt", ",", "wie", "es", "ihm", "durch", "sei\u00b7ner", "Lie\u00b7der", "Klang"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "PPER", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gelang!", "tokens": ["Ge\u00b7lang", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "In gro\u00dfer G\u00e4nseschaar", "tokens": ["In", "gro\u00b7\u00dfer", "G\u00e4n\u00b7se\u00b7schaar"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Schwamm mit der Schwan,", "tokens": ["Schwamm", "mit", "der", "Schwan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Und da es dunkel war", "tokens": ["Und", "da", "es", "dun\u00b7kel", "war"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "(und man", "tokens": ["(", "und", "man"], "token_info": ["punct", "word", "word"], "pos": ["$(", "KON", "PIS"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Im Dunkel denn nicht sehen kann),", "tokens": ["Im", "Dun\u00b7kel", "denn", "nicht", "se\u00b7hen", "kann", ")", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ADV", "PTKNEG", "VVINF", "VMFIN", "$(", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Vergriff sich schon sein Pflegersmann,", "tokens": ["Ver\u00b7griff", "sich", "schon", "sein", "Pfle\u00b7gers\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und, lieber Schwan, Dir droht", "tokens": ["Und", ",", "lie\u00b7ber", "Schwan", ",", "Dir", "droht"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "ADV", "NN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Unedler Tod.", "tokens": ["U\u00b7ned\u00b7ler", "Tod", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Der ahnende Prophet begann", "tokens": ["Der", "ah\u00b7nen\u00b7de", "Pro\u00b7phet", "be\u00b7gann"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Sein Sterbelied.", "tokens": ["Sein", "Ster\u00b7be\u00b7lied", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Es zieht, es zieht", "tokens": ["Es", "zieht", ",", "es", "zieht"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Zwar nicht", "tokens": ["Zwar", "nicht"], "token_info": ["word", "word"], "pos": ["ADV", "PTKNEG"], "meter": "-+", "measure": "iambic.single"}, "line.16": {"text": "Den Mond vom Himmel, aber zieht", "tokens": ["Den", "Mond", "vom", "Him\u00b7mel", ",", "a\u00b7ber", "zieht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Den G\u00e4rtner aus der Nacht.", "tokens": ["Den", "G\u00e4rt\u00b7ner", "aus", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Er sieht", "tokens": ["Er", "sieht"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.19": {"text": "Mit klarem Angesicht:", "tokens": ["Mit", "kla\u00b7rem", "An\u00b7ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "\u00bbnein, edler Schwan (und ward des Irrthums roth),", "tokens": ["\u00bb", "nein", ",", "ed\u00b7ler", "Schwan", "(", "und", "ward", "des", "Irr\u00b7thums", "roth", ")", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKANT", "$,", "ADJA", "NN", "$(", "KON", "VAFIN", "ART", "NN", "ADJD", "$(", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Du stirbst nicht", "tokens": ["Du", "stirbst", "nicht"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG"], "meter": "+-+", "measure": "trochaic.di"}, "line.22": {"text": "G\u00e4nsetod.\u00ab", "tokens": ["G\u00e4n\u00b7se\u00b7tod", ".", "\u00ab"], "token_info": ["word", "punct", "punct"], "pos": ["NN", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}}}}}