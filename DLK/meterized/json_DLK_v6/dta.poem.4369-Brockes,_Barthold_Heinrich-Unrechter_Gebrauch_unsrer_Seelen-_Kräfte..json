{"dta.poem.4369": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Unrechter Gebrauch unsrer Seelen-  \n Kr\u00e4fte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ob wir gleich, was vom Bilde Gottes im Menschen", "tokens": ["Ob", "wir", "gleich", ",", "was", "vom", "Bil\u00b7de", "Got\u00b7tes", "im", "Men\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "$,", "PRELS", "APPRART", "NN", "NN", "APPRART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Moses dorten schrieb,", "tokens": ["Mo\u00b7ses", "dor\u00b7ten", "schrieb", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Nicht nach dem Wort-Verstande nehmen; so stecket, nach dem", "tokens": ["Nicht", "nach", "dem", "Wor\u00b7tVer\u00b7stan\u00b7de", "neh\u00b7men", ";", "so", "ste\u00b7cket", ",", "nach", "dem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "APPR", "ART", "NN", "VVINF", "$.", "ADV", "VVFIN", "$,", "APPR", "ART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "wahren Sinn,", "tokens": ["wah\u00b7ren", "Sinn", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "In der Geschichte von der Sch\u00f6pfung, erwegt mans, doch", "tokens": ["In", "der", "Ge\u00b7schich\u00b7te", "von", "der", "Sch\u00f6p\u00b7fung", ",", "er\u00b7wegt", "mans", ",", "doch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,", "VVFIN", "PIS", "$,", "ADV"], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}, "line.6": {"text": "so viel darinn,", "tokens": ["so", "viel", "da\u00b7rinn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PAV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Da\u00df alles erst erschaffen worden. Da\u00df hierauf GOttes", "tokens": ["Da\u00df", "al\u00b7les", "erst", "er\u00b7schaf\u00b7fen", "wor\u00b7den", ".", "Da\u00df", "hier\u00b7auf", "Got\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "VVPP", "VAPP", "$.", "KOUS", "PAV", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Lieb' Jhn trieb,", "tokens": ["Lieb'", "Jhn", "trieb", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Auch ein vern\u00fcnftiges Gesch\u00f6pf, zu diesem Zweck, hervor-", "tokens": ["Auch", "ein", "ver\u00b7n\u00fcnf\u00b7ti\u00b7ges", "Ge\u00b7sch\u00f6pf", ",", "zu", "die\u00b7sem", "Zweck", ",", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "APPR", "PDAT", "NN", "$,", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "zubringen,", "tokens": ["zu\u00b7brin\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Die Creaturen zu beherrschen, so wie es lautet. Aber", "tokens": ["Die", "Crea\u00b7tu\u00b7ren", "zu", "be\u00b7herr\u00b7schen", ",", "so", "wie", "es", "lau\u00b7tet", ".", "A\u00b7ber"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "ADV", "KOKOM", "PPER", "VVFIN", "$.", "KON"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "nicht", "tokens": ["nicht"], "token_info": ["word"], "pos": ["PTKNEG"], "meter": "-", "measure": "single.down"}, "line.13": {"text": "Die Gottheit Selber zu erkennen, und, mit erschaffenem", "tokens": ["Die", "Got\u00b7theit", "Sel\u00b7ber", "zu", "er\u00b7ken\u00b7nen", ",", "und", ",", "mit", "er\u00b7schaf\u00b7fe\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$,", "KON", "$,", "APPR", "ADJA"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Gesicht,", "tokens": ["Ge\u00b7sicht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.15": {"text": "Ins undurchdringlich- unerme\u00dflich- ins ewig unerschaffne", "tokens": ["Ins", "un\u00b7durch\u00b7dring\u00b7lich", "un\u00b7er\u00b7me\u00df\u00b7lich", "ins", "e\u00b7wig", "un\u00b7er\u00b7schaff\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "TRUNC", "TRUNC", "APPRART", "ADJD", "ADJA"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "Licht,", "tokens": ["Licht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.17": {"text": "Mit denen ihm geschenkten Kr\u00e4ften, als die umschr\u00e4nket,", "tokens": ["Mit", "de\u00b7nen", "ihm", "ge\u00b7schenk\u00b7ten", "Kr\u00e4f\u00b7ten", ",", "als", "die", "um\u00b7schr\u00e4n\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJA", "NN", "$,", "KOUS", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "einzudringen.", "tokens": ["ein\u00b7zu\u00b7drin\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.19": {"text": "Da Adams Geist doch, vor dem Fall, wie wir es in der Bibel", "tokens": ["Da", "A\u00b7dams", "Geist", "doch", ",", "vor", "dem", "Fall", ",", "wie", "wir", "es", "in", "der", "Bi\u00b7bel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "NN", "ADV", "$,", "APPR", "ART", "NN", "$,", "PWAV", "PPER", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.20": {"text": "lesen,", "tokens": ["le\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.21": {"text": "Noch wohl von einer andern St\u00e4rke, als unser Geist itzt ist,", "tokens": ["Noch", "wohl", "von", "ei\u00b7ner", "an\u00b7dern", "St\u00e4r\u00b7ke", ",", "als", "un\u00b7ser", "Geist", "itzt", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$,", "KOUS", "PPOSAT", "NN", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.22": {"text": "gewesen.", "tokens": ["ge\u00b7we\u00b7sen", "."], "token_info": ["word", "punct"], "pos": ["VAPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}