{"dta.poem.18917": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "An Herren Martin Opitzen  \n F\u00fcrtrefflichen Teutschen Poeten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Jn dem mein Ohr/ hand/ mund schier m\u00fcd/ die", "tokens": ["In", "dem", "mein", "Ohr", "/", "hand", "/", "mund", "schier", "m\u00fcd", "/", "die"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "PPOSAT", "NN", "$(", "NN", "$(", "NN", "ADJD", "ADJD", "$(", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "schwere plagen", "tokens": ["schwe\u00b7re", "pla\u00b7gen"], "token_info": ["word", "word"], "pos": ["ADJA", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Die diser grosse Krieg mit hunger/ schwert/ pest/", "tokens": ["Die", "di\u00b7ser", "gros\u00b7se", "Krieg", "mit", "hun\u00b7ger", "/", "schwert", "/", "pest", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "APPR", "ADJA", "$(", "VVFIN", "$(", "VVFIN", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "brand/", "tokens": ["brand", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Vnd vnerh\u00f6rter wuht auff vnser Vatterland", "tokens": ["Vnd", "vner\u00b7h\u00f6r\u00b7ter", "wuht", "auff", "vn\u00b7ser", "Vat\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Au\u00df giesset/ ohn abla\u00df zu h\u00f6ren/ schreiben/ klagen/", "tokens": ["Au\u00df", "gies\u00b7set", "/", "ohn", "ab\u00b7la\u00df", "zu", "h\u00f6\u00b7ren", "/", "schrei\u00b7ben", "/", "kla\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "$(", "APPR", "NN", "PTKZU", "VVINF", "$(", "VVINF", "$(", "VVINF", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Da ward mit wunder mir vnd mit wohn f\u00fcrgetrage:", "tokens": ["Da", "ward", "mit", "wun\u00b7der", "mir", "vnd", "mit", "wohn", "f\u00fcr\u00b7ge\u00b7tra\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NN", "PPER", "KON", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-++-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Mein Opitz/ deiner lieb vnd freindschafft wehrtes", "tokens": ["Mein", "O\u00b7pitz", "/", "dei\u00b7ner", "lieb", "vnd", "freind\u00b7schafft", "wehr\u00b7tes"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "ADJD", "KON", "VVFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "pfand/", "tokens": ["pfand", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Pfand/ welches mir al\u00dfbald die feder au\u00df der ha\u0303d/", "tokens": ["Pfand", "/", "wel\u00b7ches", "mir", "al\u00df\u00b7bald", "die", "fe\u00b7der", "au\u00df", "der", "h\u00e3d", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PWS", "PPER", "ADV", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vnd au\u00df dem mund vnd gaist die klag vnd layd", "tokens": ["Vnd", "au\u00df", "dem", "mund", "vnd", "gaist", "die", "klag", "vnd", "layd"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "KON", "VVFIN", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "geschlagen.", "tokens": ["ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Dan ja dein Orgelstraich/ vnd deiner Harpfen klang", "tokens": ["Dan", "ja", "dein", "Or\u00b7gel\u00b7straich", "/", "vnd", "dei\u00b7ner", "Harp\u00b7fen", "klang"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "$(", "KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So lieblich das geh\u00f6r vnd hertz zugleich ber\u00fchren/", "tokens": ["So", "lieb\u00b7lich", "das", "ge\u00b7h\u00f6r", "vnd", "hertz", "zu\u00b7gleich", "be\u00b7r\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "KON", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df (wer sin\u0303reich) mit mir erforschet jhre\u0304 zwang/", "tokens": ["Da\u00df", "(", "wer", "si\u00f1reich", ")", "mit", "mir", "er\u00b7for\u00b7schet", "jhr\u0113", "zwang", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "PWS", "ADJD", "$(", "APPR", "PPER", "VVFIN", "PPER", "VVFIN", "$("], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Der kan nichts dan dein werck vnd wehrt zu hertzen", "tokens": ["Der", "kan", "nichts", "dan", "dein", "werck", "vnd", "wehrt", "zu", "hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PIS", "ADV", "PPOSAT", "NN", "KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "f\u00fchren/", "tokens": ["f\u00fch\u00b7ren", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Vnd sein mund mu\u00df dich bald mit einem lobge-", "tokens": ["Vnd", "sein", "mund", "mu\u00df", "dich", "bald", "mit", "ei\u00b7nem", "lob\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "PPER", "ADV", "APPR", "ART", "TRUNC"], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "sang/", "tokens": ["sang", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Vnd seine hand dein haupt mit Lorb\u00f6rzweigen", "tokens": ["Vnd", "sei\u00b7ne", "hand", "dein", "haupt", "mit", "Lor\u00b7b\u00f6r\u00b7zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "zieren.", "tokens": ["zie\u00b7ren", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}