{"dta.poem.18663": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Lied und Gebilde .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Mag der Grieche seinen Thon", "tokens": ["Mag", "der", "Grie\u00b7che", "sei\u00b7nen", "Thon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu Gestalten dr\u00fccken,", "tokens": ["Zu", "Ge\u00b7stal\u00b7ten", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "An der eignen H\u00e4nde Sohn", "tokens": ["An", "der", "eig\u00b7nen", "H\u00e4n\u00b7de", "Sohn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Steigern sein Entz\u00fccken;", "tokens": ["Stei\u00b7gern", "sein", "Ent\u00b7z\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Aber uns ist wonnereich", "tokens": ["A\u00b7ber", "uns", "ist", "won\u00b7ne\u00b7reich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In den Euphrat greifen,", "tokens": ["In", "den", "Eu\u00b7ph\u00b7rat", "grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und im fl\u00fcssgen Element", "tokens": ["Und", "im", "fl\u00fcss\u00b7gen", "E\u00b7le\u00b7ment"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hin und wieder schweifen.", "tokens": ["Hin", "und", "wie\u00b7der", "schwei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "L\u00f6scht ich so der Seele Brand", "tokens": ["L\u00f6scht", "ich", "so", "der", "See\u00b7le", "Brand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lied es wird erschallen;", "tokens": ["Lied", "es", "wird", "er\u00b7schal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VAFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sch\u00f6pft des Dichters reine Hand", "tokens": ["Sch\u00f6pft", "des", "Dich\u00b7ters", "rei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wasser wird sich ballen.", "tokens": ["Was\u00b7ser", "wird", "sich", "bal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PRF", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}