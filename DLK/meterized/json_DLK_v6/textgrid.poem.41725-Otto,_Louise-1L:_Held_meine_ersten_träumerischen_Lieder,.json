{"textgrid.poem.41725": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Held meine ersten tr\u00e4umerischen Lieder,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Held meine ersten tr\u00e4umerischen Lieder,", "tokens": ["Held", "mei\u00b7ne", "ers\u00b7ten", "tr\u00e4u\u00b7me\u00b7ri\u00b7schen", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Idol dem einstens sich mein Herz geweiht,", "tokens": ["I\u00b7dol", "dem", "eins\u00b7tens", "sich", "mein", "Herz", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PRF", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Wie damals tret ich vor dein Antlitz wieder", "tokens": ["Wie", "da\u00b7mals", "tret", "ich", "vor", "dein", "Ant\u00b7litz", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und schau Dich an Du Sohn der neuen Zeit!", "tokens": ["Und", "schau", "Dich", "an", "Du", "Sohn", "der", "neu\u00b7en", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPER", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und schau Dich an und alte Schmerzen wachen", "tokens": ["Und", "schau", "Dich", "an", "und", "al\u00b7te", "Schmer\u00b7zen", "wa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "KON", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In meiner Seele auf und schrein zu Dir.", "tokens": ["In", "mei\u00b7ner", "See\u00b7le", "auf", "und", "schrein", "zu", "Dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "KON", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und h\u00f6hnische D\u00e4monen h\u00f6r ich lachen,", "tokens": ["Und", "h\u00f6h\u00b7ni\u00b7sche", "D\u00e4\u00b7mo\u00b7nen", "h\u00f6r", "ich", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dich h\u00f6r' ich rufen: \u00bbWas willst du von mir?\u00ab", "tokens": ["Dich", "h\u00f6r'", "ich", "ru\u00b7fen", ":", "\u00bb", "Was", "willst", "du", "von", "mir", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$.", "$(", "PWS", "VMFIN", "PPER", "APPR", "PPER", "$.", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00bbarmselig Kind! bist Du die tolle M\u00fccke!", "tokens": ["\u00bb", "arm\u00b7se\u00b7lig", "Kind", "!", "bist", "Du", "die", "tol\u00b7le", "M\u00fc\u00b7cke", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "NN", "$.", "VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Die wild in meines Geistes Flamme flog,", "tokens": ["Die", "wild", "in", "mei\u00b7nes", "Geis\u00b7tes", "Flam\u00b7me", "flog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die ich versengt zu jenen tausend schicke,", "tokens": ["Die", "ich", "ver\u00b7sengt", "zu", "je\u00b7nen", "tau\u00b7send", "schi\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "PDAT", "CARD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die so wie Dich mein Feuergl\u00fch'n betrog?", "tokens": ["Die", "so", "wie", "Dich", "mein", "Feu\u00b7er\u00b7gl\u00fch'n", "be\u00b7trog", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "KOKOM", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und willst Du nun mich schelten und mich h\u00f6hnen,", "tokens": ["Und", "willst", "Du", "nun", "mich", "schel\u00b7ten", "und", "mich", "h\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "PPER", "VVFIN", "KON", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mein k\u00fchnes Flackern sei ein Irrlichtschein \u2013", "tokens": ["Mein", "k\u00fch\u00b7nes", "Fla\u00b7ckern", "sei", "ein", "Irr\u00b7licht\u00b7schein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ihr konntet Euch an diese Glut gew\u00f6hnen,", "tokens": ["Ihr", "konn\u00b7tet", "Euch", "an", "die\u00b7se", "Glut", "ge\u00b7w\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ich rief Euch nicht, Ihr st\u00fcrztet selbst hinein.\u00ab", "tokens": ["Ich", "rief", "Euch", "nicht", ",", "Ihr", "st\u00fcrz\u00b7tet", "selbst", "hin\u00b7ein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VVFIN", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nein Dichter, nein! wir wolln Dich nicht verklagen,", "tokens": ["Nein", "Dich\u00b7ter", ",", "nein", "!", "wir", "wolln", "Dich", "nicht", "ver\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "$,", "PTKANT", "$.", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du trugst zu viel schon an der eignen Pein,", "tokens": ["Du", "trugst", "zu", "viel", "schon", "an", "der", "eig\u00b7nen", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIS", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und solltest nun auch noch die fremde tragen", "tokens": ["Und", "soll\u00b7test", "nun", "auch", "noch", "die", "frem\u00b7de", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "ADV", "ADV", "ART", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schuld an dem Elend uns'rer Zeit zu sein?", "tokens": ["Schuld", "an", "dem", "E\u00b7lend", "un\u00b7s'\u00b7rer", "Zeit", "zu", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PPOSAT", "NN", "PTKZU", "VAINF", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Und sei's und sei's da\u00df wir die Ketten f\u00fchlen", "tokens": ["Und", "sei's", "und", "sei's", "da\u00df", "wir", "die", "Ket\u00b7ten", "f\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "KON", "NE", "KOUS", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Erdenqual wie Du gef\u00fchlt sie hast,", "tokens": ["Der", "Er\u00b7den\u00b7qual", "wie", "Du", "ge\u00b7f\u00fchlt", "sie", "hast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "PPER", "VVFIN", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ruhlos in Schmerzen und in Zweifeln w\u00fchlen", "tokens": ["Ruh\u00b7los", "in", "Schmer\u00b7zen", "und", "in", "Zwei\u00b7feln", "w\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "KON", "APPR", "NN", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.8": {"text": "Gleich Dir empfindend dieses Staubes Last. \u2013", "tokens": ["Gleich", "Dir", "emp\u00b7fin\u00b7dend", "die\u00b7ses", "Stau\u00b7bes", "Last", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PPER", "VVPP", "PDAT", "NN", "NE", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Hab Dank', hab Dank', denn besser ist's die Ketten", "tokens": ["Hab", "Dank'", ",", "hab", "Dank'", ",", "denn", "bes\u00b7ser", "ist's", "die", "Ket\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "$,", "VAFIN", "NN", "$,", "KON", "ADJD", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Du trugst und die unsern, endlich seh'n,", "tokens": ["Die", "Du", "trugst", "und", "die", "un\u00b7sern", ",", "end\u00b7lich", "seh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "KON", "ART", "NN", "$,", "ADV", "VVINF", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Zeigst Du ein Mittel doch uns zu erretten", "tokens": ["Zeigst", "Du", "ein", "Mit\u00b7tel", "doch", "uns", "zu", "er\u00b7ret\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und zu dem unsern machen wir dein Flehn:", "tokens": ["Und", "zu", "dem", "un\u00b7sern", "ma\u00b7chen", "wir", "dein", "Flehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "F\u00fcr Herzenwunden suchst Du Schlachtenwunden,", "tokens": ["F\u00fcr", "Her\u00b7zen\u00b7wun\u00b7den", "suchst", "Du", "Schlach\u00b7ten\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "F\u00fcr Glaubenszweifel eine edle That:", "tokens": ["F\u00fcr", "Glau\u00b7bens\u00b7zwei\u00b7fel", "ei\u00b7ne", "ed\u00b7le", "That", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Heil Dir! Du hast das Kriegergrab gefunden", "tokens": ["Heil", "Dir", "!", "Du", "hast", "das", "Krie\u00b7ger\u00b7grab", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$.", "PPER", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Um das dein Herz im Freiheitskampfe bat.", "tokens": ["Um", "das", "dein", "Herz", "im", "Frei\u00b7heits\u00b7kamp\u00b7fe", "bat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "O k\u00f6nnte ich so unser Schicksal wenden!", "tokens": ["O", "k\u00f6nn\u00b7te", "ich", "so", "un\u00b7ser", "Schick\u00b7sal", "wen\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn uns verlangt danach wie Dich verlangt", "tokens": ["Denn", "uns", "ver\u00b7langt", "da\u00b7nach", "wie", "Dich", "ver\u00b7langt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PAV", "PWAV", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im Freiheitskampfe siegend zu vollenden,", "tokens": ["Im", "Frei\u00b7heits\u00b7kamp\u00b7fe", "sie\u00b7gend", "zu", "voll\u00b7en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vor einem Knechtstod nur sind wir erbangt.", "tokens": ["Vor", "ei\u00b7nem", "Knechts\u00b7tod", "nur", "sind", "wir", "er\u00b7bangt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In Liedern s\u00e4en wir der Freiheit Samen", "tokens": ["In", "Lie\u00b7dern", "s\u00e4\u00b7en", "wir", "der", "Frei\u00b7heit", "Sa\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und hoffen, da\u00df er spurlos nicht verweht,", "tokens": ["Und", "hof\u00b7fen", ",", "da\u00df", "er", "spur\u00b7los", "nicht", "ver\u00b7weht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da\u00df, wie er ausges\u00e4't in Gottes Namen,", "tokens": ["Da\u00df", ",", "wie", "er", "aus\u00b7ge\u00b7s\u00e4't", "in", "Got\u00b7tes", "Na\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "PPER", "VVFIN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein reiches Erntefeld aus ihm ersteht.", "tokens": ["Ein", "rei\u00b7ches", "Ern\u00b7te\u00b7feld", "aus", "ihm", "er\u00b7steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}