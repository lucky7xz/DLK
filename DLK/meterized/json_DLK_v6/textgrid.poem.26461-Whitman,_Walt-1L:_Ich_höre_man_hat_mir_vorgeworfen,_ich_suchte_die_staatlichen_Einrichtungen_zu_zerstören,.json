{"textgrid.poem.26461": {"metadata": {"author": {"name": "Whitman, Walt", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich h\u00f6re man hat mir vorgeworfen, ich suchte die staatlichen Einrichtungen zu zerst\u00f6ren,", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich h\u00f6re man hat mir vorgeworfen, ich suchte die staatlichen Einrichtungen zu zerst\u00f6ren,", "tokens": ["Ich", "h\u00f6\u00b7re", "man", "hat", "mir", "vor\u00b7ge\u00b7wor\u00b7fen", ",", "ich", "such\u00b7te", "die", "staat\u00b7li\u00b7chen", "Ein\u00b7rich\u00b7tun\u00b7gen", "zu", "zer\u00b7st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "VAFIN", "PPER", "VVPP", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-+--+--+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Ich bin aber weder f\u00fcr noch gegen Institutionen!", "tokens": ["Ich", "bin", "a\u00b7ber", "we\u00b7der", "f\u00fcr", "noch", "ge\u00b7gen", "Ins\u00b7ti\u00b7tu\u00b7ti\u00b7o\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "KON", "APPR", "ADV", "APPR", "NN", "$."], "meter": "++--+-+-+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.3": {"text": "(was habe ich mit ihnen zu tun oder mit ihrer Zerst\u00f6rung?)", "tokens": ["(", "was", "ha\u00b7be", "ich", "mit", "ih\u00b7nen", "zu", "tun", "o\u00b7der", "mit", "ih\u00b7rer", "Zer\u00b7st\u00f6\u00b7rung", "?", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VAFIN", "PPER", "APPR", "PPER", "PTKZU", "VVINF", "KON", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Ich will nur in Manahatta und in jeder Stadt dieser Staaten, im Innern wie am Meeresufer,", "tokens": ["Ich", "will", "nur", "in", "Ma\u00b7na\u00b7hat\u00b7ta", "und", "in", "je\u00b7der", "Stadt", "die\u00b7ser", "Staa\u00b7ten", ",", "im", "In\u00b7nern", "wie", "am", "Mee\u00b7res\u00b7u\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "NE", "KON", "APPR", "PIAT", "NN", "PDAT", "NN", "$,", "APPRART", "NN", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+--+--+--+-+--+--+-+-+-+-", "measure": "amphibrach.tetra.plus"}, "line.5": {"text": "In den Feldern und W\u00e4ldern, \u00fcber jedem Schiffskiel, der das Wasser durchfurcht,", "tokens": ["In", "den", "Fel\u00b7dern", "und", "W\u00e4l\u00b7dern", ",", "\u00fc\u00b7ber", "je\u00b7dem", "Schiffs\u00b7kiel", ",", "der", "das", "Was\u00b7ser", "durch\u00b7furcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "$,", "APPR", "PIAT", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "--+--+-+-+-+-+-+--+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Ohne Staatsgeb\u00e4ude, Regeln, Bevollm\u00e4chtigte oder Beweisgr\u00fcnde,", "tokens": ["Oh\u00b7ne", "Staats\u00b7ge\u00b7b\u00e4u\u00b7de", ",", "Re\u00b7geln", ",", "Be\u00b7voll\u00b7m\u00e4ch\u00b7tig\u00b7te", "o\u00b7der", "Be\u00b7weis\u00b7gr\u00fcn\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "VVFIN", "KON", "NN", "$,"], "meter": "+-+-+-+--++--+---+-", "measure": "trochaic.octa.plus.relaxed"}, "line.7": {"text": "Die Institution kameradschaftlicher Liebe errichten.", "tokens": ["Die", "Ins\u00b7ti\u00b7tu\u00b7ti\u00b7on", "ka\u00b7me\u00b7rad\u00b7schaft\u00b7li\u00b7cher", "Lie\u00b7be", "er\u00b7rich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}}}}}