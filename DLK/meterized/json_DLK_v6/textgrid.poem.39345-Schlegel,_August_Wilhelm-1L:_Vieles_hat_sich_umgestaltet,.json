{"textgrid.poem.39345": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vieles hat sich umgestaltet,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vieles hat sich umgestaltet,", "tokens": ["Vie\u00b7les", "hat", "sich", "um\u00b7ge\u00b7stal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Manches Neu' ist schon veraltet,", "tokens": ["Man\u00b7ches", "Neu'", "ist", "schon", "ver\u00b7al\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zwietracht hat sich mehr zerspaltet,", "tokens": ["Zwiet\u00b7racht", "hat", "sich", "mehr", "zer\u00b7spal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Grausam hat die Zeit geschaltet,", "tokens": ["Grau\u00b7sam", "hat", "die", "Zeit", "ge\u00b7schal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch die Lieb' ist nicht erkaltet,", "tokens": ["Doch", "die", "Lieb'", "ist", "nicht", "er\u00b7kal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "So die Schwingen erst entfaltet,", "tokens": ["So", "die", "Schwin\u00b7gen", "erst", "ent\u00b7fal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.7": {"text": "Als ich jene Lieder sang.", "tokens": ["Als", "ich", "je\u00b7ne", "Lie\u00b7der", "sang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was der J\u00fcngling zu vollenden", "tokens": ["Was", "der", "J\u00fcng\u00b7ling", "zu", "voll\u00b7en\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stolzen Muths sich konnte blenden,", "tokens": ["Stol\u00b7zen", "Muths", "sich", "konn\u00b7te", "blen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Will das Leben anders wenden;", "tokens": ["Will", "das", "Le\u00b7ben", "an\u00b7ders", "wen\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kaum beginnend, mu\u00df man enden.", "tokens": ["Kaum", "be\u00b7gin\u00b7nend", ",", "mu\u00df", "man", "en\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "VMFIN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nehmt denn aus des Mannes H\u00e4nden,", "tokens": ["Nehmt", "denn", "aus", "des", "Man\u00b7nes", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deutsche, die geringen Spenden:", "tokens": ["Deut\u00b7sche", ",", "die", "ge\u00b7rin\u00b7gen", "Spen\u00b7den", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Euer bin ich Lebenslang.", "tokens": ["Eu\u00b7er", "bin", "ich", "Le\u00b7bens\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}