{"dta.poem.5234": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Der weinende Amor,  \n bey Betrachtung einer Bilds\u00e4ule zu Char-  \n lottenburg im Garten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-200905197778", "language": ["de:0.99"], "booktitle": "Karsch, Anna Luise: Auserlesene Gedichte. Berlin, 1764."}, "poem": {"stanza.1": {"line.1": {"text": "Soll meiner Macht ein Sohn der Erde lachen?", "tokens": ["Soll", "mei\u00b7ner", "Macht", "ein", "Sohn", "der", "Er\u00b7de", "la\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich hie\u00df den Donnergott zum Schwan,", "tokens": ["Ich", "hie\u00df", "den", "Don\u00b7ner\u00b7gott", "zum", "Schwan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum Stier, ich hie\u00df ihn sich zur goldnen Wolke machen,", "tokens": ["Zum", "Stier", ",", "ich", "hie\u00df", "ihn", "sich", "zur", "gold\u00b7nen", "Wol\u00b7ke", "ma\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PPER", "VVFIN", "PPER", "PRF", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mir v\u00f6llig unterthan.", "tokens": ["Mir", "v\u00f6l\u00b7lig", "un\u00b7ter\u00b7than", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}