{"dta.poem.13199": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z ehndes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201esie wandt sich hin und sprach: \u201eder Himmel gab sie mir;", "tokens": ["\u201e", "sie", "wandt", "sich", "hin", "und", "sprach", ":", "\u201e", "der", "Him\u00b7mel", "gab", "sie", "mir", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PRF", "PTKVZ", "KON", "VVFIN", "$.", "$(", "ART", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201edurch ihn gebar sie mich; ich sag\u2019 ihm Danck daf\u00fcr,", "tokens": ["\u201e", "durch", "ihn", "ge\u00b7bar", "sie", "mich", ";", "ich", "sag'", "ihm", "Danck", "da\u00b7f\u00fcr", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPER", "VVFIN", "PPER", "PRF", "$.", "PPER", "VVFIN", "PPER", "NN", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eund ihr um jene Sorg in der sie mich erzoge,", "tokens": ["\u201e", "und", "ihr", "um", "je\u00b7ne", "Sorg", "in", "der", "sie", "mich", "er\u00b7zo\u00b7ge", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPER", "APPR", "PDAT", "NN", "APPR", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eum alles, was ihr Herz zu meinem Wohlseyn pfloge.", "tokens": ["\u201e", "um", "al\u00b7les", ",", "was", "ihr", "Herz", "zu", "mei\u00b7nem", "Wohl\u00b7seyn", "pflo\u00b7ge", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PIS", "$,", "PRELS", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "185\u201dVon dir Gro\u00dfm\u00e4chtigste! ward mir die Kunst gelehrt,", "tokens": ["\"", "Von", "dir", "Gro\u00df\u00b7m\u00e4ch\u00b7tigs\u00b7te", "!", "ward", "mir", "die", "Kunst", "ge\u00b7lehrt", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPER", "NN", "$.", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "\u201edie man in meiner Kron und Majest\u00e4t verehrt.", "tokens": ["\u201e", "die", "man", "in", "mei\u00b7ner", "Kron", "und", "Ma\u00b7jes\u00b7t\u00e4t", "ver\u00b7ehrt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PRELS", "PIS", "APPR", "PPOSAT", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201emein Dir-Gehorsam-seyn, und dein Befehl-ertheilen,", "tokens": ["\u201e", "mein", "Dir\u00b7Ge\u00b7hor\u00b7sam\u00b7seyn", ",", "und", "dein", "Be\u00b7fehl\u00b7ert\u00b7hei\u00b7len", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201eseynd meiner W\u00fcrde Grund, des Thrones st\u00e4rckste S\u00e4ulen.", "tokens": ["\u201e", "seynd", "mei\u00b7ner", "W\u00fcr\u00b7de", "Grund", ",", "des", "Thro\u00b7nes", "st\u00e4rcks\u00b7te", "S\u00e4u\u00b7len", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPOSAT", "NN", "NN", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201egOtt ists, der mir den Schmuck der K\u00f6nigs-W\u00fcrde gab,", "tokens": ["\u201e", "gOtt", "ists", ",", "der", "mir", "den", "Schmuck", "der", "K\u00f6\u00b7nigs\u00b7W\u00fcr\u00b7de", "gab", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "FM.la", "$,", "PRELS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "190\u201dW\u00e4r es nicht sein Befehl, so tr\u00e4t ich ihn dir ab.", "tokens": ["\"", "W\u00e4r", "es", "nicht", "sein", "Be\u00b7fehl", ",", "so", "tr\u00e4t", "ich", "ihn", "dir", "ab", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201emithin nimm, was mein Herz zu der Bek\u00e4nntnis treibet:", "tokens": ["\u201e", "mi\u00b7thin", "nimm", ",", "was", "mein", "Herz", "zu", "der", "Be\u00b7k\u00e4nnt\u00b7nis", "trei\u00b7bet", ":"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVIMP", "$,", "PRELS", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "\u201eda\u00df es dem deinigen sich selber schuldig bleibet.", "tokens": ["\u201e", "da\u00df", "es", "dem", "dei\u00b7ni\u00b7gen", "sich", "sel\u00b7ber", "schul\u00b7dig", "blei\u00b7bet", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ART", "PPOSS", "PRF", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}