{"dta.poem.7806": {"metadata": {"author": {"name": "Birken, Sigmund von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1665", "urn": "urn:nbn:de:kobv:b4-200905198447", "language": ["de:0.99"], "booktitle": "Birken, Sigmund von: Pegnesische Gespr\u00e4chspiel-Gesellschaft von Nymfen und Hirten. N\u00fcrnberg, 1665."}, "poem": {"stanza.1": {"line.1": {"text": "Wir reden hier von Euch/ ihr hohe See-\nlen!", "tokens": ["Wir", "re\u00b7den", "hier", "von", "Euch", "/", "ihr", "ho\u00b7he", "See", "len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "$(", "PPOSAT", "ADJA", "TRUNC", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Viel kleiner ist das Werk/ als die Begier", "tokens": ["Viel", "klei\u00b7ner", "ist", "das", "Werk", "/", "als", "die", "Be\u00b7gier"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "$(", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und unsre Pflicht. So wenig Bl\u00e4tter hier/", "tokens": ["und", "uns\u00b7re", "Pflicht", ".", "So", "we\u00b7nig", "Bl\u00e4t\u00b7ter", "hier", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$.", "ADV", "PIAT", "NN", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "nicht Eure Preise zehlen/ noch erzehlen.", "tokens": ["nicht", "Eu\u00b7re", "Prei\u00b7se", "zeh\u00b7len", "/", "noch", "er\u00b7zeh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "VVFIN", "$(", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wir wollen Euch/ der Ehre selbst/ empfehle\u0304:", "tokens": ["Wir", "wol\u00b7len", "Euch", "/", "der", "Eh\u00b7re", "selbst", "/", "emp\u00b7fehl\u0113", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$(", "ART", "NN", "ADV", "$(", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "die Eurem Wehrt soll zahlen die Geb\u00fchr.", "tokens": ["die", "Eu\u00b7rem", "Wehrt", "soll", "zah\u00b7len", "die", "Ge\u00b7b\u00fchr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VMFIN", "VVINF", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Tritt Fama doch/ zu reden/ selbst herf\u00fcr:", "tokens": ["Tritt", "Fa\u00b7ma", "doch", "/", "zu", "re\u00b7den", "/", "selbst", "her\u00b7f\u00fcr", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "$(", "PTKZU", "VVINF", "$(", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "dieweil Verdienst und Gl\u00fcck sich izt verm\u00e4h-", "tokens": ["die\u00b7weil", "Ver\u00b7dienst", "und", "Gl\u00fcck", "sich", "izt", "ver\u00b7m\u00e4h"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "PRF", "ADV", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "So wachse dan\u0304 empor/ du Gr\u00e4flichs Haus!", "tokens": ["So", "wach\u00b7se", "dan\u0304", "em\u00b7por", "/", "du", "Gr\u00e4f\u00b7lichs", "Haus", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PAV", "PTKVZ", "$(", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "und breite dich in hohe Aester aus/", "tokens": ["und", "brei\u00b7te", "dich", "in", "ho\u00b7he", "A\u00b7es\u00b7ter", "aus", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+----+", "measure": "unknown.measure.tetra"}, "line.11": {"text": "steig Wolken-an/ werd Nachbar mit den", "tokens": ["steig", "Wol\u00b7ken\u00b7an", "/", "werd", "Nach\u00b7bar", "mit", "den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "$(", "VAFIN", "NN", "APPR", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Denk/ da\u00df du uns viel Helden schuldig seyst", "tokens": ["Denk", "/", "da\u00df", "du", "uns", "viel", "Hel\u00b7den", "schul\u00b7dig", "seyst"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "KOUS", "PPER", "PRF", "PIAT", "NN", "ADJD", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "und Heldinnen/ von solchem Preis und Deist.", "tokens": ["und", "Hel\u00b7din\u00b7nen", "/", "von", "sol\u00b7chem", "Preis", "und", "De\u00b7ist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$(", "APPR", "PIAT", "NN", "KON", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "Es soll/ von dir/ die Nachwelt reden lernen.", "tokens": ["Es", "soll", "/", "von", "dir", "/", "die", "Nach\u00b7welt", "re\u00b7den", "ler\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$(", "APPR", "PPER", "$(", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.15": {"text": "Ihr. Ihr. HochGr\u00e4fl. Gd. Gd.", "tokens": ["Ihr", ".", "Ihr", ".", "Hoch", "Gr\u00e4fl", ".", "Gd", ".", "Gd", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "$.", "PPER", "$.", "NE", "NE", "$.", "NN", "$.", "NN", "$."], "meter": "+-++-+", "measure": "unknown.measure.tetra"}}}}}