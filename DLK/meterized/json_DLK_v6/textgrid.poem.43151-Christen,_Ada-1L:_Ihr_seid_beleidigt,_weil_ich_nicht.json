{"textgrid.poem.43151": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr seid beleidigt, weil ich nicht", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr seid beleidigt, weil ich nicht", "tokens": ["Ihr", "seid", "be\u00b7lei\u00b7digt", ",", "weil", "ich", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "KOUS", "PPER", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ger\u00fchrt in Eure Arme st\u00fcrze", "tokens": ["Ge\u00b7r\u00fchrt", "in", "Eu\u00b7re", "Ar\u00b7me", "st\u00fcr\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und das Verzeihungs-Arangement", "tokens": ["Und", "das", "Ver\u00b7zei\u00b7hungs\u00b7A\u00b7ran\u00b7ge\u00b7ment"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit keiner Reuescene w\u00fcrze.", "tokens": ["Mit", "kei\u00b7ner", "Reu\u00b7es\u00b7ce\u00b7ne", "w\u00fcr\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich flehte nicht, Ihr selber seid", "tokens": ["Ich", "fleh\u00b7te", "nicht", ",", "Ihr", "sel\u00b7ber", "seid"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PPER", "ADV", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nun pl\u00f6tzlich gn\u00e4dig mir gewogen;", "tokens": ["Nun", "pl\u00f6tz\u00b7lich", "gn\u00e4\u00b7dig", "mir", "ge\u00b7wo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Doch legt die Gnadenmienen ab,", "tokens": ["Doch", "legt", "die", "Gna\u00b7den\u00b7mie\u00b7nen", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Schaut, welche Kluft Ihr einst gezogen.", "tokens": ["Schaut", ",", "wel\u00b7che", "Kluft", "Ihr", "einst", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAT", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Setzt nur her\u00fcber k\u00fchnen Sprungs,", "tokens": ["Setzt", "nur", "her\u00b7\u00fc\u00b7ber", "k\u00fch\u00b7nen", "Sprungs", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Seid einmal menschlich-unbesonnen. ...", "tokens": ["Seid", "ein\u00b7mal", "menschlich\u00b7un\u00b7be\u00b7son\u00b7nen", ".", "..."], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VAIMP", "ADV", "VVINF", "$.", "$("], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.11": {"text": "Brecht Ihr auch das Genick dabei,", "tokens": ["Brecht", "Ihr", "auch", "das", "Ge\u00b7nick", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "PAV", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.12": {"text": "Hat Welt und H\u00f6lle nur gewonnen.", "tokens": ["Hat", "Welt", "und", "H\u00f6l\u00b7le", "nur", "ge\u00b7won\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}