{"textgrid.poem.50427": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "16. Dieselbe in Knittel-Versen", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor meine Brunst hab' ich gelitten,", "tokens": ["Vor", "mei\u00b7ne", "Brunst", "hab'", "ich", "ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Schelm von Ohm hat mich ", "tokens": ["Dein", "Schelm", "von", "Ohm", "hat", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VAFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Schad' ist mein, und schmertzt mich sehr:", "tokens": ["Der", "Schad'", "ist", "mein", ",", "und", "schmertzt", "mich", "sehr", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "$,", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch deinetwegen schmertzt's mich mehr.", "tokens": ["Doch", "dei\u00b7net\u00b7we\u00b7gen", "schmertzt's", "mich", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie werd' ich nun im Bette pfeiffen,", "tokens": ["Wie", "werd'", "ich", "nun", "im", "Bet\u00b7te", "pfeif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn du umsonst wirst um dich greiffen;", "tokens": ["Wenn", "du", "um\u00b7sonst", "wirst", "um", "dich", "greif\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wie wird es mir, Frau Lise, gehn,", "tokens": ["Wie", "wird", "es", "mir", ",", "Frau", "Li\u00b7se", ",", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPER", "$,", "NN", "NE", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn ich so schlecht werd' hier bestehn?", "tokens": ["Wenn", "ich", "so", "schlecht", "werd'", "hier", "be\u00b7stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Verzeihe mir die frembde Frage:", "tokens": ["Ver\u00b7zei\u00b7he", "mir", "die", "fremb\u00b7de", "Fra\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Weil bey verst\u00f6rtem ", "tokens": ["Weil", "bey", "ver\u00b7st\u00f6r\u00b7tem"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "So wenig ich weiss, ", "tokens": ["So", "we\u00b7nig", "ich", "weiss", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Als ich kan sagen, ", "tokens": ["Als", "ich", "kan", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}}}}}