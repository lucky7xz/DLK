{"textgrid.poem.67584": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Bauer fand (es wintert' hart)", "tokens": ["Ein", "Bau\u00b7er", "fand", "(", "es", "win\u00b7tert'", "hart", ")"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "PPER", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein buntes Schl\u00e4ngelein,", "tokens": ["Ein", "bun\u00b7tes", "Schl\u00e4n\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von Frost erstarrt.", "tokens": ["Von", "Frost", "er\u00b7starrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "\u00bbdas bring' ich nun den Kindern mein!\u00ab", "tokens": ["\u00bb", "das", "bring'", "ich", "nun", "den", "Kin\u00b7dern", "mein", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VVFIN", "PPER", "ADV", "ART", "NN", "PPOSAT", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und barg es in den Busen 'nein.", "tokens": ["Und", "barg", "es", "in", "den", "Bu\u00b7sen", "'n\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Das Schl\u00e4ngelein", "tokens": ["Das", "Schl\u00e4n\u00b7ge\u00b7lein"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Erwarmte fein,", "tokens": ["Er\u00b7warm\u00b7te", "fein", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Und stach.", "tokens": ["Und", "stach", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Vertrau Du sch\u00f6nen Geistern Dich!", "tokens": ["Ver\u00b7trau", "Du", "sch\u00f6\u00b7nen", "Geis\u00b7tern", "Dich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie lohnen Dich", "tokens": ["Sie", "loh\u00b7nen", "Dich"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mit Natternstich.", "tokens": ["Mit", "Nat\u00b7tern\u00b7stich", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Verr\u00e4ther! Du, mit allen Deinen Musen", "tokens": ["Ver\u00b7r\u00e4\u00b7ther", "!", "Du", ",", "mit", "al\u00b7len", "Dei\u00b7nen", "Mu\u00b7sen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "$,", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Warst mir im Busen.", "tokens": ["Warst", "mir", "im", "Bu\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}