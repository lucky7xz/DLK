{"textgrid.poem.26191": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Die Eule ruft, als lacht ein Narr", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Eule ruft, als lacht ein Narr", "tokens": ["Die", "Eu\u00b7le", "ruft", ",", "als", "lacht", "ein", "Narr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und rennt starr seinen Kopf an B\u00e4ume ein,", "tokens": ["Und", "rennt", "starr", "sei\u00b7nen", "Kopf", "an", "B\u00e4u\u00b7me", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und alle B\u00e4ume lachen hinterdrein.", "tokens": ["Und", "al\u00b7le", "B\u00e4u\u00b7me", "la\u00b7chen", "hin\u00b7ter\u00b7drein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Im Mond verwandelt sich gern jeder Stein", "tokens": ["Im", "Mond", "ver\u00b7wan\u00b7delt", "sich", "gern", "je\u00b7der", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PRF", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und will gegr\u00fc\u00dft sein, und auch angesprochen,", "tokens": ["Und", "will", "ge\u00b7gr\u00fc\u00dft", "sein", ",", "und", "auch", "an\u00b7ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "VVPP", "VAINF", "$,", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und alle tun, als ob sie nach dir krochen.", "tokens": ["Und", "al\u00b7le", "tun", ",", "als", "ob", "sie", "nach", "dir", "kro\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVINF", "$,", "KOKOM", "KOUS", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du h\u00f6rst noch einer Sense schartigen Klang.", "tokens": ["Du", "h\u00f6rst", "noch", "ei\u00b7ner", "Sen\u00b7se", "schar\u00b7ti\u00b7gen", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Ein sp\u00e4ter M\u00e4her geht im Mond entlang", "tokens": ["Ein", "sp\u00e4\u00b7ter", "M\u00e4\u00b7her", "geht", "im", "Mond", "ent\u00b7lang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "APPO"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und haut ins Gras, hart ohne Sang.", "tokens": ["Und", "haut", "ins", "Gras", ",", "hart", "oh\u00b7ne", "Sang", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}