{"textgrid.poem.59669": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Gesang der Christenlerche", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es klingt ein Klang der Klage", "tokens": ["Es", "klingt", "ein", "Klang", "der", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Rings durch die Welt umher:", "tokens": ["Rings", "durch", "die", "Welt", "um\u00b7her", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbkurz sind der Menschen Tage", "tokens": ["\u00bb", "kurz", "sind", "der", "Men\u00b7schen", "Ta\u00b7ge"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und ihre M\u00fchen schwer,", "tokens": ["Und", "ih\u00b7re", "M\u00fc\u00b7hen", "schwer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nach leichtem Jugendspiele", "tokens": ["Nach", "leich\u00b7tem", "Ju\u00b7gend\u00b7spie\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Treibt Arbeit, M\u00fch' und Not", "tokens": ["Treibt", "Ar\u00b7beit", ",", "M\u00fch'", "und", "Not"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sie rastlos fort zum Ziele,", "tokens": ["Sie", "rast\u00b7los", "fort", "zum", "Zie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKVZ", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und dieses Ziel ist Tod.\u00ab", "tokens": ["Und", "die\u00b7ses", "Ziel", "ist", "Tod", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O Klang voll bittrer Wehen!", "tokens": ["O", "Klang", "voll", "bit\u00b7trer", "We\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Uralter Heidenklang!", "tokens": ["Ur\u00b7al\u00b7ter", "Hei\u00b7den\u00b7klang", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus Tiefen rings und H\u00f6hen", "tokens": ["Aus", "Tie\u00b7fen", "rings", "und", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie klingst du grausig bang!", "tokens": ["Wie", "klingst", "du", "grau\u00b7sig", "bang", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mit Zweifeln, Zittern, Zagen,", "tokens": ["Mit", "Zwei\u00b7feln", ",", "Zit\u00b7tern", ",", "Za\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit ungestilltem Schmerz", "tokens": ["Mit", "un\u00b7ge\u00b7still\u00b7tem", "Schmerz"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Stellst du die scharfen Fragen", "tokens": ["Stellst", "du", "die", "schar\u00b7fen", "Fra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ans arme Menschenherz.", "tokens": ["Ans", "ar\u00b7me", "Men\u00b7schen\u00b7herz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So mag ein Sandkorn schweben", "tokens": ["So", "mag", "ein", "Sand\u00b7korn", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf hoher Meeresh\u00f6h',", "tokens": ["Auf", "ho\u00b7her", "Mee\u00b7res\u00b7h\u00f6h'", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie Menschen st\u00fcrmisch beben", "tokens": ["Wie", "Men\u00b7schen", "st\u00fcr\u00b7misch", "be\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf wilder Lebenssee:", "tokens": ["Auf", "wil\u00b7der", "Le\u00b7bens\u00b7see", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ach! Zwischen F\u00fcrchten, Hoffen", "tokens": ["Ach", "!", "Zwi\u00b7schen", "F\u00fcrch\u00b7ten", ",", "Hof\u00b7fen"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["ITJ", "$.", "ADJA", "NN", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie hielten sie's wohl aus,", "tokens": ["Wie", "hiel\u00b7ten", "sie's", "wohl", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "St\u00fcndst du zum Trost nicht offen,", "tokens": ["St\u00fcndst", "du", "zum", "Trost", "nicht", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Du Grabesfriedenshaus?", "tokens": ["Du", "Gra\u00b7bes\u00b7frie\u00b7dens\u00b7haus", "?"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Fort, Heidenklang! Verklinge!", "tokens": ["Fort", ",", "Hei\u00b7den\u00b7klang", "!", "Ver\u00b7klin\u00b7ge", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$.", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verkling, uraltes Weh!", "tokens": ["Ver\u00b7kling", ",", "ur\u00b7al\u00b7tes", "Weh", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Komm, Christenlerche, singe", "tokens": ["Komm", ",", "Chris\u00b7ten\u00b7ler\u00b7che", ",", "sin\u00b7ge"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Lied aus h\u00f6hrer H\u00f6h',", "tokens": ["Ein", "Lied", "aus", "h\u00f6h\u00b7rer", "H\u00f6h'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Lied vom sch\u00f6nern Glauben,", "tokens": ["Ein", "Lied", "vom", "sch\u00f6\u00b7nern", "Glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von s\u00fc\u00dfern Friedens Ruh',", "tokens": ["Von", "s\u00fc\u00b7\u00dfern", "Frie\u00b7dens", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Komm, trag mit Noahs Tauben", "tokens": ["Komm", ",", "trag", "mit", "No\u00b7ahs", "Tau\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Uns gr\u00fcne Hoffnung zu.", "tokens": ["Uns", "gr\u00fc\u00b7ne", "Hoff\u00b7nung", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Komm, Christenlerche, singe,", "tokens": ["Komm", ",", "Chris\u00b7ten\u00b7ler\u00b7che", ",", "sin\u00b7ge", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was du so selig wei\u00dft,", "tokens": ["Was", "du", "so", "se\u00b7lig", "wei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Lust des Himmels singe,", "tokens": ["Die", "Lust", "des", "Him\u00b7mels", "sin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Held und Heiland hei\u00dft,", "tokens": ["Die", "Held", "und", "Hei\u00b7land", "hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Wahrheit hei\u00dft und Leben", "tokens": ["Die", "Wahr\u00b7heit", "hei\u00dft", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und Licht der Erdennacht,", "tokens": ["Und", "Licht", "der", "Er\u00b7den\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df nun kein Leid mehr beben,", "tokens": ["Da\u00df", "nun", "kein", "Leid", "mehr", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Kein Tod mehr grauen macht.", "tokens": ["Kein", "Tod", "mehr", "grau\u00b7en", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PIAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O s\u00fc\u00dfer Klang der Freude!", "tokens": ["O", "s\u00fc\u00b7\u00dfer", "Klang", "der", "Freu\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O Klang der Seligkeit!", "tokens": ["O", "Klang", "der", "Se\u00b7lig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nicht mehr der Stunden Beute,", "tokens": ["Nicht", "mehr", "der", "Stun\u00b7den", "Beu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich hei\u00dfe Ewigkeit.", "tokens": ["Ich", "hei\u00b7\u00dfe", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Verlisch, du Erdensonne!", "tokens": ["Ver\u00b7lisch", ",", "du", "Er\u00b7den\u00b7son\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Tu, finstres Grab, dich auf!", "tokens": ["Tu", ",", "finst\u00b7res", "Grab", ",", "dich", "auf", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hell flieget meine Wonne", "tokens": ["Hell", "flie\u00b7get", "mei\u00b7ne", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zum h\u00f6chsten Stern hinauf.", "tokens": ["Zum", "h\u00f6chs\u00b7ten", "Stern", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}