{"textgrid.poem.24140": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was w\u00e4r ich, h\u00e4tt ich nicht die hohe Kunst", "tokens": ["Was", "w\u00e4r", "ich", ",", "h\u00e4tt", "ich", "nicht", "die", "ho\u00b7he", "Kunst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "$,", "VAFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des sch\u00f6n gesetzten Wortes und die Kraft,", "tokens": ["Des", "sch\u00f6n", "ge\u00b7setz\u00b7ten", "Wor\u00b7tes", "und", "die", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit einem Strom von Strophen mir den Schmerz", "tokens": ["Mit", "ei\u00b7nem", "Strom", "von", "Stro\u00b7phen", "mir", "den", "Schmerz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und alles Dumpfe aus der Brust zu schwemmen.", "tokens": ["Und", "al\u00b7les", "Dump\u00b7fe", "aus", "der", "Brust", "zu", "schwem\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wieviel vers\u00e4umt ich! Wieviel Fr\u00fcchte lie\u00df", "tokens": ["Wie\u00b7viel", "ver\u00b7s\u00e4umt", "ich", "!", "Wie\u00b7viel", "Fr\u00fcch\u00b7te", "lie\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$.", "PWAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich auf der Lebenstafel unber\u00fchrt!", "tokens": ["Ich", "auf", "der", "Le\u00b7ben\u00b7sta\u00b7fel", "un\u00b7be\u00b7r\u00fchrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wieviel versah ich! Wieviel B\u00f6ses sann", "tokens": ["Wie\u00b7viel", "ver\u00b7sah", "ich", "!", "Wie\u00b7viel", "B\u00f6\u00b7ses", "sann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$.", "NE", "NE", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mein Herz, und wieviel s\u00fcndigte die Hand!", "tokens": ["Mein", "Herz", ",", "und", "wie\u00b7viel", "s\u00fcn\u00b7dig\u00b7te", "die", "Hand", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "PWAV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch einen sch\u00f6nen Reim zu r\u00fcnden war", "tokens": ["Doch", "ei\u00b7nen", "sch\u00f6\u00b7nen", "Reim", "zu", "r\u00fcn\u00b7den", "war"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "PTKZU", "VVINF", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich nie zu tr\u00e4ge, und ich frevelte", "tokens": ["Ich", "nie", "zu", "tr\u00e4\u00b7ge", ",", "und", "ich", "fre\u00b7vel\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "PTKA", "ADJD", "$,", "KON", "PPER", "VVFIN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Nie b\u00f6sen Sinnes gegen dich, oh Gut", "tokens": ["Nie", "b\u00f6\u00b7sen", "Sin\u00b7nes", "ge\u00b7gen", "dich", ",", "oh", "Gut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJA", "NN", "APPR", "PPER", "$,", "FM", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der G\u00fcter, das mir in der Wiege lag,", "tokens": ["Der", "G\u00fc\u00b7ter", ",", "das", "mir", "in", "der", "Wie\u00b7ge", "lag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Als ich der Mutter Wort zum ersten Mal", "tokens": ["Als", "ich", "der", "Mut\u00b7ter", "Wort", "zum", "ers\u00b7ten", "Mal"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Vernahm: Oh deutsche Sprache, allerherrlichste!", "tokens": ["Ver\u00b7nahm", ":", "Oh", "deut\u00b7sche", "Spra\u00b7che", ",", "al\u00b7ler\u00b7herr\u00b7lichs\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$.", "XY", "ADJA", "NN", "$,", "ADJA", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Kein Kind wird einst von mir im Leben stehn,", "tokens": ["Kein", "Kind", "wird", "einst", "von", "mir", "im", "Le\u00b7ben", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "APPR", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Wenn ich ins Nichts zur\u00fcckgegangen bin", "tokens": ["Wenn", "ich", "ins", "Nichts", "zu\u00b7r\u00fcck\u00b7ge\u00b7gan\u00b7gen", "bin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "PIS", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Und all mein Leben, all mein Schmerz und Lust", "tokens": ["Und", "all", "mein", "Le\u00b7ben", ",", "all", "mein", "Schmerz", "und", "Lust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "$,", "PIAT", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Vor\u00fcber und verschwunden wie die Wolke ist,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "und", "ver\u00b7schwun\u00b7den", "wie", "die", "Wol\u00b7ke", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "KOKOM", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die eben noch, durchgl\u00fcht von Sonnengold,", "tokens": ["Die", "e\u00b7ben", "noch", ",", "durch\u00b7gl\u00fcht", "von", "Son\u00b7nen\u00b7gold", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Wie eine ganze Welt voll Licht und Saft", "tokens": ["Wie", "ei\u00b7ne", "gan\u00b7ze", "Welt", "voll", "Licht", "und", "Saft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADJD", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Am hohen Himmel stand. \u2013 Dann wird vielleicht", "tokens": ["Am", "ho\u00b7hen", "Him\u00b7mel", "stand", ".", "\u2013", "Dann", "wird", "viel\u00b7leicht"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$.", "$(", "ADV", "VAFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Ein kleiner Vers von mir lebendig noch", "tokens": ["Ein", "klei\u00b7ner", "Vers", "von", "mir", "le\u00b7ben\u00b7dig", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "ADJD", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "In eines deutschen M\u00e4dchens Herzen bl\u00fchn,", "tokens": ["In", "ei\u00b7nes", "deut\u00b7schen", "M\u00e4d\u00b7chens", "Her\u00b7zen", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.21": {"text": "Von ihren Lippen wehen, wie der Duft,", "tokens": ["Von", "ih\u00b7ren", "Lip\u00b7pen", "we\u00b7hen", ",", "wie", "der", "Duft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Der aus dem Innersten der Rose kommt.", "tokens": ["Der", "aus", "dem", "In\u00b7ners\u00b7ten", "der", "Ro\u00b7se", "kommt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}