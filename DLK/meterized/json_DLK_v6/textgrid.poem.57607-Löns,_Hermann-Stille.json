{"textgrid.poem.57607": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Stille", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jetzt ist es still \u2013 kein Ast bewegt,", "tokens": ["Jetzt", "ist", "es", "still", "\u2013", "kein", "Ast", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$(", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur die nerv\u00f6sen Zitterpappeln", "tokens": ["Nur", "die", "ner\u00b7v\u00f6\u00b7sen", "Zit\u00b7ter\u00b7pap\u00b7peln"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Von dem Get\u00f6se aufgeregt", "tokens": ["Von", "dem", "Ge\u00b7t\u00f6\u00b7se", "auf\u00b7ge\u00b7regt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An allen Zweigen zappeln.", "tokens": ["An", "al\u00b7len", "Zwei\u00b7gen", "zap\u00b7peln", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Hinweggefegt vom Sturme sind", "tokens": ["Hin\u00b7weg\u00b7ge\u00b7fegt", "vom", "Stur\u00b7me", "sind"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der wilden Wolkenschar Gardinen,", "tokens": ["Der", "wil\u00b7den", "Wol\u00b7ken\u00b7schar", "Gar\u00b7di\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der gute Mond grinst wohlgesinnt", "tokens": ["Der", "gu\u00b7te", "Mond", "grinst", "wohl\u00b7ge\u00b7sinnt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit philistr\u00f6sen Mienen.", "tokens": ["Mit", "phi\u00b7lis\u00b7tr\u00f6\u00b7sen", "Mie\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er ist noch abgespannt und bleich", "tokens": ["Er", "ist", "noch", "ab\u00b7ge\u00b7spannt", "und", "bleich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von dem Getobe und Gebr\u00fclle.", "tokens": ["Von", "dem", "Ge\u00b7to\u00b7be", "und", "Ge\u00b7br\u00fcl\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch macht gesund und wohl ihn gleich", "tokens": ["Doch", "macht", "ge\u00b7sund", "und", "wohl", "ihn", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "KON", "ADV", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die hergestellte Stille.", "tokens": ["Die", "her\u00b7ge\u00b7stell\u00b7te", "Stil\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Um meine Seele leis sich spinnt", "tokens": ["Um", "mei\u00b7ne", "See\u00b7le", "leis", "sich", "spinnt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "ADJD", "PRF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Kindertr\u00e4ume bange Kette, \u2013", "tokens": ["Der", "Kin\u00b7der\u00b7tr\u00e4u\u00b7me", "ban\u00b7ge", "Ket\u00b7te", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "NN", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich bin nicht gerne weich gesinnt,", "tokens": ["Ich", "bin", "nicht", "ger\u00b7ne", "weich", "ge\u00b7sinnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geh lieber drum zu Bette.", "tokens": ["Geh", "lie\u00b7ber", "drum", "zu", "Bet\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PAV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}