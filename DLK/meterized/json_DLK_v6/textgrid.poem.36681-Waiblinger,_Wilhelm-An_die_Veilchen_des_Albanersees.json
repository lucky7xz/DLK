{"textgrid.poem.36681": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An die Veilchen des Albanersees", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alles Sch\u00f6ne feiern die Dichter, Alles", "tokens": ["Al\u00b7les", "Sch\u00f6\u00b7ne", "fei\u00b7ern", "die", "Dich\u00b7ter", ",", "Al\u00b7les"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "$,", "PIS"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "So im Schoo\u00df der Mutter Natur, und so im", "tokens": ["So", "im", "Schoo\u00df", "der", "Mut\u00b7ter", "Na\u00b7tur", ",", "und", "so", "im"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "NN", "$,", "KON", "ADV", "APPRART"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Menschenherzen schlummert, warum nicht euch auch,", "tokens": ["Men\u00b7schen\u00b7her\u00b7zen", "schlum\u00b7mert", ",", "wa\u00b7rum", "nicht", "euch", "auch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PWAV", "PTKNEG", "PPER", "ADV", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Duftende Wesen,", "tokens": ["Duf\u00b7ten\u00b7de", "We\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Die ihr mein Elysium schm\u00fcckt, vom Ufer", "tokens": ["Die", "ihr", "mein", "E\u00b7ly\u00b7si\u00b7um", "schm\u00fcckt", ",", "vom", "U\u00b7fer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVFIN", "$,", "APPRART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Meiner Lieblingsfluth in den k\u00fchlen Schatten", "tokens": ["Mei\u00b7ner", "Lieb\u00b7lings\u00b7fluth", "in", "den", "k\u00fch\u00b7len", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Immergr\u00fcner Eichen die Blumenfelsen", "tokens": ["Im\u00b7mer\u00b7gr\u00fc\u00b7ner", "Ei\u00b7chen", "die", "Blu\u00b7men\u00b7fel\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Freundlich emporbl\u00fcht!", "tokens": ["Freund\u00b7lich", "em\u00b7por\u00b7bl\u00fcht", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "+--++", "measure": "adoneus"}}, "stanza.3": {"line.1": {"text": "Was von allem Zarten der Erde glich' euch,", "tokens": ["Was", "von", "al\u00b7lem", "Zar\u00b7ten", "der", "Er\u00b7de", "glich'", "euch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PIS", "NN", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "O ihr s\u00fc\u00df Verletzbaren? Ausgeathmet", "tokens": ["O", "ihr", "s\u00fc\u00df", "Ver\u00b7letz\u00b7ba\u00b7ren", "?", "Aus\u00b7ge\u00b7ath\u00b7met"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$.", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Im versch\u00e4mten Mutterverlangen hat als", "tokens": ["Im", "ver\u00b7sch\u00e4m\u00b7ten", "Mut\u00b7ter\u00b7ver\u00b7lan\u00b7gen", "hat", "als"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "KOKOM"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Br\u00e4utlichen Seufzer", "tokens": ["Br\u00e4ut\u00b7li\u00b7chen", "Seuf\u00b7zer"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Euch die Fr\u00fchlingserde: zum erstenmale", "tokens": ["Euch", "die", "Fr\u00fch\u00b7lings\u00b7er\u00b7de", ":", "zum", "ers\u00b7ten\u00b7ma\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ART", "NN", "$.", "APPRART", "ADJA"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Ihr verborgnes Schmachten bekennend, l\u00e4chelt", "tokens": ["Ihr", "ver\u00b7borg\u00b7nes", "Schmach\u00b7ten", "be\u00b7ken\u00b7nend", ",", "l\u00e4\u00b7chelt"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$,", "VVFIN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Sie aus blauen Augen zum Himmel, ihrem", "tokens": ["Sie", "aus", "blau\u00b7en", "Au\u00b7gen", "zum", "Him\u00b7mel", ",", "ih\u00b7rem"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "APPR", "ADJA", "NN", "APPRART", "NN", "$,", "PPOSAT"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ewig Geliebten!", "tokens": ["E\u00b7wig", "Ge\u00b7lieb\u00b7ten", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Tiefe heil'ge Stille, wie dieser Landschaft", "tokens": ["Tie\u00b7fe", "heil'\u00b7ge", "Stil\u00b7le", ",", "wie", "die\u00b7ser", "Land\u00b7schaft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "$,", "PWAV", "PDAT", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Ist auch euer Geist: ihr geh\u00f6rt der sichern", "tokens": ["Ist", "auch", "eu\u00b7er", "Geist", ":", "ihr", "ge\u00b7h\u00f6rt", "der", "si\u00b7chern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ART", "ADJA"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Gegenwart nicht an, nur der tiefern Ahnung,", "tokens": ["Ge\u00b7gen\u00b7wart", "nicht", "an", ",", "nur", "der", "tie\u00b7fern", "Ah\u00b7nung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PTKVZ", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-------+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Nur der Erinn'rung.", "tokens": ["Nur", "der", "Er\u00b7inn'\u00b7rung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Eure Farbe wohl ist die sch\u00f6nste: denn sie", "tokens": ["Eu\u00b7re", "Far\u00b7be", "wohl", "ist", "die", "sch\u00f6ns\u00b7te", ":", "denn", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "VAFIN", "ART", "ADJA", "$.", "KON", "PPER"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Tr\u00e4gt und liebt Hesperiens See und Himmel,", "tokens": ["Tr\u00e4gt", "und", "liebt", "Hes\u00b7pe\u00b7ri\u00b7ens", "See", "und", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "NE", "NN", "KON", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Nur da\u00df eurem Blau noch der Sehnsucht Purpur", "tokens": ["Nur", "da\u00df", "eu\u00b7rem", "Blau", "noch", "der", "Sehn\u00b7sucht", "Pur\u00b7pur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPOSAT", "NN", "ADV", "ART", "NN", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Lieblich ent\u00e4ugelt.", "tokens": ["Lieb\u00b7lich", "en\u00b7t\u00e4u\u00b7gelt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Alte Sagen kehren zur\u00fcck und fromme", "tokens": ["Al\u00b7te", "Sa\u00b7gen", "keh\u00b7ren", "zu\u00b7r\u00fcck", "und", "from\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ", "KON", "ADJA"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Fabeln, ja die heitern Gedanken schweifen", "tokens": ["Fa\u00b7beln", ",", "ja", "die", "hei\u00b7tern", "Ge\u00b7dan\u00b7ken", "schwei\u00b7fen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "In die goldne Zeit, da Askan euch pfl\u00fcckte,", "tokens": ["In", "die", "gold\u00b7ne", "Zeit", ",", "da", "As\u00b7kan", "euch", "pfl\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KOUS", "NE", "PPER", "VVFIN", "$,"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sinnende Bl\u00fcmchen.", "tokens": ["Sin\u00b7nen\u00b7de", "Bl\u00fcm\u00b7chen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "Dort am Fels, das nieder am Wasserspiegel", "tokens": ["Dort", "am", "Fels", ",", "das", "nie\u00b7der", "am", "Was\u00b7ser\u00b7spie\u00b7gel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "$,", "PRELS", "ADV", "APPRART", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit der Last der \u00fcppigen W\u00e4lder gr\u00fcnet,", "tokens": ["Mit", "der", "Last", "der", "\u00fcp\u00b7pi\u00b7gen", "W\u00e4l\u00b7der", "gr\u00fc\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "F\u00fchrte ja Aeneas gekr\u00f6nter Sohn den", "tokens": ["F\u00fchr\u00b7te", "ja", "A\u00b7e\u00b7neas", "ge\u00b7kr\u00f6n\u00b7ter", "Sohn", "den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "NE", "ADJA", "NN", "ART"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Scepter von Alba.", "tokens": ["Scep\u00b7ter", "von", "Al\u00b7ba", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.9": {"line.1": {"text": "Aber Alba sank, und des Troerf\u00fcrsten", "tokens": ["A\u00b7ber", "Al\u00b7ba", "sank", ",", "und", "des", "Tro\u00b7er\u00b7f\u00fcrs\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "$,", "KON", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Enkel alle, Weiber und Greis' und Kinder", "tokens": ["En\u00b7kel", "al\u00b7le", ",", "Wei\u00b7ber", "und", "Greis'", "und", "Kin\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PIS", "$,", "NN", "KON", "NE", "KON", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Zogen mit den G\u00f6ttern aus den gest\u00fcrzten", "tokens": ["Zo\u00b7gen", "mit", "den", "G\u00f6t\u00b7tern", "aus", "den", "ge\u00b7st\u00fcrz\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Mauern der Heimath", "tokens": ["Mau\u00b7ern", "der", "Hei\u00b7math"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.10": {"line.1": {"text": "Roma's immerwachsenden Herrscherthoren", "tokens": ["Ro\u00b7ma's", "im\u00b7mer\u00b7wach\u00b7sen\u00b7den", "Herr\u00b7scher\u00b7tho\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Weinend zu; da ward es an diesen Ufern", "tokens": ["Wei\u00b7nend", "zu", ";", "da", "ward", "es", "an", "die\u00b7sen", "U\u00b7fern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "$.", "ADV", "VAFIN", "PPER", "APPR", "PDAT", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Oed' und wild, statt K\u00f6nigen spro\u00dften", "tokens": ["O\u00b7ed'", "und", "wild", ",", "statt", "K\u00f6\u00b7ni\u00b7gen", "spro\u00df\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "$,", "KOUI", "NN", "VVFIN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Einsame Veilchen.", "tokens": ["Ein\u00b7sa\u00b7me", "Veil\u00b7chen", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.11": {"line.1": {"text": "Und wie selbst die r\u00e4chende Hand des Schicksals", "tokens": ["Und", "wie", "selbst", "die", "r\u00e4\u00b7chen\u00b7de", "Hand", "des", "Schick\u00b7sals"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADV", "ART", "ADJA", "NN", "ART", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Rom auch traf, und furchtbar die Tempel st\u00fcrzten,", "tokens": ["Rom", "auch", "traf", ",", "und", "furcht\u00b7bar", "die", "Tem\u00b7pel", "st\u00fcrz\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "$,", "KON", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Wo Triumphatoren den nun gefallnen", "tokens": ["Wo", "Tri\u00b7um\u00b7pha\u00b7to\u00b7ren", "den", "nun", "ge\u00b7fall\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ART", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "G\u00f6ttern die Sch\u00e4tze", "tokens": ["G\u00f6t\u00b7tern", "die", "Sch\u00e4t\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.12": {"line.1": {"text": "Der besiegten Erde zum Opfer brachten,", "tokens": ["Der", "be\u00b7sieg\u00b7ten", "Er\u00b7de", "zum", "Op\u00b7fer", "brach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Dennoch bliebt ihr, sicher in eurem Laube,", "tokens": ["Den\u00b7noch", "bliebt", "ihr", ",", "si\u00b7cher", "in", "eu\u00b7rem", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Wo's der Gott im Tempel nicht war, der C\u00e4sar", "tokens": ["Wo's", "der", "Gott", "im", "Tem\u00b7pel", "nicht", "war", ",", "der", "C\u00e4\u00b7sar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ART", "NN", "APPRART", "NN", "PTKNEG", "VAFIN", "$,", "ART", "NE"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Nicht im Palaste,", "tokens": ["Nicht", "im", "Pa\u00b7las\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.13": {"line.1": {"text": "Noch dieselben, wie ihr gebl\u00fcht, als dr\u00fcben", "tokens": ["Noch", "die\u00b7sel\u00b7ben", ",", "wie", "ihr", "ge\u00b7bl\u00fcht", ",", "als", "dr\u00fc\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PDAT", "$,", "PWAV", "PPER", "VVPP", "$,", "KOUS", "ADV"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Dort am R\u00fccken einst des Vulkans im Haine", "tokens": ["Dort", "am", "R\u00fc\u00b7cken", "einst", "des", "Vul\u00b7kans", "im", "Hai\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ADV", "ART", "NN", "APPRART", "NN"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ferentina's Latiums B\u00fcrger sich am", "tokens": ["Fe\u00b7ren\u00b7ti\u00b7na's", "La\u00b7ti\u00b7ums", "B\u00fcr\u00b7ger", "sich", "am"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "NN", "PRF", "APPRART"], "meter": "+---+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Altar versammelt.", "tokens": ["Al\u00b7tar", "ver\u00b7sam\u00b7melt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.14": {"line.1": {"text": "Ihr d\u00fcrft nicht erzittern, so wie die Eiche,", "tokens": ["Ihr", "d\u00fcrft", "nicht", "er\u00b7zit\u00b7tern", ",", "so", "wie", "die", "Ei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "ADV", "KOKOM", "ART", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Deren Kron' umwirbelt der Sturm, ihr schaut dem", "tokens": ["De\u00b7ren", "Kron'", "um\u00b7wir\u00b7belt", "der", "Sturm", ",", "ihr", "schaut", "dem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "NN", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "V\u00f6lkerwechsel zu, und am Ende pfl\u00fcckt euch", "tokens": ["V\u00f6l\u00b7ker\u00b7wech\u00b7sel", "zu", ",", "und", "am", "En\u00b7de", "pfl\u00fcckt", "euch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "KON", "APPRART", "NN", "VVFIN", "PPER"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Selbst nur die Liebe.", "tokens": ["Selbst", "nur", "die", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.15": {"line.1": {"text": "Ich allein, holdl\u00e4chelnde Fr\u00fchlingskinder,", "tokens": ["Ich", "al\u00b7lein", ",", "hold\u00b7l\u00e4\u00b7cheln\u00b7de", "Fr\u00fch\u00b7lings\u00b7kin\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich, der, sterblicher ich als ihr, der Liebe", "tokens": ["Ich", ",", "der", ",", "sterb\u00b7li\u00b7cher", "ich", "als", "ihr", ",", "der", "Lie\u00b7be"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "PRELS", "$,", "PRELS", "PPER", "KOUS", "PPER", "$,", "ART", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Sanfte Freuden lange nicht anders als im", "tokens": ["Sanf\u00b7te", "Freu\u00b7den", "lan\u00b7ge", "nicht", "an\u00b7ders", "als", "im"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADV", "PTKNEG", "ADV", "KOKOM", "APPRART"], "meter": "+-+-+--+-++", "measure": "sapphicusminor"}, "line.4": {"text": "Liede gekannt hat,", "tokens": ["Lie\u00b7de", "ge\u00b7kannt", "hat", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.16": {"line.1": {"text": "Ach, ich pfl\u00fcck' euch nicht! Als ein tr\u00fcber Fremdling", "tokens": ["Ach", ",", "ich", "pfl\u00fcck", "euch", "nicht", "!", "Als", "ein", "tr\u00fc\u00b7ber", "Fremd\u00b7ling"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$.", "KOUS", "ART", "ADJA", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wandl' ich nur in eurer bescheidnen Heimath,", "tokens": ["Wandl'", "ich", "nur", "in", "eu\u00b7rer", "be\u00b7scheid\u00b7nen", "Hei\u00b7math", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+---", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Meine Liebe suchend, die mit des Lebens", "tokens": ["Mei\u00b7ne", "Lie\u00b7be", "su\u00b7chend", ",", "die", "mit", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVPP", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Bl\u00fcthen verschwunden.", "tokens": ["Bl\u00fc\u00b7then", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.17": {"line.1": {"text": "Eines f\u00e4llt mir ein, ob Diana, dieser", "tokens": ["Ei\u00b7nes", "f\u00e4llt", "mir", "ein", ",", "ob", "Di\u00b7a\u00b7na", ",", "die\u00b7ser"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PIS", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "NE", "$,", "PDAT"], "meter": "+-+-+-+----", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Ufer Schutzgottheit, mir die Liebesfreuden", "tokens": ["U\u00b7fer", "Schutz\u00b7got\u00b7theit", ",", "mir", "die", "Lie\u00b7bes\u00b7freu\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "$,", "PPER", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Nicht in euch verwandelt, da ihr so s\u00fc\u00df, o", "tokens": ["Nicht", "in", "euch", "ver\u00b7wan\u00b7delt", ",", "da", "ihr", "so", "s\u00fc\u00df", ",", "o"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PTKNEG", "APPR", "PPER", "VVPP", "$,", "KOUS", "PPER", "ADV", "ADJD", "$,", "FM"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Veilchen, mich anseht!", "tokens": ["Veil\u00b7chen", ",", "mich", "an\u00b7seht", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}