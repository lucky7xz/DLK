{"textgrid.poem.37190": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "Der Wein keine Panacee", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In jedem Kummer, jedem Schmerz", "tokens": ["In", "je\u00b7dem", "Kum\u00b7mer", ",", "je\u00b7dem", "Schmerz"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Preist immerhin den Wein als einen Wunderth\u00e4ter,", "tokens": ["Preist", "im\u00b7mer\u00b7hin", "den", "Wein", "als", "ei\u00b7nen", "Wun\u00b7der\u00b7th\u00e4\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sagt, er erhellt den Geist, entf\u00fchrt ihn himmelw\u00e4rts,", "tokens": ["Sagt", ",", "er", "er\u00b7hellt", "den", "Geist", ",", "ent\u00b7f\u00fchrt", "ihn", "him\u00b7mel\u00b7w\u00e4rts", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gibt kranken Seelen Schwung, macht leicht das Blut wie Aether,", "tokens": ["Gibt", "kran\u00b7ken", "See\u00b7len", "Schwung", ",", "macht", "leicht", "das", "Blut", "wie", "A\u00b7e\u00b7ther", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "NN", "$,", "VVFIN", "ADJD", "ART", "NN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.5": {"text": "Hebt den Entschlu\u00df zur That und zur Vers\u00f6hnung r\u00e4th er:", "tokens": ["Hebt", "den", "Ent\u00b7schlu\u00df", "zur", "That", "und", "zur", "Ver\u00b7s\u00f6h\u00b7nung", "r\u00e4\u00b7th", "er", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "KON", "APPRART", "NN", "VVFIN", "PPER", "$."], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.6": {"text": "Mir unterh\u00e4lt, erw\u00e4rmt, entflammt er nur das Herz,", "tokens": ["Mir", "un\u00b7ter\u00b7h\u00e4lt", ",", "er\u00b7w\u00e4rmt", ",", "ent\u00b7flammt", "er", "nur", "das", "Herz", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVPP", "$,", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mahlt Daphnen Reiz mir vor und ihren s\u00fc\u00dfen Scherz,", "tokens": ["Mahlt", "Daph\u00b7nen", "Reiz", "mir", "vor", "und", "ih\u00b7ren", "s\u00fc\u00b7\u00dfen", "Scherz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "PPER", "PTKVZ", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und \u2013 ach! von ihrem Stolz schweigt einzig der Verr\u00e4ther.", "tokens": ["Und", "\u2013", "ach", "!", "von", "ih\u00b7rem", "Stolz", "schweigt", "ein\u00b7zig", "der", "Ver\u00b7r\u00e4\u00b7ther", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "ITJ", "$.", "APPR", "PPOSAT", "NN", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}