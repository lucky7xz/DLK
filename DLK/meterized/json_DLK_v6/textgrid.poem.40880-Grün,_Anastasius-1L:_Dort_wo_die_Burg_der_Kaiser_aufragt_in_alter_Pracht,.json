{"textgrid.poem.40880": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dort wo die Burg der Kaiser aufragt in alter Pracht,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dort wo die Burg der Kaiser aufragt in alter Pracht,", "tokens": ["Dort", "wo", "die", "Burg", "der", "Kai\u00b7ser", "auf\u00b7ragt", "in", "al\u00b7ter", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "NN", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dort lagert K\u00f6nig Maxens gewalt'ge Heeresmacht;", "tokens": ["Dort", "la\u00b7gert", "K\u00f6\u00b7nig", "Ma\u00b7xens", "ge\u00b7walt'\u00b7ge", "Hee\u00b7res\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "NE", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Denn drin hat der Magyare die letzte Kraft verschanzt", "tokens": ["Denn", "drin", "hat", "der", "Ma\u00b7gy\u00b7a\u00b7re", "die", "letz\u00b7te", "Kraft", "ver\u00b7schanzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "VVPP"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und in die gew\u00f6lbten Fenster sein Donnergesch\u00fctz gepflanzt.", "tokens": ["Und", "in", "die", "ge\u00b7w\u00f6lb\u00b7ten", "Fens\u00b7ter", "sein", "Don\u00b7ner\u00b7ge\u00b7sch\u00fctz", "ge\u00b7pflanzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+--+-+--+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Hier sandten F\u00fcrsten und Schranzen einst Gnadenblicke heraus,", "tokens": ["Hier", "sand\u00b7ten", "F\u00fcrs\u00b7ten", "und", "Schran\u00b7zen", "einst", "Gna\u00b7den\u00b7bli\u00b7cke", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "ADV", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und wem solch einer gegolten, der eilte froher nach Haus;", "tokens": ["Und", "wem", "solch", "ei\u00b7ner", "ge\u00b7gol\u00b7ten", ",", "der", "eil\u00b7te", "fro\u00b7her", "nach", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PIAT", "ART", "ADJA", "$,", "PRELS", "VVFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+--+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Mit wem es jetzt lieb\u00e4ugelt aus diesen Fenstern nieder,", "tokens": ["Mit", "wem", "es", "jetzt", "lie\u00b7b\u00e4u\u00b7gelt", "aus", "die\u00b7sen", "Fens\u00b7tern", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "PPER", "ADV", "VVFIN", "APPR", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Auch der kehrt flugs zur Heimat mit pochendem Herzen wieder.", "tokens": ["Auch", "der", "kehrt", "flugs", "zur", "Hei\u00b7mat", "mit", "po\u00b7chen\u00b7dem", "Her\u00b7zen", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "VVFIN", "ADV", "APPRART", "NN", "APPR", "ADJA", "NN", "ADV", "$."], "meter": "--+--+--+--+-+-", "measure": "anapaest.tetra.plus"}}, "stanza.3": {"line.1": {"text": "Wo seid ihr, Kaiseradler, was hat euch fortgeschreckt?", "tokens": ["Wo", "seid", "ihr", ",", "Kai\u00b7se\u00b7rad\u00b7ler", ",", "was", "hat", "euch", "fort\u00b7ge\u00b7schreckt", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "NN", "$,", "PWS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+----+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Nur einer blieb, \u2013 der oben am Stephansthurme heckt;", "tokens": ["Nur", "ei\u00b7ner", "blieb", ",", "\u2013", "der", "o\u00b7ben", "am", "Ste\u00b7phanst\u00b7hur\u00b7me", "heckt", ";"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "$,", "$(", "ART", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Auch dieser w\u00e4r' entflogen, wenn nicht sein Leib von Stein.", "tokens": ["Auch", "die\u00b7ser", "w\u00e4r'", "ent\u00b7flo\u00b7gen", ",", "wenn", "nicht", "sein", "Leib", "von", "Stein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VAFIN", "VVPP", "$,", "KOUS", "PTKNEG", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ha, oder ahnt er Fr\u00fchroth nach n\u00e4chtlichem Wetterschein?", "tokens": ["Ha", ",", "o\u00b7der", "ahnt", "er", "Fr\u00fch\u00b7roth", "nach", "n\u00e4cht\u00b7li\u00b7chem", "Wet\u00b7ter\u00b7schein", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KON", "VVFIN", "PPER", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+--+--+-+", "measure": "iambic.hexa.invert"}}, "stanza.4": {"line.1": {"text": "Horch, Trommeln und Trompeten! Wie Maxens Faust sich ballt!", "tokens": ["Horch", ",", "Trom\u00b7meln", "und", "Trom\u00b7pe\u00b7ten", "!", "Wie", "Ma\u00b7xens", "Faust", "sich", "ballt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "KON", "NN", "$.", "PWAV", "NE", "NN", "PRF", "VVFIN", "$."], "meter": "-+--++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00bbhei, drauf und dran, ihr Br\u00fcder!\u00ab Wie's kracht und ras't und knallt!", "tokens": ["\u00bb", "hei", ",", "drauf", "und", "dran", ",", "ihr", "Br\u00fc\u00b7der", "!", "\u00ab", "Wie's", "kracht", "und", "ras't", "und", "knallt", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "PAV", "KON", "PAV", "$,", "PPOSAT", "NN", "$.", "$(", "NE", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Dicht an die Burg schl\u00e4gt Feldruf und mordender Kugeln Macht;", "tokens": ["Dicht", "an", "die", "Burg", "schl\u00e4gt", "Feld\u00b7ruf", "und", "mor\u00b7den\u00b7der", "Ku\u00b7geln", "Macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "NN", "KON", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wenn drin ein Kaiser schliefe, jetzt w\u00e4r' er wohl erwacht.", "tokens": ["Wenn", "drin", "ein", "Kai\u00b7ser", "schlie\u00b7fe", ",", "jetzt", "w\u00e4r'", "er", "wohl", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Auf Leitern klimmen aufw\u00e4rts der Krieger k\u00fchnste Reihn.", "tokens": ["Auf", "Lei\u00b7tern", "klim\u00b7men", "auf\u00b7w\u00e4rts", "der", "Krie\u00b7ger", "k\u00fchns\u00b7te", "Reihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ei, meint ihr einzusteigen zu Liebchens Fensterlein?", "tokens": ["Ei", ",", "meint", "ihr", "ein\u00b7zu\u00b7stei\u00b7gen", "zu", "Lieb\u00b7chens", "Fens\u00b7ter\u00b7lein", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "VVFIN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Schon harrt das Sch\u00e4tzchen und windet aus Rosen purpurroth", "tokens": ["Schon", "harrt", "das", "Sch\u00e4tz\u00b7chen", "und", "win\u00b7det", "aus", "Ro\u00b7sen", "pur\u00b7pur\u00b7roth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "APPR", "NE", "NE"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Um euer Haupt ein Kr\u00e4nzlein; \u2013 wie l\u00e4\u00dft so sch\u00f6n das Roth!", "tokens": ["Um", "eu\u00b7er", "Haupt", "ein", "Kr\u00e4nz\u00b7lein", ";", "\u2013", "wie", "l\u00e4\u00dft", "so", "sch\u00f6n", "das", "Roth", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "ART", "NN", "$.", "$(", "PWAV", "VVFIN", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Es k\u00e4mpft an Maxens Seite ein Rittersmann, der spricht:", "tokens": ["Es", "k\u00e4mpft", "an", "Ma\u00b7xens", "Sei\u00b7te", "ein", "Rit\u00b7ters\u00b7mann", ",", "der", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "NN", "ART", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00bbmein F\u00fcrst, ihr werdet pl\u00f6tzlich so bleich im Angesicht.\u00ab", "tokens": ["\u00bb", "mein", "F\u00fcrst", ",", "ihr", "wer\u00b7det", "pl\u00f6tz\u00b7lich", "so", "bleich", "im", "An\u00b7ge\u00b7sicht", ".", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "ADJD", "ADV", "ADJD", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "\u00bbla\u00df, Freund, und werd' ich bla\u00df auch, wie k\u00f6nnt' es anders sein?", "tokens": ["\u00bb", "la\u00df", ",", "Freund", ",", "und", "werd'", "ich", "bla\u00df", "auch", ",", "wie", "k\u00f6nnt'", "es", "an\u00b7ders", "sein", "?"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "$,", "NN", "$,", "KON", "VAFIN", "PPER", "ADJD", "ADV", "$,", "PWAV", "VMFIN", "PPER", "ADV", "VAINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Von Schild und blanken Waffen ist's nur der Widerschein.", "tokens": ["Von", "Schild", "und", "blan\u00b7ken", "Waf\u00b7fen", "ist's", "nur", "der", "Wi\u00b7der\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Sturm! drauf und dran, ihr Br\u00fcder!\u00ab \u2013 Staub h\u00fcllt die Mauern ein,", "tokens": ["Sturm", "!", "drauf", "und", "dran", ",", "ihr", "Br\u00fc\u00b7der", "!", "\u00ab", "\u2013", "Staub", "h\u00fcllt", "die", "Mau\u00b7ern", "ein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PTKVZ", "KON", "PAV", "$,", "PPOSAT", "NN", "$.", "$(", "$(", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Von Schwertern und Feuerschl\u00fcnden blitzt rother Flammenschein;", "tokens": ["Von", "Schwer\u00b7tern", "und", "Feu\u00b7er\u00b7schl\u00fcn\u00b7den", "blitzt", "ro\u00b7ther", "Flam\u00b7men\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+--+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Heim treibt ein Hirt in der Ferne die Heerde rascher fort:", "tokens": ["Heim", "treibt", "ein", "Hirt", "in", "der", "Fer\u00b7ne", "die", "Heer\u00b7de", "ra\u00b7scher", "fort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Von Wien her r\u00fcckt ein Gewitter, schon wetterleuchtet's dort.", "tokens": ["Von", "Wi\u00b7en", "her", "r\u00fcckt", "ein", "Ge\u00b7wit\u00b7ter", ",", "schon", "wet\u00b7ter\u00b7leuch\u00b7tet's", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APZR", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.8": {"line.1": {"text": "Der Ritter an Maxens Seite, der sieht ihn an und spricht:", "tokens": ["Der", "Rit\u00b7ter", "an", "Ma\u00b7xens", "Sei\u00b7te", ",", "der", "sieht", "ihn", "an", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "NN", "$,", "PRELS", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00bbihr seid so roth an den Schultern, mein F\u00fcrst, ist Blut die\u00df nicht?\u00ab", "tokens": ["\u00bb", "ihr", "seid", "so", "roth", "an", "den", "Schul\u00b7tern", ",", "mein", "F\u00fcrst", ",", "ist", "Blut", "die\u00df", "nicht", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "ADJD", "APPR", "ART", "NN", "$,", "PPOSAT", "NN", "$,", "VAFIN", "NN", "PDS", "PTKNEG", "$.", "$("], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "\u00bbei guter Freund, la\u00df roth sein; dich tr\u00fcgt der Augen Schein,", "tokens": ["\u00bb", "ei", "gu\u00b7ter", "Freund", ",", "la\u00df", "roth", "sein", ";", "dich", "tr\u00fcgt", "der", "Au\u00b7gen", "Schein", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "ADJA", "NN", "$,", "VVIMP", "ADJD", "VAINF", "$.", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Es wird wohl nur ein Lappen vom Purpurmantel sein.", "tokens": ["Es", "wird", "wohl", "nur", "ein", "Lap\u00b7pen", "vom", "Pur\u00b7pur\u00b7man\u00b7tel", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "APPRART", "NN", "VAINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Ha bravo, Br\u00fcder, vorw\u00e4rts!\u00ab \u2013 Wie von den bebenden Mauern,", "tokens": ["Ha", "bra\u00b7vo", ",", "Br\u00fc\u00b7der", ",", "vor\u00b7w\u00e4rts", "!", "\u00ab", "\u2013", "Wie", "von", "den", "be\u00b7ben\u00b7den", "Mau\u00b7ern", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$,", "NN", "$,", "ADV", "$.", "$(", "$(", "PWAV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Gleich Bl\u00fcthenflocken im Lenze, die Kugeln niederschauern!", "tokens": ["Gleich", "Bl\u00fc\u00b7then\u00b7flo\u00b7cken", "im", "Len\u00b7ze", ",", "die", "Ku\u00b7geln", "nie\u00b7der\u00b7schau\u00b7ern", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPRART", "NN", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Allm\u00e4cht'ger Gott, laut krachend sinkt dort das Bollwerk ein,", "tokens": ["All\u00b7m\u00e4cht'\u00b7ger", "Gott", ",", "laut", "kra\u00b7chend", "sinkt", "dort", "das", "Boll\u00b7werk", "ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "ADJD", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und niederpoltert donnernd das rauchende Gestein!", "tokens": ["Und", "nie\u00b7der\u00b7pol\u00b7tert", "don\u00b7nernd", "das", "rau\u00b7chen\u00b7de", "Ge\u00b7stein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+---+", "measure": "iambic.penta.relaxed"}}, "stanza.10": {"line.1": {"text": "\u00bbhinan! hinan!\u00ab \u2013 Sie st\u00fcrmen durch Schuttger\u00f6ll' empor,", "tokens": ["\u00bb", "hi\u00b7nan", "!", "hi\u00b7nan", "!", "\u00ab", "\u2013", "Sie", "st\u00fcr\u00b7men", "durch", "Schutt\u00b7ge\u00b7r\u00f6ll'", "em\u00b7por", ","], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$.", "PTKVZ", "$.", "$(", "$(", "PPER", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-----+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ha, lustig wirbeln die Trommeln, laut jauchzt der Siegeschor!", "tokens": ["Ha", ",", "lus\u00b7tig", "wir\u00b7beln", "die", "Trom\u00b7meln", ",", "laut", "jauchzt", "der", "Sie\u00b7ge\u00b7schor", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJD", "VVFIN", "ART", "NN", "$,", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Den Todten Friede! \u2013 Jetzt st\u00fcrzen vom Walle Ungarns Fahnen,", "tokens": ["Den", "Tod\u00b7ten", "Frie\u00b7de", "!", "\u2013", "Jetzt", "st\u00fcr\u00b7zen", "vom", "Wal\u00b7le", "Un\u00b7garns", "Fah\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$.", "$(", "ADV", "VVFIN", "APPRART", "NN", "NN", "NN", "$,"], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und Habsburgs erstes Banner gr\u00fc\u00dft von der Burg der Ahnen.", "tokens": ["Und", "Habs\u00b7burgs", "ers\u00b7tes", "Ban\u00b7ner", "gr\u00fc\u00dft", "von", "der", "Burg", "der", "Ah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.11": {"line.1": {"text": "Als eingest\u00fcrmt die Sieger, sehn sie in weiten Hallen", "tokens": ["Als", "ein\u00b7ge\u00b7st\u00fcrmt", "die", "Sie\u00b7ger", ",", "sehn", "sie", "in", "wei\u00b7ten", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "ART", "NN", "$,", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die Leichen magyar'scher Krieger, wie H\u00fcgel an H\u00fcgel sich ballen,", "tokens": ["Die", "Lei\u00b7chen", "magya\u00b7r'\u00b7scher", "Krie\u00b7ger", ",", "wie", "H\u00fc\u00b7gel", "an", "H\u00fc\u00b7gel", "sich", "bal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "PWAV", "NN", "APPR", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-++-+--+--+--+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Die Lebenden stehn daneben, den S\u00e4bel im Arm gez\u00fcckt,", "tokens": ["Die", "Le\u00b7ben\u00b7den", "stehn", "da\u00b7ne\u00b7ben", ",", "den", "S\u00e4\u00b7bel", "im", "Arm", "ge\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "$,", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ein Seraphchor, der sch\u00fctzend auf theure Gr\u00e4ber blickt.", "tokens": ["Ein", "Se\u00b7raph\u00b7chor", ",", "der", "sch\u00fct\u00b7zend", "auf", "theu\u00b7re", "Gr\u00e4\u00b7ber", "blickt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.12": {"line.1": {"text": "Max trat zu ihrem F\u00fchrer und dr\u00fcckt ihm sanft die Hand:", "tokens": ["Max", "trat", "zu", "ih\u00b7rem", "F\u00fch\u00b7rer", "und", "dr\u00fcckt", "ihm", "sanft", "die", "Hand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00bbzieht hin, ihr edlen Streiter, in Frieden in euer Land,", "tokens": ["\u00bb", "zieht", "hin", ",", "ihr", "ed\u00b7len", "Strei\u00b7ter", ",", "in", "Frie\u00b7den", "in", "eu\u00b7er", "Land", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$,", "APPR", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "---+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wenn Feinde gleich, doch ehr' ich solch kr\u00e4ftiges Geschlecht.", "tokens": ["Wenn", "Fein\u00b7de", "gleich", ",", "doch", "ehr'", "ich", "solch", "kr\u00e4f\u00b7ti\u00b7ges", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "$,", "ADV", "VVFIN", "PPER", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "O k\u00e4mpften einst vereint wir f\u00fcr ", "tokens": ["O", "k\u00e4mpf\u00b7ten", "einst", "ver\u00b7eint", "wir", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "VVFIN", "PPER", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Er sprach's; da fa\u00dft ihn Fieber, Blut aus der Wunde bricht,", "tokens": ["Er", "sprach's", ";", "da", "fa\u00dft", "ihn", "Fie\u00b7ber", ",", "Blut", "aus", "der", "Wun\u00b7de", "bricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "NN", "$,", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Er sinkt in Freundesarme mit bleichem Angesicht;", "tokens": ["Er", "sinkt", "in", "Freun\u00b7des\u00b7ar\u00b7me", "mit", "blei\u00b7chem", "An\u00b7ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Auf einer Bahre trugen sie ihn ins stille Gemach,", "tokens": ["Auf", "ei\u00b7ner", "Bah\u00b7re", "tru\u00b7gen", "sie", "ihn", "ins", "stil\u00b7le", "Ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Doch Preis dem Herrn! bald ward er aus schwerem Schlummer wach.", "tokens": ["Doch", "Preis", "dem", "Herrn", "!", "bald", "ward", "er", "aus", "schwe\u00b7rem", "Schlum\u00b7mer", "wach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "$.", "ADV", "VAFIN", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.14": {"line.1": {"text": "Bald stand an seinem Lager Genesung, das sch\u00f6ne Weib,", "tokens": ["Bald", "stand", "an", "sei\u00b7nem", "La\u00b7ger", "Ge\u00b7ne\u00b7sung", ",", "das", "sch\u00f6\u00b7ne", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "K\u00fc\u00dft ihn auf Aug' und Wange und feit ihm den wunden Leib.", "tokens": ["K\u00fc\u00dft", "ihn", "auf", "Aug'", "und", "Wan\u00b7ge", "und", "feit", "ihm", "den", "wun\u00b7den", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "KON", "NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+-+--+--+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Da klang einst eine Zither herauf beim Abendschein,", "tokens": ["Da", "klang", "einst", "ei\u00b7ne", "Zi\u00b7ther", "her\u00b7auf", "beim", "A\u00b7bend\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und duft'ge Weste trugen ", "tokens": ["Und", "duft'\u00b7ge", "Wes\u00b7te", "tru\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "\u00bbvor manchem Pfeile schirmet das Weib des Geliebten Herz,", "tokens": ["\u00bb", "vor", "man\u00b7chem", "Pfei\u00b7le", "schir\u00b7met", "das", "Weib", "des", "Ge\u00b7lieb\u00b7ten", "Herz", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Erst wenn es ausgest\u00fcrmet, weint sie dem eignen Schmerz;", "tokens": ["Erst", "wenn", "es", "aus\u00b7ge\u00b7st\u00fcr\u00b7met", ",", "weint", "sie", "dem", "eig\u00b7nen", "Schmerz", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "So winkt zu Siegesbahnen dem Heer des Helden Hand,", "tokens": ["So", "winkt", "zu", "Sie\u00b7ges\u00b7bah\u00b7nen", "dem", "Heer", "des", "Hel\u00b7den", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Erst die ersiegten Fahnen sind seiner Wunden Verband.", "tokens": ["Erst", "die", "er\u00b7sieg\u00b7ten", "Fah\u00b7nen", "sind", "sei\u00b7ner", "Wun\u00b7den", "Ver\u00b7band", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.16": {"line.1": {"text": "So gleichen Beide dem Baume, der, wenn es hagelt und st\u00fcrmt", "tokens": ["So", "glei\u00b7chen", "Bei\u00b7de", "dem", "Bau\u00b7me", ",", "der", ",", "wenn", "es", "ha\u00b7gelt", "und", "st\u00fcrmt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "ART", "NN", "$,", "PRELS", "$,", "KOUS", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+--+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "In seinem schatt'gen Raume den bangen Wandrer schirmt;", "tokens": ["In", "sei\u00b7nem", "schatt'\u00b7gen", "Rau\u00b7me", "den", "ban\u00b7gen", "Wand\u00b7rer", "schirmt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Erst wenn die St\u00fcrme schweigen, die L\u00fcfte wieder blau,", "tokens": ["Erst", "wenn", "die", "St\u00fcr\u00b7me", "schwei\u00b7gen", ",", "die", "L\u00fcf\u00b7te", "wie\u00b7der", "blau", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "VVINF", "$,", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Dann sch\u00fcttelt er von den Zweigen den eignen Thr\u00e4nenthau.\u00ab", "tokens": ["Dann", "sch\u00fct\u00b7telt", "er", "von", "den", "Zwei\u00b7gen", "den", "eig\u00b7nen", "Thr\u00e4\u00b7nent\u00b7hau", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}