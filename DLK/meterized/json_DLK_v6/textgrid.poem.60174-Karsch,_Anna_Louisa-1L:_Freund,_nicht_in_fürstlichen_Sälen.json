{"textgrid.poem.60174": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Freund, nicht in f\u00fcrstlichen S\u00e4len", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund, nicht in f\u00fcrstlichen S\u00e4len", "tokens": ["Freund", ",", "nicht", "in", "f\u00fcrst\u00b7li\u00b7chen", "S\u00e4\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Bey dem glatsteinigten Tisch,", "tokens": ["Bey", "dem", "glats\u00b7tei\u00b7nig\u00b7ten", "Tisch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bedeckt mit k\u00f6stlicher Leinwand,", "tokens": ["Be\u00b7deckt", "mit", "k\u00f6st\u00b7li\u00b7cher", "Lein\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohnt das Vergn\u00fcgen allein!", "tokens": ["Wohnt", "das", "Ver\u00b7gn\u00fc\u00b7gen", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Auch im kleinr\u00e4umichten Hause,", "tokens": ["Auch", "im", "klein\u00b7r\u00e4u\u00b7mich\u00b7ten", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gebaut nach l\u00e4ndlicher Art,", "tokens": ["Ge\u00b7baut", "nach", "l\u00e4nd\u00b7li\u00b7cher", "Art", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Auf schlechtem reinlichem Zwillich,", "tokens": ["Auf", "schlech\u00b7tem", "rein\u00b7li\u00b7chem", "Zwil\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mit Einer Sch\u00fcssel besetzt,", "tokens": ["Mit", "Ei\u00b7ner", "Sch\u00fcs\u00b7sel", "be\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Schmeckt dem nicht w\u00e4hlenden Gaumen,", "tokens": ["Schmeckt", "dem", "nicht", "w\u00e4h\u00b7len\u00b7den", "Gau\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PTKNEG", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Die ungek\u00fcnstelte Kost:", "tokens": ["Die", "un\u00b7ge\u00b7k\u00fcns\u00b7tel\u00b7te", "Kost", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und vom Luftsch\u00f6pfen getrocknet,", "tokens": ["Und", "vom", "Luft\u00b7sch\u00f6p\u00b7fen", "ge\u00b7trock\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVPP", "$,"], "meter": "--+---+-", "measure": "anapaest.init"}, "line.4": {"text": "Schmeckt ihm vierj\u00e4hriger Wein.", "tokens": ["Schmeckt", "ihm", "vier\u00b7j\u00e4h\u00b7ri\u00b7ger", "Wein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "---+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Komm! deine liebende Freundin", "tokens": ["Komm", "!", "dei\u00b7ne", "lie\u00b7ben\u00b7de", "Freun\u00b7din"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Winkt mit gef\u00e4lligem Blick,", "tokens": ["Winkt", "mit", "ge\u00b7f\u00e4l\u00b7li\u00b7gem", "Blick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Dich zum bescheidenen Gastmahl!", "tokens": ["Dich", "zum", "be\u00b7schei\u00b7de\u00b7nen", "Gast\u00b7mahl", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dein warten Fische, die noch", "tokens": ["Dein", "war\u00b7ten", "Fi\u00b7sche", ",", "die", "noch"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Froh der mitt\u00e4glichen Sonne", "tokens": ["Froh", "der", "mit\u00b7t\u00e4g\u00b7li\u00b7chen", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Im Strom entgegen gescherzt,", "tokens": ["Im", "Strom", "ent\u00b7ge\u00b7gen", "ge\u00b7scherzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und dann vom l\u00f6chrichten Hame,", "tokens": ["Und", "dann", "vom", "l\u00f6ch\u00b7rich\u00b7ten", "Ha\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Des lauschenden Fischers ber\u00fcckt", "tokens": ["Des", "lau\u00b7schen\u00b7den", "Fi\u00b7schers", "be\u00b7r\u00fcckt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+---+-+", "measure": "dactylic.init"}}, "stanza.6": {"line.1": {"text": "Herauf gezogen, vergebens", "tokens": ["Her\u00b7auf", "ge\u00b7zo\u00b7gen", ",", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADV", "VVPP", "$,", "ADV"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dem Tod entgegen gestr\u00e4ubt!", "tokens": ["Dem", "Tod", "ent\u00b7ge\u00b7gen", "ge\u00b7str\u00e4ubt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sie starben unter dem Messer", "tokens": ["Sie", "star\u00b7ben", "un\u00b7ter", "dem", "Mes\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der hurtigen K\u00f6chin dahin.", "tokens": ["Der", "hur\u00b7ti\u00b7gen", "K\u00f6\u00b7chin", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PAV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Sie sind uns niedliche Bissen!", "tokens": ["Sie", "sind", "uns", "nied\u00b7li\u00b7che", "Bis\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ihr Salz erwecket den Durst,", "tokens": ["Ihr", "Salz", "er\u00b7we\u00b7cket", "den", "Durst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wir leeren alle die Gl\u00e4ser,", "tokens": ["Wir", "lee\u00b7ren", "al\u00b7le", "die", "Gl\u00e4\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und sagen W\u00fcnsche vorher,", "tokens": ["Und", "sa\u00b7gen", "W\u00fcn\u00b7sche", "vor\u00b7her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Mit patriotischer Inbrunst.", "tokens": ["Mit", "pat\u00b7ri\u00b7o\u00b7ti\u00b7scher", "In\u00b7brunst", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So rief der S\u00e4nger Horaz,", "tokens": ["So", "rief", "der", "S\u00e4n\u00b7ger", "Ho\u00b7raz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nie das Verlangen der R\u00f6mer,", "tokens": ["Nie", "das", "Ver\u00b7lan\u00b7gen", "der", "R\u00f6\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Den weit entfernten August;", "tokens": ["Den", "weit", "ent\u00b7fern\u00b7ten", "Au\u00b7gust", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Als wir den kriegenden K\u00f6nig.", "tokens": ["Als", "wir", "den", "krie\u00b7gen\u00b7den", "K\u00f6\u00b7nig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ihn tr\u00e4gt das muthige Ro\u00df;", "tokens": ["Ihn", "tr\u00e4gt", "das", "mut\u00b7hi\u00b7ge", "Ro\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der Staub bedecket die Stirne,", "tokens": ["Der", "Staub", "be\u00b7de\u00b7cket", "die", "Stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die zweene Cronen verdient.", "tokens": ["Die", "zwee\u00b7ne", "Cro\u00b7nen", "ver\u00b7dient", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.10": {"line.1": {"text": "Sie spricht Befehle der Feldschlacht.", "tokens": ["Sie", "spricht", "Be\u00b7feh\u00b7le", "der", "Feld\u00b7schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Er schl\u00e4gt mit r\u00e4chendem Schwerd,", "tokens": ["Er", "schl\u00e4gt", "mit", "r\u00e4\u00b7chen\u00b7dem", "Schwerd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Siegt dreymahl, ehe die Linde", "tokens": ["Siegt", "drey\u00b7mahl", ",", "e\u00b7he", "die", "Lin\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "ART", "NE"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Die kranken Bl\u00e4tter verliert!", "tokens": ["Die", "kran\u00b7ken", "Bl\u00e4t\u00b7ter", "ver\u00b7liert", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.11": {"line.1": {"text": "Um Frieden bitten die Feinde,", "tokens": ["Um", "Frie\u00b7den", "bit\u00b7ten", "die", "Fein\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und aus halb g\u00f6ttlicher Hand", "tokens": ["Und", "aus", "halb", "g\u00f6tt\u00b7li\u00b7cher", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Giebt er die grosse Vers\u00f6hnung,", "tokens": ["Giebt", "er", "die", "gros\u00b7se", "Ver\u00b7s\u00f6h\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Und baut, was Feinde zerst\u00f6hrt.", "tokens": ["Und", "baut", ",", "was", "Fein\u00b7de", "zer\u00b7st\u00f6hrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}