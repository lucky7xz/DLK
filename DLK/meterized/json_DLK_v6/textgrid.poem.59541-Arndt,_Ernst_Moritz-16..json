{"textgrid.poem.59541": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Nacht ist nun vergangen,", "tokens": ["Die", "Nacht", "ist", "nun", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Morgen steht so herrlich da,", "tokens": ["Der", "Mor\u00b7gen", "steht", "so", "herr\u00b7lich", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und alle Blumen prangen", "tokens": ["Und", "al\u00b7le", "Blu\u00b7men", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und alle B\u00e4ume fern und nach;", "tokens": ["Und", "al\u00b7le", "B\u00e4u\u00b7me", "fern", "und", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJD", "KON", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf Feldern und auf Wiesen,", "tokens": ["Auf", "Fel\u00b7dern", "und", "auf", "Wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In Wald und Berg und Tal", "tokens": ["In", "Wald", "und", "Berg", "und", "Tal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wird Gottes Lust gepriesen", "tokens": ["Wird", "Got\u00b7tes", "Lust", "ge\u00b7prie\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "NN", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Von Stimmen ohne Zahl.", "tokens": ["Von", "Stim\u00b7men", "oh\u00b7ne", "Zahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die frommen Nachtigallen,", "tokens": ["Die", "from\u00b7men", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie klingen hellen Freudenklang,", "tokens": ["Sie", "klin\u00b7gen", "hel\u00b7len", "Freu\u00b7den\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Lerchen, h\u00f6chst vor allen,", "tokens": ["Die", "Ler\u00b7chen", ",", "h\u00f6chst", "vor", "al\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "APPR", "PIAT", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum Himmel tragen sie Gesang,", "tokens": ["Zum", "Him\u00b7mel", "tra\u00b7gen", "sie", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Kuckuck auf den Zweigen", "tokens": ["Der", "Ku\u00b7ckuck", "auf", "den", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und auch das Zeisiglein,", "tokens": ["Und", "auch", "das", "Zei\u00b7si\u00b7glein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sie wollen sich dankbar zeigen,", "tokens": ["Sie", "wol\u00b7len", "sich", "dank\u00b7bar", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "'s will keiner hinten sein.", "tokens": ["'s", "will", "kei\u00b7ner", "hin\u00b7ten", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "ADV", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und ich? ich sollte schweigen,", "tokens": ["Und", "ich", "?", "ich", "soll\u00b7te", "schwei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$.", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich, Gottes reiches Ebenbild?", "tokens": ["Ich", ",", "Got\u00b7tes", "rei\u00b7ches", "E\u00b7ben\u00b7bild", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch das mit Liebesneigen", "tokens": ["Durch", "das", "mit", "Lie\u00b7bes\u00b7nei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "APPR", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Der Feuerstrom der Gottheit quillt,", "tokens": ["Der", "Feu\u00b7er\u00b7strom", "der", "Got\u00b7theit", "quillt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dem er die Sternenlichter", "tokens": ["Dem", "er", "die", "Ster\u00b7nen\u00b7lich\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zur Br\u00fcderschar geweiht", "tokens": ["Zur", "Br\u00fc\u00b7der\u00b7schar", "ge\u00b7weiht"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und Engelangesichter", "tokens": ["Und", "En\u00b7ge\u00b7lan\u00b7ge\u00b7sich\u00b7ter"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Verkl\u00e4rt in Herrlichkeit?", "tokens": ["Ver\u00b7kl\u00e4rt", "in", "Herr\u00b7lich\u00b7keit", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das Wild im gr\u00fcnen Walde,", "tokens": ["Das", "Wild", "im", "gr\u00fc\u00b7nen", "Wal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Vogel auf dem gr\u00fcnen Baum,", "tokens": ["Der", "Vo\u00b7gel", "auf", "dem", "gr\u00fc\u00b7nen", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie priesen alsobalde", "tokens": ["Sie", "prie\u00b7sen", "al\u00b7so\u00b7bal\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Vater \u00fcberm Sternenraum?", "tokens": ["Den", "Va\u00b7ter", "\u00fc\u00b7berm", "Ster\u00b7nen\u00b7raum", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es sumsete die Imme,", "tokens": ["Es", "sum\u00b7se\u00b7te", "die", "Im\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das W\u00fcrmchen seine Lust,", "tokens": ["Das", "W\u00fcrm\u00b7chen", "sei\u00b7ne", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und ich h\u00e4tt' keine Stimme", "tokens": ["Und", "ich", "h\u00e4tt'", "kei\u00b7ne", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des Lobes in der Brust?", "tokens": ["Des", "Lo\u00b7bes", "in", "der", "Brust", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nein, Vater aller G\u00fcte,", "tokens": ["Nein", ",", "Va\u00b7ter", "al\u00b7ler", "G\u00fc\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du meiner Seele Freudenlicht,", "tokens": ["Du", "mei\u00b7ner", "See\u00b7le", "Freu\u00b7den\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie gern will mein Gem\u00fcte!", "tokens": ["Wie", "gern", "will", "mein", "Ge\u00b7m\u00fc\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch meine Worte k\u00f6nnen nicht.", "tokens": ["Doch", "mei\u00b7ne", "Wor\u00b7te", "k\u00f6n\u00b7nen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer mag dich w\u00fcrdig preisen,", "tokens": ["Wer", "mag", "dich", "w\u00fcr\u00b7dig", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Durch den die Welten sind,", "tokens": ["Durch", "den", "die", "Wel\u00b7ten", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VAFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Vor dem die tiefsten Weisen", "tokens": ["Vor", "dem", "die", "tiefs\u00b7ten", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Kaum lallen wie ein Kind!", "tokens": ["Kaum", "lal\u00b7len", "wie", "ein", "Kind", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O Herr, la\u00df mich auch heute", "tokens": ["O", "Herr", ",", "la\u00df", "mich", "auch", "heu\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VVIMP", "PPER", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In deiner Liebe wandeln treu,", "tokens": ["In", "dei\u00b7ner", "Lie\u00b7be", "wan\u00b7deln", "treu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich der S\u00fcnden Beute,", "tokens": ["Da\u00df", "ich", "der", "S\u00fcn\u00b7den", "Beu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Eitelkeiten Spiel nicht sei,", "tokens": ["Der", "Ei\u00b7tel\u00b7kei\u00b7ten", "Spiel", "nicht", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKNEG", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "La\u00df mich nach deinem Bilde", "tokens": ["La\u00df", "mich", "nach", "dei\u00b7nem", "Bil\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Weg der Tugend gehn,", "tokens": ["Den", "Weg", "der", "Tu\u00b7gend", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So wird der Tag mir milde,", "tokens": ["So", "wird", "der", "Tag", "mir", "mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So kommt die Nacht mir sch\u00f6n.", "tokens": ["So", "kommt", "die", "Nacht", "mir", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}