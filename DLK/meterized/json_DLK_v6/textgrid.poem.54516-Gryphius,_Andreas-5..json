{"textgrid.poem.54516": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So gehst du nunmehr hin/ und tr\u00e4gst den Fluch mit dir!", "tokens": ["So", "gehst", "du", "nun\u00b7mehr", "hin", "/", "und", "tr\u00e4gst", "den", "Fluch", "mit", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$(", "KON", "VVFIN", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den K\u00f6nig/ dir die Welt zum letzten Abscheid giebet/", "tokens": ["Den", "K\u00f6\u00b7nig", "/", "dir", "die", "Welt", "zum", "letz\u00b7ten", "Ab\u00b7scheid", "gie\u00b7bet", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PPER", "ART", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein Land/ das du vielmehr noch als dich selbst geliebet/", "tokens": ["Dein", "Land", "/", "das", "du", "viel\u00b7mehr", "noch", "als", "dich", "selbst", "ge\u00b7lie\u00b7bet", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PRELS", "PPER", "ADV", "ADV", "KOUS", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das dir das Creutz aufflegt: Ach was verzieh ich hier/", "tokens": ["Das", "dir", "das", "Creutz", "auf\u00b7flegt", ":", "Ach", "was", "ver\u00b7zieh", "ich", "hier", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "NN", "VVPP", "$.", "ITJ", "PWS", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Ich seh das Jesus selbst mich zu dem Leben f\u00fchr/", "tokens": ["Ich", "seh", "das", "Je\u00b7sus", "selbst", "mich", "zu", "dem", "Le\u00b7ben", "f\u00fchr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "PPER", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch nur durch Schmach und Tod. Warum denn so betr\u00fcbet?", "tokens": ["Doch", "nur", "durch", "Schmach", "und", "Tod", ".", "Wa\u00b7rum", "denn", "so", "be\u00b7tr\u00fc\u00b7bet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "KON", "NN", "$.", "PWAV", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er rufft: Weint nicht um mich/ Ihr die die Bu\u00df auffschiebet/", "tokens": ["Er", "rufft", ":", "Weint", "nicht", "um", "mich", "/", "Ihr", "die", "die", "Bu\u00df", "auff\u00b7schie\u00b7bet", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "VVFIN", "PTKNEG", "APPR", "PPER", "$(", "PPER", "ART", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weint \u00fcber euch: Der Grimm/ die Straffe kommt nach mir.", "tokens": ["Weint", "\u00fc\u00b7ber", "euch", ":", "Der", "Grimm", "/", "die", "Straf\u00b7fe", "kommt", "nach", "mir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$.", "ART", "NE", "$(", "ART", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Fort dann mit Jesu fort? Last uns weil Zeit/ entfliehen", "tokens": ["Fort", "dann", "mit", "Je\u00b7su", "fort", "?", "Last", "uns", "weil", "Zeit", "/", "ent\u00b7flie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "ADV", "APPR", "NE", "PTKVZ", "$.", "VVFIN", "PPER", "KOUS", "NN", "$(", "VVINF"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.10": {"text": "Ob schon man sonder Creutz aus Salem nicht kan ziehen/", "tokens": ["Ob", "schon", "man", "son\u00b7der", "Creutz", "aus", "Sa\u00b7lem", "nicht", "kan", "zie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIS", "PIAT", "NN", "APPR", "NE", "PTKNEG", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es zeigt uns dieses Holtz/ was man von hinnen tr\u00e4gt.", "tokens": ["Es", "zeigt", "uns", "die\u00b7ses", "Holtz", "/", "was", "man", "von", "hin\u00b7nen", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PDAT", "NN", "$(", "PWS", "PIS", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Besitze was du wilt/ du wirst noch alles lassen/", "tokens": ["Be\u00b7sit\u00b7ze", "was", "du", "wilt", "/", "du", "wirst", "noch", "al\u00b7les", "las\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PWS", "PPER", "VMFIN", "$(", "PPER", "VAFIN", "ADV", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ein St\u00fcck ists von dem Creutz/ was dich nur wird umfassen/", "tokens": ["Ein", "St\u00fcck", "ists", "von", "dem", "Creutz", "/", "was", "dich", "nur", "wird", "um\u00b7fas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "$(", "PWS", "PPER", "ADV", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Drey Bretter sind/ darmit man dich ins Grab einlegt.", "tokens": ["Drey", "Bret\u00b7ter", "sind", "/", "dar\u00b7mit", "man", "dich", "ins", "Grab", "ein\u00b7legt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "$(", "KOUS", "PIS", "PRF", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}