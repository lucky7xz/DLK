{"textgrid.poem.49953": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Zum Neujahr", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An tausend W\u00fcnsche, federleicht,", "tokens": ["An", "tau\u00b7send", "W\u00fcn\u00b7sche", ",", "fe\u00b7der\u00b7leicht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird sich kein Gott noch Engel kehren,", "tokens": ["Wird", "sich", "kein", "Gott", "noch", "En\u00b7gel", "keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "PIAT", "NN", "ADV", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ja, wenn es so viel Fl\u00fcche w\u00e4ren,", "tokens": ["Ja", ",", "wenn", "es", "so", "viel", "Fl\u00fc\u00b7che", "w\u00e4\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "ADV", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Teufel w\u00e4ren sie zu seicht.", "tokens": ["Dem", "Teu\u00b7fel", "w\u00e4\u00b7ren", "sie", "zu", "seicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch wenn ein Freund in Lieb und Treu", "tokens": ["Doch", "wenn", "ein", "Freund", "in", "Lieb", "und", "Treu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem andern den Kalender segnet,", "tokens": ["Dem", "an\u00b7dern", "den", "Ka\u00b7len\u00b7der", "seg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "So steht ein guter Geist dabei.", "tokens": ["So", "steht", "ein", "gu\u00b7ter", "Geist", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Du denkst an mich, was Liebes dir begegnet,", "tokens": ["Du", "denkst", "an", "mich", ",", "was", "Lie\u00b7bes", "dir", "be\u00b7geg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "PWS", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ob dir's auch ohne das beschieden sei.", "tokens": ["Ob", "dir's", "auch", "oh\u00b7ne", "das", "be\u00b7schie\u00b7den", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "APPR", "ART", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}