{"textgrid.poem.64879": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Mutter sah ich bleich im Sarge liegen,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Mutter sah ich bleich im Sarge liegen,", "tokens": ["Die", "Mut\u00b7ter", "sah", "ich", "bleich", "im", "Sar\u00b7ge", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die frommen H\u00e4nde zum Gebet gefaltet;", "tokens": ["Die", "from\u00b7men", "H\u00e4n\u00b7de", "zum", "Ge\u00b7bet", "ge\u00b7fal\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In des Geliebten todtenblassen Z\u00fcgen", "tokens": ["In", "des", "Ge\u00b7lieb\u00b7ten", "tod\u00b7ten\u00b7blas\u00b7sen", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hat sich ein dunkles Reich f\u00fcr mich entfaltet.", "tokens": ["Hat", "sich", "ein", "dunk\u00b7les", "Reich", "f\u00fcr", "mich", "ent\u00b7fal\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ART", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch, trotz der strenge wiederholten Mahnung,", "tokens": ["Doch", ",", "trotz", "der", "stren\u00b7ge", "wie\u00b7der\u00b7hol\u00b7ten", "Mah\u00b7nung", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Blieb mir doch fremd des Tod's geheimste Ahnung,", "tokens": ["Blieb", "mir", "doch", "fremd", "des", "To\u00b7d's", "ge\u00b7heims\u00b7te", "Ah\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Und was ich sah, schien mir ein Sterben nur!", "tokens": ["Und", "was", "ich", "sah", ",", "schien", "mir", "ein", "Ster\u00b7ben", "nur", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Denn, als sie schieden aus dem Weltgetriebe,", "tokens": ["Denn", ",", "als", "sie", "schie\u00b7den", "aus", "dem", "Welt\u00b7ge\u00b7trie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Sprach noch ihr letzter Blick von ew'ger Liebe;", "tokens": ["Sprach", "noch", "ihr", "letz\u00b7ter", "Blick", "von", "ew'\u00b7ger", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Ihr letzter Seufzer war ein Liebesschwur.", "tokens": ["Ihr", "letz\u00b7ter", "Seuf\u00b7zer", "war", "ein", "Lie\u00b7bes\u00b7schwur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du aber lehrtest, als du mich verlassen", "tokens": ["Du", "a\u00b7ber", "lehr\u00b7test", ",", "als", "du", "mich", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "$,", "KOUS", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In fl\u00fccht'gen Leichsinns herzlos roher Richtung,", "tokens": ["In", "fl\u00fccht'\u00b7gen", "Leich\u00b7sinns", "herz\u00b7los", "ro\u00b7her", "Rich\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mich jenen furchtbaren Gedanken fassen", "tokens": ["Mich", "je\u00b7nen", "furcht\u00b7ba\u00b7ren", "Ge\u00b7dan\u00b7ken", "fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "PDAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der rettungslosen ewigen Vernichtung.", "tokens": ["Der", "ret\u00b7tungs\u00b7lo\u00b7sen", "e\u00b7wi\u00b7gen", "Ver\u00b7nich\u00b7tung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dein Treubruch warf des Todesahnung Schatten", "tokens": ["Dein", "Treu\u00b7bruch", "warf", "des", "To\u00b7de\u00b7sah\u00b7nung", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Auf meines Lebens hoffnungshelle Matten \u2013", "tokens": ["Auf", "mei\u00b7nes", "Le\u00b7bens", "hoff\u00b7nungs\u00b7hel\u00b7le", "Mat\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und sicher, fest scheint mir nichts mehr fortan.", "tokens": ["Und", "si\u00b7cher", ",", "fest", "scheint", "mir", "nichts", "mehr", "for\u00b7tan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJD", "VVFIN", "PPER", "PIS", "ADV", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Als deiner Freundschaft Traumbild mir zerronnen,", "tokens": ["Als", "dei\u00b7ner", "Freund\u00b7schaft", "Traum\u00b7bild", "mir", "zer\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Da hat des dunkeln Schnitters Werk begonnen \u2013", "tokens": ["Da", "hat", "des", "dun\u00b7keln", "Schnit\u00b7ters", "Werk", "be\u00b7gon\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Zu seiner Ernte reif' ich schnell heran!", "tokens": ["Zu", "sei\u00b7ner", "Ern\u00b7te", "reif'", "ich", "schnell", "he\u00b7ran", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}