{"dta.poem.16280": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zu den Makrobiern ein Abgesandter kam,", "tokens": ["Zu", "den", "Mak\u00b7ro\u00b7biern", "ein", "Ab\u00b7ge\u00b7sand\u00b7ter", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Der staunend in Betracht des Landes Wunder nahm.", "tokens": ["Der", "stau\u00b7nend", "in", "Be\u00b7tracht", "des", "Lan\u00b7des", "Wun\u00b7der", "nahm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zuletzt, damit er noch erstaunen m\u00fc\u00dfte st\u00e4rker,", "tokens": ["Zu\u00b7letzt", ",", "da\u00b7mit", "er", "noch", "er\u00b7stau\u00b7nen", "m\u00fc\u00df\u00b7te", "st\u00e4r\u00b7ker", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADV", "VVINF", "VMFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lie\u00dfen sie ihren Gast besichtigen die Kerker,", "tokens": ["Lie\u00b7\u00dfen", "sie", "ih\u00b7ren", "Gast", "be\u00b7sich\u00b7ti\u00b7gen", "die", "Ker\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Wo die Gefangenen Goldketten trugen alle,", "tokens": ["Wo", "die", "Ge\u00b7fan\u00b7ge\u00b7nen", "Gold\u00b7ket\u00b7ten", "tru\u00b7gen", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN", "PIS", "$,"], "meter": "---+--+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Weil nicht das Land erzeugt unedlere Metalle.", "tokens": ["Weil", "nicht", "das", "Land", "er\u00b7zeugt", "u\u00b7ned\u00b7le\u00b7re", "Me\u00b7tal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch er sah's unerstaunt, als sei es ihm gel\u00e4ufig,", "tokens": ["Doch", "er", "sah's", "un\u00b7er\u00b7staunt", ",", "als", "sei", "es", "ihm", "ge\u00b7l\u00e4u\u00b7fig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "$,", "KOKOM", "VAFIN", "PPER", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und l\u00e4chelnd sprach er: Dis hab' ich gesehn gar h\u00e4ufig.", "tokens": ["Und", "l\u00e4\u00b7chelnd", "sprach", "er", ":", "Dis", "hab'", "ich", "ge\u00b7sehn", "gar", "h\u00e4u\u00b7fig", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$.", "PDS", "VAFIN", "PPER", "VVPP", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Denn wem ist unbekannt des Goldes starke Kraft,", "tokens": ["Denn", "wem", "ist", "un\u00b7be\u00b7kannt", "des", "Gol\u00b7des", "star\u00b7ke", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "ADJD", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die jeden \u00fcbermannt und alle legt in Haft?", "tokens": ["Die", "je\u00b7den", "\u00fc\u00b7berm\u00b7annt", "und", "al\u00b7le", "legt", "in", "Haft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJD", "KON", "PIS", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der Unterschied ist nur, da\u00df goldgefangen seien", "tokens": ["Der", "Un\u00b7ter\u00b7schied", "ist", "nur", ",", "da\u00df", "gold\u00b7ge\u00b7fan\u00b7gen", "sei\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hier die Gefangenen, bei uns daheim die Freien.", "tokens": ["Hier", "die", "Ge\u00b7fan\u00b7ge\u00b7nen", ",", "bei", "uns", "da\u00b7heim", "die", "Frei\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "APPR", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}