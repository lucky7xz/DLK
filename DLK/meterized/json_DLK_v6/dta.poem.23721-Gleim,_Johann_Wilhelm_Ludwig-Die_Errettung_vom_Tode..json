{"dta.poem.23721": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Errettung vom Tode.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und sprach ein bi\u00dfgen zornig:", "tokens": ["Und", "sprach", "ein", "bi\u00df\u00b7gen", "zor\u00b7nig", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du meinst, ich kan nicht treffen.", "tokens": ["Du", "meinst", ",", "ich", "kan", "nicht", "tref\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Lauf nur, ich will dich schiessen.", "tokens": ["Lauf", "nur", ",", "ich", "will", "dich", "schies\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PPER", "VMFIN", "PRF", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Da scho\u00df er mich ins Herze,", "tokens": ["Da", "scho\u00df", "er", "mich", "ins", "Her\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da solt ich pl\u00f6tzlich sterben,", "tokens": ["Da", "solt", "ich", "pl\u00f6tz\u00b7lich", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und nicht noch einmal k\u00fcssen;", "tokens": ["Und", "nicht", "noch", "ein\u00b7mal", "k\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Allein, ich seufzte traurig:", "tokens": ["Al\u00b7lein", ",", "ich", "seufz\u00b7te", "trau\u00b7rig", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nun kan ich, liebster Amor,", "tokens": ["Nun", "kan", "ich", ",", "liebs\u00b7ter", "A\u00b7mor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "ADJA", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Mein M\u00e4dchen nicht mehr lieben!", "tokens": ["Mein", "M\u00e4d\u00b7chen", "nicht", "mehr", "lie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Dis iammerte den Amor,", "tokens": ["Dis", "i\u00b7am\u00b7mer\u00b7te", "den", "A\u00b7mor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NE", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.11": {"text": "Drum lie\u00df er mich nicht sterben.", "tokens": ["Drum", "lie\u00df", "er", "mich", "nicht", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}