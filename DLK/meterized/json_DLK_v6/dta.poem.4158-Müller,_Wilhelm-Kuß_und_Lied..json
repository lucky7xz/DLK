{"dta.poem.4158": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Ku\u00df und Lied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Liedchen sa\u00df auf meinen Lippen,", "tokens": ["Ein", "Lied\u00b7chen", "sa\u00df", "auf", "mei\u00b7nen", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und aus dem Liedchen ward ein Ku\u00df.", "tokens": ["Und", "aus", "dem", "Lied\u00b7chen", "ward", "ein", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Jetzt ist mein M\u00e4dchen fern von mir;", "tokens": ["Jetzt", "ist", "mein", "M\u00e4d\u00b7chen", "fern", "von", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Kusse will mein Mund sich schwellen,", "tokens": ["Zum", "Kus\u00b7se", "will", "mein", "Mund", "sich", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPOSAT", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und aus dem Kusse wird ein Lied.", "tokens": ["Und", "aus", "dem", "Kus\u00b7se", "wird", "ein", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Fliegt nun, ihr lieben Verse, hin,", "tokens": ["Fliegt", "nun", ",", "ihr", "lie\u00b7ben", "Ver\u00b7se", ",", "hin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und dr\u00fcckt sie euch an ihre Lippen,", "tokens": ["Und", "dr\u00fcckt", "sie", "euch", "an", "ih\u00b7re", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So werdet wieder, was ihr war't!", "tokens": ["So", "wer\u00b7det", "wie\u00b7der", ",", "was", "ihr", "wa\u00b7r't", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}