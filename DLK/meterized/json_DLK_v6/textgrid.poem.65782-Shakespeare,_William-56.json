{"textgrid.poem.65782": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "56", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erneu', o Liebe, deine Macht! Man schilt", "tokens": ["Er\u00b7neu'", ",", "o", "Lie\u00b7be", ",", "dei\u00b7ne", "Macht", "!", "Man", "schilt"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "FM", "NN", "$,", "PPOSAT", "NN", "$.", "PIS", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dich stumpfer sonst, als Hungers Leidenschaft,", "tokens": ["Dich", "stump\u00b7fer", "sonst", ",", "als", "Hun\u00b7gers", "Lei\u00b7den\u00b7schaft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "$,", "KOUS", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die heute zwar mit Speise wird gestillt,", "tokens": ["Die", "heu\u00b7te", "zwar", "mit", "Spei\u00b7se", "wird", "ge\u00b7stillt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch morgen wiederkehrt in alter Kraft.", "tokens": ["Doch", "mor\u00b7gen", "wie\u00b7der\u00b7kehrt", "in", "al\u00b7ter", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So mu\u00dft du, Liebe, sein! und h\u00e4tt auch dies Gericht", "tokens": ["So", "mu\u00dft", "du", ",", "Lie\u00b7be", ",", "sein", "!", "und", "h\u00e4tt", "auch", "dies", "Ge\u00b7richt"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "$,", "NN", "$,", "VAINF", "$.", "KON", "VAFIN", "ADV", "PDS", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Des Sehns zum Sinken dir dein hungrig Auge heut", "tokens": ["Des", "Sehns", "zum", "Sin\u00b7ken", "dir", "dein", "hung\u00b7rig", "Au\u00b7ge", "heut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "PPER", "PPOSAT", "ADJD", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Erf\u00fcllt, sich morgen wieder! t\u00f6te nicht", "tokens": ["Er\u00b7f\u00fcllt", ",", "sich", "mor\u00b7gen", "wie\u00b7der", "!", "t\u00f6\u00b7te", "nicht"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "PRF", "ADV", "ADV", "$.", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der Liebe Geist in steter Schl\u00e4frigkeit.", "tokens": ["Der", "Lie\u00b7be", "Geist", "in", "ste\u00b7ter", "Schl\u00e4f\u00b7rig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Die tr\u00fcbe Zwischenzeit sei wie ein Meer,", "tokens": ["Die", "tr\u00fc\u00b7be", "Zwi\u00b7schen\u00b7zeit", "sei", "wie", "ein", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Das Ufer sondert, wo zwei Neuverbundne", "tokens": ["Das", "U\u00b7fer", "son\u00b7dert", ",", "wo", "zwei", "Neu\u00b7ver\u00b7bund\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Sich t\u00e4glich sehn, der Liebe Wiederkehr", "tokens": ["Sich", "t\u00e4g\u00b7lich", "sehn", ",", "der", "Lie\u00b7be", "Wie\u00b7der\u00b7kehr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "ADJD", "VVINF", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Zu feiern, zwiefach froh um die gefundne.", "tokens": ["Zu", "fei\u00b7ern", ",", "zwie\u00b7fach", "froh", "um", "die", "ge\u00b7fund\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "ADV", "ADJD", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Ja, nenne Winter sie, des bange Nacht", "tokens": ["Ja", ",", "nen\u00b7ne", "Win\u00b7ter", "sie", ",", "des", "ban\u00b7ge", "Nacht"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "VVFIN", "NN", "PPER", "$,", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.14": {"text": "Des Sommers Gru\u00df dreimal ersehnter macht.", "tokens": ["Des", "Som\u00b7mers", "Gru\u00df", "drei\u00b7mal", "er\u00b7sehn\u00b7ter", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}