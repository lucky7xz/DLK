{"dta.poem.23345": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Partus monstrosus vulgaris Lin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es giebt im menschlichen Geschlechte", "tokens": ["Es", "giebt", "im", "menschli\u00b7chen", "Ge\u00b7schlech\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "F\u00fcr Mi\u00dfgeburten keine Rechte,", "tokens": ["F\u00fcr", "Mi\u00df\u00b7ge\u00b7bur\u00b7ten", "kei\u00b7ne", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und dennoch hat der Frack ein Recht:", "tokens": ["Und", "den\u00b7noch", "hat", "der", "Frack", "ein", "Recht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer w\u00e4re nicht des Frackes Knecht?", "tokens": ["Wer", "w\u00e4\u00b7re", "nicht", "des", "Fra\u00b7ckes", "Knecht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie k\u00f6nnt ihr je von Freiheit tr\u00e4umen,", "tokens": ["Wie", "k\u00f6nnt", "ihr", "je", "von", "Frei\u00b7heit", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wollt ihr dem Frack ein Recht einr\u00e4umen!", "tokens": ["Wollt", "ihr", "dem", "Frack", "ein", "Recht", "ein\u00b7r\u00e4u\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Erfahrt's, wenn ihr's noch nicht erfuhrt:", "tokens": ["Er\u00b7fahrt's", ",", "wenn", "ih\u00b7r's", "noch", "nicht", "er\u00b7fuhrt", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Frack ist eine Mi\u00dfgeburt.", "tokens": ["Der", "Frack", "ist", "ei\u00b7ne", "Mi\u00df\u00b7ge\u00b7burt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}