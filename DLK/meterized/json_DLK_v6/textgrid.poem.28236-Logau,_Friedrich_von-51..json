{"textgrid.poem.28236": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "51.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unsres Lebens beste Kost", "tokens": ["Uns\u00b7res", "Le\u00b7bens", "bes\u00b7te", "Kost"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist von erstem zartes Gras.", "tokens": ["Ist", "von", "ers\u00b7tem", "zar\u00b7tes", "Gras", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unser Leben selbst ist das", "tokens": ["Un\u00b7ser", "Le\u00b7ben", "selbst", "ist", "das"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "VAFIN", "ART"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Samm der Ehr und aller Lust.", "tokens": ["Samm", "der", "Ehr", "und", "al\u00b7ler", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Br\u00e4chte jenes nichts von Fr\u00fcchten,", "tokens": ["Br\u00e4ch\u00b7te", "je\u00b7nes", "nichts", "von", "Fr\u00fcch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "PIS", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bliebs im Felde leichtlich liegen.", "tokens": ["Bliebs", "im", "Fel\u00b7de", "leicht\u00b7lich", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Menschen w\u00fcrden wenig t\u00fcgen,", "tokens": ["Men\u00b7schen", "w\u00fcr\u00b7den", "we\u00b7nig", "t\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wann sie nicht in Himmel t\u00fcchten.", "tokens": ["Wann", "sie", "nicht", "in", "Him\u00b7mel", "t\u00fcch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}