{"textgrid.poem.25321": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: An Hals und Kn\u00f6cheln klirren g\u00fcldne Spangen", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An Hals und Kn\u00f6cheln klirren g\u00fcldne Spangen", "tokens": ["An", "Hals", "und", "Kn\u00f6\u00b7cheln", "klir\u00b7ren", "g\u00fcld\u00b7ne", "Span\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die Spiegel funkeln grell vom Glanz umflossen.", "tokens": ["die", "Spie\u00b7gel", "fun\u00b7keln", "grell", "vom", "Glanz", "um\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf Teppichen drin Ambraduft gefangen", "tokens": ["Auf", "Tep\u00b7pi\u00b7chen", "drin", "Am\u00b7bra\u00b7duft", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "NN", "VVPP"], "meter": "-+--+-++-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "liegt ihres Leibes wei\u00dfer Kelch ergossen", "tokens": ["liegt", "ih\u00b7res", "Lei\u00b7bes", "wei\u00b7\u00dfer", "Kelch", "er\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "von dunklem Haar in losem Kranz umschlungen", "tokens": ["von", "dunk\u00b7lem", "Haar", "in", "lo\u00b7sem", "Kranz", "um\u00b7schlun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "die Augen wie zu schwerem Schlaf geschlossen", "tokens": ["die", "Au\u00b7gen", "wie", "zu", "schwe\u00b7rem", "Schlaf", "ge\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KOKOM", "APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "tr\u00e4umen in leichtem Rausch von eines jungen", "tokens": ["tr\u00e4u\u00b7men", "in", "leich\u00b7tem", "Rausch", "von", "ei\u00b7nes", "jun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "APPR", "ART", "ADJA"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "goldblonden Griechenknaben weichen Br\u00fcsten.", "tokens": ["gold\u00b7blon\u00b7den", "Grie\u00b7chen\u00b7kna\u00b7ben", "wei\u00b7chen", "Br\u00fcs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Fern ist das Lied der Sklavinnen verklungen", "tokens": ["Fern", "ist", "das", "Lied", "der", "Skla\u00b7vin\u00b7nen", "ver\u00b7klun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "ART", "NN", "VVINF"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "die Lippen zucken schlaff als ob sie k\u00fc\u00dften", "tokens": ["die", "Lip\u00b7pen", "zu\u00b7cken", "schlaff", "als", "ob", "sie", "k\u00fc\u00df\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "VVFIN", "KOKOM", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und drau\u00dfen wo die finstern Wachen kreisen", "tokens": ["und", "drau\u00b7\u00dfen", "wo", "die", "fins\u00b7tern", "Wa\u00b7chen", "krei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PWAV", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "lehnt bleich der Henker an den Marmorb\u00fcsten.", "tokens": ["lehnt", "bleich", "der", "Hen\u00b7ker", "an", "den", "Mar\u00b7mor\u00b7b\u00fcs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Rot tanzt die Sonne auf dem nackten Eisen.", "tokens": ["Rot", "tanzt", "die", "Son\u00b7ne", "auf", "dem", "nack\u00b7ten", "Ei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}