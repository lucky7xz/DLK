{"dta.poem.23551": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Trinklied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Glas in der Rechten,", "tokens": ["Das", "Glas", "in", "der", "Rech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die Flasch' in der Linken,", "tokens": ["Die", "Fla\u00b7sch'", "in", "der", "Lin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So wollen wir fechten,", "tokens": ["So", "wol\u00b7len", "wir", "fech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Nicht wanken, nicht sinken!", "tokens": ["Nicht", "wan\u00b7ken", ",", "nicht", "sin\u00b7ken", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$,", "PTKNEG", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Krieg dem Durst und Krieg dem Kummer!", "tokens": ["Krieg", "dem", "Durst", "und", "Krieg", "dem", "Kum\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und ein B\u00fcndni\u00df mit dem Wein!", "tokens": ["Und", "ein", "B\u00fcn\u00b7dni\u00df", "mit", "dem", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Krieg der Nacht und Krieg dem Schlummer!", "tokens": ["Krieg", "der", "Nacht", "und", "Krieg", "dem", "Schlum\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schenkt mir Muth und Feuer ein!", "tokens": ["Schenkt", "mir", "Muth", "und", "Feu\u00b7er", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Das Glas in der Rechten,", "tokens": ["Das", "Glas", "in", "der", "Rech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die Flasch' in der Linken,", "tokens": ["Die", "Fla\u00b7sch'", "in", "der", "Lin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So wollen wir fechten,", "tokens": ["So", "wol\u00b7len", "wir", "fech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Nicht wanken, nicht sinken!", "tokens": ["Nicht", "wan\u00b7ken", ",", "nicht", "sin\u00b7ken", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$,", "PTKNEG", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Wohlig sitzen wir im Weinhaus,", "tokens": ["Woh\u00b7lig", "sit\u00b7zen", "wir", "im", "Wein\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unser Krieg ist wie ein Traum;", "tokens": ["Un\u00b7ser", "Krieg", "ist", "wie", "ein", "Traum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Selbst die Welt, das alte Beinhaus,", "tokens": ["Selbst", "die", "Welt", ",", "das", "al\u00b7te", "Be\u00b7in\u00b7haus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Hat Respect und r\u00fchrt sich kaum.", "tokens": ["Hat", "Res\u00b7pect", "und", "r\u00fchrt", "sich", "kaum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "VVFIN", "PRF", "ADV", "$."], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}}, "stanza.3": {"line.1": {"text": "Das Glas in der Rechten,", "tokens": ["Das", "Glas", "in", "der", "Rech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die Flasch' in der Linken,", "tokens": ["Die", "Fla\u00b7sch'", "in", "der", "Lin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So wollen wir fechten,", "tokens": ["So", "wol\u00b7len", "wir", "fech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Nicht wanken, nicht sinken!", "tokens": ["Nicht", "wan\u00b7ken", ",", "nicht", "sin\u00b7ken", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$,", "PTKNEG", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Eine Flasche hat geschlagen", "tokens": ["Ei\u00b7ne", "Fla\u00b7sche", "hat", "ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unsre Feinde kreuz und quer;", "tokens": ["Uns\u00b7re", "Fein\u00b7de", "kreuz", "und", "quer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und da stehen wir und fragen:", "tokens": ["Und", "da", "ste\u00b7hen", "wir", "und", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "KON", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Giebt's denn keine Feinde mehr?", "tokens": ["Giebt's", "denn", "kei\u00b7ne", "Fein\u00b7de", "mehr", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "PIAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Das Glas in der Rechten,", "tokens": ["Das", "Glas", "in", "der", "Rech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die Flasch' in der Linken,", "tokens": ["Die", "Fla\u00b7sch'", "in", "der", "Lin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So wollen wir fechten,", "tokens": ["So", "wol\u00b7len", "wir", "fech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Nicht wanken, nicht sinken!", "tokens": ["Nicht", "wan\u00b7ken", ",", "nicht", "sin\u00b7ken", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$,", "PTKNEG", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Und das Ende von dem Liede?", "tokens": ["Und", "das", "En\u00b7de", "von", "dem", "Lie\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ei, was machen wir uns draus!", "tokens": ["Ei", ",", "was", "ma\u00b7chen", "wir", "uns", "draus", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VVFIN", "PPER", "PRF", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Alles Strebens Frucht ist Friede \u2014", "tokens": ["Al\u00b7les", "Stre\u00b7bens", "Frucht", "ist", "Frie\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "VAFIN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wir, wir gehn im Sturm nach Haus.", "tokens": ["Wir", ",", "wir", "gehn", "im", "Sturm", "nach", "Haus", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "VVFIN", "APPRART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}