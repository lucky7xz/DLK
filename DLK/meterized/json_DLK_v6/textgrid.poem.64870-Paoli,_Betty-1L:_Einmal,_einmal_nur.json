{"textgrid.poem.64870": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einmal, einmal nur", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einmal, einmal nur", "tokens": ["Ein\u00b7mal", ",", "ein\u00b7mal", "nur"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADV", "$,", "ADV", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Hab' ich dich geschaut,", "tokens": ["Hab'", "ich", "dich", "ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Einmal nur vernommen", "tokens": ["Ein\u00b7mal", "nur", "ver\u00b7nom\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Deiner Stimme Laut;", "tokens": ["Dei\u00b7ner", "Stim\u00b7me", "Laut", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Aber h\u00f6r' den Schwur,", "tokens": ["A\u00b7ber", "h\u00f6r'", "den", "Schwur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Den dir zuschickt Deine Geistesbraut:", "tokens": ["Den", "dir", "zu\u00b7schickt", "Dei\u00b7ne", "Geis\u00b7tes\u00b7braut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Meine Seele weilt", "tokens": ["Mei\u00b7ne", "See\u00b7le", "weilt"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Wo dein Athem weht,", "tokens": ["Wo", "dein", "A\u00b7them", "weht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Was an Gl\u00fcck mag kommen", "tokens": ["Was", "an", "Gl\u00fcck", "mag", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Kommt f\u00fcr mich zu sp\u00e4t,", "tokens": ["Kommt", "f\u00fcr", "mich", "zu", "sp\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PTKA", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Denn es k\u00fchlt und heilt", "tokens": ["Denn", "es", "k\u00fchlt", "und", "heilt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Nichts den Schmerz, da\u00df ich dich nicht erfleht.", "tokens": ["Nichts", "den", "Schmerz", ",", "da\u00df", "ich", "dich", "nicht", "er\u00b7fleht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "$,", "KOUS", "PPER", "PRF", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}