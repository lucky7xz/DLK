{"textgrid.poem.37260": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[fr\u00fcher, da ich unerfahren]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fcher, da ich unerfahren", "tokens": ["Fr\u00fc\u00b7her", ",", "da", "ich", "un\u00b7er\u00b7fah\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und bescheidner war als heute,", "tokens": ["Und", "be\u00b7scheid\u00b7ner", "war", "als", "heu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "KOKOM", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hatten meine h\u00f6chste Achtung", "tokens": ["Hat\u00b7ten", "mei\u00b7ne", "h\u00f6chs\u00b7te", "Ach\u00b7tung"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Andre Leute.", "tokens": ["And\u00b7re", "Leu\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Sp\u00e4ter traf ich auf der Weide", "tokens": ["Sp\u00e4\u00b7ter", "traf", "ich", "auf", "der", "Wei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Au\u00dfer mir noch mehre K\u00e4lber,", "tokens": ["Au\u00b7\u00dfer", "mir", "noch", "meh\u00b7re", "K\u00e4l\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und nun sch\u00e4tz ich, sozusagen,", "tokens": ["Und", "nun", "sch\u00e4tz", "ich", ",", "so\u00b7zu\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Erst mich selber.", "tokens": ["Erst", "mich", "sel\u00b7ber", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}