{"textgrid.poem.45158": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Dem Geber der Preussischen Konstitution", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf dein Erfindereigentum", "tokens": ["Auf", "dein", "Er\u00b7fin\u00b7der\u00b7ei\u00b7gen\u00b7tum"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Brauchst du kein Privilegium.", "tokens": ["Brauchst", "du", "kein", "Pri\u00b7vi\u00b7le\u00b7gi\u00b7um", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Wer Sachen will und nicht blo\u00df Namen,", "tokens": ["Wer", "Sa\u00b7chen", "will", "und", "nicht", "blo\u00df", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VMFIN", "KON", "PTKNEG", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Versucht wohl kaum, dirs nachzuahmen.", "tokens": ["Ver\u00b7sucht", "wohl", "kaum", ",", "dirs", "nach\u00b7zu\u00b7ah\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "$,", "PIS", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}