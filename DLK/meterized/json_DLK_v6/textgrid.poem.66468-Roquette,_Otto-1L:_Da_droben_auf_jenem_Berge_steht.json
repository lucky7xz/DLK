{"textgrid.poem.66468": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da droben auf jenem Berge steht", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da droben auf jenem Berge steht", "tokens": ["Da", "dro\u00b7ben", "auf", "je\u00b7nem", "Ber\u00b7ge", "steht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "PDAT", "NN", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Holunderbaum vom Wind umweht,", "tokens": ["Ein", "Ho\u00b7lun\u00b7der\u00b7baum", "vom", "Wind", "um\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Gewieget zu der Erden.", "tokens": ["Ge\u00b7wie\u00b7get", "zu", "der", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Nacht ist hell und die Luft ist k\u00fchl,", "tokens": ["Die", "Nacht", "ist", "hell", "und", "die", "Luft", "ist", "k\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zwei Buhlen weinen der Thr\u00e4nen so viel,", "tokens": ["Zwei", "Buh\u00b7len", "wei\u00b7nen", "der", "Thr\u00e4\u00b7nen", "so", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVINF", "ART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie m\u00fcssen scheiden, ja scheiden.", "tokens": ["Sie", "m\u00fcs\u00b7sen", "schei\u00b7den", ",", "ja", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "ADV", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Sie rissen mit Thr\u00e4nen, mit Thr\u00e4nen sich los,", "tokens": ["Sie", "ris\u00b7sen", "mit", "Thr\u00e4\u00b7nen", ",", "mit", "Thr\u00e4\u00b7nen", "sich", "los", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "APPR", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Der Schmerz war tief, und der Schmerz war gro\u00df.", "tokens": ["Der", "Schmerz", "war", "tief", ",", "und", "der", "Schmerz", "war", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "KON", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie sahen sich niemals wieder.", "tokens": ["Sie", "sa\u00b7hen", "sich", "nie\u00b7mals", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADV", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Er zog wohl \u00fcber das weite Meer,", "tokens": ["Er", "zog", "wohl", "\u00fc\u00b7ber", "das", "wei\u00b7te", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie h\u00f6rt' eine schaurige Todesm\u00e4hr,", "tokens": ["Sie", "h\u00f6rt'", "ei\u00b7ne", "schau\u00b7ri\u00b7ge", "To\u00b7des\u00b7m\u00e4hr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und ging weit \u00fcber die Berge.", "tokens": ["Und", "ging", "weit", "\u00fc\u00b7ber", "die", "Ber\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Verschollen ist ihr Nam' im Land,", "tokens": ["Ver\u00b7schol\u00b7len", "ist", "ihr", "Nam'", "im", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Mond nur ihre Gr\u00e4ber fand,", "tokens": ["Der", "Mond", "nur", "ih\u00b7re", "Gr\u00e4\u00b7ber", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drauf spielen die L\u00fcfte des Maien.", "tokens": ["Drauf", "spie\u00b7len", "die", "L\u00fcf\u00b7te", "des", "Mai\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.6": {"line.1": {"text": "Der gr\u00fcne Baum steht droben noch", "tokens": ["Der", "gr\u00fc\u00b7ne", "Baum", "steht", "dro\u00b7ben", "noch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit tausend Bl\u00fcthen sprossend hoch,", "tokens": ["Mit", "tau\u00b7send", "Bl\u00fc\u00b7then", "spros\u00b7send", "hoch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gewieget zu der Erden.", "tokens": ["Ge\u00b7wie\u00b7get", "zu", "der", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}