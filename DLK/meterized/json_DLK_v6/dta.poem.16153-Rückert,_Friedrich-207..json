{"dta.poem.16153": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "207.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wozu ein gro\u00dfes Haus? es n\u00fctzt nicht voll noch leer.", "tokens": ["Wo\u00b7zu", "ein", "gro\u00b7\u00dfes", "Haus", "?", "es", "n\u00fctzt", "nicht", "voll", "noch", "leer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "PTKNEG", "ADJD", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu einem gro\u00dfen Haus geh\u00f6rt ein gro\u00dfes Heer.", "tokens": ["Zu", "ei\u00b7nem", "gro\u00b7\u00dfen", "Haus", "ge\u00b7h\u00f6rt", "ein", "gro\u00b7\u00dfes", "Heer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zu einem gro\u00dfen Heer geh\u00f6rt ein reicher Sold,", "tokens": ["Zu", "ei\u00b7nem", "gro\u00b7\u00dfen", "Heer", "ge\u00b7h\u00f6rt", "ein", "rei\u00b7cher", "Sold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum reichen Sold geh\u00f6rt ein eigner Schacht von Gold.", "tokens": ["Zum", "rei\u00b7chen", "Sold", "ge\u00b7h\u00f6rt", "ein", "eig\u00b7ner", "Schacht", "von", "Gold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Zum Schacht von Gold geh\u00f6rt viel M\u00fch wol, ihn zu graben;", "tokens": ["Zum", "Schacht", "von", "Gold", "ge\u00b7h\u00f6rt", "viel", "M\u00fch", "wol", ",", "ihn", "zu", "gra\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "VVFIN", "PIAT", "NN", "ADV", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drum will ich auf der Welt ein kleines Haus nur haben.", "tokens": ["Drum", "will", "ich", "auf", "der", "Welt", "ein", "klei\u00b7nes", "Haus", "nur", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Das gr\u00f6\u00dfte Haus ist eng, das kleinste Haus ist weit,", "tokens": ["Das", "gr\u00f6\u00df\u00b7te", "Haus", "ist", "eng", ",", "das", "kleins\u00b7te", "Haus", "ist", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$,", "ART", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn dort ist ein Gedr\u00e4ng und hier Zufriedenheit.", "tokens": ["Wenn", "dort", "ist", "ein", "Ge\u00b7dr\u00e4ng", "und", "hier", "Zu\u00b7frie\u00b7den\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VAFIN", "ART", "NN", "KON", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}