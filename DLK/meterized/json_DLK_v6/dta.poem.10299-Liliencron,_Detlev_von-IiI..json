{"dta.poem.10299": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "IiI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Wie klar erschienst du heute mir im Traum,", "tokens": ["Wie", "klar", "er\u00b7schienst", "du", "heu\u00b7te", "mir", "im", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "ADV", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wir sa\u00dfen in der Kneipe fest und tranken,", "tokens": ["Wir", "sa\u00b7\u00dfen", "in", "der", "Knei\u00b7pe", "fest", "und", "tran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bis wir ger\u00fchrt uns in die Arme sanken,", "tokens": ["Bis", "wir", "ge\u00b7r\u00fchrt", "uns", "in", "die", "Ar\u00b7me", "san\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf unsern Lippen lag der erste Flaum.", "tokens": ["Auf", "un\u00b7sern", "Lip\u00b7pen", "lag", "der", "ers\u00b7te", "Flaum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dein falber Wallach schleifte Zeug und Zaum,", "tokens": ["Dein", "fal\u00b7ber", "Wal\u00b7lach", "schleif\u00b7te", "Zeug", "und", "Zaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und bi\u00df und schlug und warf den Hals, den schlanken.", "tokens": ["Und", "bi\u00df", "und", "schlug", "und", "warf", "den", "Hals", ",", "den", "schlan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "KON", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,", "ART", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Sattel, sah ich dich, erschossen, schwanken,", "tokens": ["Im", "Sat\u00b7tel", ",", "sah", "ich", "dich", ",", "er\u00b7schos\u00b7sen", ",", "schwan\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVFIN", "PPER", "PRF", "$,", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und hinst\u00fcrzen am wilden Apfelbaum.", "tokens": ["Und", "hin\u00b7st\u00fcr\u00b7zen", "am", "wil\u00b7den", "Ap\u00b7fel\u00b7baum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Die Watten stinken wie das Leichenfeld,", "tokens": ["Die", "Wat\u00b7ten", "stin\u00b7ken", "wie", "das", "Lei\u00b7chen\u00b7feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo viel Erschlagne faulen nach der Schlacht,", "tokens": ["Wo", "viel", "Er\u00b7schlag\u00b7ne", "fau\u00b7len", "nach", "der", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Tag\u00fcber sonnbeschienen ohne Zelt.", "tokens": ["Ta\u00b7g\u00fc\u00b7ber", "sonn\u00b7be\u00b7schie\u00b7nen", "oh\u00b7ne", "Zelt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Geheimni\u00dfvoll, wie tot in Bann und Acht,", "tokens": ["Ge\u00b7heim\u00b7ni\u00df\u00b7voll", ",", "wie", "tot", "in", "Bann", "und", "Acht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ADJD", "APPR", "NN", "KON", "CARD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sinkt, grau und goldumhaucht, die Halligwelt,", "tokens": ["Sinkt", ",", "grau", "und", "gold\u00b7um\u00b7haucht", ",", "die", "Hal\u00b7lig\u00b7welt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "KON", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und aus der Abendr\u00f6te steigt die Nacht.", "tokens": ["Und", "aus", "der", "A\u00b7bend\u00b7r\u00f6\u00b7te", "steigt", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}