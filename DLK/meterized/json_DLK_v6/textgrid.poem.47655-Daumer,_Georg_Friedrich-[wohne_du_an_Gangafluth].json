{"textgrid.poem.47655": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wohne du an Gangafluth]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohne du an Gangafluth,", "tokens": ["Woh\u00b7ne", "du", "an", "Gan\u00b7ga\u00b7fluth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "S\u00fcndebeschw\u00f6rend quellender!", "tokens": ["S\u00fcn\u00b7de\u00b7be\u00b7schw\u00f6\u00b7rend", "quel\u00b7len\u00b7der", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Mich la\u00df wohnen an zarter Brust,", "tokens": ["Mich", "la\u00df", "woh\u00b7nen", "an", "zar\u00b7ter", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Sinnebeth\u00f6rend schwellender!", "tokens": ["Sin\u00b7ne\u00b7bet\u00b7h\u00f6\u00b7rend", "schwel\u00b7len\u00b7der", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Stirbt, o Frommer, am Gangarand,", "tokens": ["Stirbt", ",", "o", "From\u00b7mer", ",", "am", "Gan\u00b7ga\u00b7rand", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "FM", "NN", "$,", "APPRART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "La\u00df dich st\u00fcrzen in's heil'ge Na\u00df!", "tokens": ["La\u00df", "dich", "st\u00fcr\u00b7zen", "in's", "heil'\u00b7ge", "Na\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Mich ausathmen an diesem Mund,", "tokens": ["Mich", "au\u00b7sath\u00b7men", "an", "die\u00b7sem", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Mich hinsterben in Wonne la\u00df!", "tokens": ["Mich", "hins\u00b7ter\u00b7ben", "in", "Won\u00b7ne", "la\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}