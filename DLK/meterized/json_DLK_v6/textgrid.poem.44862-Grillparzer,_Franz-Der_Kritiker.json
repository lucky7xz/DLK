{"textgrid.poem.44862": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Der Kritiker", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Dichtkunst, sagt man oft und sagt es laut,", "tokens": ["Die", "Dicht\u00b7kunst", ",", "sagt", "man", "oft", "und", "sagt", "es", "laut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PIS", "ADV", "KON", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie sei ein Bild und Spiegel dieses Lebens:", "tokens": ["Sie", "sei", "ein", "Bild", "und", "Spie\u00b7gel", "die\u00b7ses", "Le\u00b7bens", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Drum, wenn ein Affe in das Dichtwerk schaut,", "tokens": ["Drum", ",", "wenn", "ein", "Af\u00b7fe", "in", "das", "Dicht\u00b7werk", "schaut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hofft er auf einen Sokrates vergebens.", "tokens": ["Hofft", "er", "auf", "ei\u00b7nen", "Sok\u00b7ra\u00b7tes", "ver\u00b7ge\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NE", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}