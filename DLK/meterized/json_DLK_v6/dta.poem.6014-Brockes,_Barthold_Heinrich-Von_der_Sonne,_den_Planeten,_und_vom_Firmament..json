{"dta.poem.6014": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "An diesem hellen Herrn der Jahre, Tag' und Stunden,", "tokens": ["An", "die\u00b7sem", "hel\u00b7len", "Herrn", "der", "Jah\u00b7re", ",", "Tag'", "und", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "ART", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sind der Bewegungen Gesetze stets verbunden.", "tokens": ["Sind", "der", "Be\u00b7we\u00b7gun\u00b7gen", "Ge\u00b7set\u00b7ze", "stets", "ver\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "ADV", "VVPP", "$."], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.3": {"text": "In ihm f\u00e4ngt alles an sich zu bewegen,", "tokens": ["In", "ihm", "f\u00e4ngt", "al\u00b7les", "an", "sich", "zu", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PIS", "APPR", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus ihm entsteht das hefftigschnelle Regen.", "tokens": ["Aus", "ihm", "ent\u00b7steht", "das", "heff\u00b7tigsc\u00b7hnel\u00b7le", "Re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mit seinem hellen Wunder-Schein", "tokens": ["Mit", "sei\u00b7nem", "hel\u00b7len", "Wun\u00b7der\u00b7Schein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Stimmt alles \u00fcberein,", "tokens": ["Stimmt", "al\u00b7les", "\u00fc\u00b7be\u00b7re\u00b7in", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Und alles folgt der sch\u00f6nen Harmonie.", "tokens": ["Und", "al\u00b7les", "folgt", "der", "sch\u00f6\u00b7nen", "Har\u00b7mo\u00b7nie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Jm Mittel-Punct der Krafft vereinet sie", "tokens": ["Jm", "Mit\u00b7tel\u00b7Punct", "der", "Krafft", "ver\u00b7ei\u00b7net", "sie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Die gantze Macht; die breitet sich", "tokens": ["Die", "gant\u00b7ze", "Macht", ";", "die", "brei\u00b7tet", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "So dann in Stuffen aus. Durch sie allein entstehn", "tokens": ["So", "dann", "in", "Stuf\u00b7fen", "aus", ".", "Durch", "sie", "al\u00b7lein", "ent\u00b7stehn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "NN", "PTKVZ", "$.", "APPR", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Dinge, welche wir in solcher Ordnung sehn,", "tokens": ["Die", "Din\u00b7ge", ",", "wel\u00b7che", "wir", "in", "sol\u00b7cher", "Ord\u00b7nung", "sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die sch\u00f6n und unver\u00e4nderlich.", "tokens": ["Die", "sch\u00f6n", "und", "un\u00b7ver\u00b7\u00e4n\u00b7der\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "In dem Revier der nimmer stillen Sterne,", "tokens": ["In", "dem", "Re\u00b7vier", "der", "nim\u00b7mer", "stil\u00b7len", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "In dem die Kugeln sich in weiten Creysen drehn,", "tokens": ["In", "dem", "die", "Ku\u00b7geln", "sich", "in", "wei\u00b7ten", "Crey\u00b7sen", "drehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "PRF", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Miss\u2019t sich ihr Lauff nach ihrer Ferne.", "tokens": ["Miss't", "sich", "ihr", "Lauff", "nach", "ih\u00b7rer", "Fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Die, so der Sonnen nah, sieht man viel schneller gehn:", "tokens": ["Die", ",", "so", "der", "Son\u00b7nen", "nah", ",", "sieht", "man", "viel", "schnel\u00b7ler", "gehn", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "ART", "NN", "ADJD", "$,", "VVFIN", "PIS", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Und die, so weiter weg in den entfernten Creysen,", "tokens": ["Und", "die", ",", "so", "wei\u00b7ter", "weg", "in", "den", "ent\u00b7fern\u00b7ten", "Crey\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "ADV", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Sieht man so hefftig nicht, nicht so geschwinde reisen.", "tokens": ["Sieht", "man", "so", "heff\u00b7tig", "nicht", ",", "nicht", "so", "ge\u00b7schwin\u00b7de", "rei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADJD", "PTKNEG", "$,", "PTKNEG", "ADV", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Es gehet alles gleich vom Mittel bis zur H\u00f6he,", "tokens": ["Es", "ge\u00b7het", "al\u00b7les", "gleich", "vom", "Mit\u00b7tel", "bis", "zur", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADV", "APPRART", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ohn da\u00df das mindste, sich verhind\u2019rend, stille stehe.", "tokens": ["Ohn", "da\u00df", "das", "minds\u00b7te", ",", "sich", "ver\u00b7hin\u00b7d'\u00b7rend", ",", "stil\u00b7le", "ste\u00b7he", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "ADJA", "$,", "PRF", "VVPP", "$,", "ADJA", "VVFIN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "O! wundernsw\u00fcrdige Gleichf\u00f6rmigkeit!", "tokens": ["O", "!", "wun\u00b7derns\u00b7w\u00fcr\u00b7di\u00b7ge", "Gleich\u00b7f\u00f6r\u00b7mig\u00b7keit", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "O! wunderreiche Einfachheit!", "tokens": ["O", "!", "wun\u00b7der\u00b7rei\u00b7che", "Ein\u00b7fach\u00b7heit", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}