{"dta.poem.9078": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Fern liegt ein Land!", "tokens": ["Fern", "liegt", "ein", "Land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "In dunklen N\u00e4chten", "tokens": ["In", "dunk\u00b7len", "N\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "rauschten schwerm\u00fctig seine Eichen.", "tokens": ["rauschten", "schwer\u00b7m\u00fc\u00b7tig", "sei\u00b7ne", "Ei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weiche Flocken deckten mein Grab.", "tokens": ["Wei\u00b7che", "Flo\u00b7cken", "deck\u00b7ten", "mein", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Jetzt bl\u00fchn die Primeln,", "tokens": ["Jetzt", "bl\u00fchn", "die", "Pri\u00b7meln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "die Drossel singt,", "tokens": ["die", "Dros\u00b7sel", "singt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "und \u00fcber gr\u00fcne Wiesen, um den blauen See", "tokens": ["und", "\u00fc\u00b7ber", "gr\u00fc\u00b7ne", "Wie\u00b7sen", ",", "um", "den", "blau\u00b7en", "See"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "$,", "KOUI", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "treibt der Sch\u00e4fer seine Schafe.", "tokens": ["treibt", "der", "Sch\u00e4\u00b7fer", "sei\u00b7ne", "Scha\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Weisse W\u00f6lkchen gleiten.", "tokens": ["Weis\u00b7se", "W\u00f6lk\u00b7chen", "glei\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Du s\u00fcsse Welt!", "tokens": ["Du", "s\u00fcs\u00b7se", "Welt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Auf deinen gl\u00e4nzendsten Stern", "tokens": ["Auf", "dei\u00b7nen", "gl\u00e4n\u00b7zends\u00b7ten", "Stern"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "hast du ein Herz, das dich liebt, gerettet!", "tokens": ["hast", "du", "ein", "Herz", ",", "das", "dich", "liebt", ",", "ge\u00b7ret\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}