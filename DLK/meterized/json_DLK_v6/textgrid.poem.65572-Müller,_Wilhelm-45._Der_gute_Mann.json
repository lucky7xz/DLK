{"textgrid.poem.65572": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "45. Der gute Mann", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er ist ein guter Mann,", "tokens": ["Er", "ist", "ein", "gu\u00b7ter", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Er l\u00e4\u00dft, was er nicht kann:", "tokens": ["Er", "l\u00e4\u00dft", ",", "was", "er", "nicht", "kann", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "PTKNEG", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Trinkt nie aus einer leeren Flasche", "tokens": ["Trinkt", "nie", "aus", "ei\u00b7ner", "lee\u00b7ren", "Fla\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und steckt keinen Kirchthurm in die Tasche.", "tokens": ["Und", "steckt", "kei\u00b7nen", "Kirch\u00b7thurm", "in", "die", "Ta\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}}}}