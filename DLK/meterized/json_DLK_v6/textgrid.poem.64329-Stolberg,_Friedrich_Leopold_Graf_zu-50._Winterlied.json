{"textgrid.poem.64329": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "50. Winterlied", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich einmal der Stadt entrinn',", "tokens": ["Wenn", "ich", "ein\u00b7mal", "der", "Stadt", "ent\u00b7rinn'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird mir so wohl in meinem Sinn,", "tokens": ["Wird", "mir", "so", "wohl", "in", "mei\u00b7nem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich gr\u00fc\u00dfe Himmel, Meer und Feld", "tokens": ["Ich", "gr\u00fc\u00b7\u00dfe", "Him\u00b7mel", ",", "Meer", "und", "Feld"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In meiner lieben Gotteswelt.", "tokens": ["In", "mei\u00b7ner", "lie\u00b7ben", "Got\u00b7tes\u00b7welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich sehe froh und frisch hinein,", "tokens": ["Ich", "se\u00b7he", "froh", "und", "frisch", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So gl\u00fccklich wie ein V\u00f6gelein,", "tokens": ["So", "gl\u00fcck\u00b7lich", "wie", "ein", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das aus dem engen K\u00e4ficht fleucht,", "tokens": ["Das", "aus", "dem", "en\u00b7gen", "K\u00e4\u00b7ficht", "fleucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und singend in die L\u00fcfte steigt.", "tokens": ["Und", "sin\u00b7gend", "in", "die", "L\u00fcf\u00b7te", "steigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Auch sieht mich alles freundlich an,", "tokens": ["Auch", "sieht", "mich", "al\u00b7les", "freund\u00b7lich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Schmuck des Winters angethan:", "tokens": ["Im", "Schmuck", "des", "Win\u00b7ters", "an\u00b7ge\u00b7than", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Meer gepanzert, wei\u00df und hart;", "tokens": ["Das", "Meer", "ge\u00b7pan\u00b7zert", ",", "wei\u00df", "und", "hart", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der krause Wald, der blinkend starrt.", "tokens": ["Der", "krau\u00b7se", "Wald", ",", "der", "blin\u00b7kend", "starrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der lieben S\u00e4nger buntes Heer", "tokens": ["Der", "lie\u00b7ben", "S\u00e4n\u00b7ger", "bun\u00b7tes", "Heer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00fcpft auf den \u00c4sten hin und her,", "tokens": ["H\u00fcpft", "auf", "den", "\u00c4s\u00b7ten", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und sonnet sich am jungen Licht,", "tokens": ["Und", "son\u00b7net", "sich", "am", "jun\u00b7gen", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das durch die braunen Zweige bricht.", "tokens": ["Das", "durch", "die", "brau\u00b7nen", "Zwei\u00b7ge", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Hier keimt die zarte Saat empor,", "tokens": ["Hier", "keimt", "die", "zar\u00b7te", "Saat", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und kucket aus dem Schnee hervor;", "tokens": ["Und", "ku\u00b7cket", "aus", "dem", "Schnee", "her\u00b7vor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dort lockt des Thales weiches Moos", "tokens": ["Dort", "lockt", "des", "Tha\u00b7les", "wei\u00b7ches", "Moos"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das junge Reh auf seinen Scho\u00df.", "tokens": ["Das", "jun\u00b7ge", "Reh", "auf", "sei\u00b7nen", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Natur, du wirst mir nimmer alt", "tokens": ["Na\u00b7tur", ",", "du", "wirst", "mir", "nim\u00b7mer", "alt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VAFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In deiner wechselnden Gestalt!", "tokens": ["In", "dei\u00b7ner", "wech\u00b7seln\u00b7den", "Ge\u00b7stalt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Natur, so hehr, so wunderbar,", "tokens": ["Na\u00b7tur", ",", "so", "hehr", ",", "so", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und doch so traut, und doch so wahr!", "tokens": ["Und", "doch", "so", "traut", ",", "und", "doch", "so", "wahr", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "$,", "KON", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Auf! Atalante, renne frisch!", "tokens": ["Auf", "!", "A\u00b7tal\u00b7an\u00b7te", ",", "ren\u00b7ne", "frisch", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "$.", "NN", "$,", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich wittre schon den frohen Tisch.", "tokens": ["Ich", "witt\u00b7re", "schon", "den", "fro\u00b7hen", "Tisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der goldne Haber harret dein,", "tokens": ["Der", "gold\u00b7ne", "Ha\u00b7ber", "har\u00b7ret", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mein der goldne deutsche Wein!", "tokens": ["Und", "mein", "der", "gold\u00b7ne", "deut\u00b7sche", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}