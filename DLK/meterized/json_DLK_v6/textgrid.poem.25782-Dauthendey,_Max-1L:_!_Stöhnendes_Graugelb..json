{"textgrid.poem.25782": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: ! St\u00f6hnendes Graugelb.", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "! St\u00f6hnendes Graugelb.", "tokens": ["!", "St\u00f6h\u00b7nen\u00b7des", "Grau\u00b7gelb", "."], "token_info": ["punct", "word", "word", "punct"], "pos": ["$.", "NE", "NE", "$."], "meter": "+--++", "measure": "adoneus"}, "line.2": {"text": "Aber das St\u00f6hnen nur im Blick.", "tokens": ["A\u00b7ber", "das", "St\u00f6h\u00b7nen", "nur", "im", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Lautlos sonst und mit unterdr\u00fccktem Atem.", "tokens": ["Laut\u00b7los", "sonst", "und", "mit", "un\u00b7ter\u00b7dr\u00fcck\u00b7tem", "A\u00b7tem", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "KON", "APPR", "ADJA", "NN", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}}, "stanza.2": {"line.1": {"text": "! Und ein Blau,", "tokens": ["!", "Und", "ein", "Blau", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$.", "KON", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Ein Blau, aus dem ganz zarte silberne Glocken spiele singen,", "tokens": ["Ein", "Blau", ",", "aus", "dem", "ganz", "zar\u00b7te", "sil\u00b7ber\u00b7ne", "Glo\u00b7cken", "spie\u00b7le", "sin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "ADV", "ADJA", "ADJA", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Und ein Duft geht von Sonnenw\u00e4rme und Mandelbl\u00fcten.", "tokens": ["Und", "ein", "Duft", "geht", "von", "Son\u00b7nen\u00b7w\u00e4r\u00b7me", "und", "Man\u00b7del\u00b7bl\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "--+--+-++-+-+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "! Silber dar\u00fcber.", "tokens": ["!", "Sil\u00b7ber", "da\u00b7r\u00fc\u00b7ber", "."], "token_info": ["punct", "word", "word", "punct"], "pos": ["$.", "NN", "PAV", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Duftleeres, schneek\u00fchles Silber.", "tokens": ["Duft\u00b7lee\u00b7res", ",", "schnee\u00b7k\u00fch\u00b7les", "Sil\u00b7ber", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Aber aus allem hebt sich steif", "tokens": ["A\u00b7ber", "aus", "al\u00b7lem", "hebt", "sich", "steif"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIS", "VVFIN", "PRF", "ADJD"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und hebt sich fahl, wie Gewitterlicht,", "tokens": ["Und", "hebt", "sich", "fahl", ",", "wie", "Ge\u00b7wit\u00b7ter\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "VVFIN", "$,", "PWAV", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Das stumme Graugelb.", "tokens": ["Das", "stum\u00b7me", "Grau\u00b7gelb", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Und hebt sich lautlos st\u00f6hnend wie Asche,", "tokens": ["Und", "hebt", "sich", "laut\u00b7los", "st\u00f6h\u00b7nend", "wie", "A\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Mit welkem darbenden Blick.", "tokens": ["Mit", "wel\u00b7kem", "dar\u00b7ben\u00b7den", "Blick", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "! Ein Gesicht \u2013 die starre Maske eines Toten \u2013", "tokens": ["!", "Ein", "Ge\u00b7sicht", "\u2013", "die", "star\u00b7re", "Mas\u00b7ke", "ei\u00b7nes", "To\u00b7ten", "\u2013"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$.", "ART", "NN", "$(", "ART", "ADJA", "NN", "ART", "NN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Ein Kopf \u2013 aus dem Blau \u2013 aus dem blauen, glatten Wasser.", "tokens": ["Ein", "Kopf", "\u2013", "aus", "dem", "Blau", "\u2013", "aus", "dem", "blau\u00b7en", ",", "glat\u00b7ten", "Was\u00b7ser", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "APPR", "ART", "NN", "$(", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Braunviolette Str\u00e4hne \u2013 Haare in die Stirn,", "tokens": ["Braun\u00b7vi\u00b7o\u00b7let\u00b7te", "Str\u00e4h\u00b7ne", "\u2013", "Haa\u00b7re", "in", "die", "Stirn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das eine Auge schief, spitze Wangenknochen,", "tokens": ["Das", "ei\u00b7ne", "Au\u00b7ge", "schief", ",", "spit\u00b7ze", "Wan\u00b7gen\u00b7kno\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVFIN", "$,", "VVFIN", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Und trieft von den Schl\u00e4fen das braunviolette Haar", "tokens": ["Und", "trieft", "von", "den", "Schl\u00e4\u00b7fen", "das", "braun\u00b7vi\u00b7o\u00b7let\u00b7te", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.6": {"text": "\u00dcber das \u00f6de aschige Gelb.", "tokens": ["\u00dc\u00b7ber", "das", "\u00f6\u00b7de", "asc\u00b7hi\u00b7ge", "Gelb", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Und dar\u00fcber: \u00fcber das blaue Wasser", "tokens": ["Und", "da\u00b7r\u00fc\u00b7ber", ":", "\u00fc\u00b7ber", "das", "blau\u00b7e", "Was\u00b7ser"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PAV", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Silbern ein Schwan.", "tokens": ["Sil\u00b7bern", "ein", "Schwan", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Silbern die Reflexe von Wolken,", "tokens": ["Sil\u00b7bern", "die", "Re\u00b7fle\u00b7xe", "von", "Wol\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Duftleer, schneek\u00fchl.", "tokens": ["Duft\u00b7leer", ",", "schnee\u00b7k\u00fchl", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "In das Blau,", "tokens": ["In", "das", "Blau", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "In das Silber", "tokens": ["In", "das", "Sil\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Ragt der gelbaschige Kopf des Ertrunkenen.", "tokens": ["Ragt", "der", "gel\u00b7ba\u00b7schi\u00b7ge", "Kopf", "des", "Er\u00b7trun\u00b7ke\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und der Schwan zieht reglos vorbei,", "tokens": ["Und", "der", "Schwan", "zieht", "reg\u00b7los", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Reglos die Reflexe der Wolken.", "tokens": ["Reg\u00b7los", "die", "Re\u00b7fle\u00b7xe", "der", "Wol\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}