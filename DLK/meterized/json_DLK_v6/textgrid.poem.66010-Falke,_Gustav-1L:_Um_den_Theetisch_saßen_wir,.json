{"textgrid.poem.66010": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Um den Theetisch sa\u00dfen wir,", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um den Theetisch sa\u00dfen wir,", "tokens": ["Um", "den", "Thee\u00b7tisch", "sa\u00b7\u00dfen", "wir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Oder tranken wir Kaffee oder Chokolade,", "tokens": ["O\u00b7der", "tran\u00b7ken", "wir", "Kaf\u00b7fee", "o\u00b7der", "Cho\u00b7ko\u00b7la\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Ein Traum nur war es,", "tokens": ["Ein", "Traum", "nur", "war", "es", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "PPER", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Und alles lebt nur wie Schatten noch,", "tokens": ["Und", "al\u00b7les", "lebt", "nur", "wie", "Schat\u00b7ten", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "KOKOM", "NN", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wie Bilder aus einer Laterne magika", "tokens": ["Wie", "Bil\u00b7der", "aus", "ei\u00b7ner", "La\u00b7ter\u00b7ne", "ma\u00b7gi\u00b7ka"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "NE"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "In meiner Erinnerung.", "tokens": ["In", "mei\u00b7ner", "E\u00b7rin\u00b7ne\u00b7rung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.7": {"text": "Deutlich nur seh ich", "tokens": ["Deut\u00b7lich", "nur", "seh", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Zur Rechten mir das kleine zierliche M\u00e4dchen,", "tokens": ["Zur", "Rech\u00b7ten", "mir", "das", "klei\u00b7ne", "zier\u00b7li\u00b7che", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Zw\u00f6lfj\u00e4hrig, kaum \u00e4lter.", "tokens": ["Zw\u00f6lf\u00b7j\u00e4h\u00b7rig", ",", "kaum", "\u00e4l\u00b7ter", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Unendlich traurig", "tokens": ["Un\u00b7end\u00b7lich", "trau\u00b7rig"], "token_info": ["word", "word"], "pos": ["ADJD", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Sah es mit gro\u00dfen blauen Augen", "tokens": ["Sah", "es", "mit", "gro\u00b7\u00dfen", "blau\u00b7en", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "In seinen Scho\u00df,", "tokens": ["In", "sei\u00b7nen", "Scho\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Die einzige Betr\u00fcbte in unserem heitern,", "tokens": ["Die", "ein\u00b7zi\u00b7ge", "Be\u00b7tr\u00fcb\u00b7te", "in", "un\u00b7se\u00b7rem", "hei\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "$,"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.14": {"text": "Scherzbelebten Kreis.", "tokens": ["Scherz\u00b7be\u00b7leb\u00b7ten", "Kreis", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Was fehlt dir Alice?", "tokens": ["Was", "fehlt", "dir", "A\u00b7li\u00b7ce", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Warum denn so still heute?", "tokens": ["Wa\u00b7rum", "denn", "so", "still", "heu\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ADJD", "ADV", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Ach, so klang es von rosigen Kinderlippen,", "tokens": ["Ach", ",", "so", "klang", "es", "von", "ro\u00b7si\u00b7gen", "Kin\u00b7der\u00b7lip\u00b7pen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Ich bin so schwerm\u00fctig heute \u2013", "tokens": ["Ich", "bin", "so", "schwer\u00b7m\u00fc\u00b7tig", "heu\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ADV", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ich trage Gedichte.", "tokens": ["Ich", "tra\u00b7ge", "Ge\u00b7dich\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Was? du tr\u00e4gst Gedichte, Alice?", "tokens": ["Was", "?", "du", "tr\u00e4gst", "Ge\u00b7dich\u00b7te", ",", "A\u00b7li\u00b7ce", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "$.", "PPER", "VVFIN", "NN", "$,", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Und endloses Gel\u00e4chter umschwirrte dich,", "tokens": ["Und", "end\u00b7lo\u00b7ses", "Ge\u00b7l\u00e4ch\u00b7ter", "um\u00b7schwirr\u00b7te", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "\u00dcberm\u00fctig,", "tokens": ["\u00dc\u00b7berm\u00b7\u00fc\u00b7tig", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Wie ausgelassene Tagv\u00f6gel", "tokens": ["Wie", "aus\u00b7ge\u00b7las\u00b7se\u00b7ne", "Tag\u00b7v\u00f6\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJA", "NN"], "meter": "-+-+--++-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die alte ernste, unzufriedene Eule umspotten.", "tokens": ["Die", "al\u00b7te", "erns\u00b7te", ",", "un\u00b7zu\u00b7frie\u00b7de\u00b7ne", "Eu\u00b7le", "um\u00b7spot\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Ich trage Gedichte...", "tokens": ["Ich", "tra\u00b7ge", "Ge\u00b7dich\u00b7te", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Wachend h\u00f6r' ich immer noch", "tokens": ["Wa\u00b7chend", "h\u00f6r'", "ich", "im\u00b7mer", "noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Diese zaghafte, traurige Antwort,", "tokens": ["Die\u00b7se", "zag\u00b7haf\u00b7te", ",", "trau\u00b7ri\u00b7ge", "Ant\u00b7wort", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Die mich so tief r\u00fchrte,", "tokens": ["Die", "mich", "so", "tief", "r\u00fchr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.10": {"text": "Aus Kindermund so tief r\u00fchrte.", "tokens": ["Aus", "Kin\u00b7der\u00b7mund", "so", "tief", "r\u00fchr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}}, "stanza.4": {"line.1": {"text": "Ich trage Gedichte...", "tokens": ["Ich", "tra\u00b7ge", "Ge\u00b7dich\u00b7te", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Was wissen die anderen,", "tokens": ["Was", "wis\u00b7sen", "die", "an\u00b7de\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Leicht frohen Alltagsseelen,", "tokens": ["Leicht", "fro\u00b7hen", "All\u00b7tags\u00b7see\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie einem zu Mute ist,", "tokens": ["Wie", "ei\u00b7nem", "zu", "Mu\u00b7te", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "APPR", "NN", "VAFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wie uns beiden zu Mute ist, Alice,", "tokens": ["Wie", "uns", "bei\u00b7den", "zu", "Mu\u00b7te", "ist", ",", "A\u00b7li\u00b7ce", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "APPR", "NN", "VAFIN", "$,", "NE", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Wenn wir Gedichte tragen.", "tokens": ["Wenn", "wir", "Ge\u00b7dich\u00b7te", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wie weh, wie krank unsere Seele sein kann,", "tokens": ["Wie", "weh", ",", "wie", "krank", "un\u00b7se\u00b7re", "See\u00b7le", "sein", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "PPOSAT", "NN", "VAINF", "VMFIN", "$,"], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Wenn's drin keimt,", "tokens": ["Wenn's", "drin", "keimt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Wenn's drin zuckt,", "tokens": ["Wenn's", "drin", "zuckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "Mit ersten leisen Regungen,", "tokens": ["Mit", "ers\u00b7ten", "lei\u00b7sen", "Re\u00b7gun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "In Schmerzen empfangen,", "tokens": ["In", "Schmer\u00b7zen", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Mit Schmerzen geboren,", "tokens": ["Mit", "Schmer\u00b7zen", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Seele von unserer Seele,", "tokens": ["See\u00b7le", "von", "un\u00b7se\u00b7rer", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.14": {"text": "Blut von unserem Blut.", "tokens": ["Blut", "von", "un\u00b7se\u00b7rem", "Blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Kleine schmerzdurchzuckte Dichterin,", "tokens": ["Klei\u00b7ne", "schmerz\u00b7durch\u00b7zuck\u00b7te", "Dich\u00b7te\u00b7rin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Freue dich.", "tokens": ["Freu\u00b7e", "dich", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Dein Reich war der Traum.", "tokens": ["Dein", "Reich", "war", "der", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Die Sonne des Morgens k\u00fcsste dich auf,", "tokens": ["Die", "Son\u00b7ne", "des", "Mor\u00b7gens", "k\u00fcss\u00b7te", "dich", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Dich und deine Schmerzen,", "tokens": ["Dich", "und", "dei\u00b7ne", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Wie den Nachttau von den Bl\u00e4ttern der Blumen,", "tokens": ["Wie", "den", "Nacht\u00b7tau", "von", "den", "Bl\u00e4t\u00b7tern", "der", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+---+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Denen du in ernster Lieblichkeit glichst.", "tokens": ["De\u00b7nen", "du", "in", "erns\u00b7ter", "Lieb\u00b7lich\u00b7keit", "glichst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.6": {"line.1": {"text": "Ich aber lebe.", "tokens": ["Ich", "a\u00b7ber", "le\u00b7be", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Mein Tag ist kein Traum,", "tokens": ["Mein", "Tag", "ist", "kein", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und wenn ich schwerm\u00fctig bin", "tokens": ["Und", "wenn", "ich", "schwer\u00b7m\u00fc\u00b7tig", "bin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VAFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und Gedichte trage,", "tokens": ["Und", "Ge\u00b7dich\u00b7te", "tra\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Darf ich's nicht einmal sagen am Theetisch.", "tokens": ["Darf", "ich's", "nicht", "ein\u00b7mal", "sa\u00b7gen", "am", "Thee\u00b7tisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PTKNEG", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Sie w\u00fcrden mich auslachen,", "tokens": ["Sie", "w\u00fcr\u00b7den", "mich", "aus\u00b7la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wie sie dich auslachten,", "tokens": ["Wie", "sie", "dich", "aus\u00b7lach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVIZU", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Nur thut's noch zehnmal weher,", "tokens": ["Nur", "thut's", "noch", "zehn\u00b7mal", "we\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Am hellen, wirklichen Tage ausgelacht zu werden,", "tokens": ["Am", "hel\u00b7len", ",", "wirk\u00b7li\u00b7chen", "Ta\u00b7ge", "aus\u00b7ge\u00b7lacht", "zu", "wer\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN", "VVPP", "PTKZU", "VAINF", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Und unsere Schmerzen", "tokens": ["Und", "un\u00b7se\u00b7re", "Schmer\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Sind ihnen immer l\u00e4cherlich.", "tokens": ["Sind", "ih\u00b7nen", "im\u00b7mer", "l\u00e4\u00b7cher\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Sie verstehen uns nicht.", "tokens": ["Sie", "ver\u00b7ste\u00b7hen", "uns", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "Wie sch\u00f6n, sagen sie, dichten zu k\u00f6nnen,", "tokens": ["Wie", "sch\u00f6n", ",", "sa\u00b7gen", "sie", ",", "dich\u00b7ten", "zu", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "VVFIN", "PPER", "$,", "ADJA", "PTKZU", "VMINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Wenn wir es doch auch k\u00f6nnten.", "tokens": ["Wenn", "wir", "es", "doch", "auch", "k\u00f6nn\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Ist es sehr schwer mein Herr?", "tokens": ["Ist", "es", "sehr", "schwer", "mein", "Herr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}