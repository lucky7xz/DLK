{"textgrid.poem.37886": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "R\u00e4thsel", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist die wundersch\u00f6nste Br\u00fcck,", "tokens": ["Es", "ist", "die", "wun\u00b7der\u00b7sch\u00f6ns\u00b7te", "Br\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dar\u00fcber noch kein Mensch gegangen,", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "noch", "kein", "Mensch", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch ist daran ein seltsam St\u00fcck,", "tokens": ["Doch", "ist", "da\u00b7ran", "ein", "selt\u00b7sam", "St\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PAV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df \u00fcber ihr die Wasser hangen,", "tokens": ["Da\u00df", "\u00fc\u00b7ber", "ihr", "die", "Was\u00b7ser", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und unter ihr die Leute gehn", "tokens": ["Und", "un\u00b7ter", "ihr", "die", "Leu\u00b7te", "gehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ganz trocken, und sie froh ansehn,", "tokens": ["Ganz", "tro\u00b7cken", ",", "und", "sie", "froh", "an\u00b7sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KON", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Schiffe segelnd durch sie ziehn,", "tokens": ["Die", "Schif\u00b7fe", "se\u00b7gelnd", "durch", "sie", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die V\u00f6gel sie durchfliegen k\u00fchn;", "tokens": ["Die", "V\u00f6\u00b7gel", "sie", "durch\u00b7flie\u00b7gen", "k\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Doch stehet sie im Sturme fest,", "tokens": ["Doch", "ste\u00b7het", "sie", "im", "Stur\u00b7me", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Kein Zoll noch Weggeld zahlen l\u00e4\u00dft.", "tokens": ["Kein", "Zoll", "noch", "Weg\u00b7geld", "zah\u00b7len", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}