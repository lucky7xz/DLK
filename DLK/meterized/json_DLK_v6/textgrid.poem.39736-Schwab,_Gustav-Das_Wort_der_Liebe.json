{"textgrid.poem.39736": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Das Wort der Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O aller Berge Quellen,", "tokens": ["O", "al\u00b7ler", "Ber\u00b7ge", "Quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "T\u00f6nt mit berauschten Wellen", "tokens": ["T\u00f6nt", "mit", "be\u00b7rauschten", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Vernehmlich durch die Luft!", "tokens": ["Ver\u00b7nehm\u00b7lich", "durch", "die", "Luft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "O aller Th\u00e4ler B\u00e4ume,", "tokens": ["O", "al\u00b7ler", "Th\u00e4\u00b7ler", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "S\u00e4uselt mir leise Tr\u00e4ume,", "tokens": ["S\u00e4u\u00b7selt", "mir", "lei\u00b7se", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Und sendet s\u00fc\u00dfen Duft!", "tokens": ["Und", "sen\u00b7det", "s\u00fc\u00b7\u00dfen", "Duft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Es sollen alle Sinne", "tokens": ["Es", "sol\u00b7len", "al\u00b7le", "Sin\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Freude werden inne,", "tokens": ["Der", "Freu\u00b7de", "wer\u00b7den", "in\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAINF", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die heut mein Herz begeht,", "tokens": ["Die", "heut", "mein", "Herz", "be\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In allen Farben, T\u00f6nen", "tokens": ["In", "al\u00b7len", "Far\u00b7ben", ",", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Lebe das Wort der Sch\u00f6nen,", "tokens": ["Le\u00b7be", "das", "Wort", "der", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Das mir im Geiste steht!", "tokens": ["Das", "mir", "im", "Geis\u00b7te", "steht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Liebe Wort, das zitternd", "tokens": ["Der", "Lie\u00b7be", "Wort", ",", "das", "zit\u00b7ternd"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und inniglich ersch\u00fctternd", "tokens": ["Und", "in\u00b7nig\u00b7lich", "er\u00b7sch\u00fct\u00b7ternd"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Durch meine Seele dringt,", "tokens": ["Durch", "mei\u00b7ne", "See\u00b7le", "dringt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In ew'gen Wiederhallen", "tokens": ["In", "ew'\u00b7gen", "Wie\u00b7der\u00b7hal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "H\u00f6r' ich es rings erschallen,", "tokens": ["H\u00f6r'", "ich", "es", "rings", "er\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So da\u00df es nie verklingt.", "tokens": ["So", "da\u00df", "es", "nie", "ver\u00b7klingt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und wenn die Quellen schweigen", "tokens": ["Und", "wenn", "die", "Quel\u00b7len", "schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und wenn die B\u00e4ume neigen", "tokens": ["Und", "wenn", "die", "B\u00e4u\u00b7me", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Haupt in welker Zier;", "tokens": ["Ihr", "Haupt", "in", "wel\u00b7ker", "Zier", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PWAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Im Herzen ewig klingen,", "tokens": ["Im", "Her\u00b7zen", "e\u00b7wig", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bl\u00fchen und lieblich singen", "tokens": ["Bl\u00fc\u00b7hen", "und", "lieb\u00b7lich", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Wird doch das Wort von Ihr.", "tokens": ["Wird", "doch", "das", "Wort", "von", "Ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}