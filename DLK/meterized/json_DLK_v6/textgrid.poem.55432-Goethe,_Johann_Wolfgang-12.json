{"textgrid.poem.55432": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "12", "genre": "verse", "period": "N.A.", "pub_year": 1812, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus jenen L\u00e4ndern echten Sonnenscheines", "tokens": ["Aus", "je\u00b7nen", "L\u00e4n\u00b7dern", "ech\u00b7ten", "Son\u00b7nen\u00b7schei\u00b7nes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Begl\u00fcckten oft mich Gaben der Gefilde:", "tokens": ["Be\u00b7gl\u00fcck\u00b7ten", "oft", "mich", "Ga\u00b7ben", "der", "Ge\u00b7fil\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Agrumen reizend, Feigen s\u00fc\u00df und milde,", "tokens": ["Ag\u00b7ru\u00b7men", "rei\u00b7zend", ",", "Fei\u00b7gen", "s\u00fc\u00df", "und", "mil\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Mandeln Milch, die Feuerkraft des Weines.", "tokens": ["Der", "Man\u00b7deln", "Milch", ",", "die", "Feu\u00b7er\u00b7kraft", "des", "Wei\u00b7nes", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So manches Musenwerk erregte meines", "tokens": ["So", "man\u00b7ches", "Mu\u00b7sen\u00b7werk", "er\u00b7reg\u00b7te", "mei\u00b7nes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nordl\u00e4nd'schen Geistes innigste Gebilde,", "tokens": ["Nord\u00b7l\u00e4n\u00b7d'\u00b7schen", "Geis\u00b7tes", "in\u00b7nigs\u00b7te", "Ge\u00b7bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wie an Achilleus' lebensreichem Schilde", "tokens": ["Wie", "an", "A\u00b7chil\u00b7leus'", "le\u00b7bens\u00b7rei\u00b7chem", "Schil\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NE", "ADJA", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Erfreut ich mich des g\u00fcnstigsten Vereines.", "tokens": ["Er\u00b7freut", "ich", "mich", "des", "g\u00fcns\u00b7tigs\u00b7ten", "Ver\u00b7ei\u00b7nes", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und da\u00df ich mich daran begn\u00fcgen k\u00f6nnte,", "tokens": ["Und", "da\u00df", "ich", "mich", "da\u00b7ran", "be\u00b7gn\u00fc\u00b7gen", "k\u00f6nn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "PAV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "War mir sogar ein Kunstbesitz bereitet,", "tokens": ["War", "mir", "so\u00b7gar", "ein", "Kunst\u00b7be\u00b7sitz", "be\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erquickend mich durch Anmut wie durch St\u00e4rke.", "tokens": ["Er\u00b7qui\u00b7ckend", "mich", "durch", "An\u00b7mut", "wie", "durch", "St\u00e4r\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "APPR", "NN", "KOKOM", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch nichts erschien im gr\u00f6\u00dferen Momente,", "tokens": ["Doch", "nichts", "er\u00b7schien", "im", "gr\u00f6\u00b7\u00dfe\u00b7ren", "Mo\u00b7men\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Voll innern Werts, von so viel Gl\u00fcck begleitet", "tokens": ["Voll", "in\u00b7nern", "Werts", ",", "von", "so", "viel", "Gl\u00fcck", "be\u00b7glei\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "$,", "APPR", "ADV", "PIAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als durch ", "tokens": ["Als", "durch"], "token_info": ["word", "word"], "pos": ["KOUS", "APPR"], "meter": "-+", "measure": "iambic.single"}}}}}