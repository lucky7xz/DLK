{"dta.poem.20927": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sittenore an Friedenheim.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Freund/ ach gute Nacht! was sag ich gut?", "tokens": ["Mein", "Freund", "/", "ach", "gu\u00b7te", "Nacht", "!", "was", "sag", "ich", "gut", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "XY", "ADJA", "NN", "$.", "PWS", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "was meine?", "tokens": ["was", "mei\u00b7ne", "?"], "token_info": ["word", "word", "punct"], "pos": ["PWS", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Weil du mich hassen solst/ und ich dich lassen", "tokens": ["Weil", "du", "mich", "has\u00b7sen", "solst", "/", "und", "ich", "dich", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$(", "KON", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "mu\u00df?", "tokens": ["mu\u00df", "?"], "token_info": ["word", "punct"], "pos": ["VMFIN", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Der Himmel wolle doch da\u00df meine Feder weine/", "tokens": ["Der", "Him\u00b7mel", "wol\u00b7le", "doch", "da\u00df", "mei\u00b7ne", "Fe\u00b7der", "wei\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "KOUS", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und dir verk\u00fcndige des Jammers Uberflu\u00df.", "tokens": ["Und", "dir", "ver\u00b7k\u00fcn\u00b7di\u00b7ge", "des", "Jam\u00b7mers", "U\u00b7ber\u00b7flu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein Wetter voller Angst zeucht \u00fcber mir zusammen/", "tokens": ["Ein", "Wet\u00b7ter", "vol\u00b7ler", "Angst", "zeucht", "\u00fc\u00b7ber", "mir", "zu\u00b7sam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Es st\u00fcrmt das Ungemach aus Nord/ Sud/ Ost u. West/", "tokens": ["Es", "st\u00fcrmt", "das", "Un\u00b7ge\u00b7mach", "aus", "Nord", "/", "Sud", "/", "Ost", "u.", "West", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "abbreviation", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NE", "$(", "NE", "$(", "NN", "APPR", "NE", "$("], "meter": "-+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.9": {"text": "Ich schaue sonsten nichts/ als Donner/ Blitz un\u0303 Flam-", "tokens": ["Ich", "schau\u00b7e", "sons\u00b7ten", "nichts", "/", "als", "Don\u00b7ner", "/", "Blitz", "u\u00f1", "Flam"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "$(", "KOUS", "NN", "$(", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "men/", "tokens": ["men", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.11": {"text": "Ach da\u00df der Himmel mich dergleichen melden l\u00e4st!", "tokens": ["Ach", "da\u00df", "der", "Him\u00b7mel", "mich", "derg\u00b7lei\u00b7chen", "mel\u00b7den", "l\u00e4st", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "ART", "NN", "PPER", "PIS", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der angenehme Brief/ den du mir hast geschrieben/", "tokens": ["Der", "an\u00b7ge\u00b7neh\u00b7me", "Brief", "/", "den", "du", "mir", "hast", "ge\u00b7schrie\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "PPER", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ligt itzt zu unsern Spott in Bruder Carles Handt/", "tokens": ["Ligt", "itzt", "zu", "un\u00b7sern", "Spott", "in", "Bru\u00b7der", "Car\u00b7les", "Handt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "APPR", "NN", "NE", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Dein Brief und meine Brust verrathen unser lieben/", "tokens": ["Dein", "Brief", "und", "mei\u00b7ne", "Brust", "ver\u00b7ra\u00b7then", "un\u00b7ser", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVFIN", "PPOSAT", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und was verborgen lag/ wird aller Welt bekannt.", "tokens": ["Und", "was", "ver\u00b7bor\u00b7gen", "lag", "/", "wird", "al\u00b7ler", "Welt", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVPP", "VVFIN", "$(", "VAFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Dein Schreiben schw\u00e4rtzt der Hof mit giftigen Ge-", "tokens": ["Dein", "Schrei\u00b7ben", "schw\u00e4rtzt", "der", "Hof", "mit", "gif\u00b7ti\u00b7gen", "Ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "dancken/", "tokens": ["dan\u00b7cken", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.18": {"text": "Der Neid geust \u00fcberall gef\u00e4hrlich Oele bey/", "tokens": ["Der", "Neid", "geust", "\u00fc\u00b7be\u00b7rall", "ge\u00b7f\u00e4hr\u00b7lich", "O\u00b7e\u00b7le", "bey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "ADV", "ADJD", "NN", "APPR", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.19": {"text": "Es glaubt nicht iederman/ da\u00df unsrer Liebe Schran-", "tokens": ["Es", "glaubt", "nicht", "ie\u00b7der\u00b7man", "/", "da\u00df", "uns\u00b7rer", "Lie\u00b7be", "Schran"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "PIS", "$(", "KOUS", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "cken/", "tokens": ["cken", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "-", "measure": "single.down"}, "line.21": {"text": "Zugleich ein Paradie\u00df der Lust gewesen sey.", "tokens": ["Zu\u00b7gleich", "ein", "Pa\u00b7ra\u00b7die\u00df", "der", "Lust", "ge\u00b7we\u00b7sen", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Man kr\u00e4ncket Silb\u2019 und Wort mit doppelten Ver-", "tokens": ["Man", "kr\u00e4n\u00b7cket", "Silb'", "und", "Wort", "mit", "dop\u00b7pel\u00b7ten", "Ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "NN", "KON", "NN", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "stande/", "tokens": ["stan\u00b7de", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.24": {"text": "Man leget ab und zu/ und prest die Falschheit aus/", "tokens": ["Man", "le\u00b7get", "ab", "und", "zu", "/", "und", "prest", "die", "Falschheit", "aus", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKVZ", "KON", "PTKZU", "$(", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Dein Lieben heist man List/ und meines heist man", "tokens": ["Dein", "Lie\u00b7ben", "heist", "man", "List", "/", "und", "mei\u00b7nes", "heist", "man"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "VVFIN", "PIS", "NN", "$(", "KON", "PPOSAT", "VVFIN", "PIS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Schande;", "tokens": ["Schan\u00b7de", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.27": {"text": "Ja vieler Hoffarth nach/ befleckst du unser Hau\u00df;", "tokens": ["Ja", "vie\u00b7ler", "Hof\u00b7farth", "nach", "/", "be\u00b7fleckst", "du", "un\u00b7ser", "Hau\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PIAT", "NN", "APPR", "$(", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Es schl\u00e4gt der gantze Hof f\u00fcr mir die Augen nieder/", "tokens": ["Es", "schl\u00e4gt", "der", "gant\u00b7ze", "Hof", "f\u00fcr", "mir", "die", "Au\u00b7gen", "nie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPER", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Mein Frauen Zimmer selbst spricht mich mit Furchten", "tokens": ["Mein", "Frau\u00b7en", "Zim\u00b7mer", "selbst", "spricht", "mich", "mit", "Furch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "ADV", "VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.30": {"text": "an/", "tokens": ["an", "/"], "token_info": ["word", "punct"], "pos": ["APPR", "$("], "meter": "+", "measure": "single.up"}, "line.31": {"text": "Es scheinet Sonn und Luft die werden mir zuwider/", "tokens": ["Es", "schei\u00b7net", "Sonn", "und", "Luft", "die", "wer\u00b7den", "mir", "zu\u00b7wi\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "ART", "VAFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Doch bleibet di\u00df mein Trost/ ich habe nichts gethan.", "tokens": ["Doch", "blei\u00b7bet", "di\u00df", "mein", "Trost", "/", "ich", "ha\u00b7be", "nichts", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "PPOSAT", "NN", "$(", "PPER", "VAFIN", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "O hartes Donner Wort/ ich soll dich g\u00e4ntzlich hassen/", "tokens": ["O", "har\u00b7tes", "Don\u00b7ner", "Wort", "/", "ich", "soll", "dich", "g\u00e4ntz\u00b7lich", "has\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "NN", "$(", "PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Du solst forthin nicht mehr ins K\u00f6nigs Zimmer gehn!", "tokens": ["Du", "solst", "for\u00b7thin", "nicht", "mehr", "ins", "K\u00f6\u00b7nigs", "Zim\u00b7mer", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "ADV", "APPRART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ach wolte mich der Arm des Todes doch umbfassen/", "tokens": ["Ach", "wol\u00b7te", "mich", "der", "Arm", "des", "To\u00b7des", "doch", "umb\u00b7fas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PRF", "ART", "NN", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und k\u00f6nt ich in der Gruft der lieben Aeltern stehn!", "tokens": ["Und", "k\u00f6nt", "ich", "in", "der", "Gruft", "der", "lie\u00b7ben", "A\u00b7el\u00b7tern", "stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.37": {"text": "Hand und auch Feder sinckt aus Schwachheit zu der", "tokens": ["Hand", "und", "auch", "Fe\u00b7der", "sinckt", "aus", "Schwach\u00b7heit", "zu", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADV", "NN", "VVFIN", "APPR", "NN", "APPR", "ART"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.38": {"text": "Erden/", "tokens": ["Er\u00b7den", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.39": {"text": "Ich mercke wie die Kraft zum Schreiben mir gebricht/", "tokens": ["Ich", "mer\u00b7cke", "wie", "die", "Kraft", "zum", "Schrei\u00b7ben", "mir", "ge\u00b7bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "APPRART", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und so die Tinte mir zu fahl beginnt zuwerden/", "tokens": ["Und", "so", "die", "Tin\u00b7te", "mir", "zu", "fahl", "be\u00b7ginnt", "zu\u00b7wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PPER", "PTKA", "ADJD", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "So dencke nur sie wird au\u00df Thr\u00e4nen zugericht.", "tokens": ["So", "den\u00b7cke", "nur", "sie", "wird", "au\u00df", "Thr\u00e4\u00b7nen", "zu\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Es scheidet uns die Noth: du solt in Deutschland rei-", "tokens": ["Es", "schei\u00b7det", "uns", "die", "Noth", ":", "du", "solt", "in", "Deutschland", "rei"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$.", "PPER", "VMFIN", "APPR", "NE", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "sen/", "tokens": ["sen", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "-", "measure": "single.down"}, "line.44": {"text": "Und ich soll ohne dich in meines Brudern Landt/", "tokens": ["Und", "ich", "soll", "oh\u00b7ne", "dich", "in", "mei\u00b7nes", "Bru\u00b7dern", "Landt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "APPR", "PRF", "APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Kanst du nicht Leit Stern seyn und mir die Strasse", "tokens": ["Kanst", "du", "nicht", "Leit", "Stern", "seyn", "und", "mir", "die", "Stras\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PTKNEG", "NN", "NN", "VAINF", "KON", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.46": {"text": "weisen/", "tokens": ["wei\u00b7sen", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.47": {"text": "So lauft mein schwaches Schiff auf Klippen und", "tokens": ["So", "lauft", "mein", "schwa\u00b7ches", "Schiff", "auf", "Klip\u00b7pen", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "NN", "KON"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.48": {"text": "auf Sand.", "tokens": ["auf", "Sand", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.49": {"text": "Mir tr\u00e4umet albereit von Brausen/ Sturm und Wel-", "tokens": ["Mir", "tr\u00e4u\u00b7met", "al\u00b7be\u00b7reit", "von", "Brau\u00b7sen", "/", "Sturm", "und", "Wel"]