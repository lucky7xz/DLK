{"textgrid.poem.64336": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "57. An den Verfasser von Stillings Jugend", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem B\u00fcchlein dein bin ich gar hold;", "tokens": ["Dem", "B\u00fcch\u00b7lein", "dein", "bin", "ich", "gar", "hold", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist's doch so rein wie lauter Gold,", "tokens": ["Ist's", "doch", "so", "rein", "wie", "lau\u00b7ter", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "ADJD", "KOKOM", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Voll Unschuld, liebevoll und wahr,", "tokens": ["Voll", "Un\u00b7schuld", ",", "lie\u00b7be\u00b7voll", "und", "wahr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wie der Morgentau so klar:", "tokens": ["Und", "wie", "der", "Mor\u00b7gen\u00b7tau", "so", "klar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der auf dem jungen Bl\u00fcmchen bebt,", "tokens": ["Der", "auf", "dem", "jun\u00b7gen", "Bl\u00fcm\u00b7chen", "bebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und seine Bl\u00e4tter neu belebt;", "tokens": ["Und", "sei\u00b7ne", "Bl\u00e4t\u00b7ter", "neu", "be\u00b7lebt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Tr\u00f6pfchen schimmert hell und mild", "tokens": ["Im", "Tr\u00f6pf\u00b7chen", "schim\u00b7mert", "hell", "und", "mild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Morgensonne Flammenbild:", "tokens": ["Der", "Mor\u00b7gen\u00b7son\u00b7ne", "Flam\u00b7men\u00b7bild", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So spiegelt auch dein B\u00fcchlein klein", "tokens": ["So", "spie\u00b7gelt", "auch", "dein", "B\u00fcch\u00b7lein", "klein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der h\u00f6chsten Weisheit Himmelschein,", "tokens": ["Der", "h\u00f6chs\u00b7ten", "Weis\u00b7heit", "Him\u00b7mel\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und tr\u00e4nket freundlich unser Herz", "tokens": ["Und", "tr\u00e4n\u00b7ket", "freund\u00b7lich", "un\u00b7ser", "Herz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit ernster Freud' und s\u00fc\u00dfem Schmerz.", "tokens": ["Mit", "erns\u00b7ter", "Freud'", "und", "s\u00fc\u00b7\u00dfem", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich lebte gern im stillen Thal", "tokens": ["Ich", "leb\u00b7te", "gern", "im", "stil\u00b7len", "Thal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit deinen Menschen allzumal,", "tokens": ["Mit", "dei\u00b7nen", "Men\u00b7schen", "all\u00b7zu\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich s\u00e4nge Wald und Strom und Au,", "tokens": ["Ich", "s\u00e4n\u00b7ge", "Wald", "und", "Strom", "und", "Au", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und n\u00e4hme Dortchen mir zur Frau.", "tokens": ["Und", "n\u00e4h\u00b7me", "Dort\u00b7chen", "mir", "zur", "Frau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dein weiser frommer Nikolas", "tokens": ["Dein", "wei\u00b7ser", "from\u00b7mer", "Ni\u00b7ko\u00b7las"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sah nicht durch ein getr\u00fcbtes Glas,", "tokens": ["Sah", "nicht", "durch", "ein", "ge\u00b7tr\u00fcb\u00b7tes", "Glas", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie mancher Pfaff den Sonnenschein", "tokens": ["Wie", "man\u00b7cher", "Pfaff", "den", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erhellt mit der Laterne sein.", "tokens": ["Er\u00b7hellt", "mit", "der", "La\u00b7ter\u00b7ne", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Einfalt und der Liebe Sinn", "tokens": ["Der", "Ein\u00b7falt", "und", "der", "Lie\u00b7be", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sei unser Kleinod und Gewinn!", "tokens": ["Sei", "un\u00b7ser", "Klei\u00b7nod", "und", "Ge\u00b7winn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie reichen uns den Pilgerstab,", "tokens": ["Sie", "rei\u00b7chen", "uns", "den", "Pil\u00b7ger\u00b7stab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und f\u00fchren l\u00e4cheln uns ins Grab.", "tokens": ["Und", "f\u00fch\u00b7ren", "l\u00e4\u00b7cheln", "uns", "ins", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}