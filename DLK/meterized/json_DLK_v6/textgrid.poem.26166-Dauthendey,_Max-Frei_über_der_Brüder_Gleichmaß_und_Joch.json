{"textgrid.poem.26166": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Frei \u00fcber der Br\u00fcder Gleichma\u00df und Joch", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Walddach steht glatt wie mit Sensen geschnitten,", "tokens": ["Das", "Wald\u00b7dach", "steht", "glatt", "wie", "mit", "Sen\u00b7sen", "ge\u00b7schnit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KOKOM", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Als sei ein M\u00e4her hoch \u00fcber die Wipfel geschritten.", "tokens": ["Als", "sei", "ein", "M\u00e4\u00b7her", "hoch", "\u00fc\u00b7ber", "die", "Wip\u00b7fel", "ge\u00b7schrit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ART", "NN", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nur eine einzige Eiche h\u00f6her als die andern ragt,", "tokens": ["Nur", "ei\u00b7ne", "ein\u00b7zi\u00b7ge", "Ei\u00b7che", "h\u00f6\u00b7her", "als", "die", "an\u00b7dern", "ragt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ADJD", "KOKOM", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Wie ein Kopf, an den sich das Messer nicht gewagt.", "tokens": ["Wie", "ein", "Kopf", ",", "an", "den", "sich", "das", "Mes\u00b7ser", "nicht", "ge\u00b7wagt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "APPR", "PRELS", "PRF", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der streckt sich sehend in den Abend hoch,", "tokens": ["Der", "streckt", "sich", "se\u00b7hend", "in", "den", "A\u00b7bend", "hoch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADJD", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Frei \u00fcber der Br\u00fcder Gleichma\u00df und Joch.", "tokens": ["Frei", "\u00fc\u00b7ber", "der", "Br\u00fc\u00b7der", "Gleich\u00b7ma\u00df", "und", "Joch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "+---+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Und selbst der Blitz darf ihm am Stamm hinschaben,", "tokens": ["Und", "selbst", "der", "Blitz", "darf", "ihm", "am", "Stamm", "hin\u00b7scha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VMFIN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Als k\u00f6nnen ihm die t\u00f6dlichsten Feuer nichts anhaben,", "tokens": ["Als", "k\u00f6n\u00b7nen", "ihm", "die", "t\u00f6d\u00b7lichs\u00b7ten", "Feu\u00b7er", "nichts", "an\u00b7ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "ART", "ADJA", "NN", "PIS", "VVINF", "$,"], "meter": "-+---+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Solange allj\u00e4hrlich die Fr\u00fchlingslust noch umarmt den Riesenknaben.", "tokens": ["So\u00b7lan\u00b7ge", "all\u00b7j\u00e4hr\u00b7lich", "die", "Fr\u00fch\u00b7lings\u00b7lust", "noch", "um\u00b7armt", "den", "Rie\u00b7sen\u00b7kna\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-+--+-+-+-", "measure": "amphibrach.tri.plus"}}}}}