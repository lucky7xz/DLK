{"textgrid.poem.51355": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Durch fliehn entflieh ich nicht,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch fliehn entflieh ich nicht,", "tokens": ["Durch", "fliehn", "ent\u00b7flieh", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Wann ich durch Wind und Wellen", "tokens": ["Wann", "ich", "durch", "Wind", "und", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gleich meinen Lauff wil stellen,", "tokens": ["Gleich", "mei\u00b7nen", "Lauff", "wil", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Folgt doch das sch\u00f6ne Licht:", "tokens": ["Folgt", "doch", "das", "sch\u00f6\u00b7ne", "Licht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Durch Berge, Thal und Wald", "tokens": ["Durch", "Ber\u00b7ge", ",", "Thal", "und", "Wald"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Seh ich stets vor mir stehen,", "tokens": ["Seh", "ich", "stets", "vor", "mir", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Seh ich stets vor mir gehen", "tokens": ["Seh", "ich", "stets", "vor", "mir", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Die freundliche Gestalt.", "tokens": ["Die", "freund\u00b7li\u00b7che", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Mich m\u00fcst ich selber fliehn,", "tokens": ["Mich", "m\u00fcst", "ich", "sel\u00b7ber", "fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Dieweil hier steckt im Hertzen,", "tokens": ["Die\u00b7weil", "hier", "steckt", "im", "Hert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Dein Bild voll Liebes Schmertzen,", "tokens": ["Dein", "Bild", "voll", "Lie\u00b7bes", "Schmert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Dem ich mich wil entziehn.", "tokens": ["Dem", "ich", "mich", "wil", "ent\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "VMFIN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}