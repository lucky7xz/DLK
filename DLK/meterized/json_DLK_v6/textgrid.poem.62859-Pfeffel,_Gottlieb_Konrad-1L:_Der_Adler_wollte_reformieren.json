{"textgrid.poem.62859": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Adler wollte reformieren", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Adler wollte reformieren", "tokens": ["Der", "Ad\u00b7ler", "woll\u00b7te", "re\u00b7for\u00b7mie\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und schaffte die Polygamie", "tokens": ["Und", "schaff\u00b7te", "die", "Po\u00b7ly\u00b7ga\u00b7mie"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bey dem gesammten Federvieh", "tokens": ["Bey", "dem", "ge\u00b7samm\u00b7ten", "Fe\u00b7der\u00b7vieh"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf einmal ab. Den armen Thieren", "tokens": ["Auf", "ein\u00b7mal", "ab", ".", "Den", "ar\u00b7men", "Thie\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADV", "PTKVZ", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mi\u00dffiel die strenge Polizey,", "tokens": ["Mi\u00df\u00b7fiel", "die", "stren\u00b7ge", "Po\u00b7li\u00b7zey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zumal dem Hahn. Er trat herbey,", "tokens": ["Zu\u00b7mal", "dem", "Hahn", ".", "Er", "trat", "her\u00b7bey", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$.", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Um feyerlich zu protestieren,", "tokens": ["Um", "fey\u00b7er\u00b7lich", "zu", "pro\u00b7tes\u00b7tie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und von des K\u00f6nigs Majest\u00e4t", "tokens": ["Und", "von", "des", "K\u00f6\u00b7nigs", "Ma\u00b7jes\u00b7t\u00e4t"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "An die Natur zu appellieren.", "tokens": ["An", "die", "Na\u00b7tur", "zu", "ap\u00b7pel\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Er schlug mir Macht, wie ein Prophet,", "tokens": ["Er", "schlug", "mir", "Macht", ",", "wie", "ein", "Pro\u00b7phet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Dem neuen Solon ans Gewissen,", "tokens": ["Dem", "neu\u00b7en", "So\u00b7lon", "ans", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.12": {"text": "Und sprach mit sanfter Energie", "tokens": ["Und", "sprach", "mit", "sanf\u00b7ter", "E\u00b7ner\u00b7gie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Von seiner Weiber Harmonie.", "tokens": ["Von", "sei\u00b7ner", "Wei\u00b7ber", "Har\u00b7mo\u00b7nie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Hier ward der Sultan hingerissen.", "tokens": ["Hier", "ward", "der", "Sul\u00b7tan", "hin\u00b7ge\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Wohlan, ich kann nicht widerstehn,", "tokens": ["Wo\u00b7hlan", ",", "ich", "kann", "nicht", "wi\u00b7der\u00b7stehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Rief er, ich mu\u00df dein Harem sehn.", "tokens": ["Rief", "er", ",", "ich", "mu\u00df", "dein", "Ha\u00b7rem", "sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Er folgt ihm huldreich aus dem Hayne", "tokens": ["Er", "folgt", "ihm", "huld\u00b7reich", "aus", "dem", "Hay\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "In einen Hof. Der Patriarch", "tokens": ["In", "ei\u00b7nen", "Hof", ".", "Der", "Pat\u00b7ri\u00b7arch"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Lockt seinen Hennen. Der Monarch", "tokens": ["Lockt", "sei\u00b7nen", "Hen\u00b7nen", ".", "Der", "Mon\u00b7arch"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "Verschlang sie alle bis auf eine", "tokens": ["Ver\u00b7schlang", "sie", "al\u00b7le", "bis", "auf", "ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PIS", "ADV", "APPR", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Und sprach mit h\u00f6hnischem Gesicht:", "tokens": ["Und", "sprach", "mit", "h\u00f6h\u00b7ni\u00b7schem", "Ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Es ist des weisen F\u00fcrsten Pflicht", "tokens": ["Es", "ist", "des", "wei\u00b7sen", "F\u00fcrs\u00b7ten", "Pflicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Den Unterthan vor den Gefahren", "tokens": ["Den", "Un\u00b7ter\u00b7than", "vor", "den", "Ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Des Ungehorsams zu bewahren.", "tokens": ["Des", "Un\u00b7ge\u00b7hor\u00b7sams", "zu", "be\u00b7wah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}