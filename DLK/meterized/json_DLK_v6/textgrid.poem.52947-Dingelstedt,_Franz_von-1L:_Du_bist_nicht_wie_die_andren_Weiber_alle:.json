{"textgrid.poem.52947": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du bist nicht wie die andren Weiber alle:", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist nicht wie die andren Weiber alle:", "tokens": ["Du", "bist", "nicht", "wie", "die", "an\u00b7dren", "Wei\u00b7ber", "al\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "KOKOM", "ART", "ADJA", "NN", "PIS", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du forschest nicht auf meinem Seelengrunde", "tokens": ["Du", "for\u00b7schest", "nicht", "auf", "mei\u00b7nem", "See\u00b7len\u00b7grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nach l\u00e4ngst versunknem Lieben oder Leben;", "tokens": ["Nach", "l\u00e4ngst", "ver\u00b7sun\u00b7knem", "Lie\u00b7ben", "o\u00b7der", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "ADJA", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du putzest minder Dich zu einem Balle", "tokens": ["Du", "put\u00b7zest", "min\u00b7der", "Dich", "zu", "ei\u00b7nem", "Bal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Als f\u00fcr den Freund zu stiller Sch\u00e4ferstunde", "tokens": ["Als", "f\u00fcr", "den", "Freund", "zu", "stil\u00b7ler", "Sch\u00e4\u00b7fer\u00b7stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und hast Dich, ohne Schwur und Widerstreben,", "tokens": ["Und", "hast", "Dich", ",", "oh\u00b7ne", "Schwur", "und", "Wi\u00b7der\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "$,", "KOUI", "VVFIN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Mir ganz dahingegeben.", "tokens": ["Mir", "ganz", "da\u00b7hin\u00b7ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "J\u00fcngst k\u00fc\u00dfte ich den Saum an Deinem Kleide,", "tokens": ["J\u00fcngst", "k\u00fc\u00df\u00b7te", "ich", "den", "Saum", "an", "Dei\u00b7nem", "Klei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Da wardst Du b\u00f6se und botest s\u00fc\u00df-beklommen", "tokens": ["Da", "wardst", "Du", "b\u00f6\u00b7se", "und", "bo\u00b7test", "s\u00fc\u00df\u00b7be\u00b7klom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "KON", "VVFIN", "ADJA"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Den Mund mir dar; auch abends, wenn ich scheide,", "tokens": ["Den", "Mund", "mir", "dar", ";", "auch", "a\u00b7bends", ",", "wenn", "ich", "schei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKVZ", "$.", "ADV", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Fragst Du mich nie: wann wirst du wiederkommen?", "tokens": ["Fragst", "Du", "mich", "nie", ":", "wann", "wirst", "du", "wie\u00b7der\u00b7kom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "$.", "PWAV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "O M\u00e4dchen, M\u00e4dchen, lehre mich vergessen,", "tokens": ["O", "M\u00e4d\u00b7chen", ",", "M\u00e4d\u00b7chen", ",", "leh\u00b7re", "mich", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Da\u00df ich schon andere vor Dir besessen!", "tokens": ["Da\u00df", "ich", "schon", "an\u00b7de\u00b7re", "vor", "Dir", "be\u00b7ses\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIS", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}