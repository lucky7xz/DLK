{"textgrid.poem.53519": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Nationale Verteidigung", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das pa\u00dft euch so. Ihr gr\u00f6lt und br\u00fcllt", "tokens": ["Das", "pa\u00dft", "euch", "so", ".", "Ihr", "gr\u00f6lt", "und", "br\u00fcllt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$.", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "von Friedensdemokraten;", "tokens": ["von", "Frie\u00b7dens\u00b7de\u00b7mo\u00b7kra\u00b7ten", ";"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "in dicken Phrasenrauch geh\u00fcllt", "tokens": ["in", "di\u00b7cken", "Phra\u00b7sen\u00b7rauch", "ge\u00b7h\u00fcllt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ruft ihr nach mehr Soldaten.", "tokens": ["ruft", "ihr", "nach", "mehr", "Sol\u00b7da\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Obristenfrauen schrein und kr\u00e4hn", "tokens": ["O\u00b7bris\u00b7ten\u00b7frau\u00b7en", "schrein", "und", "kr\u00e4hn"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "mit euch: \u00bbMarsch-Marsch! nach Flandern!\u00ab", "tokens": ["mit", "euch", ":", "\u00bb", "Mar\u00b7schMarsch", "!", "nach", "Flan\u00b7dern", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "$.", "$(", "NE", "$.", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Es sollen dorthin sterben gehn", "tokens": ["Es", "sol\u00b7len", "dor\u00b7thin", "ster\u00b7ben", "gehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "die andern, die andern!", "tokens": ["die", "an\u00b7dern", ",", "die", "an\u00b7dern", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "PIS", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Todespein der andern schwand", "tokens": ["Die", "To\u00b7de\u00b7spein", "der", "an\u00b7dern", "schwand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "in Urlaubstag und -n\u00e4chten.", "tokens": ["in", "Ur\u00b7laubs\u00b7tag", "und", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr liebt nicht euer Vaterland!", "tokens": ["Ihr", "liebt", "nicht", "eu\u00b7er", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr h\u00e4ngt an Vorzugsrechten!", "tokens": ["Ihr", "h\u00e4ngt", "an", "Vor\u00b7zugs\u00b7rech\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das hamstert, schickt und schwatzt so nett", "tokens": ["Das", "hams\u00b7tert", ",", "schickt", "und", "schwatzt", "so", "nett"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "bei braungebratenen Zandern.", "tokens": ["bei", "braun\u00b7ge\u00b7bra\u00b7te\u00b7nen", "Zan\u00b7dern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Die zwanzig Gramm vom Pflanzenfett", "tokens": ["Die", "zwan\u00b7zig", "Gramm", "vom", "Pflan\u00b7zen\u00b7fett"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "CARD", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "den andern, den andern!", "tokens": ["den", "an\u00b7dern", ",", "den", "an\u00b7dern", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Die Zeit ist aus. Die andern stehn", "tokens": ["Die", "Zeit", "ist", "aus", ".", "Die", "an\u00b7dern", "stehn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PTKVZ", "$.", "ART", "ADJA", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und recken ihre Glieder.", "tokens": ["und", "re\u00b7cken", "ih\u00b7re", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So lang geduckt, und nunmehr sehn", "tokens": ["So", "lang", "ge\u00b7duckt", ",", "und", "nun\u00b7mehr", "sehn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVPP", "$,", "KON", "ADV", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "sie sich als Menschen wieder.", "tokens": ["sie", "sich", "als", "Men\u00b7schen", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "KOUS", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Friede kommt. Und ist er hier,", "tokens": ["Der", "Frie\u00b7de", "kommt", ".", "Und", "ist", "er", "hier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "KON", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "dann kommt das Heimw\u00e4rtswandern.", "tokens": ["dann", "kommt", "das", "Heim\u00b7w\u00e4rts\u00b7wan\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Zeit ist aus. Jetzt kommen wir:", "tokens": ["Die", "Zeit", "ist", "aus", ".", "Jetzt", "kom\u00b7men", "wir", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die andern! Die andern!", "tokens": ["Die", "an\u00b7dern", "!", "Die", "an\u00b7dern", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$.", "ART", "ADJA", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}