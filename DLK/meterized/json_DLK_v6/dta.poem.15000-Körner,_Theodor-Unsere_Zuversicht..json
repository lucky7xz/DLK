{"dta.poem.15000": {"metadata": {"author": {"name": "K\u00f6rner, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Unsere Zuversicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1814", "urn": "urn:nbn:de:kobv:b4-200905193258", "language": ["de:0.99"], "booktitle": "K\u00f6rner, Theodor: Leyer und Schwerdt. Berlin, 1814."}, "poem": {"stanza.1": {"line.1": {"text": "Und halten fest an deinem Wort!", "tokens": ["Und", "hal\u00b7ten", "fest", "an", "dei\u00b7nem", "Wort", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die H\u00f6lle soll uns nicht ber\u00fccken,", "tokens": ["Die", "H\u00f6l\u00b7le", "soll", "uns", "nicht", "be\u00b7r\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Durch Aberwitz und Meuchelmord,", "tokens": ["Durch", "A\u00b7ber\u00b7witz", "und", "Meu\u00b7chel\u00b7mord", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und was auch rings, in Tr\u00fcmmern geht,", "tokens": ["Und", "was", "auch", "rings", ",", "in", "Tr\u00fcm\u00b7mern", "geht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ADV", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir wissens, da\u00df dein Wort besteht.", "tokens": ["Wir", "wis\u00b7sens", ",", "da\u00df", "dein", "Wort", "be\u00b7steht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}