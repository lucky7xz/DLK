{"textgrid.poem.66293": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Frei, wie ein Engel, stand er da,", "genre": "verse", "period": "N.A.", "pub_year": 1766, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frei, wie ein Engel, stand er da,", "tokens": ["Frei", ",", "wie", "ein", "En\u00b7gel", ",", "stand", "er", "da", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der Mann vor seinen Richtern;", "tokens": ["Der", "Mann", "vor", "sei\u00b7nen", "Rich\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verborgne Wuth und Rache sah", "tokens": ["Ver\u00b7borg\u00b7ne", "Wuth", "und", "Ra\u00b7che", "sah"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus grimmigen Gesichtern.", "tokens": ["Aus", "grim\u00b7mi\u00b7gen", "Ge\u00b7sich\u00b7tern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch Stephanus erzittert nicht", "tokens": ["Doch", "Ste\u00b7pha\u00b7nus", "er\u00b7zit\u00b7tert", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NE", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und schaut mit gl\u00fchendem Gesicht", "tokens": ["Und", "schaut", "mit", "gl\u00fc\u00b7hen\u00b7dem", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Hinauf zu seinem Mittler.", "tokens": ["Hin\u00b7auf", "zu", "sei\u00b7nem", "Mitt\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es sieht sein Sonnenauge weit,", "tokens": ["Es", "sieht", "sein", "Son\u00b7nen\u00b7au\u00b7ge", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weit \u00fcber blauen H\u00f6hen", "tokens": ["Weit", "\u00fc\u00b7ber", "blau\u00b7en", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Glanz von Gottes Herrlichkeit", "tokens": ["Den", "Glanz", "von", "Got\u00b7tes", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Jesum Christum stehen.", "tokens": ["Und", "Je\u00b7sum", "Chris\u00b7tum", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Entz\u00fccken zittert durch die Brust,", "tokens": ["Ent\u00b7z\u00fc\u00b7cken", "zit\u00b7tert", "durch", "die", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er stammelt, aufgel\u00f6st in Lust:", "tokens": ["Er", "stam\u00b7melt", ",", "auf\u00b7ge\u00b7l\u00f6st", "in", "Lust", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich seh' den Himmel offen.", "tokens": ["Ich", "seh'", "den", "Him\u00b7mel", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nur seine M\u00f6rder knirschten Wuth", "tokens": ["Nur", "sei\u00b7ne", "M\u00f6r\u00b7der", "knirschten", "Wuth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Und blickten ihm Verderben!", "tokens": ["Und", "blick\u00b7ten", "ihm", "Ver\u00b7der\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und Stephanus! dein M\u00e4rt'rerblut", "tokens": ["Und", "Ste\u00b7pha\u00b7nus", "!", "dein", "M\u00e4rt'\u00b7rerblut"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "NE", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Soll ihre Steine f\u00e4rben.", "tokens": ["Soll", "ih\u00b7re", "Stei\u00b7ne", "f\u00e4r\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Jedoch, dein Seufzer steigt hinauf:", "tokens": ["Je\u00b7doch", ",", "dein", "Seuf\u00b7zer", "steigt", "hin\u00b7auf", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nimm meinen Geist, Herr Jesu, auf!", "tokens": ["Nimm", "mei\u00b7nen", "Geist", ",", "Herr", "Je\u00b7su", ",", "auf", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "NN", "NE", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dann sinkst du stumm zur Erde.", "tokens": ["Dann", "sinkst", "du", "stumm", "zur", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und Felsenst\u00fccke st\u00fcrzen schwer,", "tokens": ["Und", "Fel\u00b7sen\u00b7st\u00fc\u00b7cke", "st\u00fcr\u00b7zen", "schwer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zermalmend auf ihn nieder;", "tokens": ["Zer\u00b7mal\u00b7mend", "auf", "ihn", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch seufzt er nicht, nur betet er", "tokens": ["Doch", "seufzt", "er", "nicht", ",", "nur", "be\u00b7tet", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$,", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr m\u00f6rderische Br\u00fcder.", "tokens": ["F\u00fcr", "m\u00f6r\u00b7de\u00b7ri\u00b7sche", "Br\u00fc\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er hebt die morsche Hand und spricht:", "tokens": ["Er", "hebt", "die", "mor\u00b7sche", "Hand", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Behalte ihre S\u00fcnden nicht!", "tokens": ["Be\u00b7hal\u00b7te", "ih\u00b7re", "S\u00fcn\u00b7den", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sinkt nieder und entschlummert.", "tokens": ["Sinkt", "nie\u00b7der", "und", "ent\u00b7schlum\u00b7mert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Erbarmer, auf der finstern Bahn", "tokens": ["Er\u00b7bar\u00b7mer", ",", "auf", "der", "fins\u00b7tern", "Bahn"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Todes darf ich hoffen,", "tokens": ["Des", "To\u00b7des", "darf", "ich", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df meine Seele sprechen kann:", "tokens": ["Da\u00df", "mei\u00b7ne", "See\u00b7le", "spre\u00b7chen", "kann", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich seh' den Himmel offen!", "tokens": ["Ich", "seh'", "den", "Him\u00b7mel", "of\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Kann ich gleich nicht mit meinem Blut,", "tokens": ["Kann", "ich", "gleich", "nicht", "mit", "mei\u00b7nem", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So will ich doch mit Christenmuth", "tokens": ["So", "will", "ich", "doch", "mit", "Chris\u00b7ten\u00b7muth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dich in dem Tode preisen.", "tokens": ["Dich", "in", "dem", "To\u00b7de", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Doch, Vater, eh' ich sterben mu\u00df,", "tokens": ["Doch", ",", "Va\u00b7ter", ",", "eh'", "ich", "ster\u00b7ben", "mu\u00df", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und kann fast nimmer reden:", "tokens": ["Und", "kann", "fast", "nim\u00b7mer", "re\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So la\u00df mich noch, wie Stephanus,", "tokens": ["So", "la\u00df", "mich", "noch", ",", "wie", "Ste\u00b7pha\u00b7nus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "$,", "PWAV", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr meine Feinde beten.", "tokens": ["F\u00fcr", "mei\u00b7ne", "Fein\u00b7de", "be\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sein gro\u00dfer Seufzer flamm' in mir:", "tokens": ["Sein", "gro\u00b7\u00dfer", "Seuf\u00b7zer", "flamm'", "in", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Herr Jesu, nimm den Geist zu dir!", "tokens": ["Herr", "Je\u00b7su", ",", "nimm", "den", "Geist", "zu", "dir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "VVIMP", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wer kann mich so verdammen?", "tokens": ["Wer", "kann", "mich", "so", "ver\u00b7dam\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}