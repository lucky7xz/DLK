{"textgrid.poem.65793": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "67", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, warum mu\u00df er mit Verdorbnen leben,", "tokens": ["Ach", ",", "wa\u00b7rum", "mu\u00df", "er", "mit", "Ver\u00b7dorb\u00b7nen", "le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df S\u00fcndern seine Gegenwart gedeiht,", "tokens": ["Da\u00df", "S\u00fcn\u00b7dern", "sei\u00b7ne", "Ge\u00b7gen\u00b7wart", "ge\u00b7deiht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ruchlose freventlich sich \u00fcberheben,", "tokens": ["Ruch\u00b7lo\u00b7se", "fre\u00b7vent\u00b7lich", "sich", "\u00fc\u00b7ber\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PRF", "VVINF", "$,"], "meter": "+--++--+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Verlarvt in seines Umgangs Lieblichkeit!", "tokens": ["Ver\u00b7larvt", "in", "sei\u00b7nes", "Um\u00b7gangs", "Lieb\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Warum soll Farbentrug nachahmen seine Wangen?", "tokens": ["Wa\u00b7rum", "soll", "Far\u00b7ben\u00b7trug", "nac\u00b7hah\u00b7men", "sei\u00b7ne", "Wan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was stiehlt man toten Schein von seinem Lebensrot?", "tokens": ["Was", "stiehlt", "man", "to\u00b7ten", "Schein", "von", "sei\u00b7nem", "Le\u00b7bens\u00b7rot", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PIS", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wird arme Sch\u00f6nheit schlau mit Schattenrosen prangen,", "tokens": ["Wird", "ar\u00b7me", "Sch\u00f6n\u00b7heit", "schlau", "mit", "Schat\u00b7ten\u00b7ro\u00b7sen", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wo seiner echten Rosen Purpur droht?", "tokens": ["Wo", "sei\u00b7ner", "ech\u00b7ten", "Ro\u00b7sen", "Pur\u00b7pur", "droht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Nicht mehr err\u00f6ten kann mit frischer Adern Blute:", "tokens": ["Nicht", "mehr", "er\u00b7r\u00f6\u00b7ten", "kann", "mit", "fri\u00b7scher", "A\u00b7dern", "Blu\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVINF", "VMFIN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Denn in ihm spart sie ihren letzten Hort,", "tokens": ["Denn", "in", "ihm", "spart", "sie", "ih\u00b7ren", "letz\u00b7ten", "Hort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "N\u00e4hrt sich, auf viele stolz, nur noch von seinem Gute. \u2013", "tokens": ["N\u00e4hrt", "sich", ",", "auf", "vie\u00b7le", "stolz", ",", "nur", "noch", "von", "sei\u00b7nem", "Gu\u00b7te", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PRF", "$,", "APPR", "PIS", "ADJD", "$,", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ja, ihn begabend, zeigt sie wie erlesen,", "tokens": ["Ja", ",", "ihn", "be\u00b7ga\u00b7bend", ",", "zeigt", "sie", "wie", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVPP", "$,", "VVFIN", "PPER", "KOKOM", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Wie reich in bess'rer Zeit, lang vor uns sie gewesen.", "tokens": ["Wie", "reich", "in", "bess'\u00b7rer", "Zeit", ",", "lang", "vor", "uns", "sie", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "ADJA", "NN", "$,", "ADJD", "APPR", "PPER", "PPER", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}