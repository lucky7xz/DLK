{"textgrid.poem.67631": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "52.", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum kranken Huhn kam einst als Pfau", "tokens": ["Zum", "kran\u00b7ken", "Huhn", "kam", "einst", "als", "Pfau"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ADV", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gekleidet Iltis. \u00bbGute Frau,", "tokens": ["Ge\u00b7klei\u00b7det", "Il\u00b7tis", ".", "\u00bb", "Gu\u00b7te", "Frau", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["VVPP", "NE", "$.", "$(", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie steht's mit Ihr? Schlecht, wie ich seh'!", "tokens": ["Wie", "steht's", "mit", "Ihr", "?", "Schlecht", ",", "wie", "ich", "seh'", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPR", "PPER", "$.", "NN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Ist Schade!", "tokens": ["Ist", "Scha\u00b7de", "!"], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Vermag ich was kraft meiner G\u00fct' und Gnade \u2013\u00ab", "tokens": ["Ver\u00b7mag", "ich", "was", "kraft", "mei\u00b7ner", "G\u00fct'", "und", "Gna\u00b7de", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PIS", "NN", "PPOSAT", "NN", "KON", "NN", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "\u00bbach, gn\u00e4d'ge Frau,", "tokens": ["\u00bb", "ach", ",", "gn\u00e4d'\u00b7ge", "Frau", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "ADJA", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Verzeihen Sie, mir wird so weh,", "tokens": ["Ver\u00b7zei\u00b7hen", "Sie", ",", "mir", "wird", "so", "weh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "VAFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wenn ich ", "tokens": ["Wenn", "ich"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "++", "measure": "spondeus"}}, "stanza.2": {"line.1": {"text": "\u00bbvertraut nur, Kinder, meinem Gouverneur", "tokens": ["\u00bb", "ver\u00b7traut", "nur", ",", "Kin\u00b7der", ",", "mei\u00b7nem", "Gou\u00b7ver\u00b7neur"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["$(", "ADJD", "ADV", "$,", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "All Eu'r Beschwer!\u00ab", "tokens": ["All", "Eu'r", "Be\u00b7schwer", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "\u00bbach, gn\u00e4d'ger Herr, nur Eins beschwert uns sehr,", "tokens": ["\u00bb", "ach", ",", "gn\u00e4d'\u00b7ger", "Herr", ",", "nur", "Eins", "be\u00b7schwert", "uns", "sehr", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "ADJA", "NN", "$,", "ADV", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Gouverneur!\u00ab", "tokens": ["Der", "Gou\u00b7ver\u00b7neur", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ART", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Man dankt f\u00fcr Gnaden,", "tokens": ["Man", "dankt", "f\u00fcr", "Gna\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Wenn man sie hat,", "tokens": ["Wenn", "man", "sie", "hat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VAFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und mehr f\u00fcr Gnaden,", "tokens": ["Und", "mehr", "f\u00fcr", "Gna\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Eh man sie hat.", "tokens": ["Eh", "man", "sie", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VAFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Dort ist die stumme Freude Dank,", "tokens": ["Dort", "ist", "die", "stum\u00b7me", "Freu\u00b7de", "Dank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hier macht die laute Gnade krank.", "tokens": ["Hier", "macht", "die", "lau\u00b7te", "Gna\u00b7de", "krank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}