{"dta.poem.7854": {"metadata": {"author": {"name": "Birken, Sigmund von", "birth": "N.A.", "death": "N.A."}, "title": "Freuden-Lied.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1665", "urn": "urn:nbn:de:kobv:b4-200905198447", "language": ["de:0.99"], "booktitle": "Birken, Sigmund von: Pegnesische Gespr\u00e4chspiel-Gesellschaft von Nymfen und Hirten. N\u00fcrnberg, 1665."}, "poem": {"stanza.1": {"line.1": {"text": "Schreibt/ ihr Sch\u00e4fer/ diesen Tag/", "tokens": ["Schreibt", "/", "ihr", "Sch\u00e4\u00b7fer", "/", "die\u00b7sen", "Tag", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$(", "PPOSAT", "NN", "$(", "PDAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "lasset/ von ihm/ unsre Sag", "tokens": ["las\u00b7set", "/", "von", "ihm", "/", "uns\u00b7re", "Sag"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$(", "APPR", "PPER", "$(", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "in der B\u00e4ume Wunden finden.", "tokens": ["in", "der", "B\u00e4u\u00b7me", "Wun\u00b7den", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieser Tag/ sey unser Fest/", "tokens": ["Die\u00b7ser", "Tag", "/", "sey", "un\u00b7ser", "Fest", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$(", "VAFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "das wir j\u00e4hrlich wollen feyren:", "tokens": ["das", "wir", "j\u00e4hr\u00b7lich", "wol\u00b7len", "fey\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "der vergn\u00fcgbar unsrem Theuren/", "tokens": ["der", "ver\u00b7gn\u00fcg\u00b7bar", "uns\u00b7rem", "Theu\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "meinem Helden/ ist gewest.", "tokens": ["mei\u00b7nem", "Hel\u00b7den", "/", "ist", "ge\u00b7west", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Pegnitz! k\u00fcsse deinen Rand:", "tokens": ["Peg\u00b7nitz", "!", "k\u00fcs\u00b7se", "dei\u00b7nen", "Rand", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "den itzt adelt hohe Freude.", "tokens": ["den", "itzt", "a\u00b7delt", "ho\u00b7he", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Blum-beschw\u00e4ngre diesen Sand/", "tokens": ["Blum\u00b7be\u00b7schw\u00e4n\u00b7gre", "die\u00b7sen", "Sand", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "mach ihn zur Smaragdnen Heyde.", "tokens": ["mach", "ihn", "zur", "Sma\u00b7ragd\u00b7nen", "Hey\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "NE", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Deiner Nymfen flaches Dach/", "tokens": ["Dei\u00b7ner", "Nym\u00b7fen", "fla\u00b7ches", "Dach", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "ziegle zierlich mit Krystallen.", "tokens": ["zieg\u00b7le", "zier\u00b7lich", "mit", "Krys\u00b7tal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "la\u00df von Leonoren lallen/", "tokens": ["la\u00df", "von", "Le\u00b7o\u00b7no\u00b7ren", "lal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "NE", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "deiner Fluten Wudel-Sprach.", "tokens": ["dei\u00b7ner", "Flu\u00b7ten", "Wu\u00b7del\u00b7Sprach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": " sonne! Unser Mec\u00e4nat/", "tokens": ["son\u00b7ne", "!", "Un\u00b7ser", "Me\u00b7c\u00e4\u00b7nat", "/"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "ob du schon bist untergangen/", "tokens": ["ob", "du", "schon", "bist", "un\u00b7ter\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "dannoch seine Sonne hat/", "tokens": ["dan\u00b7noch", "sei\u00b7ne", "Son\u00b7ne", "hat", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "siht ein Rosen-Liecht der Wangen.", "tokens": ["siht", "ein", "Ro\u00b7sen\u00b7Liecht", "der", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nimm itzt meine Bitt mit dir!", "tokens": ["Nimm", "itzt", "mei\u00b7ne", "Bitt", "mit", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gebe doch der Himmel Beyden:", "tokens": ["Ge\u00b7be", "doch", "der", "Him\u00b7mel", "Bey\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sonne! da\u00df du Sie in Freuden", "tokens": ["Son\u00b7ne", "!", "da\u00df", "du", "Sie", "in", "Freu\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "KOUS", "PPER", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "m\u00f6gest sehen f\u00fcr und f\u00fcr!", "tokens": ["m\u00f6\u00b7gest", "se\u00b7hen", "f\u00fcr", "und", "f\u00fcr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "APPR", "KON", "APPR", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Cynthia! wilst du heut nicht", "tokens": ["Cyn\u00b7thia", "!", "wilst", "du", "heut", "nicht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "VMFIN", "PPER", "ADV", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "an dem Himmel sichtbar werden?", "tokens": ["an", "dem", "Him\u00b7mel", "sicht\u00b7bar", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "oder weichst/ du bleiches Liecht/", "tokens": ["o\u00b7der", "weichst", "/", "du", "blei\u00b7ches", "Liecht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "PPER", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "unsrer Cynthien auf Erden?", "tokens": ["uns\u00b7rer", "Cyn\u00b7thien", "auf", "Er\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Was fragt nach dem Mondes-Schein/", "tokens": ["Was", "fragt", "nach", "dem", "Mon\u00b7des\u00b7Schein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "unser Held? es soll darneben/", "tokens": ["un\u00b7ser", "Held", "?", "es", "soll", "dar\u00b7ne\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPER", "VMFIN", "PAV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "seinen N\u00e4chten Liecht zu geben/", "tokens": ["sei\u00b7nen", "N\u00e4ch\u00b7ten", "Liecht", "zu", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Leonora Luna seyn.", "tokens": ["Le\u00b7o\u00b7no\u00b7ra", "Lu\u00b7na", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nacht! es mu\u00df itzt allzu-nacht/", "tokens": ["Nacht", "!", "es", "mu\u00df", "itzt", "all\u00b7zu\u00b7nacht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VMFIN", "ADV", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "auf so hellen Tag/ nit werden.", "tokens": ["auf", "so", "hel\u00b7len", "Tag", "/", "nit", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$(", "PTKNEG", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit dem Goldgestickten Pracht/", "tokens": ["Mit", "dem", "Gold\u00b7ge\u00b7stick\u00b7ten", "Pracht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "deck heut dieses Zelt der Erden:", "tokens": ["deck", "heut", "die\u00b7ses", "Zelt", "der", "Er\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PDAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "f\u00fchr herf\u00fcr die Sternen Wacht.", "tokens": ["f\u00fchr", "her\u00b7f\u00fcr", "die", "Ster\u00b7nen", "Wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Unsren Hochverm\u00e4hlten Beyden", "tokens": ["Un\u00b7sren", "Hoch\u00b7ver\u00b7m\u00e4hl\u00b7ten", "Bey\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "lach\u2019 ein Tausend-Liecht der Freuden!", "tokens": ["lach'", "ein", "Tau\u00b7sen\u00b7dLiecht", "der", "Freu\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Trauer-N\u00e4chte! gute Nacht!", "tokens": ["Trau\u00b7e\u00b7rN\u00e4ch\u00b7te", "!", "gu\u00b7te", "Nacht", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Hesperus! zeig Ihnen du", "tokens": ["Hes\u00b7pe\u00b7rus", "!", "zeig", "Ih\u00b7nen", "du"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "VVFIN", "PPER", "PPER"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "deines Mundes g\u00fcldne Zinken;", "tokens": ["dei\u00b7nes", "Mun\u00b7des", "g\u00fcld\u00b7ne", "Zin\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "und la\u00df/ Ihre Liebes-Ruh/", "tokens": ["und", "la\u00df", "/", "Ih\u00b7re", "Lie\u00b7bes\u00b7Ruh", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$(", "PPOSAT", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "allerb\u00e4sten Einflu\u00df trinken.", "tokens": ["al\u00b7ler\u00b7b\u00e4s\u00b7ten", "Ein\u00b7flu\u00df", "trin\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sey mit deinem Br\u00fcder-Heer/", "tokens": ["Sey", "mit", "dei\u00b7nem", "Br\u00fc\u00b7der\u00b7Heer", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "itzund ihre Hochzeit Fakel.", "tokens": ["it\u00b7zund", "ih\u00b7re", "Hoch\u00b7zeit", "Fa\u00b7kel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "und/ als ihres Gl\u00fccks Orakel/", "tokens": ["und", "/", "als", "ih\u00b7res", "Gl\u00fccks", "O\u00b7ra\u00b7kel", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "KOKOM", "PPOSAT", "NN", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Allen Unstern-blick bekehr.", "tokens": ["Al\u00b7len", "Un\u00b7stern\u00b7blick", "be\u00b7kehr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "G\u00f6nnt der Ohren s\u00fcsse Gnad/", "tokens": ["G\u00f6nnt", "der", "Oh\u00b7ren", "s\u00fcs\u00b7se", "Gnad", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "unsren heischern Pegnitz-Rohren;", "tokens": ["un\u00b7sren", "hei\u00b7schern", "Peg\u00b7nitz\u00b7Roh\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "bleibt geneigt/ mein Mec\u00e4nat!", "tokens": ["bleibt", "ge\u00b7neigt", "/", "mein", "Me\u00b7c\u00e4\u00b7nat", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$(", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Fahret wohl/ mit Leonoren!", "tokens": ["Fah\u00b7ret", "wohl", "/", "mit", "Le\u00b7o\u00b7no\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$(", "APPR", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Lebt/ vergn\u00fcgt durch Ihre Zier.", "tokens": ["Lebt", "/", "ver\u00b7gn\u00fcgt", "durch", "Ih\u00b7re", "Zier", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Eurer Namen Schall der Ehren/", "tokens": ["Eu\u00b7rer", "Na\u00b7men", "Schall", "der", "Eh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "hier in unsren Sch\u00e4fer-Ch\u00f6ren", "tokens": ["hier", "in", "un\u00b7sren", "Sch\u00e4\u00b7fer\u00b7Ch\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "soll erklingen f\u00fcr und f\u00fcr.", "tokens": ["soll", "er\u00b7klin\u00b7gen", "f\u00fcr", "und", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "APPR", "KON", "APPR", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}