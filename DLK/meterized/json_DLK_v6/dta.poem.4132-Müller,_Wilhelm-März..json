{"dta.poem.4132": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "M\u00e4rz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die kleinen wei\u00dfen Kelche gern bedecken,", "tokens": ["Die", "klei\u00b7nen", "wei\u00b7\u00dfen", "Kel\u00b7che", "gern", "be\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "M\u00f6cht' ich, wie sie, mich deinem Blick verstecken,", "tokens": ["M\u00f6cht'", "ich", ",", "wie", "sie", ",", "mich", "dei\u00b7nem", "Blick", "ver\u00b7ste\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "PWAV", "PPER", "$,", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Weil ich allein so \u00e4rmlich vor dir stehe.", "tokens": ["Weil", "ich", "al\u00b7lein", "so", "\u00e4rm\u00b7lich", "vor", "dir", "ste\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wohin ich auch nach bessern Gaben sp\u00e4he,", "tokens": ["Wo\u00b7hin", "ich", "auch", "nach", "bes\u00b7sern", "Ga\u00b7ben", "sp\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur Keim und Knospe sind' ich aller Ecken;", "tokens": ["Nur", "Keim", "und", "Knos\u00b7pe", "sind'", "ich", "al\u00b7ler", "E\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wohl m\u00f6cht' ich Laub und Bl\u00fcthe dir erwecken,", "tokens": ["Wohl", "m\u00f6cht'", "ich", "Laub", "und", "Bl\u00fc\u00b7the", "dir", "er\u00b7we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "KON", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch f\u00fcrcht' ich sehr, mein Hauch th\u00e4t' ihnen wehe.", "tokens": ["Doch", "f\u00fcrcht'", "ich", "sehr", ",", "mein", "Hauch", "th\u00e4t'", "ih\u00b7nen", "we\u00b7he", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So nimm denn, was ich bringe, als zum Pfande", "tokens": ["So", "nimm", "denn", ",", "was", "ich", "brin\u00b7ge", ",", "als", "zum", "Pfan\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVIMP", "ADV", "$,", "PWS", "PPER", "VVFIN", "$,", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der sch\u00f6nen Zeit, die ich nur darf verk\u00fcnden,", "tokens": ["Der", "sch\u00f6\u00b7nen", "Zeit", ",", "die", "ich", "nur", "darf", "ver\u00b7k\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Daher sie mich den Mond der ", "tokens": ["Da\u00b7her", "sie", "mich", "den", "Mond", "der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "PRF", "ART", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und wenn der Wonnemond regiert im Lande,", "tokens": ["Und", "wenn", "der", "Won\u00b7ne\u00b7mond", "re\u00b7giert", "im", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wirst du ", "tokens": ["Wirst", "du"], "token_info": ["word", "word"], "pos": ["VAFIN", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Und ungel\u00f6scht soll dir kein Wunsch verbrennen.", "tokens": ["Und", "un\u00b7ge\u00b7l\u00f6scht", "soll", "dir", "kein", "Wunsch", "ver\u00b7bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}