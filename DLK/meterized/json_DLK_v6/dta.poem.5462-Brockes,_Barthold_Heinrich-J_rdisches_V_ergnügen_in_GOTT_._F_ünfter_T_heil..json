{"dta.poem.5462": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dem Sch\u00f6pfer f\u00e4llt so wenig schwer,", "tokens": ["Dem", "Sch\u00f6p\u00b7fer", "f\u00e4llt", "so", "we\u00b7nig", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Sternen- und Cometen-Heer,", "tokens": ["Ein", "Ster\u00b7nen", "und", "Co\u00b7me\u00b7ten\u00b7Heer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Als einen Tropfen Thau, zu zeugen.", "tokens": ["Als", "ei\u00b7nen", "Trop\u00b7fen", "Thau", ",", "zu", "zeu\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00dft die\u00df uns einen Antrieb seyn,", "tokens": ["La\u00dft", "die\u00df", "uns", "ei\u00b7nen", "An\u00b7trieb", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PDS", "PPER", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vor ihm und seiner Macht allein,", "tokens": ["Vor", "ihm", "und", "sei\u00b7ner", "Macht", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In tiefster Ehrfurcht, uns zu beugen!", "tokens": ["In", "tiefs\u00b7ter", "Ehr\u00b7furcht", ",", "uns", "zu", "beu\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}