{"textgrid.poem.54514": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So vielmahl hundert Jahr von Anbegin der Zeiten", "tokens": ["So", "viel\u00b7mahl", "hun\u00b7dert", "Jahr", "von", "An\u00b7be\u00b7gin", "der", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "CARD", "NN", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hofft nicht Jud\u00e4a nur/ es hofft die gantze Welt/", "tokens": ["Hofft", "nicht", "Ju\u00b7d\u00e4a", "nur", "/", "es", "hofft", "die", "gant\u00b7ze", "Welt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "NE", "ADV", "$(", "PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Dieweil du noch nicht dar in Schuld und Angst verf\u00e4llt/", "tokens": ["Die\u00b7weil", "du", "noch", "nicht", "dar", "in", "Schuld", "und", "Angst", "ver\u00b7f\u00e4llt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "PTKVZ", "APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf dich/ O h\u00f6chstes Kind! O F\u00fcrst der Ewigkeiten!", "tokens": ["Auf", "dich", "/", "O", "h\u00f6chs\u00b7tes", "Kind", "!", "O", "F\u00fcrst", "der", "E\u00b7wig\u00b7kei\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$(", "NE", "ADJA", "NN", "$.", "NE", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wol! nun die Stund ist hier du wilst die Welt beschreiten/", "tokens": ["Wol", "!", "nun", "die", "Stund", "ist", "hier", "du", "wilst", "die", "Welt", "be\u00b7schrei\u00b7ten", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "ART", "NN", "VAFIN", "ADV", "PPER", "VMFIN", "ART", "NN", "VVFIN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.6": {"text": "Die schwangre Jungfrau hat sich r\u00fcstig eingestellt/", "tokens": ["Die", "schwang\u00b7re", "Jung\u00b7frau", "hat", "sich", "r\u00fcs\u00b7tig", "ein\u00b7ge\u00b7stellt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PRF", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie da\u00df man keinen Ort denn f\u00fcr dich offen h\u00e4lt?", "tokens": ["Wie", "da\u00df", "man", "kei\u00b7nen", "Ort", "denn", "f\u00fcr", "dich", "of\u00b7fen", "h\u00e4lt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PIS", "PIAT", "NN", "KON", "APPR", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie da\u00df dir niemand denn die Wiegen wil bereiten.", "tokens": ["Wie", "da\u00df", "dir", "nie\u00b7mand", "denn", "die", "Wie\u00b7gen", "wil", "be\u00b7rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PPER", "PIS", "ADV", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dein Salem das so lang gezagt in Nacht und S\u00fcnd/", "tokens": ["Dein", "Sa\u00b7lem", "das", "so", "lang", "ge\u00b7zagt", "in", "Nacht", "und", "S\u00fcnd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADV", "ADJD", "VVPP", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wird nun du Sonn auffgehst vor deinen Strahlen blind.", "tokens": ["Wird", "nun", "du", "Sonn", "auff\u00b7gehst", "vor", "dei\u00b7nen", "Strah\u00b7len", "blind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Drum l\u00e4st sie dich im Stall ohn Sorg und Wartung liegen,", "tokens": ["Drum", "l\u00e4st", "sie", "dich", "im", "Stall", "ohn", "Sorg", "und", "War\u00b7tung", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Komm werther Gast/ O komm ich \u00f6ffne Seel und Hertz/", "tokens": ["Komm", "wert\u00b7her", "Gast", "/", "O", "komm", "ich", "\u00f6ff\u00b7ne", "Seel", "und", "Hertz", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$(", "NE", "VVFIN", "PPER", "VVFIN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mein Hertz das f\u00fcr und f\u00fcr der Jammerreiche Schmertz", "tokens": ["Mein", "Hertz", "das", "f\u00fcr", "und", "f\u00fcr", "der", "Jam\u00b7mer\u00b7rei\u00b7che", "Schmertz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "APPR", "KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und stete Furcht bewegt/ sey deine liebe Wiegen.", "tokens": ["Und", "ste\u00b7te", "Furcht", "be\u00b7wegt", "/", "sey", "dei\u00b7ne", "lie\u00b7be", "Wie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "VVFIN", "$(", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}