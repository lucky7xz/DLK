{"dta.poem.21585": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Eingang .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "In lauer Nacht durchwandl' ich oft den Wald,               ", "tokens": ["In", "lau\u00b7er", "Nacht", "durch\u00b7wandl'", "ich", "oft", "den", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Leise umwogt nur von des Blattmeers Fl\u00fcstern", "tokens": ["Lei\u00b7se", "um\u00b7wogt", "nur", "von", "des", "Blatt\u00b7meers", "Fl\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "APPR", "ART", "NN", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Lenk\u2019 ich den Schritt zu Felsen schroffen, d\u00fcstern \u2014", "tokens": ["Lenk'", "ich", "den", "Schritt", "zu", "Fel\u00b7sen", "schrof\u00b7fen", ",", "d\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "APPR", "NN", "VVINF", "$,", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An meinem Lieblingsziele bin ich bald.", "tokens": ["An", "mei\u00b7nem", "Lieb\u00b7lings\u00b7zie\u00b7le", "bin", "ich", "bald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nie drang ein Ton der Nachtigall hierher,", "tokens": ["Nie", "drang", "ein", "Ton", "der", "Nach\u00b7ti\u00b7gall", "hier\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Noch eines ander\u2019n Vogels s\u00fc\u00dfes Lied;", "tokens": ["Noch", "ei\u00b7nes", "an\u00b7der'n", "Vo\u00b7gels", "s\u00fc\u00b7\u00dfes", "Lied", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nur in den B\u00e4umen rauscht es tief und schwer,", "tokens": ["Nur", "in", "den", "B\u00e4u\u00b7men", "rauscht", "es", "tief", "und", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und seufzend streicht der Wind lind durch den Ried.", "tokens": ["Und", "seuf\u00b7zend", "streicht", "der", "Wind", "lind", "durch", "den", "Ried", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dumpfbrausend w\u00e4lzt sich \u00fcber das Gestein", "tokens": ["Dumpf\u00b7brau\u00b7send", "w\u00e4lzt", "sich", "\u00fc\u00b7ber", "das", "Ge\u00b7stein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Bach in tiefem wildzerw\u00fchltem Bette,", "tokens": ["Der", "Bach", "in", "tie\u00b7fem", "wild\u00b7zer\u00b7w\u00fchl\u00b7tem", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit unst\u00e4t fahlem, geisterbleichem Schein", "tokens": ["Mit", "un\u00b7st\u00e4t", "fah\u00b7lem", ",", "geis\u00b7ter\u00b7blei\u00b7chem", "Schein"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Umspinnt der Mond die schwermuthsvolle St\u00e4tte.", "tokens": ["Um\u00b7spinnt", "der", "Mond", "die", "schwer\u00b7muths\u00b7vol\u00b7le", "St\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Hier pocht das wilde Herz nicht, Niemand sieht", "tokens": ["Hier", "pocht", "das", "wil\u00b7de", "Herz", "nicht", ",", "Nie\u00b7mand", "sieht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "$,", "PIS", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Brust voll Neugier, doch an Liebe leer,", "tokens": ["Die", "Brust", "voll", "Neu\u00b7gier", ",", "doch", "an", "Lie\u00b7be", "leer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,", "ADV", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Welch\u2019 Weh\u2019 durch meine kranke Seele zieht,", "tokens": ["Welch'", "Weh'", "durch", "mei\u00b7ne", "kran\u00b7ke", "See\u00b7le", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und l\u00e4stiges Fragen qu\u00e4lt mich hier nicht mehr.", "tokens": ["Und", "l\u00e4s\u00b7ti\u00b7ges", "Fra\u00b7gen", "qu\u00e4lt", "mich", "hier", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Du meiner G\u00f6ttin tr\u00f6stende Gestalt,", "tokens": ["Du", "mei\u00b7ner", "G\u00f6t\u00b7tin", "tr\u00f6s\u00b7ten\u00b7de", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vor der zu Schatten werden Freundschaft, Sippen,", "tokens": ["Vor", "der", "zu", "Schat\u00b7ten", "wer\u00b7den", "Freund\u00b7schaft", ",", "Sip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "APPR", "NN", "VAFIN", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "L\u00f6s\u2019st mir mit sanftem Ku\u00df die starren Lippen,", "tokens": ["L\u00f6s'st", "mir", "mit", "sanf\u00b7tem", "Ku\u00df", "die", "star\u00b7ren", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und formst zum Liederstrom des Weh\u2019s Gewalt.", "tokens": ["Und", "formst", "zum", "Lie\u00b7der\u00b7strom", "des", "Weh's", "Ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}