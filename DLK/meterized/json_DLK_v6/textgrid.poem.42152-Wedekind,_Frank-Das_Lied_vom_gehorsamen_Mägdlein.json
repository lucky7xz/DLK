{"textgrid.poem.42152": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Das Lied vom gehorsamen M\u00e4gdlein", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Mutter sprach in ernstem Ton:", "tokens": ["Die", "Mut\u00b7ter", "sprach", "in", "erns\u00b7tem", "Ton", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbdu z\u00e4hlst nun sechzehn Jahre schon;", "tokens": ["\u00bb", "du", "z\u00e4hlst", "nun", "sech\u00b7zehn", "Jah\u00b7re", "schon", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "CARD", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drum, Herzblatt, nimm dich stets in acht,", "tokens": ["Drum", ",", "Herz\u00b7blatt", ",", "nimm", "dich", "stets", "in", "acht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "NN", "$,", "VVIMP", "PPER", "ADV", "APPR", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Besonders bei der Nacht.", "tokens": ["Be\u00b7son\u00b7ders", "bei", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Verlier dich von dem Lebenspfad", "tokens": ["Ver\u00b7lier", "dich", "von", "dem", "Le\u00b7ben\u00b7spfad"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nie seitw\u00e4rts ins Geheg.", "tokens": ["Nie", "seit\u00b7w\u00e4rts", "ins", "Ge\u00b7heg", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Geh immer artig kerzengrad", "tokens": ["Geh", "im\u00b7mer", "ar\u00b7tig", "ker\u00b7zen\u00b7grad"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADV", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den goldenen Mittelweg.\u00ab", "tokens": ["Den", "gol\u00b7de\u00b7nen", "Mit\u00b7tel\u00b7weg", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Da kommt nun in der D\u00e4mmerstund", "tokens": ["Da", "kommt", "nun", "in", "der", "D\u00e4m\u00b7mer\u00b7stund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Pulverm\u00fcllers Heinrich und", "tokens": ["Des", "Pul\u00b7ver\u00b7m\u00fcl\u00b7lers", "Hein\u00b7rich", "und"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "KON"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "K\u00fc\u00dft mich \u2013 mir ward gleich angst und bang \u2013", "tokens": ["K\u00fc\u00dft", "mich", "\u2013", "mir", "ward", "gleich", "angst", "und", "bang", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "PPER", "VAFIN", "ADV", "VVPP", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohl auf die rechte Wang:", "tokens": ["Wohl", "auf", "die", "rech\u00b7te", "Wang", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbo Heinrich, das verbitt ich mir;", "tokens": ["\u00bb", "o", "Hein\u00b7rich", ",", "das", "ver\u00b7bitt", "ich", "mir", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NE", "$,", "PDS", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sieht's Mutter, setzt es Schl\u00e4g'.", "tokens": ["Sieht's", "Mut\u00b7ter", ",", "setzt", "es", "Schl\u00e4g'", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Am allerbesten w\u00e4hlen wir", "tokens": ["Am", "al\u00b7ler\u00b7bes\u00b7ten", "w\u00e4h\u00b7len", "wir"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den goldenen Mittelweg.\u00ab", "tokens": ["Den", "gol\u00b7de\u00b7nen", "Mit\u00b7tel\u00b7weg", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Und pl\u00f6tzlich schreit er glutentflammt:", "tokens": ["Und", "pl\u00f6tz\u00b7lich", "schreit", "er", "glu\u00b7tent\u00b7flammt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbich f\u00fchre dich zum Standesamt! \u2013\u00ab", "tokens": ["\u00bb", "ich", "f\u00fch\u00b7re", "dich", "zum", "Stan\u00b7des\u00b7amt", "!", "\u2013", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbschweig\u00ab, sag ich, \u00bbunversch\u00e4mter Wicht;", "tokens": ["\u00bb", "schweig", "\u00ab", ",", "sag", "ich", ",", "\u00bb", "un\u00b7ver\u00b7sch\u00e4m\u00b7ter", "Wicht", ";"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "ADJD", "$(", "$,", "VVFIN", "PPER", "$,", "$(", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dahin bringst du mich nicht!\u00ab \u2013", "tokens": ["Da\u00b7hin", "bringst", "du", "mich", "nicht", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "PTKNEG", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da fl\u00fcstert er und freut sich schier,", "tokens": ["Da", "fl\u00fcs\u00b7tert", "er", "und", "freut", "sich", "schier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weil ich's mir \u00fcberleg:", "tokens": ["Weil", "ich's", "mir", "\u00fc\u00b7ber\u00b7leg", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "\u00bbnun gut, mein Schatz, dann w\u00e4hlen wir", "tokens": ["\u00bb", "nun", "gut", ",", "mein", "Schatz", ",", "dann", "w\u00e4h\u00b7len", "wir"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ADV", "ADJD", "$,", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den goldenen Mittelweg.\u00ab", "tokens": ["Den", "gol\u00b7de\u00b7nen", "Mit\u00b7tel\u00b7weg", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Und wenn ich nun zur Ruh mich leg,", "tokens": ["Und", "wenn", "ich", "nun", "zur", "Ruh", "mich", "leg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPRART", "NN", "PPER", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir tr\u00e4umt vom goldenen Mittelweg;", "tokens": ["Mir", "tr\u00e4umt", "vom", "gol\u00b7de\u00b7nen", "Mit\u00b7tel\u00b7weg", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Mein Spielzeug macht mir kein Pl\u00e4sier,", "tokens": ["Mein", "Spiel\u00b7zeug", "macht", "mir", "kein", "Pl\u00e4\u00b7sier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich g\u00e4b es gern daf\u00fcr,", "tokens": ["Ich", "g\u00e4b", "es", "gern", "da\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "G\u00e4b meine Schuh, mein R\u00f6cklein fein,", "tokens": ["G\u00e4b", "mei\u00b7ne", "Schuh", ",", "mein", "R\u00f6ck\u00b7lein", "fein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "ADJD", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wei\u00df Gott, ich g\u00e4b noch mehr;", "tokens": ["Wei\u00df", "Gott", ",", "ich", "g\u00e4b", "noch", "mehr", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "H\u00e4tt nie geglaubt, da\u00df ich solch ein", "tokens": ["H\u00e4tt", "nie", "ge\u00b7glaubt", ",", "da\u00df", "ich", "solch", "ein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "VVPP", "$,", "KOUS", "PPER", "PIAT", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Gehorsam M\u00e4gdlein w\u00e4r.", "tokens": ["Ge\u00b7hor\u00b7sam", "M\u00e4gd\u00b7lein", "w\u00e4r", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}