{"textgrid.poem.41820": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Ziel, ein hohes, hatt' ich mir erkoren,", "tokens": ["Ein", "Ziel", ",", "ein", "ho\u00b7hes", ",", "hatt'", "ich", "mir", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,", "VAFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihm weiht ich mich mit allem was ich bin:", "tokens": ["Ihm", "weiht", "ich", "mich", "mit", "al\u00b7lem", "was", "ich", "bin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PRF", "APPR", "PIS", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Freiheit nahm mich auf als Priesterin,", "tokens": ["Die", "Frei\u00b7heit", "nahm", "mich", "auf", "als", "Pries\u00b7te\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In ihrem Dienst hielt ich, was ich geschworen.", "tokens": ["In", "ih\u00b7rem", "Dienst", "hielt", "ich", ",", "was", "ich", "ge\u00b7schwo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und war's ein Spott, den Klugen wie den Thoren,", "tokens": ["Und", "wa\u00b7r's", "ein", "Spott", ",", "den", "Klu\u00b7gen", "wie", "den", "Tho\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "$,", "ART", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ehrt' in der Frau ich auch die B\u00fcrgerin:", "tokens": ["Ehrt'", "in", "der", "Frau", "ich", "auch", "die", "B\u00fcr\u00b7ge\u00b7rin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PPER", "ADV", "ART", "NN", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Klar blieb und fest und unbeirrt mein Sinn,", "tokens": ["Klar", "blieb", "und", "fest", "und", "un\u00b7be\u00b7irrt", "mein", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "KON", "ADJD", "KON", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Nie hab' ich mich von meinem Weg verloren.", "tokens": ["Nie", "hab'", "ich", "mich", "von", "mei\u00b7nem", "Weg", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das Ewige, sich hier schon offenbarend", "tokens": ["Das", "E\u00b7wi\u00b7ge", ",", "sich", "hier", "schon", "of\u00b7fen\u00b7ba\u00b7rend"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "PRF", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In Lenz und Liebe und im Freiheitsdrang,", "tokens": ["In", "Lenz", "und", "Lie\u00b7be", "und", "im", "Frei\u00b7heits\u00b7drang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bis in den Tod in meiner Brust bewahrend.", "tokens": ["Bis", "in", "den", "Tod", "in", "mei\u00b7ner", "Brust", "be\u00b7wah\u00b7rend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und wie ich oft schon ahnend auf mich schwang,", "tokens": ["Und", "wie", "ich", "oft", "schon", "ah\u00b7nend", "auf", "mich", "schwang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "ADV", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nicht M\u00fch' und Not noch j\u00e4hen Sturz befahrend,", "tokens": ["Nicht", "M\u00fch'", "und", "Not", "noch", "j\u00e4\u00b7hen", "Sturz", "be\u00b7fah\u00b7rend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "KON", "NN", "ADV", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bin ich bereit zu meinem letzten Gang!", "tokens": ["Bin", "ich", "be\u00b7reit", "zu", "mei\u00b7nem", "letz\u00b7ten", "Gang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}