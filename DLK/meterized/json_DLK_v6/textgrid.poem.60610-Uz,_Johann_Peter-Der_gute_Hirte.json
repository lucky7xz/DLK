{"textgrid.poem.60610": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Der gute Hirte", "genre": "verse", "period": "N.A.", "pub_year": 1758, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was sorgest du? Sey stille, meine Seele!", "tokens": ["Was", "sor\u00b7gest", "du", "?", "Sey", "stil\u00b7le", ",", "mei\u00b7ne", "See\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "NE", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn Gott ist ein getreuer Hirt,", "tokens": ["Denn", "Gott", "ist", "ein", "ge\u00b7treu\u00b7er", "Hirt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der mir, auch wenn ich mich nicht qu\u00e4le,", "tokens": ["Der", "mir", ",", "auch", "wenn", "ich", "mich", "nicht", "qu\u00e4\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "ADV", "KOUS", "PPER", "PRF", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nichts mangeln lassen wird.", "tokens": ["Nichts", "man\u00b7geln", "las\u00b7sen", "wird", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVINF", "VVINF", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er weidet mich auf bluhmenreicher Aue,", "tokens": ["Er", "wei\u00b7det", "mich", "auf", "bluh\u00b7men\u00b7rei\u00b7cher", "Au\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und f\u00fchrt mich frischen Wassern zu,", "tokens": ["Und", "f\u00fchrt", "mich", "fri\u00b7schen", "Was\u00b7sern", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und bringet mich, im k\u00fchlen Thaue,", "tokens": ["Und", "brin\u00b7get", "mich", ",", "im", "k\u00fch\u00b7len", "Thau\u00b7e", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zur sichern Abendruh.", "tokens": ["Zur", "si\u00b7chern", "A\u00b7ben\u00b7druh", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Er h\u00f6rt nicht auf, mich liebreich zu beschirmen,", "tokens": ["Er", "h\u00f6rt", "nicht", "auf", ",", "mich", "lieb\u00b7reich", "zu", "be\u00b7schir\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PTKVZ", "$,", "PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Schatten vor des Tages Glut,", "tokens": ["Im", "Schat\u00b7ten", "vor", "des", "Ta\u00b7ges", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In seinem Schoo\u00dfe vor den St\u00fcrmen", "tokens": ["In", "sei\u00b7nem", "Schoo\u00b7\u00dfe", "vor", "den", "St\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und schwarzer Bosheit Wuth.", "tokens": ["Und", "schwar\u00b7zer", "Bos\u00b7heit", "Wuth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Auch wenn er mich durch finstre Th\u00e4ler leiten,", "tokens": ["Auch", "wenn", "er", "mich", "durch", "finst\u00b7re", "Th\u00e4\u00b7ler", "lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PRF", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mich durch die W\u00fcste f\u00fchren wird,", "tokens": ["Mich", "durch", "die", "W\u00fcs\u00b7te", "f\u00fch\u00b7ren", "wird", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Will ich nichts f\u00fcrchten! Mir zur Seiten", "tokens": ["Will", "ich", "nichts", "f\u00fcrch\u00b7ten", "!", "Mir", "zur", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PIS", "VVINF", "$.", "NE", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Geht dieser treue Hirt.", "tokens": ["Geht", "die\u00b7ser", "treu\u00b7e", "Hirt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich sehe schon, da\u00df mir von meinem Freunde", "tokens": ["Ich", "se\u00b7he", "schon", ",", "da\u00df", "mir", "von", "mei\u00b7nem", "Freun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein reichrer Tisch bereitet ist,", "tokens": ["Ein", "reich\u00b7rer", "Tisch", "be\u00b7rei\u00b7tet", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Angesichte meiner Feinde,", "tokens": ["Im", "An\u00b7ge\u00b7sich\u00b7te", "mei\u00b7ner", "Fein\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Trotz ihrer Hinterlist.", "tokens": ["Trotz", "ih\u00b7rer", "Hin\u00b7ter\u00b7list", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sie sehn den Schutz des H\u00f6chsten, und sie sch\u00e4men", "tokens": ["Sie", "sehn", "den", "Schutz", "des", "H\u00f6chs\u00b7ten", ",", "und", "sie", "sch\u00e4\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,", "KON", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich ihrer schwach erfundnen Macht.", "tokens": ["Sich", "ih\u00b7rer", "schwach", "er\u00b7fund\u00b7nen", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie sollten mir die Menschen nehmen,", "tokens": ["Wie", "soll\u00b7ten", "mir", "die", "Men\u00b7schen", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was Gott mir zugedacht!", "tokens": ["Was", "Gott", "mir", "zu\u00b7ge\u00b7dacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ich aber will ihn preisen und ihm danken!", "tokens": ["Ich", "a\u00b7ber", "will", "ihn", "prei\u00b7sen", "und", "ihm", "dan\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PPER", "VVFIN", "KON", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich halt alt meinem Hirten fest;", "tokens": ["Ich", "halt", "alt", "mei\u00b7nem", "Hir\u00b7ten", "fest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und mein Vertrauen soll nicht wanken,", "tokens": ["Und", "mein", "Ver\u00b7trau\u00b7en", "soll", "nicht", "wan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn alles mich verl\u00e4\u00dft.", "tokens": ["Wenn", "al\u00b7les", "mich", "ver\u00b7l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}