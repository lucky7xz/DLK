{"textgrid.poem.64350": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "71. Wunsch f\u00fcr mich und meine Freunde", "genre": "verse", "period": "N.A.", "pub_year": 1781, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott gebe Perlen und Edelgestein", "tokens": ["Gott", "ge\u00b7be", "Per\u00b7len", "und", "E\u00b7del\u00b7ge\u00b7stein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "KON", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Und Goldes die F\u00fclle den Narren sein;", "tokens": ["Und", "Gol\u00b7des", "die", "F\u00fcl\u00b7le", "den", "Nar\u00b7ren", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "ART", "NN", "VAINF", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Uns geb' er Schatten im Sonnenschein,", "tokens": ["Uns", "geb'", "er", "Schat\u00b7ten", "im", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und wenn wir durstig sind, alten Wein!", "tokens": ["Und", "wenn", "wir", "durs\u00b7tig", "sind", ",", "al\u00b7ten", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}