{"textgrid.poem.50070": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "26. An unsre Deutsche Poeten", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Tichter, wenn ein Verss aus eurer Feder quillt", "tokens": ["Ihr", "Tich\u00b7ter", ",", "wenn", "ein", "Verss", "aus", "eu\u00b7rer", "Fe\u00b7der", "quillt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um eure Phillis zu bedienen,", "tokens": ["Um", "eu\u00b7re", "Phil\u00b7lis", "zu", "be\u00b7die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So zeigt sich gleich ein ", "tokens": ["So", "zeigt", "sich", "gleich", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "ART"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Ihr ", "tokens": ["Ihr"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Und eure ", "tokens": ["Und", "eu\u00b7re"], "token_info": ["word", "word"], "pos": ["KON", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Weil sie in einem ", "tokens": ["Weil", "sie", "in", "ei\u00b7nem"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Mit einer ", "tokens": ["Mit", "ei\u00b7ner"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "In ", "tokens": ["In"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.11": {"text": "Der Freundschafft f\u00e4hig und verschwiegen,", "tokens": ["Der", "Freund\u00b7schafft", "f\u00e4\u00b7hig", "und", "ver\u00b7schwie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Kurtz, ", "tokens": ["Kurtz", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+", "measure": "single.up"}, "line.13": {"text": "In England wo sie schalt- und walten,", "tokens": ["In", "En\u00b7gland", "wo", "sie", "schal\u00b7t", "und", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PWAV", "PPER", "TRUNC", "KON", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Da werden sie von nichts als ", "tokens": ["Da", "wer\u00b7den", "sie", "von", "nichts", "als"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PIS", "KOKOM"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Ich aber wollt verkehrt ", "tokens": ["Ich", "a\u00b7ber", "wollt", "ver\u00b7kehrt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "VMFIN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Denn eure ", "tokens": ["Denn", "eu\u00b7re"], "token_info": ["word", "word"], "pos": ["KON", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}}}}}