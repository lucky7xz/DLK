{"textgrid.poem.44146": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Schwangern Appetit", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Schwangern Appetit", "tokens": ["Der", "Schwan\u00b7gern", "Ap\u00b7pe\u00b7tit"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ist oftmahls wunderlich.", "tokens": ["Ist", "oft\u00b7mahls", "wun\u00b7der\u00b7lich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn meine Muse nun nach Tobacksliedern krei\u00dfet,", "tokens": ["Wenn", "mei\u00b7ne", "Mu\u00b7se", "nun", "nach", "To\u00b7backs\u00b7lie\u00b7dern", "krei\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So sehnt sie sich nach dem, was Knaster hei\u00dfet.", "tokens": ["So", "sehnt", "sie", "sich", "nach", "dem", ",", "was", "Knas\u00b7ter", "hei\u00b7\u00dfet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "$,", "PWS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das wird ihr wohl von deiner G\u00fctigkeit gew\u00e4hrt;", "tokens": ["Das", "wird", "ihr", "wohl", "von", "dei\u00b7ner", "G\u00fc\u00b7tig\u00b7keit", "ge\u00b7w\u00e4hrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Denn wo sie ihn entbehrt", "tokens": ["Denn", "wo", "sie", "ihn", "ent\u00b7behrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und nachmahls einen Fehl gebiehret,", "tokens": ["Und", "nach\u00b7mahls", "ei\u00b7nen", "Fehl", "ge\u00b7bieh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So wird die Schuld auf dich gef\u00fchret,", "tokens": ["So", "wird", "die", "Schuld", "auf", "dich", "ge\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Geehrter G\u00f6nner, ja, auf dich.", "tokens": ["Ge\u00b7ehr\u00b7ter", "G\u00f6n\u00b7ner", ",", "ja", ",", "auf", "dich", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PTKANT", "$,", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Entschuldige nur mich!", "tokens": ["Ent\u00b7schul\u00b7di\u00b7ge", "nur", "mich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Der Schwangern Appetit", "tokens": ["Der", "Schwan\u00b7gern", "Ap\u00b7pe\u00b7tit"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Ist oftmahls wunderlich.", "tokens": ["Ist", "oft\u00b7mahls", "wun\u00b7der\u00b7lich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}