{"textgrid.poem.60018": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Erhaben ist der innre Friede", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erhaben ist der innre Friede", "tokens": ["Er\u00b7ha\u00b7ben", "ist", "der", "inn\u00b7re", "Frie\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Weisen, g\u00f6ttlich seine Ruh!", "tokens": ["Des", "Wei\u00b7sen", ",", "g\u00f6tt\u00b7lich", "sei\u00b7ne", "Ruh", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gro\u00df ist der Mann, o Philaide!", "tokens": ["Gro\u00df", "ist", "der", "Mann", ",", "o", "Phi\u00b7lai\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "FM", "NE", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Doch gl\u00fccklicher vielleicht bist Du.", "tokens": ["Doch", "gl\u00fcck\u00b7li\u00b7cher", "viel\u00b7leicht", "bist", "Du", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Im schweren Kampfe bracht er Schmerzen", "tokens": ["Im", "schwe\u00b7ren", "Kamp\u00b7fe", "bracht", "er", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Furcht und Sorgen unter sich;", "tokens": ["Und", "Furcht", "und", "Sor\u00b7gen", "un\u00b7ter", "sich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "APPR", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie fortzuk\u00fcssen, fortzuscherzen,", "tokens": ["Sie", "fort\u00b7zu\u00b7k\u00fcs\u00b7sen", ",", "fort\u00b7zu\u00b7scher\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die\u00df lehrten Huldg\u00f6ttinnen dich.", "tokens": ["Die\u00df", "lehr\u00b7ten", "Huld\u00b7g\u00f6t\u00b7tin\u00b7nen", "dich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So h\u00e4ngt, wo sich das Thal geschm\u00fccket,", "tokens": ["So", "h\u00e4ngt", ",", "wo", "sich", "das", "Thal", "ge\u00b7schm\u00fc\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit wildem Drohn ein Fels herab;", "tokens": ["Mit", "wil\u00b7dem", "Drohn", "ein", "Fels", "her\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Hirtin sieht ihn nicht, und pfl\u00fccket", "tokens": ["Die", "Hir\u00b7tin", "sieht", "ihn", "nicht", ",", "und", "pfl\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "An seinem Fu\u00dfe Bl\u00fcmchen ab.", "tokens": ["An", "sei\u00b7nem", "Fu\u00b7\u00dfe", "Bl\u00fcm\u00b7chen", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O la\u00df, beym Klange s\u00fc\u00dfer Lieder,", "tokens": ["O", "la\u00df", ",", "beym", "Klan\u00b7ge", "s\u00fc\u00b7\u00dfer", "Lie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Uns l\u00e4chelnd durch das Leben gehn,", "tokens": ["Uns", "l\u00e4\u00b7chelnd", "durch", "das", "Le\u00b7ben", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und, sinkt die lange Nacht hernieder,", "tokens": ["Und", ",", "sinkt", "die", "lan\u00b7ge", "Nacht", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit diesem L\u00e4cheln stille stehn.", "tokens": ["Mit", "die\u00b7sem", "L\u00e4\u00b7cheln", "stil\u00b7le", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}