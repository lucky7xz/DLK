{"dta.poem.7785": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Die Geschichte des Davids,  \n K\u00f6nigs in Juda.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198429", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In dem wir nun also zu Silo noch verbleiben,", "tokens": ["In", "dem", "wir", "nun", "al\u00b7so", "zu", "Si\u00b7lo", "noch", "ver\u00b7blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ADV", "APPR", "NE", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gieng ich die Traurigkeit einsmahlen zu vertreiben,", "tokens": ["Gieng", "ich", "die", "Trau\u00b7rig\u00b7keit", "eins\u00b7mah\u00b7len", "zu", "ver\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit Beroa, die da des Gera Tochter war,", "tokens": ["Mit", "Be\u00b7roa", ",", "die", "da", "des", "Ge\u00b7ra", "Toch\u00b7ter", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PRELS", "ADV", "ART", "NN", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Am Abend in ein Holtz. Kaum waren wir alldar,", "tokens": ["Am", "A\u00b7bend", "in", "ein", "Holtz", ".", "Kaum", "wa\u00b7ren", "wir", "all\u00b7dar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$.", "ADV", "VAFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da sahn wir uns umringt von unbekannten Leuten,", "tokens": ["Da", "sahn", "wir", "uns", "um\u00b7ringt", "von", "un\u00b7be\u00b7kann\u00b7ten", "Leu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die uns ohn Wortgepr\u00e4ng antrieben fort zu reiten,", "tokens": ["Die", "uns", "ohn", "Wort\u00b7ge\u00b7pr\u00e4ng", "an\u00b7trie\u00b7ben", "fort", "zu", "rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "PTKVZ", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Auf zuger\u00fcste Pferd, man f\u00fchrt uns schier halb todt", "tokens": ["Auf", "zu\u00b7ge\u00b7r\u00fcs\u00b7te", "Pferd", ",", "man", "f\u00fchrt", "uns", "schier", "halb", "todt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PIS", "VVFIN", "PPER", "ADJD", "ADJD", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nach einem Berghaus zu, allda uns diese Rott", "tokens": ["Nach", "ei\u00b7nem", "Berg\u00b7haus", "zu", ",", "all\u00b7da", "uns", "die\u00b7se", "Rott"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,", "KOUS", "PPER", "PDAT", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "In eine Kammer bracht, darinn wir Ada funden,", "tokens": ["In", "ei\u00b7ne", "Kam\u00b7mer", "bracht", ",", "da\u00b7rinn", "wir", "A\u00b7da", "fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "PAV", "PPER", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Jhr Ansehn frischte auf der Seelen tieffe Wunden.", "tokens": ["Ihr", "An\u00b7sehn", "frischte", "auf", "der", "See\u00b7len", "tief\u00b7fe", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.11": {"text": "Sie gienge mich f\u00fcrbey, und sprach Beroa an,", "tokens": ["Sie", "gien\u00b7ge", "mich", "f\u00fcr\u00b7bey", ",", "und", "sprach", "Be\u00b7roa", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Zwar nicht mit H\u00f6flichkeit, dieweil sie die\u00df nicht kan", "tokens": ["Zwar", "nicht", "mit", "H\u00f6f\u00b7lich\u00b7keit", ",", "die\u00b7weil", "sie", "die\u00df", "nicht", "kan"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "APPR", "NN", "$,", "KOUS", "PPER", "PDS", "PTKNEG", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Erzeigen dessen Kind, der ihre Ehr gesch\u00e4ndet:", "tokens": ["Er\u00b7zei\u00b7gen", "des\u00b7sen", "Kind", ",", "der", "ih\u00b7re", "Ehr", "ge\u00b7sch\u00e4n\u00b7det", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So seh ich, da\u00df sich noch nicht gar mein Gl\u00fcck geendet.", "tokens": ["So", "seh", "ich", ",", "da\u00df", "sich", "noch", "nicht", "gar", "mein", "Gl\u00fcck", "ge\u00b7en\u00b7det", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PRF", "ADV", "PTKNEG", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ja! Beroa, du sollt erfahren meinen Grimm.", "tokens": ["Ja", "!", "Be\u00b7roa", ",", "du", "sollt", "er\u00b7fah\u00b7ren", "mei\u00b7nen", "Grimm", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "NE", "$,", "PPER", "VMFIN", "VVINF", "VVFIN", "NE", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.16": {"text": "Nehmt Gera Tochter hin; sprach sie mit lauter Stim\u0303,", "tokens": ["Nehmt", "Ge\u00b7ra", "Toch\u00b7ter", "hin", ";", "sprach", "sie", "mit", "lau\u00b7ter", "Stim\u0303", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NN", "PTKVZ", "$.", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Thut mit ihr was ihr wollt, ich geb sie euch zu eigen.", "tokens": ["Thut", "mit", "ihr", "was", "ihr", "wollt", ",", "ich", "geb", "sie", "euch", "zu", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "PWS", "PPER", "VMFIN", "$,", "PPER", "VVFIN", "PPER", "PPER", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Auf die\u00df geh\u00f6rte Wort, wollt Beroa sich zeigen,", "tokens": ["Auf", "die\u00df", "ge\u00b7h\u00f6r\u00b7te", "Wort", ",", "wollt", "Be\u00b7roa", "sich", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VVFIN", "NN", "$,", "VMFIN", "NE", "PRF", "VVINF", "$,"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.19": {"text": "(was kan bedr\u00e4ngte Noth) und haschet dessen Schwerdt,", "tokens": ["(", "was", "kan", "be\u00b7dr\u00e4ng\u00b7te", "Noth", ")", "und", "ha\u00b7schet", "des\u00b7sen", "Schwerdt", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VMFIN", "ADJA", "NN", "$(", "KON", "VVFIN", "PDS", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Der da zu seiner Lust und Geilheit sie begehrt.", "tokens": ["Der", "da", "zu", "sei\u00b7ner", "Lust", "und", "Geil\u00b7heit", "sie", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "KON", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Sie stellt sich in die Eck, und that sich m\u00e4nnlich wehren,", "tokens": ["Sie", "stellt", "sich", "in", "die", "Eck", ",", "und", "that", "sich", "m\u00e4nn\u00b7lich", "weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ich wollt ihr folgen nach, und ware mein Begehren,", "tokens": ["Ich", "wollt", "ihr", "fol\u00b7gen", "nach", ",", "und", "wa\u00b7re", "mein", "Be\u00b7geh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "PTKVZ", "$,", "KON", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Zu finden eh den Tod als meine Ehr geschw\u00e4cht.", "tokens": ["Zu", "fin\u00b7den", "eh", "den", "Tod", "als", "mei\u00b7ne", "Ehr", "ge\u00b7schw\u00e4cht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KOUS", "ART", "NN", "KOKOM", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Doch Ada spricht mir zu, ich sollt sie h\u00f6ren recht,", "tokens": ["Doch", "A\u00b7da", "spricht", "mir", "zu", ",", "ich", "sollt", "sie", "h\u00f6\u00b7ren", "recht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VMFIN", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Es w\u00e4re nicht um mich die\u00df Wesen angesehen,", "tokens": ["Es", "w\u00e4\u00b7re", "nicht", "um", "mich", "die\u00df", "We\u00b7sen", "an\u00b7ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "APPR", "PPER", "PDS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Besondern nur aus Rach f\u00fcr Gera so geschehen;", "tokens": ["Be\u00b7son\u00b7dern", "nur", "aus", "Rach", "f\u00fcr", "Ge\u00b7ra", "so", "ge\u00b7sche\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "APPR", "NE", "ADV", "VVPP", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Und sollt ich meinen Ha\u00df ja wenden nicht zu ihr,", "tokens": ["Und", "sollt", "ich", "mei\u00b7nen", "Ha\u00df", "ja", "wen\u00b7den", "nicht", "zu", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPOSAT", "NN", "ADV", "VVFIN", "PTKNEG", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Dann was sie h\u00e4tt ver\u00fcbt, w\u00e4r gantz nicht sch\u00e4dlich mir;", "tokens": ["Dann", "was", "sie", "h\u00e4tt", "ver\u00b7\u00fcbt", ",", "w\u00e4r", "gantz", "nicht", "sch\u00e4d\u00b7lich", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "PPER", "VAFIN", "VVPP", "$,", "VAFIN", "ADV", "PTKNEG", "ADJD", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Der Adriel w\u00e4r rein, sie h\u00e4tt es m\u00fcssen sagen,", "tokens": ["Der", "Ad\u00b7riel", "w\u00e4r", "rein", ",", "sie", "h\u00e4tt", "es", "m\u00fcs\u00b7sen", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "ADJD", "$,", "PPER", "VAFIN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Doch der so sie dazu bewegt, den wollt sie plagen", "tokens": ["Doch", "der", "so", "sie", "da\u00b7zu", "be\u00b7wegt", ",", "den", "wollt", "sie", "pla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADV", "PPER", "PAV", "VVFIN", "$,", "ART", "VMFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Mit eben solchem Schimpf, des Gera Kind sollt seyn", "tokens": ["Mit", "e\u00b7ben", "sol\u00b7chem", "Schimpf", ",", "des", "Ge\u00b7ra", "Kind", "sollt", "seyn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "PIAT", "NN", "$,", "ART", "NN", "NN", "VMFIN", "VAINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Mit ihr in gleicher Straff, in gleicher Schmach und Pein.", "tokens": ["Mit", "ihr", "in", "glei\u00b7cher", "Straff", ",", "in", "glei\u00b7cher", "Schmach", "und", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ich blieb hierob best\u00fcrtzt, erschrocken und erfreuet;", "tokens": ["Ich", "blieb", "hier\u00b7ob", "be\u00b7st\u00fcrtzt", ",", "er\u00b7schro\u00b7cken", "und", "er\u00b7freu\u00b7et", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVPP", "$,", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Da\u00df Adriel ohn Schuld, mein Liebesfeur erneuet,", "tokens": ["Da\u00df", "Ad\u00b7riel", "ohn", "Schuld", ",", "mein", "Lie\u00b7bes\u00b7feur", "er\u00b7neu\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "Da\u00df Beroa in Noth, geht mir durch meine Seel,", "tokens": ["Da\u00df", "Be\u00b7roa", "in", "Noth", ",", "geht", "mir", "durch", "mei\u00b7ne", "Seel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "NN", "$,", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.36": {"text": "Was Ada freut, ist die\u00df, wor\u00fcber ich mich qu\u00e4l.", "tokens": ["Was", "A\u00b7da", "freut", ",", "ist", "die\u00df", ",", "wo\u00b7r\u00fc\u00b7ber", "ich", "mich", "qu\u00e4l", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "VVFIN", "$,", "VAFIN", "PDS", "$,", "PWAV", "PPER", "PRF", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Es war die Beroa Melchisua verlobet,", "tokens": ["Es", "war", "die", "Be\u00b7roa", "Melc\u00b7hi\u00b7sua", "ver\u00b7lo\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Drum denck wie es mich schmertzt, da\u00df Ada also tobet.", "tokens": ["Drum", "denck", "wie", "es", "mich", "schmertzt", ",", "da\u00df", "A\u00b7da", "al\u00b7so", "to\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "KOKOM", "PPER", "PRF", "VVFIN", "$,", "KOUS", "NE", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ich meynte, Ada w\u00e4r in Haft, und sehe nun,", "tokens": ["Ich", "meyn\u00b7te", ",", "A\u00b7da", "w\u00e4r", "in", "Haft", ",", "und", "se\u00b7he", "nun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NE", "VAFIN", "APPR", "NN", "$,", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Da\u00df sie gantz frey, und das was ihr beliebt, kan thun.", "tokens": ["Da\u00df", "sie", "gantz", "frey", ",", "und", "das", "was", "ihr", "be\u00b7liebt", ",", "kan", "thun", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "$,", "KON", "PDS", "PRELS", "PPER", "ADJD", "$,", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}