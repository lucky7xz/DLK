{"dta.poem.8383": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Martin Opitzen Sch\u00e4fferey  \n von der Nimfen Hercinie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1630", "urn": "urn:nbn:de:kobv:b4-200905197863", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Sch\u00e4fferey Von der Nimfen Hercinie. Breslau, 1630."}, "poem": {"stanza.1": {"line.1": {"text": "Ist dann kein mittel nicht zue zwingen den gesellen", "tokens": ["Ist", "dann", "kein", "mit\u00b7tel", "nicht", "zue", "zwin\u00b7gen", "den", "ge\u00b7sel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "PTKNEG", "PTKZU", "VVINF", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der eine jungfraw fleucht? soll dann das heil der h\u00f6llen", "tokens": ["Der", "ei\u00b7ne", "jung\u00b7fraw", "fleucht", "?", "soll", "dann", "das", "heil", "der", "h\u00f6l\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "VVFIN", "$.", "VMFIN", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Erst sein herf\u00fcr gesucht? es mu\u00df je sonsten mir", "tokens": ["Erst", "sein", "her\u00b7f\u00fcr", "ge\u00b7sucht", "?", "es", "mu\u00df", "je", "sons\u00b7ten", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADV", "VVPP", "$.", "PPER", "VMFIN", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gehorchen was die welt in see/ in lufft vndt hier", "tokens": ["Ge\u00b7hor\u00b7chen", "was", "die", "welt", "in", "see", "/", "in", "lufft", "vndt", "hier"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PWS", "ART", "NN", "APPR", "NE", "$(", "APPR", "NN", "KON", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "In jhrer scho\u00df verbirgt: die sternen mu\u00dfen schwitzen;", "tokens": ["In", "jhrer", "scho\u00df", "ver\u00b7birgt", ":", "die", "ster\u00b7nen", "mu\u00b7\u00dfen", "schwit\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVFIN", "VVFIN", "$.", "ART", "VVINF", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Der monde stille stehn/ vndt seinen wagen st\u00fctzen;", "tokens": ["Der", "mon\u00b7de", "stil\u00b7le", "stehn", "/", "vndt", "sei\u00b7nen", "wa\u00b7gen", "st\u00fct\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$(", "KON", "PPOSAT", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Nort windt legt den sturm zue meinen f\u00fc\u00dfen hin;", "tokens": ["Der", "Nort", "windt", "legt", "den", "sturm", "zue", "mei\u00b7nen", "f\u00fc\u00b7\u00dfen", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der sommer schneyet mir: es machen wo ich bin", "tokens": ["Der", "som\u00b7mer", "schne\u00b7yet", "mir", ":", "es", "ma\u00b7chen", "wo", "ich", "bin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVFIN", "PPER", "$.", "PPER", "VVINF", "PWAV", "PPER", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die todten sich herzue; auff mein gehei\u00dfe gehen", "tokens": ["Die", "tod\u00b7ten", "sich", "her\u00b7zue", ";", "auff", "mein", "ge\u00b7hei\u00b7\u00dfe", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "PRF", "VVFIN", "$.", "APPR", "PPOSAT", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die starcken eichen fort; die fl\u00fc\u00dfe bleiben stehen;", "tokens": ["Die", "star\u00b7cken", "ei\u00b7chen", "fort", ";", "die", "fl\u00fc\u00b7\u00dfe", "blei\u00b7ben", "ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die klippen sencken sich; die saate reiffet nicht;", "tokens": ["Die", "klip\u00b7pen", "sen\u00b7cken", "sich", ";", "die", "saa\u00b7te", "reif\u00b7fet", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$.", "ART", "ADJA", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die th\u00e4ler steigen auff; der schlangen leib zerbricht;", "tokens": ["Die", "th\u00e4\u00b7ler", "stei\u00b7gen", "auff", ";", "der", "schlan\u00b7gen", "leib", "zer\u00b7bricht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die l\u00f6wen werden zahm was gilt ich wil was finden/", "tokens": ["Die", "l\u00f6\u00b7wen", "wer\u00b7den", "zahm", "was", "gilt", "ich", "wil", "was", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "PWS", "VVFIN", "PPER", "VMFIN", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Den wilden tigersinn genungsam zue entz\u00fcnden!", "tokens": ["Den", "wil\u00b7den", "ti\u00b7ger\u00b7sinn", "ge\u00b7nun\u00b7gsam", "zue", "ent\u00b7z\u00fcn\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Du dreykopff/ Hecate/ die \u00e4lter ist als ich;", "tokens": ["Du", "drey\u00b7kopff", "/", "He\u00b7ca\u00b7te", "/", "die", "\u00e4l\u00b7ter", "ist", "als", "ich", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "CARD", "$(", "NE", "$(", "ART", "ADJD", "VAFIN", "KOUS", "PPER", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "Du geist der diesen berg beherrscher h\u00f6re mich;", "tokens": ["Du", "geist", "der", "die\u00b7sen", "berg", "be\u00b7herr\u00b7scher", "h\u00f6\u00b7re", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PDAT", "NN", "ADJD", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "O Pluto/ komm herauff; ich achte nicht der sachen", "tokens": ["O", "Plu\u00b7to", "/", "komm", "her\u00b7auff", ";", "ich", "ach\u00b7te", "nicht", "der", "sa\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$(", "VVFIN", "PTKVZ", "$.", "PPER", "VVFIN", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die meines alters volck zue langsam reicher machen;", "tokens": ["Die", "mei\u00b7nes", "al\u00b7ters", "volck", "zue", "lang\u00b7sam", "rei\u00b7cher", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVFIN", "ADJD", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ich suche nicht metall/ nicht jaspis/ nicht demant;", "tokens": ["Ich", "su\u00b7che", "nicht", "me\u00b7tall", "/", "nicht", "jas\u00b7pis", "/", "nicht", "de\u00b7mant", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "NE", "$(", "PTKNEG", "ADV", "$(", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ein fester hertz' als er soll werden vmb gewandt.", "tokens": ["Ein", "fes\u00b7ter", "hertz'", "als", "er", "soll", "wer\u00b7den", "vmb", "ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KOUS", "PPER", "VMFIN", "VAINF", "APPR", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die weil kein kr\u00f6tenblut/ noch drummel in den thoren/", "tokens": ["Die", "weil", "kein", "kr\u00f6\u00b7ten\u00b7blut", "/", "noch", "drum\u00b7mel", "in", "den", "tho\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "PIAT", "NN", "$(", "ADV", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Noch federn so die eul hatt vmb ein grab verlohren/", "tokens": ["Noch", "fe\u00b7dern", "so", "die", "eul", "hatt", "vmb", "ein", "grab", "ver\u00b7loh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Noch hei\u00dfe pfeidebrunst/ kein westerhembde nicht/", "tokens": ["Noch", "hei\u00b7\u00dfe", "pfei\u00b7de\u00b7brunst", "/", "kein", "wes\u00b7ter\u00b7hemb\u00b7de", "nicht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVFIN", "$(", "PIAT", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Kein nagel von der handt/ kein haar/ kein blut/ kein liecht", "tokens": ["Kein", "na\u00b7gel", "von", "der", "handt", "/", "kein", "haar", "/", "kein", "blut", "/", "kein", "liecht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "$(", "PIAT", "NN", "$(", "PIAT", "NN", "$(", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Zue rhaten deiner trew/ o jungfraw/ derer schmertzen/", "tokens": ["Zue", "rha\u00b7ten", "dei\u00b7ner", "trew", "/", "o", "jung\u00b7fraw", "/", "de\u00b7rer", "schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "ADJD", "$(", "FM", "FM", "$(", "PDS", "VVINF", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Wie hart' vndt raw ich bin/ mir dringen selbst zue hertzen/", "tokens": ["Wie", "hart'", "vndt", "raw", "ich", "bin", "/", "mir", "drin\u00b7gen", "selbst", "zue", "hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "KON", "NN", "PPER", "VAFIN", "$(", "PPER", "VVFIN", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Bey jhm verfangen wil/ vndt ich vmbsonst gethan", "tokens": ["Bey", "jhm", "ver\u00b7fan\u00b7gen", "wil", "/", "vndt", "ich", "vmbsonst", "ge\u00b7than"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVINF", "VMFIN", "$(", "KON", "PPER", "ADV", "VVPP"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.28": {"text": "Was menschen klugheit wei\u00df/ so helffe was da kan.", "tokens": ["Was", "men\u00b7schen", "klug\u00b7heit", "wei\u00df", "/", "so", "helf\u00b7fe", "was", "da", "kan", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VVFIN", "$(", "ADV", "VVFIN", "PWS", "ADV", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}