{"dta.poem.5871": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Elementen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es scheint demnach, da\u00df wir mit Recht nicht k\u00f6nnen", "tokens": ["Es", "scheint", "dem\u00b7nach", ",", "da\u00df", "wir", "mit", "Recht", "nicht", "k\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "$,", "KOUS", "PPER", "APPR", "NN", "PTKNEG", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Lufft, Erde, Gluth und Fluth die Elemente nennen:", "tokens": ["Lufft", ",", "Er\u00b7de", ",", "Gluth", "und", "Fluth", "die", "E\u00b7le\u00b7men\u00b7te", "nen\u00b7nen", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da diese C\u00f6rper ja, besieht man sie allein,", "tokens": ["Da", "die\u00b7se", "C\u00f6r\u00b7per", "ja", ",", "be\u00b7sieht", "man", "sie", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PTKANT", "$,", "VVFIN", "PIS", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von allen auf der Welt am minsten einfach seyn:", "tokens": ["Von", "al\u00b7len", "auf", "der", "Welt", "am", "mins\u00b7ten", "ein\u00b7fach", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "APPR", "ART", "NN", "APPRART", "ADJA", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da wir dieselben nun durchaus nicht deutlich sehen;", "tokens": ["Da", "wir", "die\u00b7sel\u00b7ben", "nun", "durc\u00b7haus", "nicht", "deut\u00b7lich", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "ADV", "ADV", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kan auch kein deutlicher Begriff daraus entstehen.", "tokens": ["Kan", "auch", "kein", "deut\u00b7li\u00b7cher", "Be\u00b7griff", "da\u00b7raus", "ent\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PIAT", "ADJA", "NN", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ja, wenn man sie so gar", "tokens": ["Ja", ",", "wenn", "man", "sie", "so", "gar"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "PIS", "PPER", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Elementarisch einfach heist;", "tokens": ["E\u00b7lem\u00b7en\u00b7ta\u00b7risch", "ein\u00b7fach", "heist", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ist die Benennung doch nicht klar:", "tokens": ["Ist", "die", "Be\u00b7nen\u00b7nung", "doch", "nicht", "klar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Indem sie unsern Geist", "tokens": ["In\u00b7dem", "sie", "un\u00b7sern", "Geist"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Jm Jrrthum mit sich fort, auch wider Willen reisst.", "tokens": ["Jm", "Jrr\u00b7thum", "mit", "sich", "fort", ",", "auch", "wi\u00b7der", "Wil\u00b7len", "reisst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PRF", "PTKVZ", "$,", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wofern man etwas will kalt, feucht, warm, trocken nennen,", "tokens": ["Wo\u00b7fern", "man", "et\u00b7was", "will", "kalt", ",", "feucht", ",", "warm", ",", "tro\u00b7cken", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PIS", "VMFIN", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "(so ja die ersten Eigenschafften,", "tokens": ["(", "so", "ja", "die", "ers\u00b7ten", "Ei\u00b7gen\u00b7schaff\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Die, wie man spricht, an allen C\u00f6rpern hafften:)", "tokens": ["Die", ",", "wie", "man", "spricht", ",", "an", "al\u00b7len", "C\u00f6r\u00b7pern", "haff\u00b7ten", ":)"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "emoticon"], "pos": ["ART", "$,", "PWAV", "PIS", "VVFIN", "$,", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "So zeigt man zwar zu gleicher Zeit", "tokens": ["So", "zeigt", "man", "zwar", "zu", "glei\u00b7cher", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Derselben Streit und Einigkeit;", "tokens": ["Der\u00b7sel\u00b7ben", "Streit", "und", "Ei\u00b7nig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Allein, man lernt dadurch nicht die Natur erkennen.", "tokens": ["Al\u00b7lein", ",", "man", "lernt", "da\u00b7durch", "nicht", "die", "Na\u00b7tur", "er\u00b7ken\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PIS", "VVFIN", "PAV", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Man hat noch immer M\u00fch sich deutlich zu erkl\u00e4ren,", "tokens": ["Man", "hat", "noch", "im\u00b7mer", "M\u00fch", "sich", "deut\u00b7lich", "zu", "er\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "ADV", "NN", "PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Was warm und kalt, was trocken sey und feucht,", "tokens": ["Was", "warm", "und", "kalt", ",", "was", "tro\u00b7cken", "sey", "und", "feucht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "ADJD", "$,", "PRELS", "ADJD", "VAFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Wie weit die W\u00fcrckungen den C\u00f6rpern angeh\u00f6ren,", "tokens": ["Wie", "weit", "die", "W\u00fcr\u00b7ckun\u00b7gen", "den", "C\u00f6r\u00b7pern", "an\u00b7ge\u00b7h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.21": {"text": "Wie mit dem Werckzeug sich dieselbige vergleicht.", "tokens": ["Wie", "mit", "dem", "Wer\u00b7ck\u00b7zeug", "sich", "die\u00b7sel\u00b7bi\u00b7ge", "ver\u00b7gleicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "PRF", "PDS", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Die Wort\u2019 allein sind nichts. Und, wie ein jedes Wesen", "tokens": ["Die", "Wort'", "al\u00b7lein", "sind", "nichts", ".", "Und", ",", "wie", "ein", "je\u00b7des", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VAFIN", "PIS", "$.", "KON", "$,", "PWAV", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Aus einem Ursprung ja nothwendig erst entstand;", "tokens": ["Aus", "ei\u00b7nem", "Ur\u00b7sprung", "ja", "noth\u00b7wen\u00b7dig", "erst", "ent\u00b7stand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So ist ein Ursprung ja, der selber nicht bekannt,", "tokens": ["So", "ist", "ein", "Ur\u00b7sprung", "ja", ",", "der", "sel\u00b7ber", "nicht", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKANT", "$,", "PRELS", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}