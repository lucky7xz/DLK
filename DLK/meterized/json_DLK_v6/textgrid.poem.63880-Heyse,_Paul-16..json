{"textgrid.poem.63880": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Daran merk' ich, da\u00df lebendig mir das Herz im Busen blieb", "tokens": ["Da\u00b7ran", "merk'", "ich", ",", "da\u00df", "le\u00b7ben\u00b7dig", "mir", "das", "Herz", "im", "Bu\u00b7sen", "blieb"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "$,", "KOUS", "ADJD", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN"], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Und mein Blut noch nicht verst\u00e4ndig-k\u00fchl sich durch die Adern trieb,", "tokens": ["Und", "mein", "Blut", "noch", "nicht", "ver\u00b7st\u00e4n\u00b7dig\u00b7k\u00fchl", "sich", "durch", "die", "A\u00b7dern", "trieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "PTKNEG", "ADJD", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Da es, wenn ich Unbill schaue, schn\u00f6de Vergewaltigung,", "tokens": ["Da", "es", ",", "wenn", "ich", "Un\u00b7bill", "schau\u00b7e", ",", "schn\u00f6\u00b7de", "Ver\u00b7ge\u00b7wal\u00b7ti\u00b7gung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Gleich empor mir schwillt unb\u00e4ndig, abzuwehren Hieb mit Hieb,", "tokens": ["Gleich", "em\u00b7por", "mir", "schwillt", "un\u00b7b\u00e4n\u00b7dig", ",", "ab\u00b7zu\u00b7weh\u00b7ren", "Hieb", "mit", "Hieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "PPER", "VVFIN", "ADJD", "$,", "VVIZU", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Und ich wei\u00df doch, stets auf Erden werde Macht vor Recht ergehn,", "tokens": ["Und", "ich", "wei\u00df", "doch", ",", "stets", "auf", "Er\u00b7den", "wer\u00b7de", "Macht", "vor", "Recht", "er\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,", "ADV", "APPR", "NN", "VAFIN", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Ob ein Gott auch eigenh\u00e4ndig strenge Strafgesetze schrieb.", "tokens": ["Ob", "ein", "Gott", "auch", "ei\u00b7gen\u00b7h\u00e4n\u00b7dig", "stren\u00b7ge", "Straf\u00b7ge\u00b7set\u00b7ze", "schrieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Immer wird die Kraft bewundert, sei sie noch so frevelhaft;", "tokens": ["Im\u00b7mer", "wird", "die", "Kraft", "be\u00b7wun\u00b7dert", ",", "sei", "sie", "noch", "so", "fre\u00b7vel\u00b7haft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Gro\u00dfe s\u00fcndigen best\u00e4ndig, und man h\u00e4ngt den kleinen Dieb.", "tokens": ["Gro\u00b7\u00dfe", "s\u00fcn\u00b7di\u00b7gen", "be\u00b7st\u00e4n\u00b7dig", ",", "und", "man", "h\u00e4ngt", "den", "klei\u00b7nen", "Dieb", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "$,", "KON", "PIS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Aber ist's auch toll und t\u00f6richt, gegen ein Naturgesetz", "tokens": ["A\u00b7ber", "ist's", "auch", "toll", "und", "t\u00f6\u00b7richt", ",", "ge\u00b7gen", "ein", "Na\u00b7tur\u00b7ge\u00b7setz"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "KON", "VVFIN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Aufzub\u00e4umen, doch verst\u00e4nd' ich nie zu z\u00fcgeln diesen Trieb.", "tokens": ["Auf\u00b7zu\u00b7b\u00e4u\u00b7men", ",", "doch", "ver\u00b7st\u00e4nd'", "ich", "nie", "zu", "z\u00fc\u00b7geln", "die\u00b7sen", "Trieb", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "PDAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "War der Ritter von la Mancha gl\u00fccklicher, vom Wahn geheilt?", "tokens": ["War", "der", "Rit\u00b7ter", "von", "la", "Man\u00b7cha", "gl\u00fcck\u00b7li\u00b7cher", ",", "vom", "Wahn", "ge\u00b7heilt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "NE", "NE", "ADJD", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Drum an diesen Wahn verschwend' ich, was mir noch von Jugend blieb!", "tokens": ["Drum", "an", "die\u00b7sen", "Wahn", "ver\u00b7schwend'", "ich", ",", "was", "mir", "noch", "von", "Ju\u00b7gend", "blieb", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "PDAT", "NN", "VVFIN", "PPER", "$,", "PWS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+---+-+-+-+-+-+", "measure": "dactylic.init"}}}}}