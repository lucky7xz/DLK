{"textgrid.poem.61479": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freude schweift in die Welt hinaus,", "tokens": ["Freu\u00b7de", "schweift", "in", "die", "Welt", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "APZR", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Bricht jede Frucht und kostet jeden Wein;", "tokens": ["Bricht", "je\u00b7de", "Frucht", "und", "kos\u00b7tet", "je\u00b7den", "Wein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "KON", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Riefe dich nicht das Leid nach Haus,", "tokens": ["Rie\u00b7fe", "dich", "nicht", "das", "Leid", "nach", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "ART", "NN", "APPR", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Du kehrtest nimmer bei dir selber ein.", "tokens": ["Du", "kehr\u00b7test", "nim\u00b7mer", "bei", "dir", "sel\u00b7ber", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}