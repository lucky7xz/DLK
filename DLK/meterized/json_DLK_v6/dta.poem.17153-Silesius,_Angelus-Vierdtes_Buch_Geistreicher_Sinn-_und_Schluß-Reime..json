{"dta.poem.17153": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "Vierdtes Buch  \n Geistreicher Sinn- und  \n Schlu\u00df-Reime.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Hertze zwar wann es ein Blumenthal geworden/", "tokens": ["Mein", "Hert\u00b7ze", "zwar", "wann", "es", "ein", "Blu\u00b7ment\u00b7hal", "ge\u00b7wor\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "ADV", "PWAV", "PPER", "ART", "NN", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Seele wann sie steht im keuschen Jungfern Orden/", "tokens": ["Die", "See\u00b7le", "wann", "sie", "steht", "im", "keu\u00b7schen", "Jung\u00b7fern", "Or\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PWAV", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wohnt in diesem Thal: das neue Gnaden Licht/", "tokens": ["Und", "wohnt", "in", "die\u00b7sem", "Thal", ":", "das", "neu\u00b7e", "Gna\u00b7den", "Licht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PDAT", "NN", "$.", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wann Gott sein Ewges Wort in jhrem Geiste spricht.", "tokens": ["Wann", "Gott", "sein", "Ew\u00b7ges", "Wort", "in", "jhrem", "Geis\u00b7te", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}}}}