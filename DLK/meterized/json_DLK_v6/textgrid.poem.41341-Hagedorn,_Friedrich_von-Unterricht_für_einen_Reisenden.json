{"textgrid.poem.41341": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Unterricht f\u00fcr einen Reisenden", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn dir ein Mann, den du nicht kennst, begegnet,", "tokens": ["Wenn", "dir", "ein", "Mann", ",", "den", "du", "nicht", "kennst", ",", "be\u00b7geg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der l\u00e4chelnd schleicht, und dich durch Minen segnet,", "tokens": ["Der", "l\u00e4\u00b7chelnd", "schleicht", ",", "und", "dich", "durch", "Mi\u00b7nen", "seg\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "$,", "KON", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Scharf nach dir schielt, sich ehrerbietig kr\u00fcmmt,", "tokens": ["Scharf", "nach", "dir", "schielt", ",", "sich", "ehr\u00b7er\u00b7bie\u00b7tig", "kr\u00fcmmt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "VVFIN", "$,", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gebete brummt, und tiefe Seufzer stimmt:", "tokens": ["Ge\u00b7be\u00b7te", "brummt", ",", "und", "tie\u00b7fe", "Seuf\u00b7zer", "stimmt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und ein Husar, wovor der Anblick schrecket,", "tokens": ["Und", "ein", "Hu\u00b7sar", ",", "wo\u00b7vor", "der", "An\u00b7blick", "schre\u00b7cket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dem das Gesicht Blut, Staub und Pulver decket,", "tokens": ["Dem", "das", "Ge\u00b7sicht", "Blut", ",", "Staub", "und", "Pul\u00b7ver", "de\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "Zugleich erscheint: w\u00e4r' er, nach Mentzels Art,", "tokens": ["Zu\u00b7gleich", "er\u00b7scheint", ":", "w\u00e4r'", "er", ",", "nach", "Ment\u00b7zels", "Art", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "VAFIN", "PPER", "$,", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Frech, wie sein Pferd, und rauher, als sein Bart;", "tokens": ["Frech", ",", "wie", "sein", "Pferd", ",", "und", "rau\u00b7her", ",", "als", "sein", "Bart", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPOSAT", "NN", "$,", "KON", "ADJD", "$,", "KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "So rath ich dir, was mir Erfahrne riethen,", "tokens": ["So", "rath", "ich", "dir", ",", "was", "mir", "Er\u00b7fahr\u00b7ne", "riet\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "PWS", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Vor jenem mehr, als diesem, dich zu h\u00fcten.", "tokens": ["Vor", "je\u00b7nem", "mehr", ",", "als", "die\u00b7sem", ",", "dich", "zu", "h\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADV", "$,", "KOUS", "PDAT", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}