{"dta.poem.9174": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "IiI.  \n An seine Rosilis/ als ihm bey ihr das Hau\u00df  \n verdotten ward.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Ljebste seele siehst du nicht/", "tokens": ["Ljebs\u00b7te", "see\u00b7le", "siehst", "du", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ich mich allhier betr\u00fcbe/", "tokens": ["Wie", "ich", "mich", "all\u00b7hier", "be\u00b7tr\u00fc\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil in unsrer stillen liebe/", "tokens": ["Weil", "in", "uns\u00b7rer", "stil\u00b7len", "lie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "ADJA", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hoffnung/ r\u00e4th und trost gebricht/", "tokens": ["Hoff\u00b7nung", "/", "r\u00e4\u00b7th", "und", "trost", "ge\u00b7bricht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVFIN", "KON", "NN", "VVFIN", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "H\u00f6rst du nicht die trauer-worte/", "tokens": ["H\u00f6rst", "du", "nicht", "die", "trau\u00b7e\u00b7rwor\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Manchesmal von weiten an/", "tokens": ["Man\u00b7ches\u00b7mal", "von", "wei\u00b7ten", "an", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die ich an dem lieben orte/", "tokens": ["Die", "ich", "an", "dem", "lie\u00b7ben", "or\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nicht so frey vergiessen kan.", "tokens": ["Nicht", "so", "frey", "ver\u00b7gies\u00b7sen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Liebstes kind was machst du doch?", "tokens": ["Liebs\u00b7tes", "kind", "was", "machst", "du", "doch", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hast du irgend unterdessen/", "tokens": ["Hast", "du", "ir\u00b7gend", "un\u00b7ter\u00b7des\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Meiner allbereit vergessen/", "tokens": ["Mei\u00b7ner", "all\u00b7be\u00b7reit", "ver\u00b7ges\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder denckst du meiner noch?", "tokens": ["O\u00b7der", "denckst", "du", "mei\u00b7ner", "noch", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gl\u00e4ubst du wohl in deinem hertzen/", "tokens": ["Gl\u00e4ubst", "du", "wohl", "in", "dei\u00b7nem", "hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ob es immer m\u00f6glich ist/", "tokens": ["Ob", "es", "im\u00b7mer", "m\u00f6g\u00b7lich", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df du noch in meinem schmertzen", "tokens": ["Da\u00df", "du", "noch", "in", "mei\u00b7nem", "schmert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPOSAT", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ein betr\u00fcbtes labsal bist?", "tokens": ["Ein", "be\u00b7tr\u00fcb\u00b7tes", "lab\u00b7sal", "bist", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Freylich denck ich immerdar/", "tokens": ["Frey\u00b7lich", "denck", "ich", "im\u00b7mer\u00b7dar", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "An die liebe zeit zur\u00fccke/", "tokens": ["An", "die", "lie\u00b7be", "zeit", "zu\u00b7r\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da das wanckelbare gl\u00fccke/", "tokens": ["Da", "das", "wan\u00b7ckel\u00b7ba\u00b7re", "gl\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir ein bi\u00dfgen g\u00fcnstig war/", "tokens": ["Mir", "ein", "bi\u00df\u00b7gen", "g\u00fcns\u00b7tig", "war", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADV", "ADJD", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da ich heimlich und verschwiegen", "tokens": ["Da", "ich", "heim\u00b7lich", "und", "ver\u00b7schwie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mehr als ein bekannter hie\u00df/", "tokens": ["Mehr", "als", "ein", "be\u00b7kann\u00b7ter", "hie\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "ADJA", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und mein hertzliches vergn\u00fcgen/", "tokens": ["Und", "mein", "hertz\u00b7li\u00b7ches", "ver\u00b7gn\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Alle tage wachsen lie\u00df.", "tokens": ["Al\u00b7le", "ta\u00b7ge", "wach\u00b7sen", "lie\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Nun ist alles umgewandt/", "tokens": ["Nun", "ist", "al\u00b7les", "um\u00b7ge\u00b7wandt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "An den reden und geberden", "tokens": ["An", "den", "re\u00b7den", "und", "ge\u00b7ber\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "VVINF", "KON", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Und wir arme kinder-werden/", "tokens": ["Und", "wir", "ar\u00b7me", "kin\u00b7der\u00b7wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Endlich wieder unbekandt:", "tokens": ["End\u00b7lich", "wie\u00b7der", "un\u00b7be\u00b7kandt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und so fern wir noch zu zeiten/", "tokens": ["Und", "so", "fern", "wir", "noch", "zu", "zei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "PPER", "ADV", "APPR", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hier und da einander sehn/", "tokens": ["Hier", "und", "da", "ein\u00b7an\u00b7der", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "ADV", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mu\u00df es doch nur vor den leuten/", "tokens": ["Mu\u00df", "es", "doch", "nur", "vor", "den", "leu\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und mit schlechter luft geschehn.", "tokens": ["Und", "mit", "schlech\u00b7ter", "luft", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Ich erblicke zwar dein hau\u00df/", "tokens": ["Ich", "er\u00b7bli\u00b7cke", "zwar", "dein", "hau\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch die th\u00fcr ist mir verschlossen/", "tokens": ["Doch", "die", "th\u00fcr", "ist", "mir", "ver\u00b7schlos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "VVPP", "$("], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Und es giebet keinen possen/", "tokens": ["Und", "es", "gie\u00b7bet", "kei\u00b7nen", "pos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn die liebe lust ist aus/", "tokens": ["Denn", "die", "lie\u00b7be", "lust", "ist", "aus", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "APPR", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und weil in der menschen sachen", "tokens": ["Und", "weil", "in", "der", "men\u00b7schen", "sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Alles nur ein weilgen wehrt/", "tokens": ["Al\u00b7les", "nur", "ein", "weil\u00b7gen", "wehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ART", "ADJA", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mu\u00df mein sinn die rechnung machen/", "tokens": ["Mu\u00df", "mein", "sinn", "die", "rech\u00b7nung", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df ers nur umbsonst begehrt.", "tokens": ["Da\u00df", "ers", "nur", "um\u00b7bsonst", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "6. Liebstes hertze lebe wohl/", "tokens": ["Liebs\u00b7tes", "hert\u00b7ze", "le\u00b7be", "wohl", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Leb in angenehmen freuden", "tokens": ["Leb", "in", "an\u00b7ge\u00b7neh\u00b7men", "freu\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil ich doch mit angst und leiden/", "tokens": ["Weil", "ich", "doch", "mit", "angst", "und", "lei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "VVPP", "KON", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine seele speisen soll/", "tokens": ["Mei\u00b7ne", "see\u00b7le", "spei\u00b7sen", "soll", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Brauche deiner sch\u00f6nen jugend/", "tokens": ["Brau\u00b7che", "dei\u00b7ner", "sch\u00f6\u00b7nen", "ju\u00b7gend", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und vers\u00e4ume keinen tag/", "tokens": ["Und", "ver\u00b7s\u00e4u\u00b7me", "kei\u00b7nen", "tag", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ich bin froh da\u00df ich die tugend", "tokens": ["Ich", "bin", "froh", "da\u00df", "ich", "die", "tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "KOUS", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nur abwesend ehren mag.", "tokens": ["Nur", "ab\u00b7we\u00b7send", "eh\u00b7ren", "mag."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["ADV", "ADJD", "VVINF", "VMFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "7. Andre m\u00e4dgen k\u00f6nnen mich/", "tokens": ["And\u00b7re", "m\u00e4d\u00b7gen", "k\u00f6n\u00b7nen", "mich", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVINF", "VMFIN", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwerlich zu der liebe zwingen/", "tokens": ["Schwer\u00b7lich", "zu", "der", "lie\u00b7be", "zwin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dann ich seh vor allen dingen/", "tokens": ["Dann", "ich", "seh", "vor", "al\u00b7len", "din\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Liebstes hertze nur auff dich:", "tokens": ["Liebs\u00b7tes", "hert\u00b7ze", "nur", "auff", "dich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Werden deine sch\u00f6ne gaben", "tokens": ["Wer\u00b7den", "dei\u00b7ne", "sch\u00f6\u00b7ne", "ga\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "ADJA", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mir verriegelt und verpf\u00e4lt/", "tokens": ["Mir", "ver\u00b7rie\u00b7gelt", "und", "ver\u00b7pf\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Will ich doch die ehre haben/", "tokens": ["Will", "ich", "doch", "die", "eh\u00b7re", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VAFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df mir nicht der wille fehlt.", "tokens": ["Da\u00df", "mir", "nicht", "der", "wil\u00b7le", "fehlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}