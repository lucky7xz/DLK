{"textgrid.poem.41736": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbhier, diesen Harnisch hat ein Weib getragen\u00ab,", "tokens": ["\u00bb", "hier", ",", "die\u00b7sen", "Har\u00b7nisch", "hat", "ein", "Weib", "ge\u00b7tra\u00b7gen", "\u00ab", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "$,", "PDAT", "NN", "VAFIN", "ART", "NN", "VVPP", "$(", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sprach in der Burg der alte Kastellan.", "tokens": ["Sprach", "in", "der", "Burg", "der", "al\u00b7te", "Kas\u00b7tel\u00b7lan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Wohl gilt's jetzt nicht, das Herz in Erz zu schlagen.", "tokens": ["Wohl", "gilt's", "jetzt", "nicht", ",", "das", "Herz", "in", "Erz", "zu", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKNEG", "$,", "ART", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df nicht ermordend ihm die Feinde nahn!", "tokens": ["Da\u00df", "nicht", "er\u00b7mor\u00b7dend", "ihm", "die", "Fein\u00b7de", "nahn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "VVPP", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mein weiblich Herz wollt ihr mit Gift verwunden \u2013", "tokens": ["Mein", "weib\u00b7lich", "Herz", "wollt", "ihr", "mit", "Gift", "ver\u00b7wun\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VMFIN", "PPER", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wohl bitter hat es euer Thun empfunden!", "tokens": ["Wohl", "bit\u00b7ter", "hat", "es", "eu\u00b7er", "Thun", "emp\u00b7fun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Doch mag es nimmer andern Schirm und Schild,", "tokens": ["Doch", "mag", "es", "nim\u00b7mer", "an\u00b7dern", "Schirm", "und", "Schild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Als die Begeistrung, die vom Herzen quillt.", "tokens": ["Als", "die", "Be\u00b7geis\u00b7trung", ",", "die", "vom", "Her\u00b7zen", "quillt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hoch am Himmel stand die Sonne,", "tokens": ["Hoch", "am", "Him\u00b7mel", "stand", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gleich einem Engel mit goldenen Fl\u00fcgeln", "tokens": ["Gleich", "ei\u00b7nem", "En\u00b7gel", "mit", "gol\u00b7de\u00b7nen", "Fl\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ausgesendet vom Thron des H\u00f6chsten,", "tokens": ["Aus\u00b7ge\u00b7sen\u00b7det", "vom", "Thron", "des", "H\u00f6chs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Zu segnen die Erde mit Glanz und W\u00e4rme.", "tokens": ["Zu", "seg\u00b7nen", "die", "Er\u00b7de", "mit", "Glanz", "und", "W\u00e4r\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Und der Engel breitete", "tokens": ["Und", "der", "En\u00b7gel", "brei\u00b7te\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Die strahlenden Arme weit aus \u2013", "tokens": ["Die", "strah\u00b7len\u00b7den", "Ar\u00b7me", "weit", "aus", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PTKVZ", "$("], "meter": "-+--+---", "measure": "iambic.di.relaxed"}, "line.3": {"text": "Und es war als z\u00f6g er die aufatmende Erde", "tokens": ["Und", "es", "war", "als", "z\u00f6g", "er", "die", "auf\u00b7at\u00b7men\u00b7de", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "KOKOM", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "N\u00e4her dem Himmel, n\u00e4her der Gottheit.", "tokens": ["N\u00e4\u00b7her", "dem", "Him\u00b7mel", ",", "n\u00e4\u00b7her", "der", "Got\u00b7theit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Goldene Strahlenringe zog der Engel von seinen Fingern,", "tokens": ["Gol\u00b7de\u00b7ne", "Strah\u00b7len\u00b7rin\u00b7ge", "zog", "der", "En\u00b7gel", "von", "sei\u00b7nen", "Fin\u00b7gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+--+-+-", "measure": "iambic.septa.invert"}, "line.6": {"text": "Verteilte sie dahin und dorthin;", "tokens": ["Ver\u00b7teil\u00b7te", "sie", "da\u00b7hin", "und", "dor\u00b7thin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "KON", "ADV", "$."], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.7": {"text": "Und die Ringe wurden zu Heiligenscheinen,", "tokens": ["Und", "die", "Rin\u00b7ge", "wur\u00b7den", "zu", "Hei\u00b7li\u00b7gen\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "APPR", "NN", "$,"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Zu Himmelsglorien auf den Gipfeln der Berge,", "tokens": ["Zu", "Him\u00b7mels\u00b7glo\u00b7ri\u00b7en", "auf", "den", "Gip\u00b7feln", "der", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Dahin sie der Engel geworfen.", "tokens": ["Da\u00b7hin", "sie", "der", "En\u00b7gel", "ge\u00b7wor\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Und solch eine Himmelsglorie,", "tokens": ["Und", "solch", "ei\u00b7ne", "Him\u00b7mels\u00b7glo\u00b7rie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Solch ein Heiligenschein kr\u00f6nte noch einmal", "tokens": ["Solch", "ein", "Hei\u00b7li\u00b7gen\u00b7schein", "kr\u00f6n\u00b7te", "noch", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "ART", "NN", "VVFIN", "ADV", "ADV"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Die Krone der Burgen des Th\u00fcringer Waldes:", "tokens": ["Die", "Kro\u00b7ne", "der", "Bur\u00b7gen", "des", "Th\u00fc\u00b7rin\u00b7ger", "Wal\u00b7des", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Ich stand und schaute.", "tokens": ["Ich", "stand", "und", "schau\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "So lange ich daheim verweilt", "tokens": ["So", "lan\u00b7ge", "ich", "da\u00b7heim", "ver\u00b7weilt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein spielendes Kind, eine sinnende Jungfrau", "tokens": ["Ein", "spie\u00b7len\u00b7des", "Kind", ",", "ei\u00b7ne", "sin\u00b7nen\u00b7de", "Jung\u00b7frau"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+--+--+---+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "An den Ufern der Elbe, wo uralte Burgen", "tokens": ["An", "den", "U\u00b7fern", "der", "El\u00b7be", ",", "wo", "ur\u00b7al\u00b7te", "Bur\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NE", "$,", "PWAV", "ADJA", "NN"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.5": {"text": "Verwitterte Kl\u00f6ster unheimlich mahnen", "tokens": ["Ver\u00b7wit\u00b7ter\u00b7te", "Kl\u00f6s\u00b7ter", "un\u00b7heim\u00b7lich", "mah\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJD", "VVINF"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "An des Mittelalters ", "tokens": ["An", "des", "Mit\u00b7tel\u00b7al\u00b7ters"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "An den Ufern der Elbe, wo gr\u00fcnende Reben", "tokens": ["An", "den", "U\u00b7fern", "der", "El\u00b7be", ",", "wo", "gr\u00fc\u00b7nen\u00b7de", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NE", "$,", "PWAV", "ADJA", "NN"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.8": {"text": "Mit reifenden Trauben verhei\u00dfend mahnen", "tokens": ["Mit", "rei\u00b7fen\u00b7den", "Trau\u00b7ben", "ver\u00b7hei\u00b7\u00dfend", "mah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP", "VVFIN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "An der neuen Zeiten ", "tokens": ["An", "der", "neu\u00b7en", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "So lang ich daheim verweilt an den Ufern der Elbe,", "tokens": ["So", "lang", "ich", "da\u00b7heim", "ver\u00b7weilt", "an", "den", "U\u00b7fern", "der", "El\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NE", "$,"], "meter": "-+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Den reben- und burgbekr\u00e4nzten, so lange auch weilte", "tokens": ["Den", "re\u00b7ben", "und", "burg\u00b7be\u00b7kr\u00e4nz\u00b7ten", ",", "so", "lan\u00b7ge", "auch", "weil\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "TRUNC", "KON", "VVFIN", "$,", "ADV", "ADV", "ADV", "VVFIN"], "meter": "-+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Die Sehnsucht in meiner Brust nach der Krone der Burgen", "tokens": ["Die", "Sehn\u00b7sucht", "in", "mei\u00b7ner", "Brust", "nach", "der", "Kro\u00b7ne", "der", "Bur\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Des Th\u00fcringer Waldes: der uralten Wartburg.", "tokens": ["Des", "Th\u00fc\u00b7rin\u00b7ger", "Wal\u00b7des", ":", "der", "ur\u00b7al\u00b7ten", "Wart\u00b7burg", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+--+--++-+-", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Nun stand sie in Himmelsglorie mit dem Heiligenschein", "tokens": ["Nun", "stand", "sie", "in", "Him\u00b7mels\u00b7glo\u00b7rie", "mit", "dem", "Hei\u00b7li\u00b7gen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "APPR", "ART", "NN"], "meter": "-+--+-+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Vor den trunkenen Blicken.", "tokens": ["Vor", "den", "trun\u00b7ke\u00b7nen", "Bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Meine H\u00e4nde waren gefalten,", "tokens": ["Mei\u00b7ne", "H\u00e4n\u00b7de", "wa\u00b7ren", "ge\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Thr\u00e4nen mir in den Augen wallten,", "tokens": ["Thr\u00e4\u00b7nen", "mir", "in", "den", "Au\u00b7gen", "wall\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Nieder ein Tropfen fiel:", "tokens": ["Nie\u00b7der", "ein", "Trop\u00b7fen", "fiel", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}