{"textgrid.poem.34120": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Goethes Belobungen", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Goethe hat ihn gelobt. Das hei\u00dft: er hat ihn geadelt,", "tokens": ["Goe\u00b7the", "hat", "ihn", "ge\u00b7lobt", ".", "Das", "hei\u00dft", ":", "er", "hat", "ihn", "ge\u00b7a\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "VVPP", "$.", "PDS", "VVFIN", "$.", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+--+-+----+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Hat zum Baron ihn gemacht. F\u00fcrsten erlauben sich viel.", "tokens": ["Hat", "zum", "Ba\u00b7ron", "ihn", "ge\u00b7macht", ".", "F\u00fcrs\u00b7ten", "er\u00b7lau\u00b7ben", "sich", "viel", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "PPER", "VVPP", "$.", "NN", "VVFIN", "PRF", "ADV", "$."], "meter": "--+-+-++--+--+", "measure": "iambic.hexa.relaxed"}}}}}