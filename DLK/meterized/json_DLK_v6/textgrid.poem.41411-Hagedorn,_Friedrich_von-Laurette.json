{"textgrid.poem.41411": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Laurette", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was k\u00f6nnen Witz und Liebe nicht,", "tokens": ["Was", "k\u00f6n\u00b7nen", "Witz", "und", "Lie\u00b7be", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "NN", "KON", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn beide sich genau vereinen!", "tokens": ["Wenn", "bei\u00b7de", "sich", "ge\u00b7nau", "ver\u00b7ei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dann wird, wann uns ein Rath gebricht,", "tokens": ["Dann", "wird", ",", "wann", "uns", "ein", "Rath", "ge\u00b7bricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Anschlag von sich selbst erscheinen.", "tokens": ["Der", "An\u00b7schlag", "von", "sich", "selbst", "er\u00b7schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Denn Amor ist noch so verschmitzt,", "tokens": ["Denn", "A\u00b7mor", "ist", "noch", "so", "ver\u00b7schmitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als wir in den Geschichten lesen,", "tokens": ["Als", "wir", "in", "den", "Ge\u00b7schich\u00b7ten", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Und, wann der Schalk ein Herz besitzt,", "tokens": ["Und", ",", "wann", "der", "Schalk", "ein", "Herz", "be\u00b7sitzt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So muthig, wie er sonst gewesen.", "tokens": ["So", "mut\u00b7hig", ",", "wie", "er", "sonst", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "PPER", "ADV", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Boccaz hat ihn genau gekannt,", "tokens": ["Boc\u00b7caz", "hat", "ihn", "ge\u00b7nau", "ge\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er lehret viel von seinen Streichen,", "tokens": ["Er", "leh\u00b7ret", "viel", "von", "sei\u00b7nen", "Strei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und glaubt, es werde durch Verstand", "tokens": ["Und", "glaubt", ",", "es", "wer\u00b7de", "durch", "Ver\u00b7stand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PPER", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Liebe stets den Zweck erreichen.", "tokens": ["Die", "Lie\u00b7be", "stets", "den", "Zweck", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "In Welschland war ein junges Weib,", "tokens": ["In", "Wel\u00b7schland", "war", "ein", "jun\u00b7ges", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem weder Reiz noch Regung fehlte;", "tokens": ["Dem", "we\u00b7der", "Reiz", "noch", "Re\u00b7gung", "fehl\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KON", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nichts \u00fcbertraf den sch\u00f6nen Leib,", "tokens": ["Nichts", "\u00fc\u00b7bert\u00b7raf", "den", "sch\u00f6\u00b7nen", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als nur der Geist, der ihn beseelte.", "tokens": ["Als", "nur", "der", "Geist", ",", "der", "ihn", "be\u00b7seel\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der schwarzen Augen schlauer Scherz,", "tokens": ["Der", "schwar\u00b7zen", "Au\u00b7gen", "schlau\u00b7er", "Scherz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Anstand lockender Geberden", "tokens": ["Der", "An\u00b7stand", "lo\u00b7cken\u00b7der", "Ge\u00b7ber\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Bezauberten ein jedes Herz,", "tokens": ["Be\u00b7zau\u00b7ber\u00b7ten", "ein", "je\u00b7des", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PIAT", "NN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.8": {"text": "Und mu\u00dften Gismunds Meister werden.", "tokens": ["Und", "mu\u00df\u00b7ten", "Gis\u00b7munds", "Meis\u00b7ter", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NE", "NN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Laurette wird von ihm verehrt,", "tokens": ["Lau\u00b7ret\u00b7te", "wird", "von", "ihm", "ver\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "(so wollen wir die Sch\u00f6ne nennen;)", "tokens": ["(", "so", "wol\u00b7len", "wir", "die", "Sch\u00f6\u00b7ne", "nen\u00b7nen", ";)"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "emoticon"], "pos": ["$(", "ADV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Allein sie sch\u00e4tzet ihn nicht werth,", "tokens": ["Al\u00b7lein", "sie", "sch\u00e4t\u00b7zet", "ihn", "nicht", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ihm ihre Gegengunst zu g\u00f6nnen.", "tokens": ["Ihm", "ih\u00b7re", "Ge\u00b7gen\u00b7gunst", "zu", "g\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie widersteht der Schmeichelei,", "tokens": ["Sie", "wi\u00b7der\u00b7steht", "der", "Schmei\u00b7che\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und, was noch mehr, auch den Geschenken.", "tokens": ["Und", ",", "was", "noch", "mehr", ",", "auch", "den", "Ge\u00b7schen\u00b7ken", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "ADV", "ADV", "$,", "ADV", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Warum? sie selbst ist nicht mehr frei,", "tokens": ["Wa\u00b7rum", "?", "sie", "selbst", "ist", "nicht", "mehr", "frei", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PPER", "ADV", "VAFIN", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und kann an Guido nur gedenken;", "tokens": ["Und", "kann", "an", "Gui\u00b7do", "nur", "ge\u00b7den\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "NE", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "An Guido nur, der ihr gef\u00e4llt,", "tokens": ["An", "Gui\u00b7do", "nur", ",", "der", "ihr", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und jenem schon zuvorgekommen;", "tokens": ["Und", "je\u00b7nem", "schon", "zu\u00b7vor\u00b7ge\u00b7kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Drum wird vor Gismund, und der Welt", "tokens": ["Drum", "wird", "vor", "Gis\u00b7mund", ",", "und", "der", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VAFIN", "APPR", "NN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Ernst voll Keuschheit angenommen,", "tokens": ["Ein", "Ernst", "voll", "Keuschheit", "an\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Ein unerheitertes Gesicht,", "tokens": ["Ein", "un\u00b7er\u00b7hei\u00b7ter\u00b7tes", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ein Wohlstand, der in Ehrfurcht setzet,", "tokens": ["Ein", "Wohl\u00b7stand", ",", "der", "in", "Ehr\u00b7furcht", "set\u00b7zet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Und Tugend, Ehrbarkeit und Pflicht", "tokens": ["Und", "Tu\u00b7gend", ",", "Ehr\u00b7bar\u00b7keit", "und", "Pflicht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Viel h\u00f6her, als das Leben, sch\u00e4tzet.", "tokens": ["Viel", "h\u00f6\u00b7her", ",", "als", "das", "Le\u00b7ben", ",", "sch\u00e4t\u00b7zet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Umsonst ist seine Redekunst,", "tokens": ["Um\u00b7sonst", "ist", "sei\u00b7ne", "Re\u00b7de\u00b7kunst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Umsonst sein Flehen und Versprechen:", "tokens": ["Um\u00b7sonst", "sein", "Fle\u00b7hen", "und", "Ver\u00b7spre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Nichts, nichts erwirbt ihm ihre Gunst,", "tokens": ["Nichts", ",", "nichts", "er\u00b7wirbt", "ihm", "ih\u00b7re", "Gunst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PIS", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Nichts kann den frommen Vorsatz brechen.", "tokens": ["Nichts", "kann", "den", "from\u00b7men", "Vor\u00b7satz", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "So z\u00fcchtig sind zu aller Zeit,", "tokens": ["So", "z\u00fcch\u00b7tig", "sind", "zu", "al\u00b7ler", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "So unerbittlich viele Sch\u00f6nen,", "tokens": ["So", "un\u00b7er\u00b7bitt\u00b7lich", "vie\u00b7le", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Die doch den Wahn der Grau amkeit", "tokens": ["Die", "doch", "den", "Wahn", "der", "Grau", "am\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "In eines dritten Arm verh\u00f6hnen.", "tokens": ["In", "ei\u00b7nes", "drit\u00b7ten", "Arm", "ver\u00b7h\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch Gismund wird auf einmal k\u00fchn,", "tokens": ["Doch", "Gis\u00b7mund", "wird", "auf", "ein\u00b7mal", "k\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "APPR", "ADV", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Als man ihm heimlich kund gemachet,", "tokens": ["Als", "man", "ihm", "heim\u00b7lich", "kund", "ge\u00b7ma\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADJD", "PTKVZ", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie diese Lippen, die ihn fliehn,", "tokens": ["Wie", "die\u00b7se", "Lip\u00b7pen", ",", "die", "ihn", "fliehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sehr oft den Guido angelachet.", "tokens": ["Sehr", "oft", "den", "Gui\u00b7do", "an\u00b7ge\u00b7la\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NE", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nachdem ihm auch die Kammermagd,", "tokens": ["Nach\u00b7dem", "ihm", "auch", "die", "Kam\u00b7mer\u00b7magd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die man, errathet wie? gewonnen,", "tokens": ["Die", "man", ",", "er\u00b7ra\u00b7thet", "wie", "?", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PIS", "$,", "VVFIN", "KOKOM", "$.", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Getreuen Beistand zugesagt,", "tokens": ["Ge\u00b7treu\u00b7en", "Bei\u00b7stand", "zu\u00b7ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wird bald ein Mittel ausgesonnen.", "tokens": ["Wird", "bald", "ein", "Mit\u00b7tel", "aus\u00b7ge\u00b7son\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Er eilt Laurettens Zimmer zu,", "tokens": ["Er", "eilt", "Lau\u00b7ret\u00b7tens", "Zim\u00b7mer", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die auf des Lieblings Schoo\u00dfe lauschet,", "tokens": ["Die", "auf", "des", "Lieb\u00b7lings", "Schoo\u00b7\u00dfe", "lau\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und jetzt mit ihm, in sichrer Ruh',", "tokens": ["Und", "jetzt", "mit", "ihm", ",", "in", "sich\u00b7rer", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Die allerbesten K\u00fcsse tauschet.", "tokens": ["Die", "al\u00b7ler\u00b7bes\u00b7ten", "K\u00fcs\u00b7se", "tau\u00b7schet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Sie h\u00f6rt ihn kommen. Sie erschrickt,", "tokens": ["Sie", "h\u00f6rt", "ihn", "kom\u00b7men", ".", "Sie", "er\u00b7schrickt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$.", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und hatte Recht, sich zu erschrecken.", "tokens": ["Und", "hat\u00b7te", "Recht", ",", "sich", "zu", "er\u00b7schre\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "$,", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Ihr Guido mu\u00df, so gut sich's schickt,", "tokens": ["Ihr", "Gui\u00b7do", "mu\u00df", ",", "so", "gut", "sich's", "schickt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "VMFIN", "$,", "ADV", "ADJD", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Sich eiligst hinter's Bett verstecken.", "tokens": ["Sich", "ei\u00b7ligst", "hin\u00b7ter's", "Bett", "ver\u00b7ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Sie bebt, und glaubt, es sei der Mann;", "tokens": ["Sie", "bebt", ",", "und", "glaubt", ",", "es", "sei", "der", "Mann", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Doch als sie Gismund kaum erkannte,", "tokens": ["Doch", "als", "sie", "Gis\u00b7mund", "kaum", "er\u00b7kann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Fing der schon eine Predigt an,", "tokens": ["Fing", "der", "schon", "ei\u00b7ne", "Pre\u00b7digt", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Darin er sie nicht heilig nannte.", "tokens": ["Da\u00b7rin", "er", "sie", "nicht", "hei\u00b7lig", "nann\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PPER", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Er schw\u00f6rt, den strafbaren Betrug", "tokens": ["Er", "schw\u00f6rt", ",", "den", "straf\u00b7ba\u00b7ren", "Be\u00b7trug"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor niemand l\u00e4nger zu verschweigen,", "tokens": ["Vor", "nie\u00b7mand", "l\u00e4n\u00b7ger", "zu", "ver\u00b7schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sucht sie, ohn' einigen Verzug,", "tokens": ["Sucht", "sie", ",", "ohn'", "ei\u00b7ni\u00b7gen", "Ver\u00b7zug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Sich nicht geneigter zu erzeigen.", "tokens": ["Sich", "nicht", "ge\u00b7neig\u00b7ter", "zu", "er\u00b7zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie klagt: er droht. Sie seufzt: er lacht.", "tokens": ["Sie", "klagt", ":", "er", "droht", ".", "Sie", "seufzt", ":", "er", "lacht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie fleht um Aufschub; doch vergebens.", "tokens": ["Sie", "fleht", "um", "Auf\u00b7schub", ";", "doch", "ver\u00b7ge\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$.", "ADV", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Er will; sie endlich auch. Dies macht", "tokens": ["Er", "will", ";", "sie", "end\u00b7lich", "auch", ".", "Dies", "macht"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "$.", "PPER", "ADV", "ADV", "$.", "PDS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Endschaft alles Widerstrebens.", "tokens": ["Die", "End\u00b7schaft", "al\u00b7les", "Wi\u00b7der\u00b7stre\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Man sagt sich Lieb' und Eintracht zu,", "tokens": ["Man", "sagt", "sich", "Lieb'", "und", "Ein\u00b7tracht", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und gibt und nimmt von beiden Zeichen.", "tokens": ["Und", "gibt", "und", "nimmt", "von", "bei\u00b7den", "Zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Ach Guido! was gedachtest du?", "tokens": ["Ach", "Gui\u00b7do", "!", "was", "ge\u00b7dach\u00b7test", "du", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "PWS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Was konnte deinem Unmuth gleichen?", "tokens": ["Was", "konn\u00b7te", "dei\u00b7nem", "Un\u00b7muth", "glei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Allein, nun setzt es erst Gefahr:", "tokens": ["Al\u00b7lein", ",", "nun", "setzt", "es", "erst", "Ge\u00b7fahr", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun gibt's die schlimmsten Augenblicke.", "tokens": ["Nun", "gibt's", "die", "schlimms\u00b7ten", "Au\u00b7gen\u00b7bli\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Mann, der hier nicht n\u00f6thig war,", "tokens": ["Der", "Mann", ",", "der", "hier", "nicht", "n\u00f6\u00b7thig", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PTKNEG", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "K\u00f6mmt, eh' man es gedacht, zur\u00fccke.", "tokens": ["K\u00f6mmt", ",", "eh'", "man", "es", "ge\u00b7dacht", ",", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PIS", "PPER", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie w\u00e4re, sonder Weiberlist,", "tokens": ["Wie", "w\u00e4\u00b7re", ",", "son\u00b7der", "Wei\u00b7ber\u00b7list", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dies jemals gl\u00fccklich abgegangen?", "tokens": ["Dies", "je\u00b7mals", "gl\u00fcck\u00b7lich", "ab\u00b7ge\u00b7gan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Jedoch, wo die besch\u00e4ftigt ist,", "tokens": ["Je\u00b7doch", ",", "wo", "die", "be\u00b7sch\u00e4f\u00b7tigt", "ist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da sieht man leicht, was anzufangen.", "tokens": ["Da", "sieht", "man", "leicht", ",", "was", "an\u00b7zu\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "pun