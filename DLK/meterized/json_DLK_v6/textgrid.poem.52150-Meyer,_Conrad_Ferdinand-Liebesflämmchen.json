{"textgrid.poem.52150": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Liebesfl\u00e4mmchen", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Mutter mahnt mich abends:", "tokens": ["Die", "Mut\u00b7ter", "mahnt", "mich", "a\u00b7bends", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbtrag Sorg zur Ampel, Kind!", "tokens": ["\u00bb", "trag", "Sorg", "zur", "Am\u00b7pel", ",", "Kind", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "VVFIN", "NN", "APPRART", "NN", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "J\u00fcngst tr\u00e4umte mir von Feuer \u2013", "tokens": ["J\u00fcngst", "tr\u00e4um\u00b7te", "mir", "von", "Feu\u00b7er", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auch weht ein wilder Wind.\u00ab", "tokens": ["Auch", "weht", "ein", "wil\u00b7der", "Wind", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das Fl\u00e4mmchen auf der Ampel,", "tokens": ["Das", "Fl\u00e4mm\u00b7chen", "auf", "der", "Am\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich l\u00f6sch es mit Bedacht,", "tokens": ["Ich", "l\u00f6sch", "es", "mit", "Be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Licht in meinem Herzen", "tokens": ["Das", "Licht", "in", "mei\u00b7nem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Brennt durch die ganze Nacht.", "tokens": ["Brennt", "durch", "die", "gan\u00b7ze", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Mutter ruft mich morgens:", "tokens": ["Die", "Mut\u00b7ter", "ruft", "mich", "mor\u00b7gens", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbkind, hebe dich! 's ist Tag!\u00ab", "tokens": ["\u00bb", "kind", ",", "he\u00b7be", "dich", "!", "'s", "ist", "Tag", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NN", "$,", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie pocht an meiner T\u00fcre", "tokens": ["Sie", "pocht", "an", "mei\u00b7ner", "T\u00fc\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dreimal mit starkem Schlag", "tokens": ["Drei\u00b7mal", "mit", "star\u00b7kem", "Schlag"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Und meint, sie habe grausam", "tokens": ["Und", "meint", ",", "sie", "ha\u00b7be", "grau\u00b7sam"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PPER", "VAFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mich aus dem Schlaf geschreckt \u2013", "tokens": ["Mich", "aus", "dem", "Schlaf", "ge\u00b7schreckt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Licht in meinem Herzen", "tokens": ["Das", "Licht", "in", "mei\u00b7nem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hat l\u00e4ngst mich aufgeweckt.", "tokens": ["Hat", "l\u00e4ngst", "mich", "auf\u00b7ge\u00b7weckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}