{"textgrid.poem.65144": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da\u00df der Schlaf dich weich umschmiege", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df der Schlaf dich weich umschmiege", "tokens": ["Da\u00df", "der", "Schlaf", "dich", "weich", "um\u00b7schmie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lulle ich dich ein;", "tokens": ["Lul\u00b7le", "ich", "dich", "ein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PRF", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Silberhell auf deine Wiege", "tokens": ["Sil\u00b7ber\u00b7hell", "auf", "dei\u00b7ne", "Wie\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00e4llt des Mondes Schein.", "tokens": ["F\u00e4llt", "des", "Mon\u00b7des", "Schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Mit Gesang, mit M\u00e4rchensagen", "tokens": ["Mit", "Ge\u00b7sang", ",", "mit", "M\u00e4r\u00b7chen\u00b7sa\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bring' ich dich zur Ruh'", "tokens": ["Bring'", "ich", "dich", "zur", "Ruh'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "PRF", "APPRART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Und mit lieblichem Behagen", "tokens": ["Und", "mit", "lieb\u00b7li\u00b7chem", "Be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schlie\u00dft dein Aug' sich zu.", "tokens": ["Schlie\u00dft", "dein", "Aug'", "sich", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PRF", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Wo der Terek \u00fcber Steine", "tokens": ["Wo", "der", "Te\u00b7rek", "\u00fc\u00b7ber", "Stei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "J\u00e4hen Laufes braust,", "tokens": ["J\u00e4\u00b7hen", "Lau\u00b7fes", "braust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Lauert der Tscherkesse seine", "tokens": ["Lau\u00b7ert", "der", "Tscher\u00b7kes\u00b7se", "sei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PPOSAT"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.12": {"text": "Schaschka in der Faust.", "tokens": ["Schaschka", "in", "der", "Faust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "Doch wir wollen ihn nicht scheuen,", "tokens": ["Doch", "wir", "wol\u00b7len", "ihn", "nicht", "scheu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Denn dein Vater wacht,", "tokens": ["Denn", "dein", "Va\u00b7ter", "wacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.15": {"text": "Der aus jedem Kampf sich neuen", "tokens": ["Der", "aus", "je\u00b7dem", "Kampf", "sich", "neu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PIAT", "NN", "PRF", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Siegesruhm gebracht,", "tokens": ["Sie\u00b7ges\u00b7ruhm", "ge\u00b7bracht", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.17": {"text": "Und auch du wirst einstens ringen", "tokens": ["Und", "auch", "du", "wirst", "eins\u00b7tens", "rin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPER", "VAFIN", "ADV", "VVFIN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.18": {"text": "Auf der gleichen Bahn,", "tokens": ["Auf", "der", "glei\u00b7chen", "Bahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.19": {"text": "Freudig auf dein Ro\u00df dich schwingen", "tokens": ["Freu\u00b7dig", "auf", "dein", "Ro\u00df", "dich", "schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Waffenangethan.", "tokens": ["Waf\u00b7fe\u00b7nan\u00b7ge\u00b7than", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.21": {"text": "Sticken will mit F\u00e4den Goldes", "tokens": ["Sti\u00b7cken", "will", "mit", "F\u00e4\u00b7den", "Gol\u00b7des"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "APPR", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Ich des Sattels Saum \u2013", "tokens": ["Ich", "des", "Sat\u00b7tels", "Saum", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.23": {"text": "Schlummre du mein Kind, mein holdes!", "tokens": ["Schlumm\u00b7re", "du", "mein", "Kind", ",", "mein", "hol\u00b7des", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Tr\u00e4ume s\u00fc\u00dfen Traum.", "tokens": ["Tr\u00e4u\u00b7me", "s\u00fc\u00b7\u00dfen", "Traum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.25": {"text": "Echt und treu wird sich bew\u00e4hren", "tokens": ["Echt", "und", "treu", "wird", "sich", "be\u00b7w\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "VAFIN", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Dein kosakisch Herz,", "tokens": ["Dein", "ko\u00b7sa\u00b7kisch", "Herz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.27": {"text": "Nach Gefahr und Sieg begehren", "tokens": ["Nach", "Ge\u00b7fahr", "und", "Sieg", "be\u00b7geh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Trotz der Mutter Schmerz!", "tokens": ["Trotz", "der", "Mut\u00b7ter", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.29": {"text": "Scheidend winkt mir deine Rechte", "tokens": ["Schei\u00b7dend", "winkt", "mir", "dei\u00b7ne", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Und es ist gethan!", "tokens": ["Und", "es", "ist", "ge\u00b7than", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.31": {"text": "Welche Tage, welche N\u00e4chte", "tokens": ["Wel\u00b7che", "Ta\u00b7ge", ",", "wel\u00b7che", "N\u00e4ch\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PWAT", "NN", "$,", "PWAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Harren meiner dann!", "tokens": ["Har\u00b7ren", "mei\u00b7ner", "dann", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.33": {"text": "Bis du wieder einst geborgen", "tokens": ["Bis", "du", "wie\u00b7der", "einst", "ge\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Heimkehrst aus der Schlacht,", "tokens": ["Heim\u00b7kehrst", "aus", "der", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.35": {"text": "Werd' ich Tags um dich mich sorgen,", "tokens": ["Werd'", "ich", "Tags", "um", "dich", "mich", "sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Gr\u00e4men mich bei Nacht!", "tokens": ["Gr\u00e4\u00b7men", "mich", "bei", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.37": {"text": "Schlummre sanft, da du zur Stunde", "tokens": ["Schlumm\u00b7re", "sanft", ",", "da", "du", "zur", "Stun\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "$,", "KOUS", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Noch nicht ahnst und wei\u00dft,", "tokens": ["Noch", "nicht", "ahnst", "und", "wei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "KON", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.39": {"text": "Da\u00df nur Kampf und Wund' um Wunde,", "tokens": ["Da\u00df", "nur", "Kampf", "und", "Wund'", "um", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Was man Leben hei\u00dft.", "tokens": ["Was", "man", "Le\u00b7ben", "hei\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.41": {"text": "Schenken will ich, dich zu wahren", "tokens": ["Schen\u00b7ken", "will", "ich", ",", "dich", "zu", "wah\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "$,", "PPER", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Dir dies Heil'genbild;", "tokens": ["Dir", "dies", "Heil'\u00b7gen\u00b7bild", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PDS", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.43": {"text": "In Bedr\u00e4ngnis und Gefahren", "tokens": ["In", "Be\u00b7dr\u00e4ng\u00b7nis", "und", "Ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Dien' es dir als Schild!", "tokens": ["Dien'", "es", "dir", "als", "Schild", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "KOUS", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.45": {"text": "Gottes bist du! seinen Wegen", "tokens": ["Got\u00b7tes", "bist", "du", "!", "sei\u00b7nen", "We\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Folg in Nacht und Licht!", "tokens": ["Folg", "in", "Nacht", "und", "Licht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.47": {"text": "Halt an deiner Mutter Segen, \u2013", "tokens": ["Halt", "an", "dei\u00b7ner", "Mut\u00b7ter", "Se\u00b7gen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "APPR", "PPOSAT", "NN", "NN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Er verl\u00e4\u00dft dich nicht!", "tokens": ["Er", "ver\u00b7l\u00e4\u00dft", "dich", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.49": {"text": "Ihn ruf an, wenn schon im Schwunge", "tokens": ["Ihn", "ruf", "an", ",", "wenn", "schon", "im", "Schwun\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NN", "PTKVZ", "$,", "KOUS", "ADV", "APPRART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.50": {"text": "Dir der Mordstrahl dr\u00e4ut, \u2013", "tokens": ["Dir", "der", "Mord\u00b7strahl", "dr\u00e4ut", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "$,", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.51": {"text": "Schlumm're, du mein s\u00fc\u00dfer Junge!", "tokens": ["Schlum\u00b7m'\u00b7re", ",", "du", "mein", "s\u00fc\u00b7\u00dfer", "Jun\u00b7ge", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "Noch ist's nicht so weit!", "tokens": ["Noch", "ist's", "nicht", "so", "weit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}