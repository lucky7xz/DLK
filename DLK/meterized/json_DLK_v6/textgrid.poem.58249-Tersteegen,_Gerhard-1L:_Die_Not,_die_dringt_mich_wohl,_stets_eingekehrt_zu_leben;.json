{"textgrid.poem.58249": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Not, die dringt mich wohl, stets eingekehrt zu leben;", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Not, die dringt mich wohl, stets eingekehrt zu leben;", "tokens": ["Die", "Not", ",", "die", "dringt", "mich", "wohl", ",", "stets", "ein\u00b7ge\u00b7kehrt", "zu", "le\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PPER", "ADV", "$,", "ADV", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich selbst bin arm und schwach, die Welt gibt auch nur Pein,", "tokens": ["Ich", "selbst", "bin", "arm", "und", "schwach", ",", "die", "Welt", "gibt", "auch", "nur", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADJD", "KON", "ADJD", "$,", "ART", "NN", "VVFIN", "ADV", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es steht mein ganzes Heil, im Geist an Gott zu kleben,", "tokens": ["Es", "steht", "mein", "gan\u00b7zes", "Heil", ",", "im", "Geist", "an", "Gott", "zu", "kle\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,", "APPRART", "NN", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So flie\u00dfet Gnad' um Gnad' aus ihm in mich hinein.", "tokens": ["So", "flie\u00b7\u00dfet", "Gnad'", "um", "Gnad'", "aus", "ihm", "in", "mich", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "NN", "APPR", "PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}