{"textgrid.poem.64916": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht k\u00f6nnen was man will, mag gelten als Bedr\u00e4ngni\u00df;", "tokens": ["Nicht", "k\u00f6n\u00b7nen", "was", "man", "will", ",", "mag", "gel\u00b7ten", "als", "Be\u00b7dr\u00e4ng\u00b7ni\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "PWS", "PIS", "VMFIN", "$,", "VMFIN", "VVINF", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch ist's bei weitem nicht das bitterste Verh\u00e4ngni\u00df;", "tokens": ["Doch", "ist's", "bei", "wei\u00b7tem", "nicht", "das", "bit\u00b7ters\u00b7te", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PIS", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn hoffen d\u00fcrfen wir, da\u00df kommen wird die Zeit,", "tokens": ["Denn", "hof\u00b7fen", "d\u00fcr\u00b7fen", "wir", ",", "da\u00df", "kom\u00b7men", "wird", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "$,", "KOUS", "VVINF", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die von den Hemmungen der Au\u00dfenwelt befreit.", "tokens": ["Die", "von", "den", "Hem\u00b7mun\u00b7gen", "der", "Au\u00b7\u00dfen\u00b7welt", "be\u00b7freit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}}, "stanza.3": {"line.1": {"text": "Die hohe, lichte Zeit, wo der Unm\u00f6glichkeiten", "tokens": ["Die", "ho\u00b7he", ",", "lich\u00b7te", "Zeit", ",", "wo", "der", "Un\u00b7m\u00f6g\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bedr\u00fcckend Eisenband zersprengt wird von uns gleiten.", "tokens": ["Be\u00b7dr\u00fc\u00b7ckend", "Ei\u00b7sen\u00b7band", "zer\u00b7sprengt", "wird", "von", "uns", "glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "VVPP", "VAFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nicht k\u00f6nnen, was man will, das geht noch immer an,", "tokens": ["Nicht", "k\u00f6n\u00b7nen", ",", "was", "man", "will", ",", "das", "geht", "noch", "im\u00b7mer", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "$,", "PRELS", "PIS", "VMFIN", "$,", "PDS", "VVFIN", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ungleich schlimmer ist: nicht wollen, was man kann.", "tokens": ["Und", "un\u00b7gleich", "schlim\u00b7mer", "ist", ":", "nicht", "wol\u00b7len", ",", "was", "man", "kann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "VAFIN", "$.", "PTKNEG", "VMFIN", "$,", "PRELS", "PIS", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Willst du nicht, was du kannst in irdischer Beschr\u00e4nkung,", "tokens": ["Willst", "du", "nicht", ",", "was", "du", "kannst", "in", "ir\u00b7di\u00b7scher", "Be\u00b7schr\u00e4n\u00b7kung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "$,", "PWS", "PPER", "VMFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So w\u00e4r' dir unn\u00fctz auch der h\u00f6chsten Freiheit Schenkung.", "tokens": ["So", "w\u00e4r'", "dir", "un\u00b7n\u00fctz", "auch", "der", "h\u00f6chs\u00b7ten", "Frei\u00b7heit", "Schen\u00b7kung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADV", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Denn w\u00e4re hundertmal dir unterthan die Welt \u2013", "tokens": ["Denn", "w\u00e4\u00b7re", "hun\u00b7dert\u00b7mal", "dir", "un\u00b7ter\u00b7than", "die", "Welt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PPER", "ADJD", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was frommt's, wenn dir der Will', sie zu beherrschen, fehlt?", "tokens": ["Was", "frommt's", ",", "wenn", "dir", "der", "Will'", ",", "sie", "zu", "be\u00b7herr\u00b7schen", ",", "fehlt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}