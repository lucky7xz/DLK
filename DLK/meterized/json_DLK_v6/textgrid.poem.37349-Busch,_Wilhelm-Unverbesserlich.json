{"textgrid.poem.37349": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Unverbesserlich", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer Bildung hat, der ist emp\u00f6rt,", "tokens": ["Wer", "Bil\u00b7dung", "hat", ",", "der", "ist", "em\u00b7p\u00f6rt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "$,", "PRELS", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn er so schrecklich fluchen h\u00f6rt.", "tokens": ["Wenn", "er", "so", "schreck\u00b7lich", "flu\u00b7chen", "h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dies \u00bbNasowolltich\u00ab, dies \u00bbParbl\u00f6\u00ab,", "tokens": ["Dies", "\u00bb", "Na\u00b7so\u00b7woll\u00b7tich", "\u00ab", ",", "dies", "\u00bb", "Parb\u00b7l\u00f6", "\u00ab", ","], "token_info": ["word", "punct", "word", "punct", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["PDS", "$(", "NE", "$(", "$,", "PDS", "$(", "NN", "$(", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dies ewige \u00bbOjemine\u00ab,", "tokens": ["Dies", "e\u00b7wi\u00b7ge", "\u00bb", "O\u00b7je\u00b7mi\u00b7ne", "\u00ab", ","], "token_info": ["word", "word", "punct", "word", "punct", "punct"], "pos": ["PDS", "ADJA", "$(", "NE", "$(", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Dies \u00bbEipotztausendnocheinmal\u00ab,", "tokens": ["Dies", "\u00bb", "Ei\u00b7potz\u00b7tau\u00b7send\u00b7no\u00b7ch\u00b7ein\u00b7mal", "\u00ab", ","], "token_info": ["word", "punct", "word", "punct", "punct"], "pos": ["PDS", "$(", "NN", "$(", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ist das nicht eine Ohrenqual?", "tokens": ["Ist", "das", "nicht", "ei\u00b7ne", "Oh\u00b7ren\u00b7qual", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und gar \u00bbDa\u00dfdichdasm\u00e4usleinbei\u00df\u00ab,", "tokens": ["Und", "gar", "\u00bb", "Da\u00df\u00b7dich\u00b7das\u00b7m\u00e4us\u00b7lein\u00b7bei\u00df", "\u00ab", ","], "token_info": ["word", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "ADV", "$(", "NN", "$(", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da wird mir's immer kalt und hei\u00df.", "tokens": ["Da", "wird", "mir's", "im\u00b7mer", "kalt", "und", "hei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wie oft wohl sag ich: Es ist h\u00e4\u00dflich,", "tokens": ["Wie", "oft", "wohl", "sag", "ich", ":", "Es", "ist", "h\u00e4\u00df\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ist unanst\u00e4ndig, roh und gr\u00e4\u00dflich.", "tokens": ["Ist", "un\u00b7an\u00b7st\u00e4n\u00b7dig", ",", "roh", "und", "gr\u00e4\u00df\u00b7lich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Ich bitt und flehe: La\u00dft es sein,", "tokens": ["Ich", "bitt", "und", "fle\u00b7he", ":", "La\u00dft", "es", "sein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$.", "VVIMP", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Denn es ist s\u00fcndlich. Aber nein,", "tokens": ["Denn", "es", "ist", "s\u00fcnd\u00b7lich", ".", "A\u00b7ber", "nein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "$.", "KON", "PTKANT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Vergebens ring ich meine H\u00e4nde,", "tokens": ["Ver\u00b7ge\u00b7bens", "ring", "ich", "mei\u00b7ne", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Die Flucherei nimmt doch kein Ende.", "tokens": ["Die", "Fluc\u00b7he\u00b7rei", "nimmt", "doch", "kein", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}