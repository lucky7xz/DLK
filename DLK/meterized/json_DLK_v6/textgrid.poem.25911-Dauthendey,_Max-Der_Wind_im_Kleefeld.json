{"textgrid.poem.25911": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Der Wind im Kleefeld", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Steht die Liebste an der Mauer,", "tokens": ["Steht", "die", "Liebs\u00b7te", "an", "der", "Mau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieht zum weichen Kleefeld hin,", "tokens": ["Sieht", "zum", "wei\u00b7chen", "Klee\u00b7feld", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieht den Wind im Acker streichen,", "tokens": ["Sieht", "den", "Wind", "im", "A\u00b7cker", "strei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Furchen durch die Gr\u00e4ser ziehn.", "tokens": ["Fur\u00b7chen", "durch", "die", "Gr\u00e4\u00b7ser", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Denkt: so schneiden die Gedanken", "tokens": ["Denkt", ":", "so", "schnei\u00b7den", "die", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00fcndlich sehnend in mein Blut;", "tokens": ["St\u00fcnd\u00b7lich", "seh\u00b7nend", "in", "mein", "Blut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kann nicht gr\u00fcndlich unterscheiden,", "tokens": ["Kann", "nicht", "gr\u00fcnd\u00b7lich", "un\u00b7ter\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ob es wohl, ob's wehe tut.", "tokens": ["Ob", "es", "wohl", ",", "ob's", "we\u00b7he", "tut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "KOUS", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}