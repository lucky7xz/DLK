{"textgrid.poem.46162": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Barbara Sophia, Herzogin von Wirtemberg, geborne aus dem churf. Haus Brandenburg", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gleichwie wan mit gleichlosem glanz", "tokens": ["Gleich\u00b7wie", "wan", "mit", "gleich\u00b7lo\u00b7sem", "glanz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "die g\u00f6ttin aus Delos gezieret", "tokens": ["die", "g\u00f6t\u00b7tin", "aus", "De\u00b7los", "ge\u00b7zie\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NE", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "der sternen wunderreichen danz", "tokens": ["der", "ster\u00b7nen", "wun\u00b7der\u00b7rei\u00b7chen", "danz"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "in aller g\u00f6tter saal auff\u00fchret,", "tokens": ["in", "al\u00b7ler", "g\u00f6t\u00b7ter", "saal", "auf\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "sie mit lieb und liebreichem pracht", "tokens": ["sie", "mit", "lieb", "und", "lieb\u00b7rei\u00b7chem", "pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ADJD", "KON", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "die finsternus dem tag gleich macht:", "tokens": ["die", "fins\u00b7ter\u00b7nus", "dem", "tag", "gleich", "macht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Also Nymf, aller Nymfen blum,", "tokens": ["Al\u00b7so", "Nymf", ",", "al\u00b7ler", "Nym\u00b7fen", "blum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "PIAT", "NN", "NE", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "o f\u00fcrstliche zier aller frauen,", "tokens": ["o", "f\u00fcrst\u00b7li\u00b7che", "zier", "al\u00b7ler", "frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "ADJA", "NN", "PIAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "also Nymf, aller sch\u00f6nheit ruhm", "tokens": ["al\u00b7so", "Nymf", ",", "al\u00b7ler", "sch\u00f6n\u00b7heit", "ruhm"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "NE", "$,", "PIAT", "NN", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "mu\u00df man mit wunder euch anschauen,", "tokens": ["mu\u00df", "man", "mit", "wun\u00b7der", "euch", "an\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "als deren sch\u00f6nheit s\u00fc\u00dfe macht", "tokens": ["als", "de\u00b7ren", "sch\u00f6n\u00b7heit", "s\u00fc\u00b7\u00dfe", "macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRELAT", "NN", "ADJA", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "des himmels und der erden pracht.", "tokens": ["des", "him\u00b7mels", "und", "der", "er\u00b7den", "pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Der doppelt leuchtende planet,", "tokens": ["Der", "dop\u00b7pelt", "leuch\u00b7ten\u00b7de", "pla\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "mit welchem eure stirn sich ehret", "tokens": ["mit", "wel\u00b7chem", "eu\u00b7re", "stirn", "sich", "eh\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "PPOSAT", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "mit unverh\u00fcllter mayestet,", "tokens": ["mit", "un\u00b7ver\u00b7h\u00fcll\u00b7ter", "ma\u00b7ye\u00b7stet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "zugleich die lieb und ehr uns lehret;", "tokens": ["zu\u00b7gleich", "die", "lieb", "und", "ehr", "uns", "leh\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "ja eurer augen zierd und macht", "tokens": ["ja", "eu\u00b7rer", "au\u00b7gen", "zierd", "und", "macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "ist des tags zier und der nacht pracht.", "tokens": ["ist", "des", "tags", "zier", "und", "der", "nacht", "pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKVZ", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der mutterlosen g\u00f6ttin witz", "tokens": ["Der", "mut\u00b7ter\u00b7lo\u00b7sen", "g\u00f6t\u00b7tin", "witz"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "mu\u00df eurer weisheit weit nachgehen;", "tokens": ["mu\u00df", "eu\u00b7rer", "weis\u00b7heit", "weit", "nach\u00b7ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "die, so in Paphos ihren sitz,", "tokens": ["die", ",", "so", "in", "Pa\u00b7phos", "ih\u00b7ren", "sitz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "APPR", "NE", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "kan nicht f\u00fcr euch an zier bestehen;", "tokens": ["kan", "nicht", "f\u00fcr", "euch", "an", "zier", "be\u00b7ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "APPR", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "dan eurer zier und weisheit macht", "tokens": ["dan", "eu\u00b7rer", "zier", "und", "weis\u00b7heit", "macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "des himmels und der erden pracht.", "tokens": ["des", "him\u00b7mels", "und", "der", "er\u00b7den", "pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Die himmel haben ihre kunst,", "tokens": ["Die", "him\u00b7mel", "ha\u00b7ben", "ih\u00b7re", "kunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "so sie, gerecht, so lang gesparet,", "tokens": ["so", "sie", ",", "ge\u00b7recht", ",", "so", "lang", "ge\u00b7spa\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "ADJD", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "mit vil mehr reich, dan spater gunst", "tokens": ["mit", "vil", "mehr", "reich", ",", "dan", "spa\u00b7ter", "gunst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADV", "ADV", "ADJD", "$,", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "durch euch bei uns geoffenbaret,", "tokens": ["durch", "euch", "bei", "uns", "ge\u00b7of\u00b7fen\u00b7ba\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "da\u00df also eurer tugend macht", "tokens": ["da\u00df", "al\u00b7so", "eu\u00b7rer", "tu\u00b7gend", "macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "der g\u00f6tter und der menschen pracht.", "tokens": ["der", "g\u00f6t\u00b7ter", "und", "der", "men\u00b7schen", "pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Ja, die natur selbs, ihre macht", "tokens": ["Ja", ",", "die", "na\u00b7tur", "selbs", ",", "ih\u00b7re", "macht"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "ART", "NN", "ADV", "$,", "PPOSAT", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "f\u00fcr der welt augen zu beweisen,", "tokens": ["f\u00fcr", "der", "welt", "au\u00b7gen", "zu", "be\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "hat als ihr kunstst\u00fcck euch erdacht,", "tokens": ["hat", "als", "ihr", "kunst\u00b7st\u00fcck", "euch", "er\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "damit man sie durch euch solt preisen,", "tokens": ["da\u00b7mit", "man", "sie", "durch", "euch", "solt", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "APPR", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "und eurer augen s\u00fc\u00dfe macht", "tokens": ["und", "eu\u00b7rer", "au\u00b7gen", "s\u00fc\u00b7\u00dfe", "macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "ist diser erden wahre pracht.", "tokens": ["ist", "di\u00b7ser", "er\u00b7den", "wah\u00b7re", "pracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Darum dan unser grosse prinz", "tokens": ["Da\u00b7rum", "dan", "un\u00b7ser", "gros\u00b7se", "prinz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "hat euch sein herz gern \u00fcbergeben,", "tokens": ["hat", "euch", "sein", "herz", "gern", "\u00fc\u00b7ber\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "auf da\u00df mit ihm die ganz provinz", "tokens": ["auf", "da\u00df", "mit", "ihm", "die", "ganz", "pro\u00b7vinz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "APPR", "PPER", "ART", "ADV", "NE"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "m\u00f6cht, durch euch selig, fr\u00f6lich leben;", "tokens": ["m\u00f6cht", ",", "durch", "euch", "se\u00b7lig", ",", "fr\u00f6\u00b7lich", "le\u00b7ben", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "$,", "APPR", "PPER", "ADJD", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "und auf da\u00df eurer tugend macht", "tokens": ["und", "auf", "da\u00df", "eu\u00b7rer", "tu\u00b7gend", "macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "KOUS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "bleib dises lands und der welt pracht.", "tokens": ["bleib", "di\u00b7ses", "lands", "und", "der", "welt", "pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "ADJA", "KON", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.8": {"line.1": {"text": "O da\u00df euch beed der g\u00f6tter hand", "tokens": ["O", "da\u00df", "euch", "beed", "der", "g\u00f6t\u00b7ter", "hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "nicht bald f\u00fchr zu des himmels freuden,", "tokens": ["nicht", "bald", "f\u00fchr", "zu", "des", "him\u00b7mels", "freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "da\u00df sie nicht m\u00f6g ihn von dem land,", "tokens": ["da\u00df", "sie", "nicht", "m\u00f6g", "ihn", "von", "dem", "land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VMFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "von ihm euch, uns von euch abscheiden!", "tokens": ["von", "ihm", "euch", ",", "uns", "von", "euch", "ab\u00b7schei\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPER", "$,", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "o da\u00df auch eurer sch\u00f6nheit macht", "tokens": ["o", "da\u00df", "auch", "eu\u00b7rer", "sch\u00f6n\u00b7heit", "macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM", "KOUS", "ADV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "verbleib stets der welt wahrer pracht.", "tokens": ["ver\u00b7bleib", "stets", "der", "welt", "wah\u00b7rer", "pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}