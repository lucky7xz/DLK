{"dta.poem.16": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Gedanken  \n \u00fcber  \n  ein faules und bei der Nachtzeit schei-  \n nendes Holz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sah von Ohngefehr, da schon die\nDunkelheit,", "tokens": ["Ich", "sah", "von", "Ohn\u00b7ge\u00b7fehr", ",", "da", "schon", "die", "Dun\u00b7kel\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADV", "$,", "KOUS", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf unsrer Erden-Fl\u00e4ch die Schat-", "tokens": ["Auf", "uns\u00b7rer", "Er\u00b7den\u00b7Fl\u00e4ch", "die", "Schat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jm Garten untern Busch, ein Ding das gl\u00fchte", "tokens": ["Jm", "Gar\u00b7ten", "un\u00b7tern", "Busch", ",", "ein", "Ding", "das", "gl\u00fch\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,", "ART", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich wu\u00dft nicht, was es war; weil leicht die Sin-", "tokens": ["Ich", "wu\u00dft", "nicht", ",", "was", "es", "war", ";", "weil", "leicht", "die", "Sin"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VAFIN", "$.", "KOUS", "ADJD", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenn man ins Dunkle sieht. Ich dachte nach dem", "tokens": ["Wenn", "man", "ins", "Dunk\u00b7le", "sieht", ".", "Ich", "dach\u00b7te", "nach", "dem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPRART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "M\u00fcst es ein brennend Klump von feurgen Kohlen", "tokens": ["M\u00fcst", "es", "ein", "bren\u00b7nend", "Klump", "von", "feur\u00b7gen", "Koh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJD", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ich lief von Furcht gejagt, dies ungewohnte Bren-", "tokens": ["Ich", "lief", "von", "Furcht", "ge\u00b7jagt", ",", "dies", "un\u00b7ge\u00b7wohn\u00b7te", "Bren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "VVPP", "$,", "PDS", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Jm Busche anzusehn, genauer zu erkennen.", "tokens": ["Jm", "Bu\u00b7sche", "an\u00b7zu\u00b7sehn", ",", "ge\u00b7nau\u00b7er", "zu", "er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVIZU", "$,", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich ging, ich kam, ich sah, da war kein Feur", "tokens": ["Ich", "ging", ",", "ich", "kam", ",", "ich", "sah", ",", "da", "war", "kein", "Feur"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "ADV", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Weil ich nur faules Holz stat feurger Kohlen fand", "tokens": ["Weil", "ich", "nur", "fau\u00b7les", "Holz", "stat", "feur\u00b7ger", "Koh\u00b7len", "fand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das zu der Nachtzeit scheint; als ich es recht er-", "tokens": ["Das", "zu", "der", "Nacht\u00b7zeit", "scheint", ";", "als", "ich", "es", "recht", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "$.", "KOUS", "PPER", "PPER", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Fand ich, da\u00df mich der Schein mit leeren Nichts", "tokens": ["Fand", "ich", ",", "da\u00df", "mich", "der", "Schein", "mit", "lee\u00b7ren", "Nichts"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "So geht es \u00fcberall, es wird manch Ding der Welt,", "tokens": ["So", "geht", "es", "\u00fc\u00b7be\u00b7rall", ",", "es", "wird", "manch", "Ding", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Jm Traum der Einbildung, uns herrlich vorgestellt,", "tokens": ["Jm", "Traum", "der", "Ein\u00b7bil\u00b7dung", ",", "uns", "herr\u00b7lich", "vor\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.15": {"text": "Wenn es mit falschen Schein, als wie mit Dunst", "tokens": ["Wenn", "es", "mit", "fal\u00b7schen", "Schein", ",", "als", "wie", "mit", "Dunst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "$,", "KOUS", "KOKOM", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Wenn mans vern\u00fcnftig sieht, ist es in Nichts zer-", "tokens": ["Wenn", "mans", "ver\u00b7n\u00fcnf\u00b7tig", "sieht", ",", "ist", "es", "in", "Nichts", "zer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADJD", "VVFIN", "$,", "VAFIN", "PPER", "APPR", "PIS", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Wie mancher Mensche ist, der sich in Schmuk und", "tokens": ["Wie", "man\u00b7cher", "Men\u00b7sche", "ist", ",", "der", "sich", "in", "Schmuk", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "VAFIN", "$,", "PRELS", "PRF", "APPR", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Der Welt vor Augen stellt, und sich recht herrlich", "tokens": ["Der", "Welt", "vor", "Au\u00b7gen", "stellt", ",", "und", "sich", "recht", "herr\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$,", "KON", "PRF", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Er gl\u00e4nzet wie die Sonn, und funkelt wie die", "tokens": ["Er", "gl\u00e4n\u00b7zet", "wie", "die", "Sonn", ",", "und", "fun\u00b7kelt", "wie", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "$,", "KON", "VVFIN", "KOKOM", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Doch nur, wenn unser Aug geblendet, durch die", "tokens": ["Doch", "nur", ",", "wenn", "un\u00b7ser", "Aug", "ge\u00b7blen\u00b7det", ",", "durch", "die"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "$,", "KOUS", "PPOSAT", "NN", "VVPP", "$,", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Sehn wir ihn in der N\u00e4h; ist er ein faules Holz,", "tokens": ["Sehn", "wir", "ihn", "in", "der", "N\u00e4h", ";", "ist", "er", "ein", "fau\u00b7les", "Holz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "ART", "NN", "$.", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.22": {"text": "Der Glanz den er abstrahlt, ist nur ein eitler Stolz,", "tokens": ["Der", "Glanz", "den", "er", "abs\u00b7trahlt", ",", "ist", "nur", "ein", "eit\u00b7ler", "Stolz", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "VVFIN", "$,", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Von aussen g\u00fcldner Schmuk von innen faule Kno-", "tokens": ["Von", "aus\u00b7sen", "g\u00fcld\u00b7ner", "Schmuk", "von", "in\u00b7nen", "fau\u00b7le", "Kno"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN", "APPR", "ADV", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die wie ein m\u00fcrbes Holz gar leicht zermalmt, zer-", "tokens": ["Die", "wie", "ein", "m\u00fcr\u00b7bes", "Holz", "gar", "leicht", "zer\u00b7malmt", ",", "zer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "KOKOM", "ART", "ADJA", "NN", "ADV", "ADJD", "VVPP", "$,", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Das faule Holz mahlt sch\u00f6n ein \u00fcbert\u00fcnchtes Grab,", "tokens": ["Das", "fau\u00b7le", "Holz", "mahlt", "sch\u00f6n", "ein", "\u00fc\u00b7ber\u00b7t\u00fcnch\u00b7tes", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und einen Heuchel-Christ, mit seinen Wandel ab.", "tokens": ["Und", "ei\u00b7nen", "Heu\u00b7chel\u00b7Christ", ",", "mit", "sei\u00b7nen", "Wan\u00b7del", "ab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ein Schein-Christ brennt gleichsam, von lauter heil-", "tokens": ["Ein", "Schein\u00b7Christ", "brennt", "gleich\u00b7sam", ",", "von", "lau\u00b7ter", "heil"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "APPR", "PIAT", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Die ohne Feur und Kraft, aus kalten Herzen", "tokens": ["Die", "oh\u00b7ne", "Feur", "und", "Kraft", ",", "aus", "kal\u00b7ten", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "KON", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Er gl\u00e4nzt von aussen sch\u00f6n, ist eifrig im Gebet,", "tokens": ["Er", "gl\u00e4nzt", "von", "aus\u00b7sen", "sch\u00f6n", ",", "ist", "eif\u00b7rig", "im", "Ge\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "ADJD", "$,", "VAFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Wenn er zu seinem ", "tokens": ["Wenn", "er", "zu", "sei\u00b7nem"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.31": {"text": "Der Andacht \u00e4usrer Schein, der strahlt aus sei-", "tokens": ["Der", "An\u00b7dacht", "\u00e4us\u00b7rer", "Schein", ",", "der", "strahlt", "aus", "sei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "PRELS", "VVFIN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Doch ohne inre Glut. Will er dem H\u00f6chsten dienen,", "tokens": ["Doch", "oh\u00b7ne", "in\u00b7re", "Glut", ".", "Will", "er", "dem", "H\u00f6chs\u00b7ten", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$.", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "So dienet er sich selbst, und legt auf dem Altar,", "tokens": ["So", "die\u00b7net", "er", "sich", "selbst", ",", "und", "legt", "auf", "dem", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Zu seinen eignen Ruhm, der Falschheit Opfer dar.", "tokens": ["Zu", "sei\u00b7nen", "eig\u00b7nen", "Ruhm", ",", "der", "Falschheit", "Op\u00b7fer", "dar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "Er ist in Worten hei\u00df, und friert in seinen Wer-", "tokens": ["Er", "ist", "in", "Wor\u00b7ten", "hei\u00df", ",", "und", "friert", "in", "sei\u00b7nen", "Wer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ADJD", "$,", "KON", "VVFIN", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Er hasset jederman; doch l\u00e4st er sichs nicht merken.", "tokens": ["Er", "has\u00b7set", "je\u00b7der\u00b7man", ";", "doch", "l\u00e4st", "er", "sichs", "nicht", "mer\u00b7ken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$.", "ADV", "VVFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Wenn man von Ferne ihn, in seiner Andacht sieht:", "tokens": ["Wenn", "man", "von", "Fer\u00b7ne", "ihn", ",", "in", "sei\u00b7ner", "An\u00b7dacht", "sieht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "PPER", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "So brennt er lichter Loh, als wie ein Feur, das", "tokens": ["So", "brennt", "er", "lich\u00b7ter", "Loh", ",", "als", "wie", "ein", "Feur", ",", "das"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "NN", "$,", "KOUS", "KOKOM", "ART", "NN", "$,", "PRELS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "So ist er Mose gleich, des Angesicht gestrahlet,", "tokens": ["So", "ist", "er", "Mo\u00b7se", "gleich", ",", "des", "An\u00b7ge\u00b7sicht", "ge\u00b7strah\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NE", "ADV", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Doch nur auf solche Art, wie ihn der Mahler", "tokens": ["Doch", "nur", "auf", "sol\u00b7che", "Art", ",", "wie", "ihn", "der", "Mah\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PIAT", "NN", "$,", "PWAV", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Der Heiland sagt mit Recht: wer gute Fr\u00fcchte hegt,", "tokens": ["Der", "Hei\u00b7land", "sagt", "mit", "Recht", ":", "wer", "gu\u00b7te", "Fr\u00fcch\u00b7te", "hegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$.", "PWS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Der ist ein guter Baum; wer aber keine tr\u00e4gt", "tokens": ["Der", "ist", "ein", "gu\u00b7ter", "Baum", ";", "wer", "a\u00b7ber", "kei\u00b7ne", "tr\u00e4gt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$.", "PWS", "ADV", "PIAT", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Ist einem faulen gleich. Wer einen Heuchler nen-", "tokens": ["Ist", "ei\u00b7nem", "fau\u00b7len", "gleich", ".", "Wer", "ei\u00b7nen", "Heuch\u00b7ler", "nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ADV", "$.", "PWS", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.44": {"text": "Der nennt ein Holz das scheint, und in der That", "tokens": ["Der", "nennt", "ein", "Holz", "das", "scheint", ",", "und", "in", "der", "That"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "PDS", "VVFIN", "$,", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}