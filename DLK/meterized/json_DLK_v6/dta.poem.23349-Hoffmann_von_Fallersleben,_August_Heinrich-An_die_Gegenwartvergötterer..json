{"dta.poem.23349": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An die Gegenwartverg\u00f6tterer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach, wir sind zu sehr befangen", "tokens": ["Ach", ",", "wir", "sind", "zu", "sehr", "be\u00b7fan\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "PTKA", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der eignen Schlechtigkeit,", "tokens": ["In", "der", "eig\u00b7nen", "Schlech\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df wir immer noch verlangen", "tokens": ["Da\u00df", "wir", "im\u00b7mer", "noch", "ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Immer nach der ", "tokens": ["Im\u00b7mer", "nach", "der"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "ART"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Doch wir wollen uns bestreben,", "tokens": ["Doch", "wir", "wol\u00b7len", "uns", "be\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wollen thun wie ihr es thut,", "tokens": ["Wol\u00b7len", "thun", "wie", "ihr", "es", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KOKOM", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und so ganz dem Guten leben", "tokens": ["Und", "so", "ganz", "dem", "Gu\u00b7ten", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohne allen Zweifelsmuth.", "tokens": ["Oh\u00b7ne", "al\u00b7len", "Zwei\u00b7fels\u00b7muth", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wir wollen nicht mehr streiten,", "tokens": ["Und", "wir", "wol\u00b7len", "nicht", "mehr", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wollen sehn wie ihr es seht:", "tokens": ["Wol\u00b7len", "sehn", "wie", "ihr", "es", "seht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KOKOM", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O wie gut sind unsre Zeiten,", "tokens": ["O", "wie", "gut", "sind", "uns\u00b7re", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und wie gut doch Alles geht!", "tokens": ["Und", "wie", "gut", "doch", "Al\u00b7les", "geht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "ADV", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Gut ist Alles was bestehet,", "tokens": ["Gut", "ist", "Al\u00b7les", "was", "be\u00b7ste\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PIS", "PWS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und wie gut da\u00df ", "tokens": ["Und", "wie", "gut", "da\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADJD", "KOUS"], "meter": "-+--", "measure": "dactylic.init"}, "line.3": {"text": "Und f\u00fcr uns noch h\u00f6rt und sehet,", "tokens": ["Und", "f\u00fcr", "uns", "noch", "h\u00f6rt", "und", "se\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und f\u00fcr uns noch denkt und schreibt!", "tokens": ["Und", "f\u00fcr", "uns", "noch", "denkt", "und", "schreibt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}}}}