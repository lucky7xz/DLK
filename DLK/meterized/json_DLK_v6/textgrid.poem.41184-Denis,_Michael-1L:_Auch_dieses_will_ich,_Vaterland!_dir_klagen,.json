{"textgrid.poem.41184": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auch dieses will ich, Vaterland! dir klagen,", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auch dieses will ich, Vaterland! dir klagen,", "tokens": ["Auch", "die\u00b7ses", "will", "ich", ",", "Va\u00b7ter\u00b7land", "!", "dir", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PDS", "VMFIN", "PPER", "$,", "NN", "$.", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein ewigtreuer Sohn.", "tokens": ["Dein", "e\u00b7wig\u00b7treu\u00b7er", "Sohn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vielleicht da\u00df einstens kl\u00fcg're Zeiten sagen:", "tokens": ["Viel\u00b7leicht", "da\u00df", "eins\u00b7tens", "kl\u00fcg'\u00b7re", "Zei\u00b7ten", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die\u00df klagte Sined schon.", "tokens": ["Die\u00df", "klag\u00b7te", "Si\u00b7ned", "schon", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NE", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ebrauset, Saiten! der gerechte Kummer", "tokens": ["E\u00b7brau\u00b7set", ",", "Sai\u00b7ten", "!", "der", "ge\u00b7rech\u00b7te", "Kum\u00b7mer"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Barden ist emp\u00f6rt.", "tokens": ["Des", "Bar\u00b7den", "ist", "em\u00b7p\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich will euch kr\u00e4nzen, wenn ihr mir den Schlummer", "tokens": ["Ich", "will", "euch", "kr\u00e4n\u00b7zen", ",", "wenn", "ihr", "mir", "den", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,", "KOUS", "PPER", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Get\u00e4uschter Aeltern st\u00f6r't.", "tokens": ["Ge\u00b7t\u00e4uschter", "A\u00b7el\u00b7tern", "st\u00f6r'", "t."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["ADJA", "NN", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Oft geht von Teuts Geschlecht' ein frischer Knabe,", "tokens": ["Oft", "geht", "von", "Teuts", "Ge\u00b7schlecht'", "ein", "fri\u00b7scher", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NE", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sch\u00f6n wie der bunte Mai,", "tokens": ["Sch\u00f6n", "wie", "der", "bun\u00b7te", "Mai", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Hoch wie der Hirsch, voll jeder Heldengabe", "tokens": ["Hoch", "wie", "der", "Hirsch", ",", "voll", "je\u00b7der", "Hel\u00b7den\u00b7ga\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "ART", "NN", "$,", "ADJD", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein einsam Dach vorbei.", "tokens": ["Mein", "ein\u00b7sam", "Dach", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "PAV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da seufz' ich nach: O edles Blut! verloren", "tokens": ["Da", "seuf\u00b7z'", "ich", "nach", ":", "O", "ed\u00b7les", "Blut", "!", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$.", "NE", "ADJA", "NN", "$.", "VVPP"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das bist du f\u00fcr dein Land!", "tokens": ["Das", "bist", "du", "f\u00fcr", "dein", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Man gab dich, ach zu sp\u00e4t, zu sp\u00e4t geboren!", "tokens": ["Man", "gab", "dich", ",", "ach", "zu", "sp\u00e4t", ",", "zu", "sp\u00e4t", "ge\u00b7bo\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "$,", "ADV", "PTKA", "ADJD", "$,", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In eines Fremden Hand!", "tokens": ["In", "ei\u00b7nes", "Frem\u00b7den", "Hand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ein Fremder (nicht der Beste seines Landes,", "tokens": ["Ein", "Frem\u00b7der", "(", "nicht", "der", "Bes\u00b7te", "sei\u00b7nes", "Lan\u00b7des", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PTKNEG", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nein, dem's an Brod gebricht;", "tokens": ["Nein", ",", "dem's", "an", "Brod", "ge\u00b7bricht", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die\u00df mi\u00dft der Mann voll Tugend und Verstandes", "tokens": ["Die\u00df", "mi\u00dft", "der", "Mann", "voll", "Tu\u00b7gend", "und", "Ver\u00b7stan\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADJD", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In seiner Heimath nicht.)", "tokens": ["In", "sei\u00b7ner", "Hei\u00b7math", "nicht", ".", ")"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ein Fremder kam. Von diesem sollst du lernen,", "tokens": ["Ein", "Frem\u00b7der", "kam", ".", "Von", "die\u00b7sem", "sollst", "du", "ler\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "APPR", "PDAT", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was Deutsche bilden kann.", "tokens": ["Was", "Deut\u00b7sche", "bil\u00b7den", "kann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Liebt er sein eigen' Volk nicht? Bei den Sternen!", "tokens": ["Liebt", "er", "sein", "ei\u00b7gen'", "Volk", "nicht", "?", "Bei", "den", "Ster\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "PTKNEG", "$.", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er ist ein schlechter Mann;", "tokens": ["Er", "ist", "ein", "schlech\u00b7ter", "Mann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und liebet er sein Volk, was wird er preisen,", "tokens": ["Und", "lie\u00b7bet", "er", "sein", "Volk", ",", "was", "wird", "er", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "PWS", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als seines Volkes Macht,", "tokens": ["Als", "sei\u00b7nes", "Vol\u00b7kes", "Macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gesetze, Sitten, Werke seiner Weisen,", "tokens": ["Ge\u00b7set\u00b7ze", ",", "Sit\u00b7ten", ",", "Wer\u00b7ke", "sei\u00b7ner", "Wei\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und seiner F\u00fcrsten Pracht?", "tokens": ["Und", "sei\u00b7ner", "F\u00fcrs\u00b7ten", "Pracht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Du sitzest, horchest, glaubest seinem Prahlen.", "tokens": ["Du", "sit\u00b7zest", ",", "hor\u00b7chest", ",", "glau\u00b7best", "sei\u00b7nem", "Prah\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn ach dein Herz ist gut!", "tokens": ["Denn", "ach", "dein", "Herz", "ist", "gut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und seine Zunge schl\u00fcpfrig gleich den Aalen,", "tokens": ["Und", "sei\u00b7ne", "Zun\u00b7ge", "schl\u00fcpf\u00b7rig", "gleich", "den", "Aa\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O junges, edles Blut!", "tokens": ["O", "jun\u00b7ges", ",", "ed\u00b7les", "Blut", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Dein Land wird unvermerkt vor dir verd\u00fcstert.", "tokens": ["Dein", "Land", "wird", "un\u00b7ver\u00b7merkt", "vor", "dir", "ver\u00b7d\u00fcs\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dich leitet falscher Schein,", "tokens": ["Dich", "lei\u00b7tet", "fal\u00b7scher", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis gar der Wunsch in deiner Seele fl\u00fcstert:", "tokens": ["Bis", "gar", "der", "Wunsch", "in", "dei\u00b7ner", "See\u00b7le", "fl\u00fcs\u00b7tert", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich wollt' ein Fremder seyn!", "tokens": ["Ich", "wollt'", "ein", "Frem\u00b7der", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Auf! zeiget mir zu Gegenden die Wege,", "tokens": ["Auf", "!", "zei\u00b7get", "mir", "zu", "Ge\u00b7gen\u00b7den", "die", "We\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "PPER", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo Menschen bess'rer Art,", "tokens": ["Wo", "Men\u00b7schen", "bess'\u00b7rer", "Art", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo Sitten sind. Mein Volk ist kalt und tr\u00e4ge,", "tokens": ["Wo", "Sit\u00b7ten", "sind", ".", "Mein", "Volk", "ist", "kalt", "und", "tr\u00e4\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VAFIN", "$.", "PPOSAT", "NN", "VAFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Voll Einfalt, steif und hart.", "tokens": ["Voll", "Ein\u00b7falt", ",", "steif", "und", "hart", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Du reisest, kehrest nach verschwelgtem Gute,", "tokens": ["Du", "rei\u00b7sest", ",", "keh\u00b7rest", "nach", "ver\u00b7schwelg\u00b7tem", "Gu\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Frech, wenn der Himmel droht,", "tokens": ["Frech", ",", "wenn", "der", "Him\u00b7mel", "droht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "++-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit siechen Gliedern und verderbtem Blute,", "tokens": ["Mit", "sie\u00b7chen", "Glie\u00b7dern", "und", "ver\u00b7derb\u00b7tem", "Blu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem Vaterlande todt. \u2013", "tokens": ["Dem", "Va\u00b7ter\u00b7lan\u00b7de", "todt", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Ha, V\u00e4ter, oder sag' ich besser, M\u00fctter!", "tokens": ["Ha", ",", "V\u00e4\u00b7ter", ",", "o\u00b7der", "sag'", "ich", "bes\u00b7ser", ",", "M\u00fct\u00b7ter", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "KON", "VVFIN", "PPER", "ADJD", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist dieses eure Pflicht?", "tokens": ["Ist", "die\u00b7ses", "eu\u00b7re", "Pflicht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sind eure Kinder eure gr\u00f6\u00dften G\u00fcter?", "tokens": ["Sind", "eu\u00b7re", "Kin\u00b7der", "eu\u00b7re", "gr\u00f6\u00df\u00b7ten", "G\u00fc\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie? oder sind sie's nicht?", "tokens": ["Wie", "?", "o\u00b7der", "sind", "sie's", "nicht", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KON", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Geh\u00f6rt ihr Herz nicht ihrem Vaterlande?", "tokens": ["Ge\u00b7h\u00f6rt", "ihr", "Herz", "nicht", "ih\u00b7rem", "Va\u00b7ter\u00b7lan\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und scheint es euch erlaubt,", "tokens": ["Und", "scheint", "es", "euch", "er\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df es durch euch ein Fremder, o der Schande!", "tokens": ["Da\u00df", "es", "durch", "euch", "ein", "Frem\u00b7der", ",", "o", "der", "Schan\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ART", "NN", "$,", "FM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem Vaterlande raubt?", "tokens": ["Dem", "Va\u00b7ter\u00b7lan\u00b7de", "raubt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stan