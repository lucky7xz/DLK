], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Und wird sich der stolz einen Deutschen nennen,", "tokens": ["Und", "wird", "sich", "der", "stolz", "ei\u00b7nen", "Deut\u00b7schen", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PRF", "ART", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der nichts von Deutschland wei\u00df?", "tokens": ["Der", "nichts", "von", "Deutschland", "wei\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wird je sein Herz dem Vaterlande brennen? \u2013", "tokens": ["Wird", "je", "sein", "Herz", "dem", "Va\u00b7ter\u00b7lan\u00b7de", "bren\u00b7nen", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann brennet auch das Eis!", "tokens": ["Dann", "bren\u00b7net", "auch", "das", "Eis", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Er rang von seinem Volke sich zu rei\u00dfen.", "tokens": ["Er", "rang", "von", "sei\u00b7nem", "Vol\u00b7ke", "sich", "zu", "rei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kein Deutscher ist er mehr,", "tokens": ["Kein", "Deut\u00b7scher", "ist", "er", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "p