{"dta.poem.7343": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Dichters Naturgef\u00fchl.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Es war an einem jener Tage,", "tokens": ["Es", "war", "an", "ei\u00b7nem", "je\u00b7ner", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo Lenz und Winter sind im Streit,", "tokens": ["Wo", "Lenz", "und", "Win\u00b7ter", "sind", "im", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo na\u00df das Veilchen klebt am Haage,", "tokens": ["Wo", "na\u00df", "das", "Veil\u00b7chen", "klebt", "am", "Haa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kurz, um die erste Maienzeit;", "tokens": ["Kurz", ",", "um", "die", "ers\u00b7te", "Mai\u00b7en\u00b7zeit", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUI", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich suchte keuchend mir den Weg", "tokens": ["Ich", "such\u00b7te", "keu\u00b7chend", "mir", "den", "Weg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durch sumpfge Wiesen, d\u00fcrre Raine,", "tokens": ["Durch", "sumpf\u00b7ge", "Wie\u00b7sen", ",", "d\u00fcr\u00b7re", "Rai\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wo matt die Kr\u00f6te hockt' am Steine,", "tokens": ["Wo", "matt", "die", "Kr\u00f6\u00b7te", "hockt'", "am", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Eidechs schl\u00fcpfte \u00fcber'n Steg.", "tokens": ["Die", "Ei\u00b7dechs", "schl\u00fcpf\u00b7te", "\u00fc\u00b7ber'n", "Steg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Durch hundert kleine Wassertruhen,", "tokens": ["Durch", "hun\u00b7dert", "klei\u00b7ne", "Was\u00b7ser\u00b7tru\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die wie verk\u00fchlter Sp\u00fcligt stehn,", "tokens": ["Die", "wie", "ver\u00b7k\u00fchl\u00b7ter", "Sp\u00fc\u00b7ligt", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu stelzen mit den Gummischuhen,", "tokens": ["Zu", "stel\u00b7zen", "mit", "den", "Gum\u00b7mi\u00b7schu\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bei Gott, hei\u00dft das Spazierengehn?", "tokens": ["Bei", "Gott", ",", "hei\u00dft", "das", "Spa\u00b7zie\u00b7ren\u00b7gehn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Natur, wer auf dem Haberrohr", "tokens": ["Na\u00b7tur", ",", "wer", "auf", "dem", "Ha\u00b7berr\u00b7ohr"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Jamben, Stanzen, s\u00fc\u00dfen Phrasen", "tokens": ["In", "Jam\u00b7ben", ",", "Stan\u00b7zen", ",", "s\u00fc\u00b7\u00dfen", "Phra\u00b7sen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So manches Loblied dir geblasen,", "tokens": ["So", "man\u00b7ches", "Lob\u00b7lied", "dir", "ge\u00b7bla\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.8": {"text": "Dem stell dich auch manierlich vor!", "tokens": ["Dem", "stell", "dich", "auch", "ma\u00b7nier\u00b7lich", "vor", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da lie\u00df zur\u00fcck den Schleier wehen", "tokens": ["Da", "lie\u00df", "zu\u00b7r\u00fcck", "den", "Schlei\u00b7er", "we\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die eitle vielbesungne Frau,", "tokens": ["Die", "eit\u00b7le", "viel\u00b7be\u00b7sung\u00b7ne", "Frau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als f\u00fcrchte sie des Dichters Schm\u00e4hen;", "tokens": ["Als", "f\u00fcrch\u00b7te", "sie", "des", "Dich\u00b7ters", "Schm\u00e4\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Sonnenlichte stand die Au,", "tokens": ["Im", "Son\u00b7nen\u00b7lich\u00b7te", "stand", "die", "Au", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und bei dem ersten linden Stral", "tokens": ["Und", "bei", "dem", "ers\u00b7ten", "lin\u00b7den", "Stral"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Stieg eine Lerche aus den Schollen,", "tokens": ["Stieg", "ei\u00b7ne", "Ler\u00b7che", "aus", "den", "Schol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und lie\u00df ihr Tirilirum rollen", "tokens": ["Und", "lie\u00df", "ihr", "Ti\u00b7ri\u00b7li\u00b7rum", "rol\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Recht wacker durch den Aethersaal.", "tokens": ["Recht", "wa\u00b7cker", "durch", "den", "A\u00b7e\u00b7ther\u00b7saal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Die Quellchen, glitzernd wie Kristallen,", "tokens": ["Die", "Quell\u00b7chen", ",", "glit\u00b7zernd", "wie", "Kris\u00b7tal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Zweige, gl\u00e4nzend emaillirt \u2014", "tokens": ["Die", "Zwei\u00b7ge", ",", "gl\u00e4n\u00b7zend", "e\u00b7mail\u00b7lirt"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das kann dem Kenner schon gefallen,", "tokens": ["Das", "kann", "dem", "Ken\u00b7ner", "schon", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich nickte l\u00e4chelnd: \u201ees passirt!\u201c", "tokens": ["Ich", "nick\u00b7te", "l\u00e4\u00b7chelnd", ":", "\u201e", "es", "pas\u00b7sirt", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "$(", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und stapfte fort in eine Schluft,", "tokens": ["Und", "stapf\u00b7te", "fort", "in", "ei\u00b7ne", "Schluft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es war ein still und sonnig Fleckchen,", "tokens": ["Es", "war", "ein", "still", "und", "son\u00b7nig", "Fleck\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "KON", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wo tausend Anemonengl\u00f6ckchen", "tokens": ["Wo", "tau\u00b7send", "A\u00b7ne\u00b7mo\u00b7nen\u00b7gl\u00f6\u00b7ck\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "CARD", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Umgaukelten des Veilchens Duft.", "tokens": ["Um\u00b7gau\u00b7kel\u00b7ten", "des", "Veil\u00b7chens", "Duft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Das \u00fcpp'ge Moos \u2014 der Lerchen Lieder \u2014", "tokens": ["Das", "\u00fcpp'\u00b7ge", "Moos", "der", "Ler\u00b7chen", "Lie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der Blumen Flor \u2014 des Krautes Keim \u2014", "tokens": ["Der", "Blu\u00b7men", "Flor", "des", "Krau\u00b7tes", "Keim"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Auf meinen Mantel sa\u00df ich nieder", "tokens": ["Auf", "mei\u00b7nen", "Man\u00b7tel", "sa\u00df", "ich", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sann auf einen Fr\u00fchlingsreim.", "tokens": ["Und", "sann", "auf", "ei\u00b7nen", "Fr\u00fch\u00b7lings\u00b7reim", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da \u2014 alle Musen, welch ein Ton! \u2014", "tokens": ["Da", "al\u00b7le", "Mu\u00b7sen", ",", "welch", "ein", "Ton", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "$(", "PIAT", "NN", "$,", "PWAT", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da kam den Rain entlang gesungen", "tokens": ["Da", "kam", "den", "Rain", "ent\u00b7lang", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPO", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So eine Art von dummen Jungen,", "tokens": ["So", "ei\u00b7ne", "Art", "von", "dum\u00b7men", "Jun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Friedrich, meines Schreibers Sohn.", "tokens": ["Der", "Fried\u00b7rich", ",", "mei\u00b7nes", "Schrei\u00b7bers", "Sohn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Den Epheukranz im fl\u00e4chsnen Haare,", "tokens": ["Den", "E\u00b7pheu\u00b7kranz", "im", "fl\u00e4chs\u00b7nen", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In seiner Hand den Veilchenstrau\u00df,", "tokens": ["In", "sei\u00b7ner", "Hand", "den", "Veil\u00b7chen\u00b7strau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So trug er seine achtzehn Jahre", "tokens": ["So", "trug", "er", "sei\u00b7ne", "acht\u00b7zehn", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Romantisch in den Lenz hinaus.", "tokens": ["Ro\u00b7man\u00b7tisch", "in", "den", "Lenz", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nun schl\u00fcpft er durch des Hagens Loch,", "tokens": ["Nun", "schl\u00fcpft", "er", "durch", "des", "Ha\u00b7gens", "Loch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nun hing er an den Dornenzwecken", "tokens": ["Nun", "hing", "er", "an", "den", "Dor\u00b7nen\u00b7zwe\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wie Abrams Widder in den Hecken,", "tokens": ["Wie", "Ab\u00b7rams", "Wid\u00b7der", "in", "den", "He\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und in den Dornen pfiff er noch.", "tokens": ["Und", "in", "den", "Dor\u00b7nen", "pfiff", "er", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Bald hatt' er beugend, gleitend, springend,", "tokens": ["Bald", "hatt'", "er", "beu\u00b7gend", ",", "glei\u00b7tend", ",", "sprin\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,", "ADJD", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Blumenanger abgegrast,", "tokens": ["Den", "Blu\u00b7men\u00b7an\u00b7ger", "ab\u00b7ge\u00b7grast", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und rief nun, seine M\u00e4hnen schwingend:", "tokens": ["Und", "rief", "nun", ",", "sei\u00b7ne", "M\u00e4h\u00b7nen", "schwin\u00b7gend", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u201eviktoria, Trompeten blast!\u201c", "tokens": ["\u201e", "vik\u00b7to\u00b7ria", ",", "Trom\u00b7pe\u00b7ten", "blast", "!", "\u201c"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "$,", "NN", "VVFIN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Dann fl\u00fcstert er mit s\u00fc\u00dfem Hall:", "tokens": ["Dann", "fl\u00fcs\u00b7tert", "er", "mit", "s\u00fc\u00b7\u00dfem", "Hall", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u201eo, w\u00e4ren es die schwed'schen H\u00f6rner!\u201c", "tokens": ["\u201e", "o", ",", "w\u00e4\u00b7ren", "es", "die", "schwe\u00b7d'\u00b7schen", "H\u00f6r\u00b7ner", "!", "\u201c"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", 