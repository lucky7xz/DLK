{"textgrid.poem.61209": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun sei versiegelt jeder kleine Hader,", "tokens": ["Nun", "sei", "ver\u00b7sie\u00b7gelt", "je\u00b7der", "klei\u00b7ne", "Ha\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verstummt jedwede Klage, die wir sangen,", "tokens": ["Ver\u00b7stummt", "jed\u00b7we\u00b7de", "Kla\u00b7ge", ",", "die", "wir", "san\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da unser aller Feind sich unterfangen,", "tokens": ["Da", "un\u00b7ser", "al\u00b7ler", "Feind", "sich", "un\u00b7ter\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "PIAT", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus unsrer Burg zu brechen eine Quader.", "tokens": ["Aus", "uns\u00b7rer", "Burg", "zu", "bre\u00b7chen", "ei\u00b7ne", "Qua\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wem deutsches Blut noch f\u00fcllt die Herzensader,", "tokens": ["Wem", "deut\u00b7sches", "Blut", "noch", "f\u00fcllt", "die", "Her\u00b7zen\u00b7sa\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach anderm Recht nicht soll er jetzt verlangen,", "tokens": ["Nach", "an\u00b7derm", "Recht", "nicht", "soll", "er", "jetzt", "ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKNEG", "VMFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als schwertger\u00fcstet, Zornglut auf den Wangen,", "tokens": ["Als", "schwert\u00b7ge\u00b7r\u00fcs\u00b7tet", ",", "Zorn\u00b7glut", "auf", "den", "Wan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "$,", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu stehn mit seinen Br\u00fcdern im Geschwader.", "tokens": ["Zu", "stehn", "mit", "sei\u00b7nen", "Br\u00fc\u00b7dern", "im", "Ge\u00b7schwa\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Einm\u00fctig gilt's, das Banner hoch zu tragen,", "tokens": ["Ein\u00b7m\u00fc\u00b7tig", "gilt's", ",", "das", "Ban\u00b7ner", "hoch", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bis auf den Raub der Fremdling hat verzichtet,", "tokens": ["Bis", "auf", "den", "Raub", "der", "Fremd\u00b7ling", "hat", "ver\u00b7zich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo nicht, bis da\u00df im Blut er liegt erschlagen.", "tokens": ["Wo", "nicht", ",", "bis", "da\u00df", "im", "Blut", "er", "liegt", "er\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$,", "KON", "KOUS", "APPRART", "NN", "PPER", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wenn dann am Meer das Siegsmal aufgerichtet,", "tokens": ["Wenn", "dann", "am", "Meer", "das", "Siegs\u00b7mal", "auf\u00b7ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dann la\u00dft uns gehn, im Eichenforst zu tagen,", "tokens": ["Dann", "la\u00dft", "uns", "gehn", ",", "im", "Ei\u00b7chen\u00b7forst", "zu", "ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "VVINF", "$,", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und unser eigner Handel sei geschlichtet.", "tokens": ["Und", "un\u00b7ser", "eig\u00b7ner", "Han\u00b7del", "sei", "ge\u00b7schlich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}