{"textgrid.poem.40515": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Stehn zwei Sennenh\u00fctten ferne,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stehn zwei Sennenh\u00fctten ferne,", "tokens": ["Stehn", "zwei", "Sen\u00b7nen\u00b7h\u00fct\u00b7ten", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "NN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo die Alpenwiese lacht,", "tokens": ["Wo", "die", "Al\u00b7pen\u00b7wie\u00b7se", "lacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob den Giebeln halten Sterne,", "tokens": ["Ob", "den", "Gie\u00b7beln", "hal\u00b7ten", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blumen vor der Schwelle Wacht.", "tokens": ["Blu\u00b7men", "vor", "der", "Schwel\u00b7le", "Wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In dem Moos der einen H\u00fctte", "tokens": ["In", "dem", "Moos", "der", "ei\u00b7nen", "H\u00fct\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schl\u00e4ft die blonde Sennin leis;", "tokens": ["Schl\u00e4ft", "die", "blon\u00b7de", "Sen\u00b7nin", "leis", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Welches Alpenkind bestritte", "tokens": ["Wel\u00b7ches", "Al\u00b7pen\u00b7kind", "be\u00b7strit\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["PWAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihr der Sch\u00f6nheit ersten Preis?", "tokens": ["Ihr", "der", "Sch\u00f6n\u00b7heit", "ers\u00b7ten", "Preis", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Da\u00df mein Aug' noch Sch\u00f6n'res labe,", "tokens": ["Da\u00df", "mein", "Aug'", "noch", "Sch\u00f6n'\u00b7res", "la\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00fc\u00dft' ich wandern wahrlich weit,", "tokens": ["M\u00fc\u00dft'", "ich", "wan\u00b7dern", "wahr\u00b7lich", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn du, sch\u00f6ner J\u00e4gerknabe,", "tokens": ["Wenn", "du", ",", "sch\u00f6\u00b7ner", "J\u00e4\u00b7ger\u00b7kna\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht ihr l\u00e4gest hier zur Seit'!", "tokens": ["Nicht", "ihr", "l\u00e4\u00b7gest", "hier", "zur", "Seit'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und der Elf', der wei\u00dfe feine,", "tokens": ["Und", "der", "El\u00b7f'", ",", "der", "wei\u00b7\u00dfe", "fei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der die\u00df H\u00fcttlein treu bewacht,", "tokens": ["Der", "die\u00df", "H\u00fctt\u00b7lein", "treu", "be\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDS", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Legt zu H\u00e4upten ihnen eine", "tokens": ["Legt", "zu", "H\u00e4up\u00b7ten", "ih\u00b7nen", "ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "PPER", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Frische Rosenknospe sacht.", "tokens": ["Fri\u00b7sche", "Ro\u00b7sen\u00b7knos\u00b7pe", "sacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Als das Kn\u00f6spchen aufgegangen", "tokens": ["Als", "das", "Kn\u00f6spc\u00b7hen", "auf\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War zur bl\u00fch'nden Rose kaum,", "tokens": ["War", "zur", "bl\u00fch'n\u00b7den", "Ro\u00b7se", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat die Schlummernden umfangen", "tokens": ["Hat", "die", "Schlum\u00b7mern\u00b7den", "um\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gar ein lieblich s\u00fc\u00dfer Traum.", "tokens": ["Gar", "ein", "lieb\u00b7lich", "s\u00fc\u00b7\u00dfer", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "In dem Moos der andern H\u00fctte", "tokens": ["In", "dem", "Moos", "der", "an\u00b7dern", "H\u00fct\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schl\u00e4ft die braune Alpenmaid;", "tokens": ["Schl\u00e4ft", "die", "brau\u00b7ne", "Al\u00b7pen\u00b7maid", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Welch Gebirgskind wohl bestritte", "tokens": ["Welch", "Ge\u00b7birgs\u00b7kind", "wohl", "be\u00b7strit\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihr den Preis der H\u00e4\u00dflichkeit?", "tokens": ["Ihr", "den", "Preis", "der", "H\u00e4\u00df\u00b7lich\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Da\u00df Unholdres ich entdecke,", "tokens": ["Da\u00df", "Un\u00b7hold\u00b7res", "ich", "ent\u00b7de\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "M\u00fc\u00dft' ich wandern wahrlich weit,", "tokens": ["M\u00fc\u00dft'", "ich", "wan\u00b7dern", "wahr\u00b7lich", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn du, K\u00f6hler, schwarzer Recke,", "tokens": ["Wenn", "du", ",", "K\u00f6h\u00b7ler", ",", "schwar\u00b7zer", "Re\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht ihr l\u00e4gest hier zur Seit'!", "tokens": ["Nicht", "ihr", "l\u00e4\u00b7gest", "hier", "zur", "Seit'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Der Kobold, der braune Kleine,", "tokens": ["Der", "Ko\u00b7bold", ",", "der", "brau\u00b7ne", "Klei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "ADJA", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der die\u00df H\u00fcttlein treu bewacht,", "tokens": ["Der", "die\u00df", "H\u00fctt\u00b7lein", "treu", "be\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDS", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Legt zu H\u00e4upten ihnen eine", "tokens": ["Legt", "zu", "H\u00e4up\u00b7ten", "ih\u00b7nen", "ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "PPER", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Frische Rosenknospe sacht.", "tokens": ["Fri\u00b7sche", "Ro\u00b7sen\u00b7knos\u00b7pe", "sacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Als das Kn\u00f6spchen aufgegangen", "tokens": ["Als", "das", "Kn\u00f6spc\u00b7hen", "auf\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War zur bl\u00fch'nden Rose kaum,", "tokens": ["War", "zur", "bl\u00fch'n\u00b7den", "Ro\u00b7se", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat die Schlafenden umfangen", "tokens": ["Hat", "die", "Schla\u00b7fen\u00b7den", "um\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gar ein lieblich s\u00fc\u00dfer Traum. \u2013", "tokens": ["Gar", "ein", "lieb\u00b7lich", "s\u00fc\u00b7\u00dfer", "Traum", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "ADJD", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Morgens als erz\u00e4hlt ihr Tr\u00e4umen", "tokens": ["Mor\u00b7gens", "als", "er\u00b7z\u00e4hlt", "ihr", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses sich und jenes Paar,", "tokens": ["Die\u00b7ses", "sich", "und", "je\u00b7nes", "Paar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "PRF", "KON", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mocht' es sich gar seltsam reimen,", "tokens": ["Mocht'", "es", "sich", "gar", "selt\u00b7sam", "rei\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df derselbe Traum es war!", "tokens": ["Da\u00df", "der\u00b7sel\u00b7be", "Traum", "es", "war", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Morgens als im Himmelsgarten", "tokens": ["Mor\u00b7gens", "als", "im", "Him\u00b7mels\u00b7gar\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fr\u00fch der liebe Gott spaziert,", "tokens": ["Fr\u00fch", "der", "lie\u00b7be", "Gott", "spa\u00b7ziert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine Blumen mild zu warten,", "tokens": ["Sei\u00b7ne", "Blu\u00b7men", "mild", "zu", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deren Pracht sein Haus umziert;", "tokens": ["De\u00b7ren", "Pracht", "sein", "Haus", "um\u00b7ziert", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Fand er alle bl\u00fchn zum Besten,", "tokens": ["Fand", "er", "al\u00b7le", "bl\u00fchn", "zum", "Bes\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sonnenrosen \u00fcppig gl\u00fchn,", "tokens": ["Son\u00b7nen\u00b7ro\u00b7sen", "\u00fcp\u00b7pig", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Feuerb\u00fcsch' in Flammen\u00e4sten,", "tokens": ["Feu\u00b7er\u00b7b\u00fcsch'", "in", "Flam\u00b7me\u00b7n\u00e4s\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sternenblumen duftig spr\u00fchn;", "tokens": ["Ster\u00b7nen\u00b7blu\u00b7men", "duf\u00b7tig", "spr\u00fchn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Nur vom bl\u00fchendsten Gestr\u00e4uche,", "tokens": ["Nur", "vom", "bl\u00fc\u00b7hends\u00b7ten", "Ge\u00b7str\u00e4u\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Das ganz voll von Rosen stand,", "tokens": ["Das", "ganz", "voll", "von", "Ro\u00b7sen", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "APPR", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kamen Nachts ihm zwei ganz gleiche", "tokens": ["Ka\u00b7men", "Nachts", "ihm", "zwei", "ganz", "glei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPER", "CARD", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00f6ne Knospen heut' abhand.", "tokens": ["Sch\u00f6\u00b7ne", "Knos\u00b7pen", "heut'", "ab\u00b7hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}