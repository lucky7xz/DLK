{"textgrid.poem.59503": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Steh und falle mit eignem Kopfe,", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Steh und falle mit eignem Kopfe,", "tokens": ["Steh", "und", "fal\u00b7le", "mit", "eig\u00b7nem", "Kop\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Tu das Deine und tu es frisch!", "tokens": ["Tu", "das", "Dei\u00b7ne", "und", "tu", "es", "frisch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "PPOSAT", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Besser stolz an dem irdnen Topfe,", "tokens": ["Bes\u00b7ser", "stolz", "an", "dem", "ird\u00b7nen", "Top\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Als dem\u00fctig am goldnen Tisch:", "tokens": ["Als", "de\u00b7m\u00fc\u00b7tig", "am", "gold\u00b7nen", "Tisch", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "H\u00f6he hat Tiefe,", "tokens": ["H\u00f6\u00b7he", "hat", "Tie\u00b7fe", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Weltmeer hat Riffe,", "tokens": ["Welt\u00b7meer", "hat", "Rif\u00b7fe", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Gold hat Kummer und Schlangengezisch.", "tokens": ["Gold", "hat", "Kum\u00b7mer", "und", "Schlan\u00b7gen\u00b7ge\u00b7zisch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "KON", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Bau' dein Nest, weil der Fr\u00fchling w\u00e4hret,", "tokens": ["Bau'", "dein", "Nest", ",", "weil", "der", "Fr\u00fch\u00b7ling", "w\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Lustig bau's in die Welt hinein;", "tokens": ["Lus\u00b7tig", "bau's", "in", "die", "Welt", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Hell der Himmel sich oben kl\u00e4ret,", "tokens": ["Hell", "der", "Him\u00b7mel", "sich", "o\u00b7ben", "kl\u00e4\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PRF", "ADV", "VVFIN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Drunten duften die Bl\u00fcmelein:", "tokens": ["Drun\u00b7ten", "duf\u00b7ten", "die", "Bl\u00fc\u00b7me\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Wagen gewinnet,", "tokens": ["Wa\u00b7gen", "ge\u00b7win\u00b7net", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Schw\u00e4che zerrinnet,", "tokens": ["Schw\u00e4\u00b7che", "zer\u00b7rin\u00b7net", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Wage! Dulde! die Welt ist dein.", "tokens": ["Wa\u00b7ge", "!", "Dul\u00b7de", "!", "die", "Welt", "ist", "dein", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "ART", "NN", "VAFIN", "PPOSAT", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Steh nicht horchend, was Narren sprechen,", "tokens": ["Steh", "nicht", "hor\u00b7chend", ",", "was", "Nar\u00b7ren", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "VVPP", "$,", "PWS", "NN", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Jedem bl\u00fcht aus der Brust sein Stern;", "tokens": ["Je\u00b7dem", "bl\u00fcht", "aus", "der", "Brust", "sein", "Stern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Schicksal webet an stygischen B\u00e4chen,", "tokens": ["Schick\u00b7sal", "we\u00b7bet", "an", "sty\u00b7gi\u00b7schen", "B\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Feigen webet es schrecklich fern.", "tokens": ["Fei\u00b7gen", "we\u00b7bet", "es", "schreck\u00b7lich", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Steige hinnieder!", "tokens": ["Stei\u00b7ge", "hin\u00b7nie\u00b7der", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Fasse die Hyder!", "tokens": ["Fas\u00b7se", "die", "Hy\u00b7der", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Starken folget das Starke gern.", "tokens": ["Star\u00b7ken", "fol\u00b7get", "das", "Star\u00b7ke", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Wechselnd geht unter Leid und Freuden", "tokens": ["Wech\u00b7selnd", "geht", "un\u00b7ter", "Leid", "und", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Nicht mitf\u00fchlend der schnelle Tag.", "tokens": ["Nicht", "mit\u00b7f\u00fch\u00b7lend", "der", "schnel\u00b7le", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Jeder suche zum Kranze bescheiden,", "tokens": ["Je\u00b7der", "su\u00b7che", "zum", "Kran\u00b7ze", "be\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Was von Blumen er finden mag.", "tokens": ["Was", "von", "Blu\u00b7men", "er", "fin\u00b7den", "mag."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["PWS", "APPR", "NN", "PPER", "VVFIN", "NE"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Jugend verbl\u00fchet,", "tokens": ["Ju\u00b7gend", "ver\u00b7bl\u00fc\u00b7het", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Freude entfliehet:", "tokens": ["Freu\u00b7de", "ent\u00b7flie\u00b7het", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Lebe! Halte! doch lauf nicht nach!", "tokens": ["Le\u00b7be", "!", "Hal\u00b7te", "!", "doch", "lauf", "nicht", "nach", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "NN", "$.", "ADV", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}