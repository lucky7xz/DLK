{"textgrid.poem.62881": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Berauscht vom Wein des Gro\u00dfpapa", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Berauscht vom Wein des Gro\u00dfpapa", "tokens": ["Be\u00b7rauscht", "vom", "Wein", "des", "Gro\u00df\u00b7pa\u00b7pa"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Scho\u00df Amor einst im Scherz", "tokens": ["Scho\u00df", "A\u00b7mor", "einst", "im", "Scherz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NE", "ADV", "APPRART", "NN"], "meter": "++-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Pfeil nach ihm, und siehe da,", "tokens": ["Den", "Pfeil", "nach", "ihm", ",", "und", "sie\u00b7he", "da", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$,", "KON", "VVIMP", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er traf ihn just ins Herz.", "tokens": ["Er", "traf", "ihn", "just", "ins", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da wurde Zevs von Liebe krank,", "tokens": ["Da", "wur\u00b7de", "Zevs", "von", "Lie\u00b7be", "krank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fuhr auf die Unterwelt", "tokens": ["Fuhr", "auf", "die", "Un\u00b7ter\u00b7welt"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und machte manchen losen Schwank,", "tokens": ["Und", "mach\u00b7te", "man\u00b7chen", "lo\u00b7sen", "Schwank", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den uns Ovid erzehlt.", "tokens": ["Den", "uns", "O\u00b7vid", "er\u00b7zehlt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NE", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Und als er wieder zu sich kam,", "tokens": ["Und", "als", "er", "wie\u00b7der", "zu", "sich", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kehrt er in sein Quartier", "tokens": ["Kehrt", "er", "in", "sein", "Quar\u00b7tier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und sprach zum Amor voller Schaam:", "tokens": ["Und", "sprach", "zum", "A\u00b7mor", "vol\u00b7ler", "Schaam", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Streich bezahlst du mir.", "tokens": ["Den", "Streich", "be\u00b7zahlst", "du", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sein Zepter r\u00fchrt ihn an, und husch", "tokens": ["Sein", "Zep\u00b7ter", "r\u00fchrt", "ihn", "an", ",", "und", "husch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Str\u00e4ubt sich sein blondes Haar", "tokens": ["Str\u00e4ubt", "sich", "sein", "blon\u00b7des", "Haar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In einen hohen Federbusch,", "tokens": ["In", "ei\u00b7nen", "ho\u00b7hen", "Fe\u00b7der\u00b7busch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der noch geringelt war.", "tokens": ["Der", "noch", "ge\u00b7rin\u00b7gelt", "war", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Er ringt die Arme; diese ziehn", "tokens": ["Er", "ringt", "die", "Ar\u00b7me", ";", "die\u00b7se", "ziehn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "PDS", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Fl\u00fcgel sich zur\u00fcck", "tokens": ["In", "Fl\u00fc\u00b7gel", "sich", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "PRF", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von Gold und Lazur und Carmin,", "tokens": ["Von", "Gold", "und", "La\u00b7zur", "und", "Car\u00b7min", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie buntes G\u00fcldenst\u00fcck.", "tokens": ["Wie", "bun\u00b7tes", "G\u00fcl\u00b7den\u00b7st\u00fcck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Er wollte schreyn; stumm war der Hauch,", "tokens": ["Er", "woll\u00b7te", "schreyn", ";", "stumm", "war", "der", "Hauch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$.", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der seinem Mund entgieng.", "tokens": ["Der", "sei\u00b7nem", "Mund", "ent\u00b7gieng", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er wollte fliehn; sank auf den Bauch", "tokens": ["Er", "woll\u00b7te", "fliehn", ";", "sank", "auf", "den", "Bauch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF", "$.", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und war ein Schmetterling.", "tokens": ["Und", "war", "ein", "Schmet\u00b7ter\u00b7ling", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Das arme Kind! Sein Bogen liegt", "tokens": ["Das", "ar\u00b7me", "Kind", "!", "Sein", "Bo\u00b7gen", "liegt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erschlafft in tr\u00e4ger Ruh", "tokens": ["Er\u00b7schlafft", "in", "tr\u00e4\u00b7ger", "Ruh"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und er, stets wollustathmend, fliegt", "tokens": ["Und", "er", ",", "stets", "wol\u00b7lus\u00b7tath\u00b7mend", ",", "fliegt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "$,", "ADV", "ADJD", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Blumenbeeten zu.", "tokens": ["Den", "Blu\u00b7men\u00b7bee\u00b7ten", "zu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Itzt k\u00fc\u00dft er Nelke, Rose, Mohn", "tokens": ["Itzt", "k\u00fc\u00dft", "er", "Nel\u00b7ke", ",", "Ro\u00b7se", ",", "Mohn"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "NE", "$,", "NE", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Veilchen und Jesmin,", "tokens": ["Und", "Veil\u00b7chen", "und", "Jes\u00b7min", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NE", "$,"], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Und k\u00fc\u00dft sie kaum, so schw\u00e4rmt er schon", "tokens": ["Und", "k\u00fc\u00dft", "sie", "kaum", ",", "so", "schw\u00e4rmt", "er", "schon"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf andre Blumen hin.", "tokens": ["Auf", "and\u00b7re", "Blu\u00b7men", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Des kleinen S\u00fcnders Jammerstand", "tokens": ["Des", "klei\u00b7nen", "S\u00fcn\u00b7ders", "Jam\u00b7mer\u00b7stand"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erbarmt den Gro\u00dfpapa.", "tokens": ["Er\u00b7barmt", "den", "Gro\u00df\u00b7pa\u00b7pa", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf einen Wink von seiner Hand", "tokens": ["Auf", "ei\u00b7nen", "Wink", "von", "sei\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Stund Amor wieder da.", "tokens": ["Stund", "A\u00b7mor", "wie\u00b7der", "da", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Nur lie\u00df er, was von einem Gott", "tokens": ["Nur", "lie\u00df", "er", ",", "was", "von", "ei\u00b7nem", "Gott"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl nicht das kl\u00fcgste war,", "tokens": ["Wohl", "nicht", "das", "kl\u00fcgs\u00b7te", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "ADJA", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihm zum Ged\u00e4chtni\u00df und zum Spott", "tokens": ["Ihm", "zum", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "und", "zum", "Spott"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPRART", "NN", "KON", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Fl\u00fcgel buntes Paar.", "tokens": ["Der", "Fl\u00fc\u00b7gel", "bun\u00b7tes", "Paar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Seit dem f\u00fchrt Amor blos zum Scherz", "tokens": ["Seit", "dem", "f\u00fchrt", "A\u00b7mor", "blos", "zum", "Scherz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "VVFIN", "NE", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein zackichtes Gescho\u00df;", "tokens": ["Sein", "za\u00b7ckich\u00b7tes", "Ge\u00b7scho\u00df", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Er heftet heut sich an ein Herz", "tokens": ["Er", "hef\u00b7tet", "heut", "sich", "an", "ein", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und l\u00e4\u00dft es morgen los.", "tokens": ["Und", "l\u00e4\u00dft", "es", "mor\u00b7gen", "los", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "O wohl uns, da\u00df der Freundschaft Band", "tokens": ["O", "wohl", "uns", ",", "da\u00df", "der", "Freund\u00b7schaft", "Band"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADV", "PPER", "$,", "KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht in sein Reich geh\u00f6rt!", "tokens": ["Nicht", "in", "sein", "Reich", "ge\u00b7h\u00f6rt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nein, Minna, selbst durch Parzenhand", "tokens": ["Nein", ",", "Min\u00b7na", ",", "selbst", "durch", "Par\u00b7zen\u00b7hand"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "NE", "$,", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird unsers nicht zerst\u00f6rt.", "tokens": ["Wird", "un\u00b7sers", "nicht", "zer\u00b7st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}