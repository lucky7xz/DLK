{"dta.poem.12112": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "34.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich fuhr den See hinab und wollt' ihn recht beschaun,", "tokens": ["Ich", "fuhr", "den", "See", "hin\u00b7ab", "und", "wollt'", "ihn", "recht", "be\u00b7schaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "KON", "VMFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich fieng vom N\u00e4chsten an, dem Schifferm\u00e4dchen braun,", "tokens": ["Ich", "fi\u00b7eng", "vom", "N\u00e4chs\u00b7ten", "an", ",", "dem", "Schif\u00b7fer\u00b7m\u00e4d\u00b7chen", "braun", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Das gegen\u00fcber sa\u00df, den Blick mir zugewendet;", "tokens": ["Das", "ge\u00b7gen\u00b7\u00fc\u00b7ber", "sa\u00df", ",", "den", "Blick", "mir", "zu\u00b7ge\u00b7wen\u00b7det", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPO", "VVFIN", "$,", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ins Auge sah ich ihr, und war davon verblendet.", "tokens": ["Ins", "Au\u00b7ge", "sah", "ich", "ihr", ",", "und", "war", "da\u00b7von", "ver\u00b7blen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PPER", "$,", "KON", "VAFIN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und als ich anderer Aussichten noch genossen", "tokens": ["Und", "als", "ich", "an\u00b7de\u00b7rer", "Aus\u00b7sich\u00b7ten", "noch", "ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADJA", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als der auf ihr Gesicht, war meine Fahrt geschlossen.", "tokens": ["Als", "der", "auf", "ihr", "Ge\u00b7sicht", ",", "war", "mei\u00b7ne", "Fahrt", "ge\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPR", "PPOSAT", "NN", "$,", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}