{"textgrid.poem.46422": {"metadata": {"author": {"name": "Ewald, Johann Joachim", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du fr\u00e4gst, warum im Fr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du fr\u00e4gst, warum im Fr\u00fchling", "tokens": ["Du", "fr\u00e4gst", ",", "wa\u00b7rum", "im", "Fr\u00fch\u00b7ling"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht gleich die Rose bl\u00fche?", "tokens": ["Nicht", "gleich", "die", "Ro\u00b7se", "bl\u00fc\u00b7he", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Warum die Nachviole,", "tokens": ["Wa\u00b7rum", "die", "Nach\u00b7vi\u00b7o\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht auch bey Tage duffte?", "tokens": ["Nicht", "auch", "bey", "Ta\u00b7ge", "duff\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "--++-+-", "measure": "anapaest.init"}, "line.5": {"text": "Warum den gantzen Sommer", "tokens": ["Wa\u00b7rum", "den", "gant\u00b7zen", "Som\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nicht Philomele singe?", "tokens": ["Nicht", "Phi\u00b7lo\u00b7me\u00b7le", "sin\u00b7ge", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "NE", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "O Doris, frage lieber", "tokens": ["O", "Do\u00b7ris", ",", "fra\u00b7ge", "lie\u00b7ber"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "VVFIN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Warum ich dich nicht k\u00fcsse?", "tokens": ["Wa\u00b7rum", "ich", "dich", "nicht", "k\u00fcs\u00b7se", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}