{"textgrid.poem.38959": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Fr\u00fchling k\u00f6mmt!", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Fr\u00fchling k\u00f6mmt!", "tokens": ["Der", "Fr\u00fch\u00b7ling", "k\u00f6mmt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Die Wolken fliehn,", "tokens": ["Die", "Wol\u00b7ken", "fliehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der Himmel gl\u00e4nzt.", "tokens": ["Der", "Him\u00b7mel", "gl\u00e4nzt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Der Fr\u00fchling k\u00f6mmt!", "tokens": ["Der", "Fr\u00fch\u00b7ling", "k\u00f6mmt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und Regenbogen", "tokens": ["Und", "Re\u00b7gen\u00b7bo\u00b7gen"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Sind seines Wagens", "tokens": ["Sind", "sei\u00b7nes", "Wa\u00b7gens"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Gleitende R\u00e4der.", "tokens": ["Glei\u00b7ten\u00b7de", "R\u00e4\u00b7der", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Blumengekr\u00e4nzt,", "tokens": ["Blu\u00b7men\u00b7ge\u00b7kr\u00e4nzt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "In Sonnenstrahlen,", "tokens": ["In", "Son\u00b7nen\u00b7strah\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Schwebt unter s\u00e4uselnden Winden,", "tokens": ["Schwebt", "un\u00b7ter", "s\u00e4u\u00b7seln\u00b7den", "Win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Nieder der Gott.", "tokens": ["Nie\u00b7der", "der", "Gott", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Tausend Blumen bekr\u00e4nzen sein Haupt,", "tokens": ["Tau\u00b7send", "Blu\u00b7men", "be\u00b7kr\u00e4n\u00b7zen", "sein", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "Tausend Blumen unflechten", "tokens": ["Tau\u00b7send", "Blu\u00b7men", "un\u00b7flech\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["CARD", "NN", "ADJA"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.14": {"text": "Sein blaues Gewand:", "tokens": ["Sein", "blau\u00b7es", "Ge\u00b7wand", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Er l\u00e4chelt, \u2013", "tokens": ["Er", "l\u00e4\u00b7chelt", ",", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.16": {"text": "Aus goldnen Locken,", "tokens": ["Aus", "gold\u00b7nen", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "Vom blauen Gewande,", "tokens": ["Vom", "blau\u00b7en", "Ge\u00b7wan\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.18": {"text": "Flie\u00dfen zur Erde die Blumen hinab \u2013", "tokens": ["Flie\u00b7\u00dfen", "zur", "Er\u00b7de", "die", "Blu\u00b7men", "hin\u00b7ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "PTKVZ", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.19": {"text": "Es bl\u00fcht die Flur,", "tokens": ["Es", "bl\u00fcht", "die", "Flur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.20": {"text": "Es gr\u00fcnt der Hain,", "tokens": ["Es", "gr\u00fcnt", "der", "Hain", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.21": {"text": "Und jeder Zweig", "tokens": ["Und", "je\u00b7der", "Zweig"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.22": {"text": "Rauscht s\u00fc\u00dfen Gru\u00df", "tokens": ["Rauscht", "s\u00fc\u00b7\u00dfen", "Gru\u00df"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.23": {"text": "Dem Fr\u00fchlingsgotte.", "tokens": ["Dem", "Fr\u00fch\u00b7lings\u00b7got\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.24": {"text": "Wonnegesang,", "tokens": ["Won\u00b7ne\u00b7ge\u00b7sang", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.25": {"text": "Wonnegesang,", "tokens": ["Won\u00b7ne\u00b7ge\u00b7sang", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.26": {"text": "Rauscht durch den Palmenhain!", "tokens": ["Rauscht", "durch", "den", "Pal\u00b7men\u00b7hain", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.27": {"text": "Durch die bl\u00fchenden B\u00e4ume", "tokens": ["Durch", "die", "bl\u00fc\u00b7hen\u00b7den", "B\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.28": {"text": "S\u00e4uselt der West,", "tokens": ["S\u00e4u\u00b7selt", "der", "West", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.29": {"text": "Mit den Bl\u00fcthen scherzend.", "tokens": ["Mit", "den", "Bl\u00fc\u00b7then", "scher\u00b7zend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.30": {"text": "Da sch\u00fcttelt er Bl\u00fcthen", "tokens": ["Da", "sch\u00fct\u00b7telt", "er", "Bl\u00fc\u00b7then"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.31": {"text": "Und duftende Blumen,", "tokens": ["Und", "duf\u00b7ten\u00b7de", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.32": {"text": "Auf den gr\u00fcnen Rasen. \u2013", "tokens": ["Auf", "den", "gr\u00fc\u00b7nen", "Ra\u00b7sen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.33": {"text": "Wenn Mondschein sie k\u00fc\u00dft,", "tokens": ["Wenn", "Mond\u00b7schein", "sie", "k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.34": {"text": "Wenn Thau sie tr\u00e4nkt,", "tokens": ["Wenn", "Thau", "sie", "tr\u00e4nkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.35": {"text": "Mondschein des Fr\u00fchlings,", "tokens": ["Mond\u00b7schein", "des", "Fr\u00fch\u00b7lings", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.36": {"text": "Fr\u00fchlingsthau, \u2013", "tokens": ["Fr\u00fch\u00b7lingst\u00b7hau", ",", "\u2013"], "token_info": ["word", "punct", "punct"], "pos": ["NN", "$,", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.37": {"text": "Entschweben ihnen", "tokens": ["Ent\u00b7schwe\u00b7ben", "ih\u00b7nen"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.38": {"text": "Mit leisem Fluge,", "tokens": ["Mit", "lei\u00b7sem", "Flu\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.39": {"text": "Sch\u00f6ne blaue Schmetterlinge.", "tokens": ["Sch\u00f6\u00b7ne", "blau\u00b7e", "Schmet\u00b7ter\u00b7lin\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "In den Bl\u00fcthen der rauschenden B\u00e4ume,", "tokens": ["In", "den", "Bl\u00fc\u00b7then", "der", "rau\u00b7schen\u00b7den", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.41": {"text": "Unter den Blumen der duftenden Wiese,", "tokens": ["Un\u00b7ter", "den", "Blu\u00b7men", "der", "duf\u00b7ten\u00b7den", "Wie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.42": {"text": "Flattern und schw\u00e4rmen sie hier und bald dort:", "tokens": ["Flat\u00b7tern", "und", "schw\u00e4r\u00b7men", "sie", "hier", "und", "bald", "dort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "ADV", "KON", "ADV", "ADV", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.43": {"text": "Sie suchen die Schwestern,", "tokens": ["Sie", "su\u00b7chen", "die", "Schwes\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.44": {"text": "Sie suchen die Br\u00fcder,", "tokens": ["Sie", "su\u00b7chen", "die", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.45": {"text": "In Bl\u00fcthen und Blumen,", "tokens": ["In", "Bl\u00fc\u00b7then", "und", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.46": {"text": "Und k\u00fcssen sie alle:", "tokens": ["Und", "k\u00fcs\u00b7sen", "sie", "al\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.47": {"text": "Haben sie die Zwillingskinder aufgefunden,", "tokens": ["Ha\u00b7ben", "sie", "die", "Zwil\u00b7lings\u00b7kin\u00b7der", "auf\u00b7ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.48": {"text": "Nisten sie sich in dem v\u00e4terlichen Baum ein,", "tokens": ["Nis\u00b7ten", "sie", "sich", "in", "dem", "v\u00e4\u00b7ter\u00b7li\u00b7chen", "Baum", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.49": {"text": "Bergen sich in Bl\u00fcthen oder Blumen,", "tokens": ["Ber\u00b7gen", "sich", "in", "Bl\u00fc\u00b7then", "o\u00b7der", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.50": {"text": "An der s\u00fc\u00dfen Wiedererkennung sterbend.", "tokens": ["An", "der", "s\u00fc\u00b7\u00dfen", "Wie\u00b7der\u00b7er\u00b7ken\u00b7nung", "ster\u00b7bend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}}}}}