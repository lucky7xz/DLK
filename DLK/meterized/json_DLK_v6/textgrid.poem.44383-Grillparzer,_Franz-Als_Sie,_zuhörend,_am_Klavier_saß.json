{"textgrid.poem.44383": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Als Sie, zuh\u00f6rend, am Klavier sa\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.85", "en:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Still sa\u00df sie da, die Lieblichste von allen,", "tokens": ["Still", "sa\u00df", "sie", "da", ",", "die", "Lieb\u00b7lichs\u00b7te", "von", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aufhorchend, ohne Tadel, ohne Lob;", "tokens": ["Auf\u00b7hor\u00b7chend", ",", "oh\u00b7ne", "Ta\u00b7del", ",", "oh\u00b7ne", "Lob", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUI", "NN", "$,", "KOUI", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das dunkle Tuch war von der Brust gefallen,", "tokens": ["Das", "dunk\u00b7le", "Tuch", "war", "von", "der", "Brust", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die, nur vom Kleid bedeckt, sich atmend hob;", "tokens": ["Die", ",", "nur", "vom", "Kleid", "be\u00b7deckt", ",", "sich", "at\u00b7mend", "hob", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "APPRART", "NN", "VVPP", "$,", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das Haupt gesenkt, den Leib nach vorn gebogen,", "tokens": ["Das", "Haupt", "ge\u00b7senkt", ",", "den", "Leib", "nach", "vorn", "ge\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "APPR", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie von den fliehnden T\u00f6nen nachgezogen.", "tokens": ["Wie", "von", "den", "fliehn\u00b7den", "T\u00f6\u00b7nen", "nach\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nenn ich sie sch\u00f6n? Ist Sch\u00f6nheit doch ein Bild,", "tokens": ["Nenn", "ich", "sie", "sch\u00f6n", "?", "Ist", "Sch\u00f6n\u00b7heit", "doch", "ein", "Bild", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "$.", "VAFIN", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das selbst sich malt und nur sich selbst bedeutet,", "tokens": ["Das", "selbst", "sich", "malt", "und", "nur", "sich", "selbst", "be\u00b7deu\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PRF", "VVFIN", "KON", "ADV", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch H\u00f6heres aus diesen Z\u00fcgen quillt,", "tokens": ["Doch", "H\u00f6\u00b7he\u00b7res", "aus", "die\u00b7sen", "Z\u00fc\u00b7gen", "quillt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die wie die Z\u00fcge einer Schrift verbreitet,", "tokens": ["Die", "wie", "die", "Z\u00fc\u00b7ge", "ei\u00b7ner", "Schrift", "ver\u00b7brei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "An sich oft bildlos, unscheinbare Zeichen,", "tokens": ["An", "sich", "oft", "bild\u00b7los", ",", "un\u00b7schein\u00b7ba\u00b7re", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADV", "ADJD", "$,", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Doch himmlisch durch den Sinn, den sie erreichen.", "tokens": ["Doch", "himm\u00b7lisch", "durch", "den", "Sinn", ",", "den", "sie", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So sa\u00df sie da, das Regen nur der Wangen", "tokens": ["So", "sa\u00df", "sie", "da", ",", "das", "Re\u00b7gen", "nur", "der", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit ihren zarten Muskeln rund und weich,", "tokens": ["Mit", "ih\u00b7ren", "zar\u00b7ten", "Mus\u00b7keln", "rund", "und", "weich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Wimpern Zucken, die das Aug umhangen,", "tokens": ["Der", "Wim\u00b7pern", "Zu\u00b7cken", ",", "die", "das", "Aug", "um\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Lippen Spiel, die Purpurl\u00e4dchen gleich,", "tokens": ["Der", "Lip\u00b7pen", "Spiel", ",", "die", "Pur\u00b7pur\u00b7l\u00e4d\u00b7chen", "gleich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Den Schatz von Perlen h\u00fcllen jetzt, nun zeigen,", "tokens": ["Den", "Schatz", "von", "Per\u00b7len", "h\u00fcl\u00b7len", "jetzt", ",", "nun", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "ADV", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Verriet Gef\u00fchl, von dem die Worte schweigen.", "tokens": ["Ver\u00b7riet", "Ge\u00b7f\u00fchl", ",", "von", "dem", "die", "Wor\u00b7te", "schwei\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "APPR", "PRELS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und wie die T\u00f6ne brausend sich verwirren,", "tokens": ["Und", "wie", "die", "T\u00f6\u00b7ne", "brau\u00b7send", "sich", "ver\u00b7wir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADJD", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In stetem Kampfe stets nur halb vers\u00f6hnt,", "tokens": ["In", "ste\u00b7tem", "Kamp\u00b7fe", "stets", "nur", "halb", "ver\u00b7s\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Jetzt klagen, wie verflogne Tauben girren,", "tokens": ["Jetzt", "kla\u00b7gen", ",", "wie", "ver\u00b7flog\u00b7ne", "Tau\u00b7ben", "gir\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "PWAV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Jetzt st\u00fcrmen, wie der Gang der Wetter dr\u00f6hnt,", "tokens": ["Jetzt", "st\u00fcr\u00b7men", ",", "wie", "der", "Gang", "der", "Wet\u00b7ter", "dr\u00f6hnt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sah ich ihr Lust und Qual im Antlitz kriegen", "tokens": ["Sah", "ich", "ihr", "Lust", "und", "Qual", "im", "Ant\u00b7litz", "krie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "KON", "NN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und jeder Ton ward Bild in ihren Z\u00fcgen.", "tokens": ["Und", "je\u00b7der", "Ton", "ward", "Bild", "in", "ih\u00b7ren", "Z\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Mitleidend wollt ich schon zum K\u00fcnstler rufen:", "tokens": ["Mit\u00b7lei\u00b7dend", "wollt", "ich", "schon", "zum", "K\u00fcnst\u00b7ler", "ru\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbhalt ein! Warum zermalmst du ihre Brust?\u00ab", "tokens": ["\u00bb", "halt", "ein", "!", "Wa\u00b7rum", "zer\u00b7malmst", "du", "ih\u00b7re", "Brust", "?", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$.", "PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "$("], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Da war erreicht die schneidendste der Stufen,", "tokens": ["Da", "war", "er\u00b7reicht", "die", "schnei\u00b7dends\u00b7te", "der", "Stu\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "ART", "ADJA", "ART", "NN", "$,"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der Ton des Schmerzes ward zum Ton der Lust,", "tokens": ["Der", "Ton", "des", "Schmer\u00b7zes", "ward", "zum", "Ton", "der", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und wie Neptun, vor dem die St\u00fcrme flogen,", "tokens": ["Und", "wie", "Nep\u00b7tun", ",", "vor", "dem", "die", "St\u00fcr\u00b7me", "flo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NN", "$,", "APPR", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Hob sich der Dreiklang ebnend aus den Wogen,", "tokens": ["Hob", "sich", "der", "Drei\u00b7klang", "eb\u00b7nend", "aus", "den", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und wie die Sonne steigt; die Strahlen dringen", "tokens": ["Und", "wie", "die", "Son\u00b7ne", "steigt", ";", "die", "Strah\u00b7len", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "$.", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch der zersprengten Wetter dunkle Nacht,", "tokens": ["Durch", "der", "zer\u00b7spreng\u00b7ten", "Wet\u00b7ter", "dunk\u00b7le", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So ging ihr Aug, an dem noch Tropfen hingen,", "tokens": ["So", "ging", "ihr", "Aug", ",", "an", "dem", "noch", "Trop\u00b7fen", "hin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "APPR", "ART", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hellgl\u00e4nzend auf in sonnengleicher Pracht;", "tokens": ["Hell\u00b7gl\u00e4n\u00b7zend", "auf", "in", "son\u00b7nen\u00b7glei\u00b7cher", "Pracht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein leises Ach aus ihrem s\u00fc\u00dfen Munde,", "tokens": ["Ein", "lei\u00b7ses", "Ach", "aus", "ih\u00b7rem", "s\u00fc\u00b7\u00dfen", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sah, wie nach Mitgef\u00fchl, sie in die Runde.", "tokens": ["Sah", ",", "wie", "nach", "Mit\u00b7ge\u00b7f\u00fchl", ",", "sie", "in", "die", "Run\u00b7de", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "APPR", "NN", "$,", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Da triebs mich auf; nun soll sies h\u00f6ren!", "tokens": ["Da", "triebs", "mich", "auf", ";", "nun", "soll", "sies", "h\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$.", "ADV", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was mich schon l\u00e4ngst bewegt, nun werd ihrs kund!", "tokens": ["Was", "mich", "schon", "l\u00e4ngst", "be\u00b7wegt", ",", "nun", "werd", "ihrs", "kund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "VVFIN", "$,", "ADV", "VAFIN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch blickt sie her; den K\u00fcnstler nicht zu st\u00f6ren", "tokens": ["Doch", "blickt", "sie", "her", ";", "den", "K\u00fcnst\u00b7ler", "nicht", "zu", "st\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$.", "ART", "NN", "PTKNEG", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Befiehlt ihr Finger schwichtgend an dem Mund,", "tokens": ["Be\u00b7fiehlt", "ihr", "Fin\u00b7ger", "schwicht\u00b7gend", "an", "dem", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und wieder seh ich horchend sie sich neigen", "tokens": ["Und", "wie\u00b7der", "seh", "ich", "hor\u00b7chend", "sie", "sich", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "VVPP", "PPER", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und wieder mu\u00df ich sitzen, wieder schweigen.", "tokens": ["Und", "wie\u00b7der", "mu\u00df", "ich", "sit\u00b7zen", ",", "wie\u00b7der", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "VVINF", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}