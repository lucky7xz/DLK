{"textgrid.poem.59970": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kennt ihr, kennt ihr das freie Wort,", "genre": "verse", "period": "N.A.", "pub_year": 1840, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kennt ihr, kennt ihr das freie Wort,", "tokens": ["Kennt", "ihr", ",", "kennt", "ihr", "das", "frei\u00b7e", "Wort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Das mit der Sonne kreist?", "tokens": ["Das", "mit", "der", "Son\u00b7ne", "kreist", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das mit den Wogen donnert fort?", "tokens": ["Das", "mit", "den", "Wo\u00b7gen", "don\u00b7nert", "fort", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das mit dem Sturme reist?", "tokens": ["Das", "mit", "dem", "Stur\u00b7me", "reist", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Das reich wie Tau vom Himmel tropft,", "tokens": ["Das", "reich", "wie", "Tau", "vom", "Him\u00b7mel", "tropft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "KOKOM", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "An H\u00fctten und Pal\u00e4ste klopft?", "tokens": ["An", "H\u00fct\u00b7ten", "und", "Pa\u00b7l\u00e4s\u00b7te", "klopft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kennt ihr den freien Geist?", "tokens": ["Kennt", "ihr", "den", "frei\u00b7en", "Geist", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Kennt ihr, kennt ihr das Zauberschwert,", "tokens": ["Kennt", "ihr", ",", "kennt", "ihr", "das", "Zau\u00b7ber\u00b7schwert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Dem jede L\u00fcge f\u00e4llt?", "tokens": ["Dem", "je\u00b7de", "L\u00fc\u00b7ge", "f\u00e4llt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kennt ihr den Meister, lieb und wert?", "tokens": ["Kennt", "ihr", "den", "Meis\u00b7ter", ",", "lieb", "und", "wert", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kennt ihr der Helden Held?", "tokens": ["Kennt", "ihr", "der", "Hel\u00b7den", "Held", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von Gutenberg singt mir ein Lied,", "tokens": ["Von", "Gu\u00b7ten\u00b7berg", "singt", "mir", "ein", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Ein Lied vom guten Waffenschmied,", "tokens": ["Ein", "Lied", "vom", "gu\u00b7ten", "Waf\u00b7fen\u00b7schmied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Lied vom Mann der Welt!", "tokens": ["Ein", "Lied", "vom", "Mann", "der", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Wahrheit klingt von Ohr zu Ohr,", "tokens": ["Die", "Wahr\u00b7heit", "klingt", "von", "Ohr", "zu", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie klingt von Mund zu Mund,", "tokens": ["Sie", "klingt", "von", "Mund", "zu", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie hat wie Sterne ihren Chor", "tokens": ["Sie", "hat", "wie", "Ster\u00b7ne", "ih\u00b7ren", "Chor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "KOKOM", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und tut sich allw\u00e4rts kund;", "tokens": ["Und", "tut", "sich", "all\u00b7w\u00e4rts", "kund", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das Wort rauscht wie das freie Meer", "tokens": ["Das", "Wort", "rauscht", "wie", "das", "frei\u00b7e", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Frei um die weite Erde her", "tokens": ["Frei", "um", "die", "wei\u00b7te", "Er\u00b7de", "her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "APZR"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Und schlie\u00dft den V\u00f6lkerbund.", "tokens": ["Und", "schlie\u00dft", "den", "V\u00f6l\u00b7ker\u00b7bund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und der es so aus stummer Nacht", "tokens": ["Und", "der", "es", "so", "aus", "stum\u00b7mer", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erl\u00f6st, der das getan,", "tokens": ["Er\u00b7l\u00f6st", ",", "der", "das", "ge\u00b7tan", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "ART", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der tausendf\u00e4ltig es gemacht,", "tokens": ["Der", "tau\u00b7send\u00b7f\u00e4l\u00b7tig", "es", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem stimmt ein Loblied an!", "tokens": ["Dem", "stimmt", "ein", "Lob\u00b7lied", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Heil ihm, Heil ihm und seinem Werk!", "tokens": ["Heil", "ihm", ",", "Heil", "ihm", "und", "sei\u00b7nem", "Werk", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Dem Gutenberg, dem besten Berg!", "tokens": ["Dem", "Gu\u00b7ten\u00b7berg", ",", "dem", "bes\u00b7ten", "Berg", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Heil jedem freien Mann!", "tokens": ["Heil", "je\u00b7dem", "frei\u00b7en", "Mann", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}