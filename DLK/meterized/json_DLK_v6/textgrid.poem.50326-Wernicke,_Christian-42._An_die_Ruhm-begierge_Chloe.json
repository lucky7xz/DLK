{"textgrid.poem.50326": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "42. An die Ruhm-begierge Chloe", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du, dass ich dich r\u00fchmen soll,", "tokens": ["Willst", "du", ",", "dass", "ich", "dich", "r\u00fch\u00b7men", "soll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So deute meine Meinung woll;", "tokens": ["So", "deu\u00b7te", "mei\u00b7ne", "Mei\u00b7nung", "woll", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und denck' in dem ich r\u00fchm', auf keine ", "tokens": ["Und", "denck", "in", "dem", "ich", "r\u00fchm'", ",", "auf", "kei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVIMP", "APPR", "PRELS", "PPER", "VVFIN", "$,", "APPR", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}