{"dta.poem.8224": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "5 .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Nun ist der Tag herum;", "tokens": ["Nun", "ist", "der", "Tag", "he\u00b7rum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da schweigen alle V\u00f6gel bald", "tokens": ["Da", "schwei\u00b7gen", "al\u00b7le", "V\u00f6\u00b7gel", "bald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Walde um und um.", "tokens": ["Im", "Wal\u00b7de", "um", "und", "um", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "KON", "KOUI", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Drossel pfeift ihr letztes St\u00fcck,", "tokens": ["Die", "Dros\u00b7sel", "pfeift", "ihr", "letz\u00b7tes", "St\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein St\u00fcck zu Allerbest;", "tokens": ["Ein", "St\u00fcck", "zu", "Al\u00b7ler\u00b7best", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die Amsel schl\u00e4gt den letzten Ton,", "tokens": ["Die", "Am\u00b7sel", "schl\u00e4gt", "den", "letz\u00b7ten", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und fliegt zu Nest, zu Nest.", "tokens": ["Und", "fliegt", "zu", "Nest", ",", "zu", "Nest", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NE", "$,", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da nehm' auch ich zu guter Nacht", "tokens": ["Da", "nehm'", "auch", "ich", "zu", "gu\u00b7ter", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur Hand die Geige mein;", "tokens": ["Zur", "Hand", "die", "Gei\u00b7ge", "mein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das ist ein klingend' Nachtgebet,", "tokens": ["Das", "ist", "ein", "klin\u00b7gend'", "Nacht\u00b7ge\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und steigt zum Himmel ein.", "tokens": ["Und", "steigt", "zum", "Him\u00b7mel", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}