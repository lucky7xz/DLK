{"textgrid.poem.35179": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Ein inneres Land", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schau dir die Menschen geistig an;", "tokens": ["Schau", "dir", "die", "Men\u00b7schen", "geis\u00b7tig", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Dein leiblich Aug sieht weiter nichts,", "tokens": ["Dein", "leib\u00b7lich", "Aug", "sieht", "wei\u00b7ter", "nichts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "ADV", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als was es eben sehen kann", "tokens": ["Als", "was", "es", "e\u00b7ben", "se\u00b7hen", "kann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PWS", "PPER", "ADV", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Schein des \u00e4u\u00dferlichen Lichts.", "tokens": ["Im", "Schein", "des", "\u00e4u\u00b7\u00dfer\u00b7li\u00b7chen", "Lichts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es wohnt in einem andern Lichte", "tokens": ["Es", "wohnt", "in", "ei\u00b7nem", "an\u00b7dern", "Lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In ihm ein andres, zweites Sein,", "tokens": ["In", "ihm", "ein", "and\u00b7res", ",", "zwei\u00b7tes", "Sein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "PIS", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und dieses zu erkennen, richte", "tokens": ["Und", "die\u00b7ses", "zu", "er\u00b7ken\u00b7nen", ",", "rich\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PDS", "PTKZU", "VVINF", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den andern Blick in ihn hinein.", "tokens": ["Den", "an\u00b7dern", "Blick", "in", "ihn", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es dehnt sich da ein weites Land", "tokens": ["Es", "dehnt", "sich", "da", "ein", "wei\u00b7tes", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Oft abgrundstief, oft steil empor.", "tokens": ["Oft", "ab\u00b7grund\u00b7stief", ",", "oft", "steil", "em\u00b7por", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es d\u00fcrstet da der W\u00fcste Sand,", "tokens": ["Es", "d\u00fcrs\u00b7tet", "da", "der", "W\u00fcs\u00b7te", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es spritzt der Sumpf, es weint das Moor.", "tokens": ["Es", "spritzt", "der", "Sumpf", ",", "es", "weint", "das", "Moor", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es rauscht der Wald; es stehn zur Ernte", "tokens": ["Es", "rauscht", "der", "Wald", ";", "es", "stehn", "zur", "Ern\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Garten und das Feld bereit,", "tokens": ["Der", "Gar\u00b7ten", "und", "das", "Feld", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und sonnig hell steigt das entfernte", "tokens": ["Und", "son\u00b7nig", "hell", "steigt", "das", "ent\u00b7fern\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJD", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Gebirge auf zur Ewigkeit.", "tokens": ["Ge\u00b7bir\u00b7ge", "auf", "zur", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und dieses Land ist reich belebt", "tokens": ["Und", "die\u00b7ses", "Land", "ist", "reich", "be\u00b7lebt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "VAFIN", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von fl\u00fcchtgen Wesen ohne Zahl.", "tokens": ["Von", "fl\u00fccht\u00b7gen", "We\u00b7sen", "oh\u00b7ne", "Zahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das lacht und weint, das sorgt und strebt,", "tokens": ["Das", "lacht", "und", "weint", ",", "das", "sorgt", "und", "strebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "$,", "PRELS", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bald hoch empor, bald tief zu Thal.", "tokens": ["Bald", "hoch", "em\u00b7por", ",", "bald", "tief", "zu", "Thal", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "$,", "ADV", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es sind die r\u00fchrigen Gedanken,", "tokens": ["Es", "sind", "die", "r\u00fch\u00b7ri\u00b7gen", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die niemals schweigen, nimmer ruhn,", "tokens": ["Die", "nie\u00b7mals", "schwei\u00b7gen", ",", "nim\u00b7mer", "ruhn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Heut aufrecht gehn und morgen schwanken,", "tokens": ["Heut", "auf\u00b7recht", "gehn", "und", "mor\u00b7gen", "schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "KON", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Hier Gutes und dort B\u00f6ses thun.", "tokens": ["Hier", "Gu\u00b7tes", "und", "dort", "B\u00f6\u00b7ses", "thun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Schau dir die Menschen geistig an,", "tokens": ["Schau", "dir", "die", "Men\u00b7schen", "geis\u00b7tig", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Dann siehst du diese andre Welt,", "tokens": ["Dann", "siehst", "du", "die\u00b7se", "and\u00b7re", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die ihr Gebiet nicht Jedermann", "tokens": ["Die", "ihr", "Ge\u00b7biet", "nicht", "Je\u00b7der\u00b7mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "PTKNEG", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bequemlich vor die Augen stellt.", "tokens": ["Be\u00b7quem\u00b7lich", "vor", "die", "Au\u00b7gen", "stellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann tagt wohl auch in deinem Innern", "tokens": ["Dann", "tagt", "wohl", "auch", "in", "dei\u00b7nem", "In\u00b7nern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Welt, die dort vorhanden ist,", "tokens": ["Die", "Welt", ",", "die", "dort", "vor\u00b7han\u00b7den", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Um dich zu mahnen, zu erinnern,", "tokens": ["Um", "dich", "zu", "mah\u00b7nen", ",", "zu", "e\u00b7rin\u00b7nern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "PPER", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wie viel du ihr noch schuldig bist.", "tokens": ["Wie", "viel", "du", "ihr", "noch", "schul\u00b7dig", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}