{"textgrid.poem.53153": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer der Heyraht S\u00fcssigheit", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer der Heyraht S\u00fcssigheit", "tokens": ["Wer", "der", "Hey\u00b7raht", "S\u00fcs\u00b7sig\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Einmal recht empfunden,", "tokens": ["Ein\u00b7mal", "recht", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Macht sie ihm gleich manche zeit", "tokens": ["Macht", "sie", "ihm", "gleich", "man\u00b7che", "zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PPER", "ADV", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch so grosse Wunden,", "tokens": ["Noch", "so", "gros\u00b7se", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Dennoch bleibt jhm die Begier", "tokens": ["Den\u00b7noch", "bleibt", "jhm", "die", "Be\u00b7gier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Allzeit nur nach ihr.", "tokens": ["All\u00b7zeit", "nur", "nach", "ihr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "$."], "meter": "+-+--", "measure": "unknown.measure.di"}}, "stanza.2": {"line.1": {"text": "Thyrsis traurte zweymal schon,", "tokens": ["Thyr\u00b7sis", "traur\u00b7te", "zwey\u00b7mal", "schon", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zweymal fiel ihm nieder", "tokens": ["Zwey\u00b7mal", "fiel", "ihm", "nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Seines Hertzens Frewden-Krohn',", "tokens": ["Sei\u00b7nes", "Hert\u00b7zens", "Fre\u00b7wden\u00b7Krohn'", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jetzund freyht er wieder,", "tokens": ["Je\u00b7tzund", "freyht", "er", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Vnd bezeugt zum dritten mahl,", "tokens": ["Vnd", "be\u00b7zeugt", "zum", "drit\u00b7ten", "mahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Heyraht sey nicht Quahl.", "tokens": ["Hey\u00b7raht", "sey", "nicht", "Quahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKNEG", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Ist wo ein gejagtes Thier", "tokens": ["Ist", "wo", "ein", "ge\u00b7jag\u00b7tes", "Thier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch das Garn gerissen,", "tokens": ["Durch", "das", "Garn", "ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nachmals wird es sich daf\u00fcr", "tokens": ["Nach\u00b7mals", "wird", "es", "sich", "da\u00b7f\u00fcr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "PAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wol zu h\u00fcten wissen,", "tokens": ["Wol", "zu", "h\u00fc\u00b7ten", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Auch ein Fisch, der los seyn kan,", "tokens": ["Auch", "ein", "Fisch", ",", "der", "los", "seyn", "kan", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "ADJD", "VAINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bei\u00dft nicht wieder an.", "tokens": ["Bei\u00dft", "nicht", "wie\u00b7der", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Thyrsis liebt das Wiederspiel,", "tokens": ["Thyr\u00b7sis", "liebt", "das", "Wie\u00b7der\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn sein' Heyraht-Sachen", "tokens": ["Denn", "sein'", "Hey\u00b7raht\u00b7Sa\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Pflagen ihm der Frewden viel", "tokens": ["Pfla\u00b7gen", "ihm", "der", "Frew\u00b7den", "viel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor der Zeit zu machen,", "tokens": ["Vor", "der", "Zeit", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Darumb geht er wieder ein", "tokens": ["Da\u00b7rumb", "geht", "er", "wie\u00b7der", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ART"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Solche s\u00fcsse Pein.", "tokens": ["Sol\u00b7che", "s\u00fcs\u00b7se", "Pein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Was ist lieben Hertzen gleich,", "tokens": ["Was", "ist", "lie\u00b7ben", "Hert\u00b7zen", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die sich keusch gesellen?", "tokens": ["Die", "sich", "keusch", "ge\u00b7sel\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADJD", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sie sind, die ein Himmelreich", "tokens": ["Sie", "sind", ",", "die", "ein", "Him\u00b7mel\u00b7reich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Machen au\u00df der Hellen:", "tokens": ["Ma\u00b7chen", "au\u00df", "der", "Hel\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ihre Trew in aller Noht", "tokens": ["Ih\u00b7re", "Trew", "in", "al\u00b7ler", "Noht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wehrt bi\u00df in den Tod.", "tokens": ["Wehrt", "bi\u00df", "in", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Kommt ihr Hirten, kommt zu hauff,", "tokens": ["Kommt", "ihr", "Hir\u00b7ten", ",", "kommt", "zu", "hauff", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Thyrsis Fest zu ehren,", "tokens": ["Thyr\u00b7sis", "Fest", "zu", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Setzt ihm Laub von Myrten auff", "tokens": ["Setzt", "ihm", "Laub", "von", "Myr\u00b7ten", "auff"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "APPR", "NN", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vnd la\u00dft W\u00fcnsche h\u00f6ren,", "tokens": ["Vnd", "la\u00dft", "W\u00fcn\u00b7sche", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Da\u00df sein Heyrathwerck an Trew", "tokens": ["Da\u00df", "sein", "Hey\u00b7rath\u00b7werck", "an", "Trew"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gleich den ersten sey!", "tokens": ["Gleich", "den", "ers\u00b7ten", "sey", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}