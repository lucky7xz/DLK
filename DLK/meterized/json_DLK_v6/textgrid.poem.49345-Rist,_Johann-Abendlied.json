{"textgrid.poem.49345": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Abendlied", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Tag ist hin, der Sonnen Glanz", "tokens": ["Der", "Tag", "ist", "hin", ",", "der", "Son\u00b7nen", "Glanz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat nunmehr sich verloren ganz:", "tokens": ["Hat", "nun\u00b7mehr", "sich", "ver\u00b7lo\u00b7ren", "ganz", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PRF", "VVPP", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Itz bricht die finstre Nacht herf\u00fcr", "tokens": ["Itz", "bricht", "die", "finst\u00b7re", "Nacht", "her\u00b7f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und \u00f6ffnet uns die Sternenth\u00fcr.", "tokens": ["Und", "\u00f6ff\u00b7net", "uns", "die", "Ster\u00b7nent\u00b7h\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf, meine Seel', und hab' itz Acht,", "tokens": ["Auf", ",", "mei\u00b7ne", "Seel'", ",", "und", "hab'", "itz", "Acht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PPOSAT", "NN", "$,", "KON", "VAFIN", "ADV", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was du den ganzen Tag gemacht,", "tokens": ["Was", "du", "den", "gan\u00b7zen", "Tag", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dein Sch\u00f6pfer wil, du solst ihm nun", "tokens": ["Dein", "Sch\u00f6p\u00b7fer", "wil", ",", "du", "solst", "ihm", "nun"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "$,", "PPER", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von deinem Wandel Rechnung thun.", "tokens": ["Von", "dei\u00b7nem", "Wan\u00b7del", "Rech\u00b7nung", "thun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ich komm', o Vater, itz heran,", "tokens": ["Ich", "komm'", ",", "o", "Va\u00b7ter", ",", "itz", "he\u00b7ran", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "FM", "NN", "$,", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wiewol ich nichts mich r\u00fchmen kan;", "tokens": ["Wie\u00b7wol", "ich", "nichts", "mich", "r\u00fch\u00b7men", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ges\u00fcndigt hab' ich diesen Tag", "tokens": ["Ge\u00b7s\u00fcn\u00b7digt", "hab'", "ich", "die\u00b7sen", "Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPER", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "So, da\u00df ich kaum erscheinen mag.", "tokens": ["So", ",", "da\u00df", "ich", "kaum", "er\u00b7schei\u00b7nen", "mag."], "token_info": ["word", "punct", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "O gro\u00dfer Gott, die Dunkelheit", "tokens": ["O", "gro\u00b7\u00dfer", "Gott", ",", "die", "Dun\u00b7kel\u00b7heit"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Versetzet mich in Traurigkeit,", "tokens": ["Ver\u00b7set\u00b7zet", "mich", "in", "Trau\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Denn welch' auf b\u00f6sen Wegen gehn,", "tokens": ["Denn", "welch'", "auf", "b\u00f6\u00b7sen", "We\u00b7gen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Die m\u00fcssen stets im Dunklen stehn.", "tokens": ["Die", "m\u00fcs\u00b7sen", "stets", "im", "Dunk\u00b7len", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Wo sol ich hin? Die finstre Nacht", "tokens": ["Wo", "sol", "ich", "hin", "?", "Die", "finst\u00b7re", "Nacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "PTKVZ", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Hat, mich zu sch\u00fctzen, keine Macht,", "tokens": ["Hat", ",", "mich", "zu", "sch\u00fct\u00b7zen", ",", "kei\u00b7ne", "Macht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PPER", "PTKZU", "VVINF", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Das Unrecht l\u00e4\u00dft sich bergen nicht", "tokens": ["Das", "Un\u00b7recht", "l\u00e4\u00dft", "sich", "ber\u00b7gen", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "F\u00fcr dir, o Gott, du gro\u00dfes Licht.", "tokens": ["F\u00fcr", "dir", ",", "o", "Gott", ",", "du", "gro\u00b7\u00dfes", "Licht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "FM", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Nim wieder mich zu Gnaden an,", "tokens": ["Nim", "wie\u00b7der", "mich", "zu", "Gna\u00b7den", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPER", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Dieweil ich nicht entfliehen kan;", "tokens": ["Die\u00b7weil", "ich", "nicht", "ent\u00b7flie\u00b7hen", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Durch Jesum such' ich Fried' und Ruh',", "tokens": ["Durch", "Je\u00b7sum", "such'", "ich", "Fried'", "und", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Es decke mich sein' Unschuld zu.", "tokens": ["Es", "de\u00b7cke", "mich", "sein'", "Un\u00b7schuld", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Durch Jesum Christum lob' ich dich,", "tokens": ["Durch", "Je\u00b7sum", "Chris\u00b7tum", "lob'", "ich", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Da\u00df du mich hast so gn\u00e4diglich", "tokens": ["Da\u00df", "du", "mich", "hast", "so", "gn\u00e4\u00b7di\u00b7glich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Besch\u00fctzet diesen ganzen Tag", "tokens": ["Be\u00b7sch\u00fct\u00b7zet", "die\u00b7sen", "gan\u00b7zen", "Tag"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "F\u00fcr mancher wolverdienten Plag'.", "tokens": ["F\u00fcr", "man\u00b7cher", "wol\u00b7ver\u00b7dien\u00b7ten", "Plag'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Ach, Herr, ich bin ja nimmer wert", "tokens": ["Ach", ",", "Herr", ",", "ich", "bin", "ja", "nim\u00b7mer", "wert"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "NN", "$,", "PPER", "VAFIN", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Des Guten, so du mir beschert,", "tokens": ["Des", "Gu\u00b7ten", ",", "so", "du", "mir", "be\u00b7schert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Und was du sonst in dieser Bahn", "tokens": ["Und", "was", "du", "sonst", "in", "die\u00b7ser", "Bahn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Des Lebens hast an mir gethan.", "tokens": ["Des", "Le\u00b7bens", "hast", "an", "mir", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Gib mir in dieser Nacht doch Ruh'", "tokens": ["Gib", "mir", "in", "die\u00b7ser", "Nacht", "doch", "Ruh'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "PDAT", "NN", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Und decke mich mit Gnaden zu,", "tokens": ["Und", "de\u00b7cke", "mich", "mit", "Gna\u00b7den", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Dein Engel bleibe stets bei mir,", "tokens": ["Dein", "En\u00b7gel", "blei\u00b7be", "stets", "bei", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Auf da\u00df mich ja kein Unfall r\u00fchr'.", "tokens": ["Auf", "da\u00df", "mich", "ja", "kein", "Un\u00b7fall", "r\u00fchr'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Es m\u00fcssen Diebe, Wasser, Feur,", "tokens": ["Es", "m\u00fcs\u00b7sen", "Die\u00b7be", ",", "Was\u00b7ser", ",", "Feur", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Gespenste, Schrecken, Ungeheur", "tokens": ["Ge\u00b7spens\u00b7te", ",", "Schre\u00b7cken", ",", "Un\u00b7ge\u00b7heur"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Samt mancher Tr\u00fcbsal, Angst und Pein", "tokens": ["Samt", "man\u00b7cher", "Tr\u00fcb\u00b7sal", ",", "Angst", "und", "Pein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PIAT", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Sehr fern, o Vater, von mir sein.", "tokens": ["Sehr", "fern", ",", "o", "Va\u00b7ter", ",", "von", "mir", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "FM", "NN", "$,", "APPR", "PPER", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Herr, sch\u00fctze mich in aller Not,", "tokens": ["Herr", ",", "sch\u00fct\u00b7ze", "mich", "in", "al\u00b7ler", "Not", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PRF", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "La\u00df einen b\u00f6sen schnellen Tod", "tokens": ["La\u00df", "ei\u00b7nen", "b\u00f6\u00b7sen", "schnel\u00b7len", "Tod"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Auch diese Nacht mich treffen nicht,", "tokens": ["Auch", "die\u00b7se", "Nacht", "mich", "tref\u00b7fen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "PPER", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "La\u00df schauen mich des Tages Licht.", "tokens": ["La\u00df", "schau\u00b7en", "mich", "des", "Ta\u00b7ges", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PRF", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Verleih', Herr, wenn die finstre Nacht", "tokens": ["Ver\u00b7leih'", ",", "Herr", ",", "wenn", "die", "finst\u00b7re", "Nacht"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKVZ", "$,", "NN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Verstrichen ist, und ich erwacht,", "tokens": ["Ver\u00b7stri\u00b7chen", "ist", ",", "und", "ich", "er\u00b7wacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "KON", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Da\u00df ich zu fr\u00fcher Morgenszeit,", "tokens": ["Da\u00df", "ich", "zu", "fr\u00fc\u00b7her", "Mor\u00b7gens\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKA", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "O gro\u00dfer Gott, dein Lob ausbreit.", "tokens": ["O", "gro\u00b7\u00dfer", "Gott", ",", "dein", "Lob", "aus\u00b7breit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Hierauf nun geh ich hin zur Ruh'", "tokens": ["Hier\u00b7auf", "nun", "geh", "ich", "hin", "zur", "Ruh'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Und schlie\u00dfe Mund und Augen zu;", "tokens": ["Und", "schlie\u00b7\u00dfe", "Mund", "und", "Au\u00b7gen", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Mein Vater, la\u00df dein Kind allein", "tokens": ["Mein", "Va\u00b7ter", ",", "la\u00df", "dein", "Kind", "al\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVIMP", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "In deinen Schutz befohlen sein!", "tokens": ["In", "dei\u00b7nen", "Schutz", "be\u00b7foh\u00b7len", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}