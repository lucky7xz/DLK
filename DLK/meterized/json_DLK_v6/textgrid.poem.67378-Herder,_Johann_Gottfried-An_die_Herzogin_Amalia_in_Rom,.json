{"textgrid.poem.67378": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "An die Herzogin Amalia in Rom,", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Purpur, den, o heldenm\u00fcth'ge F\u00fcrstin,", "tokens": ["Der", "Pur\u00b7pur", ",", "den", ",", "o", "hel\u00b7den\u00b7m\u00fcth'\u00b7ge", "F\u00fcrs\u00b7tin", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der rothe Schmuck, den Du mir g\u00fctig g\u00f6nnest,", "tokens": ["Der", "ro\u00b7the", "Schmuck", ",", "den", "Du", "mir", "g\u00fc\u00b7tig", "g\u00f6n\u00b7nest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "War wol des Wunsches werth; doch h\u00f6re, F\u00fcrstin,", "tokens": ["War", "wol", "des", "Wun\u00b7sches", "werth", ";", "doch", "h\u00f6\u00b7re", ",", "F\u00fcrs\u00b7tin", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJD", "$.", "ADV", "VVFIN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie er allein jetzt meine W\u00fcnsche reize!", "tokens": ["Wie", "er", "al\u00b7lein", "jetzt", "mei\u00b7ne", "W\u00fcn\u00b7sche", "rei\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Einst zu der R\u00f6mer, zu der Griechen Zeiten", "tokens": ["Einst", "zu", "der", "R\u00f6\u00b7mer", ",", "zu", "der", "Grie\u00b7chen", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "War er der Schmuck der Sieger und der Helden,", "tokens": ["War", "er", "der", "Schmuck", "der", "Sie\u00b7ger", "und", "der", "Hel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Lohn der Tapfern, den f\u00fcr ihre Tugend", "tokens": ["Ein", "Lohn", "der", "Tap\u00b7fern", ",", "den", "f\u00fcr", "ih\u00b7re", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die K\u00f6nigin der L\u00e4nder ihnen reichte.", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7gin", "der", "L\u00e4n\u00b7der", "ih\u00b7nen", "reich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es sank ihr Reich, der Werth auch ihres Lohnes,", "tokens": ["Es", "sank", "ihr", "Reich", ",", "der", "Werth", "auch", "ih\u00b7res", "Loh\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "ART", "NN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erhabner Werth! und es erblich ihr Purpur:", "tokens": ["Er\u00b7hab\u00b7ner", "Werth", "!", "und", "es", "er\u00b7blich", "ihr", "Pur\u00b7pur", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "KON", "PPER", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Der Schmuck der Helden, der Verdienten, Edlen,", "tokens": ["Der", "Schmuck", "der", "Hel\u00b7den", ",", "der", "Ver\u00b7dien\u00b7ten", ",", "Ed\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ward Beute des Betruges und der R\u00e4nke.", "tokens": ["Ward", "Beu\u00b7te", "des", "Be\u00b7tru\u00b7ges", "und", "der", "R\u00e4n\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}