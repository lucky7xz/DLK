{"dta.poem.6058": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Circkel-Punct\u2019 und Zonen sind nicht dort,", "tokens": ["Die", "Cir\u00b7ckel\u00b7Punct'", "und", "Zo\u00b7nen", "sind", "nicht", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VAFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie sind bey uns allein, auf unsrer Welt,", "tokens": ["Sie", "sind", "bey", "uns", "al\u00b7lein", ",", "auf", "uns\u00b7rer", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Selbst durch das Licht der Sonnen vorgestellt.", "tokens": ["Selbst", "durch", "das", "Licht", "der", "Son\u00b7nen", "vor\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es misst und theilet sich, durch sie, ein jeder Ort.", "tokens": ["Es", "misst", "und", "thei\u00b7let", "sich", ",", "durch", "sie", ",", "ein", "je\u00b7der", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PRF", "$,", "APPR", "PPER", "$,", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}