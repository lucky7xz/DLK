{"textgrid.poem.61583": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wo ist, wo ist das Gl\u00fcck zu Hause,", "tokens": ["O", "wo", "ist", ",", "wo", "ist", "das", "Gl\u00fcck", "zu", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "VAFIN", "$,", "PWAV", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df ich's endlich finden mag und greifen", "tokens": ["Da\u00df", "ich's", "end\u00b7lich", "fin\u00b7den", "mag", "und", "grei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "VVINF", "VMFIN", "KON", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und mit starker Fessel an mich binden!", "tokens": ["Und", "mit", "star\u00b7ker", "Fes\u00b7sel", "an", "mich", "bin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "O wo ist, wo ist das Gl\u00fcck zu Hause?", "tokens": ["O", "wo", "ist", ",", "wo", "ist", "das", "Gl\u00fcck", "zu", "Hau\u00b7se", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "VAFIN", "$,", "PWAV", "VAFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "\u00bbwo des Mondes Sichel schwimmt im Wasser,", "tokens": ["\u00bb", "wo", "des", "Mon\u00b7des", "Si\u00b7chel", "schwimmt", "im", "Was\u00b7ser", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ART", "NN", "NE", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo das Echo schl\u00e4ft am hohlen Felsen,", "tokens": ["Wo", "das", "E\u00b7cho", "schl\u00e4ft", "am", "hoh\u00b7len", "Fel\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wo der Fu\u00df des bunten Regenbogens", "tokens": ["Wo", "der", "Fu\u00df", "des", "bun\u00b7ten", "Re\u00b7gen\u00b7bo\u00b7gens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Auf dem Rasen steht, da geh es suchen!\u00ab", "tokens": ["Auf", "dem", "Ra\u00b7sen", "steht", ",", "da", "geh", "es", "su\u00b7chen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}