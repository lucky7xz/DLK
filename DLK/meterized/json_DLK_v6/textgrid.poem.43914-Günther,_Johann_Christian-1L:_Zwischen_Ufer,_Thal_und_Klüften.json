{"textgrid.poem.43914": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zwischen Ufer, Thal und Kl\u00fcften", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwischen Ufer, Thal und Kl\u00fcften", "tokens": ["Zwi\u00b7schen", "U\u00b7fer", ",", "Thal", "und", "Kl\u00fcf\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lies der treue Saladin", "tokens": ["Lies", "der", "treu\u00b7e", "Sa\u00b7la\u00b7din"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Mit den k\u00fchlen Abendl\u00fcften", "tokens": ["Mit", "den", "k\u00fch\u00b7len", "A\u00b7ben\u00b7dl\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tausend hei\u00dfe Seufzer fliehn,", "tokens": ["Tau\u00b7send", "hei\u00b7\u00dfe", "Seuf\u00b7zer", "fliehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil kein l\u00e4ngst gehofter Brief", "tokens": ["Weil", "kein", "l\u00e4ngst", "ge\u00b7hof\u00b7ter", "Brief"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "ADV", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Seinem Wuntsch entgegen lief.", "tokens": ["Sei\u00b7nem", "Wunt\u00b7sch", "ent\u00b7ge\u00b7gen", "lief", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Die Gewalt verliebter Schmerzen", "tokens": ["Die", "Ge\u00b7walt", "ver\u00b7lieb\u00b7ter", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Warf ihn kraftlos in das Gra\u00df,", "tokens": ["Warf", "ihn", "kraft\u00b7los", "in", "das", "Gra\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo er mit bedr\u00e4ngtem Herzen", "tokens": ["Wo", "er", "mit", "be\u00b7dr\u00e4ng\u00b7tem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und gest\u00fcztem Arme sa\u00df;", "tokens": ["Und", "ge\u00b7st\u00fcz\u00b7tem", "Ar\u00b7me", "sa\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Endlich wollte seiner Pein", "tokens": ["End\u00b7lich", "woll\u00b7te", "sei\u00b7ner", "Pein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Brust und Herz zu enge seyn.", "tokens": ["Brust", "und", "Herz", "zu", "en\u00b7ge", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "ADJA", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Zeuch nur, sang er, sch\u00f6ne Gegend,", "tokens": ["Zeuch", "nur", ",", "sang", "er", ",", "sch\u00f6\u00b7ne", "Ge\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deiner Triften Reizung ein!", "tokens": ["Dei\u00b7ner", "Trif\u00b7ten", "Rei\u00b7zung", "ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jezo bistu nicht verm\u00f6gend,", "tokens": ["Je\u00b7zo", "bis\u00b7tu", "nicht", "ver\u00b7m\u00f6\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mein Gem\u00fcthe zu erfreun,", "tokens": ["Mein", "Ge\u00b7m\u00fc\u00b7the", "zu", "er\u00b7freun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "De\u00dfen Schwermuth diesen Flu\u00df", "tokens": ["De\u00b7\u00dfen", "Schwer\u00b7muth", "die\u00b7sen", "Flu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit den Thr\u00e4nen st\u00e4rcken mu\u00df.", "tokens": ["Mit", "den", "Thr\u00e4\u00b7nen", "st\u00e4r\u00b7cken", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Mit den Thr\u00e4nen, die mein Leiden", "tokens": ["Mit", "den", "Thr\u00e4\u00b7nen", ",", "die", "mein", "Lei\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Billig Scheidewa\u00dfer nennt,", "tokens": ["Bil\u00b7lig", "Schei\u00b7de\u00b7wa\u00b7\u00dfer", "nennt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil es nach dem schweren Scheiden", "tokens": ["Weil", "es", "nach", "dem", "schwe\u00b7ren", "Schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Augen, Haut und Seele brennt", "tokens": ["Au\u00b7gen", ",", "Haut", "und", "See\u00b7le", "brennt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die Lippen schmerzlich fri\u00dft,", "tokens": ["Und", "die", "Lip\u00b7pen", "schmerz\u00b7lich", "fri\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die der Abschied roh gek\u00fcst.", "tokens": ["Die", "der", "Ab\u00b7schied", "roh", "ge\u00b7k\u00fcst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Leonore, las dich finden!", "tokens": ["Le\u00b7o\u00b7no\u00b7re", ",", "las", "dich", "fin\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Irr ich oder rufst du nach?", "tokens": ["Irr", "ich", "o\u00b7der", "rufst", "du", "nach", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sagt, ihr schattenreichen Linden,", "tokens": ["Sagt", ",", "ihr", "schat\u00b7ten\u00b7rei\u00b7chen", "Lin\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "ADJA", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sage, du verschwiegner Bach,", "tokens": ["Sa\u00b7ge", ",", "du", "ver\u00b7schwieg\u00b7ner", "Bach", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ob nicht die, so mich regiert,", "tokens": ["Ob", "nicht", "die", ",", "so", "mich", "re\u00b7giert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "$,", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dann und wann hierum spaziert.", "tokens": ["Dann", "und", "wann", "hie\u00b7rum", "spa\u00b7ziert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "PWAV", "PAV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "W\u00fcrd ich auf den Blumenbeeten", "tokens": ["W\u00fcrd", "ich", "auf", "den", "Blu\u00b7men\u00b7bee\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jezo doch nur so begl\u00fcckt,", "tokens": ["Je\u00b7zo", "doch", "nur", "so", "be\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Nur in eine Spur zu treten,", "tokens": ["Nur", "in", "ei\u00b7ne", "Spur", "zu", "tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die ihr netter Fu\u00df gedr\u00fcckt,", "tokens": ["Die", "ihr", "net\u00b7ter", "Fu\u00df", "ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sollt es, scheint der Trost gleich klein,", "tokens": ["Sollt", "es", ",", "scheint", "der", "Trost", "gleich", "klein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mir doch Trost genug verleihn.", "tokens": ["Mir", "doch", "Trost", "ge\u00b7nug", "ver\u00b7leihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Seht, so schw\u00e4rmt mein blind Verlangen,", "tokens": ["Seht", ",", "so", "schw\u00e4rmt", "mein", "blind", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "VVFIN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das mich hin und wider rei\u00dft;", "tokens": ["Das", "mich", "hin", "und", "wi\u00b7der", "rei\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PTKVZ", "KON", "APPR", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der durch Leonorens Prangen", "tokens": ["Der", "durch", "Le\u00b7o\u00b7no\u00b7rens", "Pran\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Z\u00e4rtlich eingenommne Geist", "tokens": ["Z\u00e4rt\u00b7lich", "ein\u00b7ge\u00b7nomm\u00b7ne", "Geist"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bildet sich um jeden Hayn", "tokens": ["Bil\u00b7det", "sich", "um", "je\u00b7den", "Hayn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Seiner Sch\u00f6nen Ruhplaz ein.", "tokens": ["Sei\u00b7ner", "Sch\u00f6\u00b7nen", "Ruh\u00b7plaz", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Nein, mein Geist, du irrst im Bilde,", "tokens": ["Nein", ",", "mein", "Geist", ",", "du", "irrst", "im", "Bil\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "$,", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieh den Ort genauer an:", "tokens": ["Sieh", "den", "Ort", "ge\u00b7nau\u00b7er", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Diese Tiefen, dies Gefilde", "tokens": ["Die\u00b7se", "Tie\u00b7fen", ",", "dies", "Ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PDAT", "NN", "$,", "PDS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist kein schlesisch Canaan,", "tokens": ["Ist", "kein", "schle\u00b7sisch", "Ca\u00b7naan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJD", "NE", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Und zum Paradies allhier", "tokens": ["Und", "zum", "Pa\u00b7ra\u00b7dies", "all\u00b7hier"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mangelt nichts als Lorchens Zier.", "tokens": ["Man\u00b7gelt", "nichts", "als", "Lor\u00b7chens", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "KOKOM", "NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Mahle dir die Lust der Erden,", "tokens": ["Mah\u00b7le", "dir", "die", "Lust", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Adams ersten Aufenthalt", "tokens": ["A\u00b7dams", "ers\u00b7ten", "Auf\u00b7ent\u00b7halt"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nebst den Mienen und Gebehrden", "tokens": ["Nebst", "den", "Mie\u00b7nen", "und", "Ge\u00b7behr\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seiner kl\u00e4glichen Gestalt", "tokens": ["Sei\u00b7ner", "kl\u00e4g\u00b7li\u00b7chen", "Ge\u00b7stalt"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und betrachte, wenn er flieht,", "tokens": ["Und", "be\u00b7trach\u00b7te", ",", "wenn", "er", "flieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie betr\u00fcbt er r\u00fcckw\u00e4rts sieht.", "tokens": ["Wie", "be\u00b7tr\u00fcbt", "er", "r\u00fcck\u00b7w\u00e4rts", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Siehstu dies, so sieh darinnen", "tokens": ["Sieh\u00b7stu", "dies", ",", "so", "sieh", "da\u00b7rin\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PDS", "$,", "ADV", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich und meinen Zustand an,", "tokens": ["Mich", "und", "mei\u00b7nen", "Zu\u00b7stand", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "De\u00dfen Qual kein k\u00fcnstlich Sinnen", "tokens": ["De\u00b7\u00dfen", "Qual", "kein", "k\u00fcnst\u00b7lich", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "PIAT", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und kein Kiel entwerfen kan;", "tokens": ["Und", "kein", "Kiel", "ent\u00b7wer\u00b7fen", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Evens Anmuth bl\u00fcht in dir,", "tokens": ["E\u00b7vens", "An\u00b7muth", "bl\u00fcht", "in", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Adams Unruh folget mir.", "tokens": ["A\u00b7dams", "Un\u00b7ruh", "fol\u00b7get", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Da ich mich entfernt betr\u00fcbe,", "tokens": ["Da", "ich", "mich", "ent\u00b7fernt", "be\u00b7tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lernt mein Geist erst mit Verdru\u00df,", "tokens": ["Lernt", "mein", "Geist", "erst", "mit", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie getreu und starck er liebe,", "tokens": ["Wie", "ge\u00b7treu", "und", "starck", "er", "lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil er dich verla\u00dfen mu\u00df,", "tokens": ["Weil", "er", "dich", "ver\u00b7la\u00b7\u00dfen", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die Angst entri\u00dfner Brust", "tokens": ["Und", "die", "Angst", "ent\u00b7ri\u00df\u00b7ner", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Lehrt den Werth vergangner Lust.", "tokens": ["Lehrt", "den", "Werth", "ver\u00b7gang\u00b7ner", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Von den Spizen derer H\u00fcgel", "tokens": ["Von", "den", "Spi\u00b7zen", "de\u00b7rer", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PDS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seh ich oft ins Vaterland;", "tokens": ["Seh", "ich", "oft", "ins", "Va\u00b7ter\u00b7land", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00e4tt ich doch nur Taubenfl\u00fcgel", "tokens": ["H\u00e4tt", "ich", "doch", "nur", "Tau\u00b7ben\u00b7fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder Daedals Zauberhand,", "tokens": ["O\u00b7der", "Dae\u00b7dals", "Zau\u00b7ber\u00b7hand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Um nur, wie zuvor geschehn,", "tokens": ["Um", "nur", ",", "wie", "zu\u00b7vor", "ge\u00b7schehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "$,", "PWAV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dich, mein Engel, noch zu sehn.", "tokens": ["Dich", ",", "mein", "En\u00b7gel", ",", "noch", "zu", "sehn", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Dich, o Sonne meines Lebens,", "tokens": ["Dich", ",", "o", "Son\u00b7ne", "mei\u00b7nes", "Le\u00b7bens", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "FM", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dich, o Ursprung meiner Glut!", "tokens": ["Dich", ",", "o", "Ur\u00b7sprung", "mei\u00b7ner", "Glut", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "FM", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist's denn leider ganz vergebens,", "tokens": ["Ist's", "denn", "lei\u00b7der", "ganz", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADV", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df mein Mund so kl\u00e4glich thut?", "tokens": ["Da\u00df", "mein", "Mund", "so", "kl\u00e4g\u00b7lich", "thut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nein, ich weis, dein klingend Ohr", "tokens": ["Nein", ",", "ich", "weis", ",", "dein", "klin\u00b7gend", "Ohr"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "PTKVZ", "$,", "PPOSAT", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Stellt dir oft mein Leiden vor.", "tokens": ["Stellt", "dir", "oft", "mein", "Lei\u00b7den", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Ist der Tag der Erd entwichen,", "tokens": ["Ist", "der", "Tag", "der", "Erd", "ent\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So verwehrt dein Bild die Ruh;", "tokens": ["So", "ver\u00b7wehrt", "dein", "Bild", "die", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kommt ein Ostwind hergestrichen;", "tokens": ["Kommt", "ein", "Ost\u00b7wind", "her\u00b7ge\u00b7stri\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kehr ich ihm das Antliz zu;", "tokens": ["Kehr", "ich", "ihm", "das", "Ant\u00b7liz", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn mich deucht, er bringe mir", "tokens": ["Denn", "mich", "deucht", ",", "er", "brin\u00b7ge", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Manchen sanften Ku\u00df von dir.", "tokens": ["Man\u00b7chen", "sanf\u00b7ten", "Ku\u00df", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Wo ich size, steh und liege,", "tokens": ["Wo", "ich", "si\u00b7ze", ",", "steh", "und", "lie\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "St