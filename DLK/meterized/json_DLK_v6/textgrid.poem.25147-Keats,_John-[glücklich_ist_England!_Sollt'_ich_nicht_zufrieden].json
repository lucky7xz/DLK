{"textgrid.poem.25147": {"metadata": {"author": {"name": "Keats, John", "birth": "N.A.", "death": "N.A."}, "title": "[gl\u00fccklich ist England! Sollt' ich nicht zufrieden]", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fccklich ist England! Sollt' ich nicht zufrieden", "tokens": ["Gl\u00fcck\u00b7lich", "ist", "En\u00b7gland", "!", "Sollt'", "ich", "nicht", "zu\u00b7frie\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "NE", "$.", "VMFIN", "PPER", "PTKNEG", "ADJD"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Bei seiner Wiesen frischem Gr\u00fcn verweilen,", "tokens": ["Bei", "sei\u00b7ner", "Wie\u00b7sen", "fri\u00b7schem", "Gr\u00fcn", "ver\u00b7wei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein Leid in seinen hohen W\u00e4ldern heilen,", "tokens": ["Mein", "Leid", "in", "sei\u00b7nen", "ho\u00b7hen", "W\u00e4l\u00b7dern", "hei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die gr\u00fcne Panzer um die Seele schmieden?", "tokens": ["Die", "gr\u00fc\u00b7ne", "Pan\u00b7zer", "um", "die", "See\u00b7le", "schmie\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch manchmal tr\u00e4umt mein Herz vom blauen Frieden", "tokens": ["Doch", "manch\u00b7mal", "tr\u00e4umt", "mein", "Herz", "vom", "blau\u00b7en", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Italischer Himmel und verlangt bisweilen", "tokens": ["I\u00b7ta\u00b7li\u00b7scher", "Him\u00b7mel", "und", "ver\u00b7langt", "bis\u00b7wei\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "KON", "VVFIN", "ADV"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nach erdenfernen kahlen Felsensteilen,", "tokens": ["Nach", "er\u00b7den\u00b7fer\u00b7nen", "kah\u00b7len", "Fel\u00b7sen\u00b7stei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Nach einem Thron auf Alpenpyramiden.", "tokens": ["Nach", "ei\u00b7nem", "Thron", "auf", "Al\u00b7pen\u00b7py\u00b7ra\u00b7mi\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Gl\u00fccklich ist England! Seine T\u00f6chter haben", "tokens": ["Gl\u00fcck\u00b7lich", "ist", "En\u00b7gland", "!", "Sei\u00b7ne", "T\u00f6ch\u00b7ter", "ha\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "NE", "$.", "PPOSAT", "NN", "VAFIN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ein arglos Herz und schlichte Lieblichkeit.", "tokens": ["Ein", "arg\u00b7los", "Herz", "und", "schlich\u00b7te", "Lieb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gen\u00fcgen sollten mir so sch\u00f6ne Gaben!", "tokens": ["Ge\u00b7n\u00fc\u00b7gen", "soll\u00b7ten", "mir", "so", "sch\u00f6\u00b7ne", "Ga\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch oft erfa\u00dft mich tiefe Bangigkeit", "tokens": ["Doch", "oft", "er\u00b7fa\u00dft", "mich", "tie\u00b7fe", "Ban\u00b7gig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nach hei\u00dfern Frauen, deren s\u00fc\u00dfe Stimmen", "tokens": ["Nach", "hei\u00b7\u00dfern", "Frau\u00b7en", ",", "de\u00b7ren", "s\u00fc\u00b7\u00dfe", "Stim\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hell neben mir auf Sommerwassern schwimmen.", "tokens": ["Hell", "ne\u00b7ben", "mir", "auf", "Som\u00b7mer\u00b7was\u00b7sern", "schwim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}