{"textgrid.poem.26357": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Der Baum am Erdensaum", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft mein Geist im Leben klagte,", "tokens": ["Oft", "mein", "Geist", "im", "Le\u00b7ben", "klag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn kein Licht im Herzen tagte,", "tokens": ["Wenn", "kein", "Licht", "im", "Her\u00b7zen", "tag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und er nicht zu lachen wagte,", "tokens": ["Und", "er", "nicht", "zu", "la\u00b7chen", "wag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und er fragte: Wozu dieses stete Streben,", "tokens": ["Und", "er", "frag\u00b7te", ":", "Wo\u00b7zu", "die\u00b7ses", "ste\u00b7te", "Stre\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "PWAV", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Wozu dieser Tage Traum,", "tokens": ["Wo\u00b7zu", "die\u00b7ser", "Ta\u00b7ge", "Traum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wozu alles Lebens Schaum?", "tokens": ["Wo\u00b7zu", "al\u00b7les", "Le\u00b7bens", "Schaum", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gehe zu dem Baum, sagte endlich eine Stimme in dem Baum,", "tokens": ["Ge\u00b7he", "zu", "dem", "Baum", ",", "sag\u00b7te", "end\u00b7lich", "ei\u00b7ne", "Stim\u00b7me", "in", "dem", "Baum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,", "VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Zu dem Baum, der da steht am Erdensaum,", "tokens": ["Zu", "dem", "Baum", ",", "der", "da", "steht", "am", "Er\u00b7den\u00b7saum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und aus dem die Weisheit weht.", "tokens": ["Und", "aus", "dem", "die", "Weis\u00b7heit", "weht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Und ich lie\u00df die Heimat, lie\u00df mein Weib, mein Haus,", "tokens": ["Und", "ich", "lie\u00df", "die", "Hei\u00b7mat", ",", "lie\u00df", "mein", "Weib", ",", "mein", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Und ich zog der Stimme folgend", "tokens": ["Und", "ich", "zog", "der", "Stim\u00b7me", "fol\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00dcber Meere aus.", "tokens": ["\u00dc\u00b7ber", "Mee\u00b7re", "aus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Hinter mir indes kam die Welt in Brand.", "tokens": ["Hin\u00b7ter", "mir", "in\u00b7des", "kam", "die", "Welt", "in", "Brand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Jeder Weg im Feuerschein aufstand,", "tokens": ["Je\u00b7der", "Weg", "im", "Feu\u00b7er\u00b7schein", "auf\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und auf jedem Wege sich die Flamme wand.", "tokens": ["Und", "auf", "je\u00b7dem", "We\u00b7ge", "sich", "die", "Flam\u00b7me", "wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Kein Weg lie\u00df mich wieder in mein Land.", "tokens": ["Kein", "Weg", "lie\u00df", "mich", "wie\u00b7der", "in", "mein", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Doch am langen Weg nirgends jenen Baum ich fand,", "tokens": ["Doch", "am", "lan\u00b7gen", "Weg", "nir\u00b7gends", "je\u00b7nen", "Baum", "ich", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "ADV", "PDAT", "NN", "PPER", "VVFIN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Der am Berge steht und aus dem die Weisheit weht.", "tokens": ["Der", "am", "Ber\u00b7ge", "steht", "und", "aus", "dem", "die", "Weis\u00b7heit", "weht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVFIN", "KON", "APPR", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Suchte ihn am Erdenrand, suchte ab den ganzen Erdenraum", "tokens": ["Such\u00b7te", "ihn", "am", "Er\u00b7den\u00b7rand", ",", "such\u00b7te", "ab", "den", "gan\u00b7zen", "Er\u00b7den\u00b7raum"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.4": {"text": "Nach dem Baum.", "tokens": ["Nach", "dem", "Baum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Und ich fluchte dem Geschick, fluchte jeden Tag dem Brand,", "tokens": ["Und", "ich", "fluch\u00b7te", "dem", "Ge\u00b7schick", ",", "fluch\u00b7te", "je\u00b7den", "Tag", "dem", "Brand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "PIAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-++-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.2": {"text": "Der mir wehrte heimzukehren, der mit roter Flammenhand", "tokens": ["Der", "mir", "wehr\u00b7te", "heim\u00b7zu\u00b7keh\u00b7ren", ",", "der", "mit", "ro\u00b7ter", "Flam\u00b7men\u00b7hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "VVINF", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Trocken alle Meere kehrte.", "tokens": ["Tro\u00b7cken", "al\u00b7le", "Mee\u00b7re", "kehr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00fcsten wurden alle frische Meere.", "tokens": ["W\u00fcs\u00b7ten", "wur\u00b7den", "al\u00b7le", "fri\u00b7sche", "Mee\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VAFIN", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und ich stand im Sand und in toter Leere.", "tokens": ["Und", "ich", "stand", "im", "Sand", "und", "in", "to\u00b7ter", "Lee\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "KON", "APPR", "ADJA", "NN", "$."], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "M\u00fcde legte ich mich nieder auf den n\u00e4chsten Berg,", "tokens": ["M\u00fc\u00b7de", "leg\u00b7te", "ich", "mich", "nie\u00b7der", "auf", "den", "n\u00e4chs\u00b7ten", "Berg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Wo kein Atemzug sich regte.", "tokens": ["Wo", "kein", "A\u00b7tem\u00b7zug", "sich", "reg\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lange lag ich auf dem Stein", "tokens": ["Lan\u00b7ge", "lag", "ich", "auf", "dem", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Totenstill und ganz allein.", "tokens": ["To\u00b7tens\u00b7till", "und", "ganz", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Sagte mir: will mich niemals mehr von hier erheben,", "tokens": ["Sag\u00b7te", "mir", ":", "will", "mich", "nie\u00b7mals", "mehr", "von", "hier", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "VMFIN", "PRF", "ADV", "ADV", "APPR", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Will entsagen allem Leben.", "tokens": ["Will", "ent\u00b7sa\u00b7gen", "al\u00b7lem", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "PIS", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Und mein Geist zum Geiste klagte:", "tokens": ["Und", "mein", "Geist", "zum", "Geis\u00b7te", "klag\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will hier liegen, bis mein inneres Auge sich gelichtet.", "tokens": ["Will", "hier", "lie\u00b7gen", ",", "bis", "mein", "in\u00b7ne\u00b7res", "Au\u00b7ge", "sich", "ge\u00b7lich\u00b7tet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVFIN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Bis sich jener Baum aufrichtet", "tokens": ["Bis", "sich", "je\u00b7ner", "Baum", "auf\u00b7rich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRF", "PDAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mein Blick die Weisheit sichtet.", "tokens": ["Und", "mein", "Blick", "die", "Weis\u00b7heit", "sich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Und ein Regen fiel auf meinen Leib,", "tokens": ["Und", "ein", "Re\u00b7gen", "fiel", "auf", "mei\u00b7nen", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Und der Sturm erbrauste auf den Wegen,", "tokens": ["Und", "der", "Sturm", "er\u00b7braus\u00b7te", "auf", "den", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und der Feuerwurm der Blitze sauste unter hellem Fegen", "tokens": ["Und", "der", "Feu\u00b7er\u00b7wurm", "der", "Blit\u00b7ze", "saus\u00b7te", "un\u00b7ter", "hel\u00b7lem", "Fe\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Mir in meines Auges halbgeschlo\u00dfne Ritze.", "tokens": ["Mir", "in", "mei\u00b7nes", "Au\u00b7ges", "halb\u00b7ge\u00b7schlo\u00df\u00b7ne", "Rit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Sehend ward ich durch des Feuers Hitze.", "tokens": ["Se\u00b7hend", "ward", "ich", "durch", "des", "Feu\u00b7ers", "Hit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wei\u00df nicht mehr, wie lang' ich dort gelegen", "tokens": ["Wei\u00df", "nicht", "mehr", ",", "wie", "lang'", "ich", "dort", "ge\u00b7le\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ADV", "$,", "PWAV", "ADV", "PPER", "ADV", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Auf der harten Bergesspitze.", "tokens": ["Auf", "der", "har\u00b7ten", "Ber\u00b7ges\u00b7spit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wolken flogen rund im Kreis,", "tokens": ["Wol\u00b7ken", "flo\u00b7gen", "rund", "im", "Kreis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Wolken, die mich durch das Weltall zogen.", "tokens": ["Wol\u00b7ken", ",", "die", "mich", "durch", "das", "Wel\u00b7tall", "zo\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Meinem Leibe wurde kalt und hei\u00df.", "tokens": ["Mei\u00b7nem", "Lei\u00b7be", "wur\u00b7de", "kalt", "und", "hei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Sah die Erde unter mir im Bogen kaum,", "tokens": ["Sah", "die", "Er\u00b7de", "un\u00b7ter", "mir", "im", "Bo\u00b7gen", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPER", "APPRART", "NN", "ADV", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.12": {"text": "Und zu Geist ward ich im Raum.", "tokens": ["Und", "zu", "Geist", "ward", "ich", "im", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Aber wo mein Herz am Berg gelegen,", "tokens": ["A\u00b7ber", "wo", "mein", "Herz", "am", "Berg", "ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Stand mit reifer Krone gro\u00df ein Baum,", "tokens": ["Stand", "mit", "rei\u00b7fer", "Kro\u00b7ne", "gro\u00df", "ein", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Gr\u00f6\u00dfer als die Zeit,", "tokens": ["Gr\u00f6\u00b7\u00df