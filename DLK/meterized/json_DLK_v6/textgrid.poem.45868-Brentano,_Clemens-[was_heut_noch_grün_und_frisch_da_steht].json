{"textgrid.poem.45868": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[was heut noch gr\u00fcn und frisch da steht]", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was heut noch gr\u00fcn und frisch da steht,", "tokens": ["Was", "heut", "noch", "gr\u00fcn", "und", "frisch", "da", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "ADJD", "KON", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird morgen schon hinweggem\u00e4ht,", "tokens": ["Wird", "mor\u00b7gen", "schon", "hin\u00b7weg\u00b7ge\u00b7m\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die edlen Narzissen,", "tokens": ["Die", "ed\u00b7len", "Nar\u00b7zis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Zierden der Wiesen,", "tokens": ["Die", "Zier\u00b7den", "der", "Wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Die sch\u00f6n Hiazinthen,", "tokens": ["Die", "sch\u00f6n", "Hi\u00b7a\u00b7zin\u00b7then", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Die t\u00fcrkischen Binden.", "tokens": ["Die", "t\u00fcr\u00b7ki\u00b7schen", "Bin\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "H\u00fcte dich sch\u00f6ns Bl\u00fcmelein!", "tokens": ["H\u00fc\u00b7te", "dich", "sch\u00f6ns", "Bl\u00fc\u00b7me\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Viel hunderttausend ungez\u00e4hlt,", "tokens": ["Viel", "hun\u00b7dert\u00b7tau\u00b7send", "un\u00b7ge\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was nur unter die Sichel f\u00e4llt,", "tokens": ["Was", "nur", "un\u00b7ter", "die", "Si\u00b7chel", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ihr Rosen, ihr Lilien!", "tokens": ["Ihr", "Ro\u00b7sen", ",", "ihr", "Li\u00b7li\u00b7en", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Euch wird man austilgen,", "tokens": ["Euch", "wird", "man", "aus\u00b7til\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "VVIZU", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Auch die Kaiserkronen", "tokens": ["Auch", "die", "Kai\u00b7ser\u00b7kro\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Wird man nicht verschonen,", "tokens": ["Wird", "man", "nicht", "ver\u00b7scho\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "H\u00fcte dich sch\u00f6ns Bl\u00fcmelein! \u2013", "tokens": ["H\u00fc\u00b7te", "dich", "sch\u00f6ns", "Bl\u00fc\u00b7me\u00b7lein", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Das himmelfarbne Ehrenpreis,", "tokens": ["Das", "him\u00b7mel\u00b7farb\u00b7ne", "Eh\u00b7ren\u00b7preis", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Tulipane gelb und wei\u00df,", "tokens": ["Die", "Tu\u00b7li\u00b7pa\u00b7ne", "gelb", "und", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die silbernen Glocken,", "tokens": ["Die", "sil\u00b7ber\u00b7nen", "Glo\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die goldnen Flocken,", "tokens": ["Die", "gold\u00b7nen", "Flo\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Sinkt alles zur Erden,", "tokens": ["Sinkt", "al\u00b7les", "zur", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Was wird daraus werden?", "tokens": ["Was", "wird", "da\u00b7raus", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PAV", "VAINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "H\u00fcte dich sch\u00f6ns Bl\u00fcmelein!", "tokens": ["H\u00fc\u00b7te", "dich", "sch\u00f6ns", "Bl\u00fc\u00b7me\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ihr h\u00fcbsch Lavendel, Rosmarin,", "tokens": ["Ihr", "h\u00fcbsch", "La\u00b7ven\u00b7del", ",", "Ros\u00b7ma\u00b7rin", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADJD", "NE", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr vielfarbige R\u00f6selin,", "tokens": ["Ihr", "viel\u00b7far\u00b7bi\u00b7ge", "R\u00f6\u00b7se\u00b7lin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ihr stolze Schwerdlilgen,", "tokens": ["Ihr", "stol\u00b7ze", "Schwerd\u00b7lil\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Ihr krause Basilgen,", "tokens": ["Ihr", "krau\u00b7se", "Ba\u00b7sil\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ihr zarte Violen,", "tokens": ["Ihr", "zar\u00b7te", "Vi\u00b7o\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Euch wird man bald holen. \u2013", "tokens": ["Euch", "wird", "man", "bald", "ho\u00b7len", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PIS", "ADV", "VVINF", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "H\u00fcte dich sch\u00f6ns Bl\u00fcmelein! \u2013", "tokens": ["H\u00fc\u00b7te", "dich", "sch\u00f6ns", "Bl\u00fc\u00b7me\u00b7lein", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}