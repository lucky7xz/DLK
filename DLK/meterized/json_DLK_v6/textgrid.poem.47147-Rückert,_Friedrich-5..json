{"textgrid.poem.47147": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seltsam! aber wahr empfunden", "tokens": ["Selt\u00b7sam", "!", "a\u00b7ber", "wahr", "emp\u00b7fun\u00b7den"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hab' ich es in meiner Brust:", "tokens": ["Hab'", "ich", "es", "in", "mei\u00b7ner", "Brust", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Leichter als in tr\u00fcben Stunden", "tokens": ["Leich\u00b7ter", "als", "in", "tr\u00fc\u00b7ben", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stirbt es sich in froher Lust.", "tokens": ["Stirbt", "es", "sich", "in", "fro\u00b7her", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn im Ungl\u00fcck mu\u00dft du hoffen,", "tokens": ["Denn", "im", "Un\u00b7gl\u00fcck", "mu\u00dft", "du", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df dein Gl\u00fcck dir komme doch;", "tokens": ["Da\u00df", "dein", "Gl\u00fcck", "dir", "kom\u00b7me", "doch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber ist es eingetroffen,", "tokens": ["A\u00b7ber", "ist", "es", "ein\u00b7ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Worauf hoffen willst du noch?", "tokens": ["Wo\u00b7rauf", "hof\u00b7fen", "willst", "du", "noch", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVINF", "VMFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ietzo kann's das Leben denken", "tokens": ["Iet\u00b7zo", "kann's", "das", "Le\u00b7ben", "den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ohne Schauder vor dem Tod,", "tokens": ["Oh\u00b7ne", "Schau\u00b7der", "vor", "dem", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie die Sonne sich zu senken", "tokens": ["Wie", "die", "Son\u00b7ne", "sich", "zu", "sen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PRF", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In ein Liebesabendrot:", "tokens": ["In", "ein", "Lie\u00b7be\u00b7sa\u00b7ben\u00b7drot", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie die Augen froh begn\u00fcget", "tokens": ["Wie", "die", "Au\u00b7gen", "froh", "be\u00b7gn\u00fc\u00b7get"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlie\u00dft der Greis von Kanaan,", "tokens": ["Schlie\u00dft", "der", "Greis", "von", "Ka\u00b7naan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Als der Himmel es gef\u00fcget,", "tokens": ["Als", "der", "Him\u00b7mel", "es", "ge\u00b7f\u00fc\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sie Joseph wiedersahn.", "tokens": ["Da\u00df", "sie", "Jo\u00b7se\u00b7ph", "wie\u00b7der\u00b7sahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}