{"dta.poem.18442": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf eine verliebte Kauffmanns-  \n Frau.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Courtesilis kriegt einen jungen sohn/", "tokens": ["Cour\u00b7te\u00b7si\u00b7lis", "kriegt", "ei\u00b7nen", "jun\u00b7gen", "sohn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Da doch ihr mann vor mehr als viertzig wochen schon", "tokens": ["Da", "doch", "ihr", "mann", "vor", "mehr", "als", "viert\u00b7zig", "wo\u00b7chen", "schon"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "APPR", "PIAT", "KOKOM", "CARD", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In Holland ist verreist.", "tokens": ["In", "Hol\u00b7land", "ist", "ver\u00b7reist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich wei\u00df nicht was die losen leute sagen/", "tokens": ["Ich", "wei\u00df", "nicht", "was", "die", "lo\u00b7sen", "leu\u00b7te", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWS", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es h\u00e4tte sich itzt anders ausgeweist/", "tokens": ["Es", "h\u00e4t\u00b7te", "sich", "itzt", "an\u00b7ders", "aus\u00b7ge\u00b7weist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Als was zuletzt der gute Mann befahl/", "tokens": ["Als", "was", "zu\u00b7letzt", "der", "gu\u00b7te", "Mann", "be\u00b7fahl", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da\u00df sie in sein Journal", "tokens": ["Da\u00df", "sie", "in", "sein", "Jour\u00b7nal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Von frembder post nichts solte lassen tragen.", "tokens": ["Von", "fremb\u00b7der", "post", "nichts", "sol\u00b7te", "las\u00b7sen", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PIS", "VMFIN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Gewi\u00df sie thun der lieben frau zu viel;", "tokens": ["Ge\u00b7wi\u00df", "sie", "thun", "der", "lie\u00b7ben", "frau", "zu", "viel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "PIS", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Denn niemand hat die Kauffmanns-art bedacht:", "tokens": ["Denn", "nie\u00b7mand", "hat", "die", "Kauff\u00b7manns\u00b7art", "be\u00b7dacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Das kind ist ihr durch wechsel \u00fcbermacht.", "tokens": ["Das", "kind", "ist", "ihr", "durch", "wech\u00b7sel", "\u00fc\u00b7ber\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}