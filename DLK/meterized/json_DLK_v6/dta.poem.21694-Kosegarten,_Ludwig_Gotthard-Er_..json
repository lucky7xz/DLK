{"dta.poem.21694": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Er .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Falsch oder wahr . . . So viel ist klar,", "tokens": ["Falsch", "o\u00b7der", "wahr", ".", ".", ".", "So", "viel", "ist", "klar", ","], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$.", "$.", "$.", "ADV", "ADV", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dass alle Welt itzt spricht:", "tokens": ["Dass", "al\u00b7le", "Welt", "itzt", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aprillendunst, Sey Frauengunst", "tokens": ["Ap\u00b7ril\u00b7len\u00b7dunst", ",", "Sey", "Frau\u00b7en\u00b7gunst"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADV", "$,", "VAFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wandelnd Mondenlicht!", "tokens": ["Und", "wan\u00b7delnd", "Mon\u00b7den\u00b7licht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVPP", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wirb treu und wahr, Wirb Tag und Jahr", "tokens": ["Wirb", "treu", "und", "wahr", ",", "Wirb", "Tag", "und", "Jahr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "ADJD", "KON", "ADJD", "$,", "VVIMP", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Um ihre Huld und Treu.", "tokens": ["Um", "ih\u00b7re", "Huld", "und", "Treu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wohl kos't ihr Mund: Des Herzens Grund", "tokens": ["Wohl", "kos't", "ihr", "Mund", ":", "Des", "Her\u00b7zens", "Grund"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$.", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ist dennoch nicht dabey.", "tokens": ["Ist", "den\u00b7noch", "nicht", "da\u00b7bey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Ein Sch\u00f6n'rer traun! Ist kaum zu schaun,", "tokens": ["Ein", "Sch\u00f6n'\u00b7rer", "traun", "!", "Ist", "kaum", "zu", "schaun", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$.", "VAFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Spricht kaum sie freundlich an \u2014", "tokens": ["Spricht", "kaum", "sie", "freund\u00b7lich", "an"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Hin bist du, hin Aus ihrem Sinn,", "tokens": ["Hin", "bist", "du", ",", "hin", "Aus", "ih\u00b7rem", "Sinn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Bist ein gebannter Mann!", "tokens": ["Bist", "ein", "ge\u00b7bann\u00b7ter", "Mann", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}