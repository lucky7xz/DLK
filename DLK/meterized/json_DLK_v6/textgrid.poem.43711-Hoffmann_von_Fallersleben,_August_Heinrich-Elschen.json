{"textgrid.poem.43711": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Elschen", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seht mir mal mein Elschen an,", "tokens": ["Seht", "mir", "mal", "mein", "El\u00b7schen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie mein Elschen schaukeln kann", "tokens": ["Wie", "mein", "El\u00b7schen", "schau\u00b7keln", "kann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "VVINF", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Hangematte!", "tokens": ["In", "der", "Han\u00b7ge\u00b7mat\u00b7te", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Elschen fern und Elschen nah,", "tokens": ["El\u00b7schen", "fern", "und", "El\u00b7schen", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "KON", "NE", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Elschen hier und Elschen da \u2013", "tokens": ["El\u00b7schen", "hier", "und", "El\u00b7schen", "da", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "KON", "NE", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kuckuck.", "tokens": ["Ku\u00b7ckuck", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Und es rauscht das Laub im Busch,", "tokens": ["Und", "es", "rauscht", "das", "Laub", "im", "Busch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Und das Sp\u00e4tzlein fliegt husch husch", "tokens": ["Und", "das", "Sp\u00e4tz\u00b7lein", "fliegt", "husch", "husch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Um die Hangematte.", "tokens": ["Um", "die", "Han\u00b7ge\u00b7mat\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Und es kommt der Sonnenschein,", "tokens": ["Und", "es", "kommt", "der", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Flimmert durch die Zweigelein", "tokens": ["Flim\u00b7mert", "durch", "die", "Zwei\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In die Hangematte.", "tokens": ["In", "die", "Han\u00b7ge\u00b7mat\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Seht mir mal mein Elschen an,", "tokens": ["Seht", "mir", "mal", "mein", "El\u00b7schen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie mein Elschen schaukeln kann", "tokens": ["Wie", "mein", "El\u0