{"dta.poem.7558": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "57.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Doch keine Fratze gibts, die nicht als Sch\u00f6nheit preist", "tokens": ["Doch", "kei\u00b7ne", "Frat\u00b7ze", "gibts", ",", "die", "nicht", "als", "Sch\u00f6n\u00b7heit", "preist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$,", "PRELS", "PTKNEG", "KOUS", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hier ein verliebter Narr, dort ein verschrobner Geist.", "tokens": ["Hier", "ein", "ver\u00b7lieb\u00b7ter", "Narr", ",", "dort", "ein", "ver\u00b7schrob\u00b7ner", "Geist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein Wicht, der gar nichts kann als winseln, \u00e4chzen, st\u00f6hnen,", "tokens": ["Ein", "Wicht", ",", "der", "gar", "nichts", "kann", "als", "win\u00b7seln", ",", "\u00e4ch\u00b7zen", ",", "st\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PIS", "VMFIN", "KOKOM", "VVFIN", "$,", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lebt, wenn du ihm es glaubst, im Guten, Ganzen, Sch\u00f6nen!", "tokens": ["Lebt", ",", "wenn", "du", "ihm", "es", "glaubst", ",", "im", "Gu\u00b7ten", ",", "Gan\u00b7zen", ",", "Sch\u00f6\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PPER", "PPER", "VVFIN", "$,", "APPRART", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Spinnweb'ges Ideal, Idee schwinds\u00fcchtig hohl,", "tokens": ["Spinn\u00b7we\u00b7b'\u00b7ges", "I\u00b7deal", ",", "I\u00b7dee", "schwind\u00b7s\u00fcch\u00b7tig", "hohl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.2": {"text": "Bist du Idalia? erb\u00e4rmliches Idol!", "tokens": ["Bist", "du", "I\u00b7da\u00b7lia", "?", "er\u00b7b\u00e4rm\u00b7li\u00b7ches", "I\u00b7dol", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NE", "$.", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Sch\u00f6nheit G\u00f6ttin ist dem Schaum entboren zwar,", "tokens": ["Der", "Sch\u00f6n\u00b7heit", "G\u00f6t\u00b7tin", "ist", "dem", "Schaum", "ent\u00b7bo\u00b7ren", "zwar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch ist sie nicht ein Schaum, und nicht ein Abschaum gar.", "tokens": ["Doch", "ist", "sie", "nicht", "ein", "Schaum", ",", "und", "nicht", "ein", "Ab\u00b7schaum", "gar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "ART", "NN", "$,", "KON", "PTKNEG", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wie auch geschmacklos ein Geschmack sei, so vertrackt", "tokens": ["Wie", "auch", "ge\u00b7schmack\u00b7los", "ein", "Ge\u00b7schmack", "sei", ",", "so", "ver\u00b7trackt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADV", "ADJD", "ART", "NN", "VAFIN", "$,", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist keiner als der ward aus Feinheit abgeschmackt.", "tokens": ["Ist", "kei\u00b7ner", "als", "der", "ward", "aus", "Fein\u00b7heit", "ab\u00b7ge\u00b7schmackt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "KOKOM", "ART", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Zu hobeln ist der Plump', ein Dummer ist zu witzigen,", "tokens": ["Zu", "ho\u00b7beln", "ist", "der", "Plump'", ",", "ein", "Dum\u00b7mer", "ist", "zu", "wit\u00b7zi\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VAFIN", "ART", "NN", "$,", "ART", "NN", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Doch nichts zu machen mehr ist aus dem Ueberspitzigen.", "tokens": ["Doch", "nichts", "zu", "ma\u00b7chen", "mehr", "ist", "aus", "dem", "Ue\u00b7ber\u00b7spit\u00b7zi\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PTKZU", "VVINF", "ADV", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}