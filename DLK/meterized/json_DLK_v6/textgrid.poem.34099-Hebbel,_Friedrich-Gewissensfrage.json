{"textgrid.poem.34099": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Gewissensfrage", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Machte der K\u00fcnstler ein Bild und w\u00fc\u00dfte, es dauere ewig,", "tokens": ["Mach\u00b7te", "der", "K\u00fcnst\u00b7ler", "ein", "Bild", "und", "w\u00fc\u00df\u00b7te", ",", "es", "dau\u00b7e\u00b7re", "e\u00b7wig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "KON", "VVFIN", "$,", "PPER", "VVFIN", "ADJD", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Aber ein einziger Zug, tief, wie kein and'rer, versteckt,", "tokens": ["A\u00b7ber", "ein", "ein\u00b7zi\u00b7ger", "Zug", ",", "tief", ",", "wie", "kein", "an\u00b7d'\u00b7rer", ",", "ver\u00b7steckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "ADJD", "$,", "PWAV", "PIAT", "ADJA", "$,", "VVPP", "$,"], "meter": "+-+-+-+-+--+--+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Werde von Keinem erkannt der jetz'gen und k\u00fcnftigen Menschen,", "tokens": ["Wer\u00b7de", "von", "Kei\u00b7nem", "er\u00b7kannt", "der", "jetz'\u00b7gen", "und", "k\u00fcnf\u00b7ti\u00b7gen", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PIS", "VVFIN", "ART", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Bis an's Ende der Zeit, glaubt ihr, er lie\u00dfe ihn weg?", "tokens": ["Bis", "an's", "En\u00b7de", "der", "Zeit", ",", "glaubt", "ihr", ",", "er", "lie\u00b7\u00dfe", "ihn", "weg", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "ART", "NN", "$,", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}}}}}