{"textgrid.poem.36864": {"metadata": {"author": {"name": "Gellert, Christian F\u00fcrchtegott", "birth": "N.A.", "death": "N.A."}, "title": "Osterlied", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erinnre dich, mein Geist, erfreut", "tokens": ["E\u00b7rinn\u00b7re", "dich", ",", "mein", "Geist", ",", "er\u00b7freut"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des hohen Tags der Herrlichkeit;", "tokens": ["Des", "ho\u00b7hen", "Tags", "der", "Herr\u00b7lich\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Halt im Ged\u00e4chtnis Jesum Christ,", "tokens": ["Halt", "im", "Ge\u00b7d\u00e4cht\u00b7nis", "Je\u00b7sum", "Christ", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "NE", "NE", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Der von dem Tod erstanden ist!", "tokens": ["Der", "von", "dem", "Tod", "er\u00b7stan\u00b7den", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "F\u00fchl alle Dankbarkeit f\u00fcr ihn,", "tokens": ["F\u00fchl", "al\u00b7le", "Dank\u00b7bar\u00b7keit", "f\u00fcr", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als ob er heute dir erschien,", "tokens": ["Als", "ob", "er", "heu\u00b7te", "dir", "er\u00b7schien", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als spr\u00e4ch er: Friede sei mit dir!", "tokens": ["Als", "spr\u00e4ch", "er", ":", "Frie\u00b7de", "sei", "mit", "dir", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "$.", "NN", "VAFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So freue dich, mein Geist, in mir!", "tokens": ["So", "freu\u00b7e", "dich", ",", "mein", "Geist", ",", "in", "mir", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Schau \u00fcber dich, und bet ihn an.", "tokens": ["Schau", "\u00fc\u00b7ber", "dich", ",", "und", "bet", "ihn", "an", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er mi\u00dft den Sternen ihre Bahn;", "tokens": ["Er", "mi\u00dft", "den", "Ster\u00b7nen", "ih\u00b7re", "Bahn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er lebt und herrscht mit Gott vereint,", "tokens": ["Er", "lebt", "und", "herrscht", "mit", "Gott", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ist dein K\u00f6nig und dein Freund.", "tokens": ["Und", "ist", "dein", "K\u00f6\u00b7nig", "und", "dein", "Freund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Macht, Ruhm und Hoheit immerdar", "tokens": ["Macht", ",", "Ruhm", "und", "Ho\u00b7heit", "im\u00b7mer\u00b7dar"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem, der da ist, und der da war!", "tokens": ["Dem", ",", "der", "da", "ist", ",", "und", "der", "da", "war", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "ADV", "VAFIN", "$,", "KON", "ART", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Name sei gebenedeit,", "tokens": ["Sein", "Na\u00b7me", "sei", "ge\u00b7be\u00b7ne\u00b7deit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von nun an bis in Ewigkeit!", "tokens": ["Von", "nun", "an", "bis", "in", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "KON", "APPR", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "O Glaube, der das Herz erh\u00f6ht!", "tokens": ["O", "Glau\u00b7be", ",", "der", "das", "Herz", "er\u00b7h\u00f6ht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was ist der Erde Majest\u00e4t,", "tokens": ["Was", "ist", "der", "Er\u00b7de", "Ma\u00b7jes\u00b7t\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn sie mein Geist mit der vergleicht,", "tokens": ["Wenn", "sie", "mein", "Geist", "mit", "der", "ver\u00b7gleicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "ART", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Die ich durch Gottes Sohn erreicht?", "tokens": ["Die", "ich", "durch", "Got\u00b7tes", "Sohn", "er\u00b7reicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Vor seinem Thron, in seinem Reich,", "tokens": ["Vor", "sei\u00b7nem", "Thron", ",", "in", "sei\u00b7nem", "Reich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Unsterblich, heilig, Engeln gleich,", "tokens": ["U\u00b7nsterb\u00b7lich", ",", "hei\u00b7lig", ",", "En\u00b7geln", "gleich", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "NN", "ADV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und ewig, ewig selig sein;", "tokens": ["Und", "e\u00b7wig", ",", "e\u00b7wig", "se\u00b7lig", "sein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJD", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Herr, welche Herrlichkeit ist mein!", "tokens": ["Herr", ",", "wel\u00b7che", "Herr\u00b7lich\u00b7keit", "ist", "mein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAT", "NN", "VAFIN", "PPOSAT", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.7": {"line.1": {"text": "Mein Herz erliegt froh unter ihr;", "tokens": ["Mein", "Herz", "er\u00b7liegt", "froh", "un\u00b7ter", "ihr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lieb und Verwundrung k\u00e4mpft in mir,", "tokens": ["Lieb", "und", "Ver\u00b7wund\u00b7rung", "k\u00e4mpft", "in", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und voll von Ehrfurcht, Dank und Pflicht,", "tokens": ["Und", "voll", "von", "Ehr\u00b7furcht", ",", "Dank", "und", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fall ich, Gott, auf mein Angesicht.", "tokens": ["Fall", "ich", ",", "Gott", ",", "auf", "mein", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.8": {"line.1": {"text": "Du, der du in den Himmeln thronst,", "tokens": ["Du", ",", "der", "du", "in", "den", "Him\u00b7meln", "thronst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich soll da wohnen, wo du wohnst?", "tokens": ["Ich", "soll", "da", "woh\u00b7nen", ",", "wo", "du", "wohnst", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$,", "PWAV", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und du erf\u00fcllst einst mein Vertraun,", "tokens": ["Und", "du", "er\u00b7f\u00fcllst", "einst", "mein", "Ver\u00b7traun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In meinem Fleische dich zu schaun?", "tokens": ["In", "mei\u00b7nem", "Flei\u00b7sche", "dich", "zu", "schaun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ich soll, wenn du, des Lebens F\u00fcrst,", "tokens": ["Ich", "soll", ",", "wenn", "du", ",", "des", "Le\u00b7bens", "F\u00fcrst", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "PPER", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Wolken g\u00f6ttlich kommen wirst,", "tokens": ["In", "Wol\u00b7ken", "g\u00f6tt\u00b7lich", "kom\u00b7men", "wirst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erweckt aus meinem Grabe gehn,", "tokens": ["Er\u00b7weckt", "aus", "mei\u00b7nem", "Gra\u00b7be", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und rein zu deiner Rechten stehn?", "tokens": ["Und", "rein", "zu", "dei\u00b7ner", "Rech\u00b7ten", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Mit Engeln und mit Seraphim,", "tokens": ["Mit", "En\u00b7geln", "und", "mit", "Se\u00b7ra\u00b7phim", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Thronen und mit Cherubim,", "tokens": ["Mit", "Thro\u00b7nen", "und", "mit", "Che\u00b7ru\u00b7bim", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit allen Frommen aller Zeit", "tokens": ["Mit", "al\u00b7len", "From\u00b7men", "al\u00b7ler", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Soll ich mich freun in Ewigkeit?", "tokens": ["Soll", "ich", "mich", "freun", "in", "E\u00b7wig\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Zu welchem Gl\u00fcck, zu welchem Ruhm", "tokens": ["Zu", "wel\u00b7chem", "Gl\u00fcck", ",", "zu", "wel\u00b7chem", "Ruhm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "$,", "APPR", "PWAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erhebt uns nicht das Christentum!", "tokens": ["Er\u00b7hebt", "uns", "nicht", "das", "Chris\u00b7ten\u00b7tum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit dir gekreuzigt, Gottes Sohn,", "tokens": ["Mit", "dir", "ge\u00b7kreu\u00b7zigt", ",", "Got\u00b7tes", "Sohn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sind wir auch auferstanden schon.", "tokens": ["Sind", "wir", "auch", "auf\u00b7er\u00b7stan\u00b7den", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Nie komm es mir aus meinem Sinn,", "tokens": ["Nie", "komm", "es", "mir", "aus", "mei\u00b7nem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was ich, mein Heil, dir schuldig bin;", "tokens": ["Was", "ich", ",", "mein", "Heil", ",", "dir", "schul\u00b7dig", "bin", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "$,", "PPOSAT", "NN", "$,", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Damit ich mich, in Liebe treu,", "tokens": ["Da\u00b7mit", "ich", "mich", ",", "in", "Lie\u00b7be", "treu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu deinem Bilde stets erneu.", "tokens": ["Zu", "dei\u00b7nem", "Bil\u00b7de", "stets", "er\u00b7neu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Er ist's, der alles in uns schafft,", "tokens": ["Er", "ist's", ",", "der", "al\u00b7les", "in", "uns", "schafft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "PIS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein ist das Reich, sein ist die Kraft.", "tokens": ["Sein", "ist", "das", "Reich", ",", "sein", "ist", "die", "Kraft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "NN", "$,", "VAINF", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Halt im Ged\u00e4chtnis Jesum Christ,", "tokens": ["Halt", "im", "Ge\u00b7d\u00e4cht\u00b7nis", "Je\u00b7sum", "Christ", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "NE", "NE", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Der von dem Tod erstanden ist.", "tokens": ["Der", "von", "dem", "Tod", "er\u00b7stan\u00b7den", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}