{"dta.poem.2878": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "79.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O wiege dich nicht ein in tr\u00e4umenden Gef\u00fchlen,", "tokens": ["O", "wie\u00b7ge", "dich", "nicht", "ein", "in", "tr\u00e4u\u00b7men\u00b7den", "Ge\u00b7f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "ART", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Fehlhoffend Sturm und Brand mit goldnem Duft zu k\u00fchlen.", "tokens": ["Fehl\u00b7hof\u00b7fend", "Sturm", "und", "Brand", "mit", "gold\u00b7nem", "Duft", "zu", "k\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gerade wo den Feind du w\u00e4hnest \u00fcberwunden,", "tokens": ["Ge\u00b7ra\u00b7de", "wo", "den", "Feind", "du", "w\u00e4h\u00b7nest", "\u00fc\u00b7berw\u00b7un\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "NN", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im innersten Gem\u00fcth wirst du von ihm gebunden.", "tokens": ["Im", "in\u00b7ners\u00b7ten", "Ge\u00b7m\u00fcth", "wirst", "du", "von", "ihm", "ge\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}}, "stanza.3": {"line.1": {"text": "Denn heimlich mit der Lust im Bund ist die Empfindung,", "tokens": ["Denn", "heim\u00b7lich", "mit", "der", "Lust", "im", "Bund", "ist", "die", "Emp\u00b7fin\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "APPRART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Kampfe mit der Welt nur ist Welt\u00fcberwindung.", "tokens": ["Im", "Kamp\u00b7fe", "mit", "der", "Welt", "nur", "ist", "Welt\u00b7\u00fc\u00b7berw\u00b7in\u00b7dung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "ADV", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Drum leb' aus dir hinaus, und steig in dich nur nieder", "tokens": ["Drum", "leb'", "aus", "dir", "hin\u00b7aus", ",", "und", "steig", "in", "dich", "nur", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "APPR", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "PPER", "ADV", "PTKVZ"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um auszuruhn und neu hinaus zu leben wieder.", "tokens": ["Um", "aus\u00b7zu\u00b7ruhn", "und", "neu", "hin\u00b7aus", "zu", "le\u00b7ben", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "VVIZU", "KON", "ADJD", "APZR", "PTKZU", "VVINF", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wie selbst den Athem Gott dir dazu hat verliehn,", "tokens": ["Wie", "selbst", "den", "A\u00b7them", "Gott", "dir", "da\u00b7zu", "hat", "ver\u00b7liehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "PPER", "PAV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihn auszuathmen auch, nicht nur ihn einzuziehn;", "tokens": ["Ihn", "aus\u00b7zu\u00b7ath\u00b7men", "auch", ",", "nicht", "nur", "ihn", "ein\u00b7zu\u00b7ziehn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PTKNEG", "ADV", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "So wechselweise mag in sich der Geist sich senken,", "tokens": ["So", "wech\u00b7sel\u00b7wei\u00b7se", "mag", "in", "sich", "der", "Geist", "sich", "sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VMFIN", "APPR", "PRF", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um desto r\u00fcstiger sich auf die Welt zu lenken.", "tokens": ["Um", "des\u00b7to", "r\u00fcs\u00b7ti\u00b7ger", "sich", "auf", "die", "Welt", "zu", "len\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADJD", "PRF", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Nur wenn er gl\u00fccklich sich erh\u00e4lt in dieser Schwebe,", "tokens": ["Nur", "wenn", "er", "gl\u00fcck\u00b7lich", "sich", "er\u00b7h\u00e4lt", "in", "die\u00b7ser", "Schwe\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADJD", "PRF", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geht unbestrickt er durch ein doppelt Irrgewebe.", "tokens": ["Geht", "un\u00b7be\u00b7strickt", "er", "durch", "ein", "dop\u00b7pelt", "Irr\u00b7ge\u00b7we\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "APPR", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}