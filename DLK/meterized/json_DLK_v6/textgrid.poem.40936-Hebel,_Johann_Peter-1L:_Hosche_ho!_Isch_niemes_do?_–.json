{"textgrid.poem.40936": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hosche ho! Isch niemes do? \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.85", "en:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hosche ho! Isch niemes do? \u2013", "tokens": ["Ho\u00b7sche", "ho", "!", "Isch", "nie\u00b7mes", "do", "?", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "$.", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbnummen ine! Was w\u00e4r Ich lieb?\u00ab \u2013", "tokens": ["\u00bb", "num\u00b7men", "i\u00b7ne", "!", "Was", "w\u00e4r", "Ich", "lieb", "?", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "FM.la", "FM.la", "$.", "PWS", "VAFIN", "PPER", "ADJD", "$.", "$(", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Wennmer die Frau V\u00f6gtin oder die", "tokens": ["Wenn\u00b7mer", "die", "Frau", "V\u00f6g\u00b7tin", "o\u00b7der", "die"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "NE", "KON", "ART"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "J(ungfer) G(ustave) ne Briefle schrieb. \u2013", "tokens": ["J", "(", "ung\u00b7fer", ")", "G", "(", "us\u00b7ta\u00b7ve", ")", "ne", "Brief\u00b7le", "schrieb", ".", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$(", "ADJD", "$(", "NE", "$(", "NE", "$(", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "\u00bbch\u00f6mmet e andermol!", "tokens": ["\u00bb", "ch\u00f6m\u00b7met", "e", "an\u00b7der\u00b7mol", "!"], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "FM", "FM", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Mer henn jez nit der Zit!\u00ab \u2013", "tokens": ["Mer", "henn", "jez", "nit", "der", "Zit", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PIAT", "NN", "ADV", "PTKNEG", "ART", "NN", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Numme au ne halb's, mi Weg", "tokens": ["Num\u00b7me", "au", "ne", "halb's", ",", "mi", "Weg"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "NE", "NE", "$,", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "isch gar gr\u00fcseli wit.", "tokens": ["isch", "gar", "gr\u00fc\u00b7se\u00b7li", "wit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Nummen au ne chleis,", "tokens": ["Num\u00b7men", "au", "ne", "chleis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "nummen au ne paar W\u00f6rtli! \u2013", "tokens": ["num\u00b7men", "au", "ne", "paar", "W\u00f6rt\u00b7li", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "NE", "NE", "PIAT", "NN", "$.", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.11": {"text": "\u00bbwartet denn, i schrieb ich eis.", "tokens": ["\u00bb", "war\u00b7tet", "denn", ",", "i", "schrieb", "ich", "eis", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "$,", "NE", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Sitzet d\u00f6rt an sell \u00d6rtli!\u00ab", "tokens": ["Sit\u00b7zet", "d\u00f6rt", "an", "sell", "\u00d6rt\u00b7li", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}