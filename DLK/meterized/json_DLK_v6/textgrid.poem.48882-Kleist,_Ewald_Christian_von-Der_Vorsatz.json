{"textgrid.poem.48882": {"metadata": {"author": {"name": "Kleist, Ewald Christian von", "birth": "N.A.", "death": "N.A."}, "title": "Der Vorsatz", "genre": "verse", "period": "N.A.", "pub_year": 1737, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dich treibt dein Eifer, wie dein Ro\u00df die Sporen,", "tokens": ["Dich", "treibt", "dein", "Ei\u00b7fer", ",", "wie", "dein", "Ro\u00df", "die", "Spo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PWAV", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O Held! was fleuchst du zu des Todes Thoren!", "tokens": ["O", "Held", "!", "was", "fleuchst", "du", "zu", "des", "To\u00b7des", "Tho\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Suchst du, damit dich Wahn und Nachruhm labe,", "tokens": ["Suchst", "du", ",", "da\u00b7mit", "dich", "Wahn", "und", "Nach\u00b7ruhm", "la\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Weg zum Grabe?", "tokens": ["Den", "Weg", "zum", "Gra\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "La\u00df Luft und Zeiten \u00fcber Thal und H\u00f6hen", "tokens": ["La\u00df", "Luft", "und", "Zei\u00b7ten", "\u00fc\u00b7ber", "Thal", "und", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "NN", "KON", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit ewgen Fl\u00fcgeln deine Thaten wehen,", "tokens": ["Mit", "ew\u00b7gen", "Fl\u00fc\u00b7geln", "dei\u00b7ne", "Tha\u00b7ten", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Feld Elysens wird von fernem Schallen", "tokens": ["Das", "Feld", "E\u00b7ly\u00b7sens", "wird", "von", "fer\u00b7nem", "Schal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht wiederhallen.", "tokens": ["Nicht", "wie\u00b7der\u00b7hal\u00b7len", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Und du, o Geitzhals! magst mit M\u00fch entdecken,", "tokens": ["Und", "du", ",", "o", "Geitz\u00b7hals", "!", "magst", "mit", "M\u00fch", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "NN", "$.", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was uns Gebirge weislich tief verstecken;", "tokens": ["Was", "uns", "Ge\u00b7bir\u00b7ge", "weis\u00b7lich", "tief", "ver\u00b7ste\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf! f\u00fcll in Peru, trotz sey Fluth und Winden,", "tokens": ["Auf", "!", "f\u00fcll", "in", "Pe\u00b7ru", ",", "trotz", "sey", "Fluth", "und", "Win\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "APPR", "NE", "$,", "APPR", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein Schiff mit S\u00fcnden.", "tokens": ["Dein", "Schiff", "mit", "S\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Gekr\u00f6nter P\u00f6bel, la\u00df in stolzen Zimmern", "tokens": ["Ge\u00b7kr\u00f6n\u00b7ter", "P\u00f6\u00b7bel", ",", "la\u00df", "in", "stol\u00b7zen", "Zim\u00b7mern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVIMP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Jaspis, Tapeten und Cristalle schimmern;", "tokens": ["Jas\u00b7pis", ",", "Ta\u00b7pe\u00b7ten", "und", "Cris\u00b7tal\u00b7le", "schim\u00b7mern", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In Schl\u00f6ssern drengt sich oft ein Schwarm von Leide", "tokens": ["In", "Schl\u00f6s\u00b7sern", "drengt", "sich", "oft", "ein", "Schwarm", "von", "Lei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PRF", "ADV", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Kleid der Freude.", "tokens": ["Im", "Kleid", "der", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Der Ruh im Schoosse, will ich eure Rotten", "tokens": ["Der", "Ruh", "im", "Schoos\u00b7se", ",", "will", "ich", "eu\u00b7re", "Rot\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "VMFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An hellen B\u00e4chen, wie mein Utz verspotten,", "tokens": ["An", "hel\u00b7len", "B\u00e4\u00b7chen", ",", "wie", "mein", "Utz", "ver\u00b7spot\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KOKOM", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Welchen die Dichtkunst, wenn sein Lied ert\u00f6net,", "tokens": ["Wel\u00b7chen", "die", "Dicht\u00b7kunst", ",", "wenn", "sein", "Lied", "er\u00b7t\u00f6\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Mit Epheu kr\u00f6net.", "tokens": ["Mit", "E\u00b7pheu", "kr\u00f6\u00b7net", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Er schwingt sich muthig in den Kreis der Sterne", "tokens": ["Er", "schwingt", "sich", "mut\u00b7hig", "in", "den", "Kreis", "der", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch Dunst und Wolken. Von der hohen Ferne", "tokens": ["Durch", "Dunst", "und", "Wol\u00b7ken", ".", "Von", "der", "ho\u00b7hen", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schaut er, wenn Schaaren wilder Krieger l\u00e4rmen,", "tokens": ["Schaut", "er", ",", "wenn", "Schaa\u00b7ren", "wil\u00b7der", "Krie\u00b7ger", "l\u00e4r\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur Wespen schw\u00e4rmen.", "tokens": ["Nur", "Wes\u00b7pen", "schw\u00e4r\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Er schaut von oben L\u00e4nder Hufen gleichen,", "tokens": ["Er", "schaut", "von", "o\u00b7ben", "L\u00e4n\u00b7der", "Hu\u00b7fen", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und St\u00e4dte L\u00f6chern; in den engen Reichen", "tokens": ["Und", "St\u00e4d\u00b7te", "L\u00f6\u00b7chern", ";", "in", "den", "en\u00b7gen", "Rei\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "NN", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schaut er in Haufen, heissen Geitz zu k\u00fchlen", "tokens": ["Schaut", "er", "in", "Hau\u00b7fen", ",", "heis\u00b7sen", "Geitz", "zu", "k\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Maulw\u00fcrfe w\u00fchlen.", "tokens": ["Maul\u00b7w\u00fcr\u00b7fe", "w\u00fch\u00b7len", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "Dann denkt er seufzend mit ger\u00fchrten Sinnen:", "tokens": ["Dann", "denkt", "er", "seuf\u00b7zend", "mit", "ge\u00b7r\u00fchr\u00b7ten", "Sin\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbwas wollt ihr Thoren endlich noch beginnen?", "tokens": ["\u00bb", "was", "wollt", "ihr", "Tho\u00b7ren", "end\u00b7lich", "noch", "be\u00b7gin\u00b7nen", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VMFIN", "PPOSAT", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr raset; meynt ihr in den schmalen Zonen", "tokens": ["Ihr", "ra\u00b7set", ";", "meynt", "ihr", "in", "den", "schma\u00b7len", "Zo\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ewig zu wohnen?", "tokens": ["E\u00b7wig", "zu", "woh\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.9": {"line.1": {"text": "Tod, Quaal und Schrecken, la\u00dft ihr, um zu siegen,", "tokens": ["Tod", ",", "Qua\u00b7al", "und", "Schre\u00b7cken", ",", "la\u00dft", "ihr", ",", "um", "zu", "sie\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,", "VVIMP", "PPER", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "++--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Aus hohlen Schl\u00fcnden auf die Br\u00fcder fliegen;", "tokens": ["Aus", "hoh\u00b7len", "Schl\u00fcn\u00b7den", "auf", "die", "Br\u00fc\u00b7der", "flie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ist eurem Hochmuth in der L\u00e4nder Menge", "tokens": ["Ist", "eu\u00b7rem", "Hoch\u00b7muth", "in", "der", "L\u00e4n\u00b7der", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Raum zu enge?", "tokens": ["Der", "Raum", "zu", "en\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "La\u00dft ihr nur darum ewge B\u00e4ue gleissen,", "tokens": ["La\u00dft", "ihr", "nur", "da\u00b7rum", "ew\u00b7ge", "B\u00e4u\u00b7e", "gleis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PAV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um schnell dieselbe wieder einzureissen?", "tokens": ["Um", "schnell", "die\u00b7sel\u00b7be", "wie\u00b7der", "ein\u00b7zu\u00b7reis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "PDAT", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Tod kommt pl\u00f6tzlich, der wird euch bey Zeiten", "tokens": ["Der", "Tod", "kommt", "pl\u00f6tz\u00b7lich", ",", "der", "wird", "euch", "bey", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "PRELS", "VAFIN", "PPER", "APPR", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "H\u00f6hlen bereiten.\u00ab", "tokens": ["H\u00f6h\u00b7len", "be\u00b7rei\u00b7ten", ".", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["NN", "VVINF", "$.", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.11": {"line.1": {"text": "Drauf greift er geitzig nach der goldnen Leyer,", "tokens": ["Drauf", "greift", "er", "geit\u00b7zig", "nach", "der", "gold\u00b7nen", "Le\u00b7yer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Bestraft des Lasters kriechend Ungeheuer,", "tokens": ["Be\u00b7straft", "des", "Las\u00b7ters", "krie\u00b7chend", "Un\u00b7ge\u00b7heu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVPP", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sein Lob der Tugend schallt in regen L\u00fcften,", "tokens": ["Sein", "Lob", "der", "Tu\u00b7gend", "schallt", "in", "re\u00b7gen", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In Wald und Kl\u00fcften.", "tokens": ["In", "Wald", "und", "Kl\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.12": {"line.1": {"text": "So soll mein Geist sich zu den Wolken schwingen,", "tokens": ["So", "soll", "mein", "Geist", "sich", "zu", "den", "Wol\u00b7ken", "schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So r\u00fchrend sollen meine Saiten klingen.", "tokens": ["So", "r\u00fch\u00b7rend", "sol\u00b7len", "mei\u00b7ne", "Sai\u00b7ten", "klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O Freund erheb mich von den seichten H\u00fcgeln!", "tokens": ["O", "Freund", "er\u00b7heb", "mich", "von", "den", "seich\u00b7ten", "H\u00fc\u00b7geln", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf deinen Fl\u00fcgeln!", "tokens": ["Auf", "dei\u00b7nen", "Fl\u00fc\u00b7geln", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}