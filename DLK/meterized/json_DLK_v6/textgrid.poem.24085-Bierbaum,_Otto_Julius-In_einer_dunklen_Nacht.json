{"textgrid.poem.24085": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "In einer dunklen Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn dieser K\u00f6rper einst zerfallen ist,", "tokens": ["Wenn", "die\u00b7ser", "K\u00f6r\u00b7per", "einst", "zer\u00b7fal\u00b7len", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Seele, du meine Seele,", "tokens": ["See\u00b7le", ",", "du", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Tr\u00e4umst du dir einen andern Leib?", "tokens": ["Tr\u00e4umst", "du", "dir", "ei\u00b7nen", "an\u00b7dern", "Leib", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Lebst du auf einem andern Stern?", "tokens": ["Lebst", "du", "auf", "ei\u00b7nem", "an\u00b7dern", "Stern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Treibst du aus deinem Drange, der die Sch\u00f6nheit will,", "tokens": ["Treibst", "du", "aus", "dei\u00b7nem", "Dran\u00b7ge", ",", "der", "die", "Sch\u00f6n\u00b7heit", "will", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,", "PRELS", "ART", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Blumen, B\u00e4ume?", "tokens": ["Blu\u00b7men", ",", "B\u00e4u\u00b7me", "?"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Oh meine Seele, wenn du nicht vergehst,", "tokens": ["Oh", "mei\u00b7ne", "See\u00b7le", ",", "wenn", "du", "nicht", "ver\u00b7gehst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["XY", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dann bleib bei ihr, die mir das Leben lieber macht", "tokens": ["Dann", "bleib", "bei", "ihr", ",", "die", "mir", "das", "Le\u00b7ben", "lie\u00b7ber", "macht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "$,", "PRELS", "PPER", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als alle Sch\u00f6nheit.", "tokens": ["Als", "al\u00b7le", "Sch\u00f6n\u00b7heit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Umbl\u00fcht sie,", "tokens": ["Um\u00b7bl\u00fcht", "sie", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Umh\u00fcte sie,", "tokens": ["Um\u00b7h\u00fc\u00b7te", "sie", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "La\u00df alle Sterne, alle Seligkeit", "tokens": ["La\u00df", "al\u00b7le", "Ster\u00b7ne", ",", "al\u00b7le", "Se\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und bleibe bei ihr.", "tokens": ["Und", "blei\u00b7be", "bei", "ihr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Und wenn auch sie dann, wachgek\u00fc\u00dft vom Tod,", "tokens": ["Und", "wenn", "auch", "sie", "dann", ",", "wach\u00b7ge\u00b7k\u00fc\u00dft", "vom", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PPER", "ADV", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sich selbst in ihrer tiefsten Reinheit lebt,", "tokens": ["Sich", "selbst", "in", "ih\u00b7rer", "tiefs\u00b7ten", "Rein\u00b7heit", "lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann geh in sie und gieb dich selber hin,", "tokens": ["Dann", "geh", "in", "sie", "und", "gieb", "dich", "sel\u00b7ber", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "KON", "VVIMP", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sei eins mit ihr.", "tokens": ["Sei", "eins", "mit", "ihr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "APPR", "PPOSAT", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Das ist die Seligkeit, die ich dir hoffe,", "tokens": ["Das", "ist", "die", "Se\u00b7lig\u00b7keit", ",", "die", "ich", "dir", "hof\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Meine Seele.", "tokens": ["Mei\u00b7ne", "See\u00b7le", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}