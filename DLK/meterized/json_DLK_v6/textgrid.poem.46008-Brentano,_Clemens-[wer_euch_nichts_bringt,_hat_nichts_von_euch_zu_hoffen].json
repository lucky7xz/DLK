{"textgrid.poem.46008": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[wer euch nichts bringt, hat nichts von euch zu hoffen]", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer euch nichts bringt, hat nichts von euch zu hoffen,", "tokens": ["Wer", "euch", "nichts", "bringt", ",", "hat", "nichts", "von", "euch", "zu", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PIS", "VVFIN", "$,", "VAFIN", "PIS", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Erzschenk habet ihr den Hals gebrochen,", "tokens": ["Dem", "Erz\u00b7schenk", "ha\u00b7bet", "ihr", "den", "Hals", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur offner Hand stehn eure Ohren offen", "tokens": ["Nur", "off\u00b7ner", "Hand", "stehn", "eu\u00b7re", "Oh\u00b7ren", "of\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ohne Klingen hilft bei euch kein Pochen!", "tokens": ["Und", "oh\u00b7ne", "Klin\u00b7gen", "hilft", "bei", "euch", "kein", "Po\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "APPR", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein armer Krieger hat hier nichts zu holen,", "tokens": ["Ein", "ar\u00b7mer", "Krie\u00b7ger", "hat", "hier", "nichts", "zu", "ho\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr f\u00fchret keinen Krieg, wo ihr nichts krieget,", "tokens": ["Ihr", "f\u00fch\u00b7ret", "kei\u00b7nen", "Krieg", ",", "wo", "ihr", "nichts", "krie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "PWAV", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und weil ihr blanken Klingen unterlieget,", "tokens": ["Und", "weil", "ihr", "blan\u00b7ken", "Klin\u00b7gen", "un\u00b7ter\u00b7lie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So ehrt ihr das Duell auf Goldpistolen.", "tokens": ["So", "ehrt", "ihr", "das", "Du\u00b7ell", "auf", "Gold\u00b7pis\u00b7to\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Poesie mu\u00df hier mit Armut leben;", "tokens": ["Die", "Poe\u00b7sie", "mu\u00df", "hier", "mit", "Ar\u00b7mut", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sing' ich Sonette euch auch noch so nette:", "tokens": ["Sing'", "ich", "So\u00b7net\u00b7te", "euch", "auch", "noch", "so", "net\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "PPER", "ADV", "ADV", "ADV", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr werdet nimmer Speise mir und Bette,", "tokens": ["Ihr", "wer\u00b7det", "nim\u00b7mer", "Spei\u00b7se", "mir", "und", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "PPER", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Statt Geld f\u00fcr Verse Fersengeld nur geben.", "tokens": ["Statt", "Geld", "f\u00fcr", "Ver\u00b7se", "Fer\u00b7sen\u00b7geld", "nur", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "NE", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ungern beherbergt ihr, und h\u00f6chstens Ungern", "tokens": ["Un\u00b7gern", "be\u00b7her\u00b7bergt", "ihr", ",", "und", "h\u00f6chs\u00b7tens", "Un\u00b7gern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$,", "KON", "ADJA", "NN"], "meter": "+--+---+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Aus Kremnitz, doch Erlanger m\u00fcssen hungern;", "tokens": ["Aus", "Krem\u00b7nitz", ",", "doch", "Er\u00b7lan\u00b7ger", "m\u00fcs\u00b7sen", "hun\u00b7gern", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KON", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur f\u00fcr Zechinen ist die Zeche drinnen.", "tokens": ["Nur", "f\u00fcr", "Ze\u00b7chi\u00b7nen", "ist", "die", "Ze\u00b7che", "drin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ein Ducka ist mir lieb, doch mit Dukaten!", "tokens": ["Ein", "Duc\u00b7ka", "ist", "mir", "lieb", ",", "doch", "mit", "Du\u00b7ka\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "PPER", "ADJD", "$,", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Souvrainen pflege ich f\u00fcr Severinen \u2013", "tokens": ["Sou\u00b7vrai\u00b7nen", "pfle\u00b7ge", "ich", "f\u00fcr", "Se\u00b7ve\u00b7ri\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Baronen ohne Bares nie zu dienen \u2013", "tokens": ["Ba\u00b7ro\u00b7nen", "oh\u00b7ne", "Ba\u00b7res", "nie", "zu", "die\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und kann mit Ahnen keine Hahnen braten!", "tokens": ["Und", "kann", "mit", "Ah\u00b7nen", "kei\u00b7ne", "Hah\u00b7nen", "bra\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "So nackt und kahl geh' ich von eurer Schwelle,", "tokens": ["So", "nackt", "und", "kahl", "geh'", "ich", "von", "eu\u00b7rer", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So n\u00fcchtern, bar und blank in voller Klarheit,", "tokens": ["So", "n\u00fcch\u00b7tern", ",", "bar", "und", "blank", "in", "vol\u00b7ler", "Klar\u00b7heit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als w\u00e4r' ich, die ich singe, \u2013 selbst die Wahrheit,", "tokens": ["Als", "w\u00e4r'", "ich", ",", "die", "ich", "sin\u00b7ge", ",", "\u2013", "selbst", "die", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$,", "$(", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn nur Reale sind bei euch's Reelle!", "tokens": ["Denn", "nur", "Re\u00b7a\u00b7le", "sind", "bei", "euch's", "Re\u00b7el\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Mit H\u00f6hnen siehst du wie ich hier vergehe,", "tokens": ["Mit", "H\u00f6h\u00b7nen", "siehst", "du", "wie", "ich", "hier", "ver\u00b7ge\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du Hofvolk, fressend Gold und Fleisch, wie Raben,", "tokens": ["Du", "Hof\u00b7volk", ",", "fres\u00b7send", "Gold", "und", "Fleisch", ",", "wie", "Ra\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "ADJD", "NN", "KON", "NN", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von dir ist nichts, du bist zum Narrn zu haben,", "tokens": ["Von", "dir", "ist", "nichts", ",", "du", "bist", "zum", "Narrn", "zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PIS", "$,", "PPER", "VAFIN", "APPRART", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich stand dein Narre hier, steh du, ich gehe!", "tokens": ["Ich", "stand", "dein", "Nar\u00b7re", "hier", ",", "steh", "du", ",", "ich", "ge\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "$,", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}}}}