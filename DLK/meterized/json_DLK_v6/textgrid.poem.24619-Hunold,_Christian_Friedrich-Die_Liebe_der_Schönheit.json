{"textgrid.poem.24619": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Liebe der Sch\u00f6nheit", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du sch\u00f6ne Mutter zeugst ein unvergleichlich Kind/", "tokens": ["Du", "sch\u00f6\u00b7ne", "Mut\u00b7ter", "zeugst", "ein", "un\u00b7ver\u00b7gleich\u00b7lich", "Kind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "ART", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Liebe/derer Macht die freyen dienstbar find.", "tokens": ["Die", "Lie\u00b7be", "/", "de\u00b7rer", "Macht", "die", "frey\u00b7en", "dienst\u00b7bar", "find", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PDS", "NN", "ART", "ADJA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ans deinem Crystallin mu\u00df sich mein Strom ergie\u00dfen/", "tokens": ["Ans", "dei\u00b7nem", "Crys\u00b7tal\u00b7lin", "mu\u00df", "sich", "mein", "Strom", "er\u00b7gie\u00b7\u00dfen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VMFIN", "PRF", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und aller Welt mit Lust in Leib und Seele flie\u00dfen.", "tokens": ["Und", "al\u00b7ler", "Welt", "mit", "Lust", "in", "Leib", "und", "See\u00b7le", "flie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}