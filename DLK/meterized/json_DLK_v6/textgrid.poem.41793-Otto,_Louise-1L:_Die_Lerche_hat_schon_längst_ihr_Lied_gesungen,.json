{"textgrid.poem.41793": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Lerche hat schon l\u00e4ngst ihr Lied gesungen,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Lerche hat schon l\u00e4ngst ihr Lied gesungen,", "tokens": ["Die", "Ler\u00b7che", "hat", "schon", "l\u00e4ngst", "ihr", "Lied", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gegr\u00fc\u00dft den ersten warmen Sonnenstrahl,", "tokens": ["Ge\u00b7gr\u00fc\u00dft", "den", "ers\u00b7ten", "war\u00b7men", "Son\u00b7nen\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Primeln sind beherzt hervorgedrungen", "tokens": ["Die", "Pri\u00b7meln", "sind", "be\u00b7herzt", "her\u00b7vor\u00b7ge\u00b7drun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Bl\u00e4tterknospen folgen ohne Zahl.", "tokens": ["Und", "Bl\u00e4t\u00b7ter\u00b7knos\u00b7pen", "fol\u00b7gen", "oh\u00b7ne", "Zahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die neue Saat sprie\u00dft fr\u00f6hlich schon hervor", "tokens": ["Die", "neu\u00b7e", "Saat", "sprie\u00dft", "fr\u00f6h\u00b7lich", "schon", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "ADV", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Strebt aus der Erde Scho\u00df zum Licht empor.", "tokens": ["Strebt", "aus", "der", "Er\u00b7de", "Scho\u00df", "zum", "Licht", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein S\u00e4nger nach dem andern kehret wieder,", "tokens": ["Ein", "S\u00e4n\u00b7ger", "nach", "dem", "an\u00b7dern", "keh\u00b7ret", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Bl\u00fcmchen nach dem andern kommt hervor,", "tokens": ["Ein", "Bl\u00fcm\u00b7chen", "nach", "dem", "an\u00b7dern", "kommt", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir schauen auf die Dornen sp\u00e4hend nieder \u2013", "tokens": ["Wir", "schau\u00b7en", "auf", "die", "Dor\u00b7nen", "sp\u00e4\u00b7hend", "nie\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da blaut und bl\u00fcht ein ganzer Veilchenflor,", "tokens": ["Da", "blaut", "und", "bl\u00fcht", "ein", "gan\u00b7zer", "Veil\u00b7chen\u00b7flor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und junges Gr\u00fcn ringsum das Aug' erquickt,", "tokens": ["Und", "jun\u00b7ges", "Gr\u00fcn", "ring\u00b7sum", "das", "Aug'", "er\u00b7quickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das \u00fcberall nach Lenzeszeichen blickt.", "tokens": ["Das", "\u00fc\u00b7be\u00b7rall", "nach", "Len\u00b7zes\u00b7zei\u00b7chen", "blickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nur eines fehlt und kluge Leute sprechen:", "tokens": ["Nur", "ei\u00b7nes", "fehlt", "und", "klu\u00b7ge", "Leu\u00b7te", "spre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So lange wir noch keine Schwalbe sehn", "tokens": ["So", "lan\u00b7ge", "wir", "noch", "kei\u00b7ne", "Schwal\u00b7be", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "ADV", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kann sich der Winter noch am Lenze r\u00e4chen,", "tokens": ["Kann", "sich", "der", "Win\u00b7ter", "noch", "am", "Len\u00b7ze", "r\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kann alle seine Herrlichkeit verwehn", "tokens": ["Kann", "al\u00b7le", "sei\u00b7ne", "Herr\u00b7lich\u00b7keit", "ver\u00b7wehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Durch Schnee und Sturm aus kaltem Ost und Nord \u2013:", "tokens": ["Durch", "Schnee", "und", "Sturm", "aus", "kal\u00b7tem", "Ost", "und", "Nord", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "ADJA", "NN", "KON", "NE", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Schwalbe nur ist unsers Fr\u00fchlings Hort.", "tokens": ["Die", "Schwal\u00b7be", "nur", "ist", "un\u00b7sers", "Fr\u00fch\u00b7lings", "Hort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wir d\u00fcrfen keinen Fr\u00fchling je vertrauen", "tokens": ["Wir", "d\u00fcr\u00b7fen", "kei\u00b7nen", "Fr\u00fch\u00b7ling", "je", "ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So lang' sich nicht die erste Schwalbe zeigt,", "tokens": ["So", "lang'", "sich", "nicht", "die", "ers\u00b7te", "Schwal\u00b7be", "zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PRF", "PTKNEG", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Noch nicht beginnt ihr trautes Nest zu bauen,", "tokens": ["Noch", "nicht", "be\u00b7ginnt", "ihr", "trau\u00b7tes", "Nest", "zu", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVFIN", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch nicht mit Zwitschern auf und niedersteigt,", "tokens": ["Noch", "nicht", "mit", "Zwit\u00b7schern", "auf", "und", "nie\u00b7ders\u00b7teigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "NN", "PTKVZ", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und fr\u00f6hlich einzieht in den alten Kreis \u2013", "tokens": ["Und", "fr\u00f6h\u00b7lich", "ein\u00b7zieht", "in", "den", "al\u00b7ten", "Kreis", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sie erst bringt uns des Fr\u00fchlings dauernd Reis.", "tokens": ["Sie", "erst", "bringt", "uns", "des", "Fr\u00fch\u00b7lings", "dau\u00b7ernd", "Reis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "ART", "NN", "VVPP", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und also ist es auch im V\u00f6lkerleben!", "tokens": ["Und", "al\u00b7so", "ist", "es", "auch", "im", "V\u00f6l\u00b7ker\u00b7le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schon manchmal ward ein hartes Joch gesprengt,", "tokens": ["Schon", "manch\u00b7mal", "ward", "ein", "har\u00b7tes", "Joch", "ge\u00b7sprengt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Schon manchmal hat es freie Flut gegeben", "tokens": ["Schon", "manch\u00b7mal", "hat", "es", "frei\u00b7e", "Flut", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und frisches Gr\u00fcn, das sich hervorgedr\u00e4ngt.", "tokens": ["Und", "fri\u00b7sches", "Gr\u00fcn", ",", "das", "sich", "her\u00b7vor\u00b7ge\u00b7dr\u00e4ngt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "PRELS", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Schon manchmal schiens, als sei es Fr\u00fchlingszeit \u2013", "tokens": ["Schon", "manch\u00b7mal", "schiens", ",", "als", "sei", "es", "Fr\u00fch\u00b7lings\u00b7zeit", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$,", "KOKOM", "VAFIN", "PPER", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dann kam ein Sturm \u2013 und alles war verschneit!", "tokens": ["Dann", "kam", "ein", "Sturm", "\u2013", "und", "al\u00b7les", "war", "ver\u00b7schneit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$(", "KON", "PIS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ein Warnungsruf! doch soll er uns nicht rauben", "tokens": ["Ein", "War\u00b7nungs\u00b7ruf", "!", "doch", "soll", "er", "uns", "nicht", "rau\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "ADV", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das frohe Hoffen, da\u00df es Fr\u00fchling wird,", "tokens": ["Das", "fro\u00b7he", "Hof\u00b7fen", ",", "da\u00df", "es", "Fr\u00fch\u00b7ling", "wird", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "PPER", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den ewigen, den hohen Zukunftsglauben!", "tokens": ["Den", "e\u00b7wi\u00b7gen", ",", "den", "ho\u00b7hen", "Zu\u00b7kunfts\u00b7glau\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er bleib in jedem Herzen unbeirrt.", "tokens": ["Er", "bleib", "in", "je\u00b7dem", "Her\u00b7zen", "un\u00b7be\u00b7irrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch niemand sei in Sicherheit gewiegt,", "tokens": ["Doch", "nie\u00b7mand", "sei", "in", "Si\u00b7cher\u00b7heit", "ge\u00b7wiegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So lange nicht zum Nest die Schwalbe fliegt.", "tokens": ["So", "lan\u00b7ge", "nicht", "zum", "Nest", "die", "Schwal\u00b7be", "fliegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "So lange nicht zu uns aus sch\u00f6nem S\u00fcden", "tokens": ["So", "lan\u00b7ge", "nicht", "zu", "uns", "aus", "sch\u00f6\u00b7nem", "S\u00fc\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PTKNEG", "APPR", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In jedes Haus ein Friedensbote kam \u2013", "tokens": ["In", "je\u00b7des", "Haus", "ein", "Frie\u00b7dens\u00b7bo\u00b7te", "kam", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So lange nicht am Herde, den wir h\u00fcten", "tokens": ["So", "lan\u00b7ge", "nicht", "am", "Her\u00b7de", ",", "den", "wir", "h\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "PTKNEG", "APPRART", "NN", "$,", "PRELS", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Freiheit Lied ein jedes Ohr vernahm \u2013", "tokens": ["Der", "Frei\u00b7heit", "Lied", "ein", "je\u00b7des", "Ohr", "ver\u00b7nahm", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So lange nicht von allen D\u00e4chern reden", "tokens": ["So", "lan\u00b7ge", "nicht", "von", "al\u00b7len", "D\u00e4\u00b7chern", "re\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PTKNEG", "APPR", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nach kecker Schwalbenart die Volkspropheten!", "tokens": ["Nach", "ke\u00b7cker", "Schwal\u00b7ben\u00b7art", "die", "Volks\u00b7pro\u00b7phe\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}