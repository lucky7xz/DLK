{"textgrid.poem.36937": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mit blauem Aug', mit blondem Haar", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit blauem Aug', mit blondem Haar", "tokens": ["Mit", "blau\u00b7em", "Aug'", ",", "mit", "blon\u00b7dem", "Haar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Mann von kr\u00e4ft'gem Wuchse war,", "tokens": ["Ein", "Mann", "von", "kr\u00e4ft'\u00b7gem", "Wuch\u00b7se", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf Herrmann's Denkmal steht er;", "tokens": ["Auf", "Herr\u00b7mann's", "Denk\u00b7mal", "steht", "er", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er wirft den Blick durch Flur und Wald,", "tokens": ["Er", "wirft", "den", "Blick", "durch", "Flur", "und", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und ruft, da\u00df es wohl m\u00e4chtig schallt:", "tokens": ["Und", "ruft", ",", "da\u00df", "es", "wohl", "m\u00e4ch\u00b7tig", "schallt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Vaterland, mein Vaterland,", "tokens": ["Mein", "Va\u00b7ter\u00b7land", ",", "mein", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du hast zu viele V\u00e4ter!", "tokens": ["Du", "hast", "zu", "vie\u00b7le", "V\u00e4\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er bricht 'nen gr\u00fcnen Eichenzweig", "tokens": ["Er", "bricht", "'nen", "gr\u00fc\u00b7nen", "Ei\u00b7chen\u00b7zweig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als Wanderstock sich ab sogleich,", "tokens": ["Als", "Wan\u00b7der\u00b7stock", "sich", "ab", "sog\u00b7leich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "APPR", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und durch die L\u00e4nder geht er,", "tokens": ["Und", "durch", "die", "L\u00e4n\u00b7der", "geht", "er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und was er h\u00f6rt und was er sieht,", "tokens": ["Und", "was", "er", "h\u00f6rt", "und", "was", "er", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "KON", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er ruft zu Allem, was geschieht:", "tokens": ["Er", "ruft", "zu", "Al\u00b7lem", ",", "was", "ge\u00b7schieht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIS", "$,", "PWS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Vaterland, mein Vaterland,", "tokens": ["Mein", "Va\u00b7ter\u00b7land", ",", "mein", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du hast zu viele V\u00e4ter!", "tokens": ["Du", "hast", "zu", "vie\u00b7le", "V\u00e4\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er ist gewandert hin und her,", "tokens": ["Er", "ist", "ge\u00b7wan\u00b7dert", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er hub den Blick, so thr\u00e4nenschwer,", "tokens": ["Er", "hub", "den", "Blick", ",", "so", "thr\u00e4\u00b7nen\u00b7schwer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu Gott und innig fleht er:", "tokens": ["Zu", "Gott", "und", "in\u00b7nig", "fleht", "er", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJD", "VVFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O schling' um unser Vaterland", "tokens": ["O", "schling'", "um", "un\u00b7ser", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Liebe und der Eintracht Band!", "tokens": ["Der", "Lie\u00b7be", "und", "der", "Ein\u00b7tracht", "Band", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Vaterland, mein Vaterland,", "tokens": ["Mein", "Va\u00b7ter\u00b7land", ",", "mein", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es hat zu viele V\u00e4ter!", "tokens": ["Es", "hat", "zu", "vie\u00b7le", "V\u00e4\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O Mutter, deutsche Freiheit, du,", "tokens": ["O", "Mut\u00b7ter", ",", "deut\u00b7sche", "Frei\u00b7heit", ",", "du", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJA", "NN", "$,", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lie\u00df' dich dein L\u00fcsten einst in Ruh',", "tokens": ["Lie\u00df'", "dich", "dein", "L\u00fcs\u00b7ten", "einst", "in", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es reute dich nicht sp\u00e4ter!", "tokens": ["Es", "reu\u00b7te", "dich", "nicht", "sp\u00e4\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Jetzt ist kein Gl\u00fcck in unserm Haus,", "tokens": ["Jetzt", "ist", "kein", "Gl\u00fcck", "in", "un\u00b7serm", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jetzt rufst du selber mit mir aus:", "tokens": ["Jetzt", "rufst", "du", "sel\u00b7ber", "mit", "mir", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Vaterland, mein Vaterland,", "tokens": ["Mein", "Va\u00b7ter\u00b7land", ",", "mein", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du hast zu viele V\u00e4ter!", "tokens": ["Du", "hast", "zu", "vie\u00b7le", "V\u00e4\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}