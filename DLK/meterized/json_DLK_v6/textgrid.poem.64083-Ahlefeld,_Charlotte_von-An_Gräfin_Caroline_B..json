{"textgrid.poem.64083": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "An Gr\u00e4fin Caroline B.", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Blumen Sprache m\u00f6chtest Du ergr\u00fcnden,", "tokens": ["Der", "Blu\u00b7men", "Spra\u00b7che", "m\u00f6ch\u00b7test", "Du", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um sanft in ihr Dein Innres zu ergiessen?", "tokens": ["Um", "sanft", "in", "ihr", "Dein", "Inn\u00b7res", "zu", "er\u00b7gies\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "APPR", "PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um in des Kranzes Harmonie zu winden", "tokens": ["Um", "in", "des", "Kran\u00b7zes", "Har\u00b7mo\u00b7nie", "zu", "win\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "APPR", "ART", "NN", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des ", "tokens": ["Des"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Die noch umh\u00fcllt von zarter Knospen Gr\u00fcn,", "tokens": ["Die", "noch", "um\u00b7h\u00fcllt", "von", "zar\u00b7ter", "Knos\u00b7pen", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nur leise Dir im Hauch der Ahndung bl\u00fchn.", "tokens": ["Nur", "lei\u00b7se", "Dir", "im", "Hauch", "der", "Ahn\u00b7dung", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Allein es ward mir nicht die Macht gegeben,", "tokens": ["Al\u00b7lein", "es", "ward", "mir", "nicht", "die", "Macht", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PPER", "PTKNEG", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu deuten Dir den seelenvollen Sinn,", "tokens": ["Zu", "deu\u00b7ten", "Dir", "den", "see\u00b7len\u00b7vol\u00b7len", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der in der Blumen still entsprosstem Leben", "tokens": ["Der", "in", "der", "Blu\u00b7men", "still", "ent\u00b7spross\u00b7tem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Uns zeigt der Mystik magischen Gewinn,", "tokens": ["Uns", "zeigt", "der", "Mys\u00b7tik", "ma\u00b7gi\u00b7schen", "Ge\u00b7winn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die im geheimnissvoll gewebten Schleier", "tokens": ["Die", "im", "ge\u00b7heim\u00b7niss\u00b7voll", "ge\u00b7web\u00b7ten", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Seele f\u00fcllt mit nahmenloser Feier.", "tokens": ["Die", "See\u00b7le", "f\u00fcllt", "mit", "nah\u00b7men\u00b7lo\u00b7ser", "Fei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich kenne nur der Blumen stilles Bl\u00fchen,", "tokens": ["Ich", "ken\u00b7ne", "nur", "der", "Blu\u00b7men", "stil\u00b7les", "Bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ihr Vergehn im Schoosse der Natur.", "tokens": ["Und", "ihr", "Ver\u00b7gehn", "im", "Schoos\u00b7se", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Im reinen Lichte einer sch\u00f6nern Flur,", "tokens": ["Im", "rei\u00b7nen", "Lich\u00b7te", "ei\u00b7ner", "sch\u00f6\u00b7nern", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und diese drei will ich Dir liebend brechen,", "tokens": ["Und", "die\u00b7se", "drei", "will", "ich", "Dir", "lie\u00b7bend", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "CARD", "VMFIN", "PPER", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Bedarfst Du mehr, Dein Innres auszusprechen? \u2013", "tokens": ["Be\u00b7darfst", "Du", "mehr", ",", "Dein", "Inn\u00b7res", "aus\u00b7zu\u00b7spre\u00b7chen", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "VVIZU", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So nimm denn aus des Sommers reicher F\u00fclle,", "tokens": ["So", "nimm", "denn", "aus", "des", "Som\u00b7mers", "rei\u00b7cher", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "APPR", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Lilie, der Unschuld Ebenbild,", "tokens": ["Die", "Li\u00b7lie", ",", "der", "Un\u00b7schuld", "E\u00b7ben\u00b7bild", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-++-+-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Die in der schimmerlosen, weissen H\u00fclle", "tokens": ["Die", "in", "der", "schim\u00b7mer\u00b7lo\u00b7sen", ",", "weis\u00b7sen", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Balsamodem spendet, s\u00fcss und mild.", "tokens": ["Den", "Bal\u00b7sa\u00b7mo\u00b7dem", "spen\u00b7det", ",", "s\u00fcss", "und", "mild", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In ihr kannst Du mit stillem Selbstvertrauen", "tokens": ["In", "ihr", "kannst", "Du", "mit", "stil\u00b7lem", "Selbst\u00b7ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dein eignes Ich in sch\u00f6ner Reinheit schauen.", "tokens": ["Dein", "eig\u00b7nes", "Ich", "in", "sch\u00f6\u00b7ner", "Rein\u00b7heit", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "PPER", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Die blaue Winde, die die zarten Ranken", "tokens": ["Die", "blau\u00b7e", "Win\u00b7de", ",", "die", "die", "zar\u00b7ten", "Ran\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im linden Hauche jedes L\u00fcftchens regt,", "tokens": ["Im", "lin\u00b7den", "Hau\u00b7che", "je\u00b7des", "L\u00fcft\u00b7chens", "regt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und seufzend s\u00e4uselt in dem steten Schwanken,", "tokens": ["Und", "seuf\u00b7zend", "s\u00e4u\u00b7selt", "in", "dem", "ste\u00b7ten", "Schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das ihrer Bl\u00fcthe tiefen Kelch bewegt \u2013", "tokens": ["Das", "ih\u00b7rer", "Bl\u00fc\u00b7the", "tie\u00b7fen", "Kelch", "be\u00b7wegt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie ist der Sehnsucht Bild, die \u2013 tief verschwistert", "tokens": ["Sie", "ist", "der", "Sehn\u00b7sucht", "Bild", ",", "die", "\u2013", "tief", "ver\u00b7schwis\u00b7tert"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,", "PRELS", "$(", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dem Sterblichen \u2013 in jedem Busen fl\u00fcstert.", "tokens": ["Dem", "Sterb\u00b7li\u00b7chen", "\u2013", "in", "je\u00b7dem", "Bu\u00b7sen", "fl\u00fcs\u00b7tert", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Die Liebe, die des Lebens Kronen windet,", "tokens": ["Die", "Lie\u00b7be", ",", "die", "des", "Le\u00b7bens", "Kro\u00b7nen", "win\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hat sich die Purpurrose vorbehalten.", "tokens": ["Hat", "sich", "die", "Pur\u00b7pur\u00b7ro\u00b7se", "vor\u00b7be\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn ihre Gluth der Lilie sich vorbindet,", "tokens": ["Wenn", "ih\u00b7re", "Gluth", "der", "Li\u00b7lie", "sich", "vor\u00b7bin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Muss sich des Daseyns h\u00f6chstes Gl\u00fcck gestalten.", "tokens": ["Muss", "sich", "des", "Da\u00b7seyns", "h\u00f6chs\u00b7tes", "Gl\u00fcck", "ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "In ihres Duftes wonnevollem Grusse", "tokens": ["In", "ih\u00b7res", "Duf\u00b7tes", "won\u00b7ne\u00b7vol\u00b7lem", "Grus\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ber\u00fchrt der Himmel uns mit s\u00fcssem Kusse.", "tokens": ["Be\u00b7r\u00fchrt", "der", "Him\u00b7mel", "uns", "mit", "s\u00fcs\u00b7sem", "Kus\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Mischt sich der Sehnsucht leicht erregtes Beben", "tokens": ["Mischt", "sich", "der", "Sehn\u00b7sucht", "leicht", "er\u00b7reg\u00b7tes", "Be\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADJD", "ADJA", "NN"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "In Deines Herzens ruhiges Entz\u00fccken,", "tokens": ["In", "Dei\u00b7nes", "Her\u00b7zens", "ru\u00b7hi\u00b7ges", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wenn Dir der Unschuld Genien das Leben", "tokens": ["Wenn", "Dir", "der", "Un\u00b7schuld", "Ge\u00b7ni\u00b7en", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Im Morgenglanz der Jugend l\u00e4chelnd schm\u00fccken,", "tokens": ["Im", "Mor\u00b7gen\u00b7glanz", "der", "Ju\u00b7gend", "l\u00e4\u00b7chelnd", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So dufte in der Zukunft dunklem Schoosse", "tokens": ["So", "duf\u00b7te", "in", "der", "Zu\u00b7kunft", "dunk\u00b7lem", "Schoos\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dir lohnend einst der Liebe Purpurrose.", "tokens": ["Dir", "loh\u00b7nend", "einst", "der", "Lie\u00b7be", "Pur\u00b7pur\u00b7ro\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}