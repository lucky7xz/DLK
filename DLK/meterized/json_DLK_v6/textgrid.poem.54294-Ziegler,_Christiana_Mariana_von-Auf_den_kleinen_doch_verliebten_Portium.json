{"textgrid.poem.54294": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "Auf den kleinen doch verliebten Portium", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie mag doch Portius, der denen Zwergen gleicht,", "tokens": ["Wie", "mag", "doch", "Por\u00b7tius", ",", "der", "de\u00b7nen", "Zwer\u00b7gen", "gleicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADV", "NE", "$,", "PRELS", "PDS", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und einer Ziegen kaum bi\u00df an die Kn\u00f6chel reicht,", "tokens": ["Und", "ei\u00b7ner", "Zie\u00b7gen", "kaum", "bi\u00df", "an", "die", "Kn\u00f6\u00b7chel", "reicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sich unter Frauenvolck offt recht verwegen mischen?", "tokens": ["Sich", "un\u00b7ter", "Frau\u00b7en\u00b7volck", "offt", "recht", "ver\u00b7we\u00b7gen", "mi\u00b7schen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Um hier und dar ein Hertz aus Sehnsucht wegzufischen.", "tokens": ["Um", "hier", "und", "dar", "ein", "Hertz", "aus", "Sehn\u00b7sucht", "weg\u00b7zu\u00b7fi\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "KON", "PAV", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So vielmahl ich nur ihn und die Statur betracht,", "tokens": ["So", "viel\u00b7mahl", "ich", "nur", "ihn", "und", "die", "Sta\u00b7tur", "be\u00b7tracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "PPER", "KON", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So vielmahl hab ich auch, und di\u00df mit Furcht, gedacht:", "tokens": ["So", "viel\u00b7mahl", "hab", "ich", "auch", ",", "und", "di\u00df", "mit", "Furcht", ",", "ge\u00b7dacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADV", "$,", "KON", "PDS", "APPR", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie leichte k\u00f6nte nicht der kleine Mensch auf Erden,", "tokens": ["Wie", "leich\u00b7te", "k\u00f6n\u00b7te", "nicht", "der", "klei\u00b7ne", "Mensch", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "VMFIN", "PTKNEG", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "In Reihen ohnversehns und Spiel getreten werden?", "tokens": ["In", "Rei\u00b7hen", "ohn\u00b7ver\u00b7sehns", "und", "Spiel", "ge\u00b7tre\u00b7ten", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "KON", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was Guckguck f\u00e4nget denn mit diesen kleinen Mann,", "tokens": ["Was", "Guck\u00b7guck", "f\u00e4n\u00b7get", "denn", "mit", "die\u00b7sen", "klei\u00b7nen", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "VVFIN", "ADV", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich w\u00fcst es w\u00fcrcklich nicht, ein Frauenzimmer an?", "tokens": ["Ich", "w\u00fcst", "es", "w\u00fcrck\u00b7lich", "nicht", ",", "ein", "Frau\u00b7en\u00b7zim\u00b7mer", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKNEG", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es w\u00e4re denn, da\u00df man ihn noch vor brauchbar sch\u00e4tzte,", "tokens": ["Es", "w\u00e4\u00b7re", "denn", ",", "da\u00df", "man", "ihn", "noch", "vor", "brauch\u00b7bar", "sch\u00e4tz\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "KOUS", "PIS", "PPER", "ADV", "APPR", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und statt des Gueridons mit zu dem Nach-Tisch setzte.", "tokens": ["Und", "statt", "des", "Gue\u00b7ri\u00b7dons", "mit", "zu", "dem", "Nach\u00b7Tisch", "setz\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Zwar mach ich leichte mir die Rechnung und den Schlu\u00df,", "tokens": ["Zwar", "mach", "ich", "leich\u00b7te", "mir", "die", "Rech\u00b7nung", "und", "den", "Schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "PPER", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Was ihn bey diesen Spott und Vorwurff sch\u00fctzen mu\u00df,", "tokens": ["Was", "ihn", "bey", "die\u00b7sen", "Spott", "und", "Vor\u00b7wurff", "sch\u00fct\u00b7zen", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PDAT", "NN", "KON", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Denn weil ihm Amors Bild und Abri\u00df deutlich weisset,", "tokens": ["Denn", "weil", "ihm", "A\u00b7mors", "Bild", "und", "Ab\u00b7ri\u00df", "deut\u00b7lich", "weis\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NE", "NN", "KON", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da\u00df seine Duodez-Statur ihm \u00e4hnlich heisset;", "tokens": ["Da\u00df", "sei\u00b7ne", "Duo\u00b7de\u00b7zSta\u00b7tur", "ihm", "\u00e4hn\u00b7lich", "heis\u00b7set", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "So bildet Portius sich steiff und sicher ein,", "tokens": ["So", "bil\u00b7det", "Por\u00b7tius", "sich", "steiff", "und", "si\u00b7cher", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "PRF", "VVFIN", "KON", "ADJD", "PTKVZ", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Er m\u00fcst auch eben so beliebt, wie jener, seyn;", "tokens": ["Er", "m\u00fcst", "auch", "e\u00b7ben", "so", "be\u00b7liebt", ",", "wie", "je\u00b7ner", ",", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ADV", "ADJD", "$,", "PWAV", "PDS", "$,", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Allein er kan sich nicht mit diesen Vorwand sch\u00fctzen,", "tokens": ["Al\u00b7lein", "er", "kan", "sich", "nicht", "mit", "die\u00b7sen", "Vor\u00b7wand", "sch\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "PRF", "PTKNEG", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Dieweil er nicht, wie der, kan Liebes-Pfeile schnitzen.", "tokens": ["Die\u00b7weil", "er", "nicht", ",", "wie", "der", ",", "kan", "Lie\u00b7bes\u00b7Pfei\u00b7le", "schnit\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "$,", "PWAV", "ART", "$,", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Inzwischen lacht man doch recht hertzlich \u00fcber ihn,", "tokens": ["I\u00b7nzwi\u00b7schen", "lacht", "man", "doch", "recht", "hertz\u00b7lich", "\u00fc\u00b7ber", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ADJD", "ADJD", "APPR", "PPER", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.22": {"text": "Wenn man di\u00df kleine Thier sich eifrig sieht bem\u00fchn,", "tokens": ["Wenn", "man", "di\u00df", "klei\u00b7ne", "Thier", "sich", "eif\u00b7rig", "sieht", "be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PDS", "ADJA", "NN", "PRF", "ADJD", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ein K\u00fc\u00dfgen in dem Spiel dem Frauen-Volck zu stehlen,", "tokens": ["Ein", "K\u00fc\u00df\u00b7gen", "in", "dem", "Spiel", "dem", "Frau\u00b7en\u00b7Volck", "zu", "steh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wobey die Leiter offt, das beste, pflegt zu fehlen.", "tokens": ["Wo\u00b7bey", "die", "Lei\u00b7ter", "offt", ",", "das", "bes\u00b7te", ",", "pflegt", "zu", "feh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "$,", "ART", "ADJA", "$,", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Drum k\u00f6mmt er in der That, ich schw\u00f6r es sicher, mir", "tokens": ["Drum", "k\u00f6mmt", "er", "in", "der", "That", ",", "ich", "schw\u00f6r", "es", "si\u00b7cher", ",", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$,", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Nicht anders, als ein klein und junges R\u00e4upchen f\u00fcr,", "tokens": ["Nicht", "an\u00b7ders", ",", "als", "ein", "klein", "und", "jun\u00b7ges", "R\u00e4upc\u00b7hen", "f\u00fcr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "KOUS", "ART", "ADJD", "KON", "ADJA", "NN", "APPR", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Das man bey d\u00fcrrer Zeit von einen Blat zum andern,", "tokens": ["Das", "man", "bey", "d\u00fcr\u00b7rer", "Zeit", "von", "ei\u00b7nen", "Blat", "zum", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "APPR", "ADJA", "NN", "APPR", "ART", "NN", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Weil es der Hunger dr\u00fcckt, sieht recht erb\u00e4rmlich wandern.", "tokens": ["Weil", "es", "der", "Hun\u00b7ger", "dr\u00fcckt", ",", "sieht", "recht", "er\u00b7b\u00e4rm\u00b7lich", "wan\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "VVFIN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ach stelle, Portius, ja dein Charmiren ein,", "tokens": ["Ach", "stel\u00b7le", ",", "Por\u00b7tius", ",", "ja", "dein", "Char\u00b7mi\u00b7ren", "ein", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$,", "NE", "$,", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Dein Hoffen ist umsonst, du wirst nicht gl\u00fccklich seyn.", "tokens": ["Dein", "Hof\u00b7fen", "ist", "um\u00b7sonst", ",", "du", "wirst", "nicht", "gl\u00fcck\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$,", "PPER", "VAFIN", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wer wolt, du N\u00e4rrchen, sich mit einen Mann bepacken", "tokens": ["Wer", "wolt", ",", "du", "N\u00e4rr\u00b7chen", ",", "sich", "mit", "ei\u00b7nen", "Mann", "be\u00b7pa\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "$,", "PPER", "NN", "$,", "PRF", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Den, lie\u00df man ihn allein, der Hau\u00df-Hahn k\u00f6nte hacken.", "tokens": ["Den", ",", "lie\u00df", "man", "ihn", "al\u00b7lein", ",", "der", "Hau\u00df\u00b7Hahn", "k\u00f6n\u00b7te", "ha\u00b7cken", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PIS", "PPER", "ADV", "$,", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Doch, wenn die Liebe dich zu sehre plagt und dr\u00fcckt,", "tokens": ["Doch", ",", "wenn", "die", "Lie\u00b7be", "dich", "zu", "seh\u00b7re", "plagt", "und", "dr\u00fcckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "NN", "PPER", "PTKZU", "VVFIN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "So wei\u00df ich, wer sich wohl vor dich am besten schickt:", "tokens": ["So", "wei\u00df", "ich", ",", "wer", "sich", "wohl", "vor", "dich", "am", "bes\u00b7ten", "schickt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWS", "PRF", "ADV", "APPR", "PPER", "PTKA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Du mu\u00dft, ich rath es dir, zu den Pygm\u00e4en reisen,", "tokens": ["Du", "mu\u00dft", ",", "ich", "rath", "es", "dir", ",", "zu", "den", "Pyg\u00b7m\u00e4\u00b7en", "rei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VVFIN", "PPER", "PPER", "$,", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Da wird kein Nympfgen dir, wie hier, ein K\u00f6rbgen weisen.", "tokens": ["Da", "wird", "kein", "Nympf\u00b7gen", "dir", ",", "wie", "hier", ",", "ein", "K\u00f6rb\u00b7gen", "wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "PPER", "$,", "PWAV", "ADV", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}