{"textgrid.poem.35719": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "43. Die Spinnerin", "genre": "verse", "period": "N.A.", "pub_year": 1791, "urn": "N.A.", "language": ["de:0.85", "id:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sa\u00df und spann vor meiner Th\u00fcr:", "tokens": ["Ich", "sa\u00df", "und", "spann", "vor", "mei\u00b7ner", "Th\u00fcr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da kam ein junger Mann gegangen.", "tokens": ["Da", "kam", "ein", "jun\u00b7ger", "Mann", "ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sein braunes Auge lachte mir,", "tokens": ["Sein", "brau\u00b7nes", "Au\u00b7ge", "lach\u00b7te", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und r\u00f6ter gl\u00fchten seine Wangen.", "tokens": ["Und", "r\u00f6\u00b7ter", "gl\u00fch\u00b7ten", "sei\u00b7ne", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich sah vom Rocken auf, und sann,", "tokens": ["Ich", "sah", "vom", "Ro\u00b7cken", "auf", ",", "und", "sann", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und sa\u00df versch\u00e4mt, und spann und spann", "tokens": ["Und", "sa\u00df", "ver\u00b7sch\u00e4mt", ",", "und", "spann", "und", "spann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "$,", "KON", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Gar freundlich bot er guten Tag,", "tokens": ["Gar", "freund\u00b7lich", "bot", "er", "gu\u00b7ten", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und trat mit holder Scheu mir n\u00e4her.", "tokens": ["Und", "trat", "mit", "hol\u00b7der", "Scheu", "mir", "n\u00e4\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mir ward so angst; der Faden brach;", "tokens": ["Mir", "ward", "so", "angst", ";", "der", "Fa\u00b7den", "brach", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Herz im Busen schlug mir h\u00f6her.", "tokens": ["Das", "Herz", "im", "Bu\u00b7sen", "schlug", "mir", "h\u00f6\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Betroffen kn\u00fcpft' ich wieder an,", "tokens": ["Be\u00b7trof\u00b7fen", "kn\u00fcpft'", "ich", "wie\u00b7der", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und sa\u00df versch\u00e4mt, und spann und spann.", "tokens": ["Und", "sa\u00df", "ver\u00b7sch\u00e4mt", ",", "und", "spann", "und", "spann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Liebkosend dr\u00fcckt' er mir die Hand,", "tokens": ["Lieb\u00b7ko\u00b7send", "dr\u00fcckt'", "er", "mir", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schwur, da\u00df keine Hand ihr gleiche,", "tokens": ["Und", "schwur", ",", "da\u00df", "kei\u00b7ne", "Hand", "ihr", "glei\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PIAT", "NN", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die sch\u00f6nste nicht im ganzen Land,", "tokens": ["Die", "sch\u00f6ns\u00b7te", "nicht", "im", "gan\u00b7zen", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PTKNEG", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An Schwanenwei\u00df' und R\u00fcnd' und Weiche.", "tokens": ["An", "Schwa\u00b7nen\u00b7wei\u00df'", "und", "R\u00fcnd'", "und", "Wei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie sehr dies Lob mein Herz gewann;", "tokens": ["Wie", "sehr", "dies", "Lob", "mein", "Herz", "ge\u00b7wann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PDS", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich sa\u00df versch\u00e4mt, und spann und spann.", "tokens": ["Ich", "sa\u00df", "ver\u00b7sch\u00e4mt", ",", "und", "spann", "und", "spann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er lehnt' auf meinen Stuhl den Arm,", "tokens": ["Er", "lehnt'", "auf", "mei\u00b7nen", "Stuhl", "den", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und r\u00fchmte sehr das feine F\u00e4dchen.", "tokens": ["Und", "r\u00fchm\u00b7te", "sehr", "das", "fei\u00b7ne", "F\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sein naher Mund, so rot und warm,", "tokens": ["Sein", "na\u00b7her", "Mund", ",", "so", "rot", "und", "warm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie z\u00e4rtlich haucht' er: S\u00fc\u00dfes M\u00e4dchen!", "tokens": ["Wie", "z\u00e4rt\u00b7lich", "haucht'", "er", ":", "S\u00fc\u00b7\u00dfes", "M\u00e4d\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie blickte mich sein Auge an!", "tokens": ["Wie", "blick\u00b7te", "mich", "sein", "Au\u00b7ge", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich sa\u00df versch\u00e4mt, und spann und spann.", "tokens": ["Ich", "sa\u00df", "ver\u00b7sch\u00e4mt", ",", "und", "spann", "und", "spann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Indes an meiner Wange her", "tokens": ["In\u00b7des", "an", "mei\u00b7ner", "Wan\u00b7ge", "her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein sch\u00f6nes Angesicht sich b\u00fcckte,", "tokens": ["Sein", "sch\u00f6\u00b7nes", "An\u00b7ge\u00b7sicht", "sich", "b\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Begegnet' ihm von ohngef\u00e4hr", "tokens": ["Be\u00b7geg\u00b7net'", "ihm", "von", "ohn\u00b7ge\u00b7f\u00e4hr"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJD"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "Mein Haupt, das sanft im Spinnen nickte;", "tokens": ["Mein", "Haupt", ",", "das", "sanft", "im", "Spin\u00b7nen", "nick\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da k\u00fc\u00dfte mich der sch\u00f6ne Mann.", "tokens": ["Da", "k\u00fc\u00df\u00b7te", "mich", "der", "sch\u00f6\u00b7ne", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich sa\u00df versch\u00e4mt, und spann und spann.", "tokens": ["Ich", "sa\u00df", "ver\u00b7sch\u00e4mt", ",", "und", "spann", "und", "spann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Mit gro\u00dfem Ernst verwies ich's ihm;", "tokens": ["Mit", "gro\u00b7\u00dfem", "Ernst", "ver\u00b7wies", "ich's", "ihm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PIS", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch ward er k\u00fchner stets und freier,", "tokens": ["Doch", "ward", "er", "k\u00fch\u00b7ner", "stets", "und", "frei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "ADV", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Umarmte mich voll Ungest\u00fcm,", "tokens": ["Um\u00b7arm\u00b7te", "mich", "voll", "Un\u00b7ge\u00b7st\u00fcm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und k\u00fc\u00dfte mich so rot wie Feuer.", "tokens": ["Und", "k\u00fc\u00df\u00b7te", "mich", "so", "rot", "wie", "Feu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "O sagt mir, Schwestern, sagt mir an:", "tokens": ["O", "sagt", "mir", ",", "Schwes\u00b7tern", ",", "sagt", "mir", "an", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "War's m\u00f6glich, da\u00df ich weiter spann?", "tokens": ["Wa\u00b7r's", "m\u00f6g\u00b7lich", ",", "da\u00df", "ich", "wei\u00b7ter", "spann", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}