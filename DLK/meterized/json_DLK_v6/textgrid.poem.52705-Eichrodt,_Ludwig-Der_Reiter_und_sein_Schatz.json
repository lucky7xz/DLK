{"textgrid.poem.52705": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Reiter und sein Schatz", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trompeten blasen, Fahnen wehn,", "tokens": ["Trom\u00b7pe\u00b7ten", "bla\u00b7sen", ",", "Fah\u00b7nen", "wehn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ade mein Schatz, leb wohl!", "tokens": ["A\u00b7de", "mein", "Schatz", ",", "leb", "wohl", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ich bin ein armer Reitersmann", "tokens": ["Ich", "bin", "ein", "ar\u00b7mer", "Rei\u00b7ters\u00b7mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit S\u00e4bel und Pistol;", "tokens": ["Mit", "S\u00e4\u00b7bel", "und", "Pis\u00b7tol", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Mein Ro\u00df, mein Rock, mein blank Gewehr,", "tokens": ["Mein", "Ro\u00df", ",", "mein", "Rock", ",", "mein", "blank", "Ge\u00b7wehr", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Leben ist nicht mein,", "tokens": ["Mein", "Le\u00b7ben", "ist", "nicht", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch habe nur Vertraun zu mir,", "tokens": ["Doch", "ha\u00b7be", "nur", "Ver\u00b7traun", "zu", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mein Herz, o Schatz, ist dein!", "tokens": ["Mein", "Herz", ",", "o", "Schatz", ",", "ist", "dein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "NN", "$,", "VAFIN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Vertrauen dir in Ewigkeit,", "tokens": ["Ver\u00b7trau\u00b7en", "dir", "in", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du lieber trotzger Mann,", "tokens": ["Du", "lie\u00b7ber", "trotz\u00b7ger", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Seit ich dich nur mit Augen schaut,", "tokens": ["Seit", "ich", "dich", "nur", "mit", "Au\u00b7gen", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das hab ich ja gethan;", "tokens": ["Das", "hab", "ich", "ja", "ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch ach! Vertraun, was hilft das uns,", "tokens": ["Doch", "ach", "!", "Ver\u00b7traun", ",", "was", "hilft", "das", "uns", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "VVINF", "$,", "PWS", "VVFIN", "ART", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Schicksal ist so karg,", "tokens": ["Das", "Schick\u00b7sal", "ist", "so", "karg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und mein Vertraun, ach, ist am End", "tokens": ["Und", "mein", "Ver\u00b7traun", ",", "ach", ",", "ist", "am", "End"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "ITJ", "$,", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Nagel an deinem Sarg.", "tokens": ["Der", "Na\u00b7gel", "an", "dei\u00b7nem", "Sarg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "O Lieb, es ist kein leeres Wort,", "tokens": ["O", "Lieb", ",", "es", "ist", "kein", "lee\u00b7res", "Wort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Treue bis zum Tod;", "tokens": ["Die", "Treu\u00b7e", "bis", "zum", "Tod", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das macht mich unersch\u00fctterlich", "tokens": ["Das", "macht", "mich", "un\u00b7er\u00b7sch\u00fct\u00b7ter\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr Alles, was uns droht.", "tokens": ["F\u00fcr", "Al\u00b7les", ",", "was", "uns", "droht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Um dich, du Engel, o wie gern", "tokens": ["Um", "dich", ",", "du", "En\u00b7gel", ",", "o", "wie", "gern"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUI", "PPER", "$,", "PPER", "NE", "$,", "FM", "KOKOM", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ertrag ich jede Pein,", "tokens": ["Er\u00b7trag", "ich", "je\u00b7de", "Pein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "La\u00df uns der liebeleeren Welt", "tokens": ["La\u00df", "uns", "der", "lie\u00b7be\u00b7lee\u00b7ren", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein gro\u00dfes Beispiel sein!", "tokens": ["Ein", "gro\u00b7\u00dfes", "Bei\u00b7spiel", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Hochm\u00fcthig ist die Welt und kalt,", "tokens": ["Hoch\u00b7m\u00fct\u00b7hig", "ist", "die", "Welt", "und", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lacht vornehm unsrer Qual,", "tokens": ["Lacht", "vor\u00b7nehm", "uns\u00b7rer", "Qual", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Zu lieben hat sie ganz verlernt,", "tokens": ["Zu", "lie\u00b7ben", "hat", "sie", "ganz", "ver\u00b7lernt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ahnt es nicht einmal;", "tokens": ["Und", "ahnt", "es", "nicht", "ein\u00b7mal", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie rechnet treue Lieb sogar", "tokens": ["Sie", "rech\u00b7net", "treu\u00b7e", "Lieb", "so\u00b7gar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zum bittern Schimpf uns an,", "tokens": ["Zum", "bit\u00b7tern", "Schimpf", "uns", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und nimmer traut sie ihr und ihm", "tokens": ["Und", "nim\u00b7mer", "traut", "sie", "ihr", "und", "ihm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "KON", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Was Gutes zu fortan.", "tokens": ["Was", "Gu\u00b7tes", "zu", "for\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PTKZU", "PTKVZ", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "O weine nicht, mein theures Kind,", "tokens": ["O", "wei\u00b7ne", "nicht", ",", "mein", "theu\u00b7res", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was hat man dir geraubt?", "tokens": ["Was", "hat", "man", "dir", "ge\u00b7raubt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PIS", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Am Ende kriegt die Welt Respekt", "tokens": ["Am", "En\u00b7de", "kriegt", "die", "Welt", "Res\u00b7pekt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor dem, was sie nicht glaubt.", "tokens": ["Vor", "dem", ",", "was", "sie", "nicht", "glaubt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Welt, sie traut sich selber nicht,", "tokens": ["Die", "Welt", ",", "sie", "traut", "sich", "sel\u00b7ber", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "PRF", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist selber ohne Halt,", "tokens": ["Ist", "sel\u00b7ber", "oh\u00b7ne", "Halt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wenn Einer nur was T\u00fcchtigs will,", "tokens": ["Wenn", "Ei\u00b7ner", "nur", "was", "T\u00fcch\u00b7tigs", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "PWS", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wird sie gar feig alsbald.", "tokens": ["Wird", "sie", "gar", "feig", "als\u00b7bald", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "So zieh hinaus! ich bleib dir treu!", "tokens": ["So", "zieh", "hin\u00b7aus", "!", "ich", "bleib", "dir", "treu", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sei wacker im Gefecht!", "tokens": ["Sei", "wa\u00b7cker", "im", "Ge\u00b7fecht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein M\u00e4dchen hast du gl\u00fccklich gemacht,", "tokens": ["Dein", "M\u00e4d\u00b7chen", "hast", "du", "gl\u00fcck\u00b7lich", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und bist ihr immer recht.", "tokens": ["Und", "bist", "ihr", "im\u00b7mer", "recht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie zaget nicht, sie klaget nicht,", "tokens": ["Sie", "za\u00b7get", "nicht", ",", "sie", "kla\u00b7get", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PPER", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie greint nicht ohne Noth,", "tokens": ["Sie", "greint", "nicht", "oh\u00b7ne", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und nur wenn du nicht wieder kommst,", "tokens": ["Und", "nur", "wenn", "du", "nicht", "wie\u00b7der", "kommst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dann weint sie sich zu Tod.", "tokens": ["Dann", "weint", "sie", "sich", "zu", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Trompeten blasen, Fahnen wehn,", "tokens": ["Trom\u00b7pe\u00b7ten", "bla\u00b7sen", ",", "Fah\u00b7nen", "wehn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jetzt hab ich gute Ruh,", "tokens": ["Jetzt", "hab", "ich", "gu\u00b7te", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und alle Kugeln treffen nicht,", "tokens": ["Und", "al\u00b7le", "Ku\u00b7geln", "tref\u00b7fen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du lieber Engel du!", "tokens": ["Du", "lie\u00b7ber", "En\u00b7gel", "du", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und trifft es mich und wirft es mich", "tokens": ["Und", "trifft", "es", "mich", "und", "wirft", "es", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PRF", "KON", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fcr immer auf den Platz,", "tokens": ["F\u00fcr", "im\u00b7mer", "auf", "den", "Platz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So bettle dich zu meinem Grab", "tokens": ["So", "bett\u00b7le", "dich", "zu", "mei\u00b7nem", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und stirb bei deinem Schatz!", "tokens": ["Und", "stirb", "bei", "dei\u00b7nem", "Schatz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}