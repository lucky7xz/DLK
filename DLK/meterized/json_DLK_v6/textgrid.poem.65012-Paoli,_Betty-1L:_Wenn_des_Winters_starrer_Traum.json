{"textgrid.poem.65012": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn des Winters starrer Traum", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn des Winters starrer Traum", "tokens": ["Wenn", "des", "Win\u00b7ters", "star\u00b7rer", "Traum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Berg und Flur mit Schnee bedecket,", "tokens": ["Berg", "und", "Flur", "mit", "Schnee", "be\u00b7de\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jeder d\u00fcrre Zweig am Baum", "tokens": ["Je\u00b7der", "d\u00fcr\u00b7re", "Zweig", "am", "Baum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jammernd sich gen Himmel strecket:", "tokens": ["Jam\u00b7mernd", "sich", "gen", "Him\u00b7mel", "stre\u00b7cket", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Kannst du da begreifen, sag'", "tokens": ["Kannst", "du", "da", "be\u00b7grei\u00b7fen", ",", "sag'"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$,", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie nach wen'gen Mondesneigen", "tokens": ["Wie", "nach", "wen'\u00b7gen", "Mon\u00b7des\u00b7nei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der jetzt frosterstarrte Hag", "tokens": ["Der", "jetzt", "fros\u00b7ter\u00b7starr\u00b7te", "Hag"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen Bl\u00fcthenflor wird zeigen?", "tokens": ["Ei\u00b7nen", "Bl\u00fc\u00b7then\u00b7flor", "wird", "zei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch du wei\u00dft, der lichte Trost", "tokens": ["Doch", "du", "wei\u00dft", ",", "der", "lich\u00b7te", "Trost"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Naht auf unsichtbaren Wegen", "tokens": ["Naht", "auf", "un\u00b7sicht\u00b7ba\u00b7ren", "We\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und im rauhen Winterfrost", "tokens": ["Und", "im", "rau\u00b7hen", "Win\u00b7ter\u00b7frost"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4chelst du dem Lenz entgegen.", "tokens": ["L\u00e4\u00b7chelst", "du", "dem", "Lenz", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und so kann, so kann auch ich", "tokens": ["Und", "so", "kann", ",", "so", "kann", "auch", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "$,", "ADV", "VMFIN", "ADV", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht begreifen und nicht fassen,", "tokens": ["Nicht", "be\u00b7grei\u00b7fen", "und", "nicht", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "KON", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie in meiner Seele sich", "tokens": ["Wie", "in", "mei\u00b7ner", "See\u00b7le", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PPOSAT", "NN", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch ein Gl\u00fcck wird ziehen lassen.", "tokens": ["Noch", "ein", "Gl\u00fcck", "wird", "zie\u00b7hen", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch ich wei\u00df: zur Wonne geht,", "tokens": ["Doch", "ich", "wei\u00df", ":", "zur", "Won\u00b7ne", "geht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wer da wallt auf Dornenbahnen,", "tokens": ["Wer", "da", "wallt", "auf", "Dor\u00b7nen\u00b7bah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und durch meinen Winter weht", "tokens": ["Und", "durch", "mei\u00b7nen", "Win\u00b7ter", "weht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein tief selig Fr\u00fchlingsahnen!", "tokens": ["Ein", "tief", "se\u00b7lig", "Fr\u00fch\u00b7lin\u00b7gsah\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}