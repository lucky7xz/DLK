{"dta.poem.14241": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Einsame .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Im beschr\u00e4nkten Kreis der H\u00fcgel,", "tokens": ["Im", "be\u00b7schr\u00e4nk\u00b7ten", "Kreis", "der", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus des stillen Weihers Spiegel", "tokens": ["Aus", "des", "stil\u00b7len", "Wei\u00b7hers", "Spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Scheue, fromme Silberschw\u00e4ne \u2014", "tokens": ["Scheu\u00b7e", ",", "from\u00b7me", "Sil\u00b7ber\u00b7schw\u00e4\u00b7ne"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fassend in des Rosses M\u00e4hne", "tokens": ["Fas\u00b7send", "in", "des", "Ros\u00b7ses", "M\u00e4h\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit dem Liebsten k\u00fchn im B\u00fcgel \u2014", "tokens": ["Mit", "dem", "Liebs\u00b7ten", "k\u00fchn", "im", "B\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "APPRART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bl\u00f6de Bande \u2014 muth'ge Fl\u00fcgel", "tokens": ["Bl\u00f6\u00b7de", "Ban\u00b7de", "mu