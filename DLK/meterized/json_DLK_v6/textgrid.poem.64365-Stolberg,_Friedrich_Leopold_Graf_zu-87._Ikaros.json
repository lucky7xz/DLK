{"textgrid.poem.64365": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "87. Ikaros", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Daidalos hub sich auf w\u00e4chsernem Fittich,", "tokens": ["Dai\u00b7da\u00b7los", "hub", "sich", "auf", "w\u00e4ch\u00b7ser\u00b7nem", "Fit\u00b7tich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+---+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ikaros weinete: Vater, dich bitt' ich,", "tokens": ["I\u00b7ka\u00b7ros", "wei\u00b7ne\u00b7te", ":", "Va\u00b7ter", ",", "dich", "bitt'", "ich", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "NN", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Lehre mich fliegen! Doch Daidalos sprach:", "tokens": ["Leh\u00b7re", "mich", "flie\u00b7gen", "!", "Doch", "Dai\u00b7da\u00b7los", "sprach", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVINF", "$.", "KON", "NN", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "H\u00e4ttest du Fl\u00fcgel, so fl\u00f6gst du mir nach!", "tokens": ["H\u00e4t\u00b7test", "du", "Fl\u00fc\u00b7gel", ",", "so", "fl\u00f6gst", "du", "mir", "nach", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber dem Ikaros braust es und pocht es", "tokens": ["A\u00b7ber", "dem", "I\u00b7ka\u00b7ros", "braust", "es", "und", "pocht", "es"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NE", "VVFIN", "PPER", "KON", "VVFIN", "PPER"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Tief in dem Herzen, und saust es und kocht es", "tokens": ["Tief", "in", "dem", "Her\u00b7zen", ",", "und", "saust", "es", "und", "kocht", "es"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "KON", "VVFIN", "PPER"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Hoch in dem K\u00f6pfchen: Du bildetest dir", "tokens": ["Hoch", "in", "dem", "K\u00f6pf\u00b7chen", ":", "Du", "bil\u00b7de\u00b7test", "dir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "PPER"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Fl\u00fcgel, so bilde doch Fl\u00fcgel auch mir!", "tokens": ["Fl\u00fc\u00b7gel", ",", "so", "bil\u00b7de", "doch", "Fl\u00fc\u00b7gel", "auch", "mir", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "ADV", "NN", "ADV", "PPER", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Nun denn, mein B\u00fcbchen, sollst Fl\u00fcgelein haben,", "tokens": ["Nun", "denn", ",", "mein", "B\u00fcb\u00b7chen", ",", "sollst", "Fl\u00fc\u00b7ge\u00b7lein", "ha\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PPOSAT", "NN", "$,", "VMFIN", "NN", "VAFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sagte der Vater zum wimmernden Knaben,", "tokens": ["Sag\u00b7te", "der", "Va\u00b7ter", "zum", "wim\u00b7mern\u00b7den", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Schritt zu der k\u00fcnstlichen Arbeit, und stracks", "tokens": ["Schritt", "zu", "der", "k\u00fcnst\u00b7li\u00b7chen", "Ar\u00b7beit", ",", "und", "stracks"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,", "KON", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Knetete Daidalos Fl\u00fcgel aus Wachs.", "tokens": ["Kne\u00b7te\u00b7te", "Dai\u00b7da\u00b7los", "Fl\u00fc\u00b7gel", "aus", "Wachs", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "APPR", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Ikaros bebet vor Wonn' und Verlangen,", "tokens": ["I\u00b7ka\u00b7ros", "be\u00b7bet", "vor", "Wonn'", "und", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+---+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Als ihm der Vater mit goldenen Spangen", "tokens": ["Als", "ihm", "der", "Va\u00b7ter", "mit", "gol\u00b7de\u00b7nen", "Span\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Heftet die Fl\u00fcgel an Schulter und Brust,", "tokens": ["Hef\u00b7tet", "die", "Fl\u00fc\u00b7gel", "an", "Schul\u00b7ter", "und", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Ist sich der Ammenmilch nicht mehr bewu\u00dft.", "tokens": ["Ist", "sich", "der", "Am\u00b7men\u00b7milch", "nicht", "mehr", "be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ART", "NN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "H\u00f6re, mein S\u00f6hnchen, der Klugheit bedarf es", "tokens": ["H\u00f6\u00b7re", ",", "mein", "S\u00f6hn\u00b7chen", ",", "der", "Klug\u00b7heit", "be\u00b7darf", "es"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "PPER"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Oben in L\u00fcften, drum achte mein scharfes", "tokens": ["O\u00b7ben", "in", "L\u00fcf\u00b7ten", ",", "drum", "ach\u00b7te", "mein", "schar\u00b7fes"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "$,", "PAV", "ADJA", "PPOSAT", "ADJA"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Weises Verbot, und bedenk nicht zu sp\u00e4t,", "tokens": ["Wei\u00b7ses", "Ver\u00b7bot", ",", "und", "be\u00b7denk", "nicht", "zu", "sp\u00e4t", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "VVFIN", "PTKNEG", "PTKA", "ADJD", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Da\u00df man aus Wachs nur was w\u00e4chsernes dreht.", "tokens": ["Da\u00df", "man", "aus", "Wachs", "nur", "was", "w\u00e4ch\u00b7ser\u00b7nes", "dreht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "ADV", "PWS", "ADJA", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Folge mir nach in den mittleren L\u00fcften,", "tokens": ["Fol\u00b7ge", "mir", "nach", "in", "den", "mitt\u00b7le\u00b7ren", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wittere nicht nach \u00e4therischen D\u00fcften,", "tokens": ["Wit\u00b7te\u00b7re", "nicht", "nach", "\u00e4t\u00b7he\u00b7ri\u00b7schen", "D\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "+----+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Weit ist die Reise nach Welschland und schwer,", "tokens": ["Weit", "ist", "die", "Rei\u00b7se", "nach", "Wel\u00b7schland", "und", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "APPR", "NN", "KON", "ADJD", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Oben die Sonne, und unten das Meer.", "tokens": ["O\u00b7ben", "die", "Son\u00b7ne", ",", "und", "un\u00b7ten", "das", "Meer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "KON", "ADV", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.7": {"line.1": {"text": "Nahest du steigend der Sonne, so schmelzen", "tokens": ["Na\u00b7hest", "du", "stei\u00b7gend", "der", "Son\u00b7ne", ",", "so", "schmel\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "$,", "ADV", "VVINF"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Flugs dir die Fittiche; tauchst du, so w\u00e4lzen", "tokens": ["Flugs", "dir", "die", "Fit\u00b7ti\u00b7che", ";", "tauchst", "du", ",", "so", "w\u00e4l\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PPER", "ART", "NN", "$.", "VVFIN", "PPER", "$,", "ADV", "VVINF"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Wogen des Meers dich ins gl\u00e4serne Haus", "tokens": ["Wo\u00b7gen", "des", "Meers", "dich", "ins", "gl\u00e4\u00b7ser\u00b7ne", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "PPER", "APPRART", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Wilder Tritonen, den Hechten zum Schmaus.", "tokens": ["Wil\u00b7der", "Tri\u00b7to\u00b7nen", ",", "den", "Hech\u00b7ten", "zum", "Schmaus", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.8": {"line.1": {"text": "Daidalos sprach es, und hub sich; der Junker", "tokens": ["Dai\u00b7da\u00b7los", "sprach", "es", ",", "und", "hub", "sich", ";", "der", "Jun\u00b7ker"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "KON", "VVFIN", "PRF", "$.", "ART", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "F\u00fchlte sich nicht vor Entz\u00fccken! K\u00fchn schwung er", "tokens": ["F\u00fchl\u00b7te", "sich", "nicht", "vor", "Ent\u00b7z\u00fc\u00b7cken", "!", "K\u00fchn", "schwung", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PRF", "PTKNEG", "APPR", "NN", "$.", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Nach sich dem Vater, hoch \u00fcber ihn hin,", "tokens": ["Nach", "sich", "dem", "Va\u00b7ter", ",", "hoch", "\u00fc\u00b7ber", "ihn", "hin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "$,", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "\u00dcber die Wolken mit trunkenem Sinn!", "tokens": ["\u00dc\u00b7ber", "die", "Wol\u00b7ken", "mit", "trun\u00b7ke\u00b7nem", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.9": {"line.1": {"text": "Aber die Fl\u00fcgel begannen zu triefen,", "tokens": ["A\u00b7ber", "die", "Fl\u00fc\u00b7gel", "be\u00b7gan\u00b7nen", "zu", "trie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "PTKZU", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Eh er es merkte, die Schwingen entliefen", "tokens": ["Eh", "er", "es", "merk\u00b7te", ",", "die", "Schwin\u00b7gen", "ent\u00b7lie\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "ART", "NN", "VVINF"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Sinkend dem Sinkenden, und er entst\u00fcrzt", "tokens": ["Sin\u00b7kend", "dem", "Sin\u00b7ken\u00b7den", ",", "und", "er", "ent\u00b7st\u00fcrzt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "$,", "KON", "PPER", "VVFIN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Purzelnd dem Flug, nicht zur Seefahrt gesch\u00fcrzt.", "tokens": ["Pur\u00b7zelnd", "dem", "Flug", ",", "nicht", "zur", "See\u00b7fahrt", "ge\u00b7sch\u00fcrzt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,", "PTKNEG", "APPRART", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.10": {"line.1": {"text": "Merke sich das der Bescheidneren Tadler,", "tokens": ["Mer\u00b7ke", "sich", "das", "der", "Be\u00b7scheid\u00b7ne\u00b7ren", "Tad\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PRF", "PDS", "ART", "NN", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Keck ist der K\u00e4fer, und k\u00fchn ist der Adler!", "tokens": ["Keck", "ist", "der", "K\u00e4\u00b7fer", ",", "und", "k\u00fchn", "ist", "der", "Ad\u00b7ler", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "KON", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Haben die G\u00f6tter dir Schwingen versagt,", "tokens": ["Ha\u00b7ben", "die", "G\u00f6t\u00b7ter", "dir", "Schwin\u00b7gen", "ver\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "NN", "VVPP", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "O so geh nicht auf \u00e4therische Jagd!", "tokens": ["O", "so", "geh", "nicht", "auf", "\u00e4t\u00b7he\u00b7ri\u00b7sche", "Jagd", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}}}}