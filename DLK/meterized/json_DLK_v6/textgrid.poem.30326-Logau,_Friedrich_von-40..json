{"textgrid.poem.30326": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "40.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Mensch, der nichts kan f\u00fcr sich selbst, wil immer doch auff Menschen bauen,", "tokens": ["Der", "Mensch", ",", "der", "nichts", "kan", "f\u00fcr", "sich", "selbst", ",", "wil", "im\u00b7mer", "doch", "auff", "Men\u00b7schen", "bau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VMFIN", "APPR", "PRF", "ADV", "$,", "VMFIN", "ADV", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "Wil Gott, der aber alles kan, noch dennoch selten viel vertrauen;", "tokens": ["Wil", "Gott", ",", "der", "a\u00b7ber", "al\u00b7les", "kan", ",", "noch", "den\u00b7noch", "sel\u00b7ten", "viel", "ver\u00b7trau\u00b7en", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "$,", "PRELS", "ADV", "PIS", "VMFIN", "$,", "ADV", "ADV", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.3": {"text": "So starck zeucht unser Ursprung uns, herab auff Erde nur zu schauen.", "tokens": ["So", "starck", "zeucht", "un\u00b7ser", "Ur\u00b7sprung", "uns", ",", "her\u00b7ab", "auff", "Er\u00b7de", "nur", "zu", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "PPER", "$,", "ADV", "APPR", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}}}}}