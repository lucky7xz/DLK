{"dta.poem.11839": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn du die Menschen siehst, mein Sohn, an einem Platze", "tokens": ["Wenn", "du", "die", "Men\u00b7schen", "siehst", ",", "mein", "Sohn", ",", "an", "ei\u00b7nem", "Plat\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "PPOSAT", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Versammelt, und sich freun wie an gefundnem Schatze;", "tokens": ["Ver\u00b7sam\u00b7melt", ",", "und", "sich", "freun", "wie", "an", "ge\u00b7fund\u00b7nem", "Schat\u00b7ze", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KON", "PRF", "VVINF", "KOKOM", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So frage nicht: worin mag dieser Schatz bestehn?", "tokens": ["So", "fra\u00b7ge", "nicht", ":", "wo\u00b7rin", "mag", "die\u00b7ser", "Schatz", "be\u00b7stehn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "$.", "PWAV", "VMFIN", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie freuen sich, da\u00df sie einander freun sich sehn.", "tokens": ["Sie", "freu\u00b7en", "sich", ",", "da\u00df", "sie", "ein\u00b7an\u00b7der", "freun", "sich", "sehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "KOUS", "PPER", "PRF", "VVFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So magst du immer auch dich freun, da\u00df sie sich freuen;", "tokens": ["So", "magst", "du", "im\u00b7mer", "auch", "dich", "freun", ",", "da\u00df", "sie", "sich", "freu\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "PPER", "VVINF", "$,", "KOUS", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und la\u00df dich das gesehn zu haben nicht gereuen.", "tokens": ["Und", "la\u00df", "dich", "das", "ge\u00b7sehn", "zu", "ha\u00b7ben", "nicht", "ge\u00b7reu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PDS", "VVINF", "PTKZU", "VAINF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}