{"textgrid.poem.24869": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Sommernacht", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nacht des S\u00fcdens, blau und heiter,", "tokens": ["Nacht", "des", "S\u00fc\u00b7dens", ",", "blau", "und", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch des Abends goldnes Thor", "tokens": ["Durch", "des", "A\u00b7bends", "gold\u00b7nes", "Thor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwebst du leuchtend wie ein zweiter,", "tokens": ["Schwebst", "du", "leuch\u00b7tend", "wie", "ein", "zwei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "KOKOM", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein sch\u00f6nrer Tag empor.", "tokens": ["Wie", "ein", "sch\u00f6n\u00b7rer", "Tag", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Deine Schatten selbst sind heller", "tokens": ["Dei\u00b7ne", "Schat\u00b7ten", "selbst", "sind", "hel\u00b7ler"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "VAFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als im Norden unser Licht,", "tokens": ["Als", "im", "Nor\u00b7den", "un\u00b7ser", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Stunden rinnen schneller,", "tokens": ["Und", "die", "Stun\u00b7den", "rin\u00b7nen", "schnel\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn die Trauer kennst du nicht.", "tokens": ["Denn", "die", "Trau\u00b7er", "kennst", "du", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wem das Herz noch unzerfallen", "tokens": ["Wem", "das", "Herz", "noch", "un\u00b7zer\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Seele klar wie du,", "tokens": ["Und", "die", "See\u00b7le", "klar", "wie", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "KOKOM", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sanft in deinen Schlummerhallen", "tokens": ["Sanft", "in", "dei\u00b7nen", "Schlum\u00b7mer\u00b7hal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schlie\u00dfe dem das Auge zu!", "tokens": ["Schlie\u00b7\u00dfe", "dem", "das", "Au\u00b7ge", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber mir, dem Ruhelosen,", "tokens": ["A\u00b7ber", "mir", ",", "dem", "Ru\u00b7he\u00b7lo\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist vertrauter dort die Nacht,", "tokens": ["Ist", "ver\u00b7trau\u00b7ter", "dort", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die Wetterb\u00e4che tosen", "tokens": ["Wo", "die", "Wet\u00b7ter\u00b7b\u00e4\u00b7che", "to\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und im Sturm die F\u00f6hre kracht;", "tokens": ["Und", "im", "Sturm", "die", "F\u00f6h\u00b7re", "kracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.5": {"line.1": {"text": "Wo die schweren Nebel triefen", "tokens": ["Wo", "die", "schwe\u00b7ren", "Ne\u00b7bel", "trie\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um den Klippenstrand der Seen,", "tokens": ["Um", "den", "Klip\u00b7pen\u00b7strand", "der", "Seen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und aus dunklen Wassertiefen", "tokens": ["Und", "aus", "dunk\u00b7len", "Was\u00b7ser\u00b7tie\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schattenbilder auferstehn.", "tokens": ["Schat\u00b7ten\u00b7bil\u00b7der", "auf\u00b7er\u00b7stehn", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}