{"textgrid.poem.37174": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "2. Dr. Stauz als er Steinbarten las.", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was rast der Mann? Wo bleibt denn das Verderben", "tokens": ["Was", "rast", "der", "Mann", "?", "Wo", "bleibt", "denn", "das", "Ver\u00b7der\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "$.", "PWAV", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das wir von Adam erben?", "tokens": ["Das", "wir", "von", "A\u00b7dam", "er\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wohlwollen, Menschenliebe,", "tokens": ["Wohl\u00b7wol\u00b7len", ",", "Men\u00b7schen\u00b7lie\u00b7be", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Geselligkeit und andre gute Triebe", "tokens": ["Ge\u00b7sel\u00b7lig\u00b7keit", "und", "and\u00b7re", "gu\u00b7te", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Im Menschen von Natur?", "tokens": ["Im", "Men\u00b7schen", "von", "Na\u00b7tur", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wie kann der Kerl auf solches Zeug verfallen?", "tokens": ["Wie", "kann", "der", "Kerl", "auf", "sol\u00b7ches", "Zeug", "ver\u00b7fal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ich f\u00fcr mein Theil, ich merke von dem allen", "tokens": ["Ich", "f\u00fcr", "mein", "Theil", ",", "ich", "mer\u00b7ke", "von", "dem", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "APPR", "ART", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "In mir nicht die geringste Spur.", "tokens": ["In", "mir", "nicht", "die", "ge\u00b7rings\u00b7te", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}