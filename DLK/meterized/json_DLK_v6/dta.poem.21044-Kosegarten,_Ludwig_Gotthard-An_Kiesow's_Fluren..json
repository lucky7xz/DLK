{"dta.poem.21044": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "An Kiesow's Fluren.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Seyd mir gegr&#252;sst, ihr gr&#252;nenden Gefilde!               ", "tokens": ["Seyd", "mir", "ge\u00b7gr", "&#252;", "sst", ",", "ihr", "gr", "&#252;", "nen\u00b7den", "Ge\u00b7fil\u00b7de", "!"], "token_info": ["word", "word", "word", "XML_entity", "word", "punct", "word", "word", "XML_entity", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "NE", "$(", "VVFIN", "$,", "PPOSAT", "NN", "$(", "ADJA", "NN", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "In euch wird mir so traurig wohl!", "tokens": ["In", "euch", "wird", "mir", "so", "trau\u00b7rig", "wohl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "ADV", "ADJD", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein trotzig Herz zerschmilzt in eurer Milde,", "tokens": ["Mein", "trot\u00b7zig", "Herz", "zer\u00b7schmilzt", "in", "eu\u00b7rer", "Mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Busens Leere str\u00f6mt so voll!", "tokens": ["Des", "Bu\u00b7sens", "Lee\u00b7re", "str\u00f6mt", "so", "voll", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In euren freyen weiten R\u00e4umen", "tokens": ["In", "eu\u00b7ren", "frey\u00b7en", "wei\u00b7ten", "R\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Erweitert sich mein Geist, umf\u00e4ngt die ganze Welt", "tokens": ["Er\u00b7wei\u00b7tert", "sich", "mein", "Geist", ",", "um\u00b7f\u00e4ngt", "die", "gan\u00b7ze", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mit Liebe, wieget sich in Paradiesestr\u00e4umen,", "tokens": ["Mit", "Lie\u00b7be", ",", "wie\u00b7get", "sich", "in", "Pa\u00b7ra\u00b7die\u00b7ses\u00b7tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und f\u00fchlt sich wieder Mann und Held!", "tokens": ["Und", "f\u00fchlt", "sich", "wie\u00b7der", "Mann", "und", "Held", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Krystallne B\u00e4che, dichtbebuschte H\u00f6hen,", "tokens": ["Krys\u00b7tall\u00b7ne", "B\u00e4\u00b7che", ",", "dicht\u00b7be\u00b7buschte", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ihr G\u00e4rten sonder Kunst und Prunk,", "tokens": ["Ihr", "G\u00e4r\u00b7ten", "son\u00b7der", "Kunst", "und", "Prunk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr stillen himmelklaren Seen,", "tokens": ["Ihr", "stil\u00b7len", "him\u00b7mel\u00b7kla\u00b7ren", "Seen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du holde Haynesd\u00e4mmerung,", "tokens": ["Du", "hol\u00b7de", "Hay\u00b7nes\u00b7d\u00e4m\u00b7me\u00b7rung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.5": {"text": "Ihr feyerlichen Tannenw\u00e4lder", "tokens": ["Ihr", "fey\u00b7er\u00b7li\u00b7chen", "Tan\u00b7nen\u00b7w\u00e4l\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Voll Rauschens des Allgegenw\u00e4rtigen,", "tokens": ["Voll", "Rau\u00b7schens", "des", "All\u00b7ge\u00b7gen\u00b7w\u00e4r\u00b7ti\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Grasreiche Triften, saatengr\u00fcne Felder", "tokens": ["Gras\u00b7rei\u00b7che", "Trif\u00b7ten", ",", "saa\u00b7ten\u00b7gr\u00fc\u00b7ne", "Fel\u00b7der"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.8": {"text": "Voll Segens des Allliebenden;", "tokens": ["Voll", "Se\u00b7gens", "des", "A\u00b7lllie\u00b7ben\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "ART", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "O welche Ruh, o welcher tiefste Friede,", "tokens": ["O", "wel\u00b7che", "Ruh", ",", "o", "wel\u00b7cher", "tiefs\u00b7te", "Frie\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAT", "NN", "$,", "FM", "PWAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ums\u00e4uselt mich in eurem Schooss!", "tokens": ["Um\u00b7s\u00e4u\u00b7selt", "mich", "in", "eu\u00b7rem", "Scho\u00b7oss", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr heilt des Wallers Lebensm\u00fcde,", "tokens": ["Ihr", "heilt", "des", "Wal\u00b7lers", "Le\u00b7bens\u00b7m\u00fc\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und s\u00f6hnt ihn aus mit seinem Thr\u00e4nenloos.", "tokens": ["Und", "s\u00f6hnt", "ihn", "aus", "mit", "sei\u00b7nem", "Thr\u00e4\u00b7nen\u00b7loos", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der erste Strahl der froherwachten Sonne", "tokens": ["Der", "ers\u00b7te", "Strahl", "der", "fro\u00b7her\u00b7wach\u00b7ten", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "K\u00fcsst mich zu jungen Freuden wach,", "tokens": ["K\u00fcsst", "mich", "zu", "jun\u00b7gen", "Freu\u00b7den", "wach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und ein Geflecht von \u00e4chter Lebenswonne", "tokens": ["Und", "ein", "Ge\u00b7flecht", "von", "\u00e4ch\u00b7ter", "Le\u00b7bens\u00b7won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Umschlingt den vollgenossnen Tag.", "tokens": ["Um\u00b7schlingt", "den", "voll\u00b7ge\u00b7noss\u00b7nen", "Tag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O, m\u00f6chte doch der Abend meiner Tage", "tokens": ["O", ",", "m\u00f6ch\u00b7te", "doch", "der", "A\u00b7bend", "mei\u00b7ner", "Ta\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "VMFIN", "ADV", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In euren Schatten mir verwehn!", "tokens": ["In", "eu\u00b7ren", "Schat\u00b7ten", "mir", "ver\u00b7wehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In euren Schatten wehmuthvoller Klage,", "tokens": ["In", "eu\u00b7ren", "Schat\u00b7ten", "weh\u00b7muth\u00b7vol\u00b7ler", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mir meine Sonne niedergehn!", "tokens": ["Mir", "mei\u00b7ne", "Son\u00b7ne", "nie\u00b7der\u00b7gehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O, wehte wenigstens auf meinen Aschenh\u00fcgel", "tokens": ["O", ",", "weh\u00b7te", "we\u00b7nigs\u00b7tens", "auf", "mei\u00b7nen", "A\u00b7schen\u00b7h\u00fc\u00b7gel"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Herbstwind euer r\u00f6thlich Laub,", "tokens": ["Der", "Herbst\u00b7wind", "eu\u00b7er", "r\u00f6th\u00b7lich", "Laub", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und mischete des Sommerhauches Fl\u00fcgel", "tokens": ["Und", "mi\u00b7sche\u00b7te", "des", "Som\u00b7mer\u00b7hau\u00b7ches", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Mit eurem stillen Staube meinen Staub!", "tokens": ["Mit", "eu\u00b7rem", "stil\u00b7len", "Stau\u00b7be", "mei\u00b7nen", "Staub", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}