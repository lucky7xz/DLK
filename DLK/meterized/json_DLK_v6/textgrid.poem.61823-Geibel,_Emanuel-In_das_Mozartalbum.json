{"textgrid.poem.61823": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "In das Mozartalbum", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mag die Welt vom einfach Sch\u00f6nen", "tokens": ["Mag", "die", "Welt", "vom", "ein\u00b7fach", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "APPRART", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich f\u00fcr kurze Zeit entw\u00f6hnen,", "tokens": ["Sich", "f\u00fcr", "kur\u00b7ze", "Zeit", "ent\u00b7w\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimmer tr\u00e4gt sie's auf die Dauer,", "tokens": ["Nim\u00b7mer", "tr\u00e4gt", "sie's", "auf", "die", "Dau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schn\u00f6dem Ungeschmack zu fr\u00f6hnen.", "tokens": ["Schn\u00f6\u00b7dem", "Un\u00b7ge\u00b7schmack", "zu", "fr\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Bald, vom Taumelfest ers\u00e4ttigt", "tokens": ["Bald", ",", "vom", "Tau\u00b7mel\u00b7fest", "er\u00b7s\u00e4t\u00b7tigt"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "APPRART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Anspruchsvoller Trugkam\u00f6nen,", "tokens": ["An\u00b7spruchs\u00b7vol\u00b7ler", "Trug\u00b7ka\u00b7m\u00f6\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sehnt sie sich zur\u00fcck zum Gipfel,", "tokens": ["Sehnt", "sie", "sich", "zu\u00b7r\u00fcck", "zum", "Gip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKVZ", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den die echten Lorbeern kr\u00f6nen,", "tokens": ["Den", "die", "ech\u00b7ten", "Lor\u00b7beern", "kr\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und mit Wonne lauscht sie wieder", "tokens": ["Und", "mit", "Won\u00b7ne", "lauscht", "sie", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Goethes Liedern, Mozarts T\u00f6nen.", "tokens": ["Goe\u00b7thes", "Lie\u00b7dern", ",", "Mo\u00b7zarts", "T\u00f6\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NE", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}