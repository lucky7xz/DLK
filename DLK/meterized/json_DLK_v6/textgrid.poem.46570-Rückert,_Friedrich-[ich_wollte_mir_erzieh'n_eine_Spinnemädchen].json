{"textgrid.poem.46570": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ich wollte mir erzieh'n eine Spinnem\u00e4dchen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wollte mir erzieh'n eine Spinnem\u00e4dchen;", "tokens": ["Ich", "woll\u00b7te", "mir", "er\u00b7zieh'n", "ei\u00b7ne", "Spin\u00b7ne\u00b7m\u00e4d\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Fr\u00fchzeitig lerne mir mein M\u00e4dchen spinnen!", "tokens": ["Fr\u00fch\u00b7zei\u00b7tig", "ler\u00b7ne", "mir", "mein", "M\u00e4d\u00b7chen", "spin\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Ich hatte schon gekauft ein Spinner\u00e4dchen;", "tokens": ["Ich", "hat\u00b7te", "schon", "ge\u00b7kauft", "ein", "Spin\u00b7ne\u00b7r\u00e4d\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und lauter Seide soll das R\u00e4dchen spinnen!", "tokens": ["Und", "lau\u00b7ter", "Sei\u00b7de", "soll", "das", "R\u00e4d\u00b7chen", "spin\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "O meiner Hoffnung arme Spinnef\u00e4dchen!", "tokens": ["O", "mei\u00b7ner", "Hoff\u00b7nung", "ar\u00b7me", "Spin\u00b7ne\u00b7f\u00e4d\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ich sah sie nicht ihr erstes F\u00e4dchen spinnen.", "tokens": ["Ich", "sah", "sie", "nicht", "ihr", "ers\u00b7tes", "F\u00e4d\u00b7chen", "spin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Nun \u00fcbers Grab, wo schl\u00e4ft mein Spinnem\u00e4dchen", "tokens": ["Nun", "\u00fc\u00b7bers", "Grab", ",", "wo", "schl\u00e4ft", "mein", "Spin\u00b7ne\u00b7m\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "$,", "PWAV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Seh' ich im Sonnschein Mad' und M\u00e4dchen spinnen.", "tokens": ["Seh'", "ich", "im", "Sonn\u00b7schein", "Mad'", "und", "M\u00e4d\u00b7chen", "spin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}