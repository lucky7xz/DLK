{"textgrid.poem.29534": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "49.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Pholoe mag lange dencken; dennoch wird sie schwerlich wissen,", "tokens": ["Pho\u00b7loe", "mag", "lan\u00b7ge", "den\u00b7cken", ";", "den\u00b7noch", "wird", "sie", "schwer\u00b7lich", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ADV", "VVINF", "$.", "ADV", "VAFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Wann sie, wo sie, und wie offte der und jener kam zu k\u00fcssen;", "tokens": ["Wann", "sie", ",", "wo", "sie", ",", "und", "wie", "off\u00b7te", "der", "und", "je\u00b7ner", "kam", "zu", "k\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "PWAV", "PPER", "$,", "KON", "PWAV", "VVFIN", "ART", "KON", "PDS", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Keiner ist wol weg geweist, der ihr gleich nicht hat gelohnt,", "tokens": ["Kei\u00b7ner", "ist", "wol", "weg", "ge\u00b7weist", ",", "der", "ihr", "gleich", "nicht", "hat", "ge\u00b7lohnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "ADV", "VVPP", "$,", "PRELS", "PPER", "ADV", "PTKNEG", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Ausser dem, der seinen Mund selbst bedencklich hat geschont.", "tokens": ["Aus\u00b7ser", "dem", ",", "der", "sei\u00b7nen", "Mund", "selbst", "be\u00b7denck\u00b7lich", "hat", "ge\u00b7schont", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PPOSAT", "NN", "ADV", "ADJD", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}}}}}