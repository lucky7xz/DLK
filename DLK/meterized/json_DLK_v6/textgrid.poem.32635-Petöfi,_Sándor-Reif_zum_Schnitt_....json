{"textgrid.poem.32635": {"metadata": {"author": {"name": "Pet\u00f6fi, S\u00e1ndor", "birth": "N.A.", "death": "N.A."}, "title": "Reif zum Schnitt ...", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Reif zum Schnitt ist schon die Frucht,", "tokens": ["Reif", "zum", "Schnitt", "ist", "schon", "die", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hei\u00df gl\u00fcht es hernieder, \u2013", "tokens": ["Hei\u00df", "gl\u00fcht", "es", "her\u00b7nie\u00b7der", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$,", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Montags, wenn der Morgen graut,", "tokens": ["Mon\u00b7tags", ",", "wenn", "der", "Mor\u00b7gen", "graut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Geht's zur Ernte wieder!", "tokens": ["Geht's", "zur", "Ern\u00b7te", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Reif ist meine Liebe auch,", "tokens": ["Reif", "ist", "mei\u00b7ne", "Lie\u00b7be", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und so hei\u00df mein Sinnen, \u2013", "tokens": ["Und", "so", "hei\u00df", "mein", "Sin\u00b7nen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "ADJD", "PPOSAT", "NN", "$,", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "K\u00f6nnt' ich doch zur Schnitterin", "tokens": ["K\u00f6nnt'", "ich", "doch", "zur", "Schnit\u00b7te\u00b7rin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dich, mein Schatz, gewinnen!", "tokens": ["Dich", ",", "mein", "Schatz", ",", "ge\u00b7win\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}