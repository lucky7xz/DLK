{"dta.poem.3759": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Liebeslieder .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Herzigs Kindlein, Zuckerm\u00fcndlein,               ", "tokens": ["Her\u00b7zigs", "Kin\u00b7dlein", ",", "Zu\u00b7cker\u00b7m\u00fcnd\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ich hab ein Wecklein, in meinem S\u00e4cklein,", "tokens": ["Ich", "hab", "ein", "Weck\u00b7lein", ",", "in", "mei\u00b7nem", "S\u00e4ck\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+---", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich will dirs bringen,", "tokens": ["Ich", "will", "dirs", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Bis nach Bingen,", "tokens": ["Bis", "nach", "Bin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "APPR", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Zerri\u00dfne Hemder,", "tokens": ["Zer\u00b7ri\u00df\u00b7ne", "Hem\u00b7der", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Die Schuh voll B\u00e4nder,", "tokens": ["Die", "Schuh", "voll", "B\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Papierne Abs\u00e4tz,", "tokens": ["Pa\u00b7pier\u00b7ne", "Ab\u00b7s\u00e4tz", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "H\u00f6lzerne Sohlen;", "tokens": ["H\u00f6l\u00b7zer\u00b7ne", "Soh\u00b7len", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Kn\u00e4blein willst du mich,", "tokens": ["Kn\u00e4\u00b7blein", "willst", "du", "mich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "So thu mich holen.", "tokens": ["So", "thu", "mich", "ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Mein Sch\u00e4tzlein, mein K\u00e4tzlein,", "tokens": ["Mein", "Sch\u00e4tz\u00b7lein", ",", "mein", "K\u00e4tz\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "O warte nur ein Jahr,", "tokens": ["O", "war\u00b7te", "nur", "ein", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wann die Weiden Kirschen tragen,", "tokens": ["Und", "wann", "die", "Wei\u00b7den", "Kir\u00b7schen", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So nehm ich dich f\u00fcrwahr.", "tokens": ["So", "nehm", "ich", "dich", "f\u00fcr\u00b7wahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Weiden tragen keine Kirschen,", "tokens": ["Die", "Wei\u00b7den", "tra\u00b7gen", "kei\u00b7ne", "Kir\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die K\u00f6nigskerze ist kein Licht,", "tokens": ["Die", "K\u00f6\u00b7nigs\u00b7ker\u00b7ze", "ist", "kein", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Also kannst du gedenken,", "tokens": ["Al\u00b7so", "kannst", "du", "ge\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Da\u00df ich dich nehme nicht.", "tokens": ["Da\u00df", "ich", "dich", "neh\u00b7me", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Und wenn ich dich schon nehme,", "tokens": ["Und", "wenn", "ich", "dich", "schon", "neh\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "So haben wir kein Haus,", "tokens": ["So", "ha\u00b7ben", "wir", "kein", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Da setzen wir uns in die Kieze,", "tokens": ["Da", "set\u00b7zen", "wir", "uns", "in", "die", "Kie\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und schauen oben raus.", "tokens": ["Und", "schau\u00b7en", "o\u00b7ben", "raus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}