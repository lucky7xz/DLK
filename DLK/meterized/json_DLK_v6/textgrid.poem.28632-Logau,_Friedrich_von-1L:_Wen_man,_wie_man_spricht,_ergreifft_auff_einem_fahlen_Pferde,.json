{"textgrid.poem.28632": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wen man, wie man spricht, ergreifft auff einem fahlen Pferde,", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wen man, wie man spricht, ergreifft auff einem fahlen Pferde,", "tokens": ["Wen", "man", ",", "wie", "man", "spricht", ",", "er\u00b7greifft", "auff", "ei\u00b7nem", "fah\u00b7len", "Pfer\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "$,", "PWAV", "PIS", "VVFIN", "$,", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Der verdient, da\u00df seine Treu gar schwartz geachtet werde;", "tokens": ["Der", "ver\u00b7dient", ",", "da\u00df", "sei\u00b7ne", "Treu", "gar", "schwartz", "ge\u00b7ach\u00b7tet", "wer\u00b7de", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "ADV", "ADJD", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "Den man, wie gemein, ergreifft auff einer falben Dame,", "tokens": ["Den", "man", ",", "wie", "ge\u00b7mein", ",", "er\u00b7greifft", "auff", "ei\u00b7ner", "fal\u00b7ben", "Da\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "PWAV", "ADJD", "$,", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Diesen trifft (warum auch nicht?) ein tapffer K\u00e4ysers-Name.", "tokens": ["Die\u00b7sen", "trifft", "(", "wa\u00b7rum", "auch", "nicht", "?", ")", "ein", "tapf\u00b7fer", "K\u00e4y\u00b7ser\u00b7sNa\u00b7me", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$(", "PWAV", "ADV", "PTKNEG", "$.", "$(", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}}}}