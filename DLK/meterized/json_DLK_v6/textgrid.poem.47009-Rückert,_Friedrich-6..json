{"textgrid.poem.47009": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Thessalierin, obgleich mit keinem Laute", "tokens": ["Thes\u00b7sa\u00b7lie\u00b7rin", ",", "ob\u00b7gleich", "mit", "kei\u00b7nem", "Lau\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "APPR", "PIAT", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Du von Thessalien je geh\u00f6rt im Traume;", "tokens": ["Du", "von", "Thes\u00b7sa\u00b7li\u00b7en", "je", "ge\u00b7h\u00f6rt", "im", "Trau\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NE", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Thessalierin! von welchem Zauberbaume,", "tokens": ["Thes\u00b7sa\u00b7lie\u00b7rin", "!", "von", "wel\u00b7chem", "Zau\u00b7ber\u00b7bau\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPR", "PWAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von welcher Zauberwurzel, Zauberkraute,", "tokens": ["Von", "wel\u00b7cher", "Zau\u00b7ber\u00b7wur\u00b7zel", ",", "Zau\u00b7ber\u00b7krau\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nahm deine Hand die Stoffe, draus sie braute", "tokens": ["Nahm", "dei\u00b7ne", "Hand", "die", "Stof\u00b7fe", ",", "draus", "sie", "brau\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "ART", "NN", "$,", "PAV", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das bittere Getr\u00e4nk, in dessen Schaume", "tokens": ["Das", "bit\u00b7te\u00b7re", "Ge\u00b7tr\u00e4nk", ",", "in", "des\u00b7sen", "Schau\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PRELAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verborgen ist, was je vom Wolkensaume", "tokens": ["Ver\u00b7bor\u00b7gen", "ist", ",", "was", "je", "vom", "Wol\u00b7ken\u00b7sau\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "$,", "PRELS", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Mittern\u00e4chte Gift'ges niedertaute?", "tokens": ["Der", "Mit\u00b7ter\u00b7n\u00e4ch\u00b7te", "Gift'\u00b7ges", "nie\u00b7der\u00b7tau\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da\u00df Gift es ist, mu\u00df ich ja wohl erkennen", "tokens": ["Da\u00df", "Gift", "es", "ist", ",", "mu\u00df", "ich", "ja", "wohl", "er\u00b7ken\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPER", "VAFIN", "$,", "VMFIN", "PPER", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Daraus, weil du aus den gef\u00fcllten Scherben,", "tokens": ["Da\u00b7raus", ",", "weil", "du", "aus", "den", "ge\u00b7f\u00fcll\u00b7ten", "Scher\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-----+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Wie sehr ich flehe, nicht zuvor willst nippen.", "tokens": ["Wie", "sehr", "ich", "fle\u00b7he", ",", "nicht", "zu\u00b7vor", "willst", "nip\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "VVFIN", "$,", "PTKNEG", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Drum, statt zu l\u00f6schen, macht es Durst entbrennen,", "tokens": ["Drum", ",", "statt", "zu", "l\u00f6\u00b7schen", ",", "macht", "es", "Durst", "ent\u00b7bren\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUI", "PTKZU", "VVINF", "$,", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und weh! wenn du nicht bald mir statt des Herben", "tokens": ["Und", "weh", "!", "wenn", "du", "nicht", "bald", "mir", "statt", "des", "Her\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKVZ", "$.", "KOUS", "PPER", "PTKNEG", "ADV", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das S\u00fc\u00dfe reichst im Becher deiner Lippen.", "tokens": ["Das", "S\u00fc\u00b7\u00dfe", "reichst", "im", "Be\u00b7cher", "dei\u00b7ner", "Lip\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}