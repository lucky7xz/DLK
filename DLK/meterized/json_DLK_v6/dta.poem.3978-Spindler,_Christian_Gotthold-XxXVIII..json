{"dta.poem.3978": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "XxXVIII.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Wer so, wie du, bey B\u00fcchern schwitzt,", "tokens": ["Wer", "so", ",", "wie", "du", ",", "bey", "B\u00fc\u00b7chern", "schwitzt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "$,", "PWAV", "PPER", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und niemahls unbem\u00fchet sitzt;", "tokens": ["Und", "nie\u00b7mahls", "un\u00b7be\u00b7m\u00fc\u00b7het", "sitzt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer unter viel gewagten Siegen,", "tokens": ["Wer", "un\u00b7ter", "viel", "ge\u00b7wag\u00b7ten", "Sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Spitzen Helicons erstiegen,", "tokens": ["Die", "Spit\u00b7zen", "He\u00b7li\u00b7cons", "er\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dem wird mit Recht ein Blat gebracht.", "tokens": ["Dem", "wird", "mit", "Recht", "ein", "Blat", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das die Verdienste kundig macht.", "tokens": ["Das", "die", "Ver\u00b7diens\u00b7te", "kun\u00b7dig", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}