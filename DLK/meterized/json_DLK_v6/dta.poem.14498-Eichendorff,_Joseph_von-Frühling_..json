{"dta.poem.14498": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchling .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.57", "nl:0.42"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Den Himmel dann, das blaue Meer", "tokens": ["Den", "Him\u00b7mel", "dann", ",", "das", "blau\u00b7e", "Meer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sehnsucht gr\u00fc\u00dft er treu,", "tokens": ["Der", "Sehn\u00b7sucht", "gr\u00fc\u00dft", "er", "treu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da stammen Lied und S\u00e4nger her", "tokens": ["Da", "stam\u00b7men", "Lied", "und", "S\u00e4n\u00b7ger", "her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sp\u00fcren's immer neu.", "tokens": ["Und", "sp\u00fc\u00b7ren's", "im\u00b7mer", "neu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}