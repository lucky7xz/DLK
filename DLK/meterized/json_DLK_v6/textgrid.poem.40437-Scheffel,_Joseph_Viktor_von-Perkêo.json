{"textgrid.poem.40437": {"metadata": {"author": {"name": "Scheffel, Joseph Viktor von", "birth": "N.A.", "death": "N.A."}, "title": "Perk\u00eao", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das war der Zwerg Perk\u00eao im Heidelberger Schlo\u00df,", "tokens": ["Das", "war", "der", "Zwerg", "Perk\u00eao", "im", "Hei\u00b7del\u00b7ber\u00b7ger", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NE", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "An Wuchse klein und winzig, an Durste riesengro\u00df.", "tokens": ["An", "Wuch\u00b7se", "klein", "und", "win\u00b7zig", ",", "an", "Durs\u00b7te", "rie\u00b7sen\u00b7gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$,", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Man schalt ihn einen Narren, er dachte: \u00bbLiebe Leut',", "tokens": ["Man", "schalt", "ihn", "ei\u00b7nen", "Nar\u00b7ren", ",", "er", "dach\u00b7te", ":", "\u00bb", "Lie\u00b7be", "Leut'", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ART", "NN", "$,", "PPER", "VVFIN", "$.", "$(", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "W\u00e4rt ihr wie ich doch alle feuchtfr\u00f6hlich und gescheut!\u00ab", "tokens": ["W\u00e4rt", "ihr", "wie", "ich", "doch", "al\u00b7le", "feucht\u00b7fr\u00f6h\u00b7lich", "und", "ge\u00b7scheut", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "PWAV", "PPER", "ADV", "PIS", "ADJD", "KON", "VVPP", "$.", "$("], "meter": "-+-+-+-++-+-+", "measure": "unknown.measure.septa"}}, "stanza.3": {"line.1": {"text": "Und als das Fa\u00df, das gro\u00dfe, mit Wein bestellet war,", "tokens": ["Und", "als", "das", "Fa\u00df", ",", "das", "gro\u00b7\u00dfe", ",", "mit", "Wein", "be\u00b7stel\u00b7let", "war", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "$,", "ART", "ADJA", "$,", "APPR", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da ward sein k\u00fcnftiger Standpunkt dem Zwergen v\u00f6llig klar.", "tokens": ["Da", "ward", "sein", "k\u00fcnf\u00b7ti\u00b7ger", "Stand\u00b7punkt", "dem", "Zwer\u00b7gen", "v\u00f6l\u00b7lig", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.4": {"line.1": {"text": "\u00bbfahr' wohl\u00ab, sprach er, \u00bbo Welt, du Katzenjammertal,", "tokens": ["\u00bb", "fahr'", "wohl", "\u00ab", ",", "sprach", "er", ",", "\u00bb", "o", "Welt", ",", "du", "Kat\u00b7zen\u00b7jam\u00b7mer\u00b7tal", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVIMP", "ADV", "$(", "$,", "VVFIN", "PPER", "$,", "$(", "FM", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was sie auf dir hantieren, ist Wurst mir und egal!", "tokens": ["Was", "sie", "auf", "dir", "han\u00b7tie\u00b7ren", ",", "ist", "Wurst", "mir", "und", "e\u00b7gal", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPER", "VVINF", "$,", "VAFIN", "NN", "PPER", "KON", "ADV", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Um lederne Ideen rauft man manch hei\u00dfen Kampf,", "tokens": ["Um", "le\u00b7der\u00b7ne", "I\u00b7deen", "rauft", "man", "manch", "hei\u00b7\u00dfen", "Kampf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "VVFIN", "PIS", "PIAT", "ADJA", "NN", "$,"], "meter": "----+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es ist im Grund doch alles nur Nebel, Rauch und Dampf.", "tokens": ["Es", "ist", "im", "Grund", "doch", "al\u00b7les", "nur", "Ne\u00b7bel", ",", "Rauch", "und", "Dampf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ADV", "PIS", "ADV", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Die Wahrheit liegt im Weine. Beim Weinschlurf sonder End'", "tokens": ["Die", "Wahr\u00b7heit", "liegt", "im", "Wei\u00b7ne", ".", "Beim", "Wein\u00b7schlurf", "son\u00b7der", "End'"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$.", "APPRART", "NN", "KON", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Erkl\u00e4r' ich alter Narre fortan mich permanent.\u00ab", "tokens": ["Er\u00b7kl\u00e4r'", "ich", "al\u00b7ter", "Nar\u00b7re", "for\u00b7tan", "mich", "per\u00b7ma\u00b7nent", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "ADV", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Perk\u00eao stieg zum Keller; er kam nicht mehr herf\u00fcr", "tokens": ["Perk\u00eao", "stieg", "zum", "Kel\u00b7ler", ";", "er", "kam", "nicht", "mehr", "her\u00b7f\u00fcr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPRART", "NN", "$.", "PPER", "VVFIN", "PTKNEG", "ADV", "ADV"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Und sog bei f\u00fcnfzehn Jahre am rheinischen Malvasier.", "tokens": ["Und", "sog", "bei", "f\u00fcnf\u00b7zehn", "Jah\u00b7re", "am", "rhei\u00b7ni\u00b7schen", "Mal\u00b7va\u00b7sier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "CARD", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "War's drunten auch stichdunkel, ihm strahlte inneres Licht,", "tokens": ["Wa\u00b7r's", "drun\u00b7ten", "auch", "stich\u00b7dun\u00b7kel", ",", "ihm", "strahl\u00b7te", "in\u00b7ne\u00b7res", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "$,", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+--+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Und wankten auch die Beine, er trank und murrte nicht.", "tokens": ["Und", "wank\u00b7ten", "auch", "die", "Bei\u00b7ne", ",", "er", "trank", "und", "murr\u00b7te", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$,", "PPER", "VVFIN", "KON", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Als er zum Fa\u00df gestiegen, stand's wohlgef\u00fcllt und schwer,", "tokens": ["Als", "er", "zum", "Fa\u00df", "ge\u00b7stie\u00b7gen", ",", "stan\u00b7d's", "wohl\u00b7ge\u00b7f\u00fcllt", "und", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVPP", "$,", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Doch als er kam zu sterben, klang's ausgesaugt und leer.", "tokens": ["Doch", "als", "er", "kam", "zu", "ster\u00b7ben", ",", "klang's", "aus\u00b7ge\u00b7saugt", "und", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "PTKZU", "VVINF", "$,", "ADJA", "VVPP", "KON", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "Da sprach er fromm: \u00bbNun preiset, ihr Leute, des Herren Macht,", "tokens": ["Da", "sprach", "er", "fromm", ":", "\u00bb", "Nun", "prei\u00b7set", ",", "ihr", "Leu\u00b7te", ",", "des", "Her\u00b7ren", "Macht", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$.", "$(", "ADV", "VVFIN", "$,", "PPOSAT", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die in mir schwachem Knirpse so Starkes hat vollbracht:", "tokens": ["Die", "in", "mir", "schwa\u00b7chem", "Knirp\u00b7se", "so", "Star\u00b7kes", "hat", "voll\u00b7bracht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "ADJA", "NN", "ADV", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.11": {"line.1": {"text": "Wie es dem kleinen David gegen Goliath einst gelang,", "tokens": ["Wie", "es", "dem", "klei\u00b7nen", "Da\u00b7vid", "ge\u00b7gen", "Go\u00b7li\u00b7ath", "einst", "ge\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NE", "APPR", "NE", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.2": {"text": "Also ich arm' Gezwerge den Riesen Durst bezwang.", "tokens": ["Al\u00b7so", "ich", "arm'", "Ge\u00b7zwer\u00b7ge", "den", "Rie\u00b7sen", "Durst", "be\u00b7zwang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADJA", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-++-+--+-+-+", "measure": "trochaic.septa.relaxed"}}, "stanza.12": {"line.1": {"text": "Nun singt ein De Profundis, da\u00df das Gew\u00f6lb' erdr\u00f6hnt,", "tokens": ["Nun", "singt", "ein", "De", "Pro\u00b7fun\u00b7dis", ",", "da\u00df", "das", "Ge\u00b7w\u00f6lb'", "er\u00b7dr\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "FM", "FM", "$,", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-++-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Das Fa\u00df steht auf der Neige, ich falle sieggekr\u00f6nt.\u00ab", "tokens": ["Das", "Fa\u00df", "steht", "auf", "der", "Nei\u00b7ge", ",", "ich", "fal\u00b7le", "sieg\u00b7ge\u00b7kr\u00f6nt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "VVFIN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.13": {"line.1": {"text": "... Perk\u00eao ward begraben. \u2013 Um seine Kellergruft", "tokens": ["...", "Perk\u00eao", "ward", "be\u00b7gra\u00b7ben", ".", "\u2013", "Um", "sei\u00b7ne", "Kel\u00b7ler\u00b7gruft"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "NE", "VAFIN", "VVPP", "$.", "$(", "KOUI", "PPOSAT", "NN"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Beim leeren Riesenfasse weht heut noch feuchte Luft,", "tokens": ["Beim", "lee\u00b7ren", "Rie\u00b7sen\u00b7fas\u00b7se", "weht", "heut", "noch", "feuch\u00b7te", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.14": {"line.1": {"text": "Und wer als frommer Pilger fr\u00fchmorgens ihr genaht:", "tokens": ["Und", "wer", "als", "from\u00b7mer", "Pil\u00b7ger", "fr\u00fch\u00b7mor\u00b7gens", "ihr", "ge\u00b7naht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "KOUS", "ADJA", "NN", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Weh' ihm! Als Weinvertilger durchtobt er nachts die Stadt.", "tokens": ["Weh'", "ihm", "!", "Als", "Wein\u00b7ver\u00b7til\u00b7ger", "durch\u00b7tobt", "er", "nachts", "die", "Stadt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$.", "KOUS", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}}}}}