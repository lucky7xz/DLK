{"textgrid.poem.42889": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Rauch verweht.", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Rauch verweht.", "tokens": ["Ein", "Rauch", "ver\u00b7weht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ein Wasser verrinnt.", "tokens": ["Ein", "Was\u00b7ser", "ver\u00b7rinnt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Eine Zeit vergeht.", "tokens": ["Ei\u00b7ne", "Zeit", "ver\u00b7geht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Eine neue beginnt.", "tokens": ["Ei\u00b7ne", "neu\u00b7e", "be\u00b7ginnt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Warum? Wozu?", "tokens": ["Wa\u00b7rum", "?", "Wo\u00b7zu", "?"], "token_info": ["word", "punct", "word", "punct"], "pos": ["PWAV", "$.", "PWAV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Denk' ich dein Fleisch hinweg, so bist", "tokens": ["Denk'", "ich", "dein", "Fleisch", "hin\u00b7weg", ",", "so", "bist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "PTKVZ", "$,", "ADV", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du ein d\u00fcnntrauriges Knochenger\u00fcst,", "tokens": ["Du", "ein", "d\u00fcnn\u00b7trau\u00b7ri\u00b7ges", "Kno\u00b7chen\u00b7ge\u00b7r\u00fcst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Allersch\u00f6nstes M\u00e4dchen du.", "tokens": ["Al\u00b7ler\u00b7sch\u00f6ns\u00b7tes", "M\u00e4d\u00b7chen", "du", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer hat das Fragen aufgebracht?", "tokens": ["Wer", "hat", "das", "Fra\u00b7gen", "auf\u00b7ge\u00b7bracht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Unsere Not.", "tokens": ["Un\u00b7se\u00b7re", "Not", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wer niemals fragte, w\u00e4re tot.", "tokens": ["Wer", "nie\u00b7mals", "frag\u00b7te", ",", "w\u00e4\u00b7re", "tot", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch kommt's drauf an, wie jemand lacht.", "tokens": ["Doch", "kommt's", "drauf", "an", ",", "wie", "je\u00b7mand", "lacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "PTKVZ", "$,", "PWAV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Bist du aus schlimmem Traum erwacht,", "tokens": ["Bist", "du", "aus", "schlim\u00b7mem", "Traum", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist eine Postanweisung da,", "tokens": ["Ist", "ei\u00b7ne", "Pos\u00b7tan\u00b7wei\u00b7sung", "da", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Telegramm, ein guter Brief, \u2013", "tokens": ["Ein", "Te\u00b7le\u00b7gramm", ",", "ein", "gu\u00b7ter", "Brief", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du atmest tief", "tokens": ["Du", "at\u00b7mest", "tief"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Wie eine Ziehharmonika.", "tokens": ["Wie", "ei\u00b7ne", "Zieh\u00b7har\u00b7mo\u00b7ni\u00b7ka", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}