{"textgrid.poem.46969": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen Pfeifenkopf mit Bl\u00fcchers Bild", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcll' mich mit edlen Bl\u00e4ttern an,", "tokens": ["F\u00fcll'", "mich", "mit", "ed\u00b7len", "Bl\u00e4t\u00b7tern", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil Bl\u00fcchers Bild hier steht,", "tokens": ["Weil", "Bl\u00fc\u00b7chers", "Bild", "hier", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und z\u00fcnde sie als Opfer an,", "tokens": ["Und", "z\u00fcn\u00b7de", "sie", "als", "Op\u00b7fer", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KOUS", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ihn der Rauch umweht.", "tokens": ["Da\u00df", "ihn", "der", "Rauch", "um\u00b7weht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der alte Held, von Pulverrauch", "tokens": ["Der", "al\u00b7te", "Held", ",", "von", "Pul\u00b7ver\u00b7rauch"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gebr\u00e4unt in manchem Kampf,", "tokens": ["Ge\u00b7br\u00e4unt", "in", "man\u00b7chem", "Kampf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bleibt hier bei seinem alten Brauch", "tokens": ["Bleibt", "hier", "bei", "sei\u00b7nem", "al\u00b7ten", "Brauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und gl\u00fcht, umwallt von Dampf.", "tokens": ["Und", "gl\u00fcht", ",", "um\u00b7wallt", "von", "Dampf", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}