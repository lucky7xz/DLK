{"textgrid.poem.43498": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Bald ist der Fr\u00fchling da!", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.85", "id:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tra ri ra!", "tokens": ["Tra", "ri", "ra", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "+--", "measure": "dactylic.init"}, "line.2": {"text": "Bald ist der Fr\u00fchling da!", "tokens": ["Bald", "ist", "der", "Fr\u00fch\u00b7ling", "da", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bald werden gr\u00fcn die Felder,", "tokens": ["Bald", "wer\u00b7den", "gr\u00fcn", "die", "Fel\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Wiesen und die W\u00e4lder.", "tokens": ["Die", "Wie\u00b7sen", "und", "die", "W\u00e4l\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Tra ri ra!", "tokens": ["Tra", "ri", "ra", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "+--", "measure": "dactylic.init"}, "line.6": {"text": "Bald ist der Fr\u00fchling da!", "tokens": ["Bald", "ist", "der", "Fr\u00fch\u00b7ling", "da", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Tra ri re!", "tokens": ["Tra", "ri", "re", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "---", "measure": "unknown.measure.zero"}, "line.2": {"text": "Schon schmilzet Eis und Schnee:", "tokens": ["Schon", "schmil\u00b7zet", "Eis", "und", "Schnee", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Quellen rauschen wieder", "tokens": ["Die", "Quel\u00b7len", "rau\u00b7schen", "wie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von allen Bergen nieder.", "tokens": ["Von", "al\u00b7len", "Ber\u00b7gen", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Tra ri re!", "tokens": ["Tra", "ri", "re", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "---", "measure": "unknown.measure.zero"}, "line.6": {"text": "Schon schmilzet Eis und Schnee.", "tokens": ["Schon", "schmil\u00b7zet", "Eis", "und", "Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Tra ri ro!", "tokens": ["Tra", "ri", "ro", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Jetzt sind wir wieder froh!", "tokens": ["Jetzt", "sind", "wir", "wie\u00b7der", "froh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ja, Trost f\u00fcr lange Plage", "tokens": ["Ja", ",", "Trost", "f\u00fcr", "lan\u00b7ge", "Pla\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verleih'n die l\u00e4ngern Tage.", "tokens": ["Ver\u00b7leih'n", "die", "l\u00e4n\u00b7gern", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Tra ri ro!", "tokens": ["Tra", "ri", "ro", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Jetzt sind wir wieder froh!", "tokens": ["Jetzt", "sind", "wir", "wie\u00b7der", "froh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Tra ri ru!", "tokens": ["Tra", "ri", "ru", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "+--", "measure": "dactylic.init"}, "line.2": {"text": "Du lieber Fr\u00fchling du,", "tokens": ["Du", "lie\u00b7ber", "Fr\u00fch\u00b7ling", "du", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df uns nicht l\u00e4nger warten!", "tokens": ["La\u00df", "uns", "nicht", "l\u00e4n\u00b7ger", "war\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Komm bald in Feld und Garten!", "tokens": ["Komm", "bald", "in", "Feld", "und", "Gar\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Tra ri ru!", "tokens": ["Tra", "ri", "ru", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "+--", "measure": "dactylic.init"}, "line.6": {"text": "Du lieber Fr\u00fchling du!", "tokens": ["Du", "lie\u00b7ber", "Fr\u00fch\u00b7ling", "du", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}