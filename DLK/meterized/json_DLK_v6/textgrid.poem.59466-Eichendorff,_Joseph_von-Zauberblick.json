{"textgrid.poem.59466": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Zauberblick", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Burg, die liegt verfallen", "tokens": ["Die", "Burg", ",", "die", "liegt", "ver\u00b7fal\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In sch\u00f6ner Einsamkeit,", "tokens": ["In", "sch\u00f6\u00b7ner", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dort sa\u00df ich vor den Hallen", "tokens": ["Dort", "sa\u00df", "ich", "vor", "den", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bei stiller Mittagszeit.", "tokens": ["Bei", "stil\u00b7ler", "Mit\u00b7tags\u00b7zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es ruhten in der K\u00fchle", "tokens": ["Es", "ruh\u00b7ten", "in", "der", "K\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Rehe auf dem Wall", "tokens": ["Die", "Re\u00b7he", "auf", "dem", "Wall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und tief in blauer Schw\u00fcle", "tokens": ["Und", "tief", "in", "blau\u00b7er", "Schw\u00fc\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die sonn'gen T\u00e4ler all.", "tokens": ["Die", "sonn'\u00b7gen", "T\u00e4\u00b7ler", "all", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Tief unten h\u00f6rt ich Glocken", "tokens": ["Tief", "un\u00b7ten", "h\u00f6rt", "ich", "Glo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In weiter Ferne gehn,", "tokens": ["In", "wei\u00b7ter", "Fer\u00b7ne", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich aber mu\u00dft erschrocken", "tokens": ["Ich", "a\u00b7ber", "mu\u00dft", "er\u00b7schro\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum alten Erker sehn.", "tokens": ["Zum", "al\u00b7ten", "Er\u00b7ker", "sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Denn in dem Fensterbogen", "tokens": ["Denn", "in", "dem", "Fens\u00b7ter\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein' sch\u00f6ne Fraue stand,", "tokens": ["Ein'", "sch\u00f6\u00b7ne", "Frau\u00b7e", "stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als h\u00fctete sie droben", "tokens": ["Als", "h\u00fc\u00b7te\u00b7te", "sie", "dro\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die W\u00e4lder und das Land.", "tokens": ["Die", "W\u00e4l\u00b7der", "und", "das", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ihr Haar, wie 'n goldner Mantel,", "tokens": ["Ihr", "Haar", ",", "wie", "'n", "gold\u00b7ner", "Man\u00b7tel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "War tief herabgerollt;", "tokens": ["War", "tief", "her\u00b7ab\u00b7ge\u00b7rollt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf einmal sie sich wandte,", "tokens": ["Auf", "ein\u00b7mal", "sie", "sich", "wand\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als ob sie sprechen wollt.", "tokens": ["Als", "ob", "sie", "spre\u00b7chen", "wollt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und als ich schauernd lauschte \u2013", "tokens": ["Und", "als", "ich", "schau\u00b7ernd", "lauschte", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da war ich aufgewacht,", "tokens": ["Da", "war", "ich", "auf\u00b7ge\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und unter mir schon rauschte", "tokens": ["Und", "un\u00b7ter", "mir", "schon", "rauschte"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So wunderbar die Nacht.", "tokens": ["So", "wun\u00b7der\u00b7bar", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Tr\u00e4umt ich im Mondesschimmer?", "tokens": ["Tr\u00e4umt", "ich", "im", "Mon\u00b7des\u00b7schim\u00b7mer", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich wei\u00df nicht, was mir graut,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "was", "mir", "graut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch das verge\u00df ich nimmer,", "tokens": ["Doch", "das", "ver\u00b7ge\u00df", "ich", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie sie mich angeschaut!", "tokens": ["Wie", "sie", "mich", "an\u00b7ge\u00b7schaut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}