{"textgrid.poem.58598": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer bin ich, gro\u00dfer Herr, ich Kind aus b\u00f6sem Samen,", "tokens": ["Wer", "bin", "ich", ",", "gro\u00b7\u00dfer", "Herr", ",", "ich", "Kind", "aus", "b\u00f6\u00b7sem", "Sa\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "ADJA", "NN", "$,", "PPER", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ich in deinem Blut so teu'r erl\u00f6set bin,", "tokens": ["Da\u00df", "ich", "in", "dei\u00b7nem", "Blut", "so", "teu'r", "er\u00b7l\u00f6\u00b7set", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ADV", "ADJD", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df du, o gro\u00dfe Gnad', mich rufst bei meinem Namen,", "tokens": ["Da\u00df", "du", ",", "o", "gro\u00b7\u00dfe", "Gnad'", ",", "mich", "rufst", "bei", "mei\u00b7nem", "Na\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "FM", "ADJA", "NN", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df ich soll deine sein \u2013 o Liebe, nimm mich hin!", "tokens": ["Da\u00df", "ich", "soll", "dei\u00b7ne", "sein", "\u2013", "o", "Lie\u00b7be", ",", "nimm", "mich", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "PPOSAT", "VAINF", "$(", "FM", "NN", "$,", "VVIMP", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "In kleinen Wassern ich dein Beisein hab' versp\u00fcret,", "tokens": ["In", "klei\u00b7nen", "Was\u00b7sern", "ich", "dein", "Bei\u00b7sein", "hab'", "ver\u00b7sp\u00fc\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Soll ich durch Str\u00f6me noch und Feuerproben gehn,", "tokens": ["Soll", "ich", "durch", "Str\u00f6\u00b7me", "noch", "und", "Feu\u00b7er\u00b7pro\u00b7ben", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "ADV", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Hilf dann, da\u00df nur mein Schaum und nicht mein Gold verlieret!", "tokens": ["Hilf", "dann", ",", "da\u00df", "nur", "mein", "Schaum", "und", "nicht", "mein", "Gold", "ver\u00b7lie\u00b7ret", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "KOUS", "ADV", "PPOSAT", "NN", "KON", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mein Heiland, Herr und Gott, la\u00df mich dich ewig sehn!", "tokens": ["Mein", "Hei\u00b7land", ",", "Herr", "und", "Gott", ",", "la\u00df", "mich", "dich", "e\u00b7wig", "sehn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "KON", "NN", "$,", "VVIMP", "PPER", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}