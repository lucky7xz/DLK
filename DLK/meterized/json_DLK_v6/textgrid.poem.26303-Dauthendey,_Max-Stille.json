{"textgrid.poem.26303": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Stille", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Haus ist's still. Ein Vogel lacht.", "tokens": ["Im", "Haus", "ist's", "still", ".", "Ein", "Vo\u00b7gel", "lacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "PTKVZ", "$.", "ART", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Garten sehn sich die Rosen um.", "tokens": ["Im", "Gar\u00b7ten", "sehn", "sich", "die", "Ro\u00b7sen", "um", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ihr Blick die Stille leichter macht.", "tokens": ["Ihr", "Blick", "die", "Stil\u00b7le", "leich\u00b7ter", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich horche auf den Donner hin,", "tokens": ["Ich", "hor\u00b7che", "auf", "den", "Don\u00b7ner", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf einer Wolke dumpf Gebrumm;", "tokens": ["Auf", "ei\u00b7ner", "Wol\u00b7ke", "dumpf", "Ge\u00b7brumm", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u2013 Wie ich mir unerkl\u00e4rlich bin!", "tokens": ["\u2013", "Wie", "ich", "mir", "un\u00b7er\u00b7kl\u00e4r\u00b7lich", "bin", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PPER", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}