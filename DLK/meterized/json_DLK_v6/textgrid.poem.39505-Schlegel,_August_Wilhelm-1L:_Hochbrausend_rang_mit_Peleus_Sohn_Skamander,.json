{"textgrid.poem.39505": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hochbrausend rang mit Peleus Sohn Skamander,", "genre": "verse", "period": "N.A.", "pub_year": 1801, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hochbrausend rang mit Peleus Sohn Skamander,", "tokens": ["Hoch\u00b7brau\u00b7send", "rang", "mit", "Pe\u00b7leus", "Sohn", "Ska\u00b7man\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "NE", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Held mu\u00df fliehn die Schlingen seiner Fluten;", "tokens": ["Der", "Held", "mu\u00df", "fliehn", "die", "Schlin\u00b7gen", "sei\u00b7ner", "Flu\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch z\u00e4hmen bald den Strom des Feuers Gluten,", "tokens": ["Doch", "z\u00e4h\u00b7men", "bald", "den", "Strom", "des", "Feu\u00b7ers", "Glu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des eignen Betts unwill'gen Salamander.", "tokens": ["Des", "eig\u00b7nen", "Betts", "un\u00b7will'\u00b7gen", "Sa\u00b7la\u00b7man\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Cydnus lud in die friedlichen Maeander,", "tokens": ["Cyd\u00b7nus", "lud", "in", "die", "fried\u00b7li\u00b7chen", "Mae\u00b7an\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Auf deren Spiegel Mittagsschatten ruhten;", "tokens": ["Auf", "de\u00b7ren", "Spie\u00b7gel", "Mit\u00b7tags\u00b7schat\u00b7ten", "ruh\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch mitten in dem s\u00fc\u00dfen Bad umfluten", "tokens": ["Doch", "mit\u00b7ten", "in", "dem", "s\u00fc\u00b7\u00dfen", "Bad", "um\u00b7flu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "NE", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Todes Schau'r den gro\u00dfen Alexander.", "tokens": ["Des", "To\u00b7des", "Schau'r", "den", "gro\u00b7\u00dfen", "A\u00b7lex\u00b7an\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "ADJA", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein gl\u00fchend Herz zagt nicht bei'm wilden Rauschen", "tokens": ["Ein", "gl\u00fc\u00b7hend", "Herz", "zagt", "nicht", "bei'm", "wil\u00b7den", "Rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PTKNEG", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Feindseligen Geschicks, und wird sich halten,", "tokens": ["Feind\u00b7se\u00b7li\u00b7gen", "Ge\u00b7schicks", ",", "und", "wird", "sich", "hal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "KON", "VAFIN", "PRF", "VVINF", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Schl\u00fcg' \u00fcber'm Haupt die Well' ihm auch zusammen.", "tokens": ["Schl\u00fcg'", "\u00fc\u00b7ber'm", "Haupt", "die", "Well'", "ihm", "auch", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "NN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch in der Wollust k\u00fchlem Schoo\u00dfe lauschen", "tokens": ["Doch", "in", "der", "Wol\u00b7lust", "k\u00fch\u00b7lem", "Schoo\u00b7\u00dfe", "lau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geheimes Grausen, b\u00e4ngliches Erkalten,", "tokens": ["Ge\u00b7hei\u00b7mes", "Grau\u00b7sen", ",", "b\u00e4ng\u00b7li\u00b7ches", "Er\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und l\u00f6schen der Begeistrung muth'ge Flammen.", "tokens": ["Und", "l\u00f6\u00b7schen", "der", "Be\u00b7geis\u00b7trung", "muth'\u00b7ge", "Flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}