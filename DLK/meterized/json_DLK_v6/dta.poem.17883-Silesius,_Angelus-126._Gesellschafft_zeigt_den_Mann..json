{"dta.poem.17883": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "126.  Gesellschafft zeigt den Mann.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Die losung der gespan. Wers gern mit Narren h\u00e4lt/", "tokens": ["Die", "lo\u00b7sung", "der", "ge\u00b7span", ".", "Wers", "gern", "mit", "Nar\u00b7ren", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVPP", "$.", "NN", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der ist kein kluger Mann: nicht gro\u00df/ wer mit der", "tokens": ["Der", "ist", "kein", "klu\u00b7ger", "Mann", ":", "nicht", "gro\u00df", "/", "wer", "mit", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PIAT", "ADJA", "NN", "$.", "PTKNEG", "ADJD", "$(", "PWS", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Welt.", "tokens": ["Welt", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}}}}}