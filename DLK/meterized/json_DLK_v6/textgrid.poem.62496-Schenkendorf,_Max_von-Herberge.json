{"textgrid.poem.62496": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Herberge", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In der Herberg' ist kein Raum!", "tokens": ["In", "der", "Her\u00b7ber\u00b7g'", "ist", "kein", "Raum", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Zu dem Stall Maria gehet,", "tokens": ["Zu", "dem", "Stall", "Ma\u00b7ria", "ge\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "--++-+-", "measure": "anapaest.init"}, "line.3": {"text": "Und ihr naht ein Himmelstraum,", "tokens": ["Und", "ihr", "naht", "ein", "Him\u00b7mel\u00b7straum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo das Thier voll Staunen stehet.", "tokens": ["Wo", "das", "Thier", "voll", "Stau\u00b7nen", "ste\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Den der Himmel selbst kaum fa\u00dft,", "tokens": ["Den", "der", "Him\u00b7mel", "selbst", "kaum", "fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird im schlechten Stall geboren;", "tokens": ["Wird", "im", "schlech\u00b7ten", "Stall", "ge\u00b7bo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Krippe liegt ein Gast,", "tokens": ["In", "der", "Krip\u00b7pe", "liegt", "ein", "Gast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dem das ew'ge Reich erkoren.", "tokens": ["Dem", "das", "ew'\u00b7ge", "Reich", "er\u00b7ko\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Merke das, du Menschenherz,", "tokens": ["Mer\u00b7ke", "das", ",", "du", "Men\u00b7schen\u00b7herz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PDS", "$,", "PPER", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Denke das an jedem Morgen!", "tokens": ["Den\u00b7ke", "das", "an", "je\u00b7dem", "Mor\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf, die Blicke himmelw\u00e4rts!", "tokens": ["Auf", ",", "die", "Bli\u00b7cke", "him\u00b7mel\u00b7w\u00e4rts", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und vergi\u00df die kleinen Sorgen.", "tokens": ["Und", "ver\u00b7gi\u00df", "die", "klei\u00b7nen", "Sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Herz, im irdischen Gedr\u00e4ng'", "tokens": ["Herz", ",", "im", "ir\u00b7di\u00b7schen", "Ge\u00b7dr\u00e4ng'"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Find'st du nimmermehr Gen\u00fcge:", "tokens": ["Fin\u00b7d'st", "du", "nim\u00b7mer\u00b7mehr", "Ge\u00b7n\u00fc\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wird die Erde dir zu eng,", "tokens": ["Wird", "die", "Er\u00b7de", "dir", "zu", "eng", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Denke nur an Jesu Wiege.", "tokens": ["Den\u00b7ke", "nur", "an", "Je\u00b7su", "Wie\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "NE", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}