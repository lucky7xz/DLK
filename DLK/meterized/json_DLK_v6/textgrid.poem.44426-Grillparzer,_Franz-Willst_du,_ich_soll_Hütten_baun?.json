{"textgrid.poem.44426": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Willst du, ich soll H\u00fctten baun?", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du, ich soll H\u00fctten baun?", "tokens": ["Willst", "du", ",", "ich", "soll", "H\u00fct\u00b7ten", "baun", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "PPER", "VMFIN", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Willst mich heimisch sehn?", "tokens": ["Willst", "mich", "hei\u00b7misch", "sehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ADJD", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Sieh im unbew\u00f6lkten Blaun", "tokens": ["Sieh", "im", "un\u00b7be\u00b7w\u00f6lk\u00b7ten", "Blaun"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hoch die Sonne stehn \u2013", "tokens": ["Hoch", "die", "Son\u00b7ne", "stehn", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVINF", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Eh sie sich im Westen neigt,", "tokens": ["Eh", "sie", "sich", "im", "Wes\u00b7ten", "neigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruft mich ein Gesch\u00e4ft,", "tokens": ["Ruft", "mich", "ein", "Ge\u00b7sch\u00e4ft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Rauh der Pfad, der Weg ist weit,", "tokens": ["Rauh", "der", "Pfad", ",", "der", "Weg", "ist", "weit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eile will sein Recht.", "tokens": ["Ei\u00b7le", "will", "sein", "Recht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Doch kehr abends ich zur\u00fcck,", "tokens": ["Doch", "kehr", "a\u00b7bends", "ich", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und du harrst noch mein.", "tokens": ["Und", "du", "harrst", "noch", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "PPOSAT", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn ich erst mein selber bin,", "tokens": ["Wenn", "ich", "erst", "mein", "sel\u00b7ber", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "ADV", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bin ich auch wohl dein.", "tokens": ["Bin", "ich", "auch", "wohl", "dein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "PPOSAT", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}