{"textgrid.poem.33995": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Freiheit der S\u00fcnde", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O glaube nicht, da\u00df du durch deine S\u00fcnde", "tokens": ["O", "glau\u00b7be", "nicht", ",", "da\u00df", "du", "durch", "dei\u00b7ne", "S\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Welt verwirrst! Wie du auch freveln m\u00f6gest,", "tokens": ["Die", "Welt", "ver\u00b7wirrst", "!", "Wie", "du", "auch", "fre\u00b7veln", "m\u00f6\u00b7gest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PWAV", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ob du Gott dein Ich auch ganz entz\u00f6gest,", "tokens": ["Und", "ob", "du", "Gott", "dein", "Ich", "auch", "ganz", "ent\u00b7z\u00f6\u00b7gest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "PPOSAT", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du hinderst nicht, da\u00df sie zum Kreis sich r\u00fcnde!", "tokens": ["Du", "hin\u00b7derst", "nicht", ",", "da\u00df", "sie", "zum", "Kreis", "sich", "r\u00fcn\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "$,", "KOUS", "PPER", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ja, ob du, in des innern Abgrunds Schl\u00fcnde", "tokens": ["Ja", ",", "ob", "du", ",", "in", "des", "in\u00b7nern", "Ab\u00b7grunds", "Schl\u00fcn\u00b7de"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "$,", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hinunter taumelnd, v\u00f6llig dich betr\u00f6gest", "tokens": ["Hin\u00b7un\u00b7ter", "tau\u00b7melnd", ",", "v\u00f6l\u00b7lig", "dich", "be\u00b7tr\u00f6\u00b7gest"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVPP", "$,", "ADJD", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und dich hinauf zur G\u00f6tter-Freiheit l\u00f6gest,", "tokens": ["Und", "dich", "hin\u00b7auf", "zur", "G\u00f6t\u00b7ter\u00b7Frei\u00b7heit", "l\u00f6\u00b7gest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch trifft dich das Gericht, das ich verk\u00fcnde!", "tokens": ["Doch", "trifft", "dich", "das", "Ge\u00b7richt", ",", "das", "ich", "ver\u00b7k\u00fcn\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wir leben nur im Ewigen und Wahren,", "tokens": ["Wir", "le\u00b7ben", "nur", "im", "E\u00b7wi\u00b7gen", "und", "Wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJA", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ihm entfliehen wollen, w\u00fcrde hei\u00dfen,", "tokens": ["Und", "ihm", "ent\u00b7flie\u00b7hen", "wol\u00b7len", ",", "w\u00fcr\u00b7de", "hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "VMFIN", "$,", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In uns'rer Brust den Odem anzuhalten;", "tokens": ["In", "un\u00b7s'\u00b7rer", "Brust", "den", "O\u00b7dem", "an\u00b7zu\u00b7hal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVIZU", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Wir k\u00f6nnen's, doch es wird sich offenbaren,", "tokens": ["Wir", "k\u00f6n\u00b7nen's", ",", "doch", "es", "wird", "sich", "of\u00b7fen\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KON", "PPER", "VAFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df wir das eig'ne Lebensband zerrei\u00dfen", "tokens": ["Da\u00df", "wir", "das", "eig'\u00b7ne", "Le\u00b7bens\u00b7band", "zer\u00b7rei\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Nichts dadurch im Aether umgestalten.", "tokens": ["Und", "Nichts", "da\u00b7durch", "im", "A\u00b7e\u00b7ther", "um\u00b7ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PAV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}}}}