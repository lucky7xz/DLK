{"dta.poem.4279": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das sch\u00f6ne W\u00fcrmchen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jm k\u00fchlen Schatten dichter Bl\u00e4tter", "tokens": ["Jm", "k\u00fch\u00b7len", "Schat\u00b7ten", "dich\u00b7ter", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sa\u00df ich, bey einem schw\u00fchlen Wetter,", "tokens": ["Sa\u00df", "ich", ",", "bey", "ei\u00b7nem", "schw\u00fch\u00b7len", "Wet\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Beschirmet vor der Sonnen Schein,", "tokens": ["Be\u00b7schir\u00b7met", "vor", "der", "Son\u00b7nen", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bedeckt von einem gr\u00fcnen Himmel,", "tokens": ["Be\u00b7deckt", "von", "ei\u00b7nem", "gr\u00fc\u00b7nen", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Entfernet von der Stadt Get\u00fcmmel,", "tokens": ["Ent\u00b7fer\u00b7net", "von", "der", "Stadt", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und dacht\u2019 auf GOtt und mich allein.", "tokens": ["Und", "dacht'", "auf", "Gott", "und", "mich", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie nun an diesem stillen Ort,", "tokens": ["Wie", "nun", "an", "die\u00b7sem", "stil\u00b7len", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nach meiner Art, bald hier, bald dort", "tokens": ["Nach", "mei\u00b7ner", "Art", ",", "bald", "hier", ",", "bald", "dort"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADV", "ADV", "$,", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die regen Blick\u2019, in gr\u00fcnen Tiefen", "tokens": ["Die", "re\u00b7gen", "Blick'", ",", "in", "gr\u00fc\u00b7nen", "Tie\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der Bl\u00e4tter, hin und wieder liefen,", "tokens": ["Der", "Bl\u00e4t\u00b7ter", ",", "hin", "und", "wie\u00b7der", "lie\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und, durch so vielfachs Schatten-Gr\u00fcn,", "tokens": ["Und", ",", "durch", "so", "viel\u00b7fachs", "Schat\u00b7ten\u00b7Gr\u00fcn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Oft in die Dunkelheit versunken,", "tokens": ["Oft", "in", "die", "Dun\u00b7kel\u00b7heit", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Erblickt\u2019 ich einen bunten Funken,", "tokens": ["Er\u00b7blickt'", "ich", "ei\u00b7nen", "bun\u00b7ten", "Fun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und sah\u2019 ihn hell und feurig gl\u00fch\u2019n,", "tokens": ["Und", "sah'", "ihn", "hell", "und", "feu\u00b7rig", "gl\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Zumahl ein kleiner Sonnen-Strahl,", "tokens": ["Zu\u00b7mahl", "ein", "klei\u00b7ner", "Son\u00b7nen\u00b7Strahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Durch eine Oefnung, auf ihn schien.", "tokens": ["Durch", "ei\u00b7ne", "Oef\u00b7nung", ",", "auf", "ihn", "schien", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Ich stutzt\u2019 ob seinem Glanz, und ging,", "tokens": ["Ich", "stutzt'", "ob", "sei\u00b7nem", "Glanz", ",", "und", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PPOSAT", "NN", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Jhn in der N\u00e4he zu beseh\u2019n,", "tokens": ["Jhn", "in", "der", "N\u00e4\u00b7he", "zu", "be\u00b7seh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Da ich in ihm ein wundersch\u00f6n", "tokens": ["Da", "ich", "in", "ihm", "ein", "wun\u00b7der\u00b7sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ART", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Gef\u00e4rbtes gl\u00e4nzend W\u00fcrmchen fing.", "tokens": ["Ge\u00b7f\u00e4rb\u00b7tes", "gl\u00e4n\u00b7zend", "W\u00fcrm\u00b7chen", "fing", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Nie hatt\u2019 ich noch ein herrlicher geschm\u00fcckt-", "tokens": ["Nie", "hatt'", "ich", "noch", "ein", "herr\u00b7li\u00b7cher", "ge\u00b7schm\u00fcck\u00b7t"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Ein herrlicher gef\u00e4rbtes Thier erblickt.", "tokens": ["Ein", "herr\u00b7li\u00b7cher", "ge\u00b7f\u00e4rb\u00b7tes", "Thier", "er\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Der Hinterleib war roht. Ein wirklicher Rubien", "tokens": ["Der", "Hin\u00b7ter\u00b7leib", "war", "roht", ".", "Ein", "wirk\u00b7li\u00b7cher", "Ru\u00b7bi\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Kann hell- und kr\u00e4ftiger nicht gl\u00fch\u2019n,", "tokens": ["Kann", "hell", "und", "kr\u00e4f\u00b7ti\u00b7ger", "nicht", "gl\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "TRUNC", "KON", "ADJD", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Als der gef\u00e4rbte Glanz, den ich in ihm erblickte,", "tokens": ["Als", "der", "ge\u00b7f\u00e4rb\u00b7te", "Glanz", ",", "den", "ich", "in", "ihm", "er\u00b7blick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Desselben obre Fl\u00e4che schm\u00fcckte.", "tokens": ["Des\u00b7sel\u00b7ben", "ob\u00b7re", "Fl\u00e4\u00b7che", "schm\u00fcck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Das Vordertheil legt, durch ein gl\u00e4nzend Blau,", "tokens": ["Das", "Vor\u00b7der\u00b7theil", "legt", ",", "durch", "ein", "gl\u00e4n\u00b7zend", "Blau", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Ein klein Sapphirchen uns zur Schau,", "tokens": ["Ein", "klein", "Sap\u00b7phir\u00b7chen", "uns", "zur", "Schau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Das aber, wenn es sich zumahl", "tokens": ["Das", "a\u00b7ber", ",", "wenn", "es", "sich", "zu\u00b7mahl"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "$,", "KOUS", "PPER", "PRF", "KOUS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Getroffen fand vom Sonnen-Strahl,", "tokens": ["Ge\u00b7trof\u00b7fen", "fand", "vom", "Son\u00b7nen\u00b7Strahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Durch ein besonders sch\u00f6n und lieblich Farben-Spiel,", "tokens": ["Durch", "ein", "be\u00b7son\u00b7ders", "sch\u00f6n", "und", "lieb\u00b7lich", "Fa\u00b7rben\u00b7Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJD", "KON", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zuweilen in ein Gr\u00fcn, fast als Smaragden, siel,", "tokens": ["Zu\u00b7wei\u00b7len", "in", "ein", "Gr\u00fcn", ",", "fast", "als", "Sma\u00b7rag\u00b7den", ",", "siel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ADV", "KOUS", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wie denn der Unterleib best\u00e4ndig gr\u00fcn,", "tokens": ["Wie", "denn", "der", "Un\u00b7ter\u00b7leib", "be\u00b7st\u00e4n\u00b7dig", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Smaragden gleich, in hellem Glanze schien.", "tokens": ["Sma\u00b7rag\u00b7den", "gleich", ",", "in", "hel\u00b7lem", "Glan\u00b7ze", "schien", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Von solchem gr\u00fcnen Glanz und Scheine", "tokens": ["Von", "sol\u00b7chem", "gr\u00fc\u00b7nen", "Glanz", "und", "Schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sind ebenfalls desselben Beine.", "tokens": ["Sind", "e\u00b7ben\u00b7falls", "des\u00b7sel\u00b7ben", "Bei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was man von Indiens bekanntem V\u00f6gelein,", "tokens": ["Was", "man", "von", "In\u00b7di\u00b7ens", "be\u00b7kann\u00b7tem", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem sch\u00f6nen Colibri (der ja so sch\u00f6n als klein,", "tokens": ["Dem", "sch\u00f6\u00b7nen", "Co\u00b7lib\u00b7ri", "(", "der", "ja", "so", "sch\u00f6n", "als", "klein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$(", "ART", "ADV", "ADV", "ADJD", "KOKOM", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der einer Fliege kaum an Gr\u00f6sse gleich soll seyn)", "tokens": ["Der", "ei\u00b7ner", "Flie\u00b7ge", "kaum", "an", "Gr\u00f6s\u00b7se", "gleich", "soll", "seyn", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "APPR", "NN", "ADV", "VMFIN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und seinen hellen Farben schreibet,", "tokens": ["Und", "sei\u00b7nen", "hel\u00b7len", "Far\u00b7ben", "schrei\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die man fast bis zum Glanz der Edelsteine treibet,", "tokens": ["Die", "man", "fast", "bis", "zum", "Glanz", "der", "E\u00b7del\u00b7stei\u00b7ne", "trei\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "APPR", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.6": {"text": "Und den, zum zierlichen Gepr\u00e4nge,", "tokens": ["Und", "den", ",", "zum", "zier\u00b7li\u00b7chen", "Ge\u00b7pr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Das Frauenzimmer dort als Ohr-Geh\u00e4nge", "tokens": ["Das", "Frau\u00b7en\u00b7zim\u00b7mer", "dort", "als", "Ohr\u00b7Ge\u00b7h\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "KOUS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Vern\u00fcnftig brauchen soll, geh\u00f6ret fast hieher;", "tokens": ["Ver\u00b7n\u00fcnf\u00b7tig", "brau\u00b7chen", "soll", ",", "ge\u00b7h\u00f6\u00b7ret", "fast", "hie\u00b7her", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VMFIN", "$,", "VVFIN", "ADV", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da jenes Federn Pracht unm\u00f6glich mehr,", "tokens": ["Da", "je\u00b7nes", "Fe\u00b7dern", "Pracht", "un\u00b7m\u00f6g\u00b7lich", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "NN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Als dieses W\u00fcrmchen, gl\u00e4nzen kann.", "tokens": ["Als", "die\u00b7ses", "W\u00fcrm\u00b7chen", ",", "gl\u00e4n\u00b7zen", "kann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "$,", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Man schau es denn so obenhin nicht an,", "tokens": ["Man", "schau", "es", "denn", "so", "o\u00b7ben\u00b7hin", "nicht", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "ADV", "ADV", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Vergn\u00fcge das Gesicht", "tokens": ["Ver\u00b7gn\u00fc\u00b7ge", "das", "Ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "An seines C\u00f6rpers buntem Licht.", "tokens": ["An", "sei\u00b7nes", "C\u00f6r\u00b7pers", "bun\u00b7tem", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Erwege Dessen Wunder-Macht,", "tokens": ["Er\u00b7we\u00b7ge", "Des\u00b7sen", "Wun\u00b7der\u00b7Macht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Der solcher Farben Glanz darinn gesenkt,", "tokens": ["Der", "sol\u00b7cher", "Far\u00b7ben", "Glanz", "da\u00b7rinn", "ge\u00b7senkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "NN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Und welcher uns das Wunder unsrer Augen,", "tokens": ["Und", "wel\u00b7cher", "uns", "das", "Wun\u00b7der", "uns\u00b7rer", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Wodurch wir das, was sch\u00f6n, zu sehen taugen,", "tokens": ["Wo\u00b7durch", "wir", "das", ",", "was", "sch\u00f6n", ",", "zu", "se\u00b7hen", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PDS", "$,", "PRELS", "ADJD", "$,", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Aus lauter Lieb\u2019, auf dieser Welt geschenkt.", "tokens": ["Aus", "lau\u00b7ter", "Lieb'", ",", "auf", "die\u00b7ser", "Welt", "ge\u00b7schenkt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}