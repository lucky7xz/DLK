{"textgrid.poem.32958": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Je \u00e4lter man wird; um so r\u00fccksichtsvoller", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Je \u00e4lter man wird; um so r\u00fccksichtsvoller", "tokens": ["Je", "\u00e4l\u00b7ter", "man", "wird", ";", "um", "so", "r\u00fcck\u00b7sichts\u00b7vol\u00b7ler"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "PIS", "VAFIN", "$.", "APPR", "ADV", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "ehrt man des andern Eigenart:", "tokens": ["ehrt", "man", "des", "an\u00b7dern", "Ei\u00b7gen\u00b7art", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "er trage, wie er will, sein Koller", "tokens": ["er", "tra\u00b7ge", ",", "wie", "er", "will", ",", "sein", "Kol\u00b7ler"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "VMFIN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und, wie ihm Spa\u00df macht, seinen Bart!", "tokens": ["und", ",", "wie", "ihm", "Spa\u00df", "macht", ",", "sei\u00b7nen", "Bart", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPER", "NN", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ob B\u00f6rsenl\u00f6we, Zeitungstiger,", "tokens": ["Ob", "B\u00f6r\u00b7sen\u00b7l\u00f6\u00b7we", ",", "Zei\u00b7tungs\u00b7ti\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KOUS", "NE", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "ob Dichter oder ob Claqueur ...", "tokens": ["ob", "Dich\u00b7ter", "o\u00b7der", "ob", "Cla\u00b7que\u00b7ur", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "KOUS", "NE", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "(am raschesten wird freilich Sieger,", "tokens": ["(", "am", "ra\u00b7sches\u00b7ten", "wird", "frei\u00b7lich", "Sie\u00b7ger", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "ADJA", "VAFIN", "ADV", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "wer Dichter und zugleich Claqueur) ...", "tokens": ["wer", "Dich\u00b7ter", "und", "zu\u00b7gleich", "Cla\u00b7que\u00b7ur", ")", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "NN", "KON", "ADV", "NN", "$(", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Ob Filz doch oder ob Cylinder,", "tokens": ["Ob", "Filz", "doch", "o\u00b7der", "ob", "Cy\u00b7lin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "KON", "KOUS", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "ob sch\u00e4big oder fein im Frack,", "tokens": ["ob", "sch\u00e4\u00b7big", "o\u00b7der", "fein", "im", "Frack", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KON", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ob Diener oder ob Bedienter,", "tokens": ["ob", "Die\u00b7ner", "o\u00b7der", "ob", "Be\u00b7dien\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hauptsache bleibt ... viel Geld im Sack!", "tokens": ["Haupt\u00b7sa\u00b7che", "bleibt", "...", "viel", "Geld", "im", "Sack", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$(", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}