{"textgrid.poem.30312": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "De\u00df B\u00fccherschreibens ist so viel; man schreibet sie mit hauffen.", "tokens": ["De\u00df", "B\u00fc\u00b7cher\u00b7schrei\u00b7bens", "ist", "so", "viel", ";", "man", "schrei\u00b7bet", "sie", "mit", "hauf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "$.", "PIS", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Niemand wird B\u00fccher schreiben mehr, so niemand sie wird kauffen.", "tokens": ["Nie\u00b7mand", "wird", "B\u00fc\u00b7cher", "schrei\u00b7ben", "mehr", ",", "so", "nie\u00b7mand", "sie", "wird", "kauf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "NN", "VVFIN", "ADV", "$,", "ADV", "PIS", "PPER", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}}}}