{"textgrid.poem.60216": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Freund, war dein Schlaf, so wie nach einer Schlacht", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund, war dein Schlaf, so wie nach einer Schlacht", "tokens": ["Freund", ",", "war", "dein", "Schlaf", ",", "so", "wie", "nach", "ei\u00b7ner", "Schlacht"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VAFIN", "PPOSAT", "NN", "$,", "ADV", "KOKOM", "APPR", "ART", "NN"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Des Feldherrn Schlummer ist, der ganze schwere N\u00e4chte", "tokens": ["Des", "Feld\u00b7herrn", "Schlum\u00b7mer", "ist", ",", "der", "gan\u00b7ze", "schwe\u00b7re", "N\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "$,", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bey seiner Lampe durchgewacht,", "tokens": ["Bey", "sei\u00b7ner", "Lam\u00b7pe", "durch\u00b7ge\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nichts, als nur daran gedacht,", "tokens": ["Und", "nichts", ",", "als", "nur", "da\u00b7ran", "ge\u00b7dacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "KOUS", "ADV", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie er den Sieg erhalten m\u00f6chte?", "tokens": ["Wie", "er", "den", "Sieg", "er\u00b7hal\u00b7ten", "m\u00f6ch\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "War deine Ruhe so, wie eines J\u00fcnglings ist,", "tokens": ["War", "dei\u00b7ne", "Ru\u00b7he", "so", ",", "wie", "ei\u00b7nes", "J\u00fcng\u00b7lings", "ist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "$,", "PWAV", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der mehr als Cicero zu \u00fcberreden wuste,", "tokens": ["Der", "mehr", "als", "Ci\u00b7ce\u00b7ro", "zu", "\u00fc\u00b7berr\u00b7e\u00b7den", "wus\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "KOKOM", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und der, die sich ergeben muste,", "tokens": ["Und", "der", ",", "die", "sich", "er\u00b7ge\u00b7ben", "mus\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "PRELS", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bewegungsgr\u00fcnde zugek\u00fc\u00dft?", "tokens": ["Be\u00b7we\u00b7gungs\u00b7gr\u00fcn\u00b7de", "zu\u00b7ge\u00b7k\u00fc\u00dft", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "War sie dem Schlaf des Schnitters gleich,", "tokens": ["War", "sie", "dem", "Schlaf", "des", "Schnit\u00b7ters", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der, ohne Gold bey schwarzem Brodte reich,", "tokens": ["Der", ",", "oh\u00b7ne", "Gold", "bey", "schwar\u00b7zem", "Brod\u00b7te", "reich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUI", "NN", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bey Wasser aus dem Quell zufrieden ist, und m\u00fcde", "tokens": ["Bey", "Was\u00b7ser", "aus", "dem", "Quell", "zu\u00b7frie\u00b7den", "ist", ",", "und", "m\u00fc\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "ADJD", "VAFIN", "$,", "KON", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Schlummerk\u00f6rner bald auf seinem Augenliede", "tokens": ["Die", "Schlum\u00b7mer\u00b7k\u00f6r\u00b7ner", "bald", "auf", "sei\u00b7nem", "Au\u00b7gen\u00b7lie\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sanft dr\u00fcckend liegen hat, wenn sie manch grosser Mann", "tokens": ["Sanft", "dr\u00fc\u00b7ckend", "lie\u00b7gen", "hat", ",", "wenn", "sie", "manch", "gros\u00b7ser", "Mann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "VVFIN", "VAFIN", "$,", "KOUS", "PPER", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Auf Purpurdecken w\u00fcnscht und nicht erseufzen kann?", "tokens": ["Auf", "Pur\u00b7pur\u00b7de\u00b7cken", "w\u00fcnscht", "und", "nicht", "er\u00b7seuf\u00b7zen", "kann", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "KON", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und schliefst du nun so s\u00fc\u00df, als Helden auf Troph\u00e4en,", "tokens": ["Und", "schliefst", "du", "nun", "so", "s\u00fc\u00df", ",", "als", "Hel\u00b7den", "auf", "Trop\u00b7h\u00e4\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$,", "KOUS", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wie ein J\u00fcngling, der am weichen Busen schlief?", "tokens": ["Und", "wie", "ein", "J\u00fcng\u00b7ling", ",", "der", "am", "wei\u00b7chen", "Bu\u00b7sen", "schlief", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "PRELS", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie Schnitter, die zur\u00fcck im Abendthaue gehen", "tokens": ["Wie", "Schnit\u00b7ter", ",", "die", "zu\u00b7r\u00fcck", "im", "A\u00b7bendt\u00b7hau\u00b7e", "ge\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "$,", "PRELS", "ADV", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vom Feld, auf welches sie die Morgensonne rief?", "tokens": ["Vom", "Feld", ",", "auf", "wel\u00b7ches", "sie", "die", "Mor\u00b7gen\u00b7son\u00b7ne", "rief", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "So sollst du deinen Traum mir sagen;", "tokens": ["So", "sollst", "du", "dei\u00b7nen", "Traum", "mir", "sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sprich! fuhrest du auf Venus Wagen,", "tokens": ["Sprich", "!", "fuh\u00b7rest", "du", "auf", "Ve\u00b7nus", "Wa\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "VVFIN", "PPER", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bespannt mit Tauben, die du l\u00e4ngst ersungen hast?", "tokens": ["Be\u00b7spannt", "mit", "Tau\u00b7ben", ",", "die", "du", "l\u00e4ngst", "er\u00b7sun\u00b7gen", "hast", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und sah dein Geist den pr\u00e4chtigsten Pallast,", "tokens": ["Und", "sah", "dein", "Geist", "den", "pr\u00e4ch\u00b7tigs\u00b7ten", "Pal\u00b7last", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.5": {"text": "Wie dort in Miltons Lied ihn schwarze Krieger bauen,", "tokens": ["Wie", "dort", "in", "Mil\u00b7tons", "Lied", "ihn", "schwar\u00b7ze", "Krie\u00b7ger", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "NE", "NN", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die aus der Erde Demant-Klippen hauen;", "tokens": ["Die", "aus", "der", "Er\u00b7de", "De\u00b7mant\u00b7Klip\u00b7pen", "hau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "War der Pallast gro\u00df wie Europa ist,", "tokens": ["War", "der", "Pal\u00b7last", "gro\u00df", "wie", "Eu\u00b7ro\u00b7pa", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "KOKOM", "NE", "VAFIN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Und waren die Tapezereyen", "tokens": ["Und", "wa\u00b7ren", "die", "Ta\u00b7pe\u00b7ze\u00b7re\u00b7yen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN"], "meter": "-+--++--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Gem\u00e4hlde, derer die im Schattenreich sich scheuen,", "tokens": ["Ge\u00b7m\u00e4hl\u00b7de", ",", "de\u00b7rer", "die", "im", "Schat\u00b7ten\u00b7reich", "sich", "scheu\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "APPRART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So oft ein Fremdling sagt, da\u00df sie die Welt vergi\u00dft,", "tokens": ["So", "oft", "ein", "Fremd\u00b7ling", "sagt", ",", "da\u00df", "sie", "die", "Welt", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Weil jedermann nur Friedrichs Thaten lie\u00dft?", "tokens": ["Weil", "je\u00b7der\u00b7mann", "nur", "Fried\u00b7richs", "Tha\u00b7ten", "lie\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und hat dich endlich dieser Wagen", "tokens": ["Und", "hat", "dich", "end\u00b7lich", "die\u00b7ser", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht vor ein pr\u00e4chtig Zelt getragen,", "tokens": ["Nicht", "vor", "ein", "pr\u00e4ch\u00b7tig", "Zelt", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo T\u00fcrken auf den Knien lagen?", "tokens": ["Wo", "T\u00fcr\u00b7ken", "auf", "den", "Kni\u00b7en", "la\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Entbothen sie des Sultans Gru\u00df,", "tokens": ["Ent\u00b7bo\u00b7then", "sie", "des", "Sul\u00b7tans", "Gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht mit versichernden Gebehrden,", "tokens": ["Nicht", "mit", "ver\u00b7si\u00b7chern\u00b7den", "Ge\u00b7behr\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Da\u00df Mustaph stolz drauf thut, ein Bundsgeno\u00df zu werden", "tokens": ["Da\u00df", "Mus\u00b7ta\u00b7ph", "stolz", "drauf", "thut", ",", "ein", "Bunds\u00b7ge\u00b7no\u00df", "zu", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADJD", "PAV", "VVFIN", "$,", "ART", "NN", "PTKZU", "VAINF"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Vom besten K\u00f6nige und gr\u00f6\u00dftem Held auf Erden?", "tokens": ["Vom", "bes\u00b7ten", "K\u00f6\u00b7ni\u00b7ge", "und", "gr\u00f6\u00df\u00b7tem", "Held", "auf", "Er\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "KON", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Und sahst du nicht erschrockne Russen fliehn,", "tokens": ["Und", "sahst", "du", "nicht", "er\u00b7schrock\u00b7ne", "Rus\u00b7sen", "fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Schaam auf ihren Wangen gl\u00fchn;", "tokens": ["Die", "Schaam", "auf", "ih\u00b7ren", "Wan\u00b7gen", "gl\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Oestreich, mit Entschlu\u00df zum Sterben oder Siegen,", "tokens": ["Und", "O\u00b7e\u00b7streich", ",", "mit", "Ent\u00b7schlu\u00df", "zum", "Ster\u00b7ben", "o\u00b7der", "Sie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "APPR", "NN", "APPRART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Noch einmal fechten und alsdann zu Boden liegen?", "tokens": ["Noch", "ein\u00b7mal", "fech\u00b7ten", "und", "als\u00b7dann", "zu", "Bo\u00b7den", "lie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "KON", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Sprich bist du nicht erwacht vom jauchzenden Berlin,", "tokens": ["Sprich", "bist", "du", "nicht", "er\u00b7wacht", "vom", "jauch\u00b7zen\u00b7den", "Ber\u00b7lin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PTKNEG", "VVPP", "APPRART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und hast, indem du bist erwacht,", "tokens": ["Und", "hast", ",", "in\u00b7dem", "du", "bist", "er\u00b7wacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "KOUS", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erst an den Frieden und zuletzt an mich gedacht?", "tokens": ["Erst", "an", "den", "Frie\u00b7den", "und", "zu\u00b7letzt", "an", "mich", "ge\u00b7dacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "KON", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}