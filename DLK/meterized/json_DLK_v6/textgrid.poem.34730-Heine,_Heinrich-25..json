{"textgrid.poem.34730": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "25.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sag mir, wer einst die Uhren erfund,", "tokens": ["Sag", "mir", ",", "wer", "einst", "die", "Uh\u00b7ren", "er\u00b7fund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PWS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Die Zeitabteilung, Minute und Stund'?", "tokens": ["Die", "Zeit\u00b7ab\u00b7tei\u00b7lung", ",", "Mi\u00b7nu\u00b7te", "und", "Stund'", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das war ein frierend trauriger Mann.", "tokens": ["Das", "war", "ein", "frie\u00b7rend", "trau\u00b7ri\u00b7ger", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Er sa\u00df in der Winternacht und sann,", "tokens": ["Er", "sa\u00df", "in", "der", "Win\u00b7ter\u00b7nacht", "und", "sann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und z\u00e4hlte der M\u00e4uschen heimliches Quicken", "tokens": ["Und", "z\u00e4hl\u00b7te", "der", "M\u00e4u\u00b7schen", "heim\u00b7li\u00b7ches", "Qui\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und des Holzwurms ebenm\u00e4\u00dfiges Picken.", "tokens": ["Und", "des", "Holz\u00b7wurms", "e\u00b7ben\u00b7m\u00e4\u00b7\u00dfi\u00b7ges", "Pi\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Sag mir, wer einst das K\u00fcssen erfund?", "tokens": ["Sag", "mir", ",", "wer", "einst", "das", "K\u00fcs\u00b7sen", "er\u00b7fund", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PWS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Das war ein gl\u00fchend gl\u00fccklicher Mund;", "tokens": ["Das", "war", "ein", "gl\u00fc\u00b7hend", "gl\u00fcck\u00b7li\u00b7cher", "Mund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Er k\u00fc\u00dfte und dachte nichts dabei.", "tokens": ["Er", "k\u00fc\u00df\u00b7te", "und", "dach\u00b7te", "nichts", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PIS", "PAV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Es war im sch\u00f6nen Monat Mai,", "tokens": ["Es", "war", "im", "sch\u00f6\u00b7nen", "Mo\u00b7nat", "Mai", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Blumen sind aus der Erde gesprungen,", "tokens": ["Die", "Blu\u00b7men", "sind", "aus", "der", "Er\u00b7de", "ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-++-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Die Sonne lachte, die V\u00f6gel sungen.", "tokens": ["Die", "Son\u00b7ne", "lach\u00b7te", ",", "die", "V\u00f6\u00b7gel", "sun\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}