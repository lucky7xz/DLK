{"textgrid.poem.51100": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es ringen die Str\u00f6me gewaltig zu Tal,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ringen die Str\u00f6me gewaltig zu Tal,", "tokens": ["Es", "rin\u00b7gen", "die", "Str\u00f6\u00b7me", "ge\u00b7wal\u00b7tig", "zu", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "APPR", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die Deutschen nach Einheit mit Feder und Stahl;", "tokens": ["Die", "Deut\u00b7schen", "nach", "Ein\u00b7heit", "mit", "Fe\u00b7der", "und", "Stahl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Der Neckar erreichet den wallenden Rhein,", "tokens": ["Der", "Ne\u00b7ckar", "er\u00b7rei\u00b7chet", "den", "wal\u00b7len\u00b7den", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NE", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Doch ewig mu\u00df deutsche Zerrissenheit sein.", "tokens": ["Doch", "e\u00b7wig", "mu\u00df", "deut\u00b7sche", "Zer\u00b7ris\u00b7sen\u00b7heit", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VMFIN", "ADJA", "NN", "VAINF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Die feindlichen St\u00e4mme, sie stritten im Land,", "tokens": ["Die", "feind\u00b7li\u00b7chen", "St\u00e4m\u00b7me", ",", "sie", "strit\u00b7ten", "im", "Land", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die Preu\u00dfen, die Hessen, die Bayern zuhand", "tokens": ["Die", "Preu\u00b7\u00dfen", ",", "die", "Hes\u00b7sen", ",", "die", "Bay\u00b7ern", "zu\u00b7hand"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Verfochten mit blutiger M\u00fche den Thron:", "tokens": ["Ver\u00b7foch\u00b7ten", "mit", "blu\u00b7ti\u00b7ger", "M\u00fc\u00b7he", "den", "Thron", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die Badischen sind gegen S\u00fcden geflohn.", "tokens": ["Die", "Ba\u00b7di\u00b7schen", "sind", "ge\u00b7gen", "S\u00fc\u00b7den", "ge\u00b7flohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.3": {"line.1": {"text": "Am Strand blieb ein H\u00e4uflein Rebellen zur\u00fcck,", "tokens": ["Am", "Strand", "blieb", "ein", "H\u00e4uf\u00b7lein", "Re\u00b7bel\u00b7len", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+--+-+---+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die finden zum Fliehn weder Furten noch Br\u00fcck',", "tokens": ["Die", "fin\u00b7den", "zum", "Fliehn", "we\u00b7der", "Fur\u00b7ten", "noch", "Br\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "KON", "NN", "ADV", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Vom Rotweine trinken die Neige sie noch", "tokens": ["Vom", "Rot\u00b7wei\u00b7ne", "trin\u00b7ken", "die", "Nei\u00b7ge", "sie", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "PPER", "ADV"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und bringen voll Wut ihrem Hecker ein Hoch.", "tokens": ["Und", "brin\u00b7gen", "voll", "Wut", "ih\u00b7rem", "He\u00b7cker", "ein", "Hoch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "NN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Da kracht es vom Walde, da blinkt es vom Berg,", "tokens": ["Da", "kracht", "es", "vom", "Wal\u00b7de", ",", "da", "blinkt", "es", "vom", "Berg", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Es fl\u00fcchtet der Fischer, es birgt sich der Ferg;", "tokens": ["Es", "fl\u00fcch\u00b7tet", "der", "Fi\u00b7scher", ",", "es", "birgt", "sich", "der", "Ferg", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ja blickt nur, ihr wilden Gesellen, euch an!", "tokens": ["Ja", "blickt", "nur", ",", "ihr", "wil\u00b7den", "Ge\u00b7sel\u00b7len", ",", "euch", "an", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,", "PPER", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wohl ist es um euere K\u00f6pfe getan!", "tokens": ["Wohl", "ist", "es", "um", "eu\u00b7e\u00b7re", "K\u00f6p\u00b7fe", "ge\u00b7tan", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Schon schimmert durch B\u00e4ume der Helm und der Speer,", "tokens": ["Schon", "schim\u00b7mert", "durch", "B\u00e4u\u00b7me", "der", "Helm", "und", "der", "Speer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Es fliegt der Husar auf der Stra\u00dfe daher;", "tokens": ["Es", "fliegt", "der", "Hu\u00b7sar", "auf", "der", "Stra\u00b7\u00dfe", "da\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "PAV", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Die Schifferin dr\u00fcben steht einsam am Bord,", "tokens": ["Die", "Schif\u00b7fe\u00b7rin", "dr\u00fc\u00b7ben", "steht", "ein\u00b7sam", "am", "Bord", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Schon schwenkt sie das Ruder, schon ist sie am Ort.", "tokens": ["Schon", "schwenkt", "sie", "das", "Ru\u00b7der", ",", "schon", "ist", "sie", "am", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "Sie springen mit bleichen Gesichtern hinein,", "tokens": ["Sie", "sprin\u00b7gen", "mit", "blei\u00b7chen", "Ge\u00b7sich\u00b7tern", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Fast ist der gebrechliche Nachen zu klein;", "tokens": ["Fast", "ist", "der", "ge\u00b7brech\u00b7li\u00b7che", "Na\u00b7chen", "zu", "klein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PTKA", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Mit M\u00e4nnern und Waffen zum Sinken beschwert,", "tokens": ["Mit", "M\u00e4n\u00b7nern", "und", "Waf\u00b7fen", "zum", "Sin\u00b7ken", "be\u00b7schwert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Hat schon sie das Schiff in die Fluten gekehrt.", "tokens": ["Hat", "schon", "sie", "das", "Schiff", "in", "die", "Flu\u00b7ten", "ge\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "Das ist eine d\u00fcstre Gesellschaft im Boot,", "tokens": ["Das", "ist", "ei\u00b7ne", "d\u00fcst\u00b7re", "Ge\u00b7sell\u00b7schaft", "im", "Boot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wie Blut weht am Hute die Feder so rot,", "tokens": ["Wie", "Blut", "weht", "am", "Hu\u00b7te", "die", "Fe\u00b7der", "so", "rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Zerrissen die Bluse, geschw\u00e4rzt das Gesicht,", "tokens": ["Zer\u00b7ris\u00b7sen", "die", "Blu\u00b7se", ",", "ge\u00b7schw\u00e4rzt", "das", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVPP", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "In den Augen flackert das Totenlicht!", "tokens": ["In", "den", "Au\u00b7gen", "fla\u00b7ckert", "das", "To\u00b7ten\u00b7licht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}, "stanza.8": {"line.1": {"text": "Ein d\u00fcrftiges F\u00e4hnlein im Winde sich rollt,", "tokens": ["Ein", "d\u00fcrf\u00b7ti\u00b7ges", "F\u00e4hn\u00b7lein", "im", "Win\u00b7de", "sich", "rollt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Aus schlechtem Kattun, das ist schwarz, rot und gold;", "tokens": ["Aus", "schlech\u00b7tem", "Kat\u00b7tun", ",", "das", "ist", "schwarz", ",", "rot", "und", "gold", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PDS", "VAFIN", "ADJD", "$,", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "So treibt auf den Wellen der schwankende Kahn,", "tokens": ["So", "treibt", "auf", "den", "Wel\u00b7len", "der", "schwan\u00b7ken\u00b7de", "Kahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die Schifferin sucht ihm die rettende Bahn.", "tokens": ["Die", "Schif\u00b7fe\u00b7rin", "sucht", "ihm", "die", "ret\u00b7ten\u00b7de", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.9": {"line.1": {"text": "Und wie sie die Mitte des Flusses erreicht,", "tokens": ["Und", "wie", "sie", "die", "Mit\u00b7te", "des", "Flus\u00b7ses", "er\u00b7reicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Schon Kugel auf Kugel das Wasser bestreicht;", "tokens": ["Schon", "Ku\u00b7gel", "auf", "Ku\u00b7gel", "das", "Was\u00b7ser", "be\u00b7streicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Sie schlagen ins Ruder, sie schlagen ins Schiff,", "tokens": ["Sie", "schla\u00b7gen", "ins", "Ru\u00b7der", ",", "sie", "schla\u00b7gen", "ins", "Schiff", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Es schweift um die Ohren der greuliche Pfiff.", "tokens": ["Es", "schweift", "um", "die", "Oh\u00b7ren", "der", "greu\u00b7li\u00b7che", "Pfiff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.10": {"line.1": {"text": "Da recken die Bursche sich fluchend empor,", "tokens": ["Da", "re\u00b7cken", "die", "Bur\u00b7sche", "sich", "flu\u00b7chend", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "ADJD", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und schnell f\u00e4hrt der schlummernde Blitz aus dem Rohr;", "tokens": ["Und", "schnell", "f\u00e4hrt", "der", "schlum\u00b7mern\u00b7de", "Blitz", "aus", "dem", "Rohr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Sie stemmen den Fu\u00df auf den schwebenden Rand", "tokens": ["Sie", "stem\u00b7men", "den", "Fu\u00df", "auf", "den", "schwe\u00b7ben\u00b7den", "Rand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und laden und senden die Kugeln ans Land.", "tokens": ["Und", "la\u00b7den", "und", "sen\u00b7den", "die", "Ku\u00b7geln", "ans", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "KON", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}}, "stanza.11": {"line.1": {"text": "Es rieselt im Nachen die purpurne Flut,", "tokens": ["Es", "rie\u00b7selt", "im", "Na\u00b7chen", "die", "pur\u00b7pur\u00b7ne", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--++-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Schifferin steht in dem tanzenden Blut;", "tokens": ["Die", "Schif\u00b7fe\u00b7rin", "steht", "in", "dem", "tan\u00b7zen\u00b7den", "Blut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Scharf streift ihr der Tod an den Br\u00fcsten vorbei,", "tokens": ["Scharf", "streift", "ihr", "der", "Tod", "an", "den", "Br\u00fcs\u00b7ten", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Das Aug h\u00e4ngt am Ziele nur sicher und frei.", "tokens": ["Das", "Aug", "h\u00e4ngt", "am", "Zie\u00b7le", "nur", "si\u00b7cher", "und", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.12": {"line.1": {"text": "Schon f\u00fchrt sie zerschossene Leichen an Bord,", "tokens": ["Schon", "f\u00fchrt", "sie", "zer\u00b7schos\u00b7se\u00b7ne", "Lei\u00b7chen", "an", "Bord", ","], "token_info": ["word",