{"dta.poem.21476": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Gralworte .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Ueber dem Dichten und Denken und Tr\u00e4umen,", "tokens": ["Ue\u00b7ber", "dem", "Dich\u00b7ten", "und", "Den\u00b7ken", "und", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Singen und Bilden und Bauen und Malen", "tokens": ["Sin\u00b7gen", "und", "Bil\u00b7den", "und", "Bau\u00b7en", "und", "Ma\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN", "KON", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Lasset uns nimmer s\u00e4umen,", "tokens": ["Las\u00b7set", "uns", "nim\u00b7mer", "s\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Uns, vom heiligen Geiste berufen,", "tokens": ["Uns", ",", "vom", "hei\u00b7li\u00b7gen", "Geis\u00b7te", "be\u00b7ru\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "N\u00e4her zu jenes Heiligthums Stufen", "tokens": ["N\u00e4\u00b7her", "zu", "je\u00b7nes", "Hei\u00b7lig\u00b7thums", "Stu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PDAT", "NN", "NN"], "meter": "---+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Die V\u00f6lker zu f\u00fchren, aus welchem strahlen", "tokens": ["Die", "V\u00f6l\u00b7ker", "zu", "f\u00fch\u00b7ren", ",", "aus", "wel\u00b7chem", "strah\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "APPR", "PWAT", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Als letzter und sch\u00f6nster Erdenlohn", "tokens": ["Als", "letz\u00b7ter", "und", "sch\u00f6ns\u00b7ter", "Er\u00b7den\u00b7lohn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "KON", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Erl\u00f6sung und Frieden hienieden schon.", "tokens": ["Er\u00b7l\u00f6\u00b7sung", "und", "Frie\u00b7den", "hien\u00b7ie\u00b7den", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "ADV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Der lockende Traum, der uns umtanzt,", "tokens": ["Der", "lo\u00b7cken\u00b7de", "Traum", ",", "der", "uns", "um\u00b7tanzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Von welchem immer die Menschen reden,", "tokens": ["Von", "wel\u00b7chem", "im\u00b7mer", "die", "Men\u00b7schen", "re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Als Eldorado, Atlantis, Eden \u2014", "tokens": ["Als", "El\u00b7do\u00b7ra\u00b7do", ",", "At\u00b7lan\u00b7tis", ",", "E\u00b7den"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "NE", "$,", "NE", "$,", "NN", "$("], "meter": "-++-+-+-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Er ward uns in die Seele gepflanzt,", "tokens": ["Er", "ward", "uns", "in", "die", "See\u00b7le", "ge\u00b7pflanzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Da\u00df wir d\u2019ran hangen und d\u2019ran glauben;", "tokens": ["Da\u00df", "wir", "d'\u00b7ran", "han\u00b7gen", "und", "d'\u00b7ran", "glau\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "VVINF", "KON", "PAV", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Nicht Noth,", "tokens": ["Nicht", "Noth", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Nicht Tod", "tokens": ["Nicht", "Tod"], "token_info": ["word", "word"], "pos": ["PTKNEG", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Soll dieses himmlischste Kleinod rauben.", "tokens": ["Soll", "die\u00b7ses", "himm\u00b7lischs\u00b7te", "Klei\u00b7nod", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "In ihm zu weben,", "tokens": ["In", "ihm", "zu", "we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Mit ihm zu streben,", "tokens": ["Mit", "ihm", "zu", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ihn weiter zu geben,", "tokens": ["Ihn", "wei\u00b7ter", "zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Den hehren Gedanken, wie Kerzenlicht,", "tokens": ["Den", "heh\u00b7ren", "Ge\u00b7dan\u00b7ken", ",", "wie", "Ker\u00b7zen\u00b7licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Wann todreif unser Leib zerbricht,", "tokens": ["Wann", "tod\u00b7reif", "un\u00b7ser", "Leib", "zer\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dies sei unser Leben,", "tokens": ["Dies", "sei", "un\u00b7ser", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.7": {"text": "Unser Ideal,               ", "tokens": ["Un\u00b7ser", "I\u00b7deal", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Heut und morgen, zu jeder Frist,", "tokens": ["Heut", "und", "mor\u00b7gen", ",", "zu", "je\u00b7der", "Frist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.9": {"text": "Bis die Erde geworden ist", "tokens": ["Bis", "die", "Er\u00b7de", "ge\u00b7wor\u00b7den", "ist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAPP", "VAFIN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "Ein heiliger Riesentempel des Gral!             ", "tokens": ["Ein", "hei\u00b7li\u00b7ger", "Rie\u00b7sen\u00b7tem\u00b7pel", "des", "Gral", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}}}}