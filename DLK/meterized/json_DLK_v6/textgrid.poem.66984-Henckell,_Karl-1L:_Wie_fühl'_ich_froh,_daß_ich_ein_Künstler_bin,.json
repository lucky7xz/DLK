{"textgrid.poem.66984": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie f\u00fchl' ich froh, da\u00df ich ein K\u00fcnstler bin,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie f\u00fchl' ich froh, da\u00df ich ein K\u00fcnstler bin,", "tokens": ["Wie", "f\u00fchl'", "ich", "froh", ",", "da\u00df", "ich", "ein", "K\u00fcnst\u00b7ler", "bin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In dem die Lust geheimer Formkraft waltet!", "tokens": ["In", "dem", "die", "Lust", "ge\u00b7hei\u00b7mer", "Form\u00b7kraft", "wal\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie f\u00fchrt mich zu dem Land der Sehnsucht hin,", "tokens": ["Sie", "f\u00fchrt", "mich", "zu", "dem", "Land", "der", "Sehn\u00b7sucht", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wo eine Sonne scheint, die nie erkaltet.", "tokens": ["Wo", "ei\u00b7ne", "Son\u00b7ne", "scheint", ",", "die", "nie", "er\u00b7kal\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "PRELS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}