{"textgrid.poem.57756": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Schneegans zieht, der Sommer geht,", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Schneegans zieht, der Sommer geht,", "tokens": ["Die", "Schnee\u00b7gans", "zieht", ",", "der", "Som\u00b7mer", "geht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Lieben ist vorbei,", "tokens": ["Das", "Lie\u00b7ben", "ist", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Leb wohl, mein Schatz, vergi\u00df mein nicht,", "tokens": ["Leb", "wohl", ",", "mein", "Schatz", ",", "ver\u00b7gi\u00df", "mein", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPOSAT", "NN", "$,", "VVIMP", "PPOSAT", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich bleib dir ewig treu;", "tokens": ["Ich", "bleib", "dir", "e\u00b7wig", "treu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vergi\u00dfmeinnicht, du Bl\u00fcmlein blau,", "tokens": ["Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", ",", "du", "Bl\u00fcm\u00b7lein", "blau", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bl\u00fcmlein blau im Morgentau,", "tokens": ["Bl\u00fcm\u00b7lein", "blau", "im", "Mor\u00b7gen\u00b7tau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Du sch\u00f6nstes auf der Au.", "tokens": ["Du", "sch\u00f6ns\u00b7tes", "auf", "der", "Au", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es rauscht der Wind im Birkenlaub,", "tokens": ["Es", "rauscht", "der", "Wind", "im", "Bir\u00b7ken\u00b7laub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Rauscht lauter Traurigkeit,", "tokens": ["Rauscht", "lau\u00b7ter", "Trau\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Leb wohl, mein Schatz, die Stunde schl\u00e4gt,", "tokens": ["Leb", "wohl", ",", "mein", "Schatz", ",", "die", "Stun\u00b7de", "schl\u00e4gt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schl\u00e4gt nichts als Herzeleid;", "tokens": ["Schl\u00e4gt", "nichts", "als", "Her\u00b7ze\u00b7leid", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "KOKOM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vergi\u00dfmeinnicht, du Bl\u00fcmlein blau,", "tokens": ["Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", ",", "du", "Bl\u00fcm\u00b7lein", "blau", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bl\u00fcmlein blau im Morgentau,", "tokens": ["Bl\u00fcm\u00b7lein", "blau", "im", "Mor\u00b7gen\u00b7tau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Du sch\u00f6nstes auf der Au.", "tokens": ["Du", "sch\u00f6ns\u00b7tes", "auf", "der", "Au", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Heide ist so taub und leer,", "tokens": ["Die", "Hei\u00b7de", "ist", "so", "taub", "und", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verbl\u00fcht ist ihre Zier", "tokens": ["Ver\u00b7bl\u00fcht", "ist", "ih\u00b7re", "Zier"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn neu der Maibaum sich begr\u00fcnt,", "tokens": ["Wenn", "neu", "der", "Mai\u00b7baum", "sich", "be\u00b7gr\u00fcnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kehr ich zur\u00fcck zu dir;", "tokens": ["Kehr", "ich", "zu\u00b7r\u00fcck", "zu", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vergi\u00dfmeinnicht, du Bl\u00fcmlein blau,", "tokens": ["Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", ",", "du", "Bl\u00fcm\u00b7lein", "blau", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bl\u00fcmlein blau im Morgentau,", "tokens": ["Bl\u00fcm\u00b7lein", "blau", "im", "Mor\u00b7gen\u00b7tau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Du sch\u00f6nstes auf der Au.", "tokens": ["Du", "sch\u00f6ns\u00b7tes", "auf", "der", "Au", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}