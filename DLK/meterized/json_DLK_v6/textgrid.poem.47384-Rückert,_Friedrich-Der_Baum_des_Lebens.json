{"textgrid.poem.47384": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Baum des Lebens", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Adam lag im Todeskampfe schon,", "tokens": ["Als", "A\u00b7dam", "lag", "im", "To\u00b7des\u00b7kamp\u00b7fe", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Schickt' er zum Paradiese seinen Sohn,", "tokens": ["Schickt'", "er", "zum", "Pa\u00b7ra\u00b7die\u00b7se", "sei\u00b7nen", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu holen einen Zweig vom Lebensbaum,", "tokens": ["Zu", "ho\u00b7len", "ei\u00b7nen", "Zweig", "vom", "Le\u00b7bens\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und zu genesen hofft' er noch davon.", "tokens": ["Und", "zu", "ge\u00b7ne\u00b7sen", "hofft'", "er", "noch", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "VVFIN", "PPER", "ADV", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Seth brach das Reis, und als er's hergebracht,", "tokens": ["Seth", "brach", "das", "Reis", ",", "und", "als", "er's", "her\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "KON", "KOUS", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "War schon des Vaters Lebenshauch entflohn.", "tokens": ["War", "schon", "des", "Va\u00b7ters", "Le\u00b7bens\u00b7hauch", "ent\u00b7flohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da pflanzten sie das Reis auf Adams Grab,", "tokens": ["Da", "pflanz\u00b7ten", "sie", "das", "Reis", "auf", "A\u00b7dams", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und fortgepflanzt ward es von Sohn zu Sohn.", "tokens": ["Und", "fort\u00b7ge\u00b7pflanzt", "ward", "es", "von", "Sohn", "zu", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "PPER", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Es wuchs, als in der Grube Joseph lag,", "tokens": ["Es", "wuchs", ",", "als", "in", "der", "Gru\u00b7be", "Jo\u00b7se\u00b7ph", "lag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "APPR", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.10": {"text": "Und Israel in der \u00e4gypt'schen Fron.", "tokens": ["Und", "Is\u00b7rael", "in", "der", "\u00e4\u00b7gypt'\u00b7schen", "Fron", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Des Baumes Bl\u00fcten gingen duftend auf,", "tokens": ["Des", "Bau\u00b7mes", "Bl\u00fc\u00b7ten", "gin\u00b7gen", "duf\u00b7tend", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Als David harfend sa\u00df auf seinem Thron.", "tokens": ["Als", "Da\u00b7vid", "har\u00b7fend", "sa\u00df", "auf", "sei\u00b7nem", "Thron", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "D\u00fcrr ward der Baum, als an dem Weg des Herrn", "tokens": ["D\u00fcrr", "ward", "der", "Baum", ",", "als", "an", "dem", "Weg", "des", "Herrn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "KOUS", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Irr' ward in seiner Weisheit Salomon.", "tokens": ["Irr'", "ward", "in", "sei\u00b7ner", "Weis\u00b7heit", "Sa\u00b7lo\u00b7mon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Doch die Geschlechter hofften, da\u00df ihn neu", "tokens": ["Doch", "die", "Ge\u00b7schlech\u00b7ter", "hoff\u00b7ten", ",", "da\u00df", "ihn", "neu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Beleben sollt' ein andrer Davidssohn.", "tokens": ["Be\u00b7le\u00b7ben", "sollt'", "ein", "an\u00b7drer", "Da\u00b7vids\u00b7sohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Das sah im Geist der Glaube, da er sa\u00df", "tokens": ["Das", "sah", "im", "Geist", "der", "Glau\u00b7be", ",", "da", "er", "sa\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Im Leid an Wasserfl\u00fcssen Babylon.", "tokens": ["Im", "Leid", "an", "Was\u00b7ser\u00b7fl\u00fcs\u00b7sen", "Ba\u00b7by\u00b7lon", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Und als der ew'ge Blitz vom Himmel kam,", "tokens": ["Und", "als", "der", "ew'\u00b7ge", "Blitz", "vom", "Him\u00b7mel", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Zerbarst der Baum mit hellem Jubelton;", "tokens": ["Zer\u00b7barst", "der", "Baum", "mit", "hel\u00b7lem", "Ju\u00b7bel\u00b7ton", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Begnadigt ward der d\u00fcrre Stamm von Gott,", "tokens": ["Be\u00b7gna\u00b7digt", "ward", "der", "d\u00fcr\u00b7re", "Stamm", "von", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Zu dienen zu dem Holz der Passion.", "tokens": ["Zu", "die\u00b7nen", "zu", "dem", "Holz", "der", "Pas\u00b7si\u00b7on", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Es zimmerte die blinde Welt aus ihm", "tokens": ["Es", "zim\u00b7mer\u00b7te", "die", "blin\u00b7de", "Welt", "aus", "ihm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Das Kreuz und schlug ihr Heil daran mit Hohn.", "tokens": ["Das", "Kreuz", "und", "schlug", "ihr", "Heil", "da\u00b7ran", "mit", "Hohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "PPOSAT", "NN", "PAV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Da trug der Baum des Lebens blut'ge Frucht,", "tokens": ["Da", "trug", "der", "Baum", "des", "Le\u00b7bens", "blut'\u00b7ge", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Da\u00df, wer sie koste, Leben sei sein Lohn.", "tokens": ["Da\u00df", ",", "wer", "sie", "kos\u00b7te", ",", "Le\u00b7ben", "sei", "sein", "Lohn", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWS", "PPER", "VVFIN", "$,", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "O ", "tokens": ["O"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.28": {"text": "Ausbreitend sich, je mehr ihm St\u00fcrme drohn.", "tokens": ["Aus\u00b7brei\u00b7tend", "sich", ",", "je", "mehr", "ihm", "St\u00fcr\u00b7me", "drohn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PRF", "$,", "ADV", "ADV", "PPER", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.29": {"text": "Die ganze Welt ruh' unter seinem Schirm!", "tokens": ["Die", "gan\u00b7ze", "Welt", "ruh'", "un\u00b7ter", "sei\u00b7nem", "Schirm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Die halbe ruht in seinem Schatten schon.", "tokens": ["Die", "hal\u00b7be", "ruht", "in", "sei\u00b7nem", "Schat\u00b7ten", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}