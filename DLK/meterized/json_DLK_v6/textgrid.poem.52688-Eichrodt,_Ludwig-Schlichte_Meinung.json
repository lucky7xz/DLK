{"textgrid.poem.52688": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Schlichte Meinung", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist nicht sch\u00f6n von Angesicht,", "tokens": ["Du", "bist", "nicht", "sch\u00f6n", "von", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die unbedachte Menge spricht,", "tokens": ["Die", "un\u00b7be\u00b7dach\u00b7te", "Men\u00b7ge", "spricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch deiner Anmuth ewger Reiz", "tokens": ["Doch", "dei\u00b7ner", "An\u00b7muth", "ew\u00b7ger", "Reiz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist nur der Urquell ihres Neids.", "tokens": ["Ist", "nur", "der", "Ur\u00b7quell", "ih\u00b7res", "Neids", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du bist nicht sch\u00f6n von Angesicht,", "tokens": ["Du", "bist", "nicht", "sch\u00f6n", "von", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Behauptet jeder Kennerwicht.", "tokens": ["Be\u00b7haup\u00b7tet", "je\u00b7der", "Ken\u00b7ner\u00b7wicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich selbst gestehs. Was mich erfreut,", "tokens": ["Ich", "selbst", "ge\u00b7stehs", ".", "Was", "mich", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$.", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist deine Liebensw\u00fcrdigkeit!", "tokens": ["Ist", "dei\u00b7ne", "Lie\u00b7bens\u00b7w\u00fcr\u00b7dig\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du bist nicht sch\u00f6n von Angesicht,", "tokens": ["Du", "bist", "nicht", "sch\u00f6n", "von", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Persianisch deine Wimper nicht,", "tokens": ["Per\u00b7si\u00b7a\u00b7nisch", "dei\u00b7ne", "Wim\u00b7per", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nicht griechisch, Theure, dein Profil,", "tokens": ["Nicht", "grie\u00b7chisch", ",", "Theu\u00b7re", ",", "dein", "Pro\u00b7fil", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "ADJA", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kein R\u00f6merzug dein Mienenspiel!", "tokens": ["Kein", "R\u00f6\u00b7mer\u00b7zug", "dein", "Mie\u00b7nen\u00b7spiel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du bist nicht sch\u00f6n von Angesicht?", "tokens": ["Du", "bist", "nicht", "sch\u00f6n", "von", "An\u00b7ge\u00b7sicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So tr\u00f6ste dich ein Lobgedicht", "tokens": ["So", "tr\u00f6s\u00b7te", "dich", "ein", "Lob\u00b7ge\u00b7dicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf deine Liebe allbekannt", "tokens": ["Auf", "dei\u00b7ne", "Lie\u00b7be", "all\u00b7be\u00b7kannt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Freigebige und schlanke Hand!", "tokens": ["Frei\u00b7ge\u00b7bi\u00b7ge", "und", "schlan\u00b7ke", "Hand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Du bist nicht sch\u00f6n von Angesicht \u2013", "tokens": ["Du", "bist", "nicht", "sch\u00f6n", "von", "An\u00b7ge\u00b7sicht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "APPR", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch wei\u00df ein kritisches Gez\u00fccht,", "tokens": ["Doch", "wei\u00df", "ein", "kri\u00b7ti\u00b7sches", "Ge\u00b7z\u00fccht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es flieht vor ihm und seinem Gru\u00df", "tokens": ["Es", "flieht", "vor", "ihm", "und", "sei\u00b7nem", "Gru\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein feiner und bescheidner Fu\u00df.", "tokens": ["Dein", "fei\u00b7ner", "und", "be\u00b7scheid\u00b7ner", "Fu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du bist nicht sch\u00f6n von Angesicht.", "tokens": ["Du", "bist", "nicht", "sch\u00f6n", "von", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "R\u00fchrt euch die Wangenrose nicht?", "tokens": ["R\u00fchrt", "euch", "die", "Wan\u00b7gen\u00b7ro\u00b7se", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Mundes s\u00fc\u00dfe Kirsche pfl\u00fcckt!", "tokens": ["Des", "Mun\u00b7des", "s\u00fc\u00b7\u00dfe", "Kir\u00b7sche", "pfl\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr armen Schlucker w\u00e4rt entz\u00fcckt.", "tokens": ["Ihr", "ar\u00b7men", "Schlu\u00b7cker", "w\u00e4rt", "ent\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Du bist nicht sch\u00f6n von Angesicht \u2013", "tokens": ["Du", "bist", "nicht", "sch\u00f6n", "von", "An\u00b7ge\u00b7sicht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "APPR", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Leucht auf, o keusches Augenlicht!", "tokens": ["Leucht", "auf", ",", "o", "keu\u00b7sches", "Au\u00b7gen\u00b7licht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Geist in deinem Glanze wohnt", "tokens": ["Ein", "Geist", "in", "dei\u00b7nem", "Glan\u00b7ze", "wohnt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als wie der Friede in dem Mond.", "tokens": ["Als", "wie", "der", "Frie\u00b7de", "in", "dem", "Mond", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Du bist nicht sch\u00f6n von Angesicht,", "tokens": ["Du", "bist", "nicht", "sch\u00f6n", "von", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Lockenhaar ist leicht Gewicht;", "tokens": ["Dein", "Lo\u00b7cken\u00b7haar", "ist", "leicht", "Ge\u00b7wicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein volles Herz doch wiegt mir mehr,", "tokens": ["Dein", "vol\u00b7les", "Herz", "doch", "wiegt", "mir", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es arm zu machen, das ist schwer.", "tokens": ["Es", "arm", "zu", "ma\u00b7chen", ",", "das", "ist", "schwer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$,", "PDS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Du bist nicht sch\u00f6n von Angesicht,", "tokens": ["Du", "bist", "nicht", "sch\u00f6n", "von", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Schwung der Brauen dirs gebricht;", "tokens": ["Am", "Schwung", "der", "Brau\u00b7en", "dirs", "ge\u00b7bricht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch deiner Seele hoher Schwung", "tokens": ["Doch", "dei\u00b7ner", "See\u00b7le", "ho\u00b7her", "Schwung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Entwaffnet die Verkleinerung.", "tokens": ["Ent\u00b7waff\u00b7net", "die", "Ver\u00b7klei\u00b7ne\u00b7rung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Du bist nicht sch\u00f6n von Angesicht,", "tokens": ["Du", "bist", "nicht", "sch\u00f6n", "von", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ach Gott! von Marmor bist du nicht!", "tokens": ["Ach", "Gott", "!", "von", "Mar\u00b7mor", "bist", "du", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "APPR", "NN", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Holdselig aber bist du, Kind,", "tokens": ["Hold\u00b7se\u00b7lig", "a\u00b7ber", "bist", "du", ",", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "ADV", "VAFIN", "PPER", "$,", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Wie Engel in dem Himmel sind!", "tokens": ["Wie", "En\u00b7gel", "in", "dem", "Him\u00b7mel", "sind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Du bist nicht sch\u00f6n von Angesicht.", "tokens": ["Du", "bist", "nicht", "sch\u00f6n", "von", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch Liebe geht nicht ins Gericht,", "tokens": ["Doch", "Lie\u00b7be", "geht", "nicht", "ins", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch Liebe ist der sch\u00f6nste \u2013 Zug,", "tokens": ["Doch", "Lie\u00b7be", "ist", "der", "sch\u00f6ns\u00b7te", "\u2013", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ART", "ADJA", "$(", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Liebe ist sich selbst genug.", "tokens": ["Und", "Lie\u00b7be", "ist", "sich", "selbst", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PRF", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}