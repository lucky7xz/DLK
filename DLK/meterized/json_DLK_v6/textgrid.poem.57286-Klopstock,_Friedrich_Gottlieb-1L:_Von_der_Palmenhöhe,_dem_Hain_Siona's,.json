{"textgrid.poem.57286": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von der Palmenh\u00f6he, dem Hain Siona's,", "genre": "verse", "period": "N.A.", "pub_year": 1766, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von der Palmenh\u00f6he, dem Hain Siona's,", "tokens": ["Von", "der", "Pal\u00b7men\u00b7h\u00f6\u00b7he", ",", "dem", "Hain", "Si\u00b7o\u00b7na's", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "NE", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kommen wir her, wir des Harfengesangs", "tokens": ["Kom\u00b7men", "wir", "her", ",", "wir", "des", "Har\u00b7fen\u00b7ge\u00b7sangs"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "PTKVZ", "$,", "PPER", "ART", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Geweihte, dass Christen noch einst", "tokens": ["Ge\u00b7weih\u00b7te", ",", "dass", "Chris\u00b7ten", "noch", "einst"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "NN", "ADV", "ADV"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Wir entflammen mit dem Feuer,", "tokens": ["Wir", "ent\u00b7flam\u00b7men", "mit", "dem", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Das zu Gott steigt! Hier in dem Hain, wo Eichen", "tokens": ["Das", "zu", "Gott", "steigt", "!", "Hier", "in", "dem", "Hain", ",", "wo", "Ei\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "APPR", "NN", "VVFIN", "$.", "ADV", "APPR", "ART", "NN", "$,", "PWAV", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Schatten, erschallst sch\u00f6ner, Telyn, auch du,", "tokens": ["Schat\u00b7ten", ",", "er\u00b7schallst", "sch\u00f6\u00b7ner", ",", "Te\u00b7lyn", ",", "auch", "du", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADJD", "$,", "NN", "$,", "ADV", "PPER", "$,"], "meter": "+---+--+-+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Wenn Sch\u00f6ne des Herzens voran", "tokens": ["Wenn", "Sch\u00f6\u00b7ne", "des", "Her\u00b7zens", "vo\u00b7ran"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ART", "NN", "PTKVZ"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Vor der Sch\u00f6nheit des Gesangs fleugt!", "tokens": ["Vor", "der", "Sch\u00f6n\u00b7heit", "des", "Ge\u00b7sangs", "fleugt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Mit Entz\u00fcckung, wall' ich im Hain der Palmen,", "tokens": ["Mit", "Ent\u00b7z\u00fc\u00b7ckung", ",", "wall'", "ich", "im", "Hain", "der", "Pal\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dichter, mit Lust, hier, wo Eich' und ihr Graun", "tokens": ["Dich\u00b7ter", ",", "mit", "Lust", ",", "hier", ",", "wo", "Eich'", "und", "ihr", "Graun"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "NN", "$,", "ADV", "$,", "PWAV", "NN", "KON", "PPOSAT", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Uns d\u00e4mmert, das Vaterland euch,", "tokens": ["Uns", "d\u00e4m\u00b7mert", ",", "das", "Va\u00b7ter\u00b7land", "euch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "PPER", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mich hinauf rief, ihm zu singen.", "tokens": ["Mich", "hin\u00b7auf", "rief", ",", "ihm", "zu", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "O bekr\u00e4nzet froh euch das Haupt, Thuiskons", "tokens": ["O", "be\u00b7kr\u00e4n\u00b7zet", "froh", "euch", "das", "Haupt", ",", "Thu\u00b7is\u00b7kons"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "VVFIN", "ADJD", "PPER", "ART", "NN", "$,", "NE"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Enkel! empfangt Braga's heiliges Laub!", "tokens": ["En\u00b7kel", "!", "emp\u00b7fangt", "Bra\u00b7ga's", "hei\u00b7li\u00b7ges", "Laub", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "NE", "ADJA", "NN", "$."], "meter": "+--++-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Er bringt es den H\u00fcgel herab,", "tokens": ["Er", "bringt", "es", "den", "H\u00fc\u00b7gel", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Wie es glanzvoll von dem Quell tr\u00e4uft!", "tokens": ["Wie", "es", "glanz\u00b7voll", "von", "dem", "Quell", "tr\u00e4uft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Mit des Stolzes T\u00f6nen erschallt, (ihr wurdet,", "tokens": ["Mit", "des", "Stol\u00b7zes", "T\u00f6\u00b7nen", "er\u00b7schallt", ",", "(", "ihr", "wur\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,", "$(", "PPER", "VAFIN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Dichter, sein Stolz!) Braga's freudiges Lied!", "tokens": ["Dich\u00b7ter", ",", "sein", "Stolz", "!", ")", "Bra\u00b7ga's", "freu\u00b7di\u00b7ges", "Lied", "!"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$.", "$(", "NE", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Ihr tranket mit ihm aus dem Quell", "tokens": ["Ihr", "tran\u00b7ket", "mit", "ihm", "aus", "dem", "Quell"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Begeistrung, und der Weisheit;", "tokens": ["Der", "Be\u00b7geis\u00b7trung", ",", "und", "der", "Weis\u00b7heit", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und ihr s\u00e4umt noch? Singet ihm nach! Ihr siegtet", "tokens": ["Und", "ihr", "s\u00e4umt", "noch", "?", "Sin\u00b7get", "ihm", "nach", "!", "Ihr", "sieg\u00b7tet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$.", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00dcber die Zeit! Deutschlands F\u00fcrsten ... Sie rief", "tokens": ["\u00dc\u00b7ber", "die", "Zeit", "!", "Deutschlands", "F\u00fcrs\u00b7ten", "...", "Sie", "rief"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "NE", "NN", "$(", "PPER", "VVFIN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Kein Stolz, euch zu leiten, herzu;", "tokens": ["Kein", "Stolz", ",", "euch", "zu", "lei\u00b7ten", ",", "her\u00b7zu", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Und allein schwangt, was auch obstand,", "tokens": ["Und", "al\u00b7lein", "schwangt", ",", "was", "auch", "obs\u00b7tand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "PRELS", "ADV", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ihr mit edler K\u00fchnheit euch auf! So werde", "tokens": ["Ihr", "mit", "ed\u00b7ler", "K\u00fchn\u00b7heit", "euch", "auf", "!", "So", "wer\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "APPR", "ADJA", "NN", "PPER", "PTKVZ", "$.", "ADV", "VAFIN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Euch denn allein auch unsterblicher Ruhm!", "tokens": ["Euch", "denn", "al\u00b7lein", "auch", "uns\u00b7terb\u00b7li\u00b7cher", "Ruhm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "+--++-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Der Name der F\u00fcrsten verweh,", "tokens": ["Der", "Na\u00b7me", "der", "F\u00fcrs\u00b7ten", "ver\u00b7weh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Wie der Nachhall, wenn der Ruf schweigt.", "tokens": ["Wie", "der", "Nach\u00b7hall", ",", "wenn", "der", "Ruf", "schweigt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Aus dem Hain Thuiskons entflieh kein sanftes", "tokens": ["Aus", "dem", "Hain", "Thu\u00b7is\u00b7kons", "ent\u00b7flieh", "kein", "sanf\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NE", "VVFIN", "PIAT", "ADJA"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Silberget\u00f6n hin zum parischen Maal,", "tokens": ["Sil\u00b7ber\u00b7ge\u00b7t\u00f6n", "hin", "zum", "pa\u00b7ri\u00b7schen", "Maal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Das keiner besucht, und das bald,", "tokens": ["Das", "kei\u00b7ner", "be\u00b7sucht", ",", "und", "das", "bald", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVPP", "$,", "KON", "PDS", "ADV", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "In den Staub sinkt der Gebeine,", "tokens": ["In", "den", "Staub", "sinkt", "der", "Ge\u00b7bei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "O wie festlich rauschet der Hain! Ich sehe", "tokens": ["O", "wie", "fest\u00b7lich", "rau\u00b7schet", "der", "Hain", "!", "Ich", "se\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "KOKOM", "ADJD", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Fliegenden Tanz! Braga f\u00fchrt den Triumph!", "tokens": ["Flie\u00b7gen\u00b7den", "Tanz", "!", "Bra\u00b7ga", "f\u00fchrt", "den", "Tri\u00b7umph", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "NE", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Unsterblichkeit! rufet das Chor,", "tokens": ["U\u00b7nsterb\u00b7lich\u00b7keit", "!", "ru\u00b7fet", "das", "Chor", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und der Hain rufts in den Schatten!", "tokens": ["Und", "der", "Hain", "rufts", "in", "den", "Schat\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.10": {"line.1": {"text": "Pyramiden sanken! der Wandrer findet", "tokens": ["Py\u00b7ra\u00b7mi\u00b7den", "san\u00b7ken", "!", "der", "Wand\u00b7rer", "fin\u00b7det"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVINF", "$.", "ART", "NN", "VVFIN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Tr\u00fcmmer nur noch! Lobschrift, welche die Burg", "tokens": ["Tr\u00fcm\u00b7mer", "nur", "noch", "!", "Lob\u00b7schrift", ",", "wel\u00b7che", "die", "Burg"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "ADV", "$.", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Des F\u00fcrsten nur kante, sie schl\u00e4ft", "tokens": ["Des", "F\u00fcrs\u00b7ten", "nur", "kan\u00b7te", ",", "sie", "schl\u00e4ft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "In dem Goldsaal, wie im Grabe!", "tokens": ["In", "dem", "Gold\u00b7saal", ",", "wie", "im", "Gra\u00b7be", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Pyramiden, liegt ihr! und schlaf, des Schmeichlers", "tokens": ["Py\u00b7ra\u00b7mi\u00b7den", ",", "liegt", "ihr", "!", "und", "schlaf", ",", "des", "Schmeich\u00b7lers"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PPER", "$.", "KON", "VVFIN", "$,", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Werk, in des Saals Gruft, nicht weckbar! Uns macht", "tokens": ["Werk", ",", "in", "des", "Saals", "Gruft", ",", "nicht", "weck\u00b7bar", "!", "Uns", "macht"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "APPR", "ART", "NN", "NN", "$,", "PTKNEG", "ADJD", "$.", "PPER", "VVFIN"], "meter": "+--++-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Unsterblich des Genius Flug,", "tokens": ["U\u00b7nsterb\u00b7lich", "des", "Ge\u00b7nius", "Flug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Und die K\u00fchnheit des Entschlusses,", "tokens": ["Und", "die", "K\u00fchn\u00b7heit", "des", "Ent\u00b7schlus\u00b7ses", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Von des Lohns Verachtung entflamt! Einst kontet,", "tokens": ["Von", "des", "Lohns", "Ver\u00b7ach\u00b7tung", "ent\u00b7flamt", "!", "Einst", "kon\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$.", "ADV", "VMFIN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "F\u00fcrsten, ihrs thun! Baut von Marmor euch jezt", "tokens": ["F\u00fcrs\u00b7ten", ",", "ihrs", "thun", "!", "Baut", "von", "Mar\u00b7mor", "euch", "jezt"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "VVINF", "$.", "VVFIN", "APPR", "NN", "PPER", "ADV"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Die Maale, vergessen zu ruhn!", "tokens": ["Die", "Maa\u00b7le", ",", "ver\u00b7ges\u00b7sen", "zu", "ruhn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Denn es schweigt euch in dem Haine.", "tokens": ["Denn", "es", "schweigt", "euch", "in", "dem", "Hai\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}