{"textgrid.poem.59313": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der verzweifelte Liebhaber", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Studieren will nichts bringen,", "tokens": ["Stu\u00b7die\u00b7ren", "will", "nichts", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Rock h\u00e4lt keinen Stich,", "tokens": ["Mein", "Rock", "h\u00e4lt", "kei\u00b7nen", "Stich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Meine Zither will nicht klingen,", "tokens": ["Mei\u00b7ne", "Zi\u00b7ther", "will", "nicht", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mein Schatz, der mag mich nicht.", "tokens": ["Mein", "Schatz", ",", "der", "mag", "mich", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich wollt, im Gr\u00fcn spazierte", "tokens": ["Ich", "wollt", ",", "im", "Gr\u00fcn", "spa\u00b7zier\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die allersch\u00f6nste Frau,", "tokens": ["Die", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Frau", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich w\u00e4r ein Drach und f\u00fchrte", "tokens": ["Ich", "w\u00e4r", "ein", "Drach", "und", "f\u00fchr\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie mit mir fort durchs Blau.", "tokens": ["Sie", "mit", "mir", "fort", "durchs", "Blau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich wollt, ich jagt ger\u00fcstet", "tokens": ["Ich", "wollt", ",", "ich", "jagt", "ge\u00b7r\u00fcs\u00b7tet"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VVFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und legt die Lanze aus,", "tokens": ["Und", "legt", "die", "Lan\u00b7ze", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und jagte all Philister", "tokens": ["Und", "jag\u00b7te", "all", "Phi\u00b7lis\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zur sch\u00f6nen Welt hinaus.", "tokens": ["Zur", "sch\u00f6\u00b7nen", "Welt", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich wollt, ich s\u00e4\u00df jetzunder", "tokens": ["Ich", "wollt", ",", "ich", "s\u00e4\u00df", "je\u00b7tzun\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Himmel still und weit,", "tokens": ["Im", "Him\u00b7mel", "still", "und", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und fr\u00fcg nach all dem Plunder", "tokens": ["Und", "fr\u00fcg", "nach", "all", "dem", "Plun\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PIAT", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nichts vor Zufriedenheit.", "tokens": ["Nichts", "vor", "Zu\u00b7frie\u00b7den\u00b7heit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}