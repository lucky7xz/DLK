{"textgrid.poem.31371": {"metadata": {"author": {"name": "Kleist, Heinrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gen Himmel schauend greift, im Volksgedr\u00e4nge,", "genre": "verse", "period": "N.A.", "pub_year": 1794, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gen Himmel schauend greift, im Volksgedr\u00e4nge,", "tokens": ["Gen", "Him\u00b7mel", "schau\u00b7end", "greift", ",", "im", "Volks\u00b7ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "ADJD", "VVFIN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Barde fromm in seine Saiten ein.", "tokens": ["Der", "Bar\u00b7de", "fromm", "in", "sei\u00b7ne", "Sai\u00b7ten", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Jetzt tr\u00f6sten, jetzt verletzen seine Kl\u00e4nge,", "tokens": ["Jetzt", "tr\u00f6s\u00b7ten", ",", "jetzt", "ver\u00b7let\u00b7zen", "sei\u00b7ne", "Kl\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und solcher Antwort kann er sich nicht freun.", "tokens": ["Und", "sol\u00b7cher", "Ant\u00b7wort", "kann", "er", "sich", "nicht", "freun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch eine denkt er in dem Kreis der Menge,", "tokens": ["Doch", "ei\u00b7ne", "denkt", "er", "in", "dem", "Kreis", "der", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der die Gef\u00fchle seiner Brust sich weihn:", "tokens": ["Der", "die", "Ge\u00b7f\u00fch\u00b7le", "sei\u00b7ner", "Brust", "sich", "weihn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPOSAT", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sie h\u00e4lt den Preis in H\u00e4nden, der ihm falle,", "tokens": ["Sie", "h\u00e4lt", "den", "Preis", "in", "H\u00e4n\u00b7den", ",", "der", "ihm", "fal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und kr\u00f6nt ihn die, so kr\u00f6nen sie ihn alle.", "tokens": ["Und", "kr\u00f6nt", "ihn", "die", ",", "so", "kr\u00f6\u00b7nen", "sie", "ihn", "al\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "$,", "ADV", "VVFIN", "PPER", "PPER", "PIS", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}