{"textgrid.poem.67859": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "7. Das Hagelwetter", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich h\u00f6rt' in Norden", "tokens": ["Ich", "h\u00f6rt'", "in", "Nor\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Ein Wetter aufstehn;", "tokens": ["Ein", "Wet\u00b7ter", "auf\u00b7stehn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Hagel rasselt", "tokens": ["Ha\u00b7gel", "ras\u00b7selt"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Auf Helmen hart!", "tokens": ["Auf", "Hel\u00b7men", "hart", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Wolkensteine", "tokens": ["Wol\u00b7ken\u00b7stei\u00b7ne"], "token_info": ["word"], "pos": ["NE"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Stieben im Wetter", "tokens": ["Stie\u00b7ben", "im", "Wet\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "In der Streiter Augen", "tokens": ["In", "der", "Strei\u00b7ter", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Vom scharfen Sturm.", "tokens": ["Vom", "schar\u00b7fen", "Sturm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Es hagelt Schlossen,", "tokens": ["Es", "ha\u00b7gelt", "Schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Jed' ein Loth schwer!", "tokens": ["Jed'", "ein", "Loth", "schwer", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Blut ins Meer,", "tokens": ["Blut", "ins", "Meer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Blut aus Wunden", "tokens": ["Blut", "aus", "Wun\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "R\u00f6thet den Speer.", "tokens": ["R\u00f6\u00b7thet", "den", "Speer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Die Leichen lagen,", "tokens": ["Die", "Lei\u00b7chen", "la\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "'s war harter Kampf,", "tokens": ["'s", "war", "har\u00b7ter", "Kampf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Das Heer der Grafen", "tokens": ["Das", "Heer", "der", "Gra\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Steht dem Kampf!", "tokens": ["Steht", "dem", "Kampf", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Der Sturmgeist grimmig", "tokens": ["Der", "Sturm\u00b7geist", "grim\u00b7mig"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Schleudert spizige", "tokens": ["Schleu\u00b7dert", "spi\u00b7zi\u00b7ge"], "token_info": ["word", "word"], "pos": ["NE", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Pfeile von den Fingern", "tokens": ["Pfei\u00b7le", "von", "den", "Fin\u00b7gern"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Den Fechtern ins Gesicht.", "tokens": ["Den", "Fech\u00b7tern", "ins", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die m\u00e4cht'gen Fechter", "tokens": ["Die", "m\u00e4cht'\u00b7gen", "Fech\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Im harten Gewitter,", "tokens": ["Im", "har\u00b7ten", "Ge\u00b7wit\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Dem Sturme stehend,", "tokens": ["Dem", "Stur\u00b7me", "ste\u00b7hend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Wichen nicht!", "tokens": ["Wi\u00b7chen", "nicht", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKNEG", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Bis da\u00df am Ende", "tokens": ["Bis", "da\u00df", "am", "En\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "KOUS", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Dem tapfern Grafen", "tokens": ["Dem", "tap\u00b7fern", "Gra\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Geschw\u00e4cht an Kr\u00e4ften", "tokens": ["Ge\u00b7schw\u00e4cht", "an", "Kr\u00e4f\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Der Muth erlag.", "tokens": ["Der", "Muth", "er\u00b7lag", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Zog ab die Flotte,", "tokens": ["Zog", "ab", "die", "Flot\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Befahl den Seinen,", "tokens": ["Be\u00b7fahl", "den", "Sei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "PPOSS", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Segel zu spannen!", "tokens": ["Se\u00b7gel", "zu", "span\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Die Wellen schlugen;", "tokens": ["Die", "Wel\u00b7len", "schlu\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "In die holen Segel", "tokens": ["In", "die", "ho\u00b7len", "Se\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Der Sturmwind blies.", "tokens": ["Der", "Sturm\u00b7wind", "blies", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}