{"dta.poem.18567": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "D er  L iesimenen  L eib-  \n  Aria.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Meine Losung heist Vergn\u00fcgen/", "tokens": ["Mei\u00b7ne", "Lo\u00b7sung", "heist", "Ver\u00b7gn\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welche sich mein Hertz erwehlt.", "tokens": ["Wel\u00b7che", "sich", "mein", "Hertz", "er\u00b7wehlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Selbst der Himmel wird es f\u00fcgen/", "tokens": ["Selbst", "der", "Him\u00b7mel", "wird", "es", "f\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df mich solche Lust beseelt/", "tokens": ["Da\u00df", "mich", "sol\u00b7che", "Lust", "be\u00b7seelt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die kein Wechsel eitler Sachen/", "tokens": ["Die", "kein", "Wech\u00b7sel", "eit\u00b7ler", "Sa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kan zu Mammelucken machen.", "tokens": ["Kan", "zu", "Mam\u00b7me\u00b7lu\u00b7cken", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ob mein Gl\u00fccke selten lachet/", "tokens": ["Ob", "mein", "Gl\u00fc\u00b7cke", "sel\u00b7ten", "la\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lachet doch mein froher Geist/", "tokens": ["La\u00b7chet", "doch", "mein", "fro\u00b7her", "Geist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der aus Dornen Rosen machet/", "tokens": ["Der", "aus", "Dor\u00b7nen", "Ro\u00b7sen", "ma\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mich mit Vergn\u00fcgen speist.", "tokens": ["Und", "mich", "mit", "Ver\u00b7gn\u00fc\u00b7gen", "speist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer die Gro\u00dfmuth sich ersehen/", "tokens": ["Wer", "die", "Gro\u00df\u00b7muth", "sich", "er\u00b7se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PRF", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kan auf lauter Liljen gehen.", "tokens": ["Kan", "auf", "lau\u00b7ter", "Lil\u00b7jen", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nichts kan meinen Ruhm versehren/", "tokens": ["Nichts", "kan", "mei\u00b7nen", "Ruhm", "ver\u00b7seh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob ein falsches Auge sticht:", "tokens": ["Ob", "ein", "fal\u00b7sches", "Au\u00b7ge", "sticht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn wenn sich die Neider mehren/", "tokens": ["Denn", "wenn", "sich", "die", "Nei\u00b7der", "meh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PRF", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gr\u00f6ssert sich der Tugend-Licht.", "tokens": ["Gr\u00f6s\u00b7sert", "sich", "der", "Tu\u00b7gen\u00b7dLicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Tugend und ein frey Gewissen/", "tokens": ["Tu\u00b7gend", "und", "ein", "frey", "Ge\u00b7wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "ADJD", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sollen hier die Unschuld k\u00fcssen.", "tokens": ["Sol\u00b7len", "hier", "die", "Un\u00b7schuld", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Diese Lust soll mich bekr\u00f6nen/", "tokens": ["Die\u00b7se", "Lust", "soll", "mich", "be\u00b7kr\u00f6\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VMFIN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So die edle Freyheit schenckt:", "tokens": ["So", "die", "ed\u00b7le", "Frey\u00b7heit", "schenckt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich will Amors Macht verh\u00f6hnen/", "tokens": ["Ich", "will", "A\u00b7mors", "Macht", "ver\u00b7h\u00f6h\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NE", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die mit s\u00fcssen Giffte tr\u00e4nckt/", "tokens": ["Die", "mit", "s\u00fcs\u00b7sen", "Giff\u00b7te", "tr\u00e4nckt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "An der Freyheit sch\u00f6nen Sch\u00e4tzen", "tokens": ["An", "der", "Frey\u00b7heit", "sch\u00f6\u00b7nen", "Sch\u00e4t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Soll sich meine Brust ergetzen.", "tokens": ["Soll", "sich", "mei\u00b7ne", "Brust", "er\u00b7get\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Soll die Liebe mich Vergn\u00fcgen/", "tokens": ["Soll", "die", "Lie\u00b7be", "mich", "Ver\u00b7gn\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo sie ein Vergn\u00fcgen ist/", "tokens": ["Wo", "sie", "ein", "Ver\u00b7gn\u00fc\u00b7gen", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So mag es der Himmel f\u00fcgen/", "tokens": ["So", "mag", "es", "der", "Him\u00b7mel", "f\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Was mich vor ein Liebster K\u00fcst:", "tokens": ["Was", "mich", "vor", "ein", "Liebs\u00b7ter", "K\u00fcst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn es brennen itzt die Kertzen/", "tokens": ["Denn", "es", "bren\u00b7nen", "itzt", "die", "Kert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Edler Freyheit noch im Hertzen.", "tokens": ["Ed\u00b7ler", "Frey\u00b7heit", "noch", "im", "Hert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch der Freundschafft sch\u00f6nste Blicke", "tokens": ["Doch", "der", "Freund\u00b7schafft", "sch\u00f6ns\u00b7te", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lachen mich in dessen an/", "tokens": ["La\u00b7chen", "mich", "in", "des\u00b7sen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "PDS", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach hier bl\u00fchet mein Gel\u00fccke/", "tokens": ["Ach", "hier", "bl\u00fc\u00b7het", "mein", "Ge\u00b7l\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df mich h\u00f6chst erfreuen kan.", "tokens": ["Da\u00df", "mich", "h\u00f6chst", "er\u00b7freu\u00b7en", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Untreu/ Falschheit und Betr\u00fcgen/", "tokens": ["Un\u00b7treu", "/", "Falschheit", "und", "Be\u00b7tr\u00fc\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$(", "NN", "KON", "NN", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "M\u00fcssen hier zum F\u00fcssen liegen.", "tokens": ["M\u00fcs\u00b7sen", "hier", "zum", "F\u00fcs\u00b7sen", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Hier verbinden sich die Hertzen", "tokens": ["Hier", "ver\u00b7bin\u00b7den", "sich", "die", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die angenehmste Treu/", "tokens": ["Durch", "die", "an\u00b7ge\u00b7nehms\u00b7te", "Treu", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Es macht das beliebte Schertzen/", "tokens": ["Es", "macht", "das", "be\u00b7lieb\u00b7te", "Schert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da\u00df die Lust vollkommen sey.", "tokens": ["Da\u00df", "die", "Lust", "voll\u00b7kom\u00b7men", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja der Freundschafft Lieblichkeiten/", "tokens": ["Ja", "der", "Freund\u00b7schafft", "Lieb\u00b7lich\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "NN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "G\u00f6nnen mir recht g\u00fcldnen Zeiten.", "tokens": ["G\u00f6n\u00b7nen", "mir", "recht", "g\u00fcld\u00b7nen", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Andre m\u00f6gen in den Netze", "tokens": ["And\u00b7re", "m\u00f6\u00b7gen", "in", "den", "Net\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unvergn\u00fcgter Seelen stehn.", "tokens": ["Un\u00b7ver\u00b7gn\u00fcg\u00b7ter", "See\u00b7len", "stehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Selbst der Himmel liebt die Sch\u00e4tze/", "tokens": ["Selbst", "der", "Him\u00b7mel", "liebt", "die", "Sch\u00e4t\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die ich mir zum Zweck ersehn.", "tokens": ["Die", "ich", "mir", "zum", "Zweck", "er\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieser Entzweck mu\u00df es f\u00fcgen/", "tokens": ["Die\u00b7ser", "Ent\u00b7zweck", "mu\u00df", "es", "f\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VMFIN", "PPER", "VVINF", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Da\u00df die Losung heist Vergn\u00fcgen.", "tokens": ["Da\u00df", "die", "Lo\u00b7sung", "heist", "Ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}