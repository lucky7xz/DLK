{"dta.poem.9843": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Aria.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gewi\u00df die jungfern kommen mir", "tokens": ["Ge\u00b7wi\u00df", "die", "jung\u00b7fern", "kom\u00b7men", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht anders als die kletten f\u00fcr/", "tokens": ["Nicht", "an\u00b7ders", "als", "die", "klet\u00b7ten", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "KOKOM", "ART", "ADJA", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die machen sich erschr\u00f6cklich gro\u00df", "tokens": ["Die", "ma\u00b7chen", "sich", "er\u00b7schr\u00f6ck\u00b7lich", "gro\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "ADJD", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und gehn vom stocke schwerlich lo\u00df.", "tokens": ["Und", "gehn", "vom", "sto\u00b7cke", "schwer\u00b7lich", "lo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch tritt man nur was n\u00e4her dran/", "tokens": ["Doch", "tritt", "man", "nur", "was", "n\u00e4\u00b7her", "dran", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADV", "PWS", "ADJD", "PAV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So h\u00e4ngen sie sich selber an.", "tokens": ["So", 