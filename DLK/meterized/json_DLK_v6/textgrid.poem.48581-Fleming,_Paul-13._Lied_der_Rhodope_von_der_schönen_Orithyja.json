{"textgrid.poem.48581": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "13. Lied der Rhodope von der sch\u00f6nen Orithyja", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Hymen zusammen brachte", "tokens": ["Als", "Hy\u00b7men", "zu\u00b7sam\u00b7men", "brach\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADV", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "den ber\u00fchmten ", "tokens": ["den", "be\u00b7r\u00fchm\u00b7ten"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "und die ", "tokens": ["und", "die"], "token_info": ["word", "word"], "pos": ["KON", "ART"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Jupiter gar lieblich lachte;", "tokens": ["Ju\u00b7pi\u00b7ter", "gar", "lieb\u00b7lich", "lach\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "VVFIN", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.5": {"text": "alle G\u00f6tter ingemein", "tokens": ["al\u00b7le", "G\u00f6t\u00b7ter", "in\u00b7ge\u00b7mein"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "sah man wunderfr\u00f6lich sein.", "tokens": ["sah", "man", "wun\u00b7der\u00b7fr\u00f6\u00b7lich", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ph\u00f6bus sprach: Mein Licht ist nichtes", "tokens": ["Ph\u00f6\u00b7bus", "sprach", ":", "Mein", "Licht", "ist", "nich\u00b7tes"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$.", "PPOSAT", "NN", "VAFIN", "PIS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "gegen ", "tokens": ["ge\u00b7gen"], "token_info": ["word"], "pos": ["APPR"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "ich bin \u00fcberwunden ganz.", "tokens": ["ich", "bin", "\u00fc\u00b7berw\u00b7un\u00b7den", "ganz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist doch ihres Angesichtes", "tokens": ["Ist", "doch", "ih\u00b7res", "An\u00b7ge\u00b7sich\u00b7tes"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "und der Augen heller Schein", "tokens": ["und", "der", "Au\u00b7gen", "hel\u00b7ler", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "kl\u00e4rer als die Stralen mein.", "tokens": ["kl\u00e4\u00b7rer", "als", "die", "Stra\u00b7len", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Venus und die kleinen Buben,", "tokens": ["Ve\u00b7nus", "und", "die", "klei\u00b7nen", "Bu\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "die da sch\u00fcrzen Liebesband',", "tokens": ["die", "da", "sch\u00fcr\u00b7zen", "Lie\u00b7bes\u00b7band'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "wann man ", "tokens": ["wann", "man"], "token_info": ["word", "word"], "pos": ["PWAV", "PIS"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "bald zu springen sich erhuben,", "tokens": ["bald", "zu", "sprin\u00b7gen", "sich", "er\u00b7hu\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVFIN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "und f\u00fcr Freuden tanzten all',", "tokens": ["und", "f\u00fcr", "Freu\u00b7den", "tanz\u00b7ten", "all'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "PIS", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "da\u00df es weit und breit erschall.", "tokens": ["da\u00df", "es", "weit", "und", "breit", "er\u00b7schall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wann das Meer durch starke Winde", "tokens": ["Wann", "das", "Meer", "durch", "star\u00b7ke", "Win\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "an die Wolken wird ger\u00fcckt,", "tokens": ["an", "die", "Wol\u00b7ken", "wird", "ge\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "und die Sch\u00f6nste herf\u00fcr blickt:", "tokens": ["und", "die", "Sch\u00f6ns\u00b7te", "her\u00b7f\u00fcr", "blickt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "in ein L\u00fcftelein gelinde,", "tokens": ["in", "ein", "L\u00fcf\u00b7tel\u00b7ein", "ge\u00b7lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "in ein sanftes Sausen gut", "tokens": ["in", "ein", "sanf\u00b7tes", "Sau\u00b7sen", "gut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "sich der Sturm verwandeln tut", "tokens": ["sich", "der", "Sturm", "ver\u00b7wan\u00b7deln", "tut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "ART", "NN", "VVINF", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Charites, in eurem Orden", "tokens": ["Cha\u00b7ri\u00b7tes", ",", "in", "eu\u00b7rem", "Or\u00b7den"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "seind zuvor gewesen drei;", "tokens": ["seind", "zu\u00b7vor", "ge\u00b7we\u00b7sen", "drei", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VAPP", "CARD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "ietzo solt ihr wissen frei,", "tokens": ["iet\u00b7zo", "solt", "ihr", "wis\u00b7sen", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "eure Zunft ist gr\u00f6\u00dfer worden:", "tokens": ["eu\u00b7re", "Zunft", "ist", "gr\u00f6\u00b7\u00dfer", "wor\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "VAPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "ist die vierde in der Zahl.", "tokens": ["ist", "die", "vier\u00b7de", "in", "der", "Zahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}