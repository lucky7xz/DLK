{"dta.poem.23797": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Uber die Creutzigung Christi.  \n Sonnet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "O Wunder die kein Mensch mit Sin\u0303en kan ergr\u00fcnden!", "tokens": ["O", "Wun\u00b7der", "die", "kein", "Mensch", "mit", "Si\u00f1en", "kan", "er\u00b7gr\u00fcn\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "PIAT", "NN", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den die erbo\u00dfte Schaar dort an das Creutze schl\u00e4gt/", "tokens": ["Den", "die", "er\u00b7bo\u00df\u00b7te", "Schaar", "dort", "an", "das", "Creut\u00b7ze", "schl\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist der/ nach dessen Winck das Firmament sich regt.", "tokens": ["Ist", "der", "/", "nach", "des\u00b7sen", "Win\u00b7ck", "das", "Fir\u00b7ma\u00b7ment", "sich", "reg\u00b7t."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["VAFIN", "ART", "$(", "APPR", "PRELAT", "NN", "ART", "NN", "PRF", "NE"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die Unschuld wird gestrafft/ und b\u00fc\u00df\u2019t f\u00fcr fremde S\u00fcnde\u0303.", "tokens": ["Die", "Un\u00b7schuld", "wird", "ge\u00b7strafft", "/", "und", "b\u00fc\u00df't", "f\u00fcr", "frem\u00b7de", "S\u00fcn\u00b7d\u1ebd", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$(", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Tod und Teufel zwingt/ l\u00e4\u00df\u2019t sich mit Stricken binde\u0303/", "tokens": ["Der", "Tod", "und", "Teu\u00b7fel", "zwingt", "/", "l\u00e4\u00df't", "sich", "mit", "Stri\u00b7cken", "bin\u00b7d\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$(", "VVFIN", "PRF", "APPR", "NN", "APPR", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Der Heyland leydet Noth/ doch wird sein Hertz bewegt/", "tokens": ["Der", "Hey\u00b7land", "ley\u00b7det", "Noth", "/", "doch", "wird", "sein", "Hertz", "be\u00b7wegt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$(", "ADV", "VAFIN", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df Er mit denen selbst ein recht Erbarmen tr\u00e4gt/", "tokens": ["Da\u00df", "Er", "mit", "de\u00b7nen", "selbst", "ein", "recht", "Er\u00b7bar\u00b7men", "tr\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PRELS", "ADV", "ART", "ADJD", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die sich zu seinem Schimpf und Tod versamlet finden.", "tokens": ["Die", "sich", "zu", "sei\u00b7nem", "Schimpf", "und", "Tod", "ver\u00b7sam\u00b7let", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "NN", "KON", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Gott stirbt/ der grosse GOtt in dem das Leben lebt.", "tokens": ["Gott", "stirbt", "/", "der", "gros\u00b7se", "Gott", "in", "dem", "das", "Le\u00b7ben", "lebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$(", "ART", "ADJA", "NN", "APPR", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was wunder da\u00df der Bau der schweren Erde bebt?", "tokens": ["Was", "wun\u00b7der", "da\u00df", "der", "Bau", "der", "schwe\u00b7ren", "Er\u00b7de", "bebt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KOUS", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df sich der Sonnen-Gluth bey Tage mu\u00df verstecken?", "tokens": ["Da\u00df", "sich", "der", "Son\u00b7nen\u00b7Gluth", "bey", "Ta\u00b7ge", "mu\u00df", "ver\u00b7ste\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Da\u00df Fel\u00df und Vorhang rei\u00df\u2019t/ da\u00df Leichen aufferstehn?", "tokens": ["Da\u00df", "Fel\u00df", "und", "Vor\u00b7hang", "rei\u00df't", "/", "da\u00df", "Lei\u00b7chen", "auf\u00b7fer\u00b7stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "$(", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich wundre mich vielmehr/ da\u00df nicht f\u00fcr Schaam und", "tokens": ["Ich", "wund\u00b7re", "mich", "viel\u00b7mehr", "/", "da\u00df", "nicht", "f\u00fcr", "Schaam", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$(", "KOUS", "PTKNEG", "APPR", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Fel\u00df/ Erde/ Sonn und Welt zerschmeltzen und vergehn.", "tokens": ["Fel\u00df", "/", "Er\u00b7de", "/", "Sonn", "und", "Welt", "zer\u00b7schmelt\u00b7zen", "und", "ver\u00b7gehn", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "NN", "$(", "NN", "KON", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}