{"textgrid.poem.66183": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da senken sie den Mann ins Grab,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da senken sie den Mann ins Grab,", "tokens": ["Da", "sen\u00b7ken", "sie", "den", "Mann", "ins", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der uns mit treuem Hirtenstab", "tokens": ["Der", "uns", "mit", "treu\u00b7em", "Hir\u00b7ten\u00b7stab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ganz nach dem Geist der Gottesschrift", "tokens": ["Ganz", "nach", "dem", "Geist", "der", "Got\u00b7tes\u00b7schrift"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geweidet auf gesunder Trift.", "tokens": ["Ge\u00b7wei\u00b7det", "auf", "ge\u00b7sun\u00b7der", "Trift", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wir aber stehen bang und schwer", "tokens": ["Wir", "a\u00b7ber", "ste\u00b7hen", "bang", "und", "schwer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um dieses Hirten Leiche her;", "tokens": ["Um", "die\u00b7ses", "Hir\u00b7ten", "Lei\u00b7che", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PDAT", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn, ach, wir irren nun verwaist", "tokens": ["Denn", ",", "ach", ",", "wir", "ir\u00b7ren", "nun", "ver\u00b7waist"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "ITJ", "$,", "PPER", "VVFIN", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und missen unsers F\u00fchrers Geist.", "tokens": ["Und", "mis\u00b7sen", "un\u00b7sers", "F\u00fch\u00b7rers", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Vor seiner Seele stand das Bild", "tokens": ["Vor", "sei\u00b7ner", "See\u00b7le", "stand", "das", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des treuen Hirten gro\u00df und mild,", "tokens": ["Des", "treu\u00b7en", "Hir\u00b7ten", "gro\u00df", "und", "mild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der seiner Herrlichkeit gewi\u00df", "tokens": ["Der", "sei\u00b7ner", "Herr\u00b7lich\u00b7keit", "ge\u00b7wi\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Leben f\u00fcr die Schafe lie\u00df.", "tokens": ["Sein", "Le\u00b7ben", "f\u00fcr", "die", "Scha\u00b7fe", "lie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Auch unser Hahn hat Tag und Nacht", "tokens": ["Auch", "un\u00b7ser", "Hahn", "hat", "Tag", "und", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr seiner Heerde Heil gewacht,", "tokens": ["F\u00fcr", "sei\u00b7ner", "Heer\u00b7de", "Heil", "ge\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und selbst sein Leben nie gescheut", "tokens": ["Und", "selbst", "sein", "Le\u00b7ben", "nie", "ge\u00b7scheut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPOSAT", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr seiner Schafe Seligkeit.", "tokens": ["F\u00fcr", "sei\u00b7ner", "Scha\u00b7fe", "Se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Er sprach mit v\u00e4terlichem Sinn:", "tokens": ["Er", "sprach", "mit", "v\u00e4\u00b7ter\u00b7li\u00b7chem", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kommt, Kindlein, kommt zu Jesu hin;", "tokens": ["Kommt", ",", "Kin\u00b7dlein", ",", "kommt", "zu", "Je\u00b7su", "hin", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "APPR", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er ist der Weg, sonst keiner mehr,", "tokens": ["Er", "ist", "der", "Weg", ",", "sonst", "kei\u00b7ner", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ADV", "PIS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Wahrheit und das Leben Er.", "tokens": ["Die", "Wahr\u00b7heit", "und", "das", "Le\u00b7ben", "Er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dem S\u00fcnder ging er sorglich nach.", "tokens": ["Dem", "S\u00fcn\u00b7der", "ging", "er", "sorg\u00b7lich", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie liebevoll er mit ihm sprach!", "tokens": ["Wie", "lie\u00b7be\u00b7voll", "er", "mit", "ihm", "sprach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er zeigt' ihm Leben und Gericht", "tokens": ["Er", "zeigt'", "ihm", "Le\u00b7ben", "und", "Ge\u00b7richt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und macht' ihm fest des Christen Pflicht.", "tokens": ["Und", "macht'", "ihm", "fest", "des", "Chris\u00b7ten", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und ri\u00df er dann mit hohem Muth", "tokens": ["Und", "ri\u00df", "er", "dann", "mit", "ho\u00b7hem", "Muth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die S\u00fcnder aus der H\u00f6llenglut,", "tokens": ["Die", "S\u00fcn\u00b7der", "aus", "der", "H\u00f6l\u00b7len\u00b7glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So freute sich der Menschenfreund,", "tokens": ["So", "freu\u00b7te", "sich", "der", "Men\u00b7schen\u00b7freund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df er vor Freude oft geweint.", "tokens": ["Da\u00df", "er", "vor", "Freu\u00b7de", "oft", "ge\u00b7weint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Sein Herz war ganz von Lieb' erf\u00fcllt,", "tokens": ["Sein", "Herz", "war", "ganz", "von", "Lieb'", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Liebe, die aus Christus quillt,", "tokens": ["Von", "Lie\u00b7be", ",", "die", "aus", "Chris\u00b7tus", "quillt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Voll Einfalt, an Erbarmen reich,", "tokens": ["Voll", "Ein\u00b7falt", ",", "an", "Er\u00b7bar\u00b7men", "reich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Herzen des Johannes gleich.", "tokens": ["Dem", "Her\u00b7zen", "des", "Jo\u00b7han\u00b7nes", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NE", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wer war dem\u00fcthiger, als er?", "tokens": ["Wer", "war", "de\u00b7m\u00fct\u00b7hi\u00b7ger", ",", "als", "er", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADJD", "$,", "KOUS", "PPER", "$."], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Sein gro\u00dfer Geist blickt' weit umher,", "tokens": ["Sein", "gro\u00b7\u00dfer", "Geist", "blickt'", "weit", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sah hoch und tief, sah lang und breit,", "tokens": ["Sah", "hoch", "und", "tief", ",", "sah", "lang", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "$,", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und blieb doch voll Bescheidenheit.", "tokens": ["Und", "blieb", "doch", "voll", "Be\u00b7schei\u00b7den\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Den Trauernden hat er erquickt,", "tokens": ["Den", "Trau\u00b7ern\u00b7den", "hat", "er", "er\u00b7quickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Den Sterbenden der Welt entr\u00fcckt;", "tokens": ["Den", "Ster\u00b7ben\u00b7den", "der", "Welt", "ent\u00b7r\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn unsers Hirten Rechte wies", "tokens": ["Denn", "un\u00b7sers", "Hir\u00b7ten", "Rech\u00b7te", "wies"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Sterbenden ins Paradies.", "tokens": ["Den", "Ster\u00b7ben\u00b7den", "ins", "Pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Schon Mancher steht vor Gottes Thron", "tokens": ["Schon", "Man\u00b7cher", "steht", "vor", "Got\u00b7tes", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "VVFIN", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und fleht f\u00fcr ihn um gro\u00dfen Lohn,", "tokens": ["Und", "fleht", "f\u00fcr", "ihn", "um", "gro\u00b7\u00dfen", "Lohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weil er, ach, schon dem Abgrund nah", "tokens": ["Weil", "er", ",", "ach", ",", "schon", "dem", "Ab\u00b7grund", "nah"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "ITJ", "$,", "ADV", "ART", "NN", "ADJD"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Durch Hahn des Himmels Pforte sah.", "tokens": ["Durch", "Hahn", "des", "Him\u00b7mels", "Pfor\u00b7te", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Du starbst so sanft, wie du gelebt,", "tokens": ["Du", "starbst", "so", "sanft", ",", "wie", "du", "ge\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "PWAV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Todesangriff unerbebt", "tokens": ["Vom", "To\u00b7de\u00b7san\u00b7griff", "un\u00b7er\u00b7bebt"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sprachst du mit sanfter Stimme Ton:", "tokens": ["Sprachst", "du", "mit", "sanf\u00b7ter", "Stim\u00b7me", "Ton", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein neues Leben f\u00fchl' ich schon.", "tokens": ["Mein", "neu\u00b7es", "Le\u00b7ben", "f\u00fchl'", "ich", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "So nimm den Dank, verkl\u00e4rter Hahn,", "tokens": ["So", "nimm", "den", "Dank", ",", "ver\u00b7kl\u00e4r\u00b7ter", "Hahn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von deinen Echterdingern an.", "tokens": ["Von", "dei\u00b7nen", "Ech\u00b7ter\u00b7din\u00b7gern", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem sp\u00e4ten Enkel sagen wir", "tokens": ["Dem", "sp\u00e4\u00b7ten", "En\u00b7kel", "sa\u00b7gen", "wir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch viel, du theurer Mann, von dir.", "tokens": ["Noch", "viel", ",", "du", "theu\u00b7rer", "Mann", ",", "von", "dir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PPER", "ADJD", "NN", "$,", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Wir weinen alle tief bewegt,", "tokens": ["Wir", "wei\u00b7nen", "al\u00b7le", "tief", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df eine Z\u00e4hr' die andre schl\u00e4gt.", "tokens": ["Da\u00df", "ei\u00b7ne", "Z\u00e4hr'", "die", "and\u00b7re", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Tief f\u00fchlen wir in unsrer Brust", "tokens": ["Tief", "f\u00fch\u00b7len", "wir", "in", "uns\u00b7rer", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den unaussp