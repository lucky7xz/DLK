{"textgrid.poem.59308": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Kleine", "genre": "verse", "period": "N.A.", "pub_year": 1811, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwischen Bergen, liebe Mutter,", "tokens": ["Zwi\u00b7schen", "Ber\u00b7gen", ",", "lie\u00b7be", "Mut\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weit den Wald entlang,", "tokens": ["Weit", "den", "Wald", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Reiten da drei junge J\u00e4ger", "tokens": ["Rei\u00b7ten", "da", "drei", "jun\u00b7ge", "J\u00e4\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "CARD", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Auf drei R\u00f6\u00dflein blank,", "tokens": ["Auf", "drei", "R\u00f6\u00df\u00b7lein", "blank", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "lieb Mutter,", "tokens": ["lieb", "Mut\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Auf drei R\u00f6\u00dflein blank.", "tokens": ["Auf", "drei", "R\u00f6\u00df\u00b7lein", "blank", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Ihr k\u00f6nnt fr\u00f6hlich sein, lieb Mutter", "tokens": ["Ihr", "k\u00f6nnt", "fr\u00f6h\u00b7lich", "sein", ",", "lieb", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "VAINF", "$,", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird es drau\u00dfen still:", "tokens": ["Wird", "es", "drau\u00b7\u00dfen", "still", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Kommt der Vater heim vom Walde,", "tokens": ["Kommt", "der", "Va\u00b7ter", "heim", "vom", "Wal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00fc\u00dft Euch, wie er will,", "tokens": ["K\u00fc\u00dft", "Euch", ",", "wie", "er", "will", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPER", "VMFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "lieb Mutter,", "tokens": ["lieb", "Mut\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "K\u00fc\u00dft Euch, wie er will.", "tokens": ["K\u00fc\u00dft", "Euch", ",", "wie", "er", "will", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPER", "VMFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Und ich werfe mich im Bettchen", "tokens": ["Und", "ich", "wer\u00b7fe", "mich", "im", "Bett\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nachts ohn Unterla\u00df,", "tokens": ["Nachts", "ohn", "Un\u00b7ter\u00b7la\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Kehr mich links und kehr mich rechts hin,", "tokens": ["Kehr", "mich", "links", "und", "kehr", "mich", "rechts", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "KON", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Nirgends hab ich was,", "tokens": ["Nir\u00b7gends", "hab", "ich", "was", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "lieb Mutter,", "tokens": ["lieb", "Mut\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Nirgends hab ich was.", "tokens": ["Nir\u00b7gends", "hab", "ich", "was", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Bin ich eine Frau erst einmal,", "tokens": ["Bin", "ich", "ei\u00b7ne", "Frau", "erst", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "ADV", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "In der Nacht dann still", "tokens": ["In", "der", "Nacht", "dann", "still"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wend ich mich nach allen Seiten,", "tokens": ["Wend", "ich", "mich", "nach", "al\u00b7len", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00fc\u00df, soviel ich will,", "tokens": ["K\u00fc\u00df", ",", "so\u00b7viel", "ich", "will", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "VMFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "lieb Mutter,", "tokens": ["lieb", "Mut\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "K\u00fc\u00df, soviel ich will.", "tokens": ["K\u00fc\u00df", ",", "so\u00b7viel", "ich", "will", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "VMFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}