{"textgrid.poem.42642": {"metadata": {"author": {"name": "Ratschky, Joseph Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ey, Meister Gottlieb! fangt doch an,", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ey, Meister Gottlieb! fangt doch an,", "tokens": ["Ey", ",", "Meis\u00b7ter", "Gott\u00b7lieb", "!", "fangt", "doch", "an", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "NE", "$.", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mal freundlich drein zu gucken!", "tokens": ["Mal", "freund\u00b7lich", "drein", "zu", "gu\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr seht ja aus, als zw\u00e4nge man", "tokens": ["Ihr", "seht", "ja", "aus", ",", "als", "zw\u00e4n\u00b7ge", "man"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$,", "KOUS", "VVFIN", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Euch Heerlinge zu schlucken.", "tokens": ["Euch", "Heer\u00b7lin\u00b7ge", "zu", "schlu\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PTKZU", "VVINF", "$."], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Wer kann, wenn Ihr es f\u00fcr und f\u00fcr", "tokens": ["Wer", "kann", ",", "wenn", "Ihr", "es", "f\u00fcr", "und", "f\u00fcr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "$,", "KOUS", "PPER", "PPER", "APPR", "KON", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So treibt, in Euch sich finden?", "tokens": ["So", "treibt", ",", "in", "Euch", "sich", "fin\u00b7den", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr schleicht herum, als solltet Ihr", "tokens": ["Ihr", "schleicht", "he\u00b7rum", ",", "als", "soll\u00b7tet", "Ihr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KOUS", "VMFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erlahmen und erblinden.", "tokens": ["Er\u00b7lah\u00b7men", "und", "er\u00b7blin\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Pfui doch, Gevatter, pfui! entsagt", "tokens": ["Pfui", "doch", ",", "Ge\u00b7vat\u00b7ter", ",", "pfui", "!", "ent\u00b7sagt"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NE", "ADV", "$,", "NN", "$,", "ITJ", "$.", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Dem st\u00e4ten Spintisiren!", "tokens": ["Dem", "st\u00e4\u00b7ten", "Spin\u00b7ti\u00b7si\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was n\u00fctzt's, dass Ihr Euch selber plagt?", "tokens": ["Was", "n\u00fctzt's", ",", "dass", "Ihr", "Euch", "sel\u00b7ber", "plagt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "'s hilft doch kein Protestiren,", "tokens": ["'s", "hilft", "doch", "kein", "Pro\u00b7tes\u00b7ti\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "M\u00fcsst Eure Scheitel schm\u00fccken sehn", "tokens": ["M\u00fcsst", "Eu\u00b7re", "Schei\u00b7tel", "schm\u00fc\u00b7cken", "sehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "VVINF", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit einem Zopfper\u00fcckchen;", "tokens": ["Mit", "ei\u00b7nem", "Zopf\u00b7pe\u00b7r\u00fcck\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn denkt, so kahl herumzugehn,", "tokens": ["Denn", "denkt", ",", "so", "kahl", "her\u00b7um\u00b7zu\u00b7gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das w\u00e4r' ein feines St\u00fcckchen.", "tokens": ["Das", "w\u00e4r'", "ein", "fei\u00b7nes", "St\u00fcck\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Zwar s\u00e4h' ich diess mein schwarzes Haar", "tokens": ["Zwar", "s\u00e4h'", "ich", "diess", "mein", "schwar\u00b7zes", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PDS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So j\u00e4mmerlich verschnitzeln,", "tokens": ["So", "j\u00e4m\u00b7mer\u00b7lich", "ver\u00b7schnit\u00b7zeln", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein solcher Streich w\u00fcrd' unfehlbar", "tokens": ["Ein", "sol\u00b7cher", "Streich", "w\u00fcrd'", "un\u00b7fehl\u00b7bar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch mich gar h\u00f6chlich kitzeln.", "tokens": ["Auch", "mich", "gar", "h\u00f6ch\u00b7lich", "kit\u00b7zeln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Allein mit Gunst! den weisen Mann", "tokens": ["Al\u00b7lein", "mit", "Gunst", "!", "den", "wei\u00b7sen", "Mann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Macht kein Geschick verlegen:", "tokens": ["Macht", "kein", "Ge\u00b7schick", "ver\u00b7le\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er stellt, wenn er's nicht \u00e4ndern kann,", "tokens": ["Er", "stellt", ",", "wenn", "er's", "nicht", "\u00e4n\u00b7dern", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PIS", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Ungl\u00fcck Trost entgegen.", "tokens": ["Dem", "Un\u00b7gl\u00fcck", "Trost", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Seht! geht's Euch mal wie Absalon,", "tokens": ["Seht", "!", "geht's", "Euch", "mal", "wie", "Ab\u00b7sa\u00b7lon", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VVFIN", "PPER", "ADV", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "(habt Euch's doch wohl notiret,", "tokens": ["(", "habt", "Euch's", "doch", "wohl", "no\u00b7ti\u00b7ret", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Was man ", "tokens": ["Was", "man"], "token_info": ["word", "word"], "pos": ["PWS", "PIS"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Uns zu Gem\u00fcth gef\u00fchret,", "tokens": ["Uns", "zu", "Ge\u00b7m\u00fcth", "ge\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Als wir als Diktatoren dicht", "tokens": ["Als", "wir", "als", "Dik\u00b7ta\u00b7to\u00b7ren", "dicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "KOUS", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An dem Katheder sassen,", "tokens": ["An", "dem", "Ka\u00b7the\u00b7der", "sas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und, um den Judenstaat uns nicht", "tokens": ["Und", ",", "um", "den", "Ju\u00b7den\u00b7staat", "uns", "nicht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUI", "ART", "NN", "PPER", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Viel k\u00fcmmernd, Kirschen assen?)", "tokens": ["Viel", "k\u00fcm\u00b7mernd", ",", "Kir\u00b7schen", "as\u00b7sen", "?", ")"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "$,", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ihr k\u00f6nnet dann gar s\u00e4uberlich", "tokens": ["Ihr", "k\u00f6n\u00b7net", "dann", "gar", "s\u00e4u\u00b7ber\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Lanzenstoss entrinnen:", "tokens": ["Dem", "Lan\u00b7zens\u00b7toss", "ent\u00b7rin\u00b7nen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr lasset die Per\u00fcck' im Stich,", "tokens": ["Ihr", "las\u00b7set", "die", "Pe\u00b7r\u00fcck", "im", "Stich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und tummelt Euch von hinnen.", "tokens": ["Und", "tum\u00b7melt", "Euch", "von", "hin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Drum, wie gesagt, ermannet Euch,", "tokens": ["Drum", ",", "wie", "ge\u00b7sagt", ",", "er\u00b7man\u00b7net", "Euch", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "$,", "PWAV", "VVPP", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und h\u00e4ngt nicht st\u00e4ts die Ohren!", "tokens": ["Und", "h\u00e4ngt", "nicht", "st\u00e4ts", "die", "Oh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr habt ja doch kein K\u00f6nigreich", "tokens": ["Ihr", "habt", "ja", "doch", "kein", "K\u00f6\u00b7nig\u00b7reich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch Euern Zopf verloren.", "tokens": ["Durch", "Eu\u00b7ern", "Zopf", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Seyd froh, dass Ihr der Todsgefahr", "tokens": ["Seyd", "froh", ",", "dass", "Ihr", "der", "Tods\u00b7ge\u00b7fahr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So leichten Kaufs entkommen,", "tokens": ["So", "leich\u00b7ten", "Kaufs", "ent\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und diessmal mit dem B\u00fcschlein Haar", "tokens": ["Und", "diess\u00b7mal", "mit", "dem", "B\u00fc\u00b7schlein", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Tod f\u00fcrlieb genommen.", "tokens": ["Der", "Tod", "f\u00fcr\u00b7lieb", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}