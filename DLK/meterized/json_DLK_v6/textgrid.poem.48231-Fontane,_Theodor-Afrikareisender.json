{"textgrid.poem.48231": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Afrikareisender", "genre": "verse", "period": "N.A.", "pub_year": 1888, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bb ... Meine Herren, was soll dieser ganze Zwist,", "tokens": ["\u00bb", "...", "Mei\u00b7ne", "Her\u00b7ren", ",", "was", "soll", "die\u00b7ser", "gan\u00b7ze", "Zwist", ","], "token_info": ["punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "PPOSAT", "NN", "$,", "PWS", "VMFIN", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Ob der Kongo gesund oder ungesund ist?", "tokens": ["Ob", "der", "Kon\u00b7go", "ge\u00b7sund", "o\u00b7der", "un\u00b7ge\u00b7sund", "ist", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NE", "ADJD", "KON", "ADJD", "VAFIN", "$."], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Ich habe drei Jahre, von Krankheit verschont,", "tokens": ["Ich", "ha\u00b7be", "drei", "Jah\u00b7re", ",", "von", "Krank\u00b7heit", "ver\u00b7schont", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Am gr\u00fcnen und schwarzen Graben gewohnt,", "tokens": ["Am", "gr\u00fc\u00b7nen", "und", "schwar\u00b7zen", "Gra\u00b7ben", "ge\u00b7wohnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "KON", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ich habe das Prachtst\u00fcck unsrer Gossen,", "tokens": ["Ich", "ha\u00b7be", "das", "Pracht\u00b7st\u00fcck", "uns\u00b7rer", "Gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die Panke, dicht an der M\u00fcndung genossen", "tokens": ["Die", "Pan\u00b7ke", ",", "dicht", "an", "der", "M\u00fcn\u00b7dung", "ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADJD", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und wohne nun schon im f\u00fcnften Quartal", "tokens": ["Und", "woh\u00b7ne", "nun", "schon", "im", "f\u00fcnf\u00b7ten", "Quar\u00b7tal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+----+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Noch immer lebendig am Kanal.", "tokens": ["Noch", "im\u00b7mer", "le\u00b7ben\u00b7dig", "am", "Ka\u00b7nal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "APPRART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Hier oder da, nah oder fern", "tokens": ["Hier", "o\u00b7der", "da", ",", "nah", "o\u00b7der", "fern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "KON", "ADV", "$,", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Macht keinen Unterschied, meine Herrn,", "tokens": ["Macht", "kei\u00b7nen", "Un\u00b7ter\u00b7schied", ",", "mei\u00b7ne", "Herrn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Und ob ", "tokens": ["Und", "ob"], "token_info": ["word", "word"], "pos": ["KON", "KOUS"], "meter": "+-", "measure": "trochaic.single"}}}}}