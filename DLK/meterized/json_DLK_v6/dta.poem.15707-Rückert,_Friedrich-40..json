{"dta.poem.15707": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "40.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In jedem Irrthum liegt von Wahrheit auch ein Kern,", "tokens": ["In", "je\u00b7dem", "Irr\u00b7thum", "liegt", "von", "Wahr\u00b7heit", "auch", "ein", "Kern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "APPR", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie in der finstern Nacht verh\u00fcllt ist mancher Stern.", "tokens": ["Wie", "in", "der", "fins\u00b7tern", "Nacht", "ver\u00b7h\u00fcllt", "ist", "man\u00b7cher", "Stern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "VVPP", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Wahrheit aber selbst, zum Aeu\u00dfersten getrieben,", "tokens": ["Die", "Wahr\u00b7heit", "a\u00b7ber", "selbst", ",", "zum", "A\u00b7e\u00b7u\u00b7\u00dfers\u00b7ten", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "In Irrthum siehst du sie dort auseinander stieben.", "tokens": ["In", "Irr\u00b7thum", "siehst", "du", "sie", "dort", "aus\u00b7ein\u00b7an\u00b7der", "stie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Den Gegner kannst du nun so oder so bestreiten,", "tokens": ["Den", "Geg\u00b7ner", "kannst", "du", "nun", "so", "o\u00b7der", "so", "be\u00b7strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "ADV", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hinaus zum Irrthum ihn, zur\u00fcck zur Wahrheit leiten.", "tokens": ["Hin\u00b7aus", "zum", "Irr\u00b7thum", "ihn", ",", "zu\u00b7r\u00fcck", "zur", "Wahr\u00b7heit", "lei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PPER", "$,", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Entfalte nur den Keim des Irrthums, welchen hegt", "tokens": ["Ent\u00b7fal\u00b7te", "nur", "den", "Keim", "des", "Irr\u00b7thums", ",", "wel\u00b7chen", "hegt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN", "$,", "PWAT", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Wahrheit, und du hast sie gl\u00e4nzend widerlegt!", "tokens": ["Die", "Wahr\u00b7heit", ",", "und", "du", "hast", "sie", "gl\u00e4n\u00b7zend", "wi\u00b7der\u00b7legt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch willst du sinnen, wie im Grund ers m\u00f6ge meinen,", "tokens": ["Doch", "willst", "du", "sin\u00b7nen", ",", "wie", "im", "Grund", "ers", "m\u00f6\u00b7ge", "mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVINF", "$,", "PWAV", "APPRART", "NN", "PIS", "VMFIN", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So kannst du ohne Streit mit jedem dich vereinen.", "tokens": ["So", "kannst", "du", "oh\u00b7ne", "Streit", "mit", "je\u00b7dem", "dich", "ver\u00b7ei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "APPR", "PIAT", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}