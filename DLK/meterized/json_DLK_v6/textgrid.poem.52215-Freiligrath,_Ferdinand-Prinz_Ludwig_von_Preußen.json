{"textgrid.poem.52215": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Prinz Ludwig von Preu\u00dfen", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie er's in der Schlacht getrieben,", "tokens": ["Wie", "er's", "in", "der", "Schlacht", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie bei Saalfeld er geblieben,", "tokens": ["Wie", "bei", "Saal\u00b7feld", "er", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NE", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Solches wi\u00dft ihr allesamt!", "tokens": ["Sol\u00b7ches", "wi\u00dft", "ihr", "al\u00b7le\u00b7samt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch kein Teufel wei\u00df jetzunder", "tokens": ["Doch", "kein", "Teu\u00b7fel", "wei\u00df", "je\u00b7tzun\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie sein S\u00e4bel, Gottes Wunder!", "tokens": ["Wie", "sein", "S\u00e4\u00b7bel", ",", "Got\u00b7tes", "Wun\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In die Z\u00f6pfe einst geflammt!", "tokens": ["In", "die", "Z\u00f6p\u00b7fe", "einst", "ge\u00b7flammt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf und la\u00dft die Fahnen wehen!", "tokens": ["Auf", "und", "la\u00dft", "die", "Fah\u00b7nen", "we\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KON", "VVIMP", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Anno f\u00fcnf ist es geschehen,", "tokens": ["An\u00b7no", "f\u00fcnf", "ist", "es", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Anno f\u00fcnf zu Altenburg!", "tokens": ["An\u00b7no", "f\u00fcnf", "zu", "Al\u00b7ten\u00b7burg", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "APPR", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Prinz Ludwig bei Spiel und Mahle", "tokens": ["Prinz", "Lud\u00b7wig", "bei", "Spiel", "und", "Mah\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "APPR", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sa\u00df allda bei Vogt im Saale,", "tokens": ["Sa\u00df", "all\u00b7da", "bei", "Vogt", "im", "Saa\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NE", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Zechte flott die Herbstnacht durch.", "tokens": ["Zech\u00b7te", "flott", "die", "Herbst\u00b7nacht", "durch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Tat's mit hundert Offizieren;", "tokens": ["Tat's", "mit", "hun\u00b7dert", "Of\u00b7fi\u00b7zie\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "CARD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trugen allzumal noch ihren", "tokens": ["Tru\u00b7gen", "all\u00b7zu\u00b7mal", "noch", "ih\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "ADV", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wohlfrisierten Puderschopf;", "tokens": ["Wohl\u00b7fri\u00b7sier\u00b7ten", "Pu\u00b7der\u00b7schopf", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Seitenl\u00f6cklein, wohlgebacken", "tokens": ["Sei\u00b7ten\u00b7l\u00f6c\u00b7klein", ",", "wohl\u00b7ge\u00b7ba\u00b7cken"], "token_info": ["word", "punct", "word"], "pos": ["PPOSAT", "$,", "PWAV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und gekleistert, und im Nacken", "tokens": ["Und", "ge\u00b7kleis\u00b7tert", ",", "und", "im", "Na\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVPP", "$,", "KON", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Steif und starr den alten Zopf.", "tokens": ["Steif", "und", "starr", "den", "al\u00b7ten", "Zopf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Gl\u00e4ser klirrten, Lieder schallten,", "tokens": ["Gl\u00e4\u00b7ser", "klirr\u00b7ten", ",", "Lie\u00b7der", "schall\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Champagnerpfropfen knallten \u2013", "tokens": ["Die", "Cham\u00b7pag\u00b7ner\u00b7pfrop\u00b7fen", "knall\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dreimal hoch das Hauptquartier!", "tokens": ["Drei\u00b7mal", "hoch", "das", "Haupt\u00b7quar\u00b7tier", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Tafelmusik rauschte munter,", "tokens": ["Ta\u00b7fel\u00b7mu\u00b7sik", "rauschte", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Meister Dussek mitten drunter", "tokens": ["Meis\u00b7ter", "Dus\u00b7sek", "mit\u00b7ten", "drun\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "ADV", "PAV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dirigierte am Klavier.", "tokens": ["Di\u00b7ri\u00b7gier\u00b7te", "am", "Kla\u00b7vier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Ist der Prinz emporgesprungen,", "tokens": ["Ist", "der", "Prinz", "em\u00b7por\u00b7ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat er hoch sein Schwert geschwungen,", "tokens": ["Hat", "er", "hoch", "sein", "Schwert", "ge\u00b7schwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zugelacht dem Freunde dann:", "tokens": ["Zu\u00b7ge\u00b7lacht", "dem", "Freun\u00b7de", "dann", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbhackbrettschl\u00e4ger, jetzt ans Hacken!", "tokens": ["\u00bb", "hack\u00b7brett\u00b7schl\u00e4\u00b7ger", ",", "jetzt", "ans", "Ha\u00b7cken", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hack' den Zopf mir aus dem Nacken!", "tokens": ["Hack'", "den", "Zopf", "mir", "aus", "dem", "Na\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Heute solln die Z\u00f6pfe dran!\u00ab", "tokens": ["Heu\u00b7te", "solln", "die", "Z\u00f6p\u00b7fe", "dran", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Meister Dussek nahm den Degen,", "tokens": ["Meis\u00b7ter", "Dus\u00b7sek", "nahm", "den", "De\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "T\u00e4t den Zopf aufs Tischtuch legen,", "tokens": ["T\u00e4t", "den", "Zopf", "aufs", "Tischtuch", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf den Knien lag der Prinz:", "tokens": ["Auf", "den", "Kni\u00b7en", "lag", "der", "Prinz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dussek hieb mit scharfem Streiche,", "tokens": ["Dus\u00b7sek", "hieb", "mit", "schar\u00b7fem", "Strei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Auf der Tafel lag die Leiche \u2013", "tokens": ["Auf", "der", "Ta\u00b7fel", "lag", "die", "Lei\u00b7che", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Achtunddrei\u00dfig Jahre sind's!", "tokens": ["Acht\u00b7und\u00b7drei\u00b7\u00dfig", "Jah\u00b7re", "sin\u00b7d's", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Tusch! Das fuhr durch alle K\u00f6pfe!", "tokens": ["Tusch", "!", "Das", "fuhr", "durch", "al\u00b7le", "K\u00f6p\u00b7fe", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PDS", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Laut scholl's: \u00bbPereant die Z\u00f6pfe!\u00ab", "tokens": ["Laut", "scholl's", ":", "\u00bb", "Pe\u00b7re\u00b7ant", "die", "Z\u00f6p\u00b7fe", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "$.", "$(", "NN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das war eine Wirtschaft heut!", "tokens": ["Das", "war", "ei\u00b7ne", "Wirt\u00b7schaft", "heut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Oberst, Kapit\u00e4n und Junker", "tokens": ["O\u00b7berst", ",", "Ka\u00b7pi\u00b7t\u00e4n", "und", "Jun\u00b7ker"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hieb sich ab den garst'gen Klunker \u2013", "tokens": ["Hieb", "sich", "ab", "den", "gar\u00b7st'\u00b7gen", "Klun\u00b7ker", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Jeder Zopf lie\u00df Haare heut!", "tokens": ["Je\u00b7der", "Zopf", "lie\u00df", "Haa\u00b7re", "heut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Dieses in dem Preu\u00dfenheere", "tokens": ["Die\u00b7ses", "in", "dem", "Preu\u00b7\u00dfen\u00b7hee\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Warn die ersten Z\u00f6pf', auf Ehre!", "tokens": ["Warn", "die", "ers\u00b7ten", "Z\u00f6pf'", ",", "auf", "Eh\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die da abgeschnitten sein!", "tokens": ["Die", "da", "ab\u00b7ge\u00b7schnit\u00b7ten", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zopflos in den lieben Himmel", "tokens": ["Zopf\u00b7los", "in", "den", "lie\u00b7ben", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "R\u00fcckt'aus Saalfelds Schlachtget\u00fcmmel", "tokens": ["R\u00fcckt'\u00b7aus", "Saal\u00b7felds", "Schlacht\u00b7ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "word"], "pos": ["NE", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ludwig Ferdinandus ein!", "tokens": ["Lud\u00b7wig", "Fer\u00b7di\u00b7nan\u00b7dus", "ein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Noch im Dreispitz mit der Krempe,", "tokens": ["Noch", "im", "Drei\u00b7spitz", "mit", "der", "Krem\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Hand die blut'ge Plempe,", "tokens": ["In", "der", "Hand", "die", "blut'\u00b7ge", "Plem\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kam er \u2013 doch der Zopf war ab!", "tokens": ["Kam", "er", "\u2013", "doch", "der", "Zopf", "war", "ab", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$(", "ADV", "ART", "NN", "VAFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Drob der Alte Fritz erstaunte,", "tokens": ["Drob", "der", "Al\u00b7te", "Fritz", "er\u00b7staun\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ihm eine gutgelaunte", "tokens": ["Und", "ihm", "ei\u00b7ne", "gut\u00b7ge\u00b7laun\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Oheimliche Nase gab! \u2013", "tokens": ["O\u00b7heim\u00b7li\u00b7che", "Na\u00b7se", "gab", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Der Armeezopf liegt erstochen,", "tokens": ["Der", "Ar\u00b7mee\u00b7zopf", "liegt", "er\u00b7sto\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Jenas Zopf auch ist gerochen,", "tokens": ["Je\u00b7nas", "Zopf", "auch", "ist", "ge\u00b7ro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch manch andrer macht sich breit!", "tokens": ["Doch", "manch", "an\u00b7drer", "macht", "sich", "breit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "VVFIN", "PRF", "ADJD", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Wann zersetzt uns ", "tokens": ["Wann", "zer\u00b7setzt", "uns"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Ludwig, schick' ein Donnerwetter", "tokens": ["Lud\u00b7wig", ",", "schick'", "ein", "Don\u00b7ner\u00b7wet\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In die Z\u00f6pfe dieser Zeit!", "tokens": ["In", "die", "Z\u00f6p\u00b7fe", "die\u00b7ser", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}