{"textgrid.poem.52497": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "17. An einen Dilettanten", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcr mein Vergn\u00fcgen schreib' ich nur,", "tokens": ["F\u00fcr", "mein", "Ver\u00b7gn\u00fc\u00b7gen", "schreib'", "ich", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sagst du, und niemand straft dich L\u00fcgen.", "tokens": ["Sagst", "du", ",", "und", "nie\u00b7mand", "straft", "dich", "L\u00fc\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "PIS", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Denn keine Seele glaubte noch,", "tokens": ["Denn", "kei\u00b7ne", "See\u00b7le", "glaub\u00b7te", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du schriebest andern zum Vergn\u00fcgen.", "tokens": ["Du", "schrie\u00b7best", "an\u00b7dern", "zum", "Ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}