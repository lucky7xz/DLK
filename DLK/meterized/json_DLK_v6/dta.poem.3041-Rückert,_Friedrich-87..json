{"dta.poem.3041": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "87.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In einer W\u00fcste flie\u00dft ein Quell durch Gottes Kraft,", "tokens": ["In", "ei\u00b7ner", "W\u00fcs\u00b7te", "flie\u00dft", "ein", "Quell", "durch", "Got\u00b7tes", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der hat f\u00fcr Durstige des Wegs die Eigenschaft:", "tokens": ["Der", "hat", "f\u00fcr", "Durs\u00b7ti\u00b7ge", "des", "Wegs", "die", "Ei\u00b7gen\u00b7schaft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wer im Vor\u00fcbergehn nur sch\u00f6pfet mit der Hand,", "tokens": ["Wer", "im", "Vor\u00b7\u00fc\u00b7ber\u00b7gehn", "nur", "sch\u00f6p\u00b7fet", "mit", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der geht erquickt und k\u00fchl hinweg im Sonnenbrand.", "tokens": ["Der", "geht", "er\u00b7quickt", "und", "k\u00fchl", "hin\u00b7weg", "im", "Son\u00b7nen\u00b7brand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVPP", "KON", "ADJD", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch wer sich niederl\u00e4\u00dft am Quell und trinkend ruht,", "tokens": ["Doch", "wer", "sich", "nie\u00b7der\u00b7l\u00e4\u00dft", "am", "Quell", "und", "trin\u00b7kend", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PRF", "VVFIN", "APPRART", "NN", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der trinkt sich durstig, und verdurstet an der Flut.", "tokens": ["Der", "trinkt", "sich", "durs\u00b7tig", ",", "und", "ver\u00b7durs\u00b7tet", "an", "der", "Flut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADJD", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ihr Pilger dieses Wegs, la\u00dft es gesagt euch seyn!", "tokens": ["Ihr", "Pil\u00b7ger", "die\u00b7ses", "Wegs", ",", "la\u00dft", "es", "ge\u00b7sagt", "euch", "seyn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDAT", "NN", "$,", "VVIMP", "PPER", "VVPP", "PPER", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sch\u00f6pft im Vor\u00fcbergehn nur mit der Hand allein.", "tokens": ["Sch\u00f6pft", "im", "Vor\u00b7\u00fc\u00b7ber\u00b7gehn", "nur", "mit", "der", "Hand", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ADV", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}