{"textgrid.poem.60929": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Dantes Grab", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dichter, es blieb dein Staub lang ohne das ehrende Denkmal,", "tokens": ["Dich\u00b7ter", ",", "es", "blieb", "dein", "Staub", "lang", "oh\u00b7ne", "das", "eh\u00b7ren\u00b7de", "Denk\u00b7mal", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Bis der venetische Leu hier in Ravenna gebot:", "tokens": ["Bis", "der", "ve\u00b7ne\u00b7ti\u00b7sche", "Leu", "hier", "in", "Ra\u00b7ven\u00b7na", "ge\u00b7bot", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "APPR", "NE", "VVFIN", "$."], "meter": "+-+---+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Dir dann baute die sch\u00f6ne Kapelle der treffliche Bembo,", "tokens": ["Dir", "dann", "bau\u00b7te", "die", "sch\u00f6\u00b7ne", "Ka\u00b7pel\u00b7le", "der", "treff\u00b7li\u00b7che", "Bem\u00b7bo", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NE", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Vater zu sein wohl wert eines ber\u00fchmteren Sohns.", "tokens": ["Va\u00b7ter", "zu", "sein", "wohl", "wert", "ei\u00b7nes", "be\u00b7r\u00fchm\u00b7te\u00b7ren", "Sohns", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VAINF", "ADV", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}}}}