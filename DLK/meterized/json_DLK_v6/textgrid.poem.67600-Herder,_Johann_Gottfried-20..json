{"textgrid.poem.67600": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Menschheit ist ein gro\u00dfer Leib voll Glieder;", "tokens": ["Die", "Menschheit", "ist", "ein", "gro\u00b7\u00dfer", "Leib", "voll", "Glie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "ADJD", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "F\u00fchlst Du Dich nicht in Deine Br\u00fcder,", "tokens": ["F\u00fchlst", "Du", "Dich", "nicht", "in", "Dei\u00b7ne", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So f\u00fchlt in Dich sich Niemand wieder.", "tokens": ["So", "f\u00fchlt", "in", "Dich", "sich", "Nie\u00b7mand", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "PRF", "PIS", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}