{"textgrid.poem.53165": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "An Reimar Leo", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jetzt schlaffen Berg' vnd Felder", "tokens": ["Jetzt", "schlaf\u00b7fen", "Ber\u00b7g'", "vnd", "Fel\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mit Reiff vnd Schnee verdeckt,", "tokens": ["Mit", "Reiff", "vnd", "Schnee", "ver\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auch haben sich die W\u00e4lder", "tokens": ["Auch", "ha\u00b7ben", "sich", "die", "W\u00e4l\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In jhr wei\u00df Kleid versteckt,", "tokens": ["In", "jhr", "wei\u00df", "Kleid", "ver\u00b7steckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Str\u00f6me stehn geschlossen,", "tokens": ["Die", "Str\u00f6\u00b7me", "stehn", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vnd sind in stiller Ruh,", "tokens": ["Vnd", "sind", "in", "stil\u00b7ler", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die lieblich sonst geflossen", "tokens": ["Die", "lieb\u00b7lich", "sonst", "ge\u00b7flos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit lauffen ab vnd zu.", "tokens": ["Mit", "lauf\u00b7fen", "ab", "vnd", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die B\u00e4ume, die sonst tragen", "tokens": ["Die", "B\u00e4u\u00b7me", ",", "die", "sonst", "tra\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sch\u00f6n Obst in gr\u00fcn verkleidt,", "tokens": ["Sch\u00f6n", "Obst", "in", "gr\u00fcn", "ver\u00b7kleidt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die m\u00fcssen jetzt beklagen", "tokens": ["Die", "m\u00fcs\u00b7sen", "jetzt", "be\u00b7kla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des strengen Nordens Neid,", "tokens": ["Des", "stren\u00b7gen", "Nor\u00b7dens", "Neid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nichts ist anjetzt zu finden,", "tokens": ["Nichts", "ist", "an\u00b7jetzt", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Was sonsten vns erfrewt,", "tokens": ["Was", "sons\u00b7ten", "vns", "er\u00b7frewt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Lust der Berg' vnd Gr\u00fcnden", "tokens": ["Die", "Lust", "der", "Ber\u00b7g'", "vnd", "Gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Ist jetzund Trawrigkeit.", "tokens": ["Ist", "je\u00b7tzund", "Traw\u00b7rig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So lange bi\u00df sich reget", "tokens": ["So", "lan\u00b7ge", "bi\u00df", "sich", "re\u00b7get"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "PRF", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der sanffte Westenwind,", "tokens": ["Der", "sanff\u00b7te", "Wes\u00b7ten\u00b7wind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vmb Berg' vnd Feld sich leget,", "tokens": ["Vmb", "Ber\u00b7g'", "vnd", "Feld", "sich", "le\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "PRF", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zun W\u00e4ldern auch sich findt", "tokens": ["Zun", "W\u00e4l\u00b7dern", "auch", "sich", "findt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "PRF", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vnd weckt, was sich verkrochen", "tokens": ["Vnd", "weckt", ",", "was", "sich", "ver\u00b7kro\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PRELS", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hatt' in den tieffen Schnee:", "tokens": ["Hatt'", "in", "den", "tief\u00b7fen", "Schnee", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Lentz ist angebrochen,", "tokens": ["Der", "Lentz", "ist", "an\u00b7ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein jedes nun auffsteh':", "tokens": ["Ein", "je\u00b7des", "nun", "auff\u00b7steh'", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Al\u00df mu\u00df die Welt erwachen,", "tokens": ["Al\u00df", "mu\u00df", "die", "Welt", "er\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Winterkleidt au\u00dfziehn,", "tokens": ["Das", "Win\u00b7ter\u00b7kleidt", "au\u00df\u00b7ziehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Berg' vnd Felder lachen,", "tokens": ["Die", "Ber\u00b7g'", "vnd", "Fel\u00b7der", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die H\u00fcgel werden gr\u00fcn,", "tokens": ["Die", "H\u00fc\u00b7gel", "wer\u00b7den", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die W\u00e4lder sich vernewen,", "tokens": ["Die", "W\u00e4l\u00b7der", "sich", "ver\u00b7ne\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ein jedes sich erfrewt,", "tokens": ["Ein", "je\u00b7des", "sich", "er\u00b7frewt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PRF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wie wann man geht zum Reyen,", "tokens": ["Wie", "wann", "man", "geht", "zum", "Re\u00b7yen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PWAV", "PIS", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vnd anders sich verkleidt.", "tokens": ["Vnd", "an\u00b7ders", "sich", "ver\u00b7kleidt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PRF", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Str\u00f6me m\u00fcssen lauffen", "tokens": ["Die", "Str\u00f6\u00b7me", "m\u00fcs\u00b7sen", "lauf\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In jhren alten Gang,", "tokens": ["In", "jhren", "al\u00b7ten", "Gang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Der V\u00f6gel leichte Hauffen", "tokens": ["Der", "V\u00f6\u00b7gel", "leich\u00b7te", "Hauf\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Stimpt an den Lobgesang,", "tokens": ["Stimpt", "an", "den", "Lob\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Lerche thut sich schwingen,", "tokens": ["Die", "Ler\u00b7che", "thut", "sich", "schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Schreyt in die Lufft hinein:", "tokens": ["Schreyt", "in", "die", "Lufft", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wir, wir, wir, wir, wir singen", "tokens": ["Wir", ",", "wir", ",", "wir", ",", "wir", ",", "wir", "sin\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "PPER", "$,", "PPER", "$,", "PPER", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dir, dir, dir Gott allein.", "tokens": ["Dir", ",", "dir", ",", "dir", "Gott", "al\u00b7lein", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "$,", "PPER", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Jetzt steht das Heer der Sternen", "tokens": ["Jetzt", "steht", "das", "Heer", "der", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am Himmel auff der Wacht,", "tokens": ["Am", "Him\u00b7mel", "auff", "der", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd leuchten vns von fernen,", "tokens": ["Vnd", "leuch\u00b7ten", "vns", "von", "fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vmb das es Mitternacht,", "tokens": ["Vmb", "das", "es", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bald wird mit jhren Straalen", "tokens": ["Bald", "wird", "mit", "jhren", "Straa\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Aurora bey vns seyn,", "tokens": ["Au\u00b7ro\u00b7ra", "bey", "vns", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Berge Spitzen mahlen,", "tokens": ["Der", "Ber\u00b7ge", "Spit\u00b7zen", "mah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Sternen f\u00fchren ein.", "tokens": ["Die", "Ster\u00b7nen", "f\u00fch\u00b7ren", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Nichts mag gefunden werden,", "tokens": ["Nichts", "mag", "ge\u00b7fun\u00b7den", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was nicht den Wechsel helt,", "tokens": ["Was", "nicht", "den", "Wech\u00b7sel", "helt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bald steht ein Ding auff Erden,", "tokens": ["Bald", "steht", "ein", "Ding", "auff", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bald hin es wieder felt,", "tokens": ["Bald", "hin", "es", "wie\u00b7der", "felt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vorau\u00df wir die wir schweben", "tokens": ["Vor\u00b7au\u00df", "wir", "die", "wir", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vmb dieses w\u00fcste rund,", "tokens": ["Vmb", "die\u00b7ses", "w\u00fcs\u00b7te", "rund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PDS", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df die\u00df sey vnser Leben,", "tokens": ["Da\u00df", "die\u00df", "sey", "vn\u00b7ser", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ist allenthalben kund.", "tokens": ["Ist", "al\u00b7len\u00b7thal\u00b7ben", "kund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wir m\u00fcssen au\u00dfgetauschet", "tokens": ["Wir", "m\u00fcs\u00b7sen", "au\u00df\u00b7ge\u00b7tau\u00b7schet"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Eins vmb das ander seyn,", "tokens": ["Eins", "vmb", "das", "an\u00b7der", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJD", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie eine Flut hinrauschet,", "tokens": ["Wie", "ei\u00b7ne", "Flut", "hin\u00b7rau\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die andre schlegt herein,", "tokens": ["Die", "and\u00b7re", "schlegt", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So bald wir vns verkriechen", "tokens": ["So", "bald", "wir", "vns", "ver\u00b7krie\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein jeder in das Grab", "tokens": ["Ein", "je\u00b7der", "in", "das", "Grab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vnd Todes sind verbliechen,", "tokens": ["Vnd", "To\u00b7des", "sind", "ver\u00b7blie\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sind die vns l\u00f6sen ab.", "tokens": ["Sind", "die", "vns", "l\u00f6\u00b7sen", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "PPER", "VVFIN", "PTKVZ",