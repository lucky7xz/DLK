{"textgrid.poem.44584": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "An Fanny Elssler", "genre": "verse", "period": "N.A.", "pub_year": 1851, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So willst du dich der Kunst entziehen?", "tokens": ["So", "willst", "du", "dich", "der", "Kunst", "ent\u00b7zie\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gib sie nicht auf, die heilge Kunst,", "tokens": ["Gib", "sie", "nicht", "auf", ",", "die", "heil\u00b7ge", "Kunst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "PTKVZ", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was uns zum Schutz ein Gott verliehen,", "tokens": ["Was", "uns", "zum", "Schutz", "ein", "Gott", "ver\u00b7lie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hat sich gel\u00f6st in Nebeldunst.", "tokens": ["Hat", "sich", "ge\u00b7l\u00f6st", "in", "Ne\u00b7bel\u00b7dunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das Gute, der Verstand, die Sitte", "tokens": ["Das", "Gu\u00b7te", ",", "der", "Ver\u00b7stand", ",", "die", "Sit\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Z\u00e4hmt nicht mehr dieses st\u00f6rrische Geschlecht,", "tokens": ["Z\u00e4hmt", "nicht", "mehr", "die\u00b7ses", "st\u00f6r\u00b7ri\u00b7sche", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Blind f\u00fcr das Unheil, taub der Bitte.", "tokens": ["Blind", "f\u00fcr", "das", "Un\u00b7heil", ",", "taub", "der", "Bit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur die Gewalt behielt ihr altes Recht.", "tokens": ["Nur", "die", "Ge\u00b7walt", "be\u00b7hielt", "ihr", "al\u00b7tes", "Recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nach au\u00dfen die Gewalt der Waffen,", "tokens": ["Nach", "au\u00b7\u00dfen", "die", "Ge\u00b7walt", "der", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nach innen zu der K\u00fcnste Macht,", "tokens": ["Nach", "in\u00b7nen", "zu", "der", "K\u00fcns\u00b7te", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die, was sie bilden, \u00e4hnlich schaffen,", "tokens": ["Die", ",", "was", "sie", "bil\u00b7den", ",", "\u00e4hn\u00b7lich", "schaf\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "PPER", "VVINF", "$,", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Urbild, das sich Gott gedacht,", "tokens": ["Dem", "Ur\u00b7bild", ",", "das", "sich", "Gott", "ge\u00b7dacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So da\u00df der Mensch in ihrem Spiegel", "tokens": ["So", "da\u00df", "der", "Mensch", "in", "ih\u00b7rem", "Spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Halb sich, halb seinen Ursprung kennt,", "tokens": ["Halb", "sich", ",", "halb", "sei\u00b7nen", "Ur\u00b7sprung", "kennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "$,", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf seiner Stirn das Geistersiegel,", "tokens": ["Auf", "sei\u00b7ner", "Stirn", "das", "Geis\u00b7ter\u00b7sie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Das nur noch leuchtet, nicht mehr brennt.", "tokens": ["Das", "nur", "noch", "leuch\u00b7tet", ",", "nicht", "mehr", "brennt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "VVFIN", "$,", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dir ward die holde Macht gegeben;", "tokens": ["Dir", "ward", "die", "hol\u00b7de", "Macht", "ge\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sei g\u00fcnstig du f\u00fcr so viel Gunst!", "tokens": ["Sei", "g\u00fcns\u00b7tig", "du", "f\u00fcr", "so", "viel", "Gunst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "PPER", "APPR", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht dir allein geh\u00f6rt dein Leben,", "tokens": ["Nicht", "dir", "al\u00b7lein", "ge\u00b7h\u00f6rt", "dein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPER", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gib sie nicht auf, die heilge Kunst!", "tokens": ["Gib", "sie", "nicht", "auf", ",", "die", "heil\u00b7ge", "Kunst", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "PTKVZ", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}