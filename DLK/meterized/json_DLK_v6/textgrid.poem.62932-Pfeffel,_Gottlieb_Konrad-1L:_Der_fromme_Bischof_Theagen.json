{"textgrid.poem.62932": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der fromme Bischof Theagen", "genre": "verse", "period": "N.A.", "pub_year": 1781, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der fromme Bischof Theagen", "tokens": ["Der", "from\u00b7me", "Bi\u00b7schof", "Thea\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Entwich in eine dunkle Grotte", "tokens": ["Ent\u00b7wich", "in", "ei\u00b7ne", "dunk\u00b7le", "Grot\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des Pallasw\u00e4ldchens bey Athen,", "tokens": ["Des", "Pal\u00b7las\u00b7w\u00e4ld\u00b7chens", "bey", "A\u00b7then", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Um weit vom Lerm mit seinem Gotte", "tokens": ["Um", "weit", "vom", "Lerm", "mit", "sei\u00b7nem", "Got\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ADJD", "APPRART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und sich vertrauter umzugehn.", "tokens": ["Und", "sich", "ver\u00b7trau\u00b7ter", "um\u00b7zu\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er lebte hier schon sieben Jahre,", "tokens": ["Er", "leb\u00b7te", "hier", "schon", "sie\u00b7ben", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Nur von Olympiern gesehn;", "tokens": ["Nur", "von", "O\u00b7lym\u00b7piern", "ge\u00b7sehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Schon fielen seine grauen Haare", "tokens": ["Schon", "fie\u00b7len", "sei\u00b7ne", "grau\u00b7en", "Haa\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wie Bl\u00e4tter, die der Nord verjagt;", "tokens": ["Wie", "Bl\u00e4t\u00b7ter", ",", "die", "der", "Nord", "ver\u00b7jagt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Schon t\u00f6nten seine Psalmen heiser", "tokens": ["Schon", "t\u00f6n\u00b7ten", "sei\u00b7ne", "Psal\u00b7men", "hei\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und t\u00e4glich schlug sein Busen leiser,", "tokens": ["Und", "t\u00e4g\u00b7lich", "schlug", "sein", "Bu\u00b7sen", "lei\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "An dem schon lang der Krebs genagt.", "tokens": ["An", "dem", "schon", "lang", "der", "Krebs", "ge\u00b7nagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Er f\u00fchlte, da\u00df sein Ende nahte;", "tokens": ["Er", "f\u00fchl\u00b7te", ",", "da\u00df", "sein", "En\u00b7de", "nah\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Sein welker Arm, der kaum den Stab", "tokens": ["Sein", "wel\u00b7ker", "Arm", ",", "der", "kaum", "den", "Stab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PRELS", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Noch halten kann, ergreift die Spate,", "tokens": ["Noch", "hal\u00b7ten", "kann", ",", "er\u00b7greift", "die", "Spa\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Und fr\u00f6lich macht er sich sein Grab.", "tokens": ["Und", "fr\u00f6\u00b7lich", "macht", "er", "sich", "sein", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PRF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Itzt ragt was aus dem Schoos der Erde;", "tokens": ["Itzt", "ragt", "was", "aus", "dem", "Schoos", "der", "Er\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Es war ein marmorner Altar,", "tokens": ["Es", "war", "ein", "mar\u00b7mor\u00b7ner", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.19": {"text": "Der einst der G\u00f6ttin heilig war.", "tokens": ["Der", "einst", "der", "G\u00f6t\u00b7tin", "hei\u00b7lig", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Doch schnell erhob sich aus dem Herde", "tokens": ["Doch", "schnell", "er\u00b7hob", "sich", "aus", "dem", "Her\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Ein kleiner goldner Aschenkrug", "tokens": ["Ein", "klei\u00b7ner", "gold\u00b7ner", "A\u00b7schen\u00b7krug"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Von hellem Sternenglanz umgeben.", "tokens": ["Von", "hel\u00b7lem", "Ster\u00b7nen\u00b7glanz", "um\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Gott! rief der Greis mit heilgem Beben,", "tokens": ["Gott", "!", "rief", "der", "Greis", "mit", "heil\u00b7gem", "Be\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Ists Wahrheit? Ist es Augentrug?", "tokens": ["Ists", "Wahr\u00b7heit", "?", "Ist", "es", "Au\u00b7gen\u00b7trug", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Er wagts die Urne wegzuheben,", "tokens": ["Er", "wagts", "die", "Ur\u00b7ne", "weg\u00b7zu\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Die kaum in seinen H\u00e4nden weilt,", "tokens": ["Die", "kaum", "in", "sei\u00b7nen", "H\u00e4n\u00b7den", "weilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "So \u00fcberstr\u00f6mt ihn neues Leben", "tokens": ["So", "\u00fc\u00b7ber\u00b7str\u00f6mt", "ihn", "neu\u00b7es", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Und schon ist seine Brust geheilt.", "tokens": ["Und", "schon", "ist", "sei\u00b7ne", "Brust", "ge\u00b7heilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Der Alte sinkt vor dem Altare", "tokens": ["Der", "Al\u00b7te", "sinkt", "vor", "dem", "Al\u00b7ta\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "In Thr\u00e4nen auf sein Angesicht:", "tokens": ["In", "Thr\u00e4\u00b7nen", "auf", "sein", "An\u00b7ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Verschm\u00e4he meine Bitte nicht,", "tokens": ["Ver\u00b7schm\u00e4\u00b7he", "mei\u00b7ne", "Bit\u00b7te", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Gott! noch ein Wunder, offenbare", "tokens": ["Gott", "!", "noch", "ein", "Wun\u00b7der", ",", "of\u00b7fen\u00b7ba\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "$.", "ADV", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Mir, Herr, wer dein Apostel ist,", "tokens": ["Mir", ",", "Herr", ",", "wer", "dein", "A\u00b7pos\u00b7tel", "ist", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "PWS", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Den dieser Aschenkrug verschlie\u00dft.", "tokens": ["Den", "die\u00b7ser", "A\u00b7schen\u00b7krug", "ver\u00b7schlie\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Der Grund des Felsen wird ersch\u00fcttert", "tokens": ["Der", "Grund", "des", "Fel\u00b7sen", "wird", "er\u00b7sch\u00fct\u00b7tert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Und eine himmlische Gestalt,", "tokens": ["Und", "ei\u00b7ne", "himm\u00b7li\u00b7sche", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Die wie ein Blitz vor\u00fcberwallt,", "tokens": ["Die", "wie", "ein", "Blitz", "vor\u00b7\u00fc\u00b7berw\u00b7allt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Ruft: Socrates! Der Alte zittert", "tokens": ["Ruft", ":", "So\u00b7cra\u00b7tes", "!", "Der", "Al\u00b7te", "zit\u00b7tert"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$.", "NE", "$.", "ART", "NN", "VVFIN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.39": {"text": "Und in dem Stralendiadem", "tokens": ["Und", "in", "dem", "Stra\u00b7len\u00b7di\u00b7a\u00b7dem"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.40": {"text": "Des Krugs, auf den sein Blick sich kehret,", "tokens": ["Des", "Krugs", ",", "auf", "den", "sein", "Blick", "sich", "keh\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Liest er besch\u00e4mt: wer Gott verehret", "tokens": ["Liest", "er", "be\u00b7sch\u00e4mt", ":", "wer", "Gott", "ver\u00b7eh\u00b7ret"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "$.", "PWS", "NN", "VVFIN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.42": {"text": "Und recht thut, ist ihm angenehm.", "tokens": ["Und", "recht", "thut", ",", "ist", "ihm", "an\u00b7ge\u00b7nehm", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$,", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}