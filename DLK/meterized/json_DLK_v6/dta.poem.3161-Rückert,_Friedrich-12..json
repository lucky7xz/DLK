{"dta.poem.3161": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Oft zu verspotten scheint das Schicksal unsern Plan,", "tokens": ["Oft", "zu", "ver\u00b7spot\u00b7ten", "scheint", "das", "Schick\u00b7sal", "un\u00b7sern", "Plan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch wir verspotten es, es ist uns unterthan.", "tokens": ["Doch", "wir", "ver\u00b7spot\u00b7ten", "es", ",", "es", "ist", "uns", "un\u00b7ter\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mit Liebe dacht' ich dein an einem stillen Abend,", "tokens": ["Mit", "Lie\u00b7be", "dacht'", "ich", "dein", "an", "ei\u00b7nem", "stil\u00b7len", "A\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPOSAT", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Lebensabend malt' ich dir so still und labend.", "tokens": ["Den", "Le\u00b7bens\u00b7a\u00b7bend", "malt'", "ich", "dir", "so", "still", "und", "la\u00b7bend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du solltest leben, bis ich meinen Sohn verm\u00e4hlte,", "tokens": ["Du", "soll\u00b7test", "le\u00b7ben", ",", "bis", "ich", "mei\u00b7nen", "Sohn", "ver\u00b7m\u00e4hl\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ein Urenkel noch ein M\u00e4hrchen dir erz\u00e4hlte.", "tokens": ["Und", "ein", "Ur\u00b7en\u00b7kel", "noch", "ein", "M\u00e4hr\u00b7chen", "dir", "er\u00b7z\u00e4hl\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Das sollte tr\u00f6sten dich f\u00fcr jeglichen Verlust,", "tokens": ["Das", "soll\u00b7te", "tr\u00f6s\u00b7ten", "dich", "f\u00fcr", "jeg\u00b7li\u00b7chen", "Ver\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "VVFIN", "PRF", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und bl\u00fch'n seh'n solltest du noch einmal deine Lust.", "tokens": ["Und", "bl\u00fch'n", "seh'n", "soll\u00b7test", "du", "noch", "ein\u00b7mal", "dei\u00b7ne", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVINF", "VMFIN", "PPER", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Am selben Abende, mir ungeahnet, fern,", "tokens": ["Am", "sel\u00b7ben", "A\u00b7ben\u00b7de", ",", "mir", "un\u00b7ge\u00b7ah\u00b7net", ",", "fern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PPER", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bist du gegangen, abgerufen von dem Herrn.", "tokens": ["Bist", "du", "ge\u00b7gan\u00b7gen", ",", "ab\u00b7ge\u00b7ru\u00b7fen", "von", "dem", "Herrn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ward von dem Schlage so der Lebensbaum vernichtet?", "tokens": ["Ward", "von", "dem", "Schla\u00b7ge", "so", "der", "Le\u00b7bens\u00b7baum", "ver\u00b7nich\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wenig nichtig ist, was Liebe je gedichtet.", "tokens": ["So", "we\u00b7nig", "nich\u00b7tig", "ist", ",", "was", "Lie\u00b7be", "je", "ge\u00b7dich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VAFIN", "$,", "PWS", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "In einem Augenblick hab' ich ein langes Leben", "tokens": ["In", "ei\u00b7nem", "Au\u00b7gen\u00b7blick", "hab'", "ich", "ein", "lan\u00b7ges", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit dir gelebt, und kann der Gruft dich ruhig geben.", "tokens": ["Mit", "dir", "ge\u00b7lebt", ",", "und", "kann", "der", "Gruft", "dich", "ru\u00b7hig", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "KON", "VMFIN", "ART", "NN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}