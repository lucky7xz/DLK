{"textgrid.poem.59833": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wir sa\u00dfen noch sp\u00e4t beisammen,", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir sa\u00dfen noch sp\u00e4t beisammen,", "tokens": ["Wir", "sa\u00b7\u00dfen", "noch", "sp\u00e4t", "bei\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der alte Wirt und ich;", "tokens": ["Der", "al\u00b7te", "Wirt", "und", "ich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Weines heilige Flammen", "tokens": ["Des", "Wei\u00b7nes", "hei\u00b7li\u00b7ge", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ergossen sich \u00fcber mich;", "tokens": ["Er\u00b7gos\u00b7sen", "sich", "\u00fc\u00b7ber", "mich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die reine Glut der Jugend", "tokens": ["Die", "rei\u00b7ne", "Glut", "der", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mir wiederzugeben schien er \u2013", "tokens": ["Mir", "wie\u00b7der\u00b7zu\u00b7ge\u00b7ben", "schien", "er", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVIZU", "VVFIN", "PPER", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Nie f\u00fchlt ich so die Tugend", "tokens": ["Nie", "f\u00fchlt", "ich", "so", "die", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des roten Kachetiner.", "tokens": ["Des", "ro\u00b7ten", "Ka\u00b7che\u00b7ti\u00b7ner", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Ich konnt' im s\u00fc\u00dfen Drang", "tokens": ["Ich", "konnt'", "im", "s\u00fc\u00b7\u00dfen", "Drang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Nur immer schl\u00fcrfen und nippen,", "tokens": ["Nur", "im\u00b7mer", "schl\u00fcr\u00b7fen", "und", "nip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Es wurden zu Gesang", "tokens": ["Es", "wur\u00b7den", "zu", "Ge\u00b7sang"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Die Worte meiner Lippen;", "tokens": ["Die", "Wor\u00b7te", "mei\u00b7ner", "Lip\u00b7pen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Wie Adam vor dem Falle,", "tokens": ["Wie", "A\u00b7dam", "vor", "dem", "Fal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "So schwamm ich in Entz\u00fccken", "tokens": ["So", "schwamm", "ich", "in", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Und w\u00fcnschte, ich k\u00f6nnte alle", "tokens": ["Und", "w\u00fcnschte", ",", "ich", "k\u00f6nn\u00b7te", "al\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PPER", "VMFIN", "PIS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Auf Erden mitbegl\u00fccken.", "tokens": ["Auf", "Er\u00b7den", "mit\u00b7be\u00b7gl\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sprach ich zum Wirt: Ich wollte,", "tokens": ["Sprach", "ich", "zum", "Wirt", ":", "Ich", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "$.", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich k\u00f6nnte in Wein zerflie\u00dfen!", "tokens": ["Ich", "k\u00f6nn\u00b7te", "in", "Wein", "zer\u00b7flie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mein fl\u00fcssiger K\u00f6rper sollte", "tokens": ["Mein", "fl\u00fcs\u00b7si\u00b7ger", "K\u00f6r\u00b7per", "soll\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ins Weltmeer sich ergie\u00dfen!", "tokens": ["Ins", "Welt\u00b7meer", "sich", "er\u00b7gie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "VVINF", "$."], "meter": "-+---+-", "measure": "dactylic.init"}, "line.5": {"text": "Und sollt' es mit Weisheit w\u00fcrzen,", "tokens": ["Und", "sollt'", "es", "mit", "Weis\u00b7heit", "w\u00fcr\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Dann sollte ins Meer zu den Fischen", "tokens": ["Dann", "soll\u00b7te", "ins", "Meer", "zu", "den", "Fi\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Die ganze Welt sich st\u00fcrzen:", "tokens": ["Die", "gan\u00b7ze", "Welt", "sich", "st\u00fcr\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Schulen und Moscheen,", "tokens": ["Die", "Schu\u00b7len", "und", "Mo\u00b7scheen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Die Heiligen, die Wunder,", "tokens": ["Die", "Hei\u00b7li\u00b7gen", ",", "die", "Wun\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Die alle darin zu sehn,", "tokens": ["Die", "al\u00b7le", "da\u00b7rin", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PAV", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Der ganze alte Plunder,", "tokens": ["Der", "gan\u00b7ze", "al\u00b7te", "Plun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Der sollte untergehn!", "tokens": ["Der", "soll\u00b7te", "un\u00b7ter\u00b7gehn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Ich wollte alles auf Erden", "tokens": ["Ich", "woll\u00b7te", "al\u00b7les", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIS", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Befreien aus seiner Haft,", "tokens": ["Be\u00b7frei\u00b7en", "aus", "sei\u00b7ner", "Haft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Es sollte zu Wasser werden", "tokens": ["Es", "soll\u00b7te", "zu", "Was\u00b7ser", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VAINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Die ganze Wissenschaft \u2013", "tokens": ["Die", "gan\u00b7ze", "Wis\u00b7sen\u00b7schaft", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Sie sollte untergehen", "tokens": ["Sie", "soll\u00b7te", "un\u00b7ter\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Und wieder auferstehen", "tokens": ["Und", "wie\u00b7der", "auf\u00b7er\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "In neuer Glut und Kraft!", "tokens": ["In", "neu\u00b7er", "Glut", "und", "Kraft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O la\u00df, Mirza-Schaffy!", "tokens": ["O", "la\u00df", ",", "Mir\u00b7za\u00b7Schaffy", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "\u2013 So sprach der alte Weinwirt \u2013", "tokens": ["\u2013", "So", "sprach", "der", "al\u00b7te", "Wein\u00b7wirt", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df deine Phantasie,", "tokens": ["La\u00df", "dei\u00b7ne", "Phan\u00b7ta\u00b7sie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und bis dein Leib zu Wein wird,", "tokens": ["Und", "bis", "dein", "Leib", "zu", "Wein", "wird", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "APPR", "NN", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bis deine Glieder zerflie\u00dfen,", "tokens": ["Bis", "dei\u00b7ne", "Glie\u00b7der", "zer\u00b7flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Zu w\u00fcrzen des Weltmeers Flut:", "tokens": ["Zu", "w\u00fcr\u00b7zen", "des", "Welt\u00b7meers", "Flut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "La\u00df sich in dich ergie\u00dfen", "tokens": ["La\u00df", "sich", "in", "dich", "er\u00b7gie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PRF", "APPR", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des Weines heilige Glut!", "tokens": ["Des", "Wei\u00b7nes", "hei\u00b7li\u00b7ge", "Glut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "La\u00df alle frommen Toren", "tokens": ["La\u00df", "al\u00b7le", "from\u00b7men", "To\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "In N\u00fcchternheit versinken;", "tokens": ["In", "N\u00fcch\u00b7tern\u00b7heit", "ver\u00b7sin\u00b7ken", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Kein Tropfen geht verloren", "tokens": ["Kein", "Trop\u00b7fen", "geht", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Von dem, was Weise trinken!", "tokens": ["Von", "dem", ",", "was", "Wei\u00b7se", "trin\u00b7ken", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PWS", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}