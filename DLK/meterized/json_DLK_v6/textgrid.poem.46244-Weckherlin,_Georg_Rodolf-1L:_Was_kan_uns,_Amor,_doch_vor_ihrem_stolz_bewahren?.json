{"textgrid.poem.46244": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was kan uns, Amor, doch vor ihrem stolz bewahren?", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was kan uns, Amor, doch vor ihrem stolz bewahren?", "tokens": ["Was", "kan", "uns", ",", "A\u00b7mor", ",", "doch", "vor", "ih\u00b7rem", "stolz", "be\u00b7wah\u00b7ren", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "$,", "NE", "$,", "ADV", "APPR", "PPOSAT", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "umsunst seind deine pfeil, umsunst ist mein unfall.", "tokens": ["um\u00b7sunst", "seind", "dei\u00b7ne", "pfeil", ",", "um\u00b7sunst", "ist", "mein", "un\u00b7fall", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "je mehr ich unsre schand mit ihrem lob erschall,", "tokens": ["je", "mehr", "ich", "uns\u00b7re", "schand", "mit", "ih\u00b7rem", "lob", "er\u00b7schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPOSAT", "VVFIN", "APPR", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "je weniger sie mich vermeinet zu entfahren.", "tokens": ["je", "we\u00b7ni\u00b7ger", "sie", "mich", "ver\u00b7mei\u00b7net", "zu", "ent\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPER", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dem bleichen agstein gleich ist der strom ihrer haaren,", "tokens": ["Dem", "blei\u00b7chen", "ags\u00b7tein", "gleich", "ist", "der", "strom", "ih\u00b7rer", "haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--++-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "ihr runde kehl und hals ist pur als ein kristall,", "tokens": ["ihr", "run\u00b7de", "kehl", "und", "hals", "ist", "pur", "als", "ein", "kris\u00b7tall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "ADV", "VAFIN", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "ein marber ihre brust, das w\u00e4rzlein ein korall,", "tokens": ["ein", "mar\u00b7ber", "ih\u00b7re", "brust", ",", "das", "w\u00e4rz\u00b7lein", "ein", "ko\u00b7rall", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "den alabasterstein h\u00e4nd und arm offenbaren.", "tokens": ["den", "a\u00b7la\u00b7bas\u00b7ter\u00b7stein", "h\u00e4nd", "und", "arm", "of\u00b7fen\u00b7ba\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und ihre zween angstern sind funkende saphir,", "tokens": ["Und", "ih\u00b7re", "zween", "angs\u00b7tern", "sind", "fun\u00b7ken\u00b7de", "sa\u00b7phir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VVFIN", "NN", "VAFIN", "VVFIN", "PPER", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.10": {"text": "ein lachender rubin auf ihrem mund prachtieret,", "tokens": ["ein", "la\u00b7chen\u00b7der", "ru\u00b7bin", "auf", "ih\u00b7rem", "mund", "prach\u00b7tie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "von hartestem demant hat sie ein herz in ihr.", "tokens": ["von", "har\u00b7tes\u00b7tem", "de\u00b7mant", "hat", "sie", "ein", "herz", "in", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ist es ein wunder dan, da\u00df sie (stolz) triumfieret,", "tokens": ["Ist", "es", "ein", "wun\u00b7der", "dan", ",", "da\u00df", "sie", "(", "stolz", ")", "tri\u00b7um\u00b7fie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "$,", "KOUS", "PPER", "$(", "ADJD", "$(", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Amor, und ist so hart stets gegen dir und mir,", "tokens": ["A\u00b7mor", ",", "und", "ist", "so", "hart", "stets", "ge\u00b7gen", "dir", "und", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "VAFIN", "ADV", "ADJD", "ADV", "APPR", "PPER", "KON", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "wan sie die natur selbs ganz steinin geformieret?", "tokens": ["wan", "sie", "die", "na\u00b7tur", "selbs", "ganz", "stei\u00b7nin", "ge\u00b7for\u00b7mie\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}}}}}