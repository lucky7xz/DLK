{"textgrid.poem.25348": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dort glimmt das Licht. Dies ist der Ort. Den Kahn", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dort glimmt das Licht. Dies ist der Ort. Den Kahn", "tokens": ["Dort", "glimmt", "das", "Licht", ".", "Dies", "ist", "der", "Ort", ".", "Den", "Kahn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "PDS", "VAFIN", "ART", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kn\u00fcpf ich im Dunkel an die schwarzen Bohlen.", "tokens": ["Kn\u00fcpf", "ich", "im", "Dun\u00b7kel", "an", "die", "schwar\u00b7zen", "Boh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und hier ist Land. Wie unter mir der Grund", "tokens": ["Und", "hier", "ist", "Land", ".", "Wie", "un\u00b7ter", "mir", "der", "Grund"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "NN", "$.", "PWAV", "APPR", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Aufknirscht, weht \u00fcbers Wasser her noch kaum", "tokens": ["Auf\u00b7knirscht", ",", "weht", "\u00fc\u00b7bers", "Was\u00b7ser", "her", "noch", "kaum"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "APPRART", "NN", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Fernab der Klang von Stimmen, k\u00f6rperlos", "tokens": ["Fern\u00b7ab", "der", "Klang", "von", "Stim\u00b7men", ",", "k\u00f6r\u00b7per\u00b7los"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$,", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In tiefe Luft gel\u00f6st. Die Stille dr\u00fcckt", "tokens": ["In", "tie\u00b7fe", "Luft", "ge\u00b7l\u00f6st", ".", "Die", "Stil\u00b7le", "dr\u00fcckt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die Wangen fiebernd gegen mich. So sei's", "tokens": ["Die", "Wan\u00b7gen", "fie\u00b7bernd", "ge\u00b7gen", "mich", ".", "So", "sei's"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJD", "APPR", "PPER", "$.", "ADV", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Gewagt. Nur wenig Schritte: Mich umf\u00e4ngt", "tokens": ["Ge\u00b7wagt", ".", "Nur", "we\u00b7nig", "Schrit\u00b7te", ":", "Mich", "um\u00b7f\u00e4ngt"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVPP", "$.", "ADV", "PIAT", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Die Schwelle. T\u00fcren tun sich auf. Mich fa\u00dft", "tokens": ["Die", "Schwel\u00b7le", ".", "T\u00fc\u00b7ren", "tun", "sich", "auf", ".", "Mich", "fa\u00dft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "NN", "VVFIN", "PRF", "PTKVZ", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Durchs Dunkel eine Hand, weicher als Glanz", "tokens": ["Durchs", "Dun\u00b7kel", "ei\u00b7ne", "Hand", ",", "wei\u00b7cher", "als", "Glanz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "ADJA", "KOUS", "NN"], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "Des wei\u00dfen Flaums vom Fittich junger V\u00f6gel.", "tokens": ["Des", "wei\u00b7\u00dfen", "Flaums", "vom", "Fit\u00b7tich", "jun\u00b7ger", "V\u00f6\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und dann ist D\u00e4mmerung des blauen Zimmers,", "tokens": ["Und", "dann", "ist", "D\u00e4m\u00b7me\u00b7rung", "des", "blau\u00b7en", "Zim\u00b7mers", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und Arme sind und Glieder ausgespannt,", "tokens": ["Und", "Ar\u00b7me", "sind", "und", "Glie\u00b7der", "aus\u00b7ge\u00b7spannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Mich zu umschlie\u00dfen, mich zu decken", "tokens": ["Mich", "zu", "um\u00b7schlie\u00b7\u00dfen", ",", "mich", "zu", "de\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Und einzufangen wie in einem Netz", "tokens": ["Und", "ein\u00b7zu\u00b7fan\u00b7gen", "wie", "in", "ei\u00b7nem", "Netz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "KOKOM", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Gestickt aus Traum und Wunder dieser Nacht,", "tokens": ["Ge\u00b7stickt", "aus", "Traum", "und", "Wun\u00b7der", "die\u00b7ser", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Und duftend Haar ist \u00fcber mich gestreut", "tokens": ["Und", "duf\u00b7tend", "Haar", "ist", "\u00fc\u00b7ber", "mich", "ge\u00b7streut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "NN", "VAFIN", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Wie aufgel\u00f6ste B\u00fcndel wilder Blumen.", "tokens": ["Wie", "auf\u00b7ge\u00b7l\u00f6s\u00b7te", "B\u00fcn\u00b7del", "wil\u00b7der", "Blu\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was zaudr' ich noch? Die \u00f6de Fr\u00fche lehnt", "tokens": ["Was", "zaudr'", "ich", "noch", "?", "Die", "\u00f6\u00b7de", "Fr\u00fc\u00b7he", "lehnt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Noch blutlos hinterm hohen Tor der Sterne,", "tokens": ["Noch", "blut\u00b7los", "hin\u00b7term", "ho\u00b7hen", "Tor", "der", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und mein ist diese Nacht \u2013 Ihr tiefstes Gl\u00fcck", "tokens": ["Und", "mein", "ist", "die\u00b7se", "Nacht", "\u2013", "Ihr", "tiefs\u00b7tes", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPOSAT", "VAFIN", "PDAT", "NN", "$(", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zieh ich wie einen Mantel um mich her.", "tokens": ["Zieh", "ich", "wie", "ei\u00b7nen", "Man\u00b7tel", "um", "mich", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Was zaudr' ich noch? Die kleine Lampe schwingt", "tokens": ["Was", "zaudr'", "ich", "noch", "?", "Die", "klei\u00b7ne", "Lam\u00b7pe", "schwingt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bet\u00f6rend ihre Strahlen durch das Finster", "tokens": ["Be\u00b7t\u00f6\u00b7rend", "ih\u00b7re", "Strah\u00b7len", "durch", "das", "Fins\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und rei\u00dft auf hellen Leitern mich empor.", "tokens": ["Und", "rei\u00dft", "auf", "hel\u00b7len", "Lei\u00b7tern", "mich", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Was r\u00fchrt mich pl\u00f6tzlich an? Ist das mein Blut,", "tokens": ["Was", "r\u00fchrt", "mich", "pl\u00f6tz\u00b7lich", "an", "?", "Ist", "das", "mein", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADJD", "PTKVZ", "$.", "VAFIN", "PDS", "PPOSAT", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das hier so pocht? Wer naht? Vom schwarzen Wasser", "tokens": ["Das", "hier", "so", "pocht", "?", "Wer", "naht", "?", "Vom", "schwar\u00b7zen", "Was\u00b7ser"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "ADV", "ADV", "VVFIN", "$.", "PWS", "VVFIN", "$.", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hebt sich ein Wind. Die Stufen schauern K\u00fchle.", "tokens": ["Hebt", "sich", "ein", "Wind", ".", "Die", "Stu\u00b7fen", "schau\u00b7ern", "K\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$.", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ganz fern schwebt jetzt das Licht, in solcher Ferne", "tokens": ["Ganz", "fern", "schwebt", "jetzt", "das", "Licht", ",", "in", "sol\u00b7cher", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "ADV", "ART", "NN", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wie eine hochgehobne Opferschale,", "tokens": ["Wie", "ei\u00b7ne", "hoch\u00b7ge\u00b7hob\u00b7ne", "Op\u00b7fer\u00b7scha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die schwankend meines Schicksals Flamme tr\u00e4gt.", "tokens": ["Die", "schwan\u00b7kend", "mei\u00b7nes", "Schick\u00b7sals", "Flam\u00b7me", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Was schaudert mir? Ein Fremdes fa\u00dft mich an.", "tokens": ["Was", "schau\u00b7dert", "mir", "?", "Ein", "Frem\u00b7des", "fa\u00dft", "mich", "an", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich sp\u00fcre eisig \u00fcber meinem Haupt", "tokens": ["Ich", "sp\u00fc\u00b7re", "ei\u00b7sig", "\u00fc\u00b7ber", "mei\u00b7nem", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vergangenes und Ungeborenes", "tokens": ["Ver\u00b7gan\u00b7ge\u00b7nes", "und", "Un\u00b7ge\u00b7bo\u00b7re\u00b7nes"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "KON", "NN"], "meter": "-+---+-+--", "measure": "dactylic.init"}, "line.4": {"text": "Mit gro\u00dfem Fl\u00fcgelschlag hinrauschen und", "tokens": ["Mit", "gro\u00b7\u00dfem", "Fl\u00fc\u00b7gel\u00b7schlag", "hin\u00b7rau\u00b7schen", "und"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF", "KON"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In einem dunkeln Sturz von fremder Flut", "tokens": ["In", "ei\u00b7nem", "dun\u00b7keln", "Sturz", "von", "frem\u00b7der", "Flut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ins Uferlose j\u00e4h mich fortgerissen.", "tokens": ["Ins", "U\u00b7fer\u00b7lo\u00b7se", "j\u00e4h", "mich", "fort\u00b7ge\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}