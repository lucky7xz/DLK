{"textgrid.poem.61015": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die stille Wasserrose", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die stille Wasserrose", "tokens": ["Die", "stil\u00b7le", "Was\u00b7ser\u00b7ro\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Steigt aus dem blauen See,", "tokens": ["Steigt", "aus", "dem", "blau\u00b7en", "See", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die feuchten Bl\u00e4tter zittern,", "tokens": ["Die", "feuch\u00b7ten", "Bl\u00e4t\u00b7ter", "zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Kelch ist wei\u00df wie Schnee.", "tokens": ["Der", "Kelch", "ist", "wei\u00df", "wie", "Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da gie\u00dft der Mond vom Himmel", "tokens": ["Da", "gie\u00dft", "der", "Mond", "vom", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "All seinen goldnen Schein,", "tokens": ["All", "sei\u00b7nen", "gold\u00b7nen", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gie\u00dft alle seine Strahlen", "tokens": ["Gie\u00dft", "al\u00b7le", "sei\u00b7ne", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In ihren Scho\u00df hinein.", "tokens": ["In", "ih\u00b7ren", "Scho\u00df", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Im Wasser um die Blume", "tokens": ["Im", "Was\u00b7ser", "um", "die", "Blu\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kreiset ein wei\u00dfer Schwan;", "tokens": ["Krei\u00b7set", "ein", "wei\u00b7\u00dfer", "Schwan", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Er singt so s\u00fc\u00df, so leise", "tokens": ["Er", "singt", "so", "s\u00fc\u00df", ",", "so", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und schaut die Blume an.", "tokens": ["Und", "schaut", "die", "Blu\u00b7me", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er singt so s\u00fc\u00df, so leise", "tokens": ["Er", "singt", "so", "s\u00fc\u00df", ",", "so", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und will im Singen vergehn -", "tokens": ["Und", "will", "im", "Sin\u00b7gen", "ver\u00b7gehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "O Blume, wei\u00dfe Blume,", "tokens": ["O", "Blu\u00b7me", ",", "wei\u00b7\u00dfe", "Blu\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kannst du das Lied verstehn?", "tokens": ["Kannst", "du", "das", "Lied", "ver\u00b7stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}