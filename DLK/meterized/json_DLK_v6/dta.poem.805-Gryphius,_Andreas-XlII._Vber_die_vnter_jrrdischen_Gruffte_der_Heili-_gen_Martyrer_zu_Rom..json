{"dta.poem.805": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XlII.  \n Vber die vnter jrrdischen Gruffte der Heili-  \n gen Martyrer zu Rom.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Hjer beuge Knie vnd Haupt! die vnter jrrd'schen g\u00e4nge ", "tokens": ["Hjer", "beu\u00b7ge", "Knie", "vnd", "Haupt", "!", "die", "vn\u00b7ter", "jrrd'\u00b7schen", "g\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "NN", "KON", "NN", "$.", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die gr\u00fcffte sonder licht/ die du best\u00fcrtzter Christ/", "tokens": ["Die", "gr\u00fcff\u00b7te", "son\u00b7der", "licht", "/", "die", "du", "be\u00b7st\u00fcrtz\u00b7ter", "Christ", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "NN", "$(", "PRELS", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nicht ohn entsetzen sihst/ die waren als die list", "tokens": ["Nicht", "ohn", "ent\u00b7set\u00b7zen", "sihst", "/", "die", "wa\u00b7ren", "als", "die", "list"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "VVINF", "VVFIN", "$(", "PDS", "VAFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd Macht Gott Krieg anbott/ nicht tausenden zu enge.", "tokens": ["Vnd", "Macht", "Gott", "Krieg", "an\u00b7bott", "/", "nicht", "tau\u00b7sen\u00b7den", "zu", "en\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "NN", "VVFIN", "$(", "PTKNEG", "VVINF", "PTKZU", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Leichen sonder zahl/ der heilaen C\u00f6rper menge", "tokens": ["Die", "Lei\u00b7chen", "son\u00b7der", "zahl", "/", "der", "hei\u00b7la\u00b7en", "C\u00f6r\u00b7per", "men\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "$(", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Sind die/ auff die sich H\u00f6ll vnd Welt vmbsonst ger\u00fc\u00dft/", "tokens": ["Sind", "die", "/", "auff", "die", "sich", "H\u00f6ll", "vnd", "Welt", "vmbsonst", "ge\u00b7r\u00fc\u00dft", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "$(", "APPR", "PRELS", "PRF", "NN", "KON", "NN", "ADV", "VVPP", "$("], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Die Pein vnd Todt gepocht! die Pfal vnd Schwerd ge-", "tokens": ["Die", "Pein", "vnd", "Todt", "ge\u00b7pocht", "!", "die", "Pfal", "vnd", "Schwerd", "ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "VVPP", "$.", "ART", "NN", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "k\u00fc\u00dft/ ", "tokens": ["k\u00fc\u00dft", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Die nach der quaal gerennt mit fr\u00f6lichem gedr\u00e4nge.", "tokens": ["Die", "nach", "der", "qua\u00b7al", "ge\u00b7rennt", "mit", "fr\u00f6\u00b7li\u00b7chem", "ge\u00b7dr\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Hier ists wo Christus Kirch/ mit fewrigen Gebetten/", "tokens": ["Hier", "ists", "wo", "Chris\u00b7tus", "Kirch", "/", "mit", "few\u00b7ri\u00b7gen", "Ge\u00b7bet\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PWAV", "NE", "NE", "$(", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von Blut vnd Thr\u00e4nen na\u00df/ Gott vor gesicht getretten", "tokens": ["Von", "Blut", "vnd", "Thr\u00e4\u00b7nen", "na\u00df", "/", "Gott", "vor", "ge\u00b7sicht", "ge\u00b7tret\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "$(", "NN", "APPR", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die stets der Welt abstarb/ must vnter leichen seyn.", "tokens": ["Die", "stets", "der", "Welt", "ab\u00b7starb", "/", "must", "vn\u00b7ter", "lei\u00b7chen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "$(", "VMFIN", "APPR", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die ewig wachsen solt; must alhier Wurtzel finden/", "tokens": ["Die", "e\u00b7wig", "wach\u00b7sen", "solt", ";", "must", "al\u00b7hier", "Wurt\u00b7zel", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVINF", "VMFIN", "$.", "VMFIN", "ADV", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In dieser finstern Nacht must jhr Licht sich entz\u00fcnden/", "tokens": ["In", "die\u00b7ser", "fins\u00b7tern", "Nacht", "must", "jhr", "Licht", "sich", "ent\u00b7z\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "PRF", "VVINF", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die auf den Fel\u00df gegr\u00fcndt/ wohnt\u2019 vnter lauter Stein.", "tokens": ["Die", "auf", "den", "Fel\u00df", "ge\u00b7gr\u00fcndt", "/", "wohnt'", "vn\u00b7ter", "lau\u00b7ter", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "$(", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}