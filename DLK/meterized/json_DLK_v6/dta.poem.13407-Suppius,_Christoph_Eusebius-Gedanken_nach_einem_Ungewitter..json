{"dta.poem.13407": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "Gedanken  \n nach einem Ungewitter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "Tr\u00f6ste dich, betr\u00fcbtes Herze,", "tokens": ["Tr\u00f6s\u00b7te", "dich", ",", "be\u00b7tr\u00fcb\u00b7tes", "Her\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun ist die Gefahr vorbey!", "tokens": ["Nun", "ist", "die", "Ge\u00b7fahr", "vor\u00b7bey", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wisse, da\u00df des Himmels Schw\u00e4rze", "tokens": ["Wis\u00b7se", ",", "da\u00df", "des", "Him\u00b7mels", "Schw\u00e4r\u00b7ze"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Keiner Welt mehr t\u00f6dtlich sey,", "tokens": ["Kei\u00b7ner", "Welt", "mehr", "t\u00f6dt\u00b7lich", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil, was Luft und Himmel tr\u00fcbt,", "tokens": ["Weil", ",", "was", "Luft", "und", "Him\u00b7mel", "tr\u00fcbt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWS", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Der Natur das Leben giebt.", "tokens": ["Der", "Na\u00b7tur", "das", "Le\u00b7ben", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Alle Wetter m\u00f6gen rasen,", "tokens": ["Al\u00b7le", "Wet\u00b7ter", "m\u00f6\u00b7gen", "ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis sich Baum und Wurzel hebt,", "tokens": ["Bis", "sich", "Baum", "und", "Wur\u00b7zel", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "NE", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zwey und dreyssig Winde blasen,", "tokens": ["Zwey", "und", "dreys\u00b7sig", "Win\u00b7de", "bla\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "KON", "CARD", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df des Himmels Veste bebt!", "tokens": ["Da\u00df", "des", "Him\u00b7mels", "Ves\u00b7te", "bebt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fallen Wasser auf uns her,", "tokens": ["Fal\u00b7len", "Was\u00b7ser", "auf", "uns", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "K\u00f6mmt doch keine S\u00fcndfluth mehr.", "tokens": ["K\u00f6mmt", "doch", "kei\u00b7ne", "S\u00fcnd\u00b7fluth", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jetzt erscheint der Regenbogen", "tokens": ["Jetzt", "er\u00b7scheint", "der", "Re\u00b7gen\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit der Farben bunter Pracht;", "tokens": ["Mit", "der", "Far\u00b7ben", "bun\u00b7ter", "Pracht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Diesen Strich hat GOtt gezogen,", "tokens": ["Die\u00b7sen", "Strich", "hat", "Gott", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als Er einen Bund gemacht,", "tokens": ["Als", "Er", "ei\u00b7nen", "Bund", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df kein langer Wolken-Gu\u00df", "tokens": ["Da\u00df", "kein", "lan\u00b7ger", "Wol\u00b7ken\u00b7Gu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Alles Fleisch mehr t\u00f6dten mu\u00df.", "tokens": ["Al\u00b7les", "Fleisch", "mehr", "t\u00f6d\u00b7ten", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ey! wie blick ich voll Vergn\u00fcgen", "tokens": ["Ey", "!", "wie", "blick", "ich", "voll", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PWAV", "VVFIN", "PPER", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses Gnaden-Zeichen an!", "tokens": ["Die\u00b7ses", "Gna\u00b7den\u00b7Zei\u00b7chen", "an", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil des H\u00f6chsten Mund nicht tr\u00fcgen,", "tokens": ["Weil", "des", "H\u00f6chs\u00b7ten", "Mund", "nicht", "tr\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mich hintergehen kann;", "tokens": ["Und", "mich", "hin\u00b7ter\u00b7ge\u00b7hen", "kann", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Er gedenkt an seinen Bund,", "tokens": ["Er", "ge\u00b7denkt", "an", "sei\u00b7nen", "Bund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dieses Zeichen macht es kund.", "tokens": ["Die\u00b7ses", "Zei\u00b7chen", "macht", "es", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sch\u00f6pfer! Wie bist Du zu preisen,", "tokens": ["Sch\u00f6p\u00b7fer", "!", "Wie", "bist", "Du", "zu", "prei\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "VAFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df Du so die Menschen liebst!", "tokens": ["Da\u00df", "Du", "so", "die", "Men\u00b7schen", "liebst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und, um deine Huld zu weisen,", "tokens": ["Und", ",", "um", "dei\u00b7ne", "Huld", "zu", "wei\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Diesen sch\u00f6nen Bogen giebst,", "tokens": ["Die\u00b7sen", "sch\u00f6\u00b7nen", "Bo\u00b7gen", "giebst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der die ganze Welt belehrt,", "tokens": ["Der", "die", "gan\u00b7ze", "Welt", "be\u00b7lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df die S\u00fcndfluth aufgeh\u00f6rt.", "tokens": ["Da\u00df", "die", "S\u00fcnd\u00b7fluth", "auf\u00b7ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "O! ich falle Dir zu F\u00fcssen,", "tokens": ["O", "!", "ich", "fal\u00b7le", "Dir", "zu", "F\u00fcs\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wesen voller Majest\u00e4t!", "tokens": ["We\u00b7sen", "vol\u00b7ler", "Ma\u00b7jes\u00b7t\u00e4t", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil Du mich dadurch l\u00e4\u00dfst wissen,", "tokens": ["Weil", "Du", "mich", "da\u00b7durch", "l\u00e4\u00df\u00b7st", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PAV", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df dein Zorn f\u00fcr\u00fcber geht,", "tokens": ["Da\u00df", "dein", "Zorn", "f\u00fc\u00b7r\u00fc\u00b7ber", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und mein Herz versichert ist,", "tokens": ["Und", "mein", "Herz", "ver\u00b7si\u00b7chert", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df Du lieber gn\u00e4dig bist.", "tokens": ["Da\u00df", "Du", "lie\u00b7ber", "gn\u00e4\u00b7dig", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ach! ich danke deinem Nahmen,", "tokens": ["Ach", "!", "ich", "dan\u00b7ke", "dei\u00b7nem", "Nah\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "G\u00fctigste Allgegenwart!", "tokens": ["G\u00fc\u00b7tigs\u00b7te", "All\u00b7ge\u00b7gen\u00b7wart", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Da\u00df Du mich aus Adams Saamen", "tokens": ["Da\u00df", "Du", "mich", "aus", "A\u00b7dams", "Saa\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr dem Untergang gespart,", "tokens": ["F\u00fcr", "dem", "Un\u00b7ter\u00b7gang", "ge\u00b7spart", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und, da ich viel Schuld geh\u00e4uft,", "tokens": ["Und", ",", "da", "ich", "viel", "Schuld", "ge\u00b7h\u00e4uft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PIAT", "NN", "VVPP", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.6": {"text": "Durch die S\u00fcndfluth nicht ers\u00e4uft.", "tokens": ["Durch", "die", "S\u00fcnd\u00b7fluth", "nicht", "er\u00b7s\u00e4uft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Wenn das Wasser meiner S\u00fcnden", "tokens": ["Wenn", "das", "Was\u00b7ser", "mei\u00b7ner", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir bis an die Seele trit,", "tokens": ["Mir", "bis", "an", "die", "See\u00b7le", "trit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Will der Hoffnung Trost verschwinden,", "tokens": ["Will", "der", "Hoff\u00b7nung", "Trost", "ver\u00b7schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So ergetze mich damit,", "tokens": ["So", "er\u00b7get\u00b7ze", "mich", "da\u00b7mit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und la\u00df deiner Gnaden Schein", "tokens": ["Und", "la\u00df", "dei\u00b7ner", "Gna\u00b7den", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPOSAT", "NN", "NN"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.6": {"text": "Einen Regenbogen seyn.", "tokens": ["Ei\u00b7nen", "Re\u00b7gen\u00b7bo\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Menschenfreund! der alle B\u00e4che", "tokens": ["Men\u00b7schen\u00b7freund", "!", "der", "al\u00b7le", "B\u00e4\u00b7che"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "ART", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Belials mit Blut bespritzt,", "tokens": ["Be\u00b7li\u00b7als", "mit", "Blut", "be\u00b7spritzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVPP", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Welcher bey der gr\u00f6\u00dften Schw\u00e4che", "tokens": ["Wel\u00b7cher", "bey", "der", "gr\u00f6\u00df\u00b7ten", "Schw\u00e4\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAT", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Grosse Felsen aufgeritzt,", "tokens": ["Gros\u00b7se", "Fel\u00b7sen", "auf\u00b7ge\u00b7ritzt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deine Seite kommet mir", "tokens": ["Dei\u00b7ne", "Sei\u00b7te", "kom\u00b7met", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie des Noah Kasten f\u00fcr.", "tokens": ["Wie", "des", "Noah", "Kas\u00b7ten", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NE", "NN", "APPR", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Heyland! wenn die frechen S\u00fcnder", "tokens": ["Hey\u00b7land", "!", "wenn", "die", "fre\u00b7chen", "S\u00fcn\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deines Zornes Rechte straft,", "tokens": ["Dei\u00b7nes", "Zor\u00b7nes", "Rech\u00b7te", "straft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die b\u00f6sen Menschenkinder", "tokens": ["Und", "die", "b\u00f6\u00b7sen", "Men\u00b7schen\u00b7kin\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu den Schwefelb\u00e4chen rafft,", "tokens": ["Zu", "den", "Schwe\u00b7fel\u00b7b\u00e4\u00b7chen", "rafft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Soll der Tauben Augenschein", "tokens": ["Soll", "der", "Tau\u00b7ben", "Au\u00b7gen\u00b7schein"], "token_info": ["word", "word", "word", "w