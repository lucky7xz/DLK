{"dta.poem.7994": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die sterbenden Helden .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "O Vater! da\u00df mich in der Jugend Kraft", "tokens": ["O", "Va\u00b7ter", "!", "da\u00df", "mich", "in", "der", "Ju\u00b7gend", "Kraft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "KOUS", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Norne rafft!", "tokens": ["Die", "Nor\u00b7ne", "rafft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Noch leuchtet keiner hohen Thaten Bild", "tokens": ["Noch", "leuch\u00b7tet", "kei\u00b7ner", "ho\u00b7hen", "Tha\u00b7ten", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf meinem Schild.", "tokens": ["Auf", "mei\u00b7nem", "Schild", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Zw\u00f6lf Richter thronen, hoch und schauerlich,", "tokens": ["Zw\u00f6lf", "Rich\u00b7ter", "thro\u00b7nen", ",", "hoch", "und", "schau\u00b7er\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die werthen nicht des Heldenmahles mich.", "tokens": ["Die", "wert\u00b7hen", "nicht", "des", "Hel\u00b7den\u00b7mah\u00b7les", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}