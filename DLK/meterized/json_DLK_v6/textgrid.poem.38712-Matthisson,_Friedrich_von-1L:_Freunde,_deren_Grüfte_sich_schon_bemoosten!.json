{"textgrid.poem.38712": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Freunde, deren Gr\u00fcfte sich schon bemoosten!", "genre": "verse", "period": "N.A.", "pub_year": 1797, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freunde, deren Gr\u00fcfte sich schon bemoosten!", "tokens": ["Freun\u00b7de", ",", "de\u00b7ren", "Gr\u00fcf\u00b7te", "sich", "schon", "be\u00b7moos\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELAT", "NN", "PRF", "ADV", "VVINF", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Wann der Vollmond \u00fcber dem Walde d\u00e4mmert,", "tokens": ["Wann", "der", "Voll\u00b7mond", "\u00fc\u00b7ber", "dem", "Wal\u00b7de", "d\u00e4m\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Schweben eure Schatten empor vom stillen", "tokens": ["Schwe\u00b7ben", "eu\u00b7re", "Schat\u00b7ten", "em\u00b7por", "vom", "stil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "APPRART", "ADJA"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Ufer der Lethe.", "tokens": ["U\u00b7fer", "der", "Le\u00b7the", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Seid mir, Unverge\u00dfliche, froh gesegnet!", "tokens": ["Seid", "mir", ",", "Un\u00b7ver\u00b7ge\u00df\u00b7li\u00b7che", ",", "froh", "ge\u00b7seg\u00b7net", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "$,", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Du vor allen, welcher im Buch der Menschheit", "tokens": ["Du", "vor", "al\u00b7len", ",", "wel\u00b7cher", "im", "Buch", "der", "Menschheit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "PIAT", "$,", "PRELS", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Mir der Hieroglyfen so viel gedeutet,", "tokens": ["Mir", "der", "Hie\u00b7ro\u00b7gly\u00b7fen", "so", "viel", "ge\u00b7deu\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Redlicher Bonnet.", "tokens": ["Red\u00b7li\u00b7cher", "Bon\u00b7net", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "L\u00e4ngst verschl\u00fcrft im Strudel der Brandung w\u00e4re", "tokens": ["L\u00e4ngst", "ver\u00b7schl\u00fcrft", "im", "Stru\u00b7del", "der", "Bran\u00b7dung", "w\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "VAFIN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wohl mein Fahrzeug oder am Riff zerschmettert,", "tokens": ["Wohl", "mein", "Fahr\u00b7zeug", "o\u00b7der", "am", "Riff", "zer\u00b7schmet\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "KON", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "H\u00e4ttet ihr nicht, Genien gleich, im Sturme", "tokens": ["H\u00e4t\u00b7tet", "ihr", "nicht", ",", "Ge\u00b7ni\u00b7en", "gleich", ",", "im", "Stur\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "NN", "ADV", "$,", "APPRART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Schirmend gewaltet.", "tokens": ["Schir\u00b7mend", "ge\u00b7wal\u00b7tet", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Wiedersehn der Liebenden! wo der Heimath", "tokens": ["Wie\u00b7der\u00b7sehn", "der", "Lie\u00b7ben\u00b7den", "!", "wo", "der", "Hei\u00b7math"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "$.", "PWAV", "ART", "NN"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Goldne Sterne leuchten, o du der armen", "tokens": ["Gold\u00b7ne", "Ster\u00b7ne", "leuch\u00b7ten", ",", "o", "du", "der", "ar\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "$,", "FM", "PPER", "ART", "ADJA"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Psyche, die gebunden im Grabthal schmachtet,", "tokens": ["Psy\u00b7che", ",", "die", "ge\u00b7bun\u00b7den", "im", "Grab\u00b7thal", "schmach\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VVPP", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Heiligste Sehnsucht.", "tokens": ["Hei\u00b7ligs\u00b7te", "Sehn\u00b7sucht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}