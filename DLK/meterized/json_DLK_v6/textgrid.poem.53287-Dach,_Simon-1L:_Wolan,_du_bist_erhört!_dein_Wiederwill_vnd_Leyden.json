{"textgrid.poem.53287": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wolan, du bist erh\u00f6rt! dein Wiederwill vnd Leyden", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wolan, du bist erh\u00f6rt! dein Wiederwill vnd Leyden", "tokens": ["Wo\u00b7lan", ",", "du", "bist", "er\u00b7h\u00f6rt", "!", "dein", "Wie\u00b7der\u00b7will", "vnd", "Ley\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PPER", "VAFIN", "VVPP", "$.", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist, wehrte Seele, noch zuletzt von Gott erkant:", "tokens": ["Ist", ",", "wehr\u00b7te", "See\u00b7le", ",", "noch", "zu\u00b7letzt", "von", "Gott", "er\u00b7kant", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "ADJA", "NN", "$,", "ADV", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er hat in Seeligheit dein Elend vmbgewandt,", "tokens": ["Er", "hat", "in", "See\u00b7lig\u00b7heit", "dein", "E\u00b7lend", "vmb\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd deine Qual vertauscht mit Rhue vnd s\u00fcssen Frewden.", "tokens": ["Vnd", "dei\u00b7ne", "Qual", "ver\u00b7tauscht", "mit", "Rhue", "vnd", "s\u00fcs\u00b7sen", "Frew\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "APPR", "NE", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wo aber war doch ich, als du nun woltest scheiden?", "tokens": ["Wo", "a\u00b7ber", "war", "doch", "ich", ",", "als", "du", "nun", "wol\u00b7test", "schei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "ADV", "PPER", "$,", "KOUS", "PPER", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum minsten h\u00e4tte dir, o Mutter, meine Handt", "tokens": ["Zum", "mins\u00b7ten", "h\u00e4t\u00b7te", "dir", ",", "o", "Mut\u00b7ter", ",", "mei\u00b7ne", "Handt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "VAFIN", "PPER", "$,", "FM", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Augen zugedruckt. Ach, da\u00df ich gar kein Pfandt", "tokens": ["Die", "Au\u00b7gen", "zu\u00b7ge\u00b7druckt", ".", "Ach", ",", "da\u00df", "ich", "gar", "kein", "Pfandt"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$.", "ITJ", "$,", "KOUS", "PPER", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Liebe lassen sehn den Vndanck zu vermeiden", "tokens": ["Der", "Lie\u00b7be", "las\u00b7sen", "sehn", "den", "Vn\u00b7danck", "zu", "ver\u00b7mei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "VVFIN", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "F\u00fcr alle Lieb vnd Trew! Ihr Gr\u00e4ber, z\u00fcrnt mit mir,", "tokens": ["F\u00fcr", "al\u00b7le", "Lieb", "vnd", "Trew", "!", "Ihr", "Gr\u00e4\u00b7ber", ",", "z\u00fcrnt", "mit", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$.", "PPOSAT", "NN", "$,", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd du, o Himmel auch! vergebens habet jhr", "tokens": ["Vnd", "du", ",", "o", "Him\u00b7mel", "auch", "!", "ver\u00b7ge\u00b7bens", "ha\u00b7bet", "jhr"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "FM", "NN", "ADV", "$.", "ADV", "VAFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hie Leib vnd Geist getrennt! es mag euch jmmer schmertzen,", "tokens": ["Hie", "Leib", "vnd", "Geist", "ge\u00b7trennt", "!", "es", "mag", "euch", "jm\u00b7mer", "schmert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVPP", "$.", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ich kan euch nichts gestehn; der grossen Gutthat Macht", "tokens": ["Ich", "kan", "euch", "nichts", "ge\u00b7stehn", ";", "der", "gros\u00b7sen", "Gut\u00b7that", "Macht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "VVPP", "$.", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat dieses Mensch so tieff mir in den Sinn gebracht,", "tokens": ["Hat", "die\u00b7ses", "Mensch", "so", "tieff", "mir", "in", "den", "Sinn", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "ADV", "ADJD", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df ich sie nimmermehr euch la\u00df aus meinem Hertzen.", "tokens": ["Da\u00df", "ich", "sie", "nim\u00b7mer\u00b7mehr", "euch", "la\u00df", "aus", "mei\u00b7nem", "Hert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}