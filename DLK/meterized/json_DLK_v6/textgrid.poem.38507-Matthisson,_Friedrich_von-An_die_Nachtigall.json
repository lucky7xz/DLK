{"textgrid.poem.38507": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An die Nachtigall", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vogel der Liebe! t\u00f6ne, wann die Rosen", "tokens": ["Vo\u00b7gel", "der", "Lie\u00b7be", "!", "t\u00f6\u00b7ne", ",", "wann", "die", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "ART", "NN", "$.", "VVFIN", "$,", "PWAV", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Auf der Wange des Abendhimmels sterben,", "tokens": ["Auf", "der", "Wan\u00b7ge", "des", "A\u00b7bend\u00b7him\u00b7mels", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Traumges\u00e4nge nieder auf das bethr\u00e4nte", "tokens": ["Traum\u00b7ge\u00b7s\u00e4n\u00b7ge", "nie\u00b7der", "auf", "das", "be\u00b7thr\u00e4n\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "APPR", "ART", "ADJA"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Lager Selindens!", "tokens": ["La\u00b7ger", "Se\u00b7lin\u00b7dens", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Da\u00df seiner Ruh' nur einen linden Tropfen,", "tokens": ["Da\u00df", "sei\u00b7ner", "Ruh'", "nur", "ei\u00b7nen", "lin\u00b7den", "Trop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus der silbernen Schlummerschale, Morpheus,", "tokens": ["Aus", "der", "sil\u00b7ber\u00b7nen", "Schlum\u00b7mer\u00b7scha\u00b7le", ",", "Mor\u00b7pheus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "NE", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Auf den einsamweinenden Blik voll Trauer,", "tokens": ["Auf", "den", "ein\u00b7sam\u00b7wei\u00b7nen\u00b7den", "Blik", "voll", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Labend ihr tr\u00e4ufle!", "tokens": ["La\u00b7bend", "ihr", "tr\u00e4uf\u00b7le", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}