{"textgrid.poem.41740": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sinnend stand ich, traumverloren", "tokens": ["Sin\u00b7nend", "stand", "ich", ",", "traum\u00b7ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor dem kleinen Altar in der Kapelle.", "tokens": ["Vor", "dem", "klei\u00b7nen", "Al\u00b7tar", "in", "der", "Ka\u00b7pel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Schwarze Gewitterwolken waren aufgezogen", "tokens": ["Schwar\u00b7ze", "Ge\u00b7wit\u00b7ter\u00b7wol\u00b7ken", "wa\u00b7ren", "auf\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VAFIN", "VVPP"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "S\u00fcdlich am Himmel.", "tokens": ["S\u00fcd\u00b7lich", "am", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Mitten in die purpurne Abendr\u00f6te", "tokens": ["Mit\u00b7ten", "in", "die", "pur\u00b7pur\u00b7ne", "A\u00b7bend\u00b7r\u00f6\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-++++-+-+-", "measure": "unknown.measure.septa"}, "line.6": {"text": "Zuckten goldene Blitze flammend in Siegesgewi\u00dfheit,", "tokens": ["Zuck\u00b7ten", "gol\u00b7de\u00b7ne", "Blit\u00b7ze", "flam\u00b7mend", "in", "Sie\u00b7ges\u00b7ge\u00b7wi\u00df\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVPP", "APPR", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.7": {"text": "Und dennoch schnell verschwindend \u2013", "tokens": ["Und", "den\u00b7noch", "schnell", "ver\u00b7schwin\u00b7dend", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Also zuckte durch meine Seele,", "tokens": ["Al\u00b7so", "zuck\u00b7te", "durch", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Blitzend ein Gedanke", "tokens": ["Blit\u00b7zend", "ein", "Ge\u00b7dan\u00b7ke"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Eine Ged\u00e4chtnistafel ", "tokens": ["Ei\u00b7ne", "Ge\u00b7d\u00e4cht\u00b7nis\u00b7ta\u00b7fel"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.11": {"text": "In meinem Innern enth\u00fcllend.", "tokens": ["In", "mei\u00b7nem", "In\u00b7nern", "ent\u00b7h\u00fcl\u00b7lend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Und auf der Tafel stand mit leuchtender Schrift:", "tokens": ["Und", "auf", "der", "Ta\u00b7fel", "stand", "mit", "leuch\u00b7ten\u00b7der", "Schrift", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Und ich stand vor dem Altar", "tokens": ["Und", "ich", "stand", "vor", "dem", "Al\u00b7tar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor dem damals die deutsche Jugend", "tokens": ["Vor", "dem", "da\u00b7mals", "die", "deut\u00b7sche", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "ART", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Siegesmutig gestanden,", "tokens": ["Sie\u00b7ges\u00b7mu\u00b7tig", "ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "In allgemeiner Liebesverbr\u00fcderung", "tokens": ["In", "all\u00b7ge\u00b7mei\u00b7ner", "Lie\u00b7bes\u00b7ver\u00b7br\u00fc\u00b7de\u00b7rung"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Sich die H\u00e4nde gereicht und das vaterl\u00e4ndische B\u00fcndnis", "tokens": ["Sich", "die", "H\u00e4n\u00b7de", "ge\u00b7reicht", "und", "das", "va\u00b7ter\u00b7l\u00e4n\u00b7di\u00b7sche", "B\u00fcnd\u00b7nis"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ART", "NN", "VVPP", "KON", "ART", "ADJA", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Auf die Hostie feierlich beschworen.", "tokens": ["Auf", "die", "Hos\u00b7tie", "fei\u00b7er\u00b7lich", "be\u00b7schwo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Und ich stand vor dem Altar", "tokens": ["Und", "ich", "stand", "vor", "dem", "Al\u00b7tar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Thr\u00e4nenden Auges!", "tokens": ["Thr\u00e4\u00b7nen\u00b7den", "Au\u00b7ges", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und doch f\u00fchlt ich wie sie, wie die hoffende Jugend,", "tokens": ["Und", "doch", "f\u00fchlt", "ich", "wie", "sie", ",", "wie", "die", "hof\u00b7fen\u00b7de", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PWAV", "PPER", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Jugendkraft in den Adern", "tokens": ["Ju\u00b7gend\u00b7kraft", "in", "den", "A\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Freiheitsglut \u2013 Todesmut", "tokens": ["Frei\u00b7heits\u00b7glut", "\u2013", "To\u00b7des\u00b7mut"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$(", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "F\u00fcr die heilige Sache des Vaterlands! \u2013", "tokens": ["F\u00fcr", "die", "hei\u00b7li\u00b7ge", "Sa\u00b7che", "des", "Va\u00b7ter\u00b7lands", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$.", "$("], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Aber ich stand und weinte.", "tokens": ["A\u00b7ber", "ich", "stand", "und", "wein\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Auch das mutige Aufjauchzen", "tokens": ["Auch", "das", "mu\u00b7ti\u00b7ge", "Auf\u00b7jauch\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus dem Herzen der deutschen Jugend", "tokens": ["Aus", "dem", "Her\u00b7zen", "der", "deut\u00b7schen", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Durfte nicht frei in die Lande dringen", "tokens": ["Durf\u00b7te", "nicht", "frei", "in", "die", "Lan\u00b7de", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PTKNEG", "ADJD", "APPR", "ART", "NN", "VVINF"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Durfte es damals nicht \u2013 darf es auch heute nicht \u2013", "tokens": ["Durf\u00b7te", "es", "da\u00b7mals", "nicht", "\u2013", "darf", "es", "auch", "heu\u00b7te", "nicht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "$(", "VMFIN", "PPER", "ADV", "ADV", "PTKNEG", "$("], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.5": {"text": "Denn es will mich bed\u00fcnken:", "tokens": ["Denn", "es", "will", "mich", "be\u00b7d\u00fcn\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PRF", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Als habe der Argwohn selbst eine Burg erbaut", "tokens": ["Als", "ha\u00b7be", "der", "Arg\u00b7wohn", "selbst", "ei\u00b7ne", "Burg", "er\u00b7baut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "ART", "NN", "ADV", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mitten im deutschen Land \u2013 auch eine Wartburg!", "tokens": ["Mit\u00b7ten", "im", "deut\u00b7schen", "Land", "\u2013", "auch", "ei\u00b7ne", "Wart\u00b7burg", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$(", "ADV", "ART", "NN", "$."], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}}}}}