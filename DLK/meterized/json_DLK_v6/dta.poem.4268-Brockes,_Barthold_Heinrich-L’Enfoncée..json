{"dta.poem.4268": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "L\u2019Enfonc\u00e9e.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In einer von Natur rund- ausgeh\u00f6hlten Tiefe,", "tokens": ["In", "ei\u00b7ner", "von", "Na\u00b7tur", "run\u00b7d", "aus\u00b7ge\u00b7h\u00f6hl\u00b7ten", "Tie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "NN", "TRUNC", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wo rings umher ein Kreis von B\u00e4umen stand,", "tokens": ["Wo", "rings", "um\u00b7her", "ein", "Kreis", "von", "B\u00e4u\u00b7men", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PTKVZ", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Schien\u2019 recht, da ich die sch\u00f6ne Stelle fand,", "tokens": ["Schien'", "recht", ",", "da", "ich", "die", "sch\u00f6\u00b7ne", "Stel\u00b7le", "fand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als ob sie mir mit stillen Worten riefe,", "tokens": ["Als", "ob", "sie", "mir", "mit", "stil\u00b7len", "Wor\u00b7ten", "rie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und spr\u00e4ch\u2019: ", "tokens": ["Und", "spr\u00e4ch'", ":"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Von einer Lauben auch.", "tokens": ["Von", "ei\u00b7ner", "Lau\u00b7ben", "auch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Worauf, wie ich den Ort mit Blicken \u00fcberliefe,", "tokens": ["Wo\u00b7rauf", ",", "wie", "ich", "den", "Ort", "mit", "Bli\u00b7cken", "\u00fc\u00b7berl\u00b7ie\u00b7fe", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PWAV", "PPER", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und da\u00df er lustig l\u00e4g\u2019, erwog und \u00fcberdachte,", "tokens": ["Und", "da\u00df", "er", "lus\u00b7tig", "l\u00e4g'", ",", "er\u00b7wog", "und", "\u00fc\u00b7berd\u00b7ach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Auch Caprifolium in grosser Menge funde;", "tokens": ["Auch", "Cap\u00b7ri\u00b7fo\u00b7li\u00b7um", "in", "gros\u00b7ser", "Men\u00b7ge", "fun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich, mit geringer M\u00fch\u2019, und schon in einer Stunde,", "tokens": ["Ich", ",", "mit", "ge\u00b7rin\u00b7ger", "M\u00fch'", ",", "und", "schon", "in", "ei\u00b7ner", "Stun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "ADJA", "NN", "$,", "KON", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Auch eine Laube hier zum Stande brachte.", "tokens": ["Auch", "ei\u00b7ne", "Lau\u00b7be", "hier", "zum", "Stan\u00b7de", "brach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Wie nun die Waldung hier", "tokens": ["Wie", "nun", "die", "Wal\u00b7dung", "hier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Am allerdicksten schier", "tokens": ["Am", "al\u00b7ler\u00b7dicks\u00b7ten", "schier"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Und ganz verwachsen ist, lie\u00df ich drey Steige graben,", "tokens": ["Und", "ganz", "ver\u00b7wach\u00b7sen", "ist", ",", "lie\u00df", "ich", "drey", "Stei\u00b7ge", "gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VAFIN", "$,", "VVFIN", "PPER", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Wodurch wir zu dem Ort so manchen Zugang haben,", "tokens": ["Wo\u00b7durch", "wir", "zu", "dem", "Ort", "so", "man\u00b7chen", "Zu\u00b7gang", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "ADV", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die alle lustig sind. Die\u00df ist ein Ort, wo man", "tokens": ["Die", "al\u00b7le", "lus\u00b7tig", "sind", ".", "Die\u00df", "ist", "ein", "Ort", ",", "wo", "man"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PIS", "ADJD", "VAFIN", "$.", "PDS", "VAFIN", "ART", "NN", "$,", "PWAV", "PIS"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Nicht weniger als sonst des Sch\u00f6pfers Ruhm vermehren,", "tokens": ["Nicht", "we\u00b7ni\u00b7ger", "als", "sonst", "des", "Sch\u00f6p\u00b7fers", "Ruhm", "ver\u00b7meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "KOKOM", "ADV", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Jhn, in Bewunderung der sch\u00f6nen Welt, verehren,", "tokens": ["Jhn", ",", "in", "Be\u00b7wun\u00b7de\u00b7rung", "der", "sch\u00f6\u00b7nen", "Welt", ",", "ver\u00b7eh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "APPR", "NN", "ART", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.19": {"text": "Jhn preisen und Jhm danken kann.", "tokens": ["Jhn", "prei\u00b7sen", "und", "Jhm", "dan\u00b7ken", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}