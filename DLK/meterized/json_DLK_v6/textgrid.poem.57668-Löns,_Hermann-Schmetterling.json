{"textgrid.poem.57668": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Schmetterling", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In blauen Streifen f\u00e4llt das Licht", "tokens": ["In", "blau\u00b7en", "Strei\u00b7fen", "f\u00e4llt", "das", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In unsern stillen Wald,", "tokens": ["In", "un\u00b7sern", "stil\u00b7len", "Wald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Schwarzspechts heller Glockenton", "tokens": ["Des", "Schwarz\u00b7spechts", "hel\u00b7ler", "Glo\u00b7cken\u00b7ton"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu uns her\u00fcberschallt.", "tokens": ["Zu", "uns", "her\u00b7\u00fc\u00b7bersc\u00b7hallt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich gehe den Weg mit leichtem Schritt,", "tokens": ["Ich", "ge\u00b7he", "den", "Weg", "mit", "leich\u00b7tem", "Schritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Einst ging ich ihn m\u00fcde und schwer,", "tokens": ["Einst", "ging", "ich", "ihn", "m\u00fc\u00b7de", "und", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ein gro\u00dfer schwarzer Schmetterling", "tokens": ["Ein", "gro\u00b7\u00dfer", "schwar\u00b7zer", "Schmet\u00b7ter\u00b7ling"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Flog lockend vor mir her.", "tokens": ["Flog", "lo\u00b7ckend", "vor", "mir", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mit seligen Augen streif ich dein Haar,", "tokens": ["Mit", "se\u00b7li\u00b7gen", "Au\u00b7gen", "streif", "ich", "dein", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "----+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Das sich im Nacken rollt,", "tokens": ["Das", "sich", "im", "Na\u00b7cken", "rollt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Schmetterling, der vor uns schwebt,", "tokens": ["Der", "Schmet\u00b7ter\u00b7ling", ",", "der", "vor", "uns", "schwebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der ist so gelb wie Gold.", "tokens": ["Der", "ist", "so", "gelb", "wie", "Gold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}