{"textgrid.poem.64967": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "56.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gef\u00e4hrlich hei\u00dft die\u00df Buch, weil leicht es zu mi\u00dfdeuten \u2013", "tokens": ["Ge\u00b7f\u00e4hr\u00b7lich", "hei\u00dft", "die\u00df", "Buch", ",", "weil", "leicht", "es", "zu", "mi\u00df\u00b7deu\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PDS", "NN", "$,", "KOUS", "ADJD", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wohl! so verwahrt es denn vor unberufnen Leuten.", "tokens": ["Wohl", "!", "so", "ver\u00b7wahrt", "es", "denn", "vor", "un\u00b7be\u00b7ruf\u00b7nen", "Leu\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Allein den Dichter selbst verschont mit Bann und Acht!", "tokens": ["Al\u00b7lein", "den", "Dich\u00b7ter", "selbst", "ver\u00b7schont", "mit", "Bann", "und", "Acht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "VVPP", "APPR", "NN", "KON", "CARD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er hat an Kluge nur, an Thoren nicht gedacht.", "tokens": ["Er", "hat", "an", "Klu\u00b7ge", "nur", ",", "an", "Tho\u00b7ren", "nicht", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ADV", "$,", "APPR", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}