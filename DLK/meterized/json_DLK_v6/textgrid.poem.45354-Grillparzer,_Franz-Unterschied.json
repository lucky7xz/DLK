{"textgrid.poem.45354": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Unterschied", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr seid Minister,", "tokens": ["Ihr", "seid", "Mi\u00b7nis\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Ihr ministriert,", "tokens": ["Ihr", "mi\u00b7nist\u00b7riert", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ich bin ein Priester,", "tokens": ["Ich", "bin", "ein", "Pries\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Der konsekriert.", "tokens": ["Der", "kon\u00b7sek\u00b7riert", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}