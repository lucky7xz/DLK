{"dta.poem.17563": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "185.  Der Weise weil er nichts hat ver-  \n liehrt nichts.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Der weise Mann ist nie umb einen Heller kommen:", "tokens": ["Der", "wei\u00b7se", "Mann", "ist", "nie", "umb", "ei\u00b7nen", "Hel\u00b7ler", "kom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er hat nie nichts gehabt/ man hat ihm nichts genom\u0304en.", "tokens": ["Er", "hat", "nie", "nichts", "ge\u00b7habt", "/", "man", "hat", "ihm", "nichts", "ge\u00b7nom\u0304en", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "VAPP", "$(", "PIS", "VAFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}