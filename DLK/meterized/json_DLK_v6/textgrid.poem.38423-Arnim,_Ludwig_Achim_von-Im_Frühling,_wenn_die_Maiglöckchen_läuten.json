{"textgrid.poem.38423": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Im Fr\u00fchling, wenn die Maigl\u00f6ckchen l\u00e4uten", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kling, kling Gl\u00f6ckchen,", "tokens": ["Kling", ",", "kling", "Gl\u00f6ck\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Im Haus steht ein D\u00f6ckchen,", "tokens": ["Im", "Haus", "steht", "ein", "D\u00f6ck\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Im Garten steht ein H\u00fcnernest,", "tokens": ["Im", "Gar\u00b7ten", "steht", "ein", "H\u00fc\u00b7ner\u00b7nest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Stehn drei seidne D\u00f6ckchen drin,", "tokens": ["Stehn", "drei", "seid\u00b7ne", "D\u00f6ck\u00b7chen", "drin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "CARD", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Eins spinnt Seiden,", "tokens": ["Eins", "spinnt", "Sei\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NE", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Eins flicht Weiden,", "tokens": ["Eins", "flicht", "Wei\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Eins schlie\u00dft den Himmel auf,", "tokens": ["Eins", "schlie\u00dft", "den", "Him\u00b7mel", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "L\u00e4\u00dft ein bischen Sonn heraus,", "tokens": ["L\u00e4\u00dft", "ein", "bi\u00b7schen", "Sonn", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "L\u00e4\u00dft ein bischen drinn,", "tokens": ["L\u00e4\u00dft", "ein", "bi\u00b7schen", "drinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADV", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Daraus die Liebfrau Maria spinn,", "tokens": ["Da\u00b7raus", "die", "Lieb\u00b7frau", "Ma\u00b7ria", "spinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ein R\u00f6cklein f\u00fcr ihr Kindelein.", "tokens": ["Ein", "R\u00f6ck\u00b7lein", "f\u00fcr", "ihr", "Kin\u00b7de\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}