Ich", "hab'", "sie", "in", "den", "Arm", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So konnt' sie nicht abhanden kommen.", "tokens": ["So", "konnt'", "sie", "nicht", "ab\u00b7han\u00b7den", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.49": {"line.1": {"text": "Verf\u00fchrend lockte auch das Meer,", "tokens": ["Ver\u00b7f\u00fch\u00b7rend", "lock\u00b7te", "auch", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Warf sich ihr stets zu F\u00fc\u00dfen her,", "tokens": ["Warf", "sich", "ihr", "stets", "zu", "F\u00fc\u00b7\u00dfen", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "PPER", "ADV", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.50": {"line.1": {"text": "Es scharrte n\u00e4chtlich um das Haus,", "tokens": ["Es", "scharr\u00b7te", "n\u00e4cht\u00b7lich", "um", "das", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ungeduldig sah es aus;", "tokens": ["Und", "un\u00b7ge\u00b7dul\u00b7dig", "sah", "es", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.51": {"line.1": {"text": "Und wenn selbst gute Leute schliefe