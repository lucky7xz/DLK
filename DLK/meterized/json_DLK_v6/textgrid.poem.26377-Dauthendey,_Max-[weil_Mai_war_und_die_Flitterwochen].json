{"textgrid.poem.26377": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[weil Mai war und die Flitterwochen]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil Mai war und die Flitterwochen,", "tokens": ["Weil", "Mai", "war", "und", "die", "Flit\u00b7ter\u00b7wo\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Waren die Blumen ausgekrochen.", "tokens": ["Wa\u00b7ren", "die", "Blu\u00b7men", "aus\u00b7ge\u00b7kro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVIZU", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Wir hielten uns mit vielen H\u00e4nden", "tokens": ["Wir", "hiel\u00b7ten", "uns", "mit", "vie\u00b7len", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ruhten an den Efeuw\u00e4nden", "tokens": ["Und", "ruh\u00b7ten", "an", "den", "E\u00b7feu\u00b7w\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf einem alten Schlo\u00df am Meer,", "tokens": ["Auf", "ei\u00b7nem", "al\u00b7ten", "Schlo\u00df", "am", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die See kroch unterm Fenster her,", "tokens": ["Die", "See", "kroch", "un\u00b7term", "Fens\u00b7ter", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie schien mir wie ein glatter Saal,", "tokens": ["Sie", "schien", "mir", "wie", "ein", "glat\u00b7ter", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der spiegelnd sich zum Tanz empfahl,", "tokens": ["Der", "spie\u00b7gelnd", "sich", "zum", "Tanz", "emp\u00b7fahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Tr\u00e4ume taten sich dort drehn", "tokens": ["Die", "Tr\u00e4u\u00b7me", "ta\u00b7ten", "sich", "dort", "drehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und lie\u00dfen uns die Zukunft sehn.", "tokens": ["Und", "lie\u00b7\u00dfen", "uns", "die", "Zu\u00b7kunft", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wir bauten manches Kartenhaus", "tokens": ["Wir", "bau\u00b7ten", "man\u00b7ches", "Kar\u00b7ten\u00b7haus"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und suchten unsern Grabstein aus.", "tokens": ["Und", "such\u00b7ten", "un\u00b7sern", "Grab\u00b7stein", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Denn wo die Tage zuckern sind,", "tokens": ["Denn", "wo", "die", "Ta\u00b7ge", "zu\u00b7ckern", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Greift mancher nach dem Salz geschwind;", "tokens": ["Greift", "man\u00b7cher", "nach", "dem", "Salz", "ge\u00b7schwind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und schmeckt im Gl\u00fcck uns jeder Wein,", "tokens": ["Und", "schmeckt", "im", "Gl\u00fcck", "uns", "je\u00b7der", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So bildet man sich Ungl\u00fcck ein.", "tokens": ["So", "bil\u00b7det", "man", "sich", "Un\u00b7gl\u00fcck", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PRF", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Frau K\u00f6nigin, sie wollte haben,", "tokens": ["Frau", "K\u00f6\u00b7ni\u00b7gin", ",", "sie", "woll\u00b7te", "ha\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPER", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Man soll sie einst ins Meer begraben.", "tokens": ["Man", "soll", "sie", "einst", "ins", "Meer", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Dar\u00fcber taten oft wir streiten,", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "ta\u00b7ten", "oft", "wir", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Meer t\u00e4t mir den Tod verleiden,", "tokens": ["Das", "Meer", "t\u00e4t", "mir", "den", "Tod", "ver\u00b7lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Ich wollt' bei einem gro\u00dfen Stein", "tokens": ["Ich", "wollt'", "bei", "ei\u00b7nem", "gro\u00b7\u00dfen", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf einem Berg begraben sein.", "tokens": ["Auf", "ei\u00b7nem", "Berg", "be\u00b7gra\u00b7ben", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Da schwieg sie, und sie a\u00df nicht viel,", "tokens": ["Da", "schwieg", "sie", ",", "und", "sie", "a\u00df", "nicht", "viel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "PPER", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil ich mein Grab im Meer nicht will.", "tokens": ["Weil", "ich", "mein", "Grab", "im", "Meer", "nicht", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPRART", "NN", "PTKNEG", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Doch in der Nacht, da sprach sie leise,", "tokens": ["Doch", "in", "der", "Nacht", ",", "da", "sprach", "sie", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie wollte ganz nach meiner Weise", "tokens": ["Sie", "woll\u00b7te", "ganz", "nach", "mei\u00b7ner", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Ihr Grab auf meinem Berg bei mir,", "tokens": ["Ihr", "Grab", "auf", "mei\u00b7nem", "Berg", "bei", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu kalt sei es im Meere ihr.", "tokens": ["Zu", "kalt", "sei", "es", "im", "Mee\u00b7re", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "PPER", "APPRART", "NN", "PPER", "$."], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.15": {"line.1": {"text": "Efeu wuchs wild durchs ganze Haus,", "tokens": ["E\u00b7feu", "wuchs", "wild", "durchs", "gan\u00b7ze", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gr\u00fcn sahen alle S\u00e4le aus,", "tokens": ["Gr\u00fcn", "sa\u00b7hen", "al\u00b7le", "S\u00e4\u00b7le", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.16": {"line.1": {"text": "Meerm\u00f6wen schwebten um die Schwellen", "tokens": ["Meer\u00b7m\u00f6\u00b7wen", "schweb\u00b7ten", "um", "die", "Schwel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wie Ampeln vor den Liebeszellen.", "tokens": ["Wie", "Am\u00b7peln", "vor", "den", "Lie\u00b7bes\u00b7zel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Wohin man von den S\u00e4len sah,", "tokens": ["Wo\u00b7hin", "man", "von", "den", "S\u00e4\u00b7len", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "War stets das, was man w\u00fcnschte, da,", "tokens": ["War", "stets", "das", ",", "was", "man", "w\u00fcnschte", ",", "da", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "PDS", "$,", "PRELS", "PIS", "VVFIN", "$,", "KOUS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Stets waren wir zu zwein im Zimmer", "tokens": ["Stets", "wa\u00b7ren", "wir", "zu", "zwein", "im", "Zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PTKA", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und nahmen uns das Sch\u00f6nste immer.", "tokens": ["Und", "nah\u00b7men", "uns", "das", "Sch\u00f6ns\u00b7te", "im\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.19": {"line.1": {"text": "Wie in dem Himmel Wolken fliegen,", "tokens": ["Wie", "in", "dem", "Him\u00b7mel", "Wol\u00b7ken", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So tat das Schlo\u00df voll Kissen liegen.", "tokens": ["So", "tat", "das", "Schlo\u00df", "voll", "Kis\u00b7sen", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.20": {"line.1": {"text": "Auf ihnen ging die Sonn' nicht unter,", "tokens": ["Auf", "ih\u00b7nen", "ging", "die", "Sonn'", "nicht", "un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "PTKNEG", "APPR", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie gl\u00fchten Tag und Nacht gleich munter.", "tokens": ["Sie", "gl\u00fch\u00b7ten", "Tag", "und", "Nacht", "gleich", "mun\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.21": {"line.1": {"text": "Wer dort nach hundert Jahren ruht,", "tokens": ["Wer", "dort", "nach", "hun\u00b7dert", "Jah\u00b7ren", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der f\u00fchlt noch dieser Kissen Glut.", "tokens": ["Der", "f\u00fchlt", "noch", "die\u00b7ser", "Kis\u00b7sen", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.22": {"line.1": {"text": "Unerf\u00fcllt ging kein Wunsch vor\u00fcber,", "tokens": ["Un\u00b7er\u00b7f\u00fcllt", "ging", "kein", "Wunsch", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Man sprach: Zu w\u00fcnschen bleibt nichts \u00fcber.", "tokens": ["Man", "sprach", ":", "Zu", "w\u00fcn\u00b7schen", "bleibt", "nichts", "\u00fc\u00b7ber", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$.", "PTKZU", "VVINF", "VVFIN", "PIS", "APPR", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.23": {"line.1": {"text": "So lebten wir im Paradies,", "tokens": ["So", "leb\u00b7ten", "wir", "im", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo man in jeden Apfel bi\u00df,", "tokens": ["Wo", "man", "in", "je\u00b7den", "Ap\u00b7fel", "bi\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.24": {"line.1": {"text": "Bi\u00df in die gr\u00fcnen und die roten,", "tokens": ["Bi\u00df", "in", "die", "gr\u00fc\u00b7nen", "und", "die", "ro\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "KON", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht ", "tokens": ["Nicht"], "token_info": ["word"], "pos": ["PTKNEG"], "meter": "+", "measure": "single.up"}}, "stanza.25": {"line.1": {"text": "Der Fr\u00fchling sa\u00df an allen Wegen,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "sa\u00df", "an", "al\u00b7len", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Tat Blumen bunt und Eier legen,", "tokens": ["Tat", "Blu\u00b7men", "bunt", "und", "Ei\u00b7er", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJD", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.26": {"line.1": {"text": "Wir wurden mit den B\u00e4umen du", "tokens": ["Wir", "wur\u00b7den", "mit", "den", "B\u00e4u\u00b7men", "du"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sahen faul dem Leben zu.", "tokens": ["Und", "sa\u00b7hen", "faul", "dem", "Le\u00b7ben", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.27": {"line.1": {"text": "Gingen wie Bienen um die Bl\u00fcten,", "tokens": ["Gin\u00b7gen", "wie", "Bie\u00b7nen", "um", "die", "Bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Lie\u00df