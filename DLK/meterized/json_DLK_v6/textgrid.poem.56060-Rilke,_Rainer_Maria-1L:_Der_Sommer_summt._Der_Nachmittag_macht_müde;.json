{"textgrid.poem.56060": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Sommer summt. Der Nachmittag macht m\u00fcde;", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Sommer summt. Der Nachmittag macht m\u00fcde;", "tokens": ["Der", "Som\u00b7mer", "summt", ".", "Der", "Nach\u00b7mit\u00b7tag", "macht", "m\u00fc\u00b7de", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "sie atmete verwirrt ihr frisches Kleid", "tokens": ["sie", "at\u00b7me\u00b7te", "ver\u00b7wirrt", "ihr", "fri\u00b7sches", "Kleid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und legte in die triftige Et\u00fcde", "tokens": ["und", "leg\u00b7te", "in", "die", "trif\u00b7ti\u00b7ge", "E\u00b7t\u00fc\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "die Ungeduld nach einer Wirklichkeit,", "tokens": ["die", "Un\u00b7ge\u00b7duld", "nach", "ei\u00b7ner", "Wirk\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "die kommen konnte: morgen, heute abend \u2013,", "tokens": ["die", "kom\u00b7men", "konn\u00b7te", ":", "mor\u00b7gen", ",", "heu\u00b7te", "a\u00b7bend", "\u2013", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "VVINF", "VMFIN", "$.", "ADV", "$,", "ADV", "VVPP", "$(", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die vielleicht da war, die man nur verbarg;", "tokens": ["die", "viel\u00b7leicht", "da", "war", ",", "die", "man", "nur", "ver\u00b7barg", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VAFIN", "$,", "PRELS", "PIS", "ADV", "VVFIN", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "und vor den Fenstern, hoch und alles habend,", "tokens": ["und", "vor", "den", "Fens\u00b7tern", ",", "hoch", "und", "al\u00b7les", "ha\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "ADJD", "KON", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "empfand sie pl\u00f6tzlich den verw\u00f6hnten Park.", "tokens": ["emp\u00b7fand", "sie", "pl\u00f6tz\u00b7lich", "den", "ver\u00b7w\u00f6hn\u00b7ten", "Park", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da brach sie ab; schaute hinaus, verschr\u00e4nkte", "tokens": ["Da", "brach", "sie", "ab", ";", "schau\u00b7te", "hin\u00b7aus", ",", "ver\u00b7schr\u00e4nk\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$.", "VVFIN", "PTKVZ", "$,", "VVFIN"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "die H\u00e4nde; w\u00fcnschte sich ein langes Buch \u2013", "tokens": ["die", "H\u00e4n\u00b7de", ";", "w\u00fcnschte", "sich", "ein", "lan\u00b7ges", "Buch", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "VVFIN", "PRF", "ART", "ADJA", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "und schob auf einmal den Jasmingeruch", "tokens": ["und", "schob", "auf", "ein\u00b7mal", "den", "Jas\u00b7min\u00b7ge\u00b7ruch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADV", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "erz\u00fcrnt zur\u00fcck. Sie fand, da\u00df er sie kr\u00e4nkte.", "tokens": ["er\u00b7z\u00fcrnt", "zu\u00b7r\u00fcck", ".", "Sie", "fand", ",", "da\u00df", "er", "sie", "kr\u00e4nk\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}