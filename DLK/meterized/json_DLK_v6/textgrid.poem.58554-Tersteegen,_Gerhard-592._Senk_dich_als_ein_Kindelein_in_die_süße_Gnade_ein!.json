{"textgrid.poem.58554": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "592. Senk dich als ein Kindelein in die s\u00fc\u00dfe Gnade ein!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "S\u00fc\u00df und sauer,", "tokens": ["S\u00fc\u00df", "und", "sau\u00b7er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Freud' und Trauer,", "tokens": ["Freud'", "und", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Alles gut, was Jesus tut;", "tokens": ["Al\u00b7les", "gut", ",", "was", "Je\u00b7sus", "tut", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "$,", "PRELS", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bleib nur liegen", "tokens": ["Bleib", "nur", "lie\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "In der Wiegen", "tokens": ["In", "der", "Wie\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.6": {"text": "Und vertrau dich seiner Hut!", "tokens": ["Und", "ver\u00b7trau", "dich", "sei\u00b7ner", "Hut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}