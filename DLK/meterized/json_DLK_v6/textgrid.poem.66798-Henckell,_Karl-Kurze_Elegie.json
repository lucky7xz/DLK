{"textgrid.poem.66798": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Kurze Elegie", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Denk' ich daran, was all das soll,", "tokens": ["Denk'", "ich", "da\u00b7ran", ",", "was", "all", "das", "soll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PAV", "$,", "PRELS", "PIAT", "PDS", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dies Dasein tr\u00fcb und jammervoll,", "tokens": ["Dies", "Da\u00b7sein", "tr\u00fcb", "und", "jam\u00b7mer\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Rauscht durch die Seele mir ein dunkler Groll.", "tokens": ["Rauscht", "durch", "die", "See\u00b7le", "mir", "ein", "dunk\u00b7ler", "Groll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des Goldes Macht,", "tokens": ["Des", "Gol\u00b7des", "Macht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Die ganze Niedertracht", "tokens": ["Die", "gan\u00b7ze", "Nie\u00b7der\u00b7tracht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Hat mir des Abscheus m\u00fcde Glut entfacht.", "tokens": ["Hat", "mir", "des", "Ab\u00b7scheus", "m\u00fc\u00b7de", "Glut", "ent\u00b7facht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "An seines Edelwildes Qualen weidet", "tokens": ["An", "sei\u00b7nes", "E\u00b7del\u00b7wil\u00b7des", "Qua\u00b7len", "wei\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Schl\u00e4chter sich, wie's r\u00f6chelt und verscheidet \u2013", "tokens": ["Der", "Schl\u00e4ch\u00b7ter", "sich", ",", "wie's", "r\u00f6\u00b7chelt", "und", "ver\u00b7schei\u00b7det", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "$,", "PWAV", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Mir ist die Welt mit ihrer Lust verleidet.", "tokens": ["Mir", "ist", "die", "Welt", "mit", "ih\u00b7rer", "Lust", "ver\u00b7lei\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}