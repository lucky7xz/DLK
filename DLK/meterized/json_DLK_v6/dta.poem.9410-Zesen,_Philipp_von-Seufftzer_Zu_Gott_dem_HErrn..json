{"dta.poem.9410": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Seufftzer  \n  Zu Gott dem HErrn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wje lange wil meiner der HErre vergessen?", "tokens": ["Wie", "lan\u00b7ge", "wil", "mei\u00b7ner", "der", "Her\u00b7re", "ver\u00b7ges\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VMFIN", "PPOSAT", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wie lange verb\u00fcrgstu dein Antlitz f\u00fcr mir?", "tokens": ["Wie", "lan\u00b7ge", "ver\u00b7b\u00fcrg\u00b7stu", "dein", "Ant\u00b7litz", "f\u00fcr", "mir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wie lange sol kummer und sorge mich fressen?", "tokens": ["Wie", "lan\u00b7ge", "sol", "kum\u00b7mer", "und", "sor\u00b7ge", "mich", "fres\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIAT", "NN", "KON", "NN", "PPER", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "und \u00e4ngsten mein Hertze/ mein Leben allhier?", "tokens": ["und", "\u00e4ngs\u00b7ten", "mein", "Hert\u00b7ze", "/", "mein", "Le\u00b7ben", "all\u00b7hier", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "VVFIN", "$(", "PPOSAT", "NN", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Wie lange sol w\u00fcten", "tokens": ["Wie", "lan\u00b7ge", "sol", "w\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VMFIN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "und toben der Feind?", "tokens": ["und", "to\u00b7ben", "der", "Feind", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Wann wiltu mir bieten", "tokens": ["Wann", "wil\u00b7tu", "mir", "bie\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Die Rechte/ die sonsten mich treulich gemeint?", "tokens": ["Die", "Rech\u00b7te", "/", "die", "sons\u00b7ten", "mich", "treu\u00b7lich", "ge\u00b7meint", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADJA", "PPER", "ADJD", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Mein flehen und bitten/ ", "tokens": ["Mein", "fle\u00b7hen", "und", "bit\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "KON", "VVINF", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Erleuchte die Augen/ la\u00df scheinen dein Licht/", "tokens": ["Er\u00b7leuch\u00b7te", "die", "Au\u00b7gen", "/", "la\u00df", "schei\u00b7nen", "dein", "Licht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$(", "VVIMP", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Dem schrecklichen schlaaffe des Todes auch wehre/", "tokens": ["Dem", "schreck\u00b7li\u00b7chen", "schlaaf\u00b7fe", "des", "To\u00b7des", "auch", "weh\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Auf da\u00df sich mein Hasser erlustige nicht;", "tokens": ["Auf", "da\u00df", "sich", "mein", "Has\u00b7ser", "er\u00b7lus\u00b7ti\u00b7ge", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PRF", "PPOSAT", "NN", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Auf da\u00df sich aufs neue/", "tokens": ["Auf", "da\u00df", "sich", "aufs", "neu\u00b7e", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PRF", "APPRART", "ADJA", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Nicht r\u00fchme der Feind;", "tokens": ["Nicht", "r\u00fch\u00b7me", "der", "Feind", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Auf da\u00df sich nicht freue", "tokens": ["Auf", "da\u00df", "sich", "nicht", "freu\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PRF", "PTKNEG", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Der/ welcher mich allezeit f\u00e4lschlich gemeint.", "tokens": ["Der", "/", "wel\u00b7cher", "mich", "al\u00b7le\u00b7zeit", "f\u00e4lschlich", "ge\u00b7meint", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PWS", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+--+-++-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch hoff\u2019 ich und traue der g\u00f6ttlichen G\u00fcte/", "tokens": ["Doch", "hoff'", "ich", "und", "trau\u00b7e", "der", "g\u00f6tt\u00b7li\u00b7chen", "G\u00fc\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KON", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die Gnade des H\u00f6chsten erfreuet mich so/", "tokens": ["Die", "Gna\u00b7de", "des", "H\u00f6chs\u00b7ten", "er\u00b7freu\u00b7et", "mich", "so", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "ADV", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Es hilffet ja gerne dein Vater-gem\u00fcthe/", "tokens": ["Es", "hilf\u00b7fet", "ja", "ger\u00b7ne", "dein", "Va\u00b7ter\u00b7ge\u00b7m\u00fc\u00b7the", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PPOSAT", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Du hilffest und machest mein Hertzerecht froh/", "tokens": ["Du", "hilf\u00b7fest", "und", "ma\u00b7chest", "mein", "Hert\u00b7ze\u00b7recht", "froh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "ADJD", "$("], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Dr\u00fcmb will ich auch singen", "tokens": ["Dr\u00fcmb", "will", "ich", "auch", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Den HErren nun an/", "tokens": ["Den", "Her\u00b7ren", "nun", "an", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKVZ", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Ich wil mich erschwingen/", "tokens": ["Ich", "wil", "mich", "er\u00b7schwin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Dieweil er mir alles zum besten gethan!", "tokens": ["Die\u00b7weil", "er", "mir", "al\u00b7les", "zum", "bes\u00b7ten", "ge\u00b7than", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PIS", "APPRART", "ADJA", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}