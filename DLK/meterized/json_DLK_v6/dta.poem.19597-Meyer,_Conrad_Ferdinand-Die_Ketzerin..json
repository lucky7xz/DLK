{"dta.poem.19597": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Die Ketzerin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Fra Dolcin, der Ketzer, der von Dante", "tokens": ["Fra", "Dol\u00b7cin", ",", "der", "Ket\u00b7zer", ",", "der", "von", "Dan\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "ART", "NN", "$,", "PRELS", "APPR", "NE"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In der H\u00f6lle neunten Kreis Gebannte,", "tokens": ["In", "der", "H\u00f6l\u00b7le", "neun\u00b7ten", "Kreis", "Ge\u00b7bann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Hat ein Weib geliebt, von dem sie sagen,", "tokens": ["Hat", "ein", "Weib", "ge\u00b7liebt", ",", "von", "dem", "sie", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$,", "APPR", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df kein sch\u00f6n'res lebt' in jenen Tagen.", "tokens": ["Da\u00df", "kein", "sch\u00f6n'\u00b7res", "lebt'", "in", "je\u00b7nen", "Ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Kamen seine J\u00fcnger ihn zu gr\u00fc\u00dfen,", "tokens": ["Ka\u00b7men", "sei\u00b7ne", "J\u00fcn\u00b7ger", "ihn", "zu", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sa\u00df die Blonde schon zu seinen F\u00fc\u00dfen,", "tokens": ["Sa\u00df", "die", "Blon\u00b7de", "schon", "zu", "sei\u00b7nen", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Segnet' er das Volk mit frevler Rechten,", "tokens": ["Seg\u00b7net'", "er", "das", "Volk", "mit", "frev\u00b7ler", "Rech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Neigte sie zuerst die goldnen Flechten;", "tokens": ["Neig\u00b7te", "sie", "zu\u00b7erst", "die", "gold\u00b7nen", "Flech\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Dem Verfehmten folgte sie, dem Flieh'nden,", "tokens": ["Dem", "Ver\u00b7fehm\u00b7ten", "folg\u00b7te", "sie", ",", "dem", "Flieh'n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Durch die Schluchten des Gebirges Zieh'nden \u2014", "tokens": ["Durch", "die", "Schluch\u00b7ten", "des", "Ge\u00b7bir\u00b7ges", "Zieh'n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Da er von den Schergen ward gefangen,", "tokens": ["Da", "er", "von", "den", "Scher\u00b7gen", "ward", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Ist sie seinen Fesseln nachgegangen;", "tokens": ["Ist", "sie", "sei\u00b7nen", "Fes\u00b7seln", "nach\u00b7ge\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Wo er in der Flamme sich gewunden,", "tokens": ["Wo", "er", "in", "der", "Flam\u00b7me", "sich", "ge\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "PRF", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Steht auch sie am Marterpfahl gebunden.", "tokens": ["Steht", "auch", "sie", "am", "Mar\u00b7ter\u00b7pfahl", "ge\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Lieblich ist, die Fra Dolcin verf\u00fchrte,", "tokens": ["Lieb\u00b7lich", "ist", ",", "die", "Fra", "Dol\u00b7cin", "ver\u00b7f\u00fchr\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "PRELS", "NE", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie noch nie ein Weib die Herzen r\u00fchrte;", "tokens": ["Wie", "noch", "nie", "ein", "Weib", "die", "Her\u00b7zen", "r\u00fchr\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Augen, unergr\u00fcndlich wunderbare,", "tokens": ["Au\u00b7gen", ",", "un\u00b7er\u00b7gr\u00fcnd\u00b7lich", "wun\u00b7der\u00b7ba\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Schaun, als ob sie zu den Sel'gen fahre.", "tokens": ["Schaun", ",", "als", "ob", "sie", "zu", "den", "Sel'\u00b7gen", "fah\u00b7re", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOKOM", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Die sie richten, fragen sich mit Grauen:", "tokens": ["Die", "sie", "rich\u00b7ten", ",", "fra\u00b7gen", "sich", "mit", "Grau\u00b7en", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "$,", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Kann die H\u00f6lle wie der Himmel schauen?", "tokens": ["Kann", "die", "H\u00f6l\u00b7le", "wie", "der", "Him\u00b7mel", "schau\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "KOKOM", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Und es zittern vor dem unschuldsvollen", "tokens": ["Und", "es", "zit\u00b7tern", "vor", "dem", "un\u00b7schulds\u00b7vol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Engelsantlitz, die sie martern wollen.", "tokens": ["En\u00b7gel\u00b7sant\u00b7litz", ",", "die", "sie", "mar\u00b7tern", "wol\u00b7len", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Selbst der Priester spricht mit ihr gelinde,", "tokens": ["Selbst", "der", "Pries\u00b7ter", "spricht", "mit", "ihr", "ge\u00b7lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Als mit einem irrgegangnen Kinde:", "tokens": ["Als", "mit", "ei\u00b7nem", "irr\u00b7ge\u00b7gang\u00b7nen", "Kin\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "\u201eschwaches Weib, der dich verleitet hatte,", "tokens": ["\u201e", "schwa\u00b7ches", "Weib", ",", "der", "dich", "ver\u00b7lei\u00b7tet", "hat\u00b7te", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "PRELS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Weder Bruder war er dir noch Gatte!", "tokens": ["We\u00b7der", "Bru\u00b7der", "war", "er", "dir", "noch", "Gat\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "PPER", "ADV", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Seine Asche treibt im Wind! Verflogen", "tokens": ["Sei\u00b7ne", "A\u00b7sche", "treibt", "im", "Wind", "!", "Ver\u00b7flo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$.", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sind die Stapfen, die dich nachgezogen!", "tokens": ["Sind", "die", "Stap\u00b7fen", ",", "die", "dich", "nach\u00b7ge\u00b7zo\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "PRF", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "B\u00fc\u00dfe! Folge reuig den Geboten", "tokens": ["B\u00fc\u00b7\u00dfe", "!", "Fol\u00b7ge", "reu\u00b7ig", "den", "Ge\u00b7bo\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "NN", "ADJD", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Unsrer heil'gen Kirche! La\u00df den Todten!\u201c", "tokens": ["Uns\u00b7rer", "heil'\u00b7gen", "Kir\u00b7che", "!", "La\u00df", "den", "Tod\u00b7ten", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "VVIMP", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "In den Banden kann sich nicht bewegen", "tokens": ["In", "den", "Ban\u00b7den", "kann", "sich", "nicht", "be\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VMFIN", "PRF", "PTKNEG", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Margherita, nur die Lippen regen:", "tokens": ["Marg\u00b7he\u00b7ri\u00b7ta", ",", "nur", "die", "Lip\u00b7pen", "re\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ART", "NN", "ADJA", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.11": {"text": "\u201eleiden mu\u00df ich, was Dolcin gelitten ...", "tokens": ["\u201e", "lei\u00b7den", "mu\u00df", "ich", ",", "was", "Dol\u00b7cin", "ge\u00b7lit\u00b7ten", "..."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVINF", "VMFIN", "PPER", "$,", "PWS", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Horch, er ruft! Ich folge seinen Schritten\u201c \u2014", "tokens": ["Horch", ",", "er", "ruft", "!", "Ich", "fol\u00b7ge", "sei\u00b7nen", "Schrit\u00b7ten", "\u201c"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$(", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Und die warmen, tiefen Blicke strahlen \u2014", "tokens": ["Und", "die", "war\u00b7men", ",", "tie\u00b7fen", "Bli\u00b7cke", "strah\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "\u201edurch die Martern folg' ich, durch die Qualen!\u201c", "tokens": ["\u201e", "durch", "die", "Mar\u00b7tern", "fol\u00b7g'", "ich", ",", "durch", "die", "Qua\u00b7len", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "ART", "NN", "VVFIN", "PPER", "$,", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "\u2014 \u201eKetzerin, dich st\u00e4rken finstre M\u00e4chte!", "tokens": ["\u201e", "Ket\u00b7ze\u00b7rin", ",", "dich", "st\u00e4r\u00b7ken", "finst\u00b7re", "M\u00e4ch\u00b7te", "!"], "token_info": ["punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "NN", "$,", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Br\u00e4nde her!\u201c .... Es r\u00fchren sich die Knechte.", "tokens": ["Br\u00e4n\u00b7de", "her", "!", "\u201c", "....", "Es", "r\u00fch\u00b7ren", "sich", "die", "Knech\u00b7te", "."], "token_info": ["word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "$(", "$.", "PPER", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Siehe da! Wie eines Blitzes Leuchten", "tokens": ["Sie\u00b7he", "da", "!", "Wie", "ei\u00b7nes", "Blit\u00b7zes", "Leuch\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "$.", "PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "F\u00e4hrt ein Ritter unter die Gescheuchten,", "tokens": ["F\u00e4hrt", "ein", "Rit\u00b7ter", "un\u00b7ter", "die", "Ge\u00b7scheuch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Will den sch\u00f6nen D\u00e4mon sich erstreiten;", "tokens": ["Will", "den", "sch\u00f6\u00b7nen", "D\u00e4\u00b7mon", "sich", "er\u00b7strei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Er bem\u00e4chtigt sich der Maledeiten,", "tokens": ["Er", "be\u00b7m\u00e4ch\u00b7tigt", "sich", "der", "Ma\u00b7le\u00b7dei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Ihre Kniee fasst er mit der Linken,", "tokens": ["Ih\u00b7re", "Kni\u00b7ee", "fasst", "er", "mit", "der", "Lin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "In der Rechten droht des Schwertes Blinken:", "tokens": ["In", "der", "Rech\u00b7ten", "droht", "des", "Schwer\u00b7tes", "Blin\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "\u201etretet aus die Glut! Bei Gottes Leibe,", "tokens": ["\u201e", "tre\u00b7tet", "aus", "die", "Glut", "!", "Bei", "Got\u00b7tes", "Lei\u00b7be", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "APPR", "ART", "NN", "$.", "APPR", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "L\u00f6scht die Fackeln! Weg von meinem Weibe!", "tokens": ["L\u00f6scht", "die", "Fa\u00b7ckeln", "!", "Weg", "von", "mei\u00b7nem", "Wei\u00b7be", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Sage Ja ... Mit einem Wink der Lider ...", "tokens": ["Sa\u00b7ge", "Ja", "...", "Mit", "ei\u00b7nem", "Wink", "der", "Li\u00b7der", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKANT", "$(", "APPR", "ART", "NN", "ART", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Und vom Scheiterhaufen steigst du nieder!", "tokens": ["Und", "vom", "Schei\u00b7ter\u00b7hau\u00b7fen", "steigst", "du", "nie\u00b7der", "!"], "token_info": ["word",