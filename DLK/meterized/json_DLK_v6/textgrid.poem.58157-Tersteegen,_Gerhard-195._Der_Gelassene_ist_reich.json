{"textgrid.poem.58157": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "195. Der Gelassene ist reich", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bald willst du dies, bald jenes haben,", "tokens": ["Bald", "willst", "du", "dies", ",", "bald", "je\u00b7nes", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PDS", "$,", "ADV", "PDS", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und immer unvergn\u00fcgt du bist;", "tokens": ["Und", "im\u00b7mer", "un\u00b7ver\u00b7gn\u00fcgt", "du", "bist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der hat den Geber mit den Gaben,", "tokens": ["Der", "hat", "den", "Ge\u00b7ber", "mit", "den", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wer v\u00f6llig Gott gelassen ist.", "tokens": ["Wer", "v\u00f6l\u00b7lig", "Gott", "ge\u00b7las\u00b7sen", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}