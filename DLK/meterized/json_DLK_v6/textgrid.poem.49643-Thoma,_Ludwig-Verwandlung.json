{"textgrid.poem.49643": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Verwandlung", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und als der Krieg die M\u00e4nner fra\u00df,", "tokens": ["Und", "als", "der", "Krieg", "die", "M\u00e4n\u00b7ner", "fra\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gab's wen, der hinterm Ofen sa\u00df", "tokens": ["Gab's", "wen", ",", "der", "hin\u00b7term", "O\u00b7fen", "sa\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVINF", "$,", "PRELS", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verzagt in seinem H\u00e4uschen:", "tokens": ["Ver\u00b7zagt", "in", "sei\u00b7nem", "H\u00e4usc\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u2013 Zar Nikol\u00e4uschen.", "tokens": ["\u2013", "Zar", "Ni\u00b7ko\u00b7l\u00e4u\u00b7schen", "."], "token_info": ["punct", "word", "word", "punct"], "pos": ["$(", "NE", "NE", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Der Friede kam nun in die Welt.", "tokens": ["Der", "Frie\u00b7de", "kam", "nun", "in", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da geht als wie ein rechter Held,", "tokens": ["Da", "geht", "als", "wie", "ein", "rech\u00b7ter", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOUS", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus seinem K\u00e4mmerchen heraus:", "tokens": ["Aus", "sei\u00b7nem", "K\u00e4m\u00b7mer\u00b7chen", "he\u00b7raus", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u2013 Zar Nikolaus.", "tokens": ["\u2013", "Zar", "Ni\u00b7ko\u00b7laus", "."], "token_info": ["punct", "word", "word", "punct"], "pos": ["$(", "NE", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Die Flotte hin, kaputt das Heer,", "tokens": ["Die", "Flot\u00b7te", "hin", ",", "ka\u00b7putt", "das", "Heer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Petersburg sah ihn nicht mehr.", "tokens": ["Und", "Pe\u00b7ters\u00b7burg", "sah", "ihn", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Es war in seinem H\u00e4uschen:", "tokens": ["Es", "war", "in", "sei\u00b7nem", "H\u00e4usc\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u2013 Zar Nikol\u00e4uschen.", "tokens": ["\u2013", "Zar", "Ni\u00b7ko\u00b7l\u00e4u\u00b7schen", "."], "token_info": ["punct", "word", "word", "punct"], "pos": ["$(", "NE", "NE", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Auf Regen folgte Sonnenschein,", "tokens": ["Auf", "Re\u00b7gen", "folg\u00b7te", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jetzt ist die Hose wieder rein,", "tokens": ["Jetzt", "ist", "die", "Ho\u00b7se", "wie\u00b7der", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es flicht sich einen Lorbeerstrau\u00df:", "tokens": ["Es", "flicht", "sich", "ei\u00b7nen", "Lor\u00b7beer\u00b7strau\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u2013 Zar Nikolaus.", "tokens": ["\u2013", "Zar", "Ni\u00b7ko\u00b7laus", "."], "token_info": ["punct", "word", "word", "punct"], "pos": ["$(", "NE", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Die Schande ward zum hohen Ruhm,", "tokens": ["Die", "Schan\u00b7de", "ward", "zum", "ho\u00b7hen", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Feigheit ward ein Heldentum,", "tokens": ["Die", "Feig\u00b7heit", "ward", "ein", "Hel\u00b7den\u00b7tum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein L\u00f6we ward das M\u00e4uschen:", "tokens": ["Ein", "L\u00f6\u00b7we", "ward", "das", "M\u00e4u\u00b7schen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u2013 Zar Nikol\u00e4uschen.", "tokens": ["\u2013", "Zar", "Ni\u00b7ko\u00b7l\u00e4u\u00b7schen", "."], "token_info": ["punct", "word", "word", "punct"], "pos": ["$(", "NE", "NE", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Er wird nun wieder \u2013 etwas sp\u00e4t \u2013", "tokens": ["Er", "wird", "nun", "wie\u00b7der", "\u2013", "et\u00b7was", "sp\u00e4t", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "$(", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der F\u00fchrer, Kriegsherr, Majest\u00e4t,", "tokens": ["Der", "F\u00fch\u00b7rer", ",", "Kriegs\u00b7herr", ",", "Ma\u00b7jes\u00b7t\u00e4t", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "H\u00e4lt Reden und teilt Orden aus.", "tokens": ["H\u00e4lt", "Re\u00b7den", "und", "teilt", "Or\u00b7den", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "VVFIN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O Nikolaus!", "tokens": ["O", "Ni\u00b7ko\u00b7laus", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}