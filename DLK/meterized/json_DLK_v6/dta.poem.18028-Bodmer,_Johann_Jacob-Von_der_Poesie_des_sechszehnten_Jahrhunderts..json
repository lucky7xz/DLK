{"dta.poem.18028": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Von der Poesie des sechszehnten  \n Jahrhunderts.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198462", "language": ["de:0.57", "nl:0.28", "en:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den Weisen oft gar manches w&#252;rret,", "tokens": ["Den", "Wei\u00b7sen", "oft", "gar", "man\u00b7ches", "w", "&#252;", "rret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "PIS", "NE", "$(", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Das die Thoren gantz nicht irret.", "tokens": ["Das", "die", "Tho\u00b7ren", "gantz", "nicht", "ir\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Weisen m&#339;chten nicht genesen,", "tokens": ["Die", "Wei\u00b7sen", "m", "&#339;", "ch\u00b7ten", "nicht", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "punct"], "pos": ["ART", "NN", "FM", "FM", "FM", "PTKNEG", "VVPP", "$,"], "meter": "-+--+--+--", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Solten sie gantz ohn Thoren wesen.", "tokens": ["Sol\u00b7ten", "sie", "gantz", "ohn", "Tho\u00b7ren", "we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}}}}