{"dta.poem.6609": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von denen Sinnlichkeiten  \n insgemein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die ungez\u00e4hlt' und nie begriffne Zahl", "tokens": ["Die", "un\u00b7ge\u00b7z\u00e4hlt'", "und", "nie", "be\u00b7griff\u00b7ne", "Zahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "KON", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von allen Dingen hier, die so verschieden seyn:", "tokens": ["Von", "al\u00b7len", "Din\u00b7gen", "hier", ",", "die", "so", "ver\u00b7schie\u00b7den", "seyn", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "$,", "PRELS", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die hangen allzumal,", "tokens": ["Die", "han\u00b7gen", "all\u00b7zu\u00b7mal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sammt ihrer Fruchtbarkeit nie leeren Folg\u2019, allein", "tokens": ["Sammt", "ih\u00b7rer", "Frucht\u00b7bar\u00b7keit", "nie", "lee\u00b7ren", "Fol\u00b7g'", ",", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "ADJA", "NN", "$,", "ADV"], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Von einer Ordnung ab, die allgemein.", "tokens": ["Von", "ei\u00b7ner", "Ord\u00b7nung", "ab", ",", "die", "all\u00b7ge\u00b7mein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,", "PRELS", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Durch Dinge, die hervorgebracht", "tokens": ["Durch", "Din\u00b7ge", ",", "die", "her\u00b7vor\u00b7ge\u00b7bracht"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von einer Ursach, die unwandelbar", "tokens": ["Von", "ei\u00b7ner", "Ur\u00b7sach", ",", "die", "un\u00b7wan\u00b7del\u00b7bar"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und deren W\u00fcrckung w\u00e4hrt, so, wie sie einmal war,", "tokens": ["Und", "de\u00b7ren", "W\u00fcr\u00b7ckung", "w\u00e4hrt", ",", "so", ",", "wie", "sie", "ein\u00b7mal", "war", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "VVFIN", "$,", "ADV", "$,", "PWAV", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}