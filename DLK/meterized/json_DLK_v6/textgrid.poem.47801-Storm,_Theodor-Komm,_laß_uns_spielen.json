{"textgrid.poem.47801": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Komm, la\u00df uns spielen", "genre": "verse", "period": "N.A.", "pub_year": 1881, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie bald des Sommers holdes Fest verging!", "tokens": ["Wie", "bald", "des", "Som\u00b7mers", "hol\u00b7des", "Fest", "ver\u00b7ging", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Rauh weht der Herbst; wird's denn auch Fr\u00fchling wieder?", "tokens": ["Rauh", "weht", "der", "Herbst", ";", "wird's", "denn", "auch", "Fr\u00fch\u00b7ling", "wie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$.", "VAFIN", "ADV", "ADV", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da f\u00e4llt ein bleicher Sonnenstrahl hernieder \u2013", "tokens": ["Da", "f\u00e4llt", "ein", "blei\u00b7cher", "Son\u00b7nen\u00b7strahl", "her\u00b7nie\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Komm, la\u00df uns spielen, wei\u00dfer Schmetterling!", "tokens": ["Komm", ",", "la\u00df", "uns", "spie\u00b7len", ",", "wei\u00b7\u00dfer", "Schmet\u00b7ter\u00b7ling", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVIMP", "PPER", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ach, keine Nelke, keine Rose mehr;", "tokens": ["Ach", ",", "kei\u00b7ne", "Nel\u00b7ke", ",", "kei\u00b7ne", "Ro\u00b7se", "mehr", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIAT", "NN", "$,", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Am Himmel f\u00e4hrt ein kalt Gew\u00f6lk daher!", "tokens": ["Am", "Him\u00b7mel", "f\u00e4hrt", "ein", "kalt", "Ge\u00b7w\u00f6lk", "da\u00b7her", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJD", "NN", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Weh, wie so bald des Sommers Lust verging \u2013", "tokens": ["Weh", ",", "wie", "so", "bald", "des", "Som\u00b7mers", "Lust", "ver\u00b7ging", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADV", "ADV", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "O komm! Wo bist du, wei\u00dfer Schmetterling?", "tokens": ["O", "komm", "!", "Wo", "bist", "du", ",", "wei\u00b7\u00dfer", "Schmet\u00b7ter\u00b7ling", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "PWAV", "VAFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}