{"textgrid.poem.58313": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "351. Immer bereit", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vielleicht mag diese Stund' noch meine letzte sein,", "tokens": ["Viel\u00b7leicht", "mag", "die\u00b7se", "Stund'", "noch", "mei\u00b7ne", "letz\u00b7te", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PDAT", "NN", "ADV", "PPOSAT", "ADJA", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drum mu\u00df ich meine Seel' in meinen H\u00e4nden tragen,", "tokens": ["Drum", "mu\u00df", "ich", "mei\u00b7ne", "Seel'", "in", "mei\u00b7nen", "H\u00e4n\u00b7den", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df ich, wann Jesus kommt, kann frei und ruhig sagen:", "tokens": ["Da\u00df", "ich", ",", "wann", "Je\u00b7sus", "kommt", ",", "kann", "frei", "und", "ru\u00b7hig", "sa\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWAV", "NE", "VVFIN", "$,", "VMFIN", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da, Jesus, legt mein Geist in deine Hand sich ein.", "tokens": ["Da", ",", "Je\u00b7sus", ",", "legt", "mein", "Geist", "in", "dei\u00b7ne", "Hand", "sich", "ein", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NE", "$,", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}