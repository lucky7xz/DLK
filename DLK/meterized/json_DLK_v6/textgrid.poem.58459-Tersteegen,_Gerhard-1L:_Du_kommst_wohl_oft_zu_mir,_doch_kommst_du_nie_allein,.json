{"textgrid.poem.58459": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du kommst wohl oft zu mir, doch kommst du nie allein,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du kommst wohl oft zu mir, doch kommst du nie allein,", "tokens": ["Du", "kommst", "wohl", "oft", "zu", "mir", ",", "doch", "kommst", "du", "nie", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "PPER", "$,", "ADV", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drum kann ich auch nicht frei und recht vertraulich sein;", "tokens": ["Drum", "kann", "ich", "auch", "nicht", "frei", "und", "recht", "ver\u00b7trau\u00b7lich", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "PTKNEG", "ADJD", "KON", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Will', ein Bild und Form die Einsamkeit uns st\u00f6ren,", "tokens": ["Ein", "Will'", ",", "ein", "Bild", "und", "Form", "die", "Ein\u00b7sam\u00b7keit", "uns", "st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "NN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df ich nicht reden mag und du nicht recht kannst h\u00f6ren.", "tokens": ["Da\u00df", "ich", "nicht", "re\u00b7den", "mag", "und", "du", "nicht", "recht", "kannst", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "KON", "PPER", "PTKNEG", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Komm ohne dich, mein Kind, herein,", "tokens": ["Komm", "oh\u00b7ne", "dich", ",", "mein", "Kind", ",", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$,", "PPOSAT", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So sind wir frei und recht allein!", "tokens": ["So", "sind", "wir", "frei", "und", "recht", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}