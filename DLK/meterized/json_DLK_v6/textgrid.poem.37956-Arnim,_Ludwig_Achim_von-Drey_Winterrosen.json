{"textgrid.poem.37956": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Drey Winterrosen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ritt ein Herr mit seinem Knecht,", "tokens": ["Es", "ritt", "ein", "Herr", "mit", "sei\u00b7nem", "Knecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Morgens in dem Thaue,", "tokens": ["Des", "Mor\u00b7gens", "in", "dem", "Thau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was fand er auf der Heide stehn?", "tokens": ["Was", "fand", "er", "auf", "der", "Hei\u00b7de", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein wundersch\u00f6ne Jungfraue.", "tokens": ["Ein", "wun\u00b7der\u00b7sch\u00f6\u00b7ne", "Jung\u00b7frau\u00b7e", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbgott gr\u00fc\u00df euch Jungfrau h\u00fcbsch und fein,", "tokens": ["\u00bb", "gott", "gr\u00fc\u00df", "euch", "Jung\u00b7frau", "h\u00fcbsch", "und", "fein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "PPER", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gott gr\u00fc\u00df euch Auserw\u00e4hlte,", "tokens": ["Gott", "gr\u00fc\u00df", "euch", "Au\u00b7ser\u00b7w\u00e4hl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wollt Gott ich sollt heut bey euch seyn,", "tokens": ["Wollt", "Gott", "ich", "sollt", "heut", "bey", "euch", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PPER", "VMFIN", "ADV", "APPR", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In euren Armen schlafen.\u00ab", "tokens": ["In", "eu\u00b7ren", "Ar\u00b7men", "schla\u00b7fen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbin meinen Armen schlaft ihr nicht,", "tokens": ["\u00bb", "in", "mei\u00b7nen", "Ar\u00b7men", "schlaft", "ihr", "nicht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr bringt mir denn drey Rosen,", "tokens": ["Ihr", "bringt", "mir", "denn", "drey", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die in dem Winter wachsen sind,", "tokens": ["Die", "in", "dem", "Win\u00b7ter", "wach\u00b7sen", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In voller Bl\u00fct erschlossen.\u00ab", "tokens": ["In", "vol\u00b7ler", "Bl\u00fct", "er\u00b7schlos\u00b7sen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er schwang sich in den Sattel frei,", "tokens": ["Er", "schwang", "sich", "in", "den", "Sat\u00b7tel", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dahin so th\u00e4t er traben,", "tokens": ["Da\u00b7hin", "so", "th\u00e4t", "er", "tra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da wo die rothen R\u00f6slein stehn,", "tokens": ["Da", "wo", "die", "ro\u00b7then", "R\u00f6s\u00b7lein", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um Fr\u00e4uleins Gunst zu haben.", "tokens": ["Um", "Fr\u00e4u\u00b7leins", "Gunst", "zu", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der R\u00f6slein warn nicht mehr denn drey,", "tokens": ["Der", "R\u00f6s\u00b7lein", "warn", "nicht", "mehr", "denn", "drey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "ADV", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er brach sie an den Stielen,", "tokens": ["Er", "brach", "sie", "an", "den", "Stie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er sch\u00fctt sie der Magd in Geren frei,", "tokens": ["Er", "sch\u00fctt", "sie", "der", "Magd", "in", "Ge\u00b7ren", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nach allem ihren Willen.", "tokens": ["Nach", "al\u00b7lem", "ih\u00b7ren", "Wil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da sie die rothen R\u00f6slein sah,", "tokens": ["Da", "sie", "die", "ro\u00b7then", "R\u00f6s\u00b7lein", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gar freundlich th\u00e4t sie lachen:", "tokens": ["Gar", "freund\u00b7lich", "th\u00e4t", "sie", "la\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbso sagt mir edle R\u00f6slein roth,", "tokens": ["\u00bb", "so", "sagt", "mir", "ed\u00b7le", "R\u00f6s\u00b7lein", "roth", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was Freud k\u00f6nnt ihr mir machen?\u00ab", "tokens": ["Was", "Freud", "k\u00f6nnt", "ihr", "mir", "ma\u00b7chen", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "NN", "VVFIN", "PPER", "PRF", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbdie Freud, die wir euch machen wohl,", "tokens": ["\u00bb", "die", "Freud", ",", "die", "wir", "euch", "ma\u00b7chen", "wohl", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die wird sich auch schon finden,", "tokens": ["Die", "wird", "sich", "auch", "schon", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PRF", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Jetzund geht ihr ein M\u00e4gdlein jung,", "tokens": ["Je\u00b7tzund", "geht", "ihr", "ein", "M\u00e4gd\u00b7lein", "jung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aufs Jahr mit einem Kinde.\u00ab", "tokens": ["Aufs", "Jahr", "mit", "ei\u00b7nem", "Kin\u00b7de", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "\u00bbgeh ich mit einem Kindelein,", "tokens": ["\u00bb", "geh", "ich", "mit", "ei\u00b7nem", "Kin\u00b7de\u00b7lein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So mu\u00df es Gott erbarmen,", "tokens": ["So", "mu\u00df", "es", "Gott", "er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hab ich doch nur eine halbe Nacht,", "tokens": ["Hab", "ich", "doch", "nur", "ei\u00b7ne", "hal\u00b7be", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Geschlafn an deinen Armen.\u00ab", "tokens": ["Ge\u00b7schlafn", "an", "dei\u00b7nen", "Ar\u00b7men", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "\u00bbso klage nicht mein T\u00f6chterlein,", "tokens": ["\u00bb", "so", "kla\u00b7ge", "nicht", "mein", "T\u00f6ch\u00b7ter\u00b7lein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und weine nicht so sehre,", "tokens": ["Und", "wei\u00b7ne", "nicht", "so", "seh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es ist geschehn; manch Jungfr\u00e4ulein", "tokens": ["Es", "ist", "ge\u00b7schehn", ";", "manch", "Jung\u00b7fr\u00e4u\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kam noch zu gro\u00dfen Ehren.\u00ab", "tokens": ["Kam", "noch", "zu", "gro\u00b7\u00dfen", "Eh\u00b7ren", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ADV", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Das hat gesungen ein Reuter gut,", "tokens": ["Das", "hat", "ge\u00b7sun\u00b7gen", "ein", "Reu\u00b7ter", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "ART", "NE", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Berggesell hat ihn verdrungen,", "tokens": ["Ein", "Berg\u00b7ge\u00b7sell", "hat", "ihn", "ver\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er trinkt viel lieber den lautern Wein,", "tokens": ["Er", "trinkt", "viel", "lie\u00b7ber", "den", "lau\u00b7tern", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Denn Wasser aus k\u00fchlem Brunnen.", "tokens": ["Denn", "Was\u00b7ser", "aus", "k\u00fch\u00b7lem", "Brun\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}