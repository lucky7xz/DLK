{"textgrid.poem.24203": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hier, werte Fraun und M\u00e4dchen,", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier, werte Fraun und M\u00e4dchen,", "tokens": ["Hier", ",", "wer\u00b7te", "Fraun", "und", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An einem rosa F\u00e4dchen", "tokens": ["An", "ei\u00b7nem", "ro\u00b7sa", "F\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ist alles aufgereiht", "tokens": ["Ist", "al\u00b7les", "auf\u00b7ge\u00b7reiht"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PIS", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit himmelblauen S\u00e4umen,", "tokens": ["Mit", "him\u00b7mel\u00b7blau\u00b7en", "S\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was mir von bunten Tr\u00e4umen", "tokens": ["Was", "mir", "von", "bun\u00b7ten", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Herblieb aus alter Zeit.", "tokens": ["Her\u00b7blieb", "aus", "al\u00b7ter", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihr k\u00f6nnt behaglich lesen,", "tokens": ["Ihr", "k\u00f6nnt", "be\u00b7hag\u00b7lich", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was ich ein Tr\u00e4umer gewesen", "tokens": ["Was", "ich", "ein", "Tr\u00e4u\u00b7mer", "ge\u00b7we\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ART", "NN", "VAPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und, ach, wie voll mein Herz;", "tokens": ["Und", ",", "ach", ",", "wie", "voll", "mein", "Herz", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ITJ", "$,", "PWAV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Es schwoll, da\u00df Gott erbarme,", "tokens": ["Es", "schwoll", ",", "da\u00df", "Gott", "er\u00b7bar\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vor lauter Lust und Harme", "tokens": ["Vor", "lau\u00b7ter", "Lust", "und", "Har\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und schwebte himmelw\u00e4rts", "tokens": ["Und", "schweb\u00b7te", "him\u00b7mel\u00b7w\u00e4rts"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADV"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.7": {"text": "Als eine bunte Blase,", "tokens": ["Als", "ei\u00b7ne", "bun\u00b7te", "Bla\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gebl\u00e4ht von reiner Ekstase, \u2013", "tokens": ["Ge\u00b7bl\u00e4ht", "von", "rei\u00b7ner", "Eks\u00b7ta\u00b7se", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Doch als es stand am h\u00f6chsten,", "tokens": ["Doch", "als", "es", "stand", "am", "h\u00f6chs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Zerplatzte es vor Schmerz.", "tokens": ["Zer\u00b7platz\u00b7te", "es", "vor", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und schaukelnd fiel hernieder,", "tokens": ["Und", "schau\u00b7kelnd", "fiel", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was von ihm \u00fcbrigblieb:", "tokens": ["Was", "von", "ihm", "\u00fcb\u00b7rig\u00b7blieb", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Eine Handvoll schw\u00e4rmender Lieder,", "tokens": ["Ei\u00b7ne", "Hand\u00b7voll", "schw\u00e4r\u00b7men\u00b7der", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Die ich, ein Tr\u00e4umer, schrieb.", "tokens": ["Die", "ich", ",", "ein", "Tr\u00e4u\u00b7mer", ",", "schrieb", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "$,", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}