{"dta.poem.14028": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Aussicht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Komm zum Garten denn, Du Holde!", "tokens": ["Komm", "zum", "Gar\u00b7ten", "denn", ",", "Du", "Hol\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ADV", "$,", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In den warmen, sch\u00f6nen Tagen", "tokens": ["In", "den", "war\u00b7men", ",", "sch\u00f6\u00b7nen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sollst Du Blumenkr\u00e4nze tragen,", "tokens": ["Sollst", "Du", "Blu\u00b7men\u00b7kr\u00e4n\u00b7ze", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und vom k\u00fchl krystall'nen Golde", "tokens": ["Und", "vom", "k\u00fchl", "kry\u00b7stall'\u00b7nen", "Gol\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit den frischen, rothen Lippen,", "tokens": ["Mit", "den", "fri\u00b7schen", ",", "ro\u00b7then", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eh' ich trinke, l\u00e4chelnd nippen.", "tokens": ["Eh'", "ich", "trin\u00b7ke", ",", "l\u00e4\u00b7chelnd", "nip\u00b7pen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ohne Maa\u00df dann, ohne Richter,", "tokens": ["Oh\u00b7ne", "Maa\u00df", "dann", ",", "oh\u00b7ne", "Rich\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "$,", "KOUI", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "K\u00fcssend, trinkend singt der Dichter", "tokens": ["K\u00fcs\u00b7send", ",", "trin\u00b7kend", "singt", "der", "Dich\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Lieder, die von selbst entschweben:", "tokens": ["Lie\u00b7der", ",", "die", "von", "selbst", "ent\u00b7schwe\u00b7ben", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wundersch\u00f6n ist doch das Leben!", "tokens": ["Wun\u00b7der\u00b7sch\u00f6n", "ist", "doch", "das", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}