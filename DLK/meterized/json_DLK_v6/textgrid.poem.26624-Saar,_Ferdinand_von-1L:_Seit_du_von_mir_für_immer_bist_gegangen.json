{"textgrid.poem.26624": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seit du von mir f\u00fcr immer bist gegangen", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seit du von mir f\u00fcr immer bist gegangen", "tokens": ["Seit", "du", "von", "mir", "f\u00fcr", "im\u00b7mer", "bist", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "APPR", "ADV", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und einsam ist mein Tag und meine Nacht,", "tokens": ["Und", "ein\u00b7sam", "ist", "mein", "Tag", "und", "mei\u00b7ne", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Seh' ich dich oft im Traum mit bleichen Wangen,", "tokens": ["Seh'", "ich", "dich", "oft", "im", "Traum", "mit", "blei\u00b7chen", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "APPRART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das dunkle Aug' in d\u00fcst'rem Schmerz entfacht.", "tokens": ["Das", "dunk\u00b7le", "Aug'", "in", "d\u00fcst'\u00b7rem", "Schmerz", "ent\u00b7facht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du trittst herein in das verwais'te Zimmer", "tokens": ["Du", "trittst", "her\u00b7ein", "in", "das", "ver\u00b7wais'\u00b7te", "Zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und siehst, wie fremd, mit langem Blick dich um \u2013", "tokens": ["Und", "siehst", ",", "wie", "fremd", ",", "mit", "lan\u00b7gem", "Blick", "dich", "um", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ADJD", "$,", "APPR", "ADJA", "NN", "PRF", "APPR", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und still verkl\u00e4rt von geisterhaftem Schimmer,", "tokens": ["Und", "still", "ver\u00b7kl\u00e4rt", "von", "geis\u00b7ter\u00b7haf\u00b7tem", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Willst du dann wieder gehen, ernst und stumm.", "tokens": ["Willst", "du", "dann", "wie\u00b7der", "ge\u00b7hen", ",", "ernst", "und", "stumm", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O bleibe! ruf' ich aus mit bangem Schauer \u2013", "tokens": ["O", "blei\u00b7be", "!", "ruf'", "ich", "aus", "mit", "ban\u00b7gem", "Schau\u00b7er", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wohin, eh' noch dein Mund den Gru\u00df mir bot?", "tokens": ["Wo\u00b7hin", ",", "eh'", "noch", "dein", "Mund", "den", "Gru\u00df", "mir", "bot", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "ADV", "PPOSAT", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da sch\u00fcttelst du das Haupt mit tiefer Trauer:", "tokens": ["Da", "sch\u00fct\u00b7telst", "du", "das", "Haupt", "mit", "tie\u00b7fer", "Trau\u00b7er", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du wei\u00dft es ja, so sprichst du, ich bin todt.", "tokens": ["Du", "wei\u00dft", "es", "ja", ",", "so", "sprichst", "du", ",", "ich", "bin", "todt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "ADV", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich aber d'rauf: Und bist du auch gestorben,", "tokens": ["Ich", "a\u00b7ber", "d'\u00b7rauf", ":", "Und", "bist", "du", "auch", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKVZ", "$.", "KON", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wir k\u00f6nnen dennoch bei einander sein;", "tokens": ["Wir", "k\u00f6n\u00b7nen", "den\u00b7noch", "bei", "ein\u00b7an\u00b7der", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PRF", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir hatten unser Gl\u00fcck so schwer erworben \u2013", "tokens": ["Wir", "hat\u00b7ten", "un\u00b7ser", "Gl\u00fcck", "so", "schwer", "er\u00b7wor\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich la\u00df' dich nicht, und f\u00fcrder bist du mein!", "tokens": ["Ich", "la\u00df'", "dich", "nicht", ",", "und", "f\u00fcr\u00b7der", "bist", "du", "mein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "KON", "ADJD", "VAFIN", "PPER", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Du schaltest nach wie vor im kleinen Hause,", "tokens": ["Du", "schal\u00b7test", "nach", "wie", "vor", "im", "klei\u00b7nen", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "KOKOM", "APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das du betreut mit sorglich lieber Hand \u2013", "tokens": ["Das", "du", "be\u00b7treut", "mit", "sorg\u00b7lich", "lie\u00b7ber", "Hand", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ADJD", "ADV", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Belebst und schm\u00fcckst, wie sonst, des Dichters Klause,", "tokens": ["Be\u00b7lebst", "und", "schm\u00fcckst", ",", "wie", "sonst", ",", "des", "Dich\u00b7ters", "Klau\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,", "PWAV", "ADV", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den keine Seele je wie ", "tokens": ["Den", "kei\u00b7ne", "See\u00b7le", "je", "wie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "ADV", "KOKOM"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und wieder sitzen wir beim schlichten Mahle,", "tokens": ["Und", "wie\u00b7der", "sit\u00b7zen", "wir", "beim", "schlich\u00b7ten", "Mah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Bissen reichend uns'rem treuen Hund,", "tokens": ["Die", "Bis\u00b7sen", "rei\u00b7chend", "un\u00b7s'\u00b7rem", "treu\u00b7en", "Hund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+----+-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Und wandeln dann begn\u00fcgt im Abendstrahle", "tokens": ["Und", "wan\u00b7deln", "dann", "be\u00b7gn\u00fcgt", "im", "A\u00b7bend\u00b7strah\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit sanften Schritten durch des Gartens Rund! \u2013", "tokens": ["Mit", "sanf\u00b7ten", "Schrit\u00b7ten", "durch", "des", "Gar\u00b7tens", "Rund", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Da bebst du auf in seligem Entz\u00fccken,", "tokens": ["Da", "bebst", "du", "auf", "in", "se\u00b7li\u00b7gem", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein Angesicht f\u00e4rbt leiser R\u00f6the Schein \u2013", "tokens": ["Dein", "An\u00b7ge\u00b7sicht", "f\u00e4rbt", "lei\u00b7ser", "R\u00f6\u00b7the", "Schein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch wie ich jetzt dich an mein Herz will dr\u00fccken,", "tokens": ["Doch", "wie", "ich", "jetzt", "dich", "an", "mein", "Herz", "will", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "PRF", "APPR", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erwach' ich auch im Dunkel \u2013 und allein ...", "tokens": ["Er\u00b7wach'", "ich", "auch", "im", "Dun\u00b7kel", "\u2013", "und", "al\u00b7lein", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPRART", "NN", "$(", "KON", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Und dennoch, sieh': mu\u00df auch der Traum zerstieben,", "tokens": ["Und", "den\u00b7noch", ",", "sieh'", ":", "mu\u00df", "auch", "der", "Traum", "zer\u00b7stie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "VVIMP", "$.", "VMFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er k\u00fcndet mir geheime Wirklichkeit;", "tokens": ["Er", "k\u00fcn\u00b7det", "mir", "ge\u00b7hei\u00b7me", "Wirk\u00b7lich\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was da gescheh'n: wir sind vereint geblieben,", "tokens": ["Was", "da", "ge\u00b7scheh'n", ":", "wir", "sind", "ver\u00b7eint", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "$.", "PPER", "VAFIN", "VVPP", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und scheinbar nur hat uns der Tod entzweit.", "tokens": ["Und", "schein\u00b7bar", "nur", "hat", "uns", "der", "Tod", "ent\u00b7zweit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VAFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Wenngleich dein Irdisches zu Staub vermodert,", "tokens": ["Wenn\u00b7gleich", "dein", "Ir\u00b7di\u00b7sches", "zu", "Staub", "ver\u00b7mo\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich wei\u00df es, da\u00df dein Geist mich stets umschwebt;", "tokens": ["Ich", "wei\u00df", "es", ",", "da\u00df", "dein", "Geist", "mich", "stets", "um\u00b7schwebt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von jener Flamme, die in dir gelodert,", "tokens": ["Von", "je\u00b7ner", "Flam\u00b7me", ",", "die", "in", "dir", "ge\u00b7lo\u00b7dert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fchl' ich f\u00fcr immer mir das Herz durchbebt.", "tokens": ["F\u00fchl'", "ich", "f\u00fcr", "im\u00b7mer", "mir", "das", "Herz", "durch\u00b7bebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ADV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Was mit den Jahren wir erlebt, erstritten,", "tokens": ["Was", "mit", "den", "Jah\u00b7ren", "wir", "er\u00b7lebt", ",", "er\u00b7strit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "PPER", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu festem Kitte ward es allgemach \u2013", "tokens": ["Zu", "fes\u00b7tem", "Kit\u00b7te", "ward", "es", "all\u00b7ge\u00b7mach", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPER", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir wurden ", "tokens": ["Wir", "wur\u00b7den"], "token_info": ["word", "word"], "pos": ["PPER", "VAFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "In dieser Welt, die viel an uns verbrach.", "tokens": ["In", "die\u00b7ser", "Welt", ",", "die", "viel", "an", "uns", "ver\u00b7brach", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Und hier am Schlusse dieser Liederreihe \u2013", "tokens": ["Und", "hier", "am", "Schlus\u00b7se", "die\u00b7ser", "Lie\u00b7der\u00b7rei\u00b7he", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ach, so vertraut dir einst in Ton und Wort \u2013", "tokens": ["Ach", ",", "so", "ver\u00b7traut", "dir", "einst", "in", "Ton", "und", "Wort", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "ADJD", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Empf\u00e4ngt erst unser Bund die letzte Weihe:", "tokens": ["Emp\u00b7f\u00e4ngt", "erst", "un\u00b7ser", "Bund", "die", "letz\u00b7te", "Wei\u00b7he", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So lang sie leben, lebst du mit mir fort!", "tokens": ["So", "lang", "sie", "le\u00b7ben", ",", "lebst", "du", "mit", "mir", "fort", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVINF", "$,", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}