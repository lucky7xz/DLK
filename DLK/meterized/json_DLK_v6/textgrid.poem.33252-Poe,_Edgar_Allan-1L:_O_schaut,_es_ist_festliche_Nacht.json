{"textgrid.poem.33252": {"metadata": {"author": {"name": "Poe, Edgar Allan", "birth": "N.A.", "death": "N.A."}, "title": "1L: O schaut, es ist festliche Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1829, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O schaut, es ist festliche Nacht", "tokens": ["O", "schaut", ",", "es", "ist", "fest\u00b7li\u00b7che", "Nacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "PPER", "VAFIN", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Inmitten einsam letzter Tage!", "tokens": ["In\u00b7mit\u00b7ten", "ein\u00b7sam", "letz\u00b7ter", "Ta\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Engelchor, schluchzend, in Fl\u00fcgelpracht", "tokens": ["Ein", "En\u00b7gel\u00b7chor", ",", "schluch\u00b7zend", ",", "in", "Fl\u00fc\u00b7gel\u00b7pracht"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ADJD", "$,", "APPR", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und Schleierflor sieht zage", "tokens": ["Und", "Schlei\u00b7er\u00b7flor", "sieht", "za\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Im Schauspielhaus ein Schauspiel an", "tokens": ["Im", "Schau\u00b7spiel\u00b7haus", "ein", "Schau\u00b7spiel", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von Hoffnung, Angst und Plage,", "tokens": ["Von", "Hoff\u00b7nung", ",", "Angst", "und", "Pla\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Derweil das Orchester dann und wann", "tokens": ["Der\u00b7weil", "das", "Or\u00b7ches\u00b7ter", "dann", "und", "wann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "KON", "PWAV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Musik haucht: Sph\u00e4renklage.", "tokens": ["Mu\u00b7sik", "haucht", ":", "Sph\u00e4\u00b7ren\u00b7kla\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Schauspieler, Gottes Ebenbilder,", "tokens": ["Schau\u00b7spie\u00b7ler", ",", "Got\u00b7tes", "E\u00b7ben\u00b7bil\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Murmeln und brummeln dumpf", "tokens": ["Mur\u00b7meln", "und", "brum\u00b7meln", "dumpf"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "ADJD"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und hasten planlos, immer wilder,", "tokens": ["Und", "has\u00b7ten", "plan\u00b7los", ",", "im\u00b7mer", "wil\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sind Puppen nur und folgen stumpf", "tokens": ["Sind", "Pup\u00b7pen", "nur", "und", "fol\u00b7gen", "stumpf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "ADV", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gewaltigen d\u00fcsteren Dingen,", "tokens": ["Ge\u00b7wal\u00b7ti\u00b7gen", "d\u00fcs\u00b7te\u00b7ren", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Die umziehn ohne Form und Rumpf", "tokens": ["Die", "um\u00b7ziehn", "oh\u00b7ne", "Form", "und", "Rumpf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "VVIZU", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und dunkles Weh aus Kondorschwingen", "tokens": ["Und", "dunk\u00b7les", "Weh", "aus", "Kon\u00b7dor\u00b7schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Schlagen voll Triumph.", "tokens": ["Schla\u00b7gen", "voll", "Tri\u00b7umph", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Dies n\u00e4rrische Drama! \u2013 O f\u00fcrwahr,", "tokens": ["Dies", "n\u00e4r\u00b7ri\u00b7sche", "Dra\u00b7ma", "!", "\u2013", "O", "f\u00fcr\u00b7wahr", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "$.", "$(", "NE", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nie wird's vergessen werden,", "tokens": ["Nie", "wird's", "ver\u00b7ges\u00b7sen", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nie sein Phantom, verfolgt f\u00fcr immerdar", "tokens": ["Nie", "sein", "Phan\u00b7tom", ",", "ver\u00b7folgt", "f\u00fcr", "im\u00b7mer\u00b7dar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "$,", "VVFIN", "APPR", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von wilder Rotte rasenden Geb\u00e4rden,", "tokens": ["Von", "wil\u00b7der", "Rot\u00b7te", "ra\u00b7sen\u00b7den", "Ge\u00b7b\u00e4r\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Verfolgt umsonst \u2013 zum alten Fleck", "tokens": ["Ver\u00b7folgt", "um\u00b7sonst", "\u2013", "zum", "al\u00b7ten", "Fleck"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$(", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kehrt stets der Kreislauf neu zur\u00fcck \u2013", "tokens": ["Kehrt", "stets", "der", "Kreis\u00b7lauf", "neu", "zu\u00b7r\u00fcck", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und nie die Tollheit, die S\u00fcnde, der Schreck", "tokens": ["Und", "nie", "die", "Toll\u00b7heit", ",", "die", "S\u00fcn\u00b7de", ",", "der", "Schreck"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und das Grausen: die Seele vom St\u00fcck.", "tokens": ["Und", "das", "Grau\u00b7sen", ":", "die", "See\u00b7le", "vom", "St\u00fcck", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$.", "ART", "NN", "APPRART", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.4": {"line.1": {"text": "Doch sieh, in die mimende Runde", "tokens": ["Doch", "sieh", ",", "in", "die", "mi\u00b7men\u00b7de", "Run\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Dr\u00e4ngt schleichend ein blutrot Ding", "tokens": ["Dr\u00e4ngt", "schlei\u00b7chend", "ein", "blut\u00b7rot", "Ding"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hervor aus \u00f6dem Hintergrunde", "tokens": ["Her\u00b7vor", "aus", "\u00f6\u00b7dem", "Hin\u00b7ter\u00b7grun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der B\u00fchne \u2013 ein blutrot Ding.", "tokens": ["Der", "B\u00fch\u00b7ne", "\u2013", "ein", "blut\u00b7rot", "Ding", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Es windet sich! \u2013 windet sich in die Bahn", "tokens": ["Es", "win\u00b7det", "sich", "!", "\u2013", "win\u00b7det", "sich", "in", "die", "Bahn"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "$.", "$(", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Der Mimen, die Angst schon t\u00f6tet;", "tokens": ["Der", "Mi\u00b7men", ",", "die", "Angst", "schon", "t\u00f6\u00b7tet", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Die Engel schluchzen, da Wurmes Zahn", "tokens": ["Die", "En\u00b7gel", "schluch\u00b7zen", ",", "da", "Wur\u00b7mes", "Zahn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "$,", "KOUS", "NN", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "In Menschenblut sich r\u00f6tet.", "tokens": ["In", "Men\u00b7schen\u00b7blut", "sich", "r\u00f6\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Aus \u2013 aus sind die Lichter \u2013 alle aus!", "tokens": ["Aus", "\u2013", "aus", "sind", "die", "Lich\u00b7ter", "\u2013", "al\u00b7le", "aus", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "$(", "APPR", "VAFIN", "ART", "NN", "$(", "PIS", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Vor jede zuckende Gestalt", "tokens": ["Vor", "je\u00b7de", "zu\u00b7cken\u00b7de", "Ge\u00b7stalt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Der Vorhang f\u00e4llt mit Wetterbraus:", "tokens": ["Der", "Vor\u00b7hang", "f\u00e4llt", "mit", "Wet\u00b7ter\u00b7braus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Leichentuch finster und kalt.", "tokens": ["Ein", "Lei\u00b7chen\u00b7tuch", "fins\u00b7ter", "und", "kalt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Die Engel schlagen die Schleier zur\u00fcck,", "tokens": ["Die", "En\u00b7gel", "schla\u00b7gen", "die", "Schlei\u00b7er", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sind erbleicht und entschweben in Sturm,", "tokens": ["Sind", "er\u00b7bleicht", "und", "ent\u00b7schwe\u00b7ben", "in", "Sturm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "KON", "VVFIN", "APPR", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.7": {"text": "\u00bbmensch\u00ab nennen sich sie das tragische St\u00fcck,", "tokens": ["\u00bb", "mensch", "\u00ab", "nen\u00b7nen", "sich", "sie", "das", "tra\u00b7gi\u00b7sche", "St\u00fcck", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$(", "VVFIN", "PRF", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.8": {"text": "Seinen Helden \u00bbEroberer Wurm\u00ab.", "tokens": ["Sei\u00b7nen", "Hel\u00b7den", "\u00bb", "Er\u00b7o\u00b7be\u00b7rer", "Wurm", "\u00ab", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$(", "ADJA", "NN", "$(", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}}}}