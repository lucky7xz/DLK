{"dta.poem.7630": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich will den Winter durch die Kr\u00e4nze lassen hangen,", "tokens": ["Ich", "will", "den", "Win\u00b7ter", "durch", "die", "Kr\u00e4n\u00b7ze", "las\u00b7sen", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "ART", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die welken, bis im Lenz die Bl\u00fcten neu entsprangen;", "tokens": ["Die", "wel\u00b7ken", ",", "bis", "im", "Lenz", "die", "Bl\u00fc\u00b7ten", "neu", "ent\u00b7spran\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "APPRART", "NN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein Zeichen nicht allein der Freuden, die verbl\u00fcht,", "tokens": ["Ein", "Zei\u00b7chen", "nicht", "al\u00b7lein", "der", "Freu\u00b7den", ",", "die", "ver\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADV", "ART", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch k\u00fcnft'ger Unterpfand dem hoffenden Gem\u00fcth.", "tokens": ["Auch", "k\u00fcnft'\u00b7ger", "Un\u00b7ter\u00b7pfand", "dem", "hof\u00b7fen\u00b7den", "Ge\u00b7m\u00fcth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ART", "ADJA", "NN