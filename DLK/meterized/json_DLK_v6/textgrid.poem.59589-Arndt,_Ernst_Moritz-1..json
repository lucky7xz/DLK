{"textgrid.poem.59589": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hilf, Herr! Wir haben viel ges\u00fcndigt,", "tokens": ["Hilf", ",", "Herr", "!", "Wir", "ha\u00b7ben", "viel", "ge\u00b7s\u00fcn\u00b7digt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$.", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Drum dr\u00fcckt uns Schmach und Ungl\u00fcck schwer.", "tokens": ["Drum", "dr\u00fcckt", "uns", "Schmach", "und", "Un\u00b7gl\u00fcck", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein heilig Wort, das du verk\u00fcndigt,", "tokens": ["Dein", "hei\u00b7lig", "Wort", ",", "das", "du", "ver\u00b7k\u00fcn\u00b7digt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das kannten wir im Trug nicht mehr,", "tokens": ["Das", "kann\u00b7ten", "wir", "im", "Trug", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPRART", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des Glaubens s\u00fc\u00dfes Himmelslicht", "tokens": ["Des", "Glau\u00b7bens", "s\u00fc\u00b7\u00dfes", "Him\u00b7mels\u00b7licht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schien unsern blinden Herzen nicht.", "tokens": ["Schien", "un\u00b7sern", "blin\u00b7den", "Her\u00b7zen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Umnebelt waren wir von D\u00fcnsten,", "tokens": ["Um\u00b7ne\u00b7belt", "wa\u00b7ren", "wir", "von", "D\u00fcns\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "APPR", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Vom gauklisch bunten H\u00f6llenschein,", "tokens": ["Vom", "gau\u00b7klisch", "bun\u00b7ten", "H\u00f6l\u00b7len\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und spannen uns mit eitlen K\u00fcnsten", "tokens": ["Und", "span\u00b7nen", "uns", "mit", "eit\u00b7len", "K\u00fcns\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stets dichter in die L\u00fcge ein,", "tokens": ["Stets", "dich\u00b7ter", "in", "die", "L\u00fc\u00b7ge", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Leben schwankte ohne Ziel,", "tokens": ["Das", "Le\u00b7ben", "schwank\u00b7te", "oh\u00b7ne", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und jeder tat, was ihm gefiel.", "tokens": ["Und", "je\u00b7der", "tat", ",", "was", "ihm", "ge\u00b7fiel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die fromme Liebe war erkaltet,", "tokens": ["Die", "from\u00b7me", "Lie\u00b7be", "war", "er\u00b7kal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die stille Demut war dahin,", "tokens": ["Die", "stil\u00b7le", "De\u00b7mut", "war", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was droben auf den Sternen waltet,", "tokens": ["Was", "dro\u00b7ben", "auf", "den", "Ster\u00b7nen", "wal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erkannte nicht der tr\u00fcbe Sinn,", "tokens": ["Er\u00b7kann\u00b7te", "nicht", "der", "tr\u00fc\u00b7be", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von eigner Weisheit aufgebl\u00e4ht", "tokens": ["Von", "eig\u00b7ner", "Weis\u00b7heit", "auf\u00b7ge\u00b7bl\u00e4ht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verga\u00df er Gottes Majest\u00e4t.", "tokens": ["Ver\u00b7ga\u00df", "er", "Got\u00b7tes", "Ma\u00b7jes\u00b7t\u00e4t", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Drum liegen wir so tief darnieder,", "tokens": ["Drum", "lie\u00b7gen", "wir", "so", "tief", "dar\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ADJD", "PAV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Drum plagt uns fremde Tyrannei,", "tokens": ["Drum", "plagt", "uns", "frem\u00b7de", "Ty\u00b7ran\u00b7nei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df Gott der Herr mit Schrecken wieder", "tokens": ["Da\u00df", "Gott", "der", "Herr", "mit", "Schre\u00b7cken", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ART", "NN", "APPR", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gesuchet und gef\u00fcrchtet sei,", "tokens": ["Ge\u00b7su\u00b7chet", "und", "ge\u00b7f\u00fcrch\u00b7tet", "sei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df wir erkennen, wie wir sind", "tokens": ["Da\u00df", "wir", "er\u00b7ken\u00b7nen", ",", "wie", "wir", "sind"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVINF", "$,", "PWAV", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vor ihm wie Sand und Spreu im Wind.", "tokens": ["Vor", "ihm", "wie", "Sand", "und", "Spreu", "im", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KOKOM", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du H\u00f6chster in des Himmels H\u00f6hen,", "tokens": ["Du", "H\u00f6chs\u00b7ter", "in", "des", "Him\u00b7mels", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Name Huld und Gnade hei\u00dft,", "tokens": ["Des", "Na\u00b7me", "Huld", "und", "Gna\u00b7de", "hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O la\u00df uns doch nicht gar vergehen!", "tokens": ["O", "la\u00df", "uns", "doch", "nicht", "gar", "ver\u00b7ge\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O sende deinen treuen Geist", "tokens": ["O", "sen\u00b7de", "dei\u00b7nen", "treu\u00b7en", "Geist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Erleucht' uns mit des Glaubens Schein", "tokens": ["Er\u00b7leucht'", "uns", "mit", "des", "Glau\u00b7bens", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und hauch' uns deine Liebe ein!", "tokens": ["Und", "hauch'", "uns", "dei\u00b7ne", "Lie\u00b7be", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dann stehn wir wieder auf in Freuden,", "tokens": ["Dann", "stehn", "wir", "wie\u00b7der", "auf", "in", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dann kommt uns wieder Sieg und Gl\u00fcck,", "tokens": ["Dann", "kommt", "uns", "wie\u00b7der", "Sieg", "und", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann heben wir aus langen Leiden", "tokens": ["Dann", "he\u00b7ben", "wir", "aus", "lan\u00b7gen", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu dir empor den frohen Blick,", "tokens": ["Zu", "dir", "em\u00b7por", "den", "fro\u00b7hen", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann klingen und dann singen wir:", "tokens": ["Dann", "klin\u00b7gen", "und", "dann", "sin\u00b7gen", "wir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "KON", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}