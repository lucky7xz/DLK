{"dta.poem.19229": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "76.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sieh, wie der Dotter so im Wei\u00df des Eies schwimmt,", "tokens": ["Sieh", ",", "wie", "der", "Dot\u00b7ter", "so", "im", "Wei\u00df", "des", "Ei\u00b7es", "schwimmt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df, wie du's drehst, er stets die obre Stell' einnimmt.", "tokens": ["Da\u00df", ",", "wie", "du's", "drehst", ",", "er", "stets", "die", "ob\u00b7re", "Stell'", "ein\u00b7nimmt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "PIS", "VVFIN", "$,", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Er liegt im wei\u00dfen Meer geschaukelt an zwei schwanker", "tokens": ["Er", "liegt", "im", "wei\u00b7\u00dfen", "Meer", "ge\u00b7schau\u00b7kelt", "an", "zwei", "schwan\u00b7ker"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "VVPP", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch starker B\u00e4nder Kraft, alswie ein Schiff vor Anker.", "tokens": ["Doch", "star\u00b7ker", "B\u00e4n\u00b7der", "Kraft", ",", "als\u00b7wie", "ein", "Schiff", "vor", "An\u00b7ker", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "$,", "KOKOM", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein Schiff, das ganz und gar aus Proviant besteht,", "tokens": ["Ein", "Schiff", ",", "das", "ganz", "und", "gar", "aus", "Pro\u00b7vi\u00b7ant", "be\u00b7steht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "KON", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcr ein Lebendiges, das aus dem Keim entsteht.", "tokens": ["F\u00fcr", "ein", "Le\u00b7ben\u00b7di\u00b7ges", ",", "das", "aus", "dem", "Keim", "ent\u00b7steht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Der Keim, auf welcher Seit' im Nest das Ei auch liegt,", "tokens": ["Der", "Keim", ",", "auf", "wel\u00b7cher", "Seit'", "im", "Nest", "das", "Ei", "auch", "liegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PWAT", "NN", "APPRART", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bleibt immer n\u00e4chst der Brust, die ihn durchw\u00e4rmt, geschmiegt.", "tokens": ["Bleibt", "im\u00b7mer", "n\u00e4chst", "der", "Brust", ",", "die", "ihn", "durch\u00b7w\u00e4rmt", ",", "ge\u00b7schmiegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Er ist nicht tief ins Ei versenkt, um zu ersticken;", "tokens": ["Er", "ist", "nicht", "tief", "ins", "Ei", "ver\u00b7senkt", ",", "um", "zu", "er\u00b7sti\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "APPRART", "NN", "VVPP", "$,", "KOUI", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der m\u00fcrben Schale nah, darf nur das H\u00fcnlein picken.", "tokens": ["Der", "m\u00fcr\u00b7ben", "Scha\u00b7le", "nah", ",", "darf", "nur", "das", "H\u00fcn\u00b7lein", "pi\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,", "VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und h\u00f6rt die Mutter drin sein Picken zart und schwach,", "tokens": ["Und", "h\u00f6rt", "die", "Mut\u00b7ter", "drin", "sein", "Pi\u00b7cken", "zart", "und", "schwach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So hilft sie au\u00dfen mit dem Schnabel leise nach.", "tokens": ["So", "hilft", "sie", "au\u00b7\u00dfen", "mit", "dem", "Schna\u00b7bel", "lei\u00b7se", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wir liegen an der Brust der Liebe noch im Ei,", "tokens": ["Wir", "lie\u00b7gen", "an", "der", "Brust", "der", "Lie\u00b7be", "noch", "im", "Ei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und werden, wenn sie hilft, von m\u00fcrben Schalen frei.", "tokens": ["Und", "wer\u00b7den", ",", "wenn", "sie", "hilft", ",", "von", "m\u00fcr\u00b7ben", "Scha\u00b7len", "frei", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAINF", "$,", "KOUS", "PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}