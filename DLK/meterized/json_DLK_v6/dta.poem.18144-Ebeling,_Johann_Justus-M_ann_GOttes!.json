{"dta.poem.18144": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "M ann GOttes!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198805", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "z\u00fcrne nicht, da\u00df rege Dankbarkeit,", "tokens": ["z\u00fcr\u00b7ne", "nicht", ",", "da\u00df", "re\u00b7ge", "Dank\u00b7bar\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "KOUS", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Dir, dieses schlechte Buch, zu einen Denk-", "tokens": ["Dir", ",", "die\u00b7ses", "schlech\u00b7te", "Buch", ",", "zu", "ei\u00b7nen", "Denk"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "PDAT", "ADJA", "NN", "$,", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "mal weiht:", "tokens": ["mal", "weiht", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Es heischt es dein Verdienst, es forderns", "tokens": ["Es", "heischt", "es", "dein", "Ver\u00b7dienst", ",", "es", "for\u00b7derns"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "PPER", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "meine Pflichten,", "tokens": ["mei\u00b7ne", "Pflich\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Ein Angeld alter Schuld, dadurch noch zu", "tokens": ["Ein", "An\u00b7geld", "al\u00b7ter", "Schuld", ",", "da\u00b7durch", "noch", "zu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "PAV", "ADV", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "entrichten.", "tokens": ["ent\u00b7rich\u00b7ten", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Ich denke noch zur\u00fck an die verschwundne", "tokens": ["Ich", "den\u00b7ke", "noch", "zu\u00b7r\u00fck", "an", "die", "ver\u00b7schwund\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Zeit,", "tokens": ["Zeit", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.10": {"text": "Da mich Dein Unterricht in Hildesheim er-", "tokens": ["Da", "mich", "Dein", "Un\u00b7ter\u00b7richt", "in", "Hil\u00b7des\u00b7heim", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "NE", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "freut;", "tokens": ["freut", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.12": {"text": "Als Dich die Stadt damahls, noch gegen-", "tokens": ["Als", "Dich", "die", "Stadt", "da\u00b7mahls", ",", "noch", "ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "$,", "ADV", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "w\u00e4rtig ehrte,", "tokens": ["w\u00e4r\u00b7tig", "ehr\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.14": {"text": "Und mich mit Weisheits-Milch in ihrer", "tokens": ["Und", "mich", "mit", "Weis\u00b7heits\u00b7Milch", "in", "ih\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Schule n\u00e4hrte.", "tokens": ["Schu\u00b7le", "n\u00e4hr\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.16": {"text": "Da machte uns dein Fleis, durch einen g\u00fcld-", "tokens": ["Da", "mach\u00b7te", "uns", "dein", "Fleis", ",", "durch", "ei\u00b7nen", "g\u00fcld"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "nen Mund,", "tokens": ["nen", "Mund", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.18": {"text": "Aus Liebe blos ger\u00fchrt, die sch\u00f6nen S\u00e4zze", "tokens": ["Aus", "Lie\u00b7be", "blos", "ge\u00b7r\u00fchrt", ",", "die", "sch\u00f6\u00b7nen", "S\u00e4z\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "VVPP", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "kund", "tokens": ["kund"], "token_info": ["word"], "pos": ["PTKVZ"], "meter": "+", "measure": "single.up"}, "line.20": {"text": "Der Gottsgelehrsamkeit. Du zeigtest was", "tokens": ["Der", "Gotts\u00b7ge\u00b7lehr\u00b7sam\u00b7keit", ".", "Du", "zeig\u00b7test", "was"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "PPER", "VVFIN", "PWS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "zu gl\u00e4uben,", "tokens": ["zu", "gl\u00e4u\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.22": {"text": "Was ", "tokens": ["Was"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}, "line.23": {"text": "treiben.", "tokens": ["trei\u00b7ben", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.24": {"text": "Ob ich gleich damahls nicht dasselbe ganz ge-", "tokens": ["Ob", "ich", "gleich", "da\u00b7mahls", "nicht", "das\u00b7sel\u00b7be", "ganz", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PTKNEG", "PDAT", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "no\u00df,", "tokens": ["no\u00df", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.26": {"text": "Was als ein klarer Strom, von deinen Lip-", "tokens": ["Was", "als", "ein", "kla\u00b7rer", "Strom", ",", "von", "dei\u00b7nen", "Lip"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "KOKOM", "ART", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "pen flo\u00df;", "tokens": ["pen", "flo\u00df", ";"], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.28": {"text": "So lernete ich doch, was man vor War-", "tokens": ["So", "ler\u00b7ne\u00b7te", "ich", "doch", ",", "was", "man", "vor", "Wa\u00b7r"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PRELS", "PIS", "APPR", "TRUNC"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "heitslehren;", "tokens": ["heits\u00b7leh\u00b7ren", ";"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.30": {"text": "Wie man dieselbigen vom Lehrstuhl w\u00fcrde", "tokens": ["Wie", "man", "die\u00b7sel\u00b7bi\u00b7gen", "vom", "Lehr\u00b7stuhl", "w\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "PDAT", "APPRART", "NN", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "h\u00f6ren.", "tokens": ["h\u00f6\u00b7ren", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.32": {"text": "Den Grund hast ", "tokens": ["Den", "Grund", "hast"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VAFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.33": {"text": "fort gebaut,", "tokens": ["fort", "ge\u00b7baut", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKVZ", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.34": {"text": "Den ich als Lehrern bin, hernachmahls an-", "tokens": ["Den", "ich", "als", "Leh\u00b7rern", "bin", ",", "her\u00b7nach\u00b7mahls", "an"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "PPER", "KOUS", "NN", "VAFIN", "$,", "ADV", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "vertraut;", "tokens": ["ver\u00b7traut", ";"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "-+", "measure": "iambic.single"}, "line.36": {"text": "Du hast den Trieb entflammt durch Dein", "tokens": ["Du", "hast", "den", "Trieb", "ent\u00b7flammt", "durch", "Dein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVFIN", "APPR", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "beliebtes Wesen,", "tokens": ["be\u00b7lieb\u00b7tes", "We\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.38": {"text": "Der Warheit nachzugehn, zu forschen und", "tokens": ["Der", "War\u00b7heit", "nach\u00b7zu\u00b7gehn", ",", "zu", "for\u00b7schen", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "$,", "PTKZU", "VVINF", "KON"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "zu lesen,", "tokens": ["zu", "le\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.40": {"text": "Was hie die Weisheit spricht; was da der", "tokens": ["Was", "hie", "die", "Weis\u00b7heit", "spricht", ";", "was", "da", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ADV", "ART", "NN", "VVFIN", "$.", "PWS", "ADV", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Sp\u00f6tter sagt,", "tokens": ["Sp\u00f6t\u00b7ter", "sagt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.42": {"text": "Der bei den hellen Licht nach neuen Son-", "tokens": ["Der", "bei", "den", "hel\u00b7len", "Licht", "nach", "neu\u00b7en", "Son"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "nen fragt;", "tokens": ["nen", "fragt", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.44": {"text": "Du hast mich auch gelehrt, als Lehrer in", "tokens": ["Du", "hast", "mich", "auch", "ge\u00b7lehrt", ",", "als", "Leh\u00b7rer", "in"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,", "KOUS", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "dem Tempel,", "tokens": ["dem", "Tem\u00b7pel", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.46": {"text": "Jm h\u00f6ren mich erwekt, dem reizenden", "tokens": ["Jm", "h\u00f6\u00b7ren", "mich", "er\u00b7wekt", ",", "dem", "rei\u00b7zen\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "VVINF", "PPER", "VVPP", "$,", "ART", "ADJA"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.47": {"text": "Exempel", "tokens": ["Ex\u00b7em\u00b7pel"], "token_info": ["word"], "pos": ["NN"], "meter": "+--", "measure": "dactylic.init"}, "line.48": {"text": "Von ferne nachzugehn, da Deine Lieblich-", "tokens": ["Von", "fer\u00b7ne", "nach\u00b7zu\u00b7gehn", ",", "da", "Dei\u00b7ne", "Lieb\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADV", "VVINF", "$,", "KOUS", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.49": {"text": "keit", "tokens": ["keit"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.50": {"text": "Die Herzen stark ger\u00fchrt, den Saamen", "tokens": ["Die", "Her\u00b7zen", "stark", "ge\u00b7r\u00fchrt", ",", "den", "Saa\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVPP", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.51": {"text": "ausgestreut:", "tokens": ["aus\u00b7ge\u00b7streut", ":"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.52": {"text": "Du hast mich offt erwekt, durch Gr\u00fcnde", "tokens": ["Du", "hast", "mich", "offt", "er\u00b7wekt", ",", "durch", "Gr\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.53": {"text": "stark bewogen,", "tokens": ["stark", "be\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.54": {"text": "Da\u00df ich der Schl\u00fcpfrigkeit der Laster bin", "tokens": ["Da\u00df", "ich", "der", "Schl\u00fcpf\u00b7rig\u00b7keit", "der", "Las\u00b7ter", "bin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.55": {"text": "entzogen.", "tokens": ["ent\u00b7zo\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.56": {"text": "Die scheuche Bl\u00f6digkeit, die da mein Fehler", "tokens": ["Die", "scheu\u00b7che", "Bl\u00f6\u00b7dig\u00b7keit", ",", "die", "da", "mein", "Feh\u00b7ler"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.57": {"text": "war,", "tokens": ["war", ","], "token_info": ["word", "punct"], "pos": ["VAFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.58": {"text": "Als ich von hinnen zog, die stellt sich jezo", "tokens": ["Als", "ich", "von", "hin\u00b7nen", "zog", ",", "die", "stellt", "sich", "je\u00b7zo"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADV", "VVFIN", "$,", "PRELS", "VVFIN", "PRF", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.59": {"text": "dar,", "tokens": ["dar", ","], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$,"], "meter": "-", "measure": "single.down"}, "line.60": {"text": "Und r\u00fchmet \u00f6ffentlich die Unterweisungs-", "tokens": ["Und", "r\u00fch\u00b7met", "\u00f6f\u00b7fent\u00b7lich", "die", "Un\u00b7ter\u00b7wei\u00b7sungs"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ART", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.61": {"text": "stunden,", "tokens": ["stun\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["ADJA", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.62": {"text": "Die zeiget durch dies Blat, wie sehr ich", "tokens": ["Die", "zei\u00b7get", "durch", "dies", "Blat", ",", "wie", "sehr", "ich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "PDS", "NN", "$,", "PWAV", "ADV", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.63": {"text": "Dir verbunden.", "tokens": ["Dir", "ver\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.64": {"text": "Nim meine Dankbarkeit, ", "tokens": ["Nim", "mei\u00b7ne", "Dank\u00b7bar\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.65": {"text": "Gottesmann!", "tokens": ["Got\u00b7tes\u00b7mann", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.66": {"text": "Und dieses kleine Buch, als deren Zeugnis", "tokens": ["Und", "die\u00b7ses", "klei\u00b7ne", "Buch", ",", "als", "de\u00b7ren", "Zeug\u00b7nis"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PDAT", "ADJA", "NN", "$,", "KOUS", "PRELAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.67": {"text": "an!", "tokens": ["an", "!"], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "+", "measure": "single.up"}, "line.68": {"text": "Nim was die Ohnmacht kan, und was die", "tokens": 