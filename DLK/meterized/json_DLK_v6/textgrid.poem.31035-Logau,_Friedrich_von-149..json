{"textgrid.poem.31035": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "149.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einen Trostspruch au\u00df der Schrifft hatte Rasa ihr erwischet,", "tokens": ["Ei\u00b7nen", "Trost\u00b7spruch", "au\u00df", "der", "Schrifft", "hat\u00b7te", "Ra\u00b7sa", "ihr", "er\u00b7wi\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VAFIN", "NE", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Da\u00df man dort mit Abraham, Isaac, Jacob ewig tischet;", "tokens": ["Da\u00df", "man", "dort", "mit", "Ab\u00b7ra\u00b7ham", ",", "I\u00b7saac", ",", "Ja\u00b7cob", "e\u00b7wig", "ti\u00b7schet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "APPR", "NE", "$,", "NE", "$,", "NE", "ADJD", "VVFIN", "$."], "meter": "+-+-+--+-+--+--", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Freuet sich auff be\u00dfre Speisen, als man hier erjagt und fischet.", "tokens": ["Freu\u00b7et", "sich", "auff", "be\u00df\u00b7re", "Spei\u00b7sen", ",", "als", "man", "hier", "er\u00b7jagt", "und", "fi\u00b7schet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "$,", "KOUS", "PIS", "ADV", "VVPP", "KON", "VVFIN", "$."], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}}}}}