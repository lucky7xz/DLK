{"textgrid.poem.24956": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Lebenswonnen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf hohen Bergesgipfeln stehn,", "tokens": ["Auf", "ho\u00b7hen", "Ber\u00b7ges\u00b7gip\u00b7feln", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Einen geliebten Freund umschlingen,", "tokens": ["Ei\u00b7nen", "ge\u00b7lieb\u00b7ten", "Freund", "um\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Hinauf zu den Wolken jubeln und singen", "tokens": ["Hin\u00b7auf", "zu", "den", "Wol\u00b7ken", "ju\u00b7beln", "und", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "KON", "VVINF"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und hinab zu den Th\u00e4lern und Seen;", "tokens": ["Und", "hin\u00b7ab", "zu", "den", "Th\u00e4\u00b7lern", "und", "Seen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "KON", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Einander im seligen Taumel schw\u00f6ren,", "tokens": ["Ein\u00b7an\u00b7der", "im", "se\u00b7li\u00b7gen", "Tau\u00b7mel", "schw\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Sich in Leben und Tod zu geh\u00f6ren,", "tokens": ["Sich", "in", "Le\u00b7ben", "und", "Tod", "zu", "ge\u00b7h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Gro\u00dfe Thaten dereinst zu vollbringen", "tokens": ["Gro\u00b7\u00dfe", "Tha\u00b7ten", "de\u00b7reinst", "zu", "voll\u00b7brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADV", "PTKZU", "VVINF"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Oder im Ringen unterzugehn;", "tokens": ["O\u00b7der", "im", "Rin\u00b7gen", "un\u00b7ter\u00b7zu\u00b7gehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Im leichten sturmgeschaukelten Boot", "tokens": ["Im", "leich\u00b7ten", "sturm\u00b7ge\u00b7schau\u00b7kel\u00b7ten", "Boot"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ueber das Meer dahingetrieben,", "tokens": ["Ue\u00b7ber", "das", "Meer", "da\u00b7hin\u00b7ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PAV", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Mit der einen, die wir lieben,", "tokens": ["Mit", "der", "ei\u00b7nen", ",", "die", "wir", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIS", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ruhen beim flammenden Abendrot;", "tokens": ["Ru\u00b7hen", "beim", "flam\u00b7men\u00b7den", "A\u00b7ben\u00b7drot", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Lippen und Herz aneinander pressen", "tokens": ["Lip\u00b7pen", "und", "Herz", "an\u00b7ein\u00b7an\u00b7der", "pres\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Und, der Erd' und des Lebens vergessen,", "tokens": ["Und", ",", "der", "Erd'", "und", "des", "Le\u00b7bens", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ART", "NN", "KON", "ART", "NN", "VVPP", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Durch der Wellen Sch\u00e4umen und Stieben", "tokens": ["Durch", "der", "Wel\u00b7len", "Sch\u00e4u\u00b7men", "und", "Stie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Entgegenjauchzen dem leuchtenden Tod;", "tokens": ["Ent\u00b7ge\u00b7gen\u00b7jauch\u00b7zen", "dem", "leuch\u00b7ten\u00b7den", "Tod", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Nachts sich unter dem Sternenzelt", "tokens": ["Nachts", "sich", "un\u00b7ter", "dem", "Ster\u00b7nen\u00b7zelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PRF", "APPR", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "In dem wogenden Lichtglanz sonnen,", "tokens": ["In", "dem", "wo\u00b7gen\u00b7den", "Licht\u00b7glanz", "son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Der aus dem unergr\u00fcndlichen Bronnen", "tokens": ["Der", "aus", "dem", "un\u00b7er\u00b7gr\u00fcnd\u00b7li\u00b7chen", "Bron\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Ewigkeit niederschauert und -f\u00e4llt,", "tokens": ["Der", "E\u00b7wig\u00b7keit", "nie\u00b7der\u00b7schau\u00b7ert", "und", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "NE", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Bis die Seele im trunknen Gesichte", "tokens": ["Bis", "die", "See\u00b7le", "im", "trunk\u00b7nen", "Ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Eins sich f\u00fchlt mit dem ewigen Lichte \u2013", "tokens": ["Eins", "sich", "f\u00fchlt", "mit", "dem", "e\u00b7wi\u00b7gen", "Lich\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PRF", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.7": {"text": "O wie schwindet nach solchen Wonnen", "tokens": ["O", "wie", "schwin\u00b7det", "nach", "sol\u00b7chen", "Won\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "VVFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Alle Freude und Gr\u00f6\u00dfe der Welt!", "tokens": ["Al\u00b7le", "Freu\u00b7de", "und", "Gr\u00f6\u00b7\u00dfe", "der", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}}}}