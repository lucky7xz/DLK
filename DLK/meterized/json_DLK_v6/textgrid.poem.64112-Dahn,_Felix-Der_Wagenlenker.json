{"textgrid.poem.64112": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Der Wagenlenker", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "R\u00e4dergeprassel und Rossegestampf,", "tokens": ["R\u00e4\u00b7der\u00b7ge\u00b7pras\u00b7sel", "und", "Ros\u00b7se\u00b7ge\u00b7stampf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Hengstegewieher und st\u00e4ubender Dampf,", "tokens": ["Hengs\u00b7te\u00b7ge\u00b7wie\u00b7her", "und", "st\u00e4u\u00b7ben\u00b7der", "Dampf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Wolken von Sand und Peitschengeknall,", "tokens": ["Wol\u00b7ken", "von", "Sand", "und", "Peit\u00b7schen\u00b7ge\u00b7knall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Tr\u00fcmmernder R\u00e4der erkrachender Prall,", "tokens": ["Tr\u00fcm\u00b7mern\u00b7der", "R\u00e4\u00b7der", "er\u00b7kra\u00b7chen\u00b7der", "Prall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Tobender R\u00f6mer verworren Geschrei:", "tokens": ["To\u00b7ben\u00b7der", "R\u00f6\u00b7mer", "ver\u00b7wor\u00b7ren", "Ge\u00b7schrei", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "\u00bbhei, der Gr\u00fcne verliert, rasch, Blauer, vorbei!\u00ab", "tokens": ["\u00bb", "hei", ",", "der", "Gr\u00fc\u00b7ne", "ver\u00b7liert", ",", "rasch", ",", "Blau\u00b7er", ",", "vor\u00b7bei", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "PTKVZ", "$,", "ART", "NN", "VVFIN", "$,", "ADJD", "$,", "NN", "$,", "PTKVZ", "$.", "$("], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Also erdr\u00f6hnt es im Hippodrom:", "tokens": ["Al\u00b7so", "er\u00b7dr\u00f6hnt", "es", "im", "Hip\u00b7po\u00b7drom", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Denn neue Triumphe feiert Rom.", "tokens": ["Denn", "neu\u00b7e", "Tri\u00b7um\u00b7phe", "fei\u00b7ert", "Rom", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "NE", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Und vor allen Quadrigen sauset verwegen", "tokens": ["Und", "vor", "al\u00b7len", "Quad\u00b7ri\u00b7gen", "sau\u00b7set", "ver\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN", "VVFIN", "VVINF"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die eine dem Ziele, dem fernen, entgegen:", "tokens": ["Die", "ei\u00b7ne", "dem", "Zie\u00b7le", ",", "dem", "fer\u00b7nen", ",", "ent\u00b7ge\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ART", "ART", "NN", "$,", "ART", "ADJA", "$,", "PTKVZ", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ein J\u00fcngling lenkt sie in keltischem Rock,", "tokens": ["Ein", "J\u00fcng\u00b7ling", "lenkt", "sie", "in", "kel\u00b7ti\u00b7schem", "Rock", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Kaum birgt ihm das Helmdach das gelbe Gelock.", "tokens": ["Kaum", "birgt", "ihm", "das", "Helm\u00b7dach", "das", "gel\u00b7be", "Ge\u00b7lock", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "In rasendem Rennen, verachtend den Tod,", "tokens": ["In", "ra\u00b7sen\u00b7dem", "Ren\u00b7nen", ",", "ver\u00b7ach\u00b7tend", "den", "Tod", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVPP", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Den gewissen, welcher dem St\u00fcrzenden droht,", "tokens": ["Den", "ge\u00b7wis\u00b7sen", ",", "wel\u00b7cher", "dem", "St\u00fcr\u00b7zen\u00b7den", "droht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Hetzt er die Tiere mit gellendem Schrei", "tokens": ["Hetzt", "er", "die", "Tie\u00b7re", "mit", "gel\u00b7len\u00b7dem", "Schrei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.8": {"text": "W\u00fctend an allen Gespannen vorbei.", "tokens": ["W\u00fc\u00b7tend", "an", "al\u00b7len", "Ge\u00b7span\u00b7nen", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.9": {"text": "Auf den Fl\u00fcgeln des Sturmwinds scheint er zu jagen,", "tokens": ["Auf", "den", "Fl\u00fc\u00b7geln", "des", "Sturm\u00b7winds", "scheint", "er", "zu", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Und von tausend jauchzenden Stimmen getragen,", "tokens": ["Und", "von", "tau\u00b7send", "jauch\u00b7zen\u00b7den", "Stim\u00b7men", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "CARD", "ADJA", "NN", "VVPP", "$,"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Erreicht der Sieger des C\u00e4sars Thron", "tokens": ["Er\u00b7reicht", "der", "Sie\u00b7ger", "des", "C\u00e4\u00b7sars", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ART", "NN", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Und schaut zu ihm auf mit stolzem Droh'n.", "tokens": ["Und", "schaut", "zu", "ihm", "auf", "mit", "stol\u00b7zem", "Droh'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "VVFIN", "APPR", "PPER", "APPR", "APPR", "ADJA", "NN", "NE"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Der aber beginnt mit heiserem Ton:", "tokens": ["Der", "a\u00b7ber", "be\u00b7ginnt", "mit", "hei\u00b7se\u00b7rem", "Ton", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbf\u00fcrwahr, ich lobe die Fahrt, mein Sohn! \u2013", "tokens": ["\u00bb", "f\u00fcr\u00b7wahr", ",", "ich", "lo\u00b7be", "die", "Fahrt", ",", "mein", "Sohn", "!", "\u2013"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "$,", "PPER", "VVFIN", "ART", "NN", "$,", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Doch sage, was sprachst du, was riefst du dabei?", "tokens": ["Doch", "sa\u00b7ge", ",", "was", "sprachst", "du", ",", "was", "riefst", "du", "da\u00b7bei", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "VVFIN", "PPER", "$,", "PWS", "VVFIN", "PPER", "PAV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Mir klang es im Ohr wie Schlachtgeschrei!", "tokens": ["Mir", "klang", "es", "im", "Ohr", "wie", "Schlacht\u00b7ge\u00b7schrei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "KOKOM", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und du standest im Wagen so trotzig k\u00fchn: \u2013", "tokens": ["Und", "du", "stan\u00b7dest", "im", "Wa\u00b7gen", "so", "trot\u00b7zig", "k\u00fchn", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "ADV", "ADJD", "ADJD", "$.", "$("], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Blaufeuer sah ich vom Aug' dir spr\u00fchn!", "tokens": ["Blau\u00b7feu\u00b7er", "sah", "ich", "vom", "Aug'", "dir", "spr\u00fchn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Was hast du gedacht bei der rasenden Fahrt?", "tokens": ["Was", "hast", "du", "ge\u00b7dacht", "bei", "der", "ra\u00b7sen\u00b7den", "Fahrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Sprich frei, dein Leben sei dir gewahrt.\u00ab", "tokens": ["Sprich", "frei", ",", "dein", "Le\u00b7ben", "sei", "dir", "ge\u00b7wahrt", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$.", "$("], "meter": "++-+-+--+", "measure": "iambic.penta.chol"}, "line.9": {"text": "Da warf in den Nacken der J\u00fcngling das Haupt:", "tokens": ["Da", "warf", "in", "den", "Na\u00b7cken", "der", "J\u00fcng\u00b7ling", "das", "Haupt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.10": {"text": "\u00bbin der Heimat hab' ich mich wieder geglaubt!", "tokens": ["\u00bb", "in", "der", "Hei\u00b7mat", "hab'", "ich", "mich", "wie\u00b7der", "ge\u00b7glaubt", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "VAFIN", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Auf Kaledoniens waldigen Heiden,", "tokens": ["Auf", "Ka\u00b7le\u00b7do\u00b7ni\u00b7ens", "wal\u00b7di\u00b7gen", "Hei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Wo mir hundert Hengste, herrliche, weiden:", "tokens": ["Wo", "mir", "hun\u00b7dert", "Hengs\u00b7te", ",", "herr\u00b7li\u00b7che", ",", "wei\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "CARD", "NN", "$,", "ADJA", "$,", "VVINF", "$."], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Un der Deva M\u00fcndung scholl wieder der Kampf:", "tokens": ["Un", "der", "De\u00b7va", "M\u00fcn\u00b7dung", "scholl", "wie\u00b7der", "der", "Kampf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NE", "NN", "ADJD", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.14": {"text": "Hei, Speergekrach und Rossegestampf!", "tokens": ["Hei", ",", "Speer\u00b7ge\u00b7krach", "und", "Ros\u00b7se\u00b7ge\u00b7stampf", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.15": {"text": "Ich lenkte des Vaters Sichelwagen.", "tokens": ["Ich", "lenk\u00b7te", "des", "Va\u00b7ters", "Si\u00b7chel\u00b7wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "O, mein K\u00f6nig, mein Vater, welch' freudig Jagen!", "tokens": ["O", ",", "mein", "K\u00f6\u00b7nig", ",", "mein", "Va\u00b7ter", ",", "welch'", "freu\u00b7dig", "Ja\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PWAT", "ADJA", "NN", "$."], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Die Adler fall'n! Das Legionenheer,", "tokens": ["Die", "Ad\u00b7ler", "fall'n", "!", "Das", "Le\u00b7gi\u00b7o\u00b7nen\u00b7heer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir hetzen es jubelnd ins heilige Meer!", "tokens": ["Wir", "het\u00b7zen", "es", "ju\u00b7belnd", "ins", "hei\u00b7li\u00b7ge", "Meer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVPP", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Dort flieht er! Er will erreichen das Schiff!", "tokens": ["Dort", "flieht", "er", "!", "Er", "will", "er\u00b7rei\u00b7chen", "das", "Schiff", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "PPER", "VMFIN", "VVINF", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Nach! Nach! Wir sind vor ihm auf dem Riff!", "tokens": ["Nach", "!", "Nach", "!", "Wir", "sind", "vor", "ihm", "auf", "dem", "Riff", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "APPR", "$.", "PPER", "VAFIN", "APPR", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Greift aus, ihr Rappen! Wir m\u00fcssen ihn fah'n,", "tokens": ["Greift", "aus", ",", "ihr", "Rap\u00b7pen", "!", "Wir", "m\u00fcs\u00b7sen", "ihn", "fah'n", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPOSAT", "NN", "$.", "PPER", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Den feigen Tyrannen Domitian!", "tokens": ["Den", "fei\u00b7gen", "Ty\u00b7ran\u00b7nen", "Do\u00b7mi\u00b7ti\u00b7an", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Ha, zu Ende der Traum und das Gl\u00fcck und der Wahn!", "tokens": ["Ha", ",", "zu", "En\u00b7de", "der", "Traum", "und", "das", "Gl\u00fcck", "und", "der", "Wahn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "NN", "ART", "NN", "KON", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Mein Leben? \u2013 Von dir nicht will ich's geschenkt,", "tokens": ["Mein", "Le\u00b7ben", "?", "\u2013", "Von", "dir", "nicht", "will", "ich's", "ge\u00b7schenkt", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "$(", "APPR", "PPER", "PTKNEG", "VMFIN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Doch ihr, Br\u00fcder daheim: \u2013 der Rache gedenkt!\u00ab", "tokens": ["Doch", "ihr", ",", "Br\u00fc\u00b7der", "da\u00b7heim", ":", "\u2013", "der", "Ra\u00b7che", "ge\u00b7denkt", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "$,", "NN", "PTKVZ", "$.", "$(", "ART", "NN", "VVPP", "$.", "$("], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Und den Dolch in die Brust vor des C\u00e4sars Thron", "tokens": ["Und", "den", "Dolch", "in", "die", "Brust", "vor", "des", "C\u00e4\u00b7sars", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "APPR", "ART", "NN", "NN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.12": {"text": "Sich stie\u00df der gefangene K\u00f6nigssohn. \u2013", "tokens": ["Sich", "stie\u00df", "der", "ge\u00b7fan\u00b7ge\u00b7ne", "K\u00f6\u00b7nigs\u00b7sohn", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PRF", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}