{"textgrid.poem.33035": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich habe N\u00e4chte daf\u00fcr geopfert,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe N\u00e4chte daf\u00fcr geopfert,", "tokens": ["Ich", "ha\u00b7be", "N\u00e4ch\u00b7te", "da\u00b7f\u00fcr", "ge\u00b7o\u00b7pfert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "ich habe Herzblut daran gegeben,", "tokens": ["ich", "ha\u00b7be", "Herz\u00b7blut", "da\u00b7ran", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "PAV", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "und feige Buben nun kommen und heben", "tokens": ["und", "fei\u00b7ge", "Bu\u00b7ben", "nun", "kom\u00b7men", "und", "he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "ADV", "VVINF", "KON", "VVINF"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "die Hand auf gegen das fertige Werk.", "tokens": ["die", "Hand", "auf", "ge\u00b7gen", "das", "fer\u00b7ti\u00b7ge", "Werk", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "\u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}}, "stanza.3": {"line.1": {"text": "Zerschlagt, zerschlagt es! wenn ihr k\u00f6nnt!", "tokens": ["Zer\u00b7schlagt", ",", "zer\u00b7schlagt", "es", "!", "wenn", "ihr", "k\u00f6nnt", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "$.", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ich glaub es nicht! jedoch ... zerschlagt's!", "tokens": ["ich", "glaub", "es", "nicht", "!", "je\u00b7doch", "...", "zer\u00b7schlagt's", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$.", "ADV", "$(", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ich steh und seh euch zu und lach!", "tokens": ["ich", "steh", "und", "seh", "euch", "zu", "und", "lach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und wenn ihr Riesen oder sonst was w\u00e4rt ...", "tokens": ["Und", "wenn", "ihr", "Rie\u00b7sen", "o\u00b7der", "sonst", "was", "w\u00e4rt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "KON", "ADV", "PWS", "VAFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "was es mir gab und war, indem es wurde,", "tokens": ["was", "es", "mir", "gab", "und", "war", ",", "in\u00b7dem", "es", "wur\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVFIN", "KON", "VAFIN", "$,", "KOUS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "das zu zerst\u00f6ren, seid ihr doch zu schwach!", "tokens": ["das", "zu", "zer\u00b7st\u00f6\u00b7ren", ",", "seid", "ihr", "doch", "zu", "schwach", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKZU", "VVINF", "$,", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}