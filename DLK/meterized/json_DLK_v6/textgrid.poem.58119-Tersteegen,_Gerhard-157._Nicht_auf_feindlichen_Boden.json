{"textgrid.poem.58119": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "157. Nicht auf feindlichen Boden", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welt, Sinne und Vernunft \u2013 drin wirken Satanskr\u00e4fte,", "tokens": ["Welt", ",", "Sin\u00b7ne", "und", "Ver\u00b7nunft", "\u2013", "drin", "wir\u00b7ken", "Sa\u00b7tans\u00b7kr\u00e4f\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$(", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gehst du zu viel hinein, so bist du in Gefahr.", "tokens": ["Gehst", "du", "zu", "viel", "hin\u00b7ein", ",", "so", "bist", "du", "in", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKA", "PIS", "PTKVZ", "$,", "ADV", "VAFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bleib immer eingekehrt, auch selber beim Gesch\u00e4fte;", "tokens": ["Bleib", "im\u00b7mer", "ein\u00b7ge\u00b7kehrt", ",", "auch", "sel\u00b7ber", "beim", "Ge\u00b7sch\u00e4f\u00b7te", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVPP", "$,", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Im Seelengrund, in Gott, wird dich kein Feind gewahr!", "tokens": ["Im", "See\u00b7len\u00b7grund", ",", "in", "Gott", ",", "wird", "dich", "kein", "Feind", "ge\u00b7wahr", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "NN", "$,", "VAFIN", "PPER", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}