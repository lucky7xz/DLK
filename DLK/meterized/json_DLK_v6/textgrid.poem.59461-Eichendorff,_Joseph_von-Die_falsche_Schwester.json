{"textgrid.poem.59461": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die falsche Schwester", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine Schwester, die spielt' an der Linde \u2013", "tokens": ["Mei\u00b7ne", "Schwes\u00b7ter", ",", "die", "spielt'", "an", "der", "Lin\u00b7de", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "VVFIN", "APPR", "ART", "NE", "$("], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Stille Zeit, wie so weit, so weit!", "tokens": ["Stil\u00b7le", "Zeit", ",", "wie", "so", "weit", ",", "so", "weit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PWAV", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Da spielten so sch\u00f6ne Kinder", "tokens": ["Da", "spiel\u00b7ten", "so", "sch\u00f6\u00b7ne", "Kin\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mit ihr in der Einsamkeit.", "tokens": ["Mit", "ihr", "in", "der", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Von ihren Locken verhangen", "tokens": ["Von", "ih\u00b7ren", "Lo\u00b7cken", "ver\u00b7han\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Schlief sie und lachte im Traum,", "tokens": ["Schlief", "sie", "und", "lach\u00b7te", "im", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Und die sch\u00f6nen Kinder sangen", "tokens": ["Und", "die", "sch\u00f6\u00b7nen", "Kin\u00b7der", "san\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die ganze Nacht unterm Baum.", "tokens": ["Die", "gan\u00b7ze", "Nacht", "un\u00b7term", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Die ganze Nacht hat gelogen,", "tokens": ["Die", "gan\u00b7ze", "Nacht", "hat", "ge\u00b7lo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sie hat mich so falsch gegr\u00fc\u00dft,", "tokens": ["Sie", "hat", "mich", "so", "falsch", "ge\u00b7gr\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Engel sind fortgeflogen,", "tokens": ["Die", "En\u00b7gel", "sind", "fort\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und Haus und Garten stehn w\u00fcst.", "tokens": ["Und", "Haus", "und", "Gar\u00b7ten", "stehn", "w\u00fcst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Es zittert die alte Linde", "tokens": ["Es", "zit\u00b7tert", "die", "al\u00b7te", "Lin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NE"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und klaget der Wind so schwer,", "tokens": ["Und", "kla\u00b7get", "der", "Wind", "so", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das macht, das macht die S\u00fcnde \u2013", "tokens": ["Das", "macht", ",", "das", "macht", "die", "S\u00fcn\u00b7de", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PDS", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich wollt, ich l\u00e4g im Meer!", "tokens": ["Ich", "wollt", ",", "ich", "l\u00e4g", "im", "Meer", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Sonne ist untergegangen", "tokens": ["Die", "Son\u00b7ne", "ist", "un\u00b7ter\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und der Mond im tiefen Meer,", "tokens": ["Und", "der", "Mond", "im", "tie\u00b7fen", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Es dunkelt schon \u00fcber dem Lande,", "tokens": ["Es", "dun\u00b7kelt", "schon", "\u00fc\u00b7ber", "dem", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Gute Nacht! seh dich nimmermehr!", "tokens": ["Gu\u00b7te", "Nacht", "!", "seh", "dich", "nim\u00b7mer\u00b7mehr", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VVFIN", "PPER", "ADV", "$."], "meter": "+-++-+-+", "measure": "unknown.measure.penta"}}}}}