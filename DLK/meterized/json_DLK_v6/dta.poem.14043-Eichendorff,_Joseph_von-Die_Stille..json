{"dta.poem.14043": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Stille.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Es wei\u00df und r\u00e4th es doch Keiner,", "tokens": ["Es", "wei\u00df", "und", "r\u00e4\u00b7th", "es", "doch", "Kei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie mir so wohl ist, so wohl!", "tokens": ["Wie", "mir", "so", "wohl", "ist", ",", "so", "wohl", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "VAFIN", "$,", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, w\u00fc\u00dft' es nur Einer, nur Einer,", "tokens": ["Ach", ",", "w\u00fc\u00dft'", "es", "nur", "Ei\u00b7ner", ",", "nur", "Ei\u00b7ner", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "ADV", "PIS", "$,", "ADV", "PIS", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Kein Mensch es sonst wissen soll!", "tokens": ["Kein", "Mensch", "es", "sonst", "wis\u00b7sen", "soll", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}