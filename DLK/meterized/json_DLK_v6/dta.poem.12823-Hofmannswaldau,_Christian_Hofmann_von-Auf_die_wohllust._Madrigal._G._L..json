{"dta.poem.12823": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf die wohllust.  \n Madrigal.  \n G. L.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich seh' den leim der wohllust liegen,", "tokens": ["Ich", "seh'", "den", "leim", "der", "wohl\u00b7lust", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch es gef\u00e4llt mir nicht", "tokens": ["Doch", "es", "ge\u00b7f\u00e4llt", "mir", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf einen ast zu fliegen,", "tokens": ["Auf", "ei\u00b7nen", "ast", "zu", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo mir die feder h\u00e4ngen bleibt.", "tokens": ["Wo", "mir", "die", "fe\u00b7der", "h\u00e4n\u00b7gen", "bleibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich wei\u00df schon, wie die kreide schreibt.", "tokens": ["Ich", "wei\u00df", "schon", ",", "wie", "die", "krei\u00b7de", "schreibt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie mancher, der sich bey der jagd", "tokens": ["Wie", "man\u00b7cher", ",", "der", "sich", "bey", "der", "jagd"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "$,", "PRELS", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zu tief in einen pusch gewagt,", "tokens": ["Zu", "tief", "in", "ei\u00b7nen", "pusch", "ge\u00b7wagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "APPR", "ART", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hat seine grentzen \u00fcberschritten", "tokens": ["Hat", "sei\u00b7ne", "grent\u00b7zen", "\u00fc\u00b7bersc\u00b7hrit\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und ist lahm wieder heim geritten.", "tokens": ["Und", "ist", "lahm", "wie\u00b7der", "heim", "ge\u00b7rit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVFIN", "ADV", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}