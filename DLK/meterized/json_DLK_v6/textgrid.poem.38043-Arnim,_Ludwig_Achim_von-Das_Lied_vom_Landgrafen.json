{"textgrid.poem.38043": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Das Lied vom Landgrafen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu singen will ich fangen an,", "tokens": ["Zu", "sin\u00b7gen", "will", "ich", "fan\u00b7gen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VMFIN", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Lob der Kayserlichen Kron,", "tokens": ["Zum", "Lob", "der", "Kay\u00b7ser\u00b7li\u00b7chen", "Kron", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem Landgrafen zu Leide,", "tokens": ["Dem", "Land\u00b7gra\u00b7fen", "zu", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.4": {"text": "Wie es ihm dann ergangen ist", "tokens": ["Wie", "es", "ihm", "dann", "er\u00b7gan\u00b7gen", "ist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPER", "ADV", "VVPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vor Ingolstadt, in kurzer Frist,", "tokens": ["Vor", "In\u00b7gol\u00b7stadt", ",", "in", "kur\u00b7zer", "Frist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das machte uns viel Freude.", "tokens": ["Das", "mach\u00b7te", "uns", "viel", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "An einem sch\u00f6nen Morgen fruh,", "tokens": ["An", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Mor\u00b7gen", "fruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Landgraf r\u00fcckte schnell herzu,", "tokens": ["Der", "Land\u00b7graf", "r\u00fcck\u00b7te", "schnell", "her\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Lager th\u00e4t er schlagen", "tokens": ["Sein", "La\u00b7ger", "th\u00e4t", "er", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In weitem Feld vor Ingolstadt,", "tokens": ["In", "wei\u00b7tem", "Feld", "vor", "In\u00b7gol\u00b7stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er meint der R\u00f6misch Kaiser drat", "tokens": ["Er", "meint", "der", "R\u00f6\u00b7misch", "Kai\u00b7ser", "drat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "W\u00fcrd ihn von Stund an fliehen.", "tokens": ["W\u00fcrd", "ihn", "von", "Stund", "an", "flie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "APPR", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Zu morgen hub er zu schiessen an", "tokens": ["Zu", "mor\u00b7gen", "hub", "er", "zu", "schies\u00b7sen", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "PTKZU", "VVINF", "APPR"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wol \u00fcber die Kaiserlichen Kron,", "tokens": ["Wol", "\u00fc\u00b7ber", "die", "Kai\u00b7ser\u00b7li\u00b7chen", "Kron", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Mit Kartaunen und Schlangen,", "tokens": ["Mit", "Kar\u00b7tau\u00b7nen", "und", "Schlan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Das trieb er wohl drey ganze Tag,", "tokens": ["Das", "trieb", "er", "wohl", "drey", "gan\u00b7ze", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die weil er dann vor Ingolstadt lag,", "tokens": ["Die", "weil", "er", "dann", "vor", "In\u00b7gol\u00b7stadt", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "PPER", "ADV", "APPR", "NE", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Der Schimpf der wolt sich machen.", "tokens": ["Der", "Schimpf", "der", "wolt", "sich", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "So will ich auch nicht grausen schon!", "tokens": ["So", "will", "ich", "auch", "nicht", "grau\u00b7sen", "schon", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PTKNEG", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da sprach die Kaiserliche Kron,", "tokens": ["Da", "sprach", "die", "Kai\u00b7ser\u00b7li\u00b7che", "Kron", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Meins Ungl\u00fccks mu\u00df ich lachen;", "tokens": ["Meins", "Un\u00b7gl\u00fccks", "mu\u00df", "ich", "la\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schie\u00df her, schie\u00df her, lieber Landgraf,", "tokens": ["Schie\u00df", "her", ",", "schie\u00df", "her", ",", "lie\u00b7ber", "Land\u00b7graf", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "ADV", "NE", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Mein Gl\u00fcck das steht in Gotteskraft,", "tokens": ["Mein", "Gl\u00fcck", "das", "steht", "in", "Got\u00b7tes\u00b7kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDS", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erst wolln wir tapfer fechten.", "tokens": ["Erst", "wolln", "wir", "tap\u00b7fer", "fech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der Kaiser die ganze Schanz ausreit,", "tokens": ["Der", "Kai\u00b7ser", "die", "gan\u00b7ze", "Schanz", "aus\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der B\u00fcchsen-Meister in kurzer Zeit", "tokens": ["Der", "B\u00fcch\u00b7sen\u00b7Meis\u00b7ter", "in", "kur\u00b7zer", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Th\u00e4t da gar tapfer schiessen,", "tokens": ["Th\u00e4t", "da", "gar", "tap\u00b7fer", "schies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wol unter die Landgr\u00e4fischen Reiter gut,", "tokens": ["Wol", "un\u00b7ter", "die", "Land\u00b7gr\u00e4\u00b7fi\u00b7schen", "Rei\u00b7ter", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "+---+-+-+-+", "measure": "dactylic.init"}, "line.5": {"text": "Sie schossen hinaus mit frischem Muth,", "tokens": ["Sie", "schos\u00b7sen", "hin\u00b7aus", "mit", "fri\u00b7schem", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.6": {"text": "Es th\u00e4t sie sehr verdriessen.", "tokens": ["Es", "th\u00e4t", "sie", "sehr", "ver\u00b7dries\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbo Ingolstadt du gemauert Hau\u00df,", "tokens": ["\u00bb", "o", "In\u00b7gol\u00b7stadt", "du", "ge\u00b7mau\u00b7ert", "Hau\u00df", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "FM", "PPER", "VVPP", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Das h\u00e4tt ich dir doch nit vertraut,", "tokens": ["Das", "h\u00e4tt", "ich", "dir", "doch", "nit", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PPER", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das du zu mir h\u00e4tst geschossen.\u00ab", "tokens": ["Das", "du", "zu", "mir", "h\u00e4tst", "ge\u00b7schos\u00b7sen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "VAFIN", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So sprach der Landgraff zum Schertel gut,", "tokens": ["So", "sprach", "der", "Land\u00b7graff", "zum", "Scher\u00b7tel", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "\u00bbdie Stadt ist uns nit wolgemut,", "tokens": ["\u00bb", "die", "Stadt", "ist", "uns", "nit", "wol\u00b7ge\u00b7mut", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wir wollen nicht darauf bauen.\u00ab", "tokens": ["Wir", "wol\u00b7len", "nicht", "da\u00b7rauf", "bau\u00b7en", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "PAV", "VVINF", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Der Landgraf warf die Augen auf,", "tokens": ["Der", "Land\u00b7graf", "warf", "die", "Au\u00b7gen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus mancher B\u00fcchse gieng der Rauch,", "tokens": ["Aus", "man\u00b7cher", "B\u00fcch\u00b7se", "gieng", "der", "Rauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich h\u00f6r das man thut schiessen;", "tokens": ["Ich", "h\u00f6r", "das", "man", "thut", "schies\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PIS", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da sprach der Landgraf zum Schertel gewandt:", "tokens": ["Da", "sprach", "der", "Land\u00b7graf", "zum", "Scher\u00b7tel", "ge\u00b7wandt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "\u00bbwir verschiessen Leut, Ehr und Land,", "tokens": ["\u00bb", "wir", "ver\u00b7schies\u00b7sen", "Leut", ",", "Ehr", "und", "Land", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADJA", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Nit l\u00e4nger w\u00f6lln wir harren.\u00ab", "tokens": ["Nit", "l\u00e4n\u00b7ger", "w\u00f6lln", "wir", "har\u00b7ren", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "ADJD", "VVFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Der Kaiser sprach die Deutschen an,", "tokens": ["Der", "Kai\u00b7ser", "sprach", "die", "Deut\u00b7schen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verhie\u00df ihn auch bey seiner Kron,", "tokens": ["Ver\u00b7hie\u00df", "ihn", "auch", "bey", "sei\u00b7ner", "Kron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von hier wollt er nit weichen,", "tokens": ["Von", "hier", "wollt", "er", "nit", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Dieweil ihm Gott das Leben leiht,", "tokens": ["Die\u00b7weil", "ihm", "Gott", "das", "Le\u00b7ben", "leiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gl\u00fcck, Ehr und Sieg in Ewigkeit,", "tokens": ["Gl\u00fcck", ",", "Ehr", "und", "Sieg", "in", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Christus von Himmelreichen.", "tokens": ["Chris\u00b7tus", "von", "Him\u00b7mel\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Der Schertel sprach die Reisigen an:", "tokens": ["Der", "Scher\u00b7tel", "sprach", "die", "Rei\u00b7si\u00b7gen", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "\u00bbwendt euch ihr lieben Reitersmann,", "tokens": ["\u00bb", "wendt", "euch", "ihr", "lie\u00b7ben", "Rei\u00b7ters\u00b7mann", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weicht ab von diesem Schiessen,", "tokens": ["Weicht", "ab", "von", "die\u00b7sem", "Schies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sonst werden wir auf diesen Tag,", "tokens": ["Sonst", "wer\u00b7den", "wir", "auf", "die\u00b7sen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dieweil kein Widerstand helfen mag,", "tokens": ["Die\u00b7weil", "kein", "Wi\u00b7der\u00b7stand", "hel\u00b7fen", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Viel Reisigen Zeug verlieren.\u00ab", "tokens": ["Viel", "Rei\u00b7si\u00b7gen", "Zeug", "ver\u00b7lie\u00b7ren", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Der Landgraf und Schertel wurden zu Rath,", "tokens": ["Der", "Land\u00b7graf", "und", "Scher\u00b7tel", "w