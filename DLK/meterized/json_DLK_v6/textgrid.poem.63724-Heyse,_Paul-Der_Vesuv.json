{"textgrid.poem.63724": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Der Vesuv", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fch erwacht im Tagesgrauen,", "tokens": ["Fr\u00fch", "er\u00b7wacht", "im", "Ta\u00b7ges\u00b7grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwang ich mich das steile Treppchen", "tokens": ["Schwang", "ich", "mich", "das", "stei\u00b7le", "Trepp\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Rasch hinan zum flachen S\u00f6ller,", "tokens": ["Rasch", "hi\u00b7nan", "zum", "fla\u00b7chen", "S\u00f6l\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und an seiner Brustwehr lehnend,", "tokens": ["Und", "an", "sei\u00b7ner", "Brust\u00b7wehr", "leh\u00b7nend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Lie\u00df ich die entz\u00fcckten Augen", "tokens": ["Lie\u00df", "ich", "die", "ent\u00b7z\u00fcck\u00b7ten", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weitum in die Runde schweifen.", "tokens": ["Wei\u00b7tum", "in", "die", "Run\u00b7de", "schwei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Noch im leichten Morgenschlummer,", "tokens": ["Noch", "im", "leich\u00b7ten", "Mor\u00b7gen\u00b7schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Zugedeckt von Nebelschleiern", "tokens": ["Zu\u00b7ge\u00b7deckt", "von", "Ne\u00b7bel\u00b7schlei\u00b7ern"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wie von flaumenleichter Decke,", "tokens": ["Wie", "von", "flau\u00b7men\u00b7leich\u00b7ter", "De\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Lag die K\u00fcste, lag der glatte", "tokens": ["Lag", "die", "K\u00fcs\u00b7te", ",", "lag", "der", "glat\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Purpurblaue Meeresspiegel,", "tokens": ["Pur\u00b7pur\u00b7blau\u00b7e", "Mee\u00b7res\u00b7spie\u00b7gel", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Zitternd in dem leisen Windhauch,", "tokens": ["Zit\u00b7ternd", "in", "dem", "lei\u00b7sen", "Wind\u00b7hauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Der dem jungen Tag vorausging.", "tokens": ["Der", "dem", "jun\u00b7gen", "Tag", "vor\u00b7aus\u00b7ging", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.14": {"text": "Und nun kommt er! Siegesprangend,", "tokens": ["Und", "nun", "kommt", "er", "!", "Sie\u00b7ge\u00b7spran\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$.", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "G\u00fcldnen Kronreif um die Stirne,", "tokens": ["G\u00fcld\u00b7nen", "Kron\u00b7reif", "um", "die", "Stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Tritt im Ost er auf die H\u00fcgel,", "tokens": ["Tritt", "im", "Ost", "er", "auf", "die", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Und sofort die Flammenpfeile", "tokens": ["Und", "so\u00b7fort", "die", "Flam\u00b7men\u00b7pfei\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Sendet er in die vertr\u00e4umte", "tokens": ["Sen\u00b7det", "er", "in", "die", "ver\u00b7tr\u00e4um\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.19": {"text": "Welt zu F\u00fc\u00dfen, da\u00df der graue", "tokens": ["Welt", "zu", "F\u00fc\u00b7\u00dfen", ",", "da\u00df", "der", "grau\u00b7e"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "$,", "KOUS", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Nebel rei\u00dft, in Glanz zerflatternd.", "tokens": ["Ne\u00b7bel", "rei\u00dft", ",", "in", "Glanz", "zer\u00b7flat\u00b7ternd", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Und die herrliche Neapel", "tokens": ["Und", "die", "herr\u00b7li\u00b7che", "Nea\u00b7pel"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.22": {"text": "Hebt sich aus dem Duft, und ihre", "tokens": ["Hebt", "sich", "aus", "dem", "Duft", ",", "und", "ih\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$,", "KON", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Kinder all, die kleinen St\u00e4dtchen", "tokens": ["Kin\u00b7der", "all", ",", "die", "klei\u00b7nen", "St\u00e4dt\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PIAT", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "L\u00e4ngs der K\u00fcste, reiben lachend", "tokens": ["L\u00e4ngs", "der", "K\u00fcs\u00b7te", ",", "rei\u00b7ben", "la\u00b7chend"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "VVFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Sich den Schlummer aus den Augen,", "tokens": ["Sich", "den", "Schlum\u00b7mer", "aus", "den", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Spiegeln sich im Meer und kr\u00e4nzen", "tokens": ["Spie\u00b7geln", "sich", "im", "Meer", "und", "kr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Seinen Strand mit Bl\u00fctenzweigen.", "tokens": ["Sei\u00b7nen", "Strand", "mit", "Bl\u00fc\u00b7ten\u00b7zwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch zur Rechten in den klaren", "tokens": ["Doch", "zur", "Rech\u00b7ten", "in", "den", "kla\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Morgenhimmel ragt der alte", "tokens": ["Mor\u00b7gen\u00b7him\u00b7mel", "ragt", "der", "al\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Feuerberg Vesuv, die Stirne", "tokens": ["Feu\u00b7er\u00b7berg", "Ve\u00b7suv", ",", "die", "Stir\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zart umgl\u00fcht von Rosenschimmer.", "tokens": ["Zart", "um\u00b7gl\u00fcht", "von", "Ro\u00b7sen\u00b7schim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ruhig steht er da, behaglich", "tokens": ["Ru\u00b7hig", "steht", "er", "da", ",", "be\u00b7hag\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$,", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seine Morgenpfeife rauchend,", "tokens": ["Sei\u00b7ne", "Mor\u00b7gen\u00b7pfei\u00b7fe", "rau\u00b7chend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Zu dem Kranz der wei\u00dfen St\u00e4dtchen", "tokens": ["Zu", "dem", "Kranz", "der", "wei\u00b7\u00dfen", "St\u00e4dt\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Niederblickend, die wie Enkel", "tokens": ["Nie\u00b7der\u00b7bli\u00b7ckend", ",", "die", "wie", "En\u00b7kel"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "PRELS", "KOKOM", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Um den Gro\u00dfpapa sich dr\u00e4ngen.", "tokens": ["Um", "den", "Gro\u00df\u00b7pa\u00b7pa", "sich", "dr\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Aber ich \u2013 gedenken mu\u00dft' ich", "tokens": ["A\u00b7ber", "ich", "\u2013", "ge\u00b7den\u00b7ken", "mu\u00dft'", "ich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "$(", "VVINF", "VMFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Alles Unheils, das der Gro\u00dfe", "tokens": ["Al\u00b7les", "Un\u00b7heils", ",", "das", "der", "Gro\u00b7\u00dfe"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PRELS", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Diesen Kleinen angestiftet,", "tokens": ["Die\u00b7sen", "Klei\u00b7nen", "an\u00b7ge\u00b7stif\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Und die Faust mit Zorngeb\u00e4rde", "tokens": ["Und", "die", "Faust", "mit", "Zorn\u00b7ge\u00b7b\u00e4r\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Nach ihm sch\u00fcttelnd, rief ich also:", "tokens": ["Nach", "ihm", "sch\u00fct\u00b7telnd", ",", "rief", "ich", "al\u00b7so", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Heuchler du, mit deiner frommen", "tokens": ["Heuch\u00b7ler", "du", ",", "mit", "dei\u00b7ner", "from\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "APPR", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Menschenfreundlich sanften Miene!", "tokens": ["Men\u00b7schen\u00b7freund\u00b7lich", "sanf\u00b7ten", "Mie\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "O, man wei\u00df, wie hei\u00dfe T\u00fccke", "tokens": ["O", ",", "man", "wei\u00df", ",", "wie", "hei\u00b7\u00dfe", "T\u00fc\u00b7cke"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PIS", "VVFIN", "$,", "PWAV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir im Busen g\u00e4rt! Umsonst nicht", "tokens": ["Dir", "im", "Bu\u00b7sen", "g\u00e4rt", "!", "Um\u00b7sonst", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "APPRART", "NN", "VVFIN", "$.", "ADV", "PTKNEG"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Nannte jener kranke Dichter", "tokens": ["Nann\u00b7te", "je\u00b7ner", "kran\u00b7ke", "Dich\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dich ", "tokens": ["Dich"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Ja, Verw\u00fcster und Verheerer", "tokens": ["Ja", ",", "Ver\u00b7w\u00fcs\u00b7ter", "und", "Ver\u00b7hee\u00b7rer"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Warst du seit den fr\u00fchesten Tagen,", "tokens": ["Warst", "du", "seit", "den", "fr\u00fc\u00b7hes\u00b7ten", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Hast die arglos guten Kinder,", "tokens": ["Hast", "die", "arg\u00b7los", "gu\u00b7ten", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Die bei dir sich angesiedelt,", "tokens": ["Die", "bei", "dir", "sich", "an\u00b7ge\u00b7sie\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Erst geh\u00e4tschelt und geliebkost,", "tokens": ["Erst", "ge\u00b7h\u00e4t\u00b7schelt", "und", "ge\u00b7lieb\u00b7kost", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "KON", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.12": {"text": "Dann bei Nacht in Aschengluten", "tokens": ["Dann", "bei", "Nacht", "in", "A\u00b7schen\u00b7glu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Sie erstickt, die Ahnungslosen,", "tokens": ["Sie", "er\u00b7stickt", ",", "die", "Ah\u00b7nungs\u00b7lo\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Ihre Spur vom Boden tilgend.", "tokens": ["Ih\u00b7re", "Spur", "vom", "Bo\u00b7den", "til\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Fluch dir! Ohne dich, du D\u00e4mon,", "tokens": ["Fluch", "dir", "!", "Oh\u00b7ne", "dich", ",", "du", "D\u00e4\u00b7mon", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "APPR", "PPER", "$,", "PPER", "NN", "$,"], "meter": "+-+-++-+", "measure": "unknown.measure.penta"}, "line.16": {"text": "W\u00fcrde dieser benedeite", "tokens": ["W\u00fcr\u00b7de", "die\u00b7ser", "be\u00b7ne\u00b7dei\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PDAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Erdenfleck ein Paradies sein,", "tokens": ["Er\u00b7den\u00b7fleck", "ein", "Pa\u00b7ra\u00b7dies", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VAINF", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.18": {"text": "Gleich dem ersten. Aber freilich,", "tokens": ["Gleich", "dem", "ers\u00b7ten", ".", "A\u00b7ber", "frei\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "$.", "KON", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Diesem auch war ein Verw\u00fcster", "tokens": ["Die\u00b7sem", "auch", "war", "ein", "Ver\u00b7w\u00fcs\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "VAFIN", "ART", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.20": {"text": "Zugesellt vom H\u00f6llenabgrund,", "tokens": ["Zu\u00b7ge\u00b7sellt", "vom", "H\u00f6l\u00b7len\u00b7ab\u00b7grund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Glei\u00dfend, wie du selbst in Sch\u00f6nheit.", "tokens": ["Glei\u00b7\u00dfend", ",", "wie", "du", "selbst", "in", "Sch\u00f6n\u00b7heit", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "So in sittlicher Entr\u00fcstung", "tokens": ["So", "in", "sitt\u00b7li\u00b7cher", "Ent\u00b7r\u00fcs\u00b7tung"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich entladend, hielt den Blick ich", "tokens": ["Mich", "ent\u00b7la\u00b7dend", ",", "hielt", "den", "Blick", "ich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVPP", "$,", "VVFIN", "ART", "NN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf des Kraters Rand geheftet,", "tokens": ["Auf", "des", "Kra\u00b7ters", "Rand", "ge\u00b7hef\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und auf einmal aus dem wei\u00dfen", "tokens": ["Und", "auf", "ein\u00b7mal", "aus", "dem", "wei\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADV", "APPR", "ART", "ADJA"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Dampf, der aus der Tiefe vorquoll,", "tokens": ["Dampf", ",", "der", "aus", "der", "Tie\u00b7fe", "vor\u00b7quoll", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Hob sich geisterhaft ein m\u00e4cht'ges", "tokens": ["Hob", "sich", "geis\u00b7ter\u00b7haft", "ein", "m\u00e4cht'\u00b7ges"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADJD", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Haupt, umweht von grauer M\u00e4hne,", "tokens": ["Haupt", ",", "um\u00b7weht", "von", "grau\u00b7er", "M\u00e4h\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wildem Bart, und unter busch'gen", "tokens": ["Wil\u00b7dem", "Bart", ",", "und", "un\u00b7ter", "busch'\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "KON", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Brauen funkelten zwei Augen,", "tokens": ["Brau\u00b7en", "fun\u00b7kel\u00b7ten", "zwei", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Blitze spr\u00fchend. Deutlich sah ich", "tokens": ["Blit\u00b7ze", "spr\u00fc\u00b7hend", ".", "Deut\u00b7lich", "sah", "ich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVPP", "$.", "NE", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Ihren Blick auf mich gerichtet,", "tokens": ["Ih\u00b7ren", "Blick", "auf", "mich", "ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Und wie Morgenwindes Sausen", "tokens": ["Und", "wie", "Mor\u00b7gen\u00b7win\u00b7des", "Sau\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Drang vernehmlich eine Stimme", "tokens": ["Drang", "ver\u00b7nehm\u00b7lich", "ei\u00b7ne", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "An mein Ohr:", "tokens": ["An", "mein", "Ohr", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Du naseweiser", "tokens": ["Du", "na\u00b7se\u00b7wei\u00b7ser"], "token_info": ["word", "word"], "pos": ["PPER", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "Tor, wie wagst du mich, den alten", "tokens": ["Tor", ",", "wie", "wagst", "du", "mich", ",", "den", "al\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "PWAV", "VVFIN", "PPER", "PRF", "$,", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Herrscher dieser Welt, zu schm\u00e4hen?", "tokens": ["Herr\u00b7scher", "die\u00b7ser", "Welt", ",", "zu", "schm\u00e4\u00b7hen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Haben nicht die ew'gen G\u00f6tter", "tokens": ["Ha\u00b7ben", "nicht", "die", "ew'\u00b7gen", "G\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Hier mir meinen Thron gegr\u00fcndet,", "tokens": ["Hier", "mir", "mei\u00b7nen", "Thron", "ge\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Gaben mir die Feuerseele,", "tokens": ["Ga\u00b7ben", "mir", "die", "Feu\u00b7er\u00b7see\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Die, ob ungez\u00e4hlte Jahre", "tokens": ["Die", ",", "ob", "un\u00b7ge\u00b7z\u00e4hl\u00b7te", "Jah\u00b7re"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "KOUS", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "\u00dcber meinem Haupt dahinziehn,", "tokens": ["\u00dc\u00b7ber", "mei\u00b7nem", "Haupt", "da\u00b7hin\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.23": {"text": "Nie erkaltet? Feuergeister", "tokens": ["Nie", "er\u00b7kal\u00b7tet", "?", "Feu\u00b7er\u00b7geis\u00b7ter"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADV", "VVPP", "$.", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "M\u00fcssen unerbittlich immer", "tokens": ["M\u00fcs\u00b7sen", "un\u00b7er\u00b7bitt\u00b7lich", "im\u00b7mer"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJD", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Wechselnd gut und B\u00f6ses stiften,", "tokens": ["Wech\u00b7selnd", "gut", "und", "B\u00f6\u00b7ses", "stif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Nicht gez\u00e4hmt von lauer Tugend.", "tokens": ["Nicht", "ge\u00b7z\u00e4hmt", "von", "lau\u00b7er", "Tu\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Und du nennst mich Heuchler? H\u00e4tt' ich", "tokens": ["Und", "du", "nennst", "mich", "Heuch\u00b7ler", "?", "H\u00e4tt'", "ich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "NN", "$.", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Je verleugnet mein Gem\u00fcte?", "tokens": ["Je", "ver\u00b7leug\u00b7net", "mein", "Ge\u00b7m\u00fc\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Wenn die Kleinen dumm-vertraulich", "tokens": ["Wenn", "die", "Klei\u00b7nen", "dum\u00b7mver\u00b7trau\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Sich geschmiegt an meine Kniee,", "tokens": ["Sich", "ge\u00b7schmiegt", "an", "mei\u00b7ne", "Kni\u00b7ee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Mu\u00dften sie gew\u00e4rtig bleiben", "tokens": ["Mu\u00df\u00b7ten", "sie", "ge\u00b7w\u00e4r\u00b7tig", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Meiner Launen. Und du schiltst mich,", "tokens": ["Mei\u00b7ner", "Lau\u00b7nen", ".", "Und", "du", "schiltst", "mich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "KON", "PPER", "VVFIN", "PPER", "$,"], "meter": "+-+--++-", "measure": "trochaic.tetra.relaxed"}, "line.33": {"text": "Da\u00df in Aschen ich begraben", "tokens": ["Da\u00df", "in", "A\u00b7schen", "ich", "be\u00b7gra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Jene zwei ber\u00fchmten St\u00e4dtchen?", "tokens": ["Je\u00b7ne", "zwei", "be\u00b7r\u00fchm\u00b7ten", "St\u00e4dt\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "CARD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Heuchler dann du selbst! Wie bist du", "tokens": ["Heuch\u00b7ler", "dann", "du", "selbst", "!", "Wie", "bist", "du"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "PPER", "ADV", "$.", "PWAV", "VAFIN", "PPER"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.36": {"text": "Erst vor kurzem in Pompeji", "tokens": ["Erst", "vor", "kur\u00b7zem", "in", "Pom\u00b7pe\u00b7ji"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "APPR", "NE"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.37": {"text": "Hoch entz\u00fcckt herumgewandelt,", "tokens": ["Hoch", "ent\u00b7z\u00fcckt", "her\u00b7um\u00b7ge\u00b7wan\u00b7delt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Hast die Bronzen, ausgegraben", "tokens": ["Hast", "die", "Bron\u00b7zen", ",", "aus\u00b7ge\u00b7gra\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VAFIN", "ART", "NN", "$,", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Aus dem Schutt von tausend Jahren,", "tokens": ["Aus", "dem", "Schutt", "von", "tau\u00b7send", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Im Museo Nazionale", "tokens": ["Im", "Mu\u00b7seo", "Na\u00b7zi\u00b7o\u00b7na\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Hoch bewundert, und dem \u00bbD\u00e4mon\u00ab,", "tokens": ["Hoch", "be\u00b7wun\u00b7dert", ",", "und", "dem", "\u00bb", "D\u00e4\u00b7mon", "\u00ab", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ADJD", "VVPP", "$,", "KON", "ART", "$(", "NN", "$(", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Der dies Schauspiel euch geg\u00f6nnt hat,", "tokens": ["Der", "dies", "Schau\u00b7spiel", "euch", "ge\u00b7g\u00f6nnt", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDS", "NN", "PPER", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Gibst du danklos schn\u00f6de Namen?", "tokens": ["Gibst", "du", "dank\u00b7los", "schn\u00f6\u00b7de", "Na\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Doch so seid ihr, wind'ge Menschlein,", 