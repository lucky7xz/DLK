{"dta.poem.5828": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Gesetzen der Bewegung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Durch die\u00df Gesetz nun nehmen und empfangen", "tokens": ["Durch", "die\u00df", "Ge\u00b7setz", "nun", "neh\u00b7men", "und", "emp\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDS", "NN", "ADV", "VVINF", "KON", "VVPP"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Die C\u00f6rper, welche weich und nicht zusammen hangen,", "tokens": ["Die", "C\u00f6r\u00b7per", ",", "wel\u00b7che", "weich", "und", "nicht", "zu\u00b7sam\u00b7men", "han\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "KON", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auch die, so fl\u00fcssig sind, und die nicht widerstehn,", "tokens": ["Auch", "die", ",", "so", "fl\u00fcs\u00b7sig", "sind", ",", "und", "die", "nicht", "wi\u00b7der\u00b7stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "ADV", "ADJD", "VAFIN", "$,", "KON", "ART", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jmgleichen, welche leicht ersch\u00fcttern und sich drehn,", "tokens": ["Jm\u00b7glei\u00b7chen", ",", "wel\u00b7che", "leicht", "er\u00b7sch\u00fct\u00b7tern", "und", "sich", "drehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ADJD", "VVINF", "KON", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die schnellesten Bewegungen in sich:", "tokens": ["Die", "schnel\u00b7les\u00b7ten", "Be\u00b7we\u00b7gun\u00b7gen", "in", "sich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PRF", "$."], "meter": "-+-+-++-++", "measure": "zehnsilber"}, "line.6": {"text": "Und machen, wie wir offt versp\u00fcren,", "tokens": ["Und", "ma\u00b7chen", ",", "wie", "wir", "offt", "ver\u00b7sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "PWAV", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df auch die festesten dieselben bald verlieren.", "tokens": ["Da\u00df", "auch", "die", "fes\u00b7tes\u00b7ten", "die\u00b7sel\u00b7ben", "bald", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "PDAT", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}