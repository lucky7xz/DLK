{"dta.poem.2812": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So hilflos zu der Welt wird nie ein Thier geboren", "tokens": ["So", "hil\u00b7flos", "zu", "der", "Welt", "wird", "nie", "ein", "Thier", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "VAFIN", "ADV", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Alswie der Mensch, der sich so hoch f\u00fchlt auserkoren.", "tokens": ["Als\u00b7wie", "der", "Mensch", ",", "der", "sich", "so", "hoch", "f\u00fchlt", "au\u00b7ser\u00b7ko\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PRF", "ADV", "ADJD", "VVFIN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Warum? Es hat Natur dadurch uns sagen wollen,", "tokens": ["Wa\u00b7rum", "?", "Es", "hat", "Na\u00b7tur", "da\u00b7durch", "uns", "sa\u00b7gen", "wol\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PPER", "VAFIN", "NN", "PAV", "PPER", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df wir uns selber und einander helfen sollen.", "tokens": ["Da\u00df", "wir", "uns", "sel\u00b7ber", "und", "ein\u00b7an\u00b7der", "hel\u00b7fen", "sol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "KON", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Mutter hilft zuerst dem Kind, der Vater dann;", "tokens": ["Die", "Mut\u00b7ter", "hilft", "zu\u00b7erst", "dem", "Kind", ",", "der", "Va\u00b7ter", "dann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "$,", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann hilft es ihnen, und sich selber hilft der Mann.", "tokens": ["Dann", "hilft", "es", "ih\u00b7nen", ",", "und", "sich", "sel\u00b7ber", "hilft", "der", "Mann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "KON", "PRF", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}