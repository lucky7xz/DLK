{"dta.poem.17712": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "334.  GOtt sch\u00e4tzt die Werke nach dem  \n wesen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Mensch de\u00df Gerechten Schlaf ist mehr bey Gott geacht/", "tokens": ["Mensch", "de\u00df", "Ge\u00b7rech\u00b7ten", "Schlaf", "ist", "mehr", "bey", "Gott", "ge\u00b7acht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VAFIN", "ADV", "APPR", "NN", "VVPP", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Als was der S\u00fcnder Beht und singt die gantze Nacht.", "tokens": ["Als", "was", "der", "S\u00fcn\u00b7der", "Beht", "und", "singt", "die", "gant\u00b7ze", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWS", "ART", "NN", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}