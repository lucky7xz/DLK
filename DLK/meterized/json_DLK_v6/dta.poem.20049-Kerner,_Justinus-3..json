{"dta.poem.20049": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Blick aus deinem Fenster, Liebe!", "tokens": ["Blick", "aus", "dei\u00b7nem", "Fens\u00b7ter", ",", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schaue \u00fcber die blauen Berge:", "tokens": ["Schau\u00b7e", "\u00fc\u00b7ber", "die", "blau\u00b7en", "Ber\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Denn dort will ich an den Himmel", "tokens": ["Denn", "dort", "will", "ich", "an", "den", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dir ein licht' Gem\u00e4lde malen.", "tokens": ["Dir", "ein", "licht'", "Ge\u00b7m\u00e4l\u00b7de", "ma\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Steigen aus der N\u00e4h' und Ferne", "tokens": ["Stei\u00b7gen", "aus", "der", "N\u00e4h'", "und", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hohe Berge an den Himmel,", "tokens": ["Ho\u00b7he", "Ber\u00b7ge", "an", "den", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "St\u00fcrzen helle, k\u00fchle Quellen", "tokens": ["St\u00fcr\u00b7zen", "hel\u00b7le", ",", "k\u00fch\u00b7le", "Quel\u00b7len"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In ein blumigt, gr\u00fcnes Thal.", "tokens": ["In", "ein", "blu\u00b7migt", ",", "gr\u00fc\u00b7nes", "Thal", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "St\u00fczt der Wanderer im Thale", "tokens": ["St\u00fczt", "der", "Wan\u00b7de\u00b7rer", "im", "Tha\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf den Stab sich, einzuathmen", "tokens": ["Auf", "den", "Stab", "sich", ",", "ein\u00b7zu\u00b7ath\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "PRF", "$,", "VVIZU"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jugend, Freiheit, Liebe, Kraft.", "tokens": ["Ju\u00b7gend", ",", "Frei\u00b7heit", ",", "Lie\u00b7be", ",", "Kraft", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Steht gelehnt an einen Felsen,", "tokens": ["Steht", "ge\u00b7lehnt", "an", "ei\u00b7nen", "Fel\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter Laub und Rebenbl\u00fcthe", "tokens": ["Un\u00b7ter", "Laub", "und", "Re\u00b7ben\u00b7bl\u00fct\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dort ein kleines Haus verborgen,", "tokens": ["Dort", "ein", "klei\u00b7nes", "Haus", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Steh' ich vor dem kleinen Haus.", "tokens": ["Steh'", "ich", "vor", "dem", "klei\u00b7nen", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Kommt vom Bache, Kr\u00e4uter tragend,", "tokens": ["Kommt", "vom", "Ba\u00b7che", ",", "Kr\u00e4u\u00b7ter", "tra\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dort ein liebes, junges Wesen,", "tokens": ["Dort", "ein", "lie\u00b7bes", ",", "jun\u00b7ges", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bist du es \u2014 die Meine l\u00e4ngst.", "tokens": ["Bist", "du", "es", "die", "Mei\u00b7ne", "l\u00e4ngst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "$(", "ART", "PPOSAT", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Ist kein Lauscher mehr zu f\u00fcrchten,", "tokens": ["Ist", "kein", "Lau\u00b7scher", "mehr", "zu", "f\u00fcrch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dr\u00fcck' ich dich, du s\u00fc\u00dfes Wesen!", "tokens": ["Dr\u00fcck", "ich", "dich", ",", "du", "s\u00fc\u00b7\u00dfes", "We\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "An ein treues Herz voll Liebe,", "tokens": ["An", "ein", "treu\u00b7es", "Herz", "voll", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Offen vor des Himmels Aug'.", "tokens": ["Of\u00b7fen", "vor", "des", "Him\u00b7mels", "Aug'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Aber weh! o wehe M\u00e4dchen!", "tokens": ["A\u00b7ber", "weh", "!", "o", "we\u00b7he", "M\u00e4d\u00b7chen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "FM", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Siehst du dort nicht jenen Raben?", "tokens": ["Siehst", "du", "dort", "nicht", "je\u00b7nen", "Ra\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aechzend fliegt er durch den Himmel,", "tokens": ["A\u00b7ech\u00b7zend", "fliegt", "er", "durch", "den", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und verl\u00f6scht mit schwarzem Fittig", "tokens": ["Und", "ver\u00b7l\u00f6scht", "mit", "schwar\u00b7zem", "Fit\u00b7tig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mein Gem\u00e4lde, weh! o weh!", "tokens": ["Mein", "Ge\u00b7m\u00e4l\u00b7de", ",", "weh", "!", "o", "weh", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PTKVZ", "$.", "FM", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}