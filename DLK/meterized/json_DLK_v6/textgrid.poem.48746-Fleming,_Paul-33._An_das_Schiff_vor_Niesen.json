{"textgrid.poem.48746": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "33. An das Schiff vor Niesen", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So sei einmal gegr\u00fc\u00dft, du durch drei halbe Jahre", "tokens": ["So", "sei", "ein\u00b7mal", "ge\u00b7gr\u00fc\u00dft", ",", "du", "durch", "drei", "hal\u00b7be", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "VVPP", "$,", "PPER", "APPR", "CARD", "ADJA", "NN"], "meter": "-+---+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "so viel gew\u00fcndschtes Haus. Du bist es, dem sein Gut", "tokens": ["so", "viel", "ge\u00b7w\u00fcnd\u00b7schtes", "Haus", ".", "Du", "bist", "es", ",", "dem", "sein", "Gut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$.", "PPER", "VAFIN", "PPER", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "das edle ", "tokens": ["das", "ed\u00b7le"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Nim ein und schaue zu, da\u00df uns, der teuren Ware,", "tokens": ["Nim", "ein", "und", "schau\u00b7e", "zu", ",", "da\u00df", "uns", ",", "der", "teu\u00b7ren", "Wa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "KON", "VVFIN", "PTKVZ", "$,", "KOUS", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "durch dich und deine Schuld kein \u00dcbel widerfahre.", "tokens": ["durch", "dich", "und", "dei\u00b7ne", "Schuld", "kein", "\u00dc\u00b7bel", "wi\u00b7der\u00b7fah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sei k\u00fchn' und wags mit uns. Wir setzen Gut und Blut", "tokens": ["Sei", "k\u00fchn'", "und", "wags", "mit", "uns", ".", "Wir", "set\u00b7zen", "Gut", "und", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "KON", "ADV", "APPR", "PPER", "$.", "PPER", "VVFIN", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und Alles bei dir auf. Wir haben guten Mut", "tokens": ["und", "Al\u00b7les", "bei", "dir", "auf", ".", "Wir", "ha\u00b7ben", "gu\u00b7ten", "Mut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PIS", "APPR", "PPER", "PTKVZ", "$.", "PPER", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "zu Gott und der dich sch\u00fctzt, der ", "tokens": ["zu", "Gott", "und", "der", "dich", "sch\u00fctzt", ",", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "KON", "ART", "PPER", "VVFIN", "$,", "PRELS"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Auf, Westwind, lege dich in unser Segel ein.", "tokens": ["Auf", ",", "West\u00b7wind", ",", "le\u00b7ge", "dich", "in", "un\u00b7ser", "Se\u00b7gel", "ein", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "VAFIN", "$,", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein auch, Nord, d\u00fcrfen wir, solln wir an ", "tokens": ["Dein", "auch", ",", "Nord", ",", "d\u00fcr\u00b7fen", "wir", ",", "solln", "wir", "an"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADV", "$,", "NE", "$,", "VMFIN", "PPER", "$,", "VMFIN", "PPER", "APPR"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Befreundet euch mit uns, ihr Wellen und ihr L\u00fcfte.", "tokens": ["Be\u00b7freun\u00b7det", "euch", "mit", "uns", ",", "ihr", "Wel\u00b7len", "und", "ihr", "L\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$,", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Bringst du uns wol zur\u00fcck' und wirds uns gl\u00fccklich gehn,", "tokens": ["Bringst", "du", "uns", "wol", "zu\u00b7r\u00fcck'", "und", "wirds", "uns", "gl\u00fcck\u00b7lich", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "PTKVZ", "KON", "VAFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "so soll dein ewigs Lob an diesem Maste stehn:", "tokens": ["so", "soll", "dein", "e\u00b7wigs", "Lob", "an", "die\u00b7sem", "Mas\u00b7te", "stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "ADJA", "NN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das erste, das war ich, das hier nach Osten schiffte!", "tokens": ["Das", "ers\u00b7te", ",", "das", "war", "ich", ",", "das", "hier", "nach", "Os\u00b7ten", "schiff\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PDS", "VAFIN", "PPER", "$,", "PRELS", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}}}}