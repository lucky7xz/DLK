{"textgrid.poem.30303": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u0153chus ward mit Ernst vermahnt in ein andre Haut zu krichen;", "tokens": ["M\u0153chus", "ward", "mit", "Ernst", "ver\u00b7mahnt", "in", "ein", "and\u00b7re", "Haut", "zu", "kri\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "NE", "VVFIN", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+-+--+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Als er dieses nun gethan, ward er dennoch au\u00dfgestrichen.", "tokens": ["Als", "er", "die\u00b7ses", "nun", "ge\u00b7than", ",", "ward", "er", "den\u00b7noch", "au\u00df\u00b7ge\u00b7stri\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "ADV", "VVPP", "$,", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+---+--+-+-+-", "measure": "trochaic.hexa.relaxed"}}}}}