{"textgrid.poem.55285": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Immer und \u00fcberall", "genre": "verse", "period": "N.A.", "pub_year": 1820, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dringe tief zu Berges Gr\u00fcften,", "tokens": ["Drin\u00b7ge", "tief", "zu", "Ber\u00b7ges", "Gr\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wolken folge hoch zu L\u00fcften;", "tokens": ["Wol\u00b7ken", "fol\u00b7ge", "hoch", "zu", "L\u00fcf\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Muse ruft zu Bach und Tale", "tokens": ["Mu\u00b7se", "ruft", "zu", "Bach", "und", "Ta\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "NE", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tausend, aber tausend Male.", "tokens": ["Tau\u00b7send", ",", "a\u00b7ber", "tau\u00b7send", "Ma\u00b7le", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "$,", "ADV", "CARD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sobald ein frisches Kelchlein bl\u00fcht,", "tokens": ["So\u00b7bald", "ein", "fri\u00b7sches", "Kelc\u00b7hlein", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es fordert neue Lieder;", "tokens": ["Es", "for\u00b7dert", "neu\u00b7e", "Lie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn die Zeit verrauschend flieht,", "tokens": ["Und", "wenn", "die", "Zeit", "ver\u00b7rau\u00b7schend", "flieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jahrszeiten kommen wieder.", "tokens": ["Jahrs\u00b7zei\u00b7ten", "kom\u00b7men", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}