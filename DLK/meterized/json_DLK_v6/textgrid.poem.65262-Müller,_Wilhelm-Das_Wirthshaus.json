{"textgrid.poem.65262": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Das Wirthshaus", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf einen Todtenacker", "tokens": ["Auf", "ei\u00b7nen", "Tod\u00b7te\u00b7na\u00b7cker"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hat mich mein Weg gebracht.", "tokens": ["Hat", "mich", "mein", "Weg", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Allhier will ich einkehren:", "tokens": ["All\u00b7hier", "will", "ich", "ein\u00b7keh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hab' ich bei mir gedacht.", "tokens": ["Hab'", "ich", "bei", "mir", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihr gr\u00fcnen Todtenkr\u00e4nze", "tokens": ["Ihr", "gr\u00fc\u00b7nen", "Tod\u00b7ten\u00b7kr\u00e4n\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "K\u00f6nnt wohl die Zeichen sein,", "tokens": ["K\u00f6nnt", "wohl", "die", "Zei\u00b7chen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die m\u00fcde Wandrer laden", "tokens": ["Die", "m\u00fc\u00b7de", "Wand\u00b7rer", "la\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In's k\u00fchle Wirthshaus ein.", "tokens": ["In's", "k\u00fch\u00b7le", "Wirths\u00b7haus", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sind denn in diesem Hause", "tokens": ["Sind", "denn", "in", "die\u00b7sem", "Hau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Kammern all' besetzt?", "tokens": ["Die", "Kam\u00b7mern", "all'", "be\u00b7setzt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bin matt zum Niedersinken", "tokens": ["Bin", "matt", "zum", "Nie\u00b7der\u00b7sin\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und t\u00f6dtlich schwer verletzt.", "tokens": ["Und", "t\u00f6dt\u00b7lich", "schwer", "ver\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O unbarmherz'ge Schenke,", "tokens": ["O", "un\u00b7barm\u00b7her\u00b7z'\u00b7ge", "Schen\u00b7ke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Doch weisest du mich ab?", "tokens": ["Doch", "wei\u00b7sest", "du", "mich", "ab", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun weiter denn, nur weiter,", "tokens": ["Nun", "wei\u00b7ter", "denn", ",", "nur", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$,", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein treuer Wanderstab!", "tokens": ["Mein", "treu\u00b7er", "Wan\u00b7der\u00b7stab", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}