{"dta.poem.8906": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxXIX .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Das f\u00fchlt' ich stets vielleicht und f\u00fchl' es heute,", "tokens": ["Das", "f\u00fchlt'", "ich", "stets", "viel\u00b7leicht", "und", "f\u00fchl'", "es", "heu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da sich's nach langen Jahren mir erneute,", "tokens": ["Da", "sich's", "nach", "lan\u00b7gen", "Jah\u00b7ren", "mir", "er\u00b7neu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nachdem ich manchen Wahn der Welt gesichtet.", "tokens": ["Nach\u00b7dem", "ich", "man\u00b7chen", "Wahn", "der", "Welt", "ge\u00b7sich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O Zeit, in der ich noch f\u00fcr dich gedichtet", "tokens": ["O", "Zeit", ",", "in", "der", "ich", "noch", "f\u00fcr", "dich", "ge\u00b7dich\u00b7tet"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "APPR", "PRELS", "PPER", "ADV", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was, au\u00dfer mir, sich keiner Leser freute!", "tokens": ["Was", ",", "au\u00b7\u00dfer", "mir", ",", "sich", "kei\u00b7ner", "Le\u00b7ser", "freu\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "APPR", "PPER", "$,", "PRF", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Noch war mein Name nicht der Welt zur Beute,", "tokens": ["Noch", "war", "mein", "Na\u00b7me", "nicht", "der", "Welt", "zur", "Beu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PTKNEG", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die selten f\u00fchlt und oft so lieblos richtet!", "tokens": ["Die", "sel\u00b7ten", "f\u00fchlt", "und", "oft", "so", "lieb\u00b7los", "rich\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "KON", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Noch unbekannt mit meinen eig'nen Trieben,", "tokens": ["Noch", "un\u00b7be\u00b7kannt", "mit", "mei\u00b7nen", "eig'\u00b7nen", "Trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu ernst, zu sch\u00fcchtern, allzusehr verschlossen,", "tokens": ["Zu", "ernst", ",", "zu", "sch\u00fcch\u00b7tern", ",", "all\u00b7zu\u00b7sehr", "ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "$,", "PTKZU", "VVINF", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bin ich dir fremd durch eigne Schuld geblieben.", "tokens": ["Bin", "ich", "dir", "fremd", "durch", "eig\u00b7ne", "Schuld", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADJD", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da wieder nun ich deines Blicks genossen,", "tokens": ["Da", "wie\u00b7der", "nun", "ich", "dei\u00b7nes", "Blicks", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Empfind' ich wieder jenen Drang, zu lieben;", "tokens": ["Emp\u00b7find'", "ich", "wie\u00b7der", "je\u00b7nen", "Drang", ",", "zu", "lie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PDAT", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch meine sch\u00f6nste Jugend ist verflossen.", "tokens": ["Doch", "mei\u00b7ne", "sch\u00f6ns\u00b7te", "Ju\u00b7gend", "ist", "ver\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}