{"textgrid.poem.56335": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "5", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer hat zum Schreier also dich bedungen?", "tokens": ["Wer", "hat", "zum", "Schrei\u00b7er", "al\u00b7so", "dich", "be\u00b7dun\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPRART", "NN", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es m\u00f6chten Lieder besser dir gedeihen,", "tokens": ["Es", "m\u00f6ch\u00b7ten", "Lie\u00b7der", "bes\u00b7ser", "dir", "ge\u00b7dei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Welchen auch gern das Ohr die meisten leihen;", "tokens": ["Wel\u00b7chen", "auch", "gern", "das", "Ohr", "die", "meis\u00b7ten", "lei\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ART", "NN", "ART", "PIS", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Hast du nicht sonst von Lieb und Wein gesungen?", "tokens": ["Hast", "du", "nicht", "sonst", "von", "Lieb", "und", "Wein", "ge\u00b7sun\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "K\u00f6nnt ich aus ehrner Brust doch tausend Zungen", "tokens": ["K\u00f6nnt", "ich", "aus", "ehr\u00b7ner", "Brust", "doch", "tau\u00b7send", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN", "ADV", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Hauch beleben, alle wollt ich weihen,", "tokens": ["Mit", "Hauch", "be\u00b7le\u00b7ben", ",", "al\u00b7le", "wollt", "ich", "wei\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,", "PIS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gellend das eine, alte Lied zu schreien,", "tokens": ["Gel\u00b7lend", "das", "ei\u00b7ne", ",", "al\u00b7te", "Lied", "zu", "schrei\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "ART", "$,", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Bis in verschlo\u00dfnen Ohren es erklungen.", "tokens": ["Bis", "in", "ver\u00b7schlo\u00df\u00b7nen", "Oh\u00b7ren", "es", "er\u00b7klun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es ist hoch an der Zeit, sie auf zu schrecken,", "tokens": ["Es", "ist", "hoch", "an", "der", "Zeit", ",", "sie", "auf", "zu", "schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "ART", "NN", "$,", "PPER", "APPR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die taumelnd um den Rand des Abgrunds wallen,", "tokens": ["Die", "tau\u00b7melnd", "um", "den", "Rand", "des", "Ab\u00b7grunds", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ob schlafend nicht, dennoch nicht zu erwecken;", "tokens": ["Ob", "schla\u00b7fend", "nicht", ",", "den\u00b7noch", "nicht", "zu", "er\u00b7we\u00b7cken", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PTKNEG", "$,", "ADV", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O mu\u00df die schwache Stimme so verhallen!", "tokens": ["O", "mu\u00df", "die", "schwa\u00b7che", "Stim\u00b7me", "so", "ver\u00b7hal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Es drohet euch der Sturz, mir blo\u00df das Schrecken; \u2013", "tokens": ["Es", "dro\u00b7het", "euch", "der", "Sturz", ",", "mir", "blo\u00df", "das", "Schre\u00b7cken", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,", "PPER", "ADV", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Vogel schwingt sich auf, wo Eichen fallen.", "tokens": ["Ein", "Vo\u00b7gel", "schwingt", "sich", "auf", ",", "wo", "Ei\u00b7chen", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PRF", "PTKVZ", "$,", "PWAV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}