{"textgrid.poem.36169": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sieg, Sieg, euer Kampf ist aus!", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieg, Sieg, euer Kampf ist aus!", "tokens": ["Sieg", ",", "Sieg", ",", "eu\u00b7er", "Kampf", "ist", "aus", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPOSAT", "NN", "VAFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihr habt den Kranz erwunden!", "tokens": ["Ihr", "habt", "den", "Kranz", "er\u00b7wun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im hei\u00dfen Streit und Strau\u00df", "tokens": ["Im", "hei\u00b7\u00dfen", "Streit", "und", "Strau\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "KON", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Seyd ihr bew\u00e4hrt erfunden.", "tokens": ["Seyd", "ihr", "be\u00b7w\u00e4hrt", "er\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das Bitterst' ist erlitten.", "tokens": ["Das", "Bit\u00b7ter\u00b7st'", "ist", "er\u00b7lit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Das Edelst' ist erstritten.", "tokens": ["Das", "E\u00b7delst'", "ist", "er\u00b7strit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Gel\u00f6s't habt ihr den Eid,", "tokens": ["Ge\u00b7l\u00f6s't", "habt", "ihr", "den", "Eid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ents\u00fcndigt das Gewissen.", "tokens": ["Ent\u00b7s\u00fcn\u00b7digt", "das", "Ge\u00b7wis\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Schuld seyd ihr entfreit.", "tokens": ["Der", "Schuld", "seyd", "ihr", "ent\u00b7freit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Bannbrief ist zerrissen.", "tokens": ["Der", "Bann\u00b7brief", "ist", "zer\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ihr drangt aus Eng' und Schw\u00fcle", "tokens": ["Ihr", "drangt", "aus", "Eng'", "und", "Schw\u00fc\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NE", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ins Freie, Weite, K\u00fchle.", "tokens": ["Ins", "Frei\u00b7e", ",", "Wei\u00b7te", ",", "K\u00fch\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nein, nein, das h\u00f6chste Gut", "tokens": ["Nein", ",", "nein", ",", "das", "h\u00f6chs\u00b7te", "Gut"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wird nicht zu theu'r erstanden!", "tokens": ["Wird", "nicht", "zu", "theu'r", "er\u00b7stan\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer Athem spart und Blut,", "tokens": ["Wer", "A\u00b7them", "spart", "und", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wird sicherlich zu Schanden.", "tokens": ["Wird", "si\u00b7cher\u00b7lich", "zu", "Schan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Durch Arbeit nur und Wunden", "tokens": ["Durch", "Ar\u00b7beit", "nur", "und", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wird die Erl\u00f6sung funden.", "tokens": ["Wird", "die", "Er\u00b7l\u00f6\u00b7sung", "fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sch\u00f6n, Br\u00fcder, sankt ihr hin,", "tokens": ["Sch\u00f6n", ",", "Br\u00fc\u00b7der", ",", "sankt", "ihr", "hin", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Freund an Freundes Seite!", "tokens": ["Der", "Freund", "an", "Freun\u00b7des", "Sei\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gehoben Blick und Sinn", "tokens": ["Ge\u00b7ho\u00b7ben", "Blick", "und", "Sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In jene H\u00f6h' und Weite.", "tokens": ["In", "je\u00b7ne", "H\u00f6h'", "und", "Wei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Im Donner und im Blitze", "tokens": ["Im", "Don\u00b7ner", "und", "im", "Blit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Flogt ihr zum Sternensitze.", "tokens": ["Flogt", "ihr", "zum", "Ster\u00b7nen\u00b7sit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O neidenswerthes Loos!", "tokens": ["O", "nei\u00b7dens\u00b7wert\u00b7hes", "Loos", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "O wollustreiches Sterben!", "tokens": ["O", "wol\u00b7lust\u00b7rei\u00b7ches", "Ster\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O sch\u00f6n, des Himmels Schlo\u00df", "tokens": ["O", "sch\u00f6n", ",", "des", "Him\u00b7mels", "Schlo\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJD", "$,", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Also im Sturm erwerben!", "tokens": ["Al\u00b7so", "im", "Sturm", "er\u00b7wer\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVINF", "$."], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.5": {"text": "Sagt nicht, da\u00df die gestorben,", "tokens": ["Sagt", "nicht", ",", "da\u00df", "die", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "KOUS", "ART", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die ew'gen Ruhm erworben!", "tokens": ["Die", "ew'\u00b7gen", "Ruhm", "er\u00b7wor\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Nein, nein, das ew'ge Gut", "tokens": ["Nein", ",", "nein", ",", "das", "ew'\u00b7ge", "Gut"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wird nicht zu theu'r errungen.", "tokens": ["Wird", "nicht", "zu", "theu'r", "er\u00b7run\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die ihr, getauft mit Blut,", "tokens": ["Die", "ihr", ",", "ge\u00b7tauft", "mit", "Blut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zu Gott euch aufgeschwungen,", "tokens": ["Zu", "Gott", "euch", "auf\u00b7ge\u00b7schwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Zeit ist euch zerronnen,", "tokens": ["Die", "Zeit", "ist", "euch", "zer\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Ewigkeit gewonnen.", "tokens": ["Die", "E\u00b7wig\u00b7keit", "ge\u00b7won\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Auf euren Sch\u00e4deln strebt", "tokens": ["Auf", "eu\u00b7ren", "Sch\u00e4\u00b7deln", "strebt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Empor der Freiheit Tempel!", "tokens": ["Em\u00b7por", "der", "Frei\u00b7heit", "Tem\u00b7pel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Enkel Brust durchbebt", "tokens": ["Der", "En\u00b7kel", "Brust", "durch\u00b7bebt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das mahnende Exempel!", "tokens": ["Das", "mah\u00b7nen\u00b7de", "Ex\u00b7em\u00b7pel", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Der Sklaven Kniee sch\u00fcttern,", "tokens": ["Der", "Skla\u00b7ven", "Kni\u00b7ee", "sch\u00fct\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und die Tyrannen zittern!", "tokens": ["Und", "die", "Ty\u00b7ran\u00b7nen", "zit\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Grabt, Br\u00fcder, nun das Grab!", "tokens": ["Grabt", ",", "Br\u00fc\u00b7der", ",", "nun", "das", "Grab", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das tiefe, weite, breite.", "tokens": ["Das", "tie\u00b7fe", ",", "wei\u00b7te", ",", "brei\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "$,", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Senkt weinend sie hinab!", "tokens": ["Senkt", "wei\u00b7nend", "sie", "hin\u00b7ab", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Freund schlaf' an Freundes Seite!", "tokens": ["Freund", "schlaf'", "an", "Freun\u00b7des", "Sei\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Ein H\u00fcgel soll sie decken.", "tokens": ["Ein", "H\u00fc\u00b7gel", "soll", "sie", "de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Einst wird Ein Tag sie wecken.", "tokens": ["Einst", "wird", "Ein", "Tag", "sie", "we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Schlaft, traute Br\u00fcder, schlaft,", "tokens": ["Schlaft", ",", "trau\u00b7te", "Br\u00fc\u00b7der", ",", "schlaft", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Schlaft sanft im Schoo\u00df' der Erde,", "tokens": ["Schlaft", "sanft", "im", "Schoo\u00df'", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bis Gott euch neu erschafft,", "tokens": ["Bis", "Gott", "euch", "neu", "er\u00b7schafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Durch sein allm\u00e4chtig Werde;", "tokens": ["Durch", "sein", "all\u00b7m\u00e4ch\u00b7tig", "Wer\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "VAFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Wann die Posaun' erklungen", "tokens": ["Wann", "die", "Po\u00b7saun'", "er\u00b7klun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und Grab und Sarg zersprungen!", "tokens": ["Und", "Grab", "und", "Sarg", "zer\u00b7sprun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Th\u00fcrmt, Br\u00fcder, nun das Maal,", "tokens": ["Th\u00fcrmt", ",", "Br\u00fc\u00b7der", ",", "nun", "das", "Maal", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Th\u00fcrmt's hoch bis an die Sterne,", "tokens": ["Th\u00fcrmt's", "hoch", "bis", "an", "die", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Th\u00fcrmt's bis zum Himmelssaal,", "tokens": ["Th\u00fcrmt's", "bis", "zum", "Him\u00b7mels\u00b7saal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Weit leucht' es in die Ferne!", "tokens": ["Weit", "leucht'", "es", "in", "die", "Fer\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das Kreuz auf seiner Spitzen", "tokens": ["Das", "Kreuz", "auf", "sei\u00b7ner", "Spit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Soll Trost in's Herz uns blitzen.", "tokens": ["Soll", "Trost", "in's", "Herz", "uns", "blit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Horch, horch! die Trommel rollt!", "tokens": ["Horch", ",", "horch", "!", "die", "Trom\u00b7mel", "rollt", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Es ruft uns die Drommete.", "tokens": ["Es", "ruft", "uns", "die", "Drom\u00b7me\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+---+-", "measure": "dactylic.init"}, "line.3": {"text": "Der Himmel ist uns hold,", "tokens": ["Der", "Him\u00b7mel", "ist", "uns", "hold", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In Osten bl\u00fcht die R\u00f6the.", "tokens": ["In", "Os\u00b7ten", "bl\u00fcht", "die", "R\u00f6\u00b7the", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Fahrt wohl, erl\u00f6ste Br\u00fcder!", "tokens": ["Fahrt", "wohl", ",", "er\u00b7l\u00f6s\u00b7te", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Fahrt wohl, wir sehn uns wieder!", "tokens": ["Fahrt", "wohl", ",", "wir", "sehn", "uns", "wie\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}