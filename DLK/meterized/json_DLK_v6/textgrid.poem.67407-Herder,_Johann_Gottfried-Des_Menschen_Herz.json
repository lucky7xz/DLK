{"textgrid.poem.67407": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Des Menschen Herz", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In ", "tokens": ["In"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Die G\u00f6tter Freud' und Schmerz;", "tokens": ["Die", "G\u00f6t\u00b7ter", "Freud'", "und", "Schmerz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie webten und erfanden", "tokens": ["Sie", "web\u00b7ten", "und", "er\u00b7fan\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Daraus ein Menschenherz.", "tokens": ["Da\u00b7raus", "ein", "Men\u00b7schen\u00b7herz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du armes Herz, gewebet", "tokens": ["Du", "ar\u00b7mes", "Herz", ",", "ge\u00b7we\u00b7bet"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Aus Lust und Traurigkeit,", "tokens": ["Aus", "Lust", "und", "Trau\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wei\u00dft Du, was Dich belebet?", "tokens": ["Wei\u00dft", "Du", ",", "was", "Dich", "be\u00b7le\u00b7bet", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ist's Freude? ist es Leid?", "tokens": ["Ist's", "Freu\u00b7de", "?", "ist", "es", "Leid", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die G\u00f6ttin selbst der Liebe", "tokens": ["Die", "G\u00f6t\u00b7tin", "selbst", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sah es bedaurend an:", "tokens": ["Sah", "es", "be\u00b7dau\u00b7rend", "an", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "\u00bbo zweifelhafte Triebe,", "tokens": ["\u00bb", "o", "zwei\u00b7fel\u00b7haf\u00b7te", "Trie\u00b7be", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die dieses Herz gewann!", "tokens": ["Die", "die\u00b7ses", "Herz", "ge\u00b7wann", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Im W\u00fcnschen nur und Sehnen", "tokens": ["Im", "W\u00fcn\u00b7schen", "nur", "und", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wohnt seine Seligkeit,", "tokens": ["Wohnt", "sei\u00b7ne", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und selbst der Freude Thr\u00e4nen", "tokens": ["Und", "selbst", "der", "Freu\u00b7de", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Verk\u00fcndigen ihm Leid.\u00ab", "tokens": ["Ver\u00b7k\u00fcn\u00b7di\u00b7gen", "ihm", "Leid", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mitleidig trat ihr Knabe", "tokens": ["Mit\u00b7lei\u00b7dig", "trat", "ihr", "Kna\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hinzu mit seinem Pfeil:", "tokens": ["Hin\u00b7zu", "mit", "sei\u00b7nem", "Pfeil", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbauf! meine beste Gabe,", "tokens": ["\u00bb", "auf", "!", "mei\u00b7ne", "bes\u00b7te", "Ga\u00b7be", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$.", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie werde Dir zu Theil!", "tokens": ["Sie", "wer\u00b7de", "Dir", "zu", "Theil", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dein unbezwinglich Streben", "tokens": ["Dein", "un\u00b7be\u00b7zwing\u00b7lich", "Stre\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sei Liebe Dir, o Herz,", "tokens": ["Sei", "Lie\u00b7be", "Dir", ",", "o", "Herz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPER", "$,", "FM", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sei Deiner Freude Leben,", "tokens": ["Sei", "Dei\u00b7ner", "Freu\u00b7de", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und S\u00fc\u00dfigkeit dem Schmerz!\u00ab", "tokens": ["Und", "S\u00fc\u00b7\u00dfig\u00b7keit", "dem", "Schmerz", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}