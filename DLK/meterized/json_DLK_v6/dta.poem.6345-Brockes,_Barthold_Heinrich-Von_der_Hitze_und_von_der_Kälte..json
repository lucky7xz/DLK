{"dta.poem.6345": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Hitze und von der K\u00e4lte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn man nun \u00f6ffters Feur, so gl\u00e4ntzt und scheinet, findet;", "tokens": ["Wenn", "man", "nun", "\u00f6ff\u00b7ters", "Feur", ",", "so", "gl\u00e4ntzt", "und", "schei\u00b7net", ",", "fin\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "NN", "$,", "ADV", "ADJD", "KON", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So trifft man auch zuweilen Feuer an,", "tokens": ["So", "trifft", "man", "auch", "zu\u00b7wei\u00b7len", "Feu\u00b7er", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ADV", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das brennt, ob man es gleich nicht sehen kan.", "tokens": ["Das", "brennt", ",", "ob", "man", "es", "gleich", "nicht", "se\u00b7hen", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "PIS", "PPER", "ADV", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein C\u00f6rper, der sich nie, dem Ansehn nach, entz\u00fcndet,", "tokens": ["Ein", "C\u00f6r\u00b7per", ",", "der", "sich", "nie", ",", "dem", "An\u00b7sehn", "nach", ",", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADV", "$,", "ART", "NN", "PTKVZ", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der brennt und hitzet hefftiglich,", "tokens": ["Der", "brennt", "und", "hit\u00b7zet", "heff\u00b7tig\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn offt ein fl\u00fc\u00dfiger und kalter C\u00f6rper sich", "tokens": ["Wenn", "offt", "ein", "fl\u00fc\u00b7\u00dfi\u00b7ger", "und", "kal\u00b7ter", "C\u00f6r\u00b7per", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "KON", "ADJA", "NN", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mit einem andern mengt, der fl\u00fc\u00dfig ist und kalt,", "tokens": ["Mit", "ei\u00b7nem", "an\u00b7dern", "mengt", ",", "der", "fl\u00fc\u00b7\u00dfig", "ist", "und", "kalt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "VVFIN", "$,", "PRELS", "ADJD", "VAFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Entz\u00fcnden sie sich offt gar hefftig und zwar bald.", "tokens": ["Ent\u00b7z\u00fcn\u00b7den", "sie", "sich", "offt", "gar", "heff\u00b7tig", "und", "zwar", "bald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADV", "ADJD", "KON", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}