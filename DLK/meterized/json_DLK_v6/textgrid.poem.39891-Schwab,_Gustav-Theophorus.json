{"textgrid.poem.39891": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Theophorus", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Vogt im r\u00f6mischen Reich, Trajan,", "tokens": ["Der", "Vogt", "im", "r\u00f6\u00b7mi\u00b7schen", "Reich", ",", "Tra\u00b7jan", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hatt' sich den Osten unterthan,", "tokens": ["Hatt'", "sich", "den", "Os\u00b7ten", "un\u00b7ter\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Schwert erwarb ihm stolzes Lob;", "tokens": ["Sein", "Schwert", "er\u00b7warb", "ihm", "stol\u00b7zes", "Lob", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da ward er \u00fcberm\u00fctig drob,", "tokens": ["Da", "ward", "er", "\u00fc\u00b7berm\u00b7\u00fc\u00b7tig", "drob", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Meinte, zu seines Sieges Lauf", "tokens": ["Mein\u00b7te", ",", "zu", "sei\u00b7nes", "Sie\u00b7ges", "Lauf"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Noch fehle der gottsel'ge Hauf", "tokens": ["Noch", "feh\u00b7le", "der", "gott\u00b7sel'\u00b7ge", "Hauf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Christen: w\u00e4re der bezwungen,", "tokens": ["Der", "Chris\u00b7ten", ":", "w\u00e4\u00b7re", "der", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "VAFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "W\u00fcrd' ihm erst ein Triumph gesungen!", "tokens": ["W\u00fcrd'", "ihm", "erst", "ein", "Tri\u00b7umph", "ge\u00b7sun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Durch Syrien er dr\u00e4uend zog,", "tokens": ["Durch", "Sy\u00b7ri\u00b7en", "er", "dr\u00e4u\u00b7end", "zog", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Unter der Teufel Dienst sie bog;", "tokens": ["Un\u00b7ter", "der", "Teu\u00b7fel", "Dienst", "sie", "bog", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PPER", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "F\u00fcr seine G\u00f6tter ging er werben,", "tokens": ["F\u00fcr", "sei\u00b7ne", "G\u00f6t\u00b7ter", "ging", "er", "wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wer nicht wollt' opfern, mu\u00dfte sterben.", "tokens": ["Wer", "nicht", "wollt'", "op\u00b7fern", ",", "mu\u00df\u00b7te", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "VMFIN", "VVINF", "$,", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zuletzt in Antiochia", "tokens": ["Zu\u00b7letzt", "in", "An\u00b7ti\u00b7o\u00b7chia"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "NE"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Vor seinen Richterstuhl man sah", "tokens": ["Vor", "sei\u00b7nen", "Rich\u00b7ter\u00b7stuhl", "man", "sah"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PIS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Geschleppt Ignaz, den Gottestr\u00e4ger,", "tokens": ["Ge\u00b7schleppt", "Ig\u00b7naz", ",", "den", "Got\u00b7tes\u00b7tr\u00e4\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Im Silberhaar des H\u00e4ufleins Pfleger.", "tokens": ["Im", "Sil\u00b7ber\u00b7haar", "des", "H\u00e4uf\u00b7leins", "Pfle\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Als er nun vor dem Kaiser stand,", "tokens": ["Als", "er", "nun", "vor", "dem", "Kai\u00b7ser", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Zornig der reckt' empor die Hand,", "tokens": ["Zor\u00b7nig", "der", "reckt'", "em\u00b7por", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Und sprach: \u00bbWer bist du, b\u00f6ser Geist,", "tokens": ["Und", "sprach", ":", "\u00bb", "Wer", "bist", "du", ",", "b\u00f6\u00b7ser", "Geist", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "PWS", "VAFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Der s\u00fcndiget und s\u00fcnd'gen hei\u00dft", "tokens": ["Der", "s\u00fcn\u00b7di\u00b7get", "und", "s\u00fcn\u00b7d'\u00b7gen", "hei\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KON", "ADJA", "VVFIN"], "meter": "-+---+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "Wider mein Wort, das Not und Tod", "tokens": ["Wi\u00b7der", "mein", "Wort", ",", "das", "Not", "und", "Tod"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "NN", "KON", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.14": {"text": "Den Feinden unsrer Tempel droht?\u00ab", "tokens": ["Den", "Fein\u00b7den", "uns\u00b7rer", "Tem\u00b7pel", "droht", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Der Greis schaut' auf, er zagte nicht,", "tokens": ["Der", "Greis", "schaut'", "auf", ",", "er", "zag\u00b7te", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Er zeigt ein Engelsangesicht,", "tokens": ["Er", "zeigt", "ein", "En\u00b7gel\u00b7san\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und l\u00e4chelnd sprach er: \u00bbSeit wann hei\u00dft", "tokens": ["Und", "l\u00e4\u00b7chelnd", "sprach", "er", ":", "\u00bb", "Seit", "wann", "hei\u00dft"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$.", "$(", "APPR", "PWAV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Ein Gottestr\u00e4ger ein b\u00f6ser Geist?\u00ab", "tokens": ["Ein", "Got\u00b7tes\u00b7tr\u00e4\u00b7ger", "ein", "b\u00f6\u00b7ser", "Geist", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "\u00bbein Gottestr\u00e4ger,\u00ab frug Trajan,", "tokens": ["\u00bb", "ein", "Got\u00b7tes\u00b7tr\u00e4\u00b7ger", ",", "\u00ab", "frug", "Tra\u00b7jan", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "$(", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "\u00bbwas mag ein solcher sein, sag' an!\u00ab", "tokens": ["\u00bb", "was", "mag", "ein", "sol\u00b7cher", "sein", ",", "sag'", "an", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VMFIN", "ART", "PIAT", "VAINF", "$,", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Der aber sprach: \u00bbEin solcher ist,", "tokens": ["Der", "a\u00b7ber", "sprach", ":", "\u00bb", "Ein", "sol\u00b7cher", "ist", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$.", "$(", "ART", "PIAT", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Wer tr\u00e4gt im Herzen Jesum Christ!\u00ab", "tokens": ["Wer", "tr\u00e4gt", "im", "Her\u00b7zen", "Je\u00b7sum", "Christ", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "APPRART", "NN", "NE", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und zorniger Trajanus rief:", "tokens": ["Und", "zor\u00b7ni\u00b7ger", "Tra\u00b7ja\u00b7nus", "rief", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbmeinst du, wir R\u00f6mer tr\u00fcgen tief", "tokens": ["\u00bb", "meinst", "du", ",", "wir", "R\u00f6\u00b7mer", "tr\u00fc\u00b7gen", "tief"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "PPER", "$,", "PPER", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht auch die G\u00f6tter im Gem\u00fcte,", "tokens": ["Nicht", "auch", "die", "G\u00f6t\u00b7ter", "im", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wir, denen Sieg leiht ihre G\u00fcte?", "tokens": ["Wir", ",", "de\u00b7nen", "Sieg", "leiht", "ih\u00b7re", "G\u00fc\u00b7te", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Drum leuchtet uns auch holden Blicks", "tokens": ["Drum", "leuch\u00b7tet", "uns", "auch", "hol\u00b7den", "Blicks"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zeus an, der Herr des Weltgeschicks.", "tokens": ["Zeus", "an", ",", "der", "Herr", "des", "Welt\u00b7ge\u00b7schicks", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch dein Gott ist ein Bild der Strafe,", "tokens": ["Doch", "dein", "Gott", "ist", "ein", "Bild", "der", "Stra\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Ist ein an's Kreuz geschlagner Sklave!", "tokens": ["Ist", "ein", "an's", "Kreuz", "ge\u00b7schlag\u00b7ner", "Skla\u00b7ve", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Sieh Jenen stehn hoch auf Alt\u00e4ren,", "tokens": ["Sieh", "Je\u00b7nen", "stehn", "hoch", "auf", "Al\u00b7t\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDS", "VVFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "H\u00f6r' auf, den Sch\u00e4cher zu verehren!\u00ab", "tokens": ["H\u00f6r'", "auf", ",", "den", "Sch\u00e4\u00b7cher", "zu", "ver\u00b7eh\u00b7ren", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "PTKVZ", "$,", "ART", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "++-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ignaz sprach: \u00bbDeines G\u00f6tzen Blick", "tokens": ["Ig\u00b7naz", "sprach", ":", "\u00bb", "Dei\u00b7nes", "G\u00f6t\u00b7zen", "Blick"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "$.", "$(", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lenkt nimmer, glaub' mir, das Geschick;", "tokens": ["Lenkt", "nim\u00b7mer", ",", "glaub'", "mir", ",", "das", "Ge\u00b7schick", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "VVFIN", "PPER", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch einen Blick ganz andrer Art", "tokens": ["Doch", "ei\u00b7nen", "Blick", "ganz", "an\u00b7drer", "Art"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hab' ich von ", "tokens": ["Hab'", "ich", "von"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPER", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "H\u00e4tt' er dich angeblickt, wie ", "tokens": ["H\u00e4tt'", "er", "dich", "an\u00b7ge\u00b7blickt", ",", "wie"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "PRF", "VVPP", "$,", "PWAV"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Du dientest jetzt ihm sicherlich!\u00ab", "tokens": ["Du", "dien\u00b7test", "jetzt", "ihm", "si\u00b7cher\u00b7lich", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "PRF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Kaiser h\u00f6hnte: \u00bbThor, wann bist", "tokens": ["Der", "Kai\u00b7ser", "h\u00f6hn\u00b7te", ":", "\u00bb", "Thor", ",", "wann", "bist"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "NN", "$,", "PWAV", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zusammentroffen du mit Christ?", "tokens": ["Zu\u00b7sam\u00b7men\u00b7trof\u00b7fen", "du", "mit", "Christ", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie freche Knecht' hat er geblickt,", "tokens": ["Wie", "fre\u00b7che", "Knecht'", "hat", "er", "ge\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ward, wie sie, zum Tod geschickt!", "tokens": ["Und", "ward", ",", "wie", "sie", ",", "zum", "Tod", "ge\u00b7schickt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "PWAV", "PPER", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Frevler starb vor achtzig Jahren,", "tokens": ["Der", "Frev\u00b7ler", "starb", "vor", "acht\u00b7zig", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie h\u00e4ttst ", "tokens": ["Wie", "h\u00e4ttst"], "token_info": ["word", "word"], "pos": ["PWAV", "VAFIN"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.6": {"line.1": {"text": "\u00bbund doch hat er mich angeschaut,\u00ab", "tokens": ["\u00bb", "und", "doch", "hat", "er", "mich", "an\u00b7ge\u00b7schaut", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "ADV", "VAFIN", "PPER", "PRF", "VVPP", "$,", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"},