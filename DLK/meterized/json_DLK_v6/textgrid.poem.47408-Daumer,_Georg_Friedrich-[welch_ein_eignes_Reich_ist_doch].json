{"textgrid.poem.47408": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[welch ein eignes Reich ist doch]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welch ein eignes Reich ist doch", "tokens": ["Welch", "ein", "eig\u00b7nes", "Reich", "ist", "doch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ART", "ADJA", "NN", "VAFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Das der Liebe! seine Wunder", "tokens": ["Das", "der", "Lie\u00b7be", "!", "sei\u00b7ne", "Wun\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "ART", "NN", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Werden nimmer ausgesagt.", "tokens": ["Wer\u00b7den", "nim\u00b7mer", "aus\u00b7ge\u00b7sagt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht befremdlich ist es uns,", "tokens": ["Nicht", "be\u00b7fremd\u00b7lich", "ist", "es", "uns", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VAFIN", "PPER", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn gef\u00fcrchtet starke L\u00f6wen", "tokens": ["Wenn", "ge\u00b7f\u00fcrch\u00b7tet", "star\u00b7ke", "L\u00f6\u00b7wen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "VVPP", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sch\u00fcchterne Gazellen jagen;", "tokens": ["Sch\u00fcch\u00b7ter\u00b7ne", "Ga\u00b7zel\u00b7len", "ja\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.7": {"text": "Die Gazelle deines Auges", "tokens": ["Die", "Ga\u00b7zel\u00b7le", "dei\u00b7nes", "Au\u00b7ges"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Macht jedoch auf L\u00f6wen Jagd.", "tokens": ["Macht", "je\u00b7doch", "auf", "L\u00f6\u00b7wen", "Jagd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}