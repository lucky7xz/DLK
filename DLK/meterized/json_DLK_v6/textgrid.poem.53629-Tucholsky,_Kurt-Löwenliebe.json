{"textgrid.poem.53629": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "L\u00f6wenliebe", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als jener junge Schopenhauer", "tokens": ["Als", "je\u00b7ner", "jun\u00b7ge", "Scho\u00b7pen\u00b7hau\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "am L\u00f6wenk\u00e4fig in Berlin", "tokens": ["am", "L\u00f6\u00b7wen\u00b7k\u00e4\u00b7fig", "in", "Ber\u00b7lin"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "APPR", "NE"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "der gelben Bestien Wollustschauer", "tokens": ["der", "gel\u00b7ben", "Be\u00b7sti\u00b7en", "Wol\u00b7lust\u00b7schau\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "sah stumm an sich vor\u00fcberziehn \u2013", "tokens": ["sah", "stumm", "an", "sich", "vor\u00b7\u00fc\u00b7ber\u00b7ziehn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PRF", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "da schrieb er auf in seinem B\u00fcchlein:", "tokens": ["da", "schrieb", "er", "auf", "in", "sei\u00b7nem", "B\u00fcch\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.2": {"text": "\u00bbder L\u00f6we liebt nicht vehement.", "tokens": ["\u00bb", "der", "L\u00f6\u00b7we", "liebt", "nicht", "ve\u00b7he\u00b7ment", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NE", "VVFIN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Leidenschaft auch nicht ein R\u00fcchlein;", "tokens": ["Von", "Lei\u00b7den\u00b7schaft", "auch", "nicht", "ein", "R\u00fcch\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "der schw\u00e4chste Mann scheint mehr potent.\u00ab", "tokens": ["der", "schw\u00e4chs\u00b7te", "Mann", "scheint", "mehr", "po\u00b7tent", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Wille macht noch kein Gewitter.", "tokens": ["Der", "Wil\u00b7le", "macht", "noch", "kein", "Ge\u00b7wit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gehirn! Gehirn geh\u00f6rt dazu.", "tokens": ["Ge\u00b7hirn", "!", "Ge\u00b7hirn", "ge\u00b7h\u00f6rt", "da\u00b7zu", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der muskelstarke Eisenritter", "tokens": ["Der", "mus\u00b7kels\u00b7tar\u00b7ke", "Ei\u00b7sen\u00b7rit\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "gibt bald im Frauenscho\u00dfe Ruh.", "tokens": ["gibt", "bald", "im", "Frau\u00b7en\u00b7scho\u00b7\u00dfe", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Du liebst. Und heller noch und wacher", "tokens": ["Du", "liebst", ".", "Und", "hel\u00b7ler", "noch", "und", "wa\u00b7cher"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "KON", "ADJD", "ADV", "KON", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "f\u00fchlt dein Gehirn und denkt dein Herz.", "tokens": ["f\u00fchlt", "dein", "Ge\u00b7hirn", "und", "denkt", "dein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Phallus ist ein Lustentfacher \u2013", "tokens": ["Der", "Phal\u00b7lus", "ist", "ein", "Lus\u00b7tent\u00b7fa\u00b7cher", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "du stehst und schwingst dich h\u00f6henw\u00e4rts.", "tokens": ["du", "stehst", "und", "schwingst", "dich", "h\u00f6\u00b7hen\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du liebst. Wo andre dumpf versinken,", "tokens": ["Du", "liebst", ".", "Wo", "and\u00b7re", "dumpf", "ver\u00b7sin\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PWAV", "PIS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "bist du erst tausendf\u00e4ltig da.", "tokens": ["bist", "du", "erst", "tau\u00b7send\u00b7f\u00e4l\u00b7tig", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df mich aus tausend Quellen trinken,", "tokens": ["La\u00df", "mich", "aus", "tau\u00b7send", "Quel\u00b7len", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "du Venus Reflectoria \u2013!", "tokens": ["du", "Ve\u00b7nus", "Re\u00b7flec\u00b7to\u00b7ria", "\u2013", "!"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPER", "NN", "NN", "$(", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Berauscht \u2013 ach, da\u00df ichs stets so bliebe!", "tokens": ["Be\u00b7rauscht", "\u2013", "ach", ",", "da\u00df", "ichs", "stets", "so", "blie\u00b7be", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "XY", "$,", "KOUS", "PIS", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Get\u00f6nt, bewu\u00dft, erh\u00f6ht, gestuft \u2013", "tokens": ["Ge\u00b7t\u00f6nt", ",", "be\u00b7wu\u00dft", ",", "er\u00b7h\u00f6ht", ",", "ge\u00b7stuft", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "$,", "VVPP", "$,", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das ist die wahre L\u00f6wenliebe.", "tokens": ["Das", "ist", "die", "wah\u00b7re", "L\u00f6\u00b7wen\u00b7lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du Raubtierfrau!", "tokens": ["Du", "Raub\u00b7tier\u00b7frau", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Es ruft. Es ruft.", "tokens": ["Es", "ruft", ".", "Es", "ruft", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}