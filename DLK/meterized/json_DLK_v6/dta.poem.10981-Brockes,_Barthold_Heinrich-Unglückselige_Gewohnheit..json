{"dta.poem.10981": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ungl\u00fcckselige Gewohnheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer in Arabiens versengten W\u00fcsteneyen,", "tokens": ["Wer", "in", "A\u00b7ra\u00b7biens", "ver\u00b7seng\u00b7ten", "W\u00fcs\u00b7te\u00b7ne\u00b7yen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "--+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Wo Wolken nichts, als Sand, statt feuchten Regen,", "tokens": ["Wo", "Wol\u00b7ken", "nichts", ",", "als", "Sand", ",", "statt", "feuch\u00b7ten", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PIS", "$,", "KOUS", "NN", "$,", "KOUI", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo das verbrannte Land von Laub und Grafe leer,", "tokens": ["Wo", "das", "ver\u00b7brann\u00b7te", "Land", "von", "Laub", "und", "Gra\u00b7fe", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gebohren und erzogen w\u00e4r;", "tokens": ["Ge\u00b7boh\u00b7ren", "und", "er\u00b7zo\u00b7gen", "w\u00e4r", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "H\u00e4tt aber nimmer Gras, nie Laub und Kraut,", "tokens": ["H\u00e4tt", "a\u00b7ber", "nim\u00b7mer", "Gras", ",", "nie", "Laub", "und", "Kraut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "NN", "$,", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nie Blumen, Bl\u00fcht und Frucht geschaut,", "tokens": ["Nie", "Blu\u00b7men", ",", "Bl\u00fcht", "und", "Frucht", "ge\u00b7schaut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und sehe denn von ungef\u00e4hr einmal,", "tokens": ["Und", "se\u00b7he", "denn", "von", "un\u00b7ge\u00b7f\u00e4hr", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Zumal zur Abendzeit, im niedern Sonnenstral,", "tokens": ["Zu\u00b7mal", "zur", "A\u00b7bend\u00b7zeit", ",", "im", "nie\u00b7dern", "Son\u00b7nen\u00b7stral", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Wiesen Glanz, bebl\u00fcmte Felder,", "tokens": ["Der", "Wie\u00b7sen", "Glanz", ",", "be\u00b7bl\u00fcm\u00b7te", "Fel\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Durchstralte Schatten-reiche W\u00e4lder,", "tokens": ["Durch\u00b7stral\u00b7te", "Schat\u00b7ten\u00b7rei\u00b7che", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und ihres Laubs fast g\u00fcldnes Gr\u00fcn;", "tokens": ["Und", "ih\u00b7res", "Laubs", "fast", "g\u00fcld\u00b7nes", "Gr\u00fcn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "W\u00fcrd ihm nicht, ausser sich f\u00fcr Freuden,", "tokens": ["W\u00fcrd", "ihm", "nicht", ",", "aus\u00b7ser", "sich", "f\u00fcr", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "$,", "APPR", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Sein Auge von so holdem Schein", "tokens": ["Sein", "Au\u00b7ge", "von", "so", "hol\u00b7dem", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und bunter Sch\u00f6nheit abzuziehn,", "tokens": ["Und", "bun\u00b7ter", "Sch\u00f6n\u00b7heit", "ab\u00b7zu\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Nicht schwer, ja fast unm\u00f6glich seyn?", "tokens": ["Nicht", "schwer", ",", "ja", "fast", "un\u00b7m\u00f6g\u00b7lich", "seyn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "W\u00fcrd nicht von ihm die\u00df Theil der Welt,", "tokens": ["W\u00fcrd", "nicht", "von", "ihm", "die\u00df", "Theil", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPR", "PPER", "PDS", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Das so viel Wunder in sich h\u00e4lt,", "tokens": ["Das", "so", "viel", "Wun\u00b7der", "in", "sich", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PIAT", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "F\u00fcr unvergleichlich sch\u00f6n gesch\u00e4tzet werden,", "tokens": ["F\u00fcr", "un\u00b7ver\u00b7gleich\u00b7lich", "sch\u00f6n", "ge\u00b7sch\u00e4t\u00b7zet", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJD", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Und die Bewohner dieser Erden", "tokens": ["Und", "die", "Be\u00b7woh\u00b7ner", "die\u00b7ser", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Von ihm nicht blo\u00df gl\u00fcckselig nur allein,", "tokens": ["Von", "ihm", "nicht", "blo\u00df", "gl\u00fcck\u00b7se\u00b7lig", "nur", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKNEG", "ADV", "ADJD", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Fast selig gar gesch\u00e4tzet seyn?", "tokens": ["Fast", "se\u00b7lig", "gar", "ge\u00b7sch\u00e4t\u00b7zet", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Da wir hingegen leider blind,", "tokens": ["Da", "wir", "hin\u00b7ge\u00b7gen", "lei\u00b7der", "blind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Und f\u00fchllos fast f\u00fcr alle Wunder sind.", "tokens": ["Und", "f\u00fchl\u00b7los", "fast", "f\u00fcr", "al\u00b7le", "Wun\u00b7der", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "APPR", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}