{"textgrid.poem.28050": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "65.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df kein Christ ietzt Wunder thut,", "tokens": ["Da\u00df", "kein", "Christ", "ietzt", "Wun\u00b7der", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Macht, der Glaub ist nicht recht gut;", "tokens": ["Macht", ",", "der", "Glaub", "ist", "nicht", "recht", "gut", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VAFIN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Drum ist rechter Glaub ietzunder", "tokens": ["Drum", "ist", "rech\u00b7ter", "Glaub", "ietz\u00b7un\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ADJA", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr sich selbst ein grosses Wunder.", "tokens": ["F\u00fcr", "sich", "selbst", "ein", "gros\u00b7ses", "Wun\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}