{"textgrid.poem.31540": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "Das erste:", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil die Seel-und Engel-Sonne / aller Himmel Klarheit Liecht /", "tokens": ["Weil", "die", "Seel\u00b7\u00b7und", "En\u00b7gel\u00b7Son\u00b7ne", "/", "al\u00b7ler", "Him\u00b7mel", "Klar\u00b7heit", "Liecht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NE", "$(", "PIAT", "NN", "NN", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "der selb-selbste Gottheit-Glanz / sich mit einer Wolk bedecket:", "tokens": ["der", "selb\u00b7\u00b7selbs\u00b7te", "Gott\u00b7heit\u00b7Glanz", "/", "sich", "mit", "ei\u00b7ner", "Wolk", "be\u00b7de\u00b7cket", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "ist es billich / da\u00df sich auch mein betr\u00fcbter Schein verstecket.", "tokens": ["ist", "es", "bil\u00b7lich", "/", "da\u00df", "sich", "auch", "mein", "be\u00b7tr\u00fcb\u00b7ter", "Schein", "ver\u00b7ste\u00b7cket", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$(", "KOUS", "PRF", "ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Wer wolt / wann Gott selber leidet / sich doch hoch betr\u00fcben nicht?", "tokens": ["Wer", "wolt", "/", "wann", "Gott", "sel\u00b7ber", "lei\u00b7det", "/", "sich", "doch", "hoch", "be\u00b7tr\u00fc\u00b7ben", "nicht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "$(", "PWAV", "NN", "ADV", "VVFIN", "$(", "PRF", "ADV", "ADJD", "VVFIN", "PTKNEG", "$."], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "der ist unwehrt meiner Strahlen / der Ihn unbetr\u00fcbt ansicht.", "tokens": ["der", "ist", "un\u00b7wehrt", "mei\u00b7ner", "Strah\u00b7len", "/", "der", "Ihn", "un\u00b7be\u00b7tr\u00fcbt", "an\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "VAFIN", "ADJD", "PPOSAT", "NN", "$(", "PRELS", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Eine neue Gnaden-Sonn diese Dunkelangst / erwecket;", "tokens": ["Ei\u00b7ne", "neu\u00b7e", "Gna\u00b7den\u00b7Sonn", "die\u00b7se", "Dun\u00b7ke\u00b7langst", "/", "er\u00b7we\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PDAT", "NN", "$(", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "vor derselben Hitz und Blitz meine Brunst und Gunst nicht klecket.", "tokens": ["vor", "der\u00b7sel\u00b7ben", "Hitz", "und", "Blitz", "mei\u00b7ne", "Brunst", "und", "Gunst", "nicht", "kle\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "KON", "NN", "PPOSAT", "NN", "KON", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "keine nebelluft noch Wolke ihr begl\u00e4nzen unterbricht.", "tokens": ["kei\u00b7ne", "ne\u00b7bel\u00b7luft", "noch", "Wol\u00b7ke", "ihr", "be\u00b7gl\u00e4n\u00b7zen", "un\u00b7ter\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Ach ich kan ja einmal nicht meine Vrquell sterben sehen /", "tokens": ["Ach", "ich", "kan", "ja", "ein\u00b7mal", "nicht", "mei\u00b7ne", "Vrquell", "ster\u00b7ben", "se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VMFIN", "ADV", "ADV", "PTKNEG", "PPOSAT", "NN", "VVINF", "VVINF", "$("], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}, "line.10": {"text": "da\u00df der Edle Mund verseuffzet / der da sprach mein werden aus.", "tokens": ["da\u00df", "der", "Ed\u00b7le", "Mund", "ver\u00b7seuff\u00b7zet", "/", "der", "da", "sprach", "mein", "wer\u00b7den", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$(", "ART", "ADV", "VVFIN", "PPOSAT", "VAINF", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "ich w\u00e4hl / vor des Liechtes-Liechts / eh mein eignes / untergehen.", "tokens": ["ich", "w\u00e4hl", "/", "vor", "des", "Liech\u00b7tes\u00b7Liechts", "/", "eh", "mein", "eig\u00b7nes", "/", "un\u00b7ter\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "APPR", "ART", "NN", "$(", "KOUS", "PPOSAT", "ADJA", "$(", "VVINF", "$."], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Ach ihr blinde Menschen / sehet eurer S\u00fcnden Greuel-Grau\u00df /", "tokens": ["Ach", "ihr", "blin\u00b7de", "Men\u00b7schen", "/", "se\u00b7het", "eu\u00b7rer", "S\u00fcn\u00b7den", "Greu\u00b7el\u00b7Grau\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "ADJA", "NN", "$(", "VVFIN", "PPOSAT", "NN", "NE", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "da\u00df ihr' H\u00f6llen-Laster-Schw\u00e4rz' auch den Gottdurchkl\u00e4rten dunkelt.", "tokens": ["da\u00df", "ih\u00b7r'", "H\u00f6l\u00b7len\u00b7Las\u00b7ter\u00b7Schw\u00e4rz'", "auch", "den", "Gott\u00b7durch\u00b7kl\u00e4r\u00b7ten", "dun\u00b7kelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.14": {"text": "Aus der \u00e4ussern eu\u00dfrers Noht / eur' Erl\u00f6sungs-Sonne funkelt.", "tokens": ["Aus", "der", "\u00e4us\u00b7sern", "eu\u00df\u00b7rers", "Noht", "/", "eur'", "Er\u00b7l\u00f6\u00b7sungs\u00b7Son\u00b7ne", "fun\u00b7kelt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$(", "PPOSAT", "NN", "VVFIN", "$."], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}