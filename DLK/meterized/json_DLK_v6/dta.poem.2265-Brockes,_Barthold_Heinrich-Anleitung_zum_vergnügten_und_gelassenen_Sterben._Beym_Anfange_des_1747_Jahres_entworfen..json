{"dta.poem.2265": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Anleitung  \n zum vergn\u00fcgten und gelassenen  \n Sterben.  \n Beym Anfange des 1747 Jahres entworfen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da sich die Erd itzt von der Sonnen in ih-", "tokens": ["Da", "sich", "die", "Erd", "itzt", "von", "der", "Son\u00b7nen", "in", "ih"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "ADV", "APPR", "ART", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "rem Kreislauf abwerts drehet,", "tokens": ["rem", "Kreis\u00b7lauf", "ab\u00b7werts", "dre\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und durch die\u00df Drehn auf unsrer Fl\u00e4che", "tokens": ["Und", "durch", "die\u00df", "Drehn", "auf", "uns\u00b7rer", "Fl\u00e4\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PDS", "CARD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "der Lenz u. Sommer uns entstehet,", "tokens": ["der", "Lenz", "u.", "Som\u00b7mer", "uns", "ent\u00b7ste\u00b7het", ","], "token_info": ["word", "word", "abbreviation", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wodurch, auf eine weise Weise, sich das", "tokens": ["Wo\u00b7durch", ",", "auf", "ei\u00b7ne", "wei\u00b7se", "Wei\u00b7se", ",", "sich", "das"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "$,", "APPR", "ART", "ADJA", "NN", "$,", "PRF", "ART"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Geb\u00e4ude dieser Welt", "tokens": ["Ge\u00b7b\u00e4u\u00b7de", "die\u00b7ser", "Welt"], "token_info": ["word", "word", "word"], "pos": ["NN", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In solcher richtgen Wunderordnung zum Nutzen der Ge-", "tokens": ["In", "sol\u00b7cher", "richt\u00b7gen", "Wun\u00b7der\u00b7ord\u00b7nung", "zum", "Nut\u00b7zen", "der", "Ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN", "APPRART", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "sch\u00f6pf erh\u00e4lt:", "tokens": ["sch\u00f6pf", "er\u00b7h\u00e4lt", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Will ich, bey dieser Wechselzeit, des Sch\u00f6pfers weise Macht", "tokens": ["Will", "ich", ",", "bey", "die\u00b7ser", "Wech\u00b7sel\u00b7zeit", ",", "des", "Sch\u00f6p\u00b7fers", "wei\u00b7se", "Macht"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "$,", "APPR", "PDAT", "NN", "$,", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "erheben,", "tokens": ["er\u00b7he\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Und, seine Liebe zu besingen, wie ich gewohnt bin, mich be-", "tokens": ["Und", ",", "sei\u00b7ne", "Lie\u00b7be", "zu", "be\u00b7sin\u00b7gen", ",", "wie", "ich", "ge\u00b7wohnt", "bin", ",", "mich", "be"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,", "PWAV", "PPER", "VVPP", "VAFIN", "$,", "PRF", "TRUNC"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "streben;", "tokens": ["stre\u00b7ben", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Wobey mein Vorsatz: Bey der Aendrung der Jahres-", "tokens": ["Wo\u00b7bey", "mein", "Vor\u00b7satz", ":", "Bey", "der", "A\u00b7en\u00b7drung", "der", "Jah\u00b7res"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "$.", "APPR", "ART", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "zeit, von einer Zeit,", "tokens": ["zeit", ",", "von", "ei\u00b7ner", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.15": {"text": "Die ferner keinen Wechsel kennet, woselbst kein Frost den", "tokens": ["Die", "fer\u00b7ner", "kei\u00b7nen", "Wech\u00b7sel", "ken\u00b7net", ",", "wo\u00b7selbst", "kein", "Frost", "den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PIAT", "NN", "VVFIN", "$,", "PWAV", "PIAT", "NN", "ART"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Lenz vertreibt,", "tokens": ["Lenz", "ver\u00b7treibt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.17": {"text": "Wo alles, was dazu beschieden, in ungest\u00f6rter Seligkeit,", "tokens": ["Wo", "al\u00b7les", ",", "was", "da\u00b7zu", "be\u00b7schie\u00b7den", ",", "in", "un\u00b7ge\u00b7st\u00f6r\u00b7ter", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$,", "PWS", "PAV", "VVPP", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.18": {"text": "In nimmerunterbrochnem Frieden und ewigem Vergn\u00fc-", "tokens": ["In", "nim\u00b7me\u00b7run\u00b7ter\u00b7broch\u00b7nem", "Frie\u00b7den", "und", "e\u00b7wi\u00b7gem", "Ver\u00b7gn\u00fc"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+---+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "gen bleibt;", "tokens": ["gen", "bleibt", ";"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.20": {"text": "Wornach vern\u00fcnftige Gesch\u00f6pfe, wenn sie hier ausgelebt,", "tokens": ["Wor\u00b7nach", "ver\u00b7n\u00fcnf\u00b7ti\u00b7ge", "Ge\u00b7sch\u00f6p\u00b7fe", ",", "wenn", "sie", "hier", "aus\u00b7ge\u00b7lebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "NN", "$,", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.21": {"text": "sich sehnen,", "tokens": ["sich", "seh\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["PRF", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.22": {"text": "Absonderlich vom ", "tokens": ["Ab\u00b7son\u00b7der\u00b7lich", "vom"], "token_info": ["word", "word"], "pos": ["ADV", "APPRART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.23": {"text": "was zu erw\u00e4hnen.", "tokens": ["was", "zu", "er\u00b7w\u00e4h\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PTKZU", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}