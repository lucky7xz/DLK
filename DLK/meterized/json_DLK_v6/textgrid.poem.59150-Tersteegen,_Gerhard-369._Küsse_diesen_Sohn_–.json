{"textgrid.poem.59150": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "369. K\u00fcsse diesen Sohn \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich beuge mich so tief zu dir,", "tokens": ["Ich", "beu\u00b7ge", "mich", "so", "tief", "zu", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dich, krankes Kindlein, zu umarmen;", "tokens": ["Dich", ",", "kran\u00b7kes", "Kin\u00b7dlein", ",", "zu", "um\u00b7ar\u00b7men", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Komm, k\u00fc\u00df mich auch, ergib dich mir", "tokens": ["Komm", ",", "k\u00fc\u00df", "mich", "auch", ",", "er\u00b7gib", "dich", "mir"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "ADV", "$,", "VVIMP", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und liege still in meinen Armen!", "tokens": ["Und", "lie\u00b7ge", "still", "in", "mei\u00b7nen", "Ar\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}