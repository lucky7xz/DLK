{"textgrid.poem.63106": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Epitaph als Epilog", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier ruhen siebenundzwanzig Jungfrauen aus Stralsund,", "tokens": ["Hier", "ru\u00b7hen", "sie\u00b7ben\u00b7und\u00b7zwan\u00b7zig", "Jung\u00b7frau\u00b7en", "aus", "Stral\u00b7sund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "CARD", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Denen ward durch einen Interpreten des Dichters neueste Dichtung kund.", "tokens": ["De\u00b7nen", "ward", "durch", "ei\u00b7nen", "In\u00b7ter\u00b7pre\u00b7ten", "des", "Dich\u00b7ters", "neu\u00b7es\u00b7te", "Dich\u00b7tung", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+--+-+--+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.3": {"text": "Die hat die empfindsamen M\u00e4dchenherzen so sehr begeistert,", "tokens": ["Die", "hat", "die", "emp\u00b7find\u00b7sa\u00b7men", "M\u00e4d\u00b7chen\u00b7her\u00b7zen", "so", "sehr", "be\u00b7geis\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+--+--+-+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Da\u00df auch nicht eine mehr ihr Gef\u00fchl gemeistert.", "tokens": ["Da\u00df", "auch", "nicht", "ei\u00b7ne", "mehr", "ihr", "Ge\u00b7f\u00fchl", "ge\u00b7meis\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PTKNEG", "ART", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Man h\u00e4ngte sich teils auf, teils ging man in die See.", "tokens": ["Man", "h\u00e4ng\u00b7te", "sich", "teils", "auf", ",", "teils", "ging", "man", "in", "die", "See", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "ADV", "PTKVZ", "$,", "ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nur eine ging zum Dichter selbst. (Und zwar aufs Kanapee.)", "tokens": ["Nur", "ei\u00b7ne", "ging", "zum", "Dich\u00b7ter", "selbst", ".", "(", "Und", "zwar", "aufs", "Ka\u00b7na\u00b7pee", ".", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "VVFIN", "APPRART", "NN", "ADV", "$.", "$(", "KON", "ADV", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}