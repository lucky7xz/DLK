{"textgrid.poem.58928": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "147. Heute! Heute!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verschwende nicht die Zeit des Lebens,", "tokens": ["Ver\u00b7schwen\u00b7de", "nicht", "die", "Zeit", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gott gibt kein'n Augenblick vergebens!", "tokens": ["Gott", "gibt", "kein'n", "Au\u00b7gen\u00b7blick", "ver\u00b7ge\u00b7bens", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sei stets im Wachen und Gebet;", "tokens": ["Sei", "stets", "im", "Wa\u00b7chen", "und", "Ge\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wer nun nicht l\u00e4uft, der kommt zu sp\u00e4t!", "tokens": ["Wer", "nun", "nicht", "l\u00e4uft", ",", "der", "kommt", "zu", "sp\u00e4t", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PTKNEG", "VVFIN", "$,", "PRELS", "VVFIN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}