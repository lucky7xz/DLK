{"textgrid.poem.32643": {"metadata": {"author": {"name": "Pet\u00f6fi, S\u00e1ndor", "birth": "N.A.", "death": "N.A."}, "title": "1L: Am Kreuzweg steh' ich hier,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Kreuzweg steh' ich hier,", "tokens": ["Am", "Kreuz\u00b7weg", "steh'", "ich", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wie geh' ich wohl am besten?", "tokens": ["Wie", "geh'", "ich", "wohl", "am", "bes\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Stra\u00dfe f\u00fchrt gen Ost,", "tokens": ["Die", "Stra\u00b7\u00dfe", "f\u00fchrt", "gen", "Ost", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und jene f\u00fchrt gen Westen.", "tokens": ["Und", "je\u00b7ne", "f\u00fchrt", "gen", "Wes\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wohin ich immer geh',", "tokens": ["Wo\u00b7hin", "ich", "im\u00b7mer", "geh'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Was w\u00e4r' auch dran gelegen?", "tokens": ["Was", "w\u00e4r'", "auch", "dran", "ge\u00b7le\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "PAV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ist doch voll Traurigkeit", "tokens": ["Ist", "doch", "voll", "Trau\u00b7rig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADJD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mein Leben allerwegen.", "tokens": ["Mein", "Le\u00b7ben", "al\u00b7ler\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O warum wei\u00df ich nicht,", "tokens": ["O", "wa\u00b7rum", "wei\u00df", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wo meiner harrt das Ende?", "tokens": ["Wo", "mei\u00b7ner", "harrt", "das", "En\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wie den graden Weg", "tokens": ["Und", "wie", "den", "gra\u00b7den", "Weg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dahin ich fr\u00fcher f\u00e4nde?", "tokens": ["Da\u00b7hin", "ich", "fr\u00fc\u00b7her", "f\u00e4n\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}