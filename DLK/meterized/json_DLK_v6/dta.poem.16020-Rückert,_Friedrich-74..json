{"dta.poem.16020": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "74.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Geist des Menschen f\u00fchlt sich v\u00f6llig zweierlei,", "tokens": ["Der", "Geist", "des", "Men\u00b7schen", "f\u00fchlt", "sich", "v\u00f6l\u00b7lig", "zwei\u00b7er\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PRF", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Abh\u00e4ngig ganz und gar, und unabh\u00e4ngig frei.", "tokens": ["Ab\u00b7h\u00e4n\u00b7gig", "ganz", "und", "gar", ",", "und", "un\u00b7ab\u00b7h\u00e4n\u00b7gig", "frei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "KON", "ADV", "$,", "KON", "ADJD", "PTKVZ", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Abh\u00e4ngig, insofern er Gott im Auge h\u00e4lt,", "tokens": ["Ab\u00b7h\u00e4n\u00b7gig", ",", "in\u00b7so\u00b7fern", "er", "Gott", "im", "Au\u00b7ge", "h\u00e4lt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Und unabh\u00e4ngig, wo er vor sich hat die Welt.", "tokens": ["Und", "un\u00b7ab\u00b7h\u00e4n\u00b7gig", ",", "wo", "er", "vor", "sich", "hat", "die", "Welt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "PPER", "APPR", "PRF", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Vorm Vater unfrei f\u00fchlt sich so ein Sohn vom Haus,", "tokens": ["Vorm", "Va\u00b7ter", "un\u00b7frei", "f\u00fchlt", "sich", "so", "ein", "Sohn", "vom", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVFIN", "PRF", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Selbst\u00e4ndig aber wohl, sobald er tritt hinaus.", "tokens": ["Selb\u00b7st\u00e4n\u00b7dig", "a\u00b7ber", "wohl", ",", "so\u00b7bald", "er", "tritt", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADV", "$,", "KOUS", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}