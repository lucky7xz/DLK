{"dta.poem.9698": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen ku\u00df.  \n E. N.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein ku\u00df! ein ku\u00df! ein ku\u00df!", "tokens": ["Ein", "ku\u00df", "!", "ein", "ku\u00df", "!", "ein", "ku\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ach ich bin gantz entz\u00fcckt/", "tokens": ["Ach", "ich", "bin", "gantz", "ent\u00b7z\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da mich doch nur ein eintziger erquickt/", "tokens": ["Da", "mich", "doch", "nur", "ein", "eint\u00b7zi\u00b7ger", "er\u00b7quickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ART", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein ku\u00df/ das ist ein ku\u00df.", "tokens": ["Ein", "ku\u00df", "/", "das", "ist", "ein", "ku\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PDS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ach soll ich noch mehr sagen/", "tokens": ["Ach", "soll", "ich", "noch", "mehr", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So mu\u00df ich noch einmahl die s\u00fcssen lippen fragen/", "tokens": ["So", "mu\u00df", "ich", "noch", "ein\u00b7mahl", "die", "s\u00fcs\u00b7sen", "lip\u00b7pen", "fra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wovor man wol solch labsal halten mu\u00df?", "tokens": ["Wo\u00b7vor", "man", "wol", "solch", "lab\u00b7sal", "hal\u00b7ten", "mu\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Doch/ Jris/ stimme nur mit hundert k\u00fc\u00dfen bey/", "tokens": ["Doch", "/", "Jris", "/", "stim\u00b7me", "nur", "mit", "hun\u00b7dert", "k\u00fc\u00b7\u00dfen", "bey", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "NE", "$(", "VVFIN", "ADV", "APPR", "CARD", "VVFIN", "APPR", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.9": {"text": "Ich sage so/ da\u00df k\u00fc\u00dfen in der liebe", "tokens": ["Ich", "sa\u00b7ge", "so", "/", "da\u00df", "k\u00fc\u00b7\u00dfen", "in", "der", "lie\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$(", "KOUS", "VVFIN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Die quinte\u00dfence sey.", "tokens": ["Die", "quin\u00b7te\u00b7\u00dfen\u00b7ce", "sey", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}