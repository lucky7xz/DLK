{"textgrid.poem.34701": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Still versteckt der Mond sich drau\u00dfen", "tokens": ["Still", "ver\u00b7steckt", "der", "Mond", "sich", "drau\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PRF", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hinterm gr\u00fcnen Tannenbaum,", "tokens": ["Hin\u00b7term", "gr\u00fc\u00b7nen", "Tan\u00b7nen\u00b7baum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und im Zimmer unsre Lampe", "tokens": ["Und", "im", "Zim\u00b7mer", "uns\u00b7re", "Lam\u00b7pe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Flackert matt und leuchtet kaum.", "tokens": ["Fla\u00b7ckert", "matt", "und", "leuch\u00b7tet", "kaum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber meine blauen Sterne", "tokens": ["A\u00b7ber", "mei\u00b7ne", "blau\u00b7en", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Strahlen auf in hellerm Licht,", "tokens": ["Strah\u00b7len", "auf", "in", "hel\u00b7lerm", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es gl\u00fchn die Purpurr\u00f6slein,", "tokens": ["Und", "es", "gl\u00fchn", "die", "Pur\u00b7pur\u00b7r\u00f6s\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und das liebe M\u00e4dchen spricht:", "tokens": ["Und", "das", "lie\u00b7be", "M\u00e4d\u00b7chen", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbkleines V\u00f6lkchen, Wichtelm\u00e4nnchen,", "tokens": ["\u00bb", "klei\u00b7nes", "V\u00f6lk\u00b7chen", ",", "Wich\u00b7tel\u00b7m\u00e4nn\u00b7chen", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stehlen unser Brot und Speck,", "tokens": ["Steh\u00b7len", "un\u00b7ser", "Brot", "und", "Speck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Abends liegt es noch im Kasten,", "tokens": ["A\u00b7bends", "liegt", "es", "noch", "im", "Kas\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und des Morgens ist es weg.", "tokens": ["Und", "des", "Mor\u00b7gens", "ist", "es", "weg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Kleines V\u00f6lkchen, unsre Sahne", "tokens": ["Klei\u00b7nes", "V\u00f6lk\u00b7chen", ",", "uns\u00b7re", "Sah\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nascht es von der Milch, und l\u00e4\u00dft", "tokens": ["Nascht", "es", "von", "der", "Milch", ",", "und", "l\u00e4\u00dft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unbedeckt die Sch\u00fcssel stehen,", "tokens": ["Un\u00b7be\u00b7deckt", "die", "Sch\u00fcs\u00b7sel", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Katze s\u00e4uft den Rest.", "tokens": ["Und", "die", "Kat\u00b7ze", "s\u00e4uft", "den", "Rest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und die Katz' ist eine Hexe,", "tokens": ["Und", "die", "Katz'", "ist", "ei\u00b7ne", "He\u00b7xe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Denn sie schleicht, bei Nacht und Sturm,", "tokens": ["Denn", "sie", "schleicht", ",", "bei", "Nacht", "und", "Sturm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dr\u00fcben nach dem Geisterberge,", "tokens": ["Dr\u00fc\u00b7ben", "nach", "dem", "Geis\u00b7ter\u00b7ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach dem altverfallnen Turm.", "tokens": ["Nach", "dem", "alt\u00b7ver\u00b7fall\u00b7nen", "Turm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Dort hat einst ein Schlo\u00df gestanden,", "tokens": ["Dort", "hat", "einst", "ein", "Schlo\u00df", "ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Voller Lust und Waffenglanz;", "tokens": ["Vol\u00b7ler", "Lust", "und", "Waf\u00b7fen\u00b7glanz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Blanke Ritter, Fraun und Knappen", "tokens": ["Blan\u00b7ke", "Rit\u00b7ter", ",", "Fraun", "und", "Knap\u00b7pen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "NE", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwangen sich im Fackeltanz.", "tokens": ["Schwan\u00b7gen", "sich", "im", "Fa\u00b7ckel\u00b7tanz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Da verw\u00fcnschte Schlo\u00df und Leute", "tokens": ["Da", "ver\u00b7w\u00fcnschte", "Schlo\u00df", "und", "Leu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN"], "meter": "--++-+-", "measure": "anapaest.init"}, "line.2": {"text": "Eine b\u00f6se Zauberin,", "tokens": ["Ei\u00b7ne", "b\u00f6\u00b7se", "Zau\u00b7be\u00b7rin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur die Tr\u00fcmmer blieben stehen,", "tokens": ["Nur", "die", "Tr\u00fcm\u00b7mer", "blie\u00b7ben", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Eulen nisten drin.", "tokens": ["Und", "die", "Eu\u00b7len", "nis\u00b7ten", "drin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Doch die sel'ge Muhme sagte:", "tokens": ["Doch", "die", "sel'\u00b7ge", "Muh\u00b7me", "sag\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn man spricht das rechte Wort,", "tokens": ["Wenn", "man", "spricht", "das", "rech\u00b7te", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "N\u00e4chtlich zu der rechten Stunde,", "tokens": ["N\u00e4cht\u00b7lich", "zu", "der", "rech\u00b7ten", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dr\u00fcben an dem rechten Ort:", "tokens": ["Dr\u00fc\u00b7ben", "an", "dem", "rech\u00b7ten", "Ort", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "So verwandeln sich die Tr\u00fcmmer", "tokens": ["So", "ver\u00b7wan\u00b7deln", "sich", "die", "Tr\u00fcm\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wieder in ein helles Schlo\u00df,", "tokens": ["Wie\u00b7der", "in", "ein", "hel\u00b7les", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es tanzen wieder lustig", "tokens": ["Und", "es", "tan\u00b7zen", "wie\u00b7der", "lus\u00b7tig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ritter, Fraun und Knappentro\u00df;", "tokens": ["Rit\u00b7ter", ",", "Fraun", "und", "Knap\u00b7pen\u00b7tro\u00df", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Und wer jenes Wort gesprochen,", "tokens": ["Und", "wer", "je\u00b7nes", "Wort", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PDAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dem geh\u00f6ren Schlo\u00df und Leut',", "tokens": ["Dem", "ge\u00b7h\u00f6\u00b7ren", "Schlo\u00df", "und", "Leut'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Pauken und Trompeten huld'gen", "tokens": ["Pau\u00b7ken", "und", "Trom\u00b7pe\u00b7ten", "huld'\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Seiner jungen Herrlichkeit.\u00ab", "tokens": ["Sei\u00b7ner", "jun\u00b7gen", "Herr\u00b7lich\u00b7keit", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Also bl\u00fchen M\u00e4rchenbilder", "tokens": ["Al\u00b7so", "bl\u00fc\u00b7hen", "M\u00e4r\u00b7chen\u00b7bil\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus des Mundes R\u00f6selein,", "tokens": ["Aus", "des", "Mun\u00b7des", "R\u00f6\u00b7se\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Augen gie\u00dfen dr\u00fcber", "tokens": ["Und", "die", "Au\u00b7gen", "gie\u00b7\u00dfen", "dr\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PAV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihren blauen Sternenschein.", "tokens": ["Ih\u00b7ren", "blau\u00b7en", "Ster\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Ihre goldnen Haare wickelt", "tokens": ["Ih\u00b7re", "gold\u00b7nen", "Haa\u00b7re", "wi\u00b7ckelt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir die Kleine um die H\u00e4nd',", "tokens": ["Mir", "die", "Klei\u00b7ne", "um", "die", "H\u00e4nd'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gibt den Fingern h\u00fcbsche Namen,", "tokens": ["Gibt", "den", "Fin\u00b7gern", "h\u00fcb\u00b7sche", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lacht und k\u00fc\u00dft, und schweigt am End'.", "tokens": ["Lacht", "und", "k\u00fc\u00dft", ",", "und", "schweigt", "am", "End'", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Und im stillen Zimmer alles", "tokens": ["Und", "im", "stil\u00b7len", "Zim\u00b7mer", "al\u00b7les"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "PIS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blickt mich an so wohlvertraut;", "tokens": ["Blickt", "mich", "an", "so", "wohl\u00b7ver\u00b7traut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tisch und Schrank, mir ist, als h\u00e4tt ich", "tokens": ["Tisch", "und", "Schrank", ",", "mir", "ist", ",", "als", "h\u00e4tt", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "PPER", "VAFIN", "$,", "KOKOM", "VAFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie schon fr\u00fcher mal geschaut.", "tokens": ["Sie", "schon", "fr\u00fc\u00b7her", "mal", "ge\u00b7schaut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Freundlich ernsthaft schwatzt die Wanduhr,", "tokens": ["Freund\u00b7lich", "ernst\u00b7haft", "schwatzt", "die", "Wan\u00b7duhr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Zither, h\u00f6rbar kaum,", "tokens": ["Und", "die", "Zi\u00b7ther", ",", "h\u00f6r\u00b7bar", "kaum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADJD", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00e4ngt von selber an zu klingen,", "tokens": ["F\u00e4ngt", "von", "sel\u00b7ber", "an", "zu", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADV", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich sitze wie im Traum.", "tokens": ["Und", "ich", "sit\u00b7ze", "wie", "im", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KOKOM", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Jetzo ist die rechte Stunde,", "tokens": ["Jet\u00b7zo", "ist", "die", "rech\u00b7te", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und es ist der rechte Ort;", "tokens": ["Und", "es", "ist", "der", "rech\u00b7te", "Ort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja, ich glaube, von den Lippen", "tokens": ["Ja", ",", "ich", "glau\u00b7be", ",", "von", "den", "Lip\u00b7pen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gleitet mir das rechte Wort.", "tokens": ["Glei\u00b7tet", "mir", "das", "rech\u00b7te", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "\u00bbsiehst du, Kindchen, wie schon d\u00e4mmert", "tokens": ["\u00bb", "siehst", "du", ",", "Kind\u00b7chen", ",", "wie", "schon", "d\u00e4m\u00b7mert"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "PPER", "$,", "NN", "$,", "PWAV", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und erbebt die Mitternacht!", "tokens": ["Und", "er\u00b7bebt", "die", "Mit\u00b7ter\u00b7nacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bach und Tannen brausen lauter,", "tokens": ["Bach", "und", "Tan\u00b7nen", "brau\u00b7sen", "lau\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der alte Berg erwacht.", "tokens": ["Und", "der", "al\u00b7te", "Berg", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.17": {"line.1": {"text": "Zitherklang und Zwergenlieder", "tokens": ["Zi\u00b7ther\u00b7klang", "und", "Zwer\u00b7gen\u00b7lie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "T\u00f6nen aus des Berges Spalt,", "tokens": ["T\u00f6\u00b7nen", "aus", "des", "Ber\u00b7ges", "Spalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es sprie\u00dfet, wie 'n toller Fr\u00fchling,", "tokens": ["Und", "es", "sprie\u00b7\u00dfet", ",", "wie", "'n", "tol\u00b7ler", "Fr\u00fch\u00b7ling", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Draus hervor ein Blumenwald; \u2013", "tokens": ["Draus", "her\u00b7vor", "ein", "Blu\u00b7men\u00b7wald", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "PTKVZ", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Blumen, k\u00fchne Wunderblumen,", "tokens": ["Blu\u00b7men", ",", "k\u00fch\u00b7ne", "Wun\u00b7der\u00b7blu\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bl\u00e4tter, breit und fabelhaft,", "tokens": ["Bl\u00e4t\u00b7ter", ",", "breit", "und", "fa\u00b7bel\u00b7haft", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Duftig bunt und hastig regsam,", "tokens": ["Duf\u00b7tig", "bunt", "und", "has\u00b7tig", "reg\u00b7sam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KON", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie gedr\u00e4ngt von Leidenschaft.", "tokens": ["Wie", "ge\u00b7dr\u00e4ngt", "von", "Lei\u00b7den\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Rosen, wild wie rote Flammen,", "tokens": ["Ro\u00b7sen", ",", "wild", "wie", "ro\u00b7te", "Flam\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "KOKOM", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spr\u00fchn aus dem Gew\u00fchl hervor;", "tokens": ["Spr\u00fchn", "aus", "dem", "Ge\u00b7w\u00fchl", "her\u00b7vor", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lilien, wie kristallne Pfeiler,", "tokens": ["Li\u00b7li\u00b7en", ",", "wie", "kris\u00b7tall\u00b7ne", "Pfei\u00b7ler", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Schie\u00dfen himmelhoch empor.", "tokens": ["Schie\u00b7\u00dfen", "him\u00b7mel\u00b7hoch", "em\u00b7por", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Und die Sterne, gro\u00df wie Sonnen,", "tokens": ["Und", "die", "Ster\u00b7ne", ",", "gro\u00df", "wie", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADJD", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schaun herab mit Sehnsuchtglut;", "tokens": ["Schaun", "her\u00b7ab", "mit", "Sehn\u00b7sucht\u00b7glut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Lilien Riesenkelche", "tokens": ["In", "der", "Li\u00b7li\u00b7en", "Rie\u00b7sen\u00b7kel\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Str\u00f6met ihre Strahlenflut.", "tokens": ["Str\u00f6\u00b7met", "ih\u00b7re", "Strah\u00b7len\u00b7flut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.21": {"line.1": {"text": "Doch wir selber, s\u00fc\u00dfes Kindchen,", "tokens": ["Doch", "wir", "sel\u00b7ber", ",", "s\u00fc\u00b7\u00dfes", "Kind\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind verwandelt noch viel mehr;", "tokens": ["Sind", "ver\u00b7wan\u00b7delt", "noch", "viel", "mehr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "ADV", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fackelglanz und Gold und Seide", "tokens": ["Fa\u00b7ckel\u00b7glanz", "und", "Gold", "und", "Sei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schimmern lustig um uns her.", "tokens": ["Schim\u00b7mern", "lus\u00b7tig", "um", "uns", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.22": {"line.1": {"text": "Du, du wurdest zur Prinzessin,", "tokens": ["Du", ",", "du", "wur\u00b7dest", "zur", "Prin\u00b7zes\u00b7sin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "VAFIN", "APPRART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Diese H\u00fctte ward zum Schlo\u00df,", "tokens": ["Die\u00b7se", "H\u00fct\u00b7te", "ward", "zum", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und da jubeln und da tanzen", "tokens": ["Und", "da", "ju\u00b7beln", "und", "da", "tan\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVINF", "KON", "ADV", "VVINF"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Ritter, Fraun und Knappentro\u00df,", "tokens": ["Rit\u00b7ter", ",", "Fraun", "und", "Knap\u00b7pen\u00b7tro\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "KON", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.23": {"line.1": {"text": "Aber ich, ich hab erworben", "tokens": ["A\u00b7ber", "ich", ",", "ich", "hab", "er\u00b7wor\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "PPER", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dich und alles, Schlo\u00df und Leut';", "tokens": ["Dich", "und", "al\u00b7les", ",", "Schlo\u00df", "und", "Leut'", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PIS", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Pauken und Trompeten huld'gen", "tokens": ["Pau\u00b7ken", "und", "Trom\u00b7pe\u00b7ten", "huld'\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Meiner jungen Herrlichkeit!\u00ab", "tokens": ["Mei\u00b7ner", "jun\u00b7gen", "Herr\u00b7lich\u00b7keit", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}