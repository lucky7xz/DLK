{"textgrid.poem.55250": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Demut", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seh ich die Werke der Meister an,", "tokens": ["Seh", "ich", "die", "Wer\u00b7ke", "der", "Meis\u00b7ter", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "So seh ich das, was sie getan;", "tokens": ["So", "seh", "ich", "das", ",", "was", "sie", "ge\u00b7tan", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDS", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Betracht ich meine Siebensachen,", "tokens": ["Be\u00b7tracht", "ich", "mei\u00b7ne", "Sie\u00b7ben\u00b7sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Seh ich, was ich h\u00e4tt sollen machen.", "tokens": ["Seh", "ich", ",", "was", "ich", "h\u00e4tt", "sol\u00b7len", "ma\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWS", "PPER", "VAFIN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}