{"textgrid.poem.34847": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl unter der Linde erklingt die Musik,", "tokens": ["Wohl", "un\u00b7ter", "der", "Lin\u00b7de", "er\u00b7klingt", "die", "Mu\u00b7sik", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Da tanzen die Burschen und M\u00e4del,", "tokens": ["Da", "tan\u00b7zen", "die", "Bur\u00b7schen", "und", "M\u00e4\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Da tanzen zwei, die niemand kennt,", "tokens": ["Da", "tan\u00b7zen", "zwei", ",", "die", "nie\u00b7mand", "kennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "CARD", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie schaun so schlank und edel.", "tokens": ["Sie", "schaun", "so", "schlank", "und", "e\u00b7del", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie schweben auf, sie schweben ab,", "tokens": ["Sie", "schwe\u00b7ben", "auf", ",", "sie", "schwe\u00b7ben", "ab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In seltsam fremder Weise;", "tokens": ["In", "selt\u00b7sam", "frem\u00b7der", "Wei\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie lachen sich an, sie sch\u00fctteln das Haupt,", "tokens": ["Sie", "la\u00b7chen", "sich", "an", ",", "sie", "sch\u00fct\u00b7teln", "das", "Haupt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das Fr\u00e4ulein fl\u00fcstert leise:", "tokens": ["Das", "Fr\u00e4u\u00b7lein", "fl\u00fcs\u00b7tert", "lei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbmein sch\u00f6ner Junker, auf Eurem Hut", "tokens": ["\u00bb", "mein", "sch\u00f6\u00b7ner", "Jun\u00b7ker", ",", "auf", "Eu\u00b7rem", "Hut"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Schwankt eine Neckenlilie,", "tokens": ["Schwankt", "ei\u00b7ne", "Ne\u00b7cken\u00b7li\u00b7lie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Die w\u00e4chst nur tief in Meeresgrund \u2013", "tokens": ["Die", "w\u00e4chst", "nur", "tief", "in", "Mee\u00b7res\u00b7grund", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "APPR", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr stammt nicht aus Adams Familie.", "tokens": ["Ihr", "stammt", "nicht", "aus", "A\u00b7dams", "Fa\u00b7mi\u00b7lie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NE", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Ihr seid der Wassermann, Ihr wollt", "tokens": ["Ihr", "seid", "der", "Was\u00b7ser\u00b7mann", ",", "Ihr", "wollt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verlocken des Dorfes Sch\u00f6nen.", "tokens": ["Ver\u00b7lo\u00b7cken", "des", "Dor\u00b7fes", "Sch\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich hab Euch erkannt, beim ersten Blick,", "tokens": ["Ich", "hab", "Euch", "er\u00b7kannt", ",", "beim", "ers\u00b7ten", "Blick", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "An Euren fischgr\u00e4tigen Z\u00e4hnen.\u00ab", "tokens": ["An", "Eu\u00b7ren", "fischgr\u00e4\u00b7ti\u00b7gen", "Z\u00e4h\u00b7nen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Sie schweben auf, sie schweben ab,", "tokens": ["Sie", "schwe\u00b7ben", "auf", ",", "sie", "schwe\u00b7ben", "ab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In seltsam fremder Weise,", "tokens": ["In", "selt\u00b7sam", "frem\u00b7der", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie lachen sich an, sie sch\u00fctteln das Haupt,", "tokens": ["Sie", "la\u00b7chen", "sich", "an", ",", "sie", "sch\u00fct\u00b7teln", "das", "Haupt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Junker fl\u00fcstert leise:", "tokens": ["Der", "Jun\u00b7ker", "fl\u00fcs\u00b7tert", "lei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbmein sch\u00f6nes Fr\u00e4ulein, sagt mir, warum", "tokens": ["\u00bb", "mein", "sch\u00f6\u00b7nes", "Fr\u00e4u\u00b7lein", ",", "sagt", "mir", ",", "wa\u00b7rum"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "$,", "VVFIN", "PPER", "$,", "PWAV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "So eiskalt Eure Hand ist?", "tokens": ["So", "eis\u00b7kalt", "Eu\u00b7re", "Hand", "ist", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sagt mir, warum so na\u00df der Saum", "tokens": ["Sagt", "mir", ",", "wa\u00b7rum", "so", "na\u00df", "der", "Saum"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ADV", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An Eurem wei\u00dfen Gewand ist?", "tokens": ["An", "Eu\u00b7rem", "wei\u00b7\u00dfen", "Ge\u00b7wand", "ist", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Ich hab Euch erkannt, beim ersten Blick,", "tokens": ["Ich", "hab", "Euch", "er\u00b7kannt", ",", "beim", "ers\u00b7ten", "Blick", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "An Eurem sp\u00f6ttischen Knickse \u2013", "tokens": ["An", "Eu\u00b7rem", "sp\u00f6t\u00b7ti\u00b7schen", "Knick\u00b7se", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Du bist kein irdisches Menschenkind,", "tokens": ["Du", "bist", "kein", "ir\u00b7di\u00b7sches", "Men\u00b7schen\u00b7kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Du bist mein M\u00fchmchen, die Nixe.\u00ab", "tokens": ["Du", "bist", "mein", "M\u00fchm\u00b7chen", ",", "die", "Ni\u00b7xe", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Die Geigen verstummen, der Tanz ist aus,", "tokens": ["Die", "Gei\u00b7gen", "ver\u00b7stum\u00b7men", ",", "der", "Tanz", "ist", "aus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VAFIN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Es trennen sich h\u00f6flich die beiden.", "tokens": ["Es", "tren\u00b7nen", "sich", "h\u00f6f\u00b7lich", "die", "bei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "ART", "PIAT", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sie kennen sich leider viel zu gut,", "tokens": ["Sie", "ken\u00b7nen", "sich", "lei\u00b7der", "viel", "zu", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Suchen sich jetzt zu vermeiden.", "tokens": ["Su\u00b7chen", "sich", "jetzt", "zu", "ver\u00b7mei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}