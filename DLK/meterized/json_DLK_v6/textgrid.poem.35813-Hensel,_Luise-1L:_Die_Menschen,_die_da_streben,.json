{"textgrid.poem.35813": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Menschen, die da streben,", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Menschen, die da streben,", "tokens": ["Die", "Men\u00b7schen", ",", "die", "da", "stre\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein junges Herz, nach dir,", "tokens": ["Mein", "jun\u00b7ges", "Herz", ",", "nach", "dir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie m\u00f6chten f\u00fcr dich ", "tokens": ["Sie", "m\u00f6ch\u00b7ten", "f\u00fcr", "dich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPER"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Dich gern erfreu'n allhier;", "tokens": ["Dich", "gern", "er\u00b7freu'n", "all\u00b7hier", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch Einer hat geworben,", "tokens": ["Doch", "Ei\u00b7ner", "hat", "ge\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der ew'ge G\u00fcter giebt,", "tokens": ["Der", "ew'\u00b7ge", "G\u00fc\u00b7ter", "giebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der ist f\u00fcr dich ", "tokens": ["Der", "ist", "f\u00fcr", "dich"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Sprich, wer hat mehr geliebt?", "tokens": ["Sprich", ",", "wer", "hat", "mehr", "ge\u00b7liebt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PWS", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und sollst du reiche Gaben", "tokens": ["Und", "sollst", "du", "rei\u00b7che", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In diesem Erdenland", "tokens": ["In", "die\u00b7sem", "Er\u00b7den\u00b7land"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und s\u00fc\u00dfe Freuden haben,", "tokens": ["Und", "s\u00fc\u00b7\u00dfe", "Freu\u00b7den", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So giebt sie Seine Hand.", "tokens": ["So", "giebt", "sie", "Sei\u00b7ne", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nimm nichts aus fremden H\u00e4nden,", "tokens": ["Nimm", "nichts", "aus", "frem\u00b7den", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Was ird'sche Liebe giebt;", "tokens": ["Was", "ird'\u00b7sche", "Lie\u00b7be", "giebt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es w\u00fcrd' Ihn von dir wenden,", "tokens": ["Es", "w\u00fcrd'", "Ihn", "von", "dir", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der eifers\u00fcchtig liebt. \u2013", "tokens": ["Der", "ei\u00b7fer\u00b7s\u00fcch\u00b7tig", "liebt", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJD", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wer hat ihn ganz empfunden,", "tokens": ["Wer", "hat", "ihn", "ganz", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den treuen Liebesmuth?", "tokens": ["Den", "treu\u00b7en", "Lie\u00b7bes\u00b7muth", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer hat gesehn die Wunden,", "tokens": ["Wer", "hat", "ge\u00b7sehn", "die", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das warme Herzensblut? \u2013", "tokens": ["Das", "war\u00b7me", "Her\u00b7zens\u00b7blut", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie oft hab' ich vermessen", "tokens": ["Wie", "oft", "hab'", "ich", "ver\u00b7mes\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dies treu'ste Herz betr\u00fcbt,", "tokens": ["Dies", "treu'\u00b7ste", "Herz", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wie undankbar vergessen,", "tokens": ["Wie", "un\u00b7dank\u00b7bar", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df es so tief mich liebt!", "tokens": ["Da\u00df", "es", "so", "tief", "mich", "liebt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Komm denn, o komm, Du Lieber!", "tokens": ["Komm", "denn", ",", "o", "komm", ",", "Du", "Lie\u00b7ber", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "FM", "VVFIN", "$,", "PPER", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hilf mir in Kampf und Schmerz", "tokens": ["Hilf", "mir", "in", "Kampf", "und", "Schmerz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und zieh mein Herz hin\u00fcber", "tokens": ["Und", "zieh", "mein", "Herz", "hin\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An Dein allliebend Herz!", "tokens": ["An", "Dein", "all\u00b7lie\u00b7bend", "Herz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O komm und gieb mir Liebe,", "tokens": ["O", "komm", "und", "gieb", "mir", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "VVIMP", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Du, der so gerne giebt,", "tokens": ["Du", ",", "der", "so", "ger\u00b7ne", "giebt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df ich Dich einzig liebe,", "tokens": ["Da\u00df", "ich", "Dich", "ein\u00b7zig", "lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dich, der mich ewig liebt.", "tokens": ["Dich", ",", "der", "mich", "e\u00b7wig", "liebt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}