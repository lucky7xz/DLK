{"dta.poem.19012": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Der Pilger Gesang  \n F\u00fcr die Hertzlose Ritter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr G\u00f6ttin zart/ jhr deren herrlichkeit", "tokens": ["Ihr", "G\u00f6t\u00b7tin", "zart", "/", "jhr", "de\u00b7ren", "herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "$(", "PPOSAT", "PRELAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die G\u00f6tter selbs nicht k\u00f6nden widerstreben/", "tokens": ["Die", "G\u00f6t\u00b7ter", "selbs", "nicht", "k\u00f6n\u00b7den", "wi\u00b7der\u00b7stre\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKNEG", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ach stehet ab von ewrer h\u00e4rtigkeit", "tokens": ["Ach", "ste\u00b7het", "ab", "von", "ew\u00b7rer", "h\u00e4r\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vnd lasset doch den Rittern hie das leben.", "tokens": ["Vnd", "las\u00b7set", "doch", "den", "Rit\u00b7tern", "hie", "das", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "ADV", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Jhr junges hertz lie\u00df sich durch der Lieb brunst/", "tokens": ["Ihr", "jun\u00b7ges", "hertz", "lie\u00df", "sich", "durch", "der", "Lieb", "brunst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die ewer aug erw\u00f6cket/ schnell verz\u00f6hren:", "tokens": ["Die", "e\u00b7wer", "aug", "er\u00b7w\u00f6\u00b7cket", "/", "schnell", "ver\u00b7z\u00f6h\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$(", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Jedoch nach lust durch eines anblicks gunst", "tokens": ["Je\u00b7doch", "nach", "lust", "durch", "ei\u00b7nes", "an\u00b7blicks", "gunst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "K\u00f6nt jhnen jhr ein newes hertz besch\u00f6ren.", "tokens": ["K\u00f6nt", "jh\u00b7nen", "jhr", "ein", "ne\u00b7wes", "hertz", "be\u00b7sch\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "So tr\u00f6stet sie mit einem s\u00fcssen blick", "tokens": ["So", "tr\u00f6s\u00b7tet", "sie", "mit", "ei\u00b7nem", "s\u00fcs\u00b7sen", "blick"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Damit sie nicht hie l\u00e4nger hertzlo\u00df ligen:", "tokens": ["Da\u00b7mit", "sie", "nicht", "hie", "l\u00e4n\u00b7ger", "hertz\u00b7lo\u00df", "li\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dan ewer ist das Lob/ vnd Jhr das Glick", "tokens": ["Dan", "e\u00b7wer", "ist", "das", "Lob", "/", "vnd", "Ihr", "das", "Glick"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "$(", "KON", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wan sie durch Euch behertzet/ muhtig sigen.", "tokens": ["Wan", "sie", "durch", "Euch", "be\u00b7hert\u00b7zet", "/", "muh\u00b7tig", "si\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPER", "VVFIN", "$(", "ADJD", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}