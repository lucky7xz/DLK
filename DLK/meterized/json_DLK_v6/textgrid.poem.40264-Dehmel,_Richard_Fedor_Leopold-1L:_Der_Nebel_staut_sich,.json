{"textgrid.poem.40264": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Nebel staut sich,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Nebel staut sich,", "tokens": ["Der", "Ne\u00b7bel", "staut", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "H\u00fctten dunkeln,", "tokens": ["H\u00fct\u00b7ten", "dun\u00b7keln", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Dorfgiebel fliegen \u00fcber Lichtern hin,", "tokens": ["Dorf\u00b7gie\u00b7bel", "flie\u00b7gen", "\u00fc\u00b7ber", "Lich\u00b7tern", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "noch bleicher wird die Nacht;", "tokens": ["noch", "blei\u00b7cher", "wird", "die", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "die jagende Wagenkette,", "tokens": ["die", "ja\u00b7gen\u00b7de", "Wa\u00b7gen\u00b7ket\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "schwenkend, strafft sich,", "tokens": ["schwen\u00b7kend", ",", "strafft", "sich", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "PRF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "die Maschine heult Warnung,", "tokens": ["die", "Ma\u00b7schi\u00b7ne", "heult", "War\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "und vorbei.", "tokens": ["und", "vor\u00b7bei", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Ein entlaubter Kirchhof,", "tokens": ["Ein", "ent\u00b7laub\u00b7ter", "Kirch\u00b7hof", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "und wieder kreisen", "tokens": ["und", "wie\u00b7der", "krei\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "um mein klirrendes Fenster", "tokens": ["um", "mein", "klir\u00b7ren\u00b7des", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "die toten Wiesen,", "tokens": ["die", "to\u00b7ten", "Wie\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "huschen B\u00fcsche,", "tokens": ["hu\u00b7schen", "B\u00fc\u00b7sche", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.14": {"text": "eilt der fahle Streifen Horizont", "tokens": ["eilt", "der", "fah\u00b7le", "Strei\u00b7fen", "Ho\u00b7ri\u00b7zont"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NE"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "auf den kriechenden W\u00e4ldern lang;", "tokens": ["auf", "den", "krie\u00b7chen\u00b7den", "W\u00e4l\u00b7dern", "lang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.16": {"text": "mich fr\u00f6stelt.", "tokens": ["mich", "fr\u00f6s\u00b7telt", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Drei Monate:", "tokens": ["Drei", "Mo\u00b7na\u00b7te", ":"], "token_info": ["word", "word", "punct"], "pos": ["CARD", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "da war die Mondnacht warm und anders.", "tokens": ["da", "war", "die", "Mond\u00b7nacht", "warm", "und", "an\u00b7ders", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "KON", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie auf Wolken", "tokens": ["Wie", "auf", "Wol\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "APPR", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "trug der kleine Kahn des stummen Fischers", "tokens": ["trug", "der", "klei\u00b7ne", "Kahn", "des", "stum\u00b7men", "Fi\u00b7schers"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "uns den Strom hinab,", "tokens": ["uns", "den", "Strom", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "selbst die Schatten gaben Licht;", "tokens": ["selbst", "die", "Schat\u00b7ten", "ga\u00b7ben", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "an meiner Seite sa\u00df ein Freund,", "tokens": ["an", "mei\u00b7ner", "Sei\u00b7te", "sa\u00df", "ein", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "und ich sagte ihm", "tokens": ["und", "ich", "sag\u00b7te", "ihm"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "alle meine S\u00fcnde \u2013 und ihr Gl\u00fcck.", "tokens": ["al\u00b7le", "mei\u00b7ne", "S\u00fcn\u00b7de", "\u2013", "und", "ihr", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$(", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Und \u00fcber ihrem Giebel,", "tokens": ["Und", "\u00fc\u00b7ber", "ih\u00b7rem", "Gie\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "unterm Baldachin der K\u00f6nigspappel,", "tokens": ["un\u00b7term", "Bal\u00b7da\u00b7ch\u00b7in", "der", "K\u00f6\u00b7nigs\u00b7pap\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.12": {"text": "als wir durch die Br\u00fccke bogen,", "tokens": ["als", "wir", "durch", "die", "Br\u00fc\u00b7cke", "bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "stand gro\u00df und strahlend,", "tokens": ["stand", "gro\u00df", "und", "strah\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "wie in einem Tabernakel,", "tokens": ["wie", "in", "ei\u00b7nem", "Ta\u00b7ber\u00b7na\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "der goldne Mond", "tokens": ["der", "gold\u00b7ne", "Mond"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "und neigte flimmernd auf das Moos des Daches", "tokens": ["und", "neig\u00b7te", "flim\u00b7mernd", "auf", "das", "Moos", "des", "Da\u00b7ches"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "sein gr\u00fcnes Haar.", "tokens": ["sein", "gr\u00fc\u00b7nes", "Haar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "Gestern aber, als ich Abschied nahm:", "tokens": ["Ge\u00b7stern", "a\u00b7ber", ",", "als", "ich", "Ab\u00b7schied", "nahm", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "\u00bbmein Fr\u00e4ulein, Gl\u00fcck?\u00ab \u2013 Und jener Freund", "tokens": ["\u00bb", "mein", "Fr\u00e4u\u00b7lein", ",", "Gl\u00fcck", "?", "\u00ab", "\u2013", "Und", "je\u00b7ner", "Freund"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "punct", "punct", "word", "word", "word"], "pos": ["$(", "PPOSAT", "NN", "$,", "NN", "$.", "$(", "$(", "KON", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "dachte wol schon damals:", "tokens": ["dach\u00b7te", "wol", "schon", "da\u00b7mals", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.21": {"text": "du Tropf und Schuft!", "tokens": ["du", "Tropf", "und", "Schuft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Mein Fenster schwitzt;", "tokens": ["Mein", "Fens\u00b7ter", "schwitzt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "das k\u00fchlt die Stirne;", "tokens": ["das", "k\u00fchlt", "die", "Stir\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "gleich und gleich gesellt sich gern.", "tokens": ["gleich", "und", "gleich", "ge\u00b7sellt", "sich", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "VVPP", "PRF", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wirbelnd rollt ein funkendurchwirkter Dampfkn\u00e4ul", "tokens": ["Wir\u00b7belnd", "rollt", "ein", "fun\u00b7ken\u00b7durch\u00b7wirk\u00b7ter", "Dampf\u00b7kn\u00e4ul"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "bleich ins bleiche Feld;", "tokens": ["bleich", "ins", "blei\u00b7che", "Feld", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "ein Dornbusch zerrei\u00dft ihn.", "tokens": ["ein", "Dorn\u00b7busch", "zer\u00b7rei\u00dft", "ihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Jetzt: dort starrt,", "tokens": ["Jetzt", ":", "dort", "starrt", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "wie durch ein Gitter ein Wahnsinnskopf,", "tokens": ["wie", "durch", "ein", "Git\u00b7ter", "ein", "Wahn\u00b7sinns\u00b7kopf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "der grelle Vollmond durch die kahlen Birken;", "tokens": ["der", "grel\u00b7le", "Voll\u00b7mond", "durch", "die", "kah\u00b7len", "Bir\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "die Zacken weichen,", "tokens": ["die", "Za\u00b7cken", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "mit seinen langen blassen F\u00fc\u00dfen", "tokens": ["mit", "sei\u00b7nen", "lan\u00b7gen", "blas\u00b7sen", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "l\u00e4uft er auf den blanken Schienen", "tokens": ["l\u00e4uft", "er", "auf", "den", "blan\u00b7ken", "Schie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "meinen rasenden Gedanken nach.", "tokens": ["mei\u00b7nen", "ra\u00b7sen\u00b7den", "Ge\u00b7dan\u00b7ken", "nach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}