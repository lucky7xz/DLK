{"dta.poem.23309": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Altfr\u00e4nkisch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Singt, da\u00df die B\u00e4chlein wieder flie\u00dfen,", "tokens": ["Singt", ",", "da\u00df", "die", "B\u00e4ch\u00b7lein", "wie\u00b7der", "flie\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Singt, da\u00df die Kr\u00e4uter wieder sprie\u00dfen,", "tokens": ["Singt", ",", "da\u00df", "die", "Kr\u00e4u\u00b7ter", "wie\u00b7der", "sprie\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Singt, da\u00df die Blumen sich entschlie\u00dfen", "tokens": ["Singt", ",", "da\u00df", "die", "Blu\u00b7men", "sich", "ent\u00b7schlie\u00b7\u00dfen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "PRF", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und des Lebens auch genie\u00dfen.", "tokens": ["Und", "des", "Le\u00b7bens", "auch", "ge\u00b7nie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Singt, da\u00df die V\u00f6gel immer singen,", "tokens": ["Singt", ",", "da\u00df", "die", "V\u00f6\u00b7gel", "im\u00b7mer", "sin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Singt, da\u00df die Heerdenglocken klingen,", "tokens": ["Singt", ",", "da\u00df", "die", "Heer\u00b7den\u00b7glo\u00b7cken", "klin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Singt, da\u00df die Schaf' und L\u00e4mmer springen,", "tokens": ["Singt", ",", "da\u00df", "die", "Schaf'", "und", "L\u00e4m\u00b7mer", "sprin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jung und Alt im Tanz sich schwingen.", "tokens": ["Jung", "und", "Alt", "im", "Tanz", "sich", "schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "APPRART", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Singt, da\u00df die L\u00fcfte wehn und weben,", "tokens": ["Singt", ",", "da\u00df", "die", "L\u00fcf\u00b7te", "wehn", "und", "we\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Singt, da\u00df erbl\u00fchn die B\u00e4um' und Reben,", "tokens": ["Singt", ",", "da\u00df", "er\u00b7bl\u00fchn", "die", "B\u00e4um'", "und", "Re\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Singt, da\u00df die Schmetterlinge schweben,", "tokens": ["Singt", ",", "da\u00df", "die", "Schmet\u00b7ter\u00b7lin\u00b7ge", "schwe\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df auch sie in Freuden leben.", "tokens": ["Da\u00df", "auch", "sie", "in", "Freu\u00b7den", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Singt, da\u00df die V\u00f6gel Nester bauen,", "tokens": ["Singt", ",", "da\u00df", "die", "V\u00f6\u00b7gel", "Nes\u00b7ter", "bau\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Singt, da\u00df die M\u00e4dchen, da\u00df die Frauen", "tokens": ["Singt", ",", "da\u00df", "die", "M\u00e4d\u00b7chen", ",", "da\u00df", "die", "Frau\u00b7en"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wieder wie Bl\u00fcmlein auf den Auen", "tokens": ["Wie\u00b7der", "wie", "Bl\u00fcm\u00b7lein", "auf", "den", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Freundlich aus den Fenstern schauen.", "tokens": ["Freund\u00b7lich", "aus", "den", "Fens\u00b7tern", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Singet des Fr\u00fchlings neue Wunder!", "tokens": ["Sin\u00b7get", "des", "Fr\u00fch\u00b7lings", "neu\u00b7e", "Wun\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Singet den Freud- und Liebeszunder!", "tokens": ["Sin\u00b7get", "den", "Freu\u00b7d", "und", "Lie\u00b7bes\u00b7zun\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "TRUNC", "KON", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Singet \u2014 und euer alter Plunder,", "tokens": ["Sin\u00b7get", "und", "eu\u00b7er", "al\u00b7ter", "Plun\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Sagt doch, pa\u00dft er noch jetzunder?", "tokens": ["Sagt", "doch", ",", "pa\u00dft", "er", "noch", "je\u00b7tzun\u00b7der", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}