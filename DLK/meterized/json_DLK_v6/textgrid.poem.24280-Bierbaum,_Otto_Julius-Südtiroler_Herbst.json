{"textgrid.poem.24280": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "S\u00fcdtiroler Herbst", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gelbleuchtend steht (wie Kapuzinerkresse)", "tokens": ["Gelb\u00b7leuch\u00b7tend", "steht", "(", "wie", "Ka\u00b7pu\u00b7zi\u00b7ner\u00b7kres\u00b7se", ")"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "$(", "KOKOM", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Latemar. Ein buntes Panterfell", "tokens": ["Der", "La\u00b7te\u00b7mar", ".", "Ein", "bun\u00b7tes", "Pan\u00b7ter\u00b7fell"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Liegt rot-gelb-braun der Mendel um die Flanken.", "tokens": ["Liegt", "rot\u00b7gelb\u00b7braun", "der", "Men\u00b7del", "um", "die", "Flan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Rebenbogen sind von Trauben leer.", "tokens": ["Die", "Re\u00b7ben\u00b7bo\u00b7gen", "sind", "von", "Trau\u00b7ben", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Aus Riesenbottichen trieft rote Maische,", "tokens": ["Aus", "Rie\u00b7sen\u00b7bot\u00b7ti\u00b7chen", "trieft", "ro\u00b7te", "Mai\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Von feisten Rindern langsam heimgef\u00fchrt", "tokens": ["Von", "feis\u00b7ten", "Rin\u00b7dern", "lang\u00b7sam", "heim\u00b7ge\u00b7f\u00fchrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Zum k\u00fchlen Keller auf staubwei\u00dfen Stra\u00dfen,", "tokens": ["Zum", "k\u00fch\u00b7len", "Kel\u00b7ler", "auf", "staub\u00b7wei\u00b7\u00dfen", "Stra\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Vorbei an Kruzifixen, wunderlich geschm\u00fcckt:", "tokens": ["Vor\u00b7bei", "an", "Kru\u00b7zi\u00b7fi\u00b7xen", ",", "wun\u00b7der\u00b7lich", "ge\u00b7schm\u00fcckt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dort wo die N\u00e4gel durch die Heilandsh\u00e4nde", "tokens": ["Dort", "wo", "die", "N\u00e4\u00b7gel", "durch", "die", "Hei\u00b7lands\u00b7h\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Kalt in das schwarze Marterholz sich bohren,", "tokens": ["Kalt", "in", "das", "schwar\u00b7ze", "Mar\u00b7ter\u00b7holz", "sich", "boh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "H\u00e4ngt, rechts und links dem vorgesenkten Haupte,", "tokens": ["H\u00e4ngt", ",", "rechts", "und", "links", "dem", "vor\u00b7ge\u00b7senk\u00b7ten", "Haup\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "KON", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Prall, Beer an Beere innig so gedr\u00e4ngt,", "tokens": ["Prall", ",", "Beer", "an", "Bee\u00b7re", "in\u00b7nig", "so", "ge\u00b7dr\u00e4ngt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "APPR", "NN", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Als sei es ", "tokens": ["Als", "sei", "es"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Je eine schwere Traube. Durch die Krone", "tokens": ["Je", "ei\u00b7ne", "schwe\u00b7re", "Trau\u00b7be", ".", "Durch", "die", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Von Dornen windet sich, Korallen gleich,", "tokens": ["Von", "Dor\u00b7nen", "win\u00b7det", "sich", ",", "Ko\u00b7ral\u00b7len", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PRF", "$,", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Aus Vogelbeeren eine rote Kette,", "tokens": ["Aus", "Vo\u00b7gel\u00b7bee\u00b7ren", "ei\u00b7ne", "ro\u00b7te", "Ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Und dunkelgelbe Kolben T\u00fcrkenkorns", "tokens": ["Und", "dun\u00b7kel\u00b7gel\u00b7be", "Kol\u00b7ben", "T\u00fcr\u00b7ken\u00b7korns"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Umrahmen samengolden diesen Gott", "tokens": ["Um\u00b7rah\u00b7men", "sa\u00b7men\u00b7gol\u00b7den", "die\u00b7sen", "Gott"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Des liebehingegebenen Schmerzengl\u00fccks.", "tokens": ["Des", "lie\u00b7be\u00b7hin\u00b7ge\u00b7ge\u00b7be\u00b7nen", "Schmer\u00b7zen\u00b7gl\u00fccks", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Es ist, als w\u00e4r es ein verstellter Pan.", "tokens": ["Es", "ist", ",", "als", "w\u00e4r", "es", "ein", "ver\u00b7stell\u00b7ter", "Pan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}