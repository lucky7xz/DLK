{"textgrid.poem.41346": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Fabel", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist Euphrast, der stets gefiel", "tokens": ["Es", "ist", "Eu\u00b7phrast", ",", "der", "stets", "ge\u00b7fiel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NE", "$,", "PRELS", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In allem, was wir von ihm lesen,", "tokens": ["In", "al\u00b7lem", ",", "was", "wir", "von", "ihm", "le\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bescheiden-sinnreich, wie Virgil,", "tokens": ["Be\u00b7schei\u00b7den\u00b7sinn\u00b7reich", ",", "wie", "Vir\u00b7gil", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erfindsam, wie Homer gewesen.", "tokens": ["Er\u00b7find\u00b7sam", ",", "wie", "Ho\u00b7mer", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "NE", "VAPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Er schrieb nicht bis ins Stufenjahr,", "tokens": ["Er", "schrieb", "nicht", "bis", "ins", "Stu\u00b7fen\u00b7jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht viel, nichts auf Befehl, nichts eilig.", "tokens": ["Nicht", "viel", ",", "nichts", "auf", "Be\u00b7fehl", ",", "nichts", "ei\u00b7lig", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "PIS", "APPR", "NN", "$,", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wie ihm die Wahrheit heilig war,", "tokens": ["Wie", "ihm", "die", "Wahr\u00b7heit", "hei\u00b7lig", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So war ihm auch die Sprache heilig.", "tokens": ["So", "war", "ihm", "auch", "die", "Spra\u00b7che", "hei\u00b7lig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Sich selbst zum Lobe redt' er nie,", "tokens": ["Sich", "selbst", "zum", "Lo\u00b7be", "redt'", "er", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPRART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Doch litt er andrer Stolz und Tr\u00e4ume,", "tokens": ["Doch", "litt", "er", "an\u00b7drer", "Stolz", "und", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Sprach selten von der Poesie,", "tokens": ["Sprach", "sel\u00b7ten", "von", "der", "Poe\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-++--", "measure": "unknown.measure.tri"}, "line.12": {"text": "Noch gegen oder f\u00fcr die Reime.", "tokens": ["Noch", "ge\u00b7gen", "o\u00b7der", "f\u00fcr", "die", "Rei\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Er war voll weiser Sittsamkeit,", "tokens": ["Er", "war", "voll", "wei\u00b7ser", "Sitt\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Drum ward er keiner Secte G\u00f6tze,", "tokens": ["Drum", "ward", "er", "kei\u00b7ner", "Sec\u00b7te", "G\u00f6t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Und hinterlie\u00df der Folgezeit", "tokens": ["Und", "hin\u00b7ter\u00b7lie\u00df", "der", "Fol\u00b7ge\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Zwar Muster, aber nicht Gesetze.", "tokens": ["Zwar", "Mus\u00b7ter", ",", "a\u00b7ber", "nicht", "Ge\u00b7set\u00b7ze", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Nur Wasser trank er, und nicht Wein.", "tokens": ["Nur", "Was\u00b7ser", "trank", "er", ",", "und", "nicht", "Wein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PPER", "$,", "KON", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Von Sch\u00f6nen liebt' er nur die alten:", "tokens": ["Von", "Sch\u00f6\u00b7nen", "liebt'", "er", "nur", "die", "al\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "ART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Blos ihrer Seelen Freund zu sein,", "tokens": ["Blos", "ih\u00b7rer", "See\u00b7len", "Freund", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und sich des Busens zu enthalten.", "tokens": ["Und", "sich", "des", "Bu\u00b7sens", "zu", "ent\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Er starb, und lie\u00df, eh' er verschied,", "tokens": ["Er", "starb", ",", "und", "lie\u00df", ",", "eh'", "er", "ver\u00b7schied", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Ein Buch, das er gemacht, verbrennen,", "tokens": ["Ein", "Buch", ",", "das", "er", "ge\u00b7macht", ",", "ver\u00b7bren\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "So sehr auch sein Verleger rieth,", "tokens": ["So", "sehr", "auch", "sein", "Ver\u00b7le\u00b7ger", "rieth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Das Werk der Welt und ihm zu g\u00f6nnen.", "tokens": ["Das", "Werk", "der", "Welt", "und", "ihm", "zu", "g\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}