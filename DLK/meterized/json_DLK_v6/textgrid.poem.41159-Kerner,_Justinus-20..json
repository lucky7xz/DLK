{"textgrid.poem.41159": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst waren zwei Kameraden,", "tokens": ["Einst", "wa\u00b7ren", "zwei", "Ka\u00b7me\u00b7ra\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "CARD", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die schwuren einen Eid,", "tokens": ["Die", "schwu\u00b7ren", "ei\u00b7nen", "Eid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df jeder auf sich wollt' laden", "tokens": ["Da\u00df", "je\u00b7der", "auf", "sich", "wollt'", "la\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "PRF", "VMFIN", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Des andern Freud' und Leid.", "tokens": ["Des", "an\u00b7dern", "Freud'", "und", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es war ein Krieg in Sachsen,", "tokens": ["Es", "war", "ein", "Krieg", "in", "Sach\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hin zogen sie voll Mut,", "tokens": ["Hin", "zo\u00b7gen", "sie", "voll", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sangen. \u00bbJuheh! verwachsen", "tokens": ["San\u00b7gen", ".", "\u00bb", "Ju\u00b7heh", "!", "ver\u00b7wach\u00b7sen"], "token_info": ["word", "punct", "punct", "word", "punct", "word"], "pos": ["VVFIN", "$.", "$(", "NE", "$.", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Sind wir mit Leib und Blut!\u00ab", "tokens": ["Sind", "wir", "mit", "Leib", "und", "Blut", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Raketen und Bomben fliegen,", "tokens": ["Ra\u00b7ke\u00b7ten", "und", "Bom\u00b7ben", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Zerrei\u00dfen des einen Bein,", "tokens": ["Zer\u00b7rei\u00b7\u00dfen", "des", "ei\u00b7nen", "Bein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Der andre lie\u00df ihn liegen,", "tokens": ["Der", "and\u00b7re", "lie\u00df", "ihn", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Floh \u00fcber Stock und Stein.", "tokens": ["Floh", "\u00fc\u00b7ber", "Stock", "und", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Doch war's ihm immer b\u00e4nger,", "tokens": ["Doch", "wa\u00b7r's", "ihm", "im\u00b7mer", "b\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.14": {"text": "\u00bbeid!\u00ab rief er, \u00bbb\u00f6ser Traum!\u00ab", "tokens": ["\u00bb", "eid", "!", "\u00ab", "rief", "er", ",", "\u00bb", "b\u00f6\u00b7ser", "Traum", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PTKVZ", "$.", "$(", "VVFIN", "PPER", "$,", "$(", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Er konnt' nicht leben l\u00e4nger,", "tokens": ["Er", "konnt'", "nicht", "le\u00b7ben", "l\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Hing sich an einen Baum.", "tokens": ["Hing", "sich", "an", "ei\u00b7nen", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.17": {"text": "Das war, als an der Wunde,", "tokens": ["Das", "war", ",", "als", "an", "der", "Wun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "KOUS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Und seit derselben Stunde", "tokens": ["Und", "seit", "der\u00b7sel\u00b7ben", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Der andre doppelt geht.", "tokens": ["Der", "and\u00b7re", "dop\u00b7pelt", "geht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Er geht als wie verwachsen", "tokens": ["Er", "geht", "als", "wie", "ver\u00b7wach\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "KOKOM", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Mit des Kameraden Leib,", "tokens": ["Mit", "des", "Ka\u00b7me\u00b7ra\u00b7den", "Leib", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Auf dem Schlachtfeld nachts in Sachsen", "tokens": ["Auf", "dem", "Schlacht\u00b7feld", "nachts", "in", "Sach\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "APPR", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Er so umher sich treibt!", "tokens": ["Er", "so", "um\u00b7her", "sich", "treibt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKVZ", "PRF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "Er stieg heut aus dem Fasse", "tokens": ["Er", "stieg", "heut", "aus", "dem", "Fas\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Der Tinte reuig auf,", "tokens": ["Der", "Tin\u00b7te", "reu\u00b7ig", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.26": {"text": "Ich hoff', da\u00df Gott erlasse", "tokens": ["Ich", "hoff'", ",", "da\u00df", "Gott", "er\u00b7las\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Ihm bald den bangen Lauf.", "tokens": ["Ihm", "bald", "den", "ban\u00b7gen", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}