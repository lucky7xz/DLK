{"textgrid.poem.33699": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "[du bist mir gut]", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist mir gut \u2013", "tokens": ["Du", "bist", "mir", "gut", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Doch tiefer an dein Herz geht eine Frage:", "tokens": ["Doch", "tie\u00b7fer", "an", "dein", "Herz", "geht", "ei\u00b7ne", "Fra\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durchleuchtet deine Liebe lautre Glut", "tokens": ["Durch\u00b7leuch\u00b7tet", "dei\u00b7ne", "Lie\u00b7be", "laut\u00b7re", "Glut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Todesmut,", "tokens": ["Und", "To\u00b7des\u00b7mut", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und wirfst du freudig alles in die Wage,", "tokens": ["Und", "wirfst", "du", "freu\u00b7dig", "al\u00b7les", "in", "die", "Wa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Ehre, Frieden, Gl\u00fcck und Gut,", "tokens": ["Die", "Eh\u00b7re", ",", "Frie\u00b7den", ",", "Gl\u00fcck", "und", "Gut", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und nicht ein Blick, kein Ach sagt \u00bbich entsage\u00ab?", "tokens": ["Und", "nicht", "ein", "Blick", ",", "kein", "Ach", "sagt", "\u00bb", "ich", "ent\u00b7sa\u00b7ge", "\u00ab", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "$,", "PIAT", "NN", "VVFIN", "$(", "PPER", "VVFIN", "$(", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du schweigst? Kein Laut", "tokens": ["Du", "schweigst", "?", "Kein", "Laut"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Begegnet mir aus deinem Herzensschlage?", "tokens": ["Be\u00b7geg\u00b7net", "mir", "aus", "dei\u00b7nem", "Her\u00b7zens\u00b7schla\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was dich zur\u00fcckh\u00e4lt, hast du's nie vertraut? \u2013", "tokens": ["Was", "dich", "zu\u00b7r\u00fcck\u00b7h\u00e4lt", ",", "hast", "du's", "nie", "ver\u00b7traut", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VAFIN", "PIS", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "O Marmorbraut,", "tokens": ["O", "Mar\u00b7mor\u00b7braut", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Unselig Traumbild du verlorner Tage!", "tokens": ["Un\u00b7se\u00b7lig", "Traum\u00b7bild", "du", "ver\u00b7lor\u00b7ner", "Ta\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "PPER", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Weh, da\u00df ich dich geschaut", "tokens": ["Weh", ",", "da\u00df", "ich", "dich", "ge\u00b7schaut"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "PRF", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und dich gek\u00fc\u00dft, du sch\u00f6ne, stumme Klage!", "tokens": ["Und", "dich", "ge\u00b7k\u00fc\u00dft", ",", "du", "sch\u00f6\u00b7ne", ",", "stum\u00b7me", "Kla\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "$,", "PPER", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}