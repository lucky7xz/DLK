{"textgrid.poem.65862": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "136", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Straft deine Seele dich, als k\u00e4m' ich dir zu nah,", "tokens": ["Straft", "dei\u00b7ne", "See\u00b7le", "dich", ",", "als", "k\u00e4m'", "ich", "dir", "zu", "nah", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PPER", "$,", "KOUS", "VVFIN", "PPER", "PPER", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann schw\u00f6r': ich war dein Will', auf deine arme Seele;", "tokens": ["Dann", "schw\u00f6r'", ":", "ich", "war", "dein", "Will'", ",", "auf", "dei\u00b7ne", "ar\u00b7me", "See\u00b7le", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und deine Seele wei\u00df, Will' ist willkommen da.", "tokens": ["Und", "dei\u00b7ne", "See\u00b7le", "wei\u00df", ",", "Will'", "ist", "will\u00b7kom\u00b7men", "da", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,", "NE", "VAFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wollt' ich, da\u00df mein Leid sich deiner Lieb' empf\u00f6hle.", "tokens": ["So", "wollt'", "ich", ",", "da\u00df", "mein", "Leid", "sich", "dei\u00b7ner", "Lieb'", "emp\u00b7f\u00f6h\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "PRF", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Will Will' in Liebe deinen Schatz bereichern", "tokens": ["Will", "Will'", "in", "Lie\u00b7be", "dei\u00b7nen", "Schatz", "be\u00b7rei\u00b7chern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "NE", "APPR", "NN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit Willens\u00fcberflu\u00df und seinem Willen mit,", "tokens": ["Mit", "Wil\u00b7len\u00b7s\u00fc\u00b7berf\u00b7lu\u00df", "und", "sei\u00b7nem", "Wil\u00b7len", "mit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ist es ein K\u00f6rnlein nur zu vollgeh\u00e4uften Speichern,", "tokens": ["Ist", "es", "ein", "K\u00f6rn\u00b7lein", "nur", "zu", "voll\u00b7ge\u00b7h\u00e4uf\u00b7ten", "Spei\u00b7chern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das nicht in Zahl und nicht in Rechnung tritt.", "tokens": ["Das", "nicht", "in", "Zahl", "und", "nicht", "in", "Rech\u00b7nung", "tritt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "APPR", "NN", "KON", "PTKNEG", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "So la\u00df mich denn auch unbeziffert leben,", "tokens": ["So", "la\u00df", "mich", "denn", "auch", "un\u00b7be\u00b7zif\u00b7fert", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Obwohl als Mitgenossen deiner Welt:", "tokens": ["Ob\u00b7wohl", "als", "Mit\u00b7ge\u00b7nos\u00b7sen", "dei\u00b7ner", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Sei ich dir nichts, wenn dies mein Nichts nur eben", "tokens": ["Sei", "ich", "dir", "nichts", ",", "wenn", "dies", "mein", "Nichts", "nur", "e\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPER", "PIS", "$,", "KOUS", "PDS", "PPOSAT", "NN", "ADV", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Als angenehmes Etwas dir gef\u00e4llt.", "tokens": ["Als", "an\u00b7ge\u00b7neh\u00b7mes", "Et\u00b7was", "dir", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Nimm meinen Namen nur zu ew'gem Liebesziel,", "tokens": ["Nimm", "mei\u00b7nen", "Na\u00b7men", "nur", "zu", "ew'\u00b7gem", "Lie\u00b7bes\u00b7ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So hast du mich lieb, denn mein Nam' ist ", "tokens": ["So", "hast", "du", "mich", "lieb", ",", "denn", "mein", "Nam'", "ist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ADJD", "$,", "KON", "PPOSAT", "NN", "VAFIN"], "meter": "-+--+-++-", "measure": "iambic.tetra.relaxed"}}}}}