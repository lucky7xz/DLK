{"textgrid.poem.52935": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du bist es, Schwan der Magyaren,", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist es, Schwan der Magyaren,", "tokens": ["Du", "bist", "es", ",", "Schwan", "der", "Ma\u00b7gy\u00b7a\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du mit der liederreichen Kehle?", "tokens": ["Du", "mit", "der", "lie\u00b7der\u00b7rei\u00b7chen", "Keh\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mann, schwarz von Augen, schwarz von Haaren,", "tokens": ["Mann", ",", "schwarz", "von", "Au\u00b7gen", ",", "schwarz", "von", "Haa\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "APPR", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schwarz in der schmerzenreichen Seele?", "tokens": ["Schwarz", "in", "der", "schmer\u00b7zen\u00b7rei\u00b7chen", "See\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$."], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ja, das sind die Mephisto-Falten,", "tokens": ["Ja", ",", "das", "sind", "die", "Me\u00b7phi\u00b7sto\u00b7Fal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Die auf der Stirn zusamenlaufen,", "tokens": ["Die", "auf", "der", "Stirn", "zu\u00b7sa\u00b7men\u00b7lau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Aus diesen Blicken flammt verhalten", "tokens": ["Aus", "die\u00b7sen", "Bli\u00b7cken", "flammt", "ver\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Savonarolas Scheiterhaufen!", "tokens": ["Sa\u00b7vo\u00b7na\u00b7ro\u00b7las", "Schei\u00b7ter\u00b7hau\u00b7fen", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Und darum bist Du fortgeschwommen", "tokens": ["Und", "da\u00b7rum", "bist", "Du", "fort\u00b7ge\u00b7schwom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch der Atlantis blaue Wogen,", "tokens": ["Durch", "der", "At\u00b7lan\u00b7tis", "blau\u00b7e", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Darum verwundet heimgekommen,", "tokens": ["Da\u00b7rum", "ver\u00b7wun\u00b7det", "heim\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wohin Dein Herz Dich hei\u00df gezogen,", "tokens": ["Wo\u00b7hin", "Dein", "Herz", "Dich", "hei\u00df", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df hier im stillen Alpentale", "tokens": ["Da\u00df", "hier", "im", "stil\u00b7len", "Al\u00b7pen\u00b7ta\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dein volles Leben sich verblute", "tokens": ["Dein", "vol\u00b7les", "Le\u00b7ben", "sich", "ver\u00b7blu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und \u2013 kaum gek\u00fc\u00dft vom Sonnenstrahle,", "tokens": ["Und", "\u2013", "kaum", "ge\u00b7k\u00fc\u00dft", "vom", "Son\u00b7nen\u00b7strah\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "ADV", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Hinab ins Meer des Todes flute?", "tokens": ["Hin\u00b7ab", "ins", "Meer", "des", "To\u00b7des", "flu\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was willst Du in den engen Bergen,", "tokens": ["Was", "willst", "Du", "in", "den", "en\u00b7gen", "Ber\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf diesen Seen voll Grabesfrieden,", "tokens": ["Auf", "die\u00b7sen", "Seen", "voll", "Gra\u00b7bes\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gen\u00fcber jenen Menschen-Zwergen,", "tokens": ["Ge\u00b7n\u00fc\u00b7ber", "je\u00b7nen", "Men\u00b7schen\u00b7Zwer\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von Deines Gleichen abgeschieden?", "tokens": ["Von", "Dei\u00b7nes", "Glei\u00b7chen", "ab\u00b7ge\u00b7schie\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du selbst ein Gletscher, ragest m\u00e4chtig", "tokens": ["Du", "selbst", "ein", "Glet\u00b7scher", ",", "ra\u00b7gest", "m\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "ART", "NN", "$,", "VVFIN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch kalt und einsam in die H\u00f6h'", "tokens": ["Doch", "kalt", "und", "ein\u00b7sam", "in", "die", "H\u00f6h'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und spiegelst Dich mild und bed\u00e4chtig", "tokens": ["Und", "spie\u00b7gelst", "Dich", "mild", "und", "be\u00b7d\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "In Deiner Lieder gr\u00fcnem See.", "tokens": ["In", "Dei\u00b7ner", "Lie\u00b7der", "gr\u00fc\u00b7nem", "See", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Komm, flieh ein Land, wo sich die Dichter", "tokens": ["Komm", ",", "flieh", "ein", "Land", ",", "wo", "sich", "die", "Dich\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "ART", "NN", "$,", "PWAV", "PRF", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verleugnen m\u00fcssen und verstecken,", "tokens": ["Ver\u00b7leug\u00b7nen", "m\u00fcs\u00b7sen", "und", "ver\u00b7ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo M\u00f6nchsgez\u00fccht und Hofgelichter", "tokens": ["Wo", "M\u00f6n\u00b7chs\u00b7ge\u00b7z\u00fccht", "und", "Hof\u00b7ge\u00b7lich\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den Staub an Kreuz und Szepter lecken,", "tokens": ["Den", "Staub", "an", "Kreuz", "und", "Szep\u00b7ter", "le\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wo nur die sinnliche Begierde", "tokens": ["Wo", "nur", "die", "sinn\u00b7li\u00b7che", "Be\u00b7gier\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Nach neuen Opfern t\u00e4glich schmachtet,", "tokens": ["Nach", "neu\u00b7en", "Op\u00b7fern", "t\u00e4g\u00b7lich", "schmach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und was sonst gilt als Volkes Zierde", "tokens": ["Und", "was", "sonst", "gilt", "als", "Vol\u00b7kes", "Zier\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ADV", "VVFIN", "KOUS", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zertreten wird und roh verachtet.", "tokens": ["Zer\u00b7tre\u00b7ten", "wird", "und", "roh", "ver\u00b7ach\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Seele gib, die zweifel-kranke,", "tokens": ["Die", "See\u00b7le", "gib", ",", "die", "zwei\u00b7fel\u00b7kran\u00b7ke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVIMP", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur preis den Str\u00f6mungen des Lebens!", "tokens": ["Nur", "preis", "den", "Str\u00f6\u00b7mun\u00b7gen", "des", "Le\u00b7bens", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Erhellen wird sich Dein Gedanke", "tokens": ["Er\u00b7hel\u00b7len", "wird", "sich", "Dein", "Ge\u00b7dan\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Spiegel des verwandten Strebens,", "tokens": ["Im", "Spie\u00b7gel", "des", "ver\u00b7wand\u00b7ten", "Stre\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du wirst nicht s\u00e4en blo\u00df, auch ernten,", "tokens": ["Du", "wirst", "nicht", "s\u00e4\u00b7en", "blo\u00df", ",", "auch", "ern\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "VVFIN", "ADV", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dein Ruhm tritt f\u00fcr die Heimat ein,", "tokens": ["Dein", "Ruhm", "tritt", "f\u00fcr", "die", "Hei\u00b7mat", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und die Dir jetzo die Entfernten,", "tokens": ["Und", "die", "Dir", "jet\u00b7zo", "die", "Ent\u00b7fern\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Sie werden Deine N\u00e4chsten sein!", "tokens": ["Sie", "wer\u00b7den", "Dei\u00b7ne", "N\u00e4chs\u00b7ten", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Sch\u00fcttle den Staub von Deinen Schwingen", "tokens": ["Sch\u00fctt\u00b7le", "den", "Staub", "von", "Dei\u00b7nen", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und eil dem Bann Dich zu entraffen,", "tokens": ["Und", "eil", "dem", "Bann", "Dich", "zu", "en\u00b7traf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du sollst uns noch was anders singen", "tokens": ["Du", "sollst", "uns", "noch", "was", "an\u00b7ders", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PWS", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als immer Faust und Papst und Pfaffen!", "tokens": ["Als", "im\u00b7mer", "Faust", "und", "Papst", "und", "Pfaf\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Steig mit den Lerchen, mit den Aaren,", "tokens": ["Steig", "mit", "den", "Ler\u00b7chen", ",", "mit", "den", "Aa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Was schert der Kauz Dich und die Eule?", "tokens": ["Was", "schert", "der", "Kauz", "Dich", "und", "die", "Eu\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PPER", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Stirb nicht, Du Schwan der Magyaren,", "tokens": ["Stirb", "nicht", ",", "Du", "Schwan", "der", "Ma\u00b7gy\u00b7a\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "$,", "PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Als Heiliger auf einer S\u00e4ule!", "tokens": ["Als", "Hei\u00b7li\u00b7ger", "auf", "ei\u00b7ner", "S\u00e4u\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}