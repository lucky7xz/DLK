{"dta.poem.11858": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "35.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Himmel, wenn er lang nicht hat geregnet mehr,", "tokens": ["Der", "Him\u00b7mel", ",", "wenn", "er", "lang", "nicht", "hat", "ge\u00b7reg\u00b7net", "mehr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ADJD", "PTKNEG", "VAFIN", "VVPP", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bis wieder ordentlich er dazu kommt, h\u00e4lts schwer.", "tokens": ["Bis", "wie\u00b7der", "or\u00b7dent\u00b7lich", "er", "da\u00b7zu", "kommt", ",", "h\u00e4lts", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJD", "PPER", "PAV", "VVFIN", "$,", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es scheint ihm rechte M\u00fch'n zu kosten, bis den Wolken", "tokens": ["Es", "scheint", "ihm", "rech\u00b7te", "M\u00fch'n", "zu", "kos\u00b7ten", ",", "bis", "den", "Wol\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "PTKZU", "VVINF", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er gleich versiegten K\u00fch'n ein Tr\u00f6pflein erst entmolken.", "tokens": ["Er", "gleich", "ver\u00b7sieg\u00b7ten", "K\u00fch'n", "ein", "Tr\u00f6pf\u00b7lein", "erst", "ent\u00b7mol\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dem Tr\u00f6pfeln folgt die Trauf', und ist es erst im Zug,", "tokens": ["Dem", "Tr\u00f6p\u00b7feln", "folgt", "die", "Trauf'", ",", "und", "ist", "es", "erst", "im", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "KON", "VAFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gehts immer leichter ab, und mehr oft als genug.", "tokens": ["Gehts", "im\u00b7mer", "leich\u00b7ter", "ab", ",", "und", "mehr", "oft", "als", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "PTKVZ", "$,", "KON", "ADV", "ADV", "KOKOM", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So wie ein st\u00e4t'scher Gaul bocksteif ist eine Frist,", "tokens": ["So", "wie", "ein", "st\u00e4t'\u00b7scher", "Gaul", "bocks\u00b7teif", "ist", "ei\u00b7ne", "Frist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NN", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und erst gelenk wird, wann er warm geworden ist.", "tokens": ["Und", "erst", "ge\u00b7lenk", "wird", ",", "wann", "er", "warm", "ge\u00b7wor\u00b7den", "ist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VAFIN", "$,", "PWAV", "PPER", "ADJD", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und wie ein Dichter, der zulang an sich gehalten,", "tokens": ["Und", "wie", "ein", "Dich\u00b7ter", ",", "der", "zu\u00b7lang", "an", "sich", "ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "PRELS", "ADV", "APPR", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Anstrengung braucht, um neu die Fl\u00fcgel zu entfalten.", "tokens": ["An\u00b7stren\u00b7gung", "braucht", ",", "um", "neu", "die", "Fl\u00fc\u00b7gel", "zu", "ent\u00b7fal\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KOUI", "ADJD", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Darum in jedem Werk, bist du einmal im Zug,", "tokens": ["Da\u00b7rum", "in", "je\u00b7dem", "Werk", ",", "bist", "du", "ein\u00b7mal", "im", "Zug", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "PIAT", "NN", "$,", "VAFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Treib zu und schaffe fort, doch mehr nicht als genug", "tokens": ["Treib", "zu", "und", "schaf\u00b7fe", "fort", ",", "doch", "mehr", "nicht", "als", "ge\u00b7nug"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "KON", "VVFIN", "PTKVZ", "$,", "ADV", "ADV", "PTKNEG", "KOKOM", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}