{"textgrid.poem.36126": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Noch bl\u00fcht mein jugendlich Leben", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch bl\u00fcht mein jugendlich Leben", "tokens": ["Noch", "bl\u00fcht", "mein", "ju\u00b7gend\u00b7lich", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJD", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie Fr\u00fchlings Morgenroth heiter. Mir tagt die Sonn'", "tokens": ["Wie", "Fr\u00fch\u00b7lings", "Mor\u00b7gen\u00b7roth", "hei\u00b7ter", ".", "Mir", "tagt", "die", "Sonn'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "NN", "ADJD", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Im j\u00fcngsten des Maienfr\u00fch.", "tokens": ["Im", "j\u00fcng\u00b7sten", "des", "Mai\u00b7en\u00b7fr\u00fch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Auch brennt mir m\u00e4chtig im Busen", "tokens": ["Auch", "brennt", "mir", "m\u00e4ch\u00b7tig", "im", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der Gedanke, rei\u00dfend, wie Donner im Schlachtgew\u00fchl:", "tokens": ["Der", "Ge\u00b7dan\u00b7ke", ",", "rei\u00b7\u00dfend", ",", "wie", "Don\u00b7ner", "im", "Schlacht\u00b7ge\u00b7w\u00fchl", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "$,", "PWAV", "NN", "APPRART", "NN", "$."], "meter": "--+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da\u00df J\u00fcngling ich bin und frei.", "tokens": ["Da\u00df", "J\u00fcng\u00b7ling", "ich", "bin", "und", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VAFIN", "KON", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich f\u00fchl's, und f\u00fchle den Adel", "tokens": ["Ich", "f\u00fchl's", ",", "und", "f\u00fch\u00b7le", "den", "A\u00b7del"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der deutschen Seele \u2013 und f\u00fchle f\u00fcr Z\u00e4rtlichkeit", "tokens": ["Der", "deut\u00b7schen", "See\u00b7le", "\u2013", "und", "f\u00fch\u00b7le", "f\u00fcr", "Z\u00e4rt\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "KON", "VVFIN", "APPR", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Tugend vollb\u00fcrtige Schwester.", "tokens": ["Der", "Tu\u00b7gend", "voll\u00b7b\u00fcr\u00b7ti\u00b7ge", "Schwes\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Auch hab' ich Freunde gefunden", "tokens": ["Auch", "hab'", "ich", "Freun\u00b7de", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In Grevesm\u00fclens heimischer stiller Flur,", "tokens": ["In", "Gre\u00b7ves\u00b7m\u00fc\u00b7lens", "hei\u00b7mi\u00b7scher", "stil\u00b7ler", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Am Balth, an der Warne, am Rikk:", "tokens": ["Am", "Balth", ",", "an", "der", "War\u00b7ne", ",", "am", "Rikk", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ART", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Und M\u00e4dchen hab' ich gefunden,", "tokens": ["Und", "M\u00e4d\u00b7chen", "hab'", "ich", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "T\u00f6chter des Sch\u00f6pferhauchs, goldgelockt, s\u00fc\u00df und mild,", "tokens": ["T\u00f6ch\u00b7ter", "des", "Sch\u00f6p\u00b7fer\u00b7hauchs", ",", "gold\u00b7ge\u00b7lockt", ",", "s\u00fc\u00df", "und", "mild", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Und rein an Busen und Seele.", "tokens": ["Und", "rein", "an", "Bu\u00b7sen", "und", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Fern wandl' ich jetzt von der Warne", "tokens": ["Fern", "wandl'", "ich", "jetzt", "von", "der", "War\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "+--++-+-", "measure": "dactylic.init"}, "line.2": {"text": "Von Balth, von Grevesm\u00fclens heimischer Flur,", "tokens": ["Von", "Balth", ",", "von", "Gre\u00b7ves\u00b7m\u00fc\u00b7lens", "hei\u00b7mi\u00b7scher", "Flur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Bald fern auch vom d\u00fcsternblauen Rikk,", "tokens": ["Bald", "fern", "auch", "vom", "d\u00fcs\u00b7tern\u00b7blau\u00b7en", "Rikk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Und weilt' ich im w\u00fcthigen Zaara", "tokens": ["Und", "weilt'", "ich", "im", "w\u00fct\u00b7hi\u00b7gen", "Zaa\u00b7ra"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Auf Tempens blumigen Feldern, auf Zembla's Schnee \u2013", "tokens": ["Auf", "Tem\u00b7pens", "blu\u00b7mi\u00b7gen", "Fel\u00b7dern", ",", "auf", "Zem\u00b7bla's", "Schnee", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,", "APPR", "NE", "NN", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ich d\u00e4chte Euer, ihr Edlen,", "tokens": ["Ich", "d\u00e4ch\u00b7te", "Eu\u00b7er", ",", "ihr", "Ed\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "D\u00e4cht' Euer im S\u00e4useln des Abends,", "tokens": ["D\u00e4cht'", "Eu\u00b7er", "im", "S\u00e4u\u00b7seln", "des", "A\u00b7bends", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Im D\u00e4mmern der Sommermondnacht, d\u00e4chte Eu'r", "tokens": ["Im", "D\u00e4m\u00b7mern", "der", "Som\u00b7mer\u00b7mond\u00b7nacht", ",", "d\u00e4ch\u00b7te", "Eu'r"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "VVFIN", "NE"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und weinte mir die Wonne der Wehmuth. \u2013", "tokens": ["Und", "wein\u00b7te", "mir", "die", "Won\u00b7ne", "der", "Weh\u00b7muth", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Wer kommt vom H\u00fcgel? Ein Edler,", "tokens": ["Wer", "kommt", "vom", "H\u00fc\u00b7gel", "?", "Ein", "Ed\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPRART", "NN", "$.", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der sucht mich lange und findet mich nicht, und spricht:", "tokens": ["Der", "sucht", "mich", "lan\u00b7ge", "und", "fin\u00b7det", "mich", "nicht", ",", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "PPER", "PTKNEG", "$,", "KON", "VVFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wo ist er, der S\u00e4nger, mein Freund?", "tokens": ["Wo", "ist", "er", ",", "der", "S\u00e4n\u00b7ger", ",", "mein", "Freund", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "ART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.10": {"line.1": {"text": "\u00bbsuche nicht den S\u00e4nger, du Edler.", "tokens": ["\u00bb", "su\u00b7che", "nicht", "den", "S\u00e4n\u00b7ger", ",", "du", "Ed\u00b7ler", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKNEG", "ART", "NN", "$,", "PPER", "NE", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Er ruht im k\u00fchleren Hause. Der Nordwind wiegt", "tokens": ["Er", "ruht", "im", "k\u00fch\u00b7le\u00b7ren", "Hau\u00b7se", ".", "Der", "Nord\u00b7wind", "wiegt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den Wermuth auf seinem Grabe.\u00ab", "tokens": ["Den", "Wer\u00b7muth", "auf", "sei\u00b7nem", "Gra\u00b7be", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Und hoch erseufzt mir der Edle:", "tokens": ["Und", "hoch", "er\u00b7seufzt", "mir", "der", "Ed\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "ADJA", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "So bist du, Guter, so fr\u00fch gefallen. Zu fr\u00fch", "tokens": ["So", "bist", "du", ",", "Gu\u00b7ter", ",", "so", "fr\u00fch", "ge\u00b7fal\u00b7len", ".", "Zu", "fr\u00fch"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "$,", "NN", "$,", "ADV", "ADJD", "VVPP", "$.", "PTKA", "ADJD"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "\u00bbbist mir gefallen, du Guter!\u00ab", "tokens": ["\u00bb", "bist", "mir", "ge\u00b7fal\u00b7len", ",", "du", "Gu\u00b7ter", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "VAFIN", "PPER", "VVPP", "$,", "PPER", "NN", "$.", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.12": {"line.1": {"text": "Er wandelt thr\u00e4nend. Von fernen", "tokens": ["Er", "wan\u00b7delt", "thr\u00e4\u00b7nend", ".", "Von", "fer\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "APPR", "ADJA"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ergeht das M\u00e4dchen meiner Seele und harrt,", "tokens": ["Er\u00b7geht", "das", "M\u00e4d\u00b7chen", "mei\u00b7ner", "See\u00b7le", "und", "harrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Und rufet: Wo bleibt mein Freund!", "tokens": ["Und", "ru\u00b7fet", ":", "Wo", "bleibt", "mein", "Freund", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PWAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.13": {"line.1": {"text": "\u00bbrufe nicht dem Freund', du Holde!", "tokens": ["\u00bb", "ru\u00b7fe", "nicht", "dem", "Freund'", ",", "du", "Hol\u00b7de", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKNEG", "ART", "NN", "$,", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Er schl\u00e4ft den eisernen Schlummer. Nicht h\u00f6rt sein Ohr", "tokens": ["Er", "schl\u00e4ft", "den", "ei\u00b7ser\u00b7nen", "Schlum\u00b7mer", ".", "Nicht", "h\u00f6rt", "sein", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$.", "PTKNEG", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Hinfort die Stimme der Theuren.\u00ab", "tokens": ["Hin\u00b7fort", "die", "Stim\u00b7me", "der", "Theu\u00b7ren", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.14": {"line.1": {"text": "Und kl\u00e4glich jammert das M\u00e4dchen.", "tokens": ["Und", "kl\u00e4g\u00b7lich", "jam\u00b7mert", "das", "M\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ihr blaues Auge bew\u00f6lkt sich. Ihr golden Haar", "tokens": ["Ihr", "blau\u00b7es", "Au\u00b7ge", "be\u00b7w\u00f6lkt", "sich", ".", "Ihr", "gol\u00b7den", "Haar"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PRF", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Flie\u00dft milde \u00fcber den Busen.", "tokens": ["Flie\u00dft", "mil\u00b7de", "\u00fc\u00b7ber", "den", "Bu\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.15": {"line.1": {"text": "Sie wandelt n\u00e4chtlich zum Grabe.", "tokens": ["Sie", "wan\u00b7delt", "n\u00e4cht\u00b7lich", "zum", "Gra\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Im Wehen der Pappeln umsaus't sie mein Schatten, und sie", "tokens": ["Im", "We\u00b7hen", "der", "Pap\u00b7peln", "ums\u00b7aus't", "sie", "mein", "Schat\u00b7ten", ",", "und", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "KON", "PPER"], "meter": "-+--+--+--+--+", "measure": "amphibrach.penta.plus"}, "line.3": {"text": "Seufzt: Ruhe mir sanft, du Werther!!", "tokens": ["Seufzt", ":", "Ru\u00b7he", "mir", "sanft", ",", "du", "Wert\u00b7her", "!!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "PPER", "ADJD", "$,", "PPER", "NE", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}