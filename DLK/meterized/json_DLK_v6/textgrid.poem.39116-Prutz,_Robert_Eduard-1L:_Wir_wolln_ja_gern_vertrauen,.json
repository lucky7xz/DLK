{"textgrid.poem.39116": {"metadata": {"author": {"name": "Prutz, Robert Eduard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wir wolln ja gern vertrauen,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir wolln ja gern vertrauen,", "tokens": ["Wir", "wolln", "ja", "gern", "ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohlan, vertraut auch ihr!", "tokens": ["Wo\u00b7hlan", ",", "ver\u00b7traut", "auch", "ihr", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "ADV", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und dann verbr\u00fcdert bauen", "tokens": ["Und", "dann", "ver\u00b7br\u00fc\u00b7dert", "bau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VVPP", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Dom der Freiheit wir.", "tokens": ["Den", "Dom", "der", "Frei\u00b7heit", "wir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihr seid die Hocherkornen,", "tokens": ["Ihr", "seid", "die", "Ho\u00b7cher\u00b7kor\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr kennt die Sehnsucht nicht.", "tokens": ["Ihr", "kennt", "die", "Sehn\u00b7sucht", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von der uns Staubgebornen", "tokens": ["Von", "der", "uns", "Staub\u00b7ge\u00b7bor\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die bange Seele bricht.", "tokens": ["Die", "ban\u00b7ge", "See\u00b7le", "bricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ihr kennet nicht die Wunden", "tokens": ["Ihr", "ken\u00b7net", "nicht", "die", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ohnm\u00e4cht'ger Sklaverei.", "tokens": ["Ohn\u00b7m\u00e4cht'\u00b7ger", "Skla\u00b7ve\u00b7rei", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr habt es nie empfunden,", "tokens": ["Ihr", "habt", "es", "nie", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie schwer die Knechtschaft sei.", "tokens": ["Wie", "schwer", "die", "Knecht\u00b7schaft", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und doch, in goldnen Mauern", "tokens": ["Und", "doch", ",", "in", "gold\u00b7nen", "Mau\u00b7ern"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Unter der Krone Last,", "tokens": ["Un\u00b7ter", "der", "Kro\u00b7ne", "Last", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Auch ihr seid zu bedauern \u2013", "tokens": ["Auch", "ihr", "seid", "zu", "be\u00b7dau\u00b7ern", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PTKZU", "VVINF", "$("], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "So einsam, so verha\u00dft!", "tokens": ["So", "ein\u00b7sam", ",", "so", "ver\u00b7ha\u00dft", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wohlan, von euren Thronen", "tokens": ["Wo\u00b7hlan", ",", "von", "eu\u00b7ren", "Thro\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So steigt auch ihr herab,", "tokens": ["So", "steigt", "auch", "ihr", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und legt die kalten Kronen,", "tokens": ["Und", "legt", "die", "kal\u00b7ten", "Kro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den toten Purpur ab.", "tokens": ["Den", "to\u00b7ten", "Pur\u00b7pur", "ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "La\u00dft uns die Herzen tauschen,", "tokens": ["La\u00dft", "uns", "die", "Her\u00b7zen", "tau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das allerw\u00e4rmste Blut!", "tokens": ["Das", "al\u00b7ler\u00b7w\u00e4rms\u00b7te", "Blut", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "La\u00dft wonnig uns durchrauschen", "tokens": ["La\u00dft", "won\u00b7nig", "uns", "durc\u00b7hrau\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ADJD", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Lebens heil'ge Flut!", "tokens": ["Des", "Le\u00b7bens", "heil'\u00b7ge", "Flut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Noch ist es Zeit zu w\u00e4hlen,", "tokens": ["Noch", "ist", "es", "Zeit", "zu", "w\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O nehmt die Zeit in acht!", "tokens": ["O", "nehmt", "die", "Zeit", "in", "acht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "CARD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Noch haben unsre Seelen", "tokens": ["Noch", "ha\u00b7ben", "uns\u00b7re", "See\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht ganz sich losgemacht!", "tokens": ["Nicht", "ganz", "sich", "los\u00b7ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PRF", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Noch wollen wir vertrauen,", "tokens": ["Noch", "wol\u00b7len", "wir", "ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohlan, vertraut auch ihr!", "tokens": ["Wo\u00b7hlan", ",", "ver\u00b7traut", "auch", "ihr", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "ADV", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und dann verbr\u00fcdert bauen", "tokens": ["Und", "dann", "ver\u00b7br\u00fc\u00b7dert", "bau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VVPP", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Dom der Freiheit wir.", "tokens": ["Den", "Dom", "der", "Frei\u00b7heit", "wir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}