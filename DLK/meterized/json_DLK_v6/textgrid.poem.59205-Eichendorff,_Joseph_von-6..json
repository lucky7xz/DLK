{"textgrid.poem.59205": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihm ist's verliehn, aus den verworrnen Tagen,", "tokens": ["Ihm", "ist's", "ver\u00b7liehn", ",", "aus", "den", "ver\u00b7worr\u00b7nen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die um die andern sich wie Kerker dichten,", "tokens": ["Die", "um", "die", "an\u00b7dern", "sich", "wie", "Ker\u00b7ker", "dich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "PRF", "KOKOM", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zum blauen Himmel sich emporzurichten,", "tokens": ["Zum", "blau\u00b7en", "Him\u00b7mel", "sich", "em\u00b7por\u00b7zu\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In Freudigkeit: Hie bin ich, Herr! zu sagen.", "tokens": ["In", "Freu\u00b7dig\u00b7keit", ":", "Hie", "bin", "ich", ",", "Herr", "!", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "ADV", "VAFIN", "PPER", "$,", "NN", "$.", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das Leben hat zum Ritter ihn geschlagen,", "tokens": ["Das", "Le\u00b7ben", "hat", "zum", "Rit\u00b7ter", "ihn", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er soll der Sch\u00f6nheit neid'sche Kerker lichten;", "tokens": ["Er", "soll", "der", "Sch\u00f6n\u00b7heit", "nei\u00b7d'\u00b7sche", "Ker\u00b7ker", "lich\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da\u00df nicht sich alle g\u00f6tterlos vernichten,", "tokens": ["Da\u00df", "nicht", "sich", "al\u00b7le", "g\u00f6t\u00b7ter\u00b7los", "ver\u00b7nich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "PRF", "PIS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Soll er die G\u00f6tter zu beschw\u00f6ren wagen.", "tokens": ["Soll", "er", "die", "G\u00f6t\u00b7ter", "zu", "be\u00b7schw\u00f6\u00b7ren", "wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Tritt erst die Lieb auf seine bl\u00fch'nden H\u00fcgel,", "tokens": ["Tritt", "erst", "die", "Lieb", "auf", "sei\u00b7ne", "bl\u00fch'n\u00b7den", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fchlt er die reichen Kr\u00e4nze in den Haaren,", "tokens": ["F\u00fchlt", "er", "die", "rei\u00b7chen", "Kr\u00e4n\u00b7ze", "in", "den", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit Morgenrot mu\u00df sich die Erde schm\u00fccken;", "tokens": ["Mit", "Mor\u00b7gen\u00b7rot", "mu\u00df", "sich", "die", "Er\u00b7de", "schm\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "S\u00fc\u00dfschauernd dehnt der Geist die gro\u00dfen Fl\u00fcgel,", "tokens": ["S\u00fc\u00df\u00b7schau\u00b7ernd", "dehnt", "der", "Geist", "die", "gro\u00b7\u00dfen", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es gl\u00e4nzt das Meer \u2013 die mut'gen Schiffe fahren,", "tokens": ["Es", "gl\u00e4nzt", "das", "Meer", "\u2013", "die", "mut'\u00b7gen", "Schif\u00b7fe", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da ist nichts mehr, was ihm nicht sollte gl\u00fccken!", "tokens": ["Da", "ist", "nichts", "mehr", ",", "was", "ihm", "nicht", "soll\u00b7te", "gl\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "$,", "PWS", "PPER", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}