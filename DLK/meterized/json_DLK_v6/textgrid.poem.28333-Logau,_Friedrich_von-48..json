{"textgrid.poem.28333": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "48.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gastro, wo er geht und steht, tr\u00e4gt den Watsack f\u00fcr sich her;", "tokens": ["Gast\u00b7ro", ",", "wo", "er", "geht", "und", "steht", ",", "tr\u00e4gt", "den", "Wat\u00b7sack", "f\u00fcr", "sich", "her", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$,", "VVFIN", "ART", "NN", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+--+--+--+--+", "measure": "amphibrach.penta.plus"}, "line.2": {"text": "Ob er gleich nun strutzend voll, nimmer oder selten leer,", "tokens": ["Ob", "er", "gleich", "nun", "strut\u00b7zend", "voll", ",", "nim\u00b7mer", "o\u00b7der", "sel\u00b7ten", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "ADJD", "$,", "ADV", "KON", "ADJD", "ADJD", "$,"], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Hab ich doch noch nie geh\u00f6rt, da\u00df ihn etwa ein Soldat,", "tokens": ["Hab", "ich", "doch", "noch", "nie", "ge\u00b7h\u00f6rt", ",", "da\u00df", "ihn", "et\u00b7wa", "ein", "Sol\u00b7dat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "ADV", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Wann er gleich wo au\u00dfgelegt, ie, wie Brauch, gepl\u00fcndert hat.", "tokens": ["Wann", "er", "gleich", "wo", "au\u00df\u00b7ge\u00b7legt", ",", "ie", ",", "wie", "Brauch", ",", "ge\u00b7pl\u00fcn\u00b7dert", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PWAV", "VVPP", "$,", "ADV", "$,", "PWAV", "NN", "$,", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}}}}}