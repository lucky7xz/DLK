{"textgrid.poem.26804": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der be\u00dfre Mensch tritt in die Welt", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der be\u00dfre Mensch tritt in die Welt", "tokens": ["Der", "be\u00df\u00b7re", "Mensch", "tritt", "in", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit fr\u00f6hlichem Vertrauen,", "tokens": ["Mit", "fr\u00f6h\u00b7li\u00b7chem", "Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er glaubt, was ihm die Seele schwellt,", "tokens": ["Er", "glaubt", ",", "was", "ihm", "die", "See\u00b7le", "schwellt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch au\u00dfer sich zu schauen,", "tokens": ["Auch", "au\u00b7\u00dfer", "sich", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und weiht, von edlem Eifer warm,", "tokens": ["Und", "weiht", ",", "von", "ed\u00b7lem", "Ei\u00b7fer", "warm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Wahrheit seinen treuen Arm.", "tokens": ["Der", "Wahr\u00b7heit", "sei\u00b7nen", "treu\u00b7en", "Arm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch alles ist so klein, so eng,", "tokens": ["Doch", "al\u00b7les", "ist", "so", "klein", ",", "so", "eng", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat er es erst erfahren,", "tokens": ["Hat", "er", "es", "erst", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da sucht er in dem Weltgedr\u00e4ng", "tokens": ["Da", "sucht", "er", "in", "dem", "Welt\u00b7ge\u00b7dr\u00e4ng"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich selbst nur zu bewahren,", "tokens": ["Sich", "selbst", "nur", "zu", "be\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das Herz in kalter stolzer Ruh", "tokens": ["Das", "Herz", "in", "kal\u00b7ter", "stol\u00b7zer", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schlie\u00dft endlich sich der Liebe zu.", "tokens": ["Schlie\u00dft", "end\u00b7lich", "sich", "der", "Lie\u00b7be", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie geben, ach! nicht immer Glut,", "tokens": ["Sie", "ge\u00b7ben", ",", "ach", "!", "nicht", "im\u00b7mer", "Glut", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "ITJ", "$.", "PTKNEG", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Wahrheit helle Strahlen.", "tokens": ["Der", "Wahr\u00b7heit", "hel\u00b7le", "Strah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wohl denen, die des Wissens Gut", "tokens": ["Wohl", "de\u00b7nen", ",", "die", "des", "Wis\u00b7sens", "Gut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PDS", "$,", "PRELS", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht mit dem Herzen zahlen!", "tokens": ["Nicht", "mit", "dem", "Her\u00b7zen", "zah\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Drum paart, zu eurem sch\u00f6nsten Gl\u00fcck,", "tokens": ["Drum", "paart", ",", "zu", "eu\u00b7rem", "sch\u00f6ns\u00b7ten", "Gl\u00fcck", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Schw\u00e4rmers Ernst des Weltmanns Blick.", "tokens": ["Mit", "Schw\u00e4r\u00b7mers", "Ernst", "des", "Welt\u00b7manns", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}