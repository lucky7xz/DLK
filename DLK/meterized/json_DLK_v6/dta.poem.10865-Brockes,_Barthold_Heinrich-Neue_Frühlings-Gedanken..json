{"dta.poem.10865": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Neue Fr\u00fchlings-Gedanken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gott Lob! wir n\u00e4hern uns aufs neu der Sonnen Stral,", "tokens": ["Gott", "Lob", "!", "wir", "n\u00e4\u00b7hern", "uns", "aufs", "neu", "der", "Son\u00b7nen", "Stral", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "PPER", "VVFIN", "PPER", "APPRART", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Licht und W\u00e4rme Quell, dem Born der Fruchtbarkeit,", "tokens": ["Der", "Licht", "und", "W\u00e4r\u00b7me", "Quell", ",", "dem", "Born", "der", "Frucht\u00b7bar\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "NN", "$,", "PRELS", "NE", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und es erscheinet abermal", "tokens": ["Und", "es", "er\u00b7schei\u00b7net", "a\u00b7ber\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die angenehme Fr\u00fchlingszeit.", "tokens": ["Die", "an\u00b7ge\u00b7neh\u00b7me", "Fr\u00fch\u00b7lings\u00b7zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des strengen Frosts noch nicht verschwundnes Angedenken", "tokens": ["Des", "stren\u00b7gen", "Frosts", "noch", "nicht", "ver\u00b7schwund\u00b7nes", "An\u00b7ge\u00b7den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "PTKNEG", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vermehret, durch den Gegensatz,", "tokens": ["Ver\u00b7meh\u00b7ret", ",", "durch", "den", "Ge\u00b7gen\u00b7satz", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den allbereit erhaltnen Schatz,", "tokens": ["Den", "all\u00b7be\u00b7reit", "er\u00b7halt\u00b7nen", "Schatz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den uns der fr\u00fche Lenz bereits beginnt, zu schenken,", "tokens": ["Den", "uns", "der", "fr\u00fc\u00b7he", "Lenz", "be\u00b7reits", "be\u00b7ginnt", ",", "zu", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "ADV", "VVFIN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wozu zugleich die Tr\u00f6sterinn der Welt,", "tokens": ["Wo\u00b7zu", "zu\u00b7gleich", "die", "Tr\u00f6s\u00b7te\u00b7rinn", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.11": {"text": "Und unsrer aufgeweckten Brust", "tokens": ["Und", "uns\u00b7rer", "auf\u00b7ge\u00b7weck\u00b7ten", "Brust"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Jm k\u00fcnftigen noch immer gr\u00f6\u00dfre Lust,", "tokens": ["Jm", "k\u00fcnf\u00b7ti\u00b7gen", "noch", "im\u00b7mer", "gr\u00f6\u00df\u00b7re", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Mit wahrer Schmeicheley, verspricht,", "tokens": ["Mit", "wah\u00b7rer", "Schmei\u00b7che\u00b7ley", ",", "ver\u00b7spricht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Drey Zeiten scheinen sich auf die Art zu bestreben,", "tokens": ["Drey", "Zei\u00b7ten", "schei\u00b7nen", "sich", "auf", "die", "Art", "zu", "be\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Euch mannigfaltge Lust zu geben,", "tokens": ["Euch", "man\u00b7nig\u00b7falt\u00b7ge", "Lust", "zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Und, durch verschiedne Seltenheit,", "tokens": ["Und", ",", "durch", "ver\u00b7schied\u00b7ne", "Sel\u00b7ten\u00b7heit", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Die jeder eigen ist, euch die Vergn\u00fcglichkeit,", "tokens": ["Die", "je\u00b7der", "ei\u00b7gen", "ist", ",", "euch", "die", "Ver\u00b7gn\u00fcg\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "VAFIN", "$,", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und eure Lust noch zu erheben.", "tokens": ["Und", "eu\u00b7re", "Lust", "noch", "zu", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Ach la\u00dft das sch\u00f6nste Theil von eurem Leben nicht", "tokens": ["Ach", "la\u00dft", "das", "sch\u00f6ns\u00b7te", "Theil", "von", "eu\u00b7rem", "Le\u00b7ben", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So ungepr\u00fcft, wie sonst, und nicht vergebens,", "tokens": ["So", "un\u00b7ge\u00b7pr\u00fcft", ",", "wie", "sonst", ",", "und", "nicht", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "ADV", "$,", "KON", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Ohn ihrer Reizung zu genie\u00dfen,", "tokens": ["Ohn", "ih\u00b7rer", "Rei\u00b7zung", "zu", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Vergehn, verschwinden und verflie\u00dfen!", "tokens": ["Ver\u00b7gehn", ",", "ver\u00b7schwin\u00b7den", "und", "ver\u00b7flie\u00b7\u00dfen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Ach seyd doch nicht so gar verkehrt,", "tokens": ["Ach", "seyd", "doch", "nicht", "so", "gar", "ver\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "ADV", "PTKNEG", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Den Winter, als noch nicht vergangen,", "tokens": ["Den", "Win\u00b7ter", ",", "als", "noch", "nicht", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Die Fr\u00fchlingszeit, als noch nicht angefangen,", "tokens": ["Die", "Fr\u00fch\u00b7lings\u00b7zeit", ",", "als", "noch", "nicht", "an\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Und als noch nicht Betrachtungs-werth,", "tokens": ["Und", "als", "noch", "nicht", "Be\u00b7trach\u00b7tungs\u00b7werth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Auf noch was besseres stets hoffend, anzusehn;", "tokens": ["Auf", "noch", "was", "bes\u00b7se\u00b7res", "stets", "hof\u00b7fend", ",", "an\u00b7zu\u00b7sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADV", "PWS", "ADJA", "ADV", "VVPP", "$,", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Ja obschon viele Ding euch, in dem Garten,", "tokens": ["Ja", "ob\u00b7schon", "vie\u00b7le", "Ding", "euch", ",", "in", "dem", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "KOUS", "PIAT", "NN", "PPER", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Und \u00fcberall, bereits zu eurer Lust entsprie\u00dfen,", "tokens": ["Und", "\u00fc\u00b7be\u00b7rall", ",", "be\u00b7reits", "zu", "eu\u00b7rer", "Lust", "ent\u00b7sprie\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Anstatt derselben zu genie\u00dfen,", "tokens": ["An\u00b7statt", "der\u00b7sel\u00b7ben", "zu", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Noch immer auf das K\u00fcnftige zu warten,", "tokens": ["Noch", "im\u00b7mer", "auf", "das", "K\u00fcnf\u00b7ti\u00b7ge", "zu", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Da doch so dann Gewohnheit, wie bisher,", "tokens": ["Da", "doch", "so", "dann", "Ge\u00b7wohn\u00b7heit", ",", "wie", "bis\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADV", "NN", "$,", "PWAV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Euch alle Lust unfehlbar raubet,", "tokens": ["Euch", "al\u00b7le", "Lust", "un\u00b7fehl\u00b7bar", "rau\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Und, euch verblendend, mehr und mehr,", "tokens": ["Und", ",", "euch", "ver\u00b7blen\u00b7dend", ",", "mehr", "und", "mehr", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPER", "VVPP", "$,", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Euch selbst zur Lust, und Gott zur Ehr,", "tokens": ["Euch", "selbst", "zur", "Lust", ",", "und", "Gott", "zur", "Ehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "$,", "KON", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Die Welt zu brauchen, nicht erlaubet.", "tokens": ["Die", "Welt", "zu", "brau\u00b7chen", ",", "nicht", "er\u00b7lau\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Wie mancher Lenz ist euch, auf diese Weis, ", "tokens": ["Wie", "man\u00b7cher", "Lenz", "ist", "euch", ",", "auf", "die\u00b7se", "Weis", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VAFIN", "PPER", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Eh ihr, durch Aufschub erst, verf\u00fchrt,", "tokens": ["Eh", "ihr", ",", "durch", "Auf\u00b7schub", "erst", ",", "ver\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "NN", "ADV", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Nachhero durch Gewohnheit unger\u00fchrt,", "tokens": ["Nach\u00b7he\u00b7ro", "durch", "Ge\u00b7wohn\u00b7heit", "un\u00b7ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADJD", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.40": {"text": "Jhn zu genie\u00dfen, angefangen.", "tokens": ["Jhn", "zu", "ge\u00b7nie\u00b7\u00dfen", ",", "an\u00b7ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Soll euch denn der Erfahrung Licht", "tokens": ["Soll", "euch", "denn", "der", "Er\u00b7fah\u00b7rung", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Nicht einmal euren Fehler zeigen?", "tokens": ["Nicht", "ein\u00b7mal", "eu\u00b7ren", "Feh\u00b7ler", "zei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Wollt, ihr so oft Betrogne, nicht", "tokens": ["Wollt", ",", "ihr", "so", "oft", "Be\u00b7trog\u00b7ne", ",", "nicht"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["VMFIN", "$,", "PPER", "ADV", "ADV", "NN", "$,", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Aus eurem Pfuhl des Ungl\u00fccks steigen?", "tokens": ["Aus", "eu\u00b7rem", "Pfuhl", "des", "Un\u00b7gl\u00fccks", "stei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Auf! la\u00dft uns, wie, in allen Dingen,", "tokens": ["Auf", "!", "la\u00dft", "uns", ",", "wie", ",", "in", "al\u00b7len", "Din\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVIMP", "PPER", "$,", "PWAV", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Sich alle Kr\u00e4fte jetzt verj\u00fcngen,", "tokens": ["Sich", "al\u00b7le", "Kr\u00e4f\u00b7te", "jetzt", "ver\u00b7j\u00fcn\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PIAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Wie Himmel, Erd und Meer so sch\u00f6n,", "tokens": ["Wie", "Him\u00b7mel", ",", "Erd", "und", "Meer", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "NN", "KON", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "In neuem Glanz und Schimmer stehn;", "tokens": ["In", "neu\u00b7em", "Glanz", "und", "Schim\u00b7mer", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Durch ihre Pracht ger\u00fchrt, besehn!", "tokens": ["Durch", "ih\u00b7re", "Pracht", "ge\u00b7r\u00fchrt", ",", "be\u00b7sehn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Und die darob, in unsrer Brust,", "tokens": ["Und", "die", "da\u00b7rob", ",", "in", "uns\u00b7rer", "Brust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PAV", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, 