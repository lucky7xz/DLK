{"textgrid.poem.44940": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[weil mich Geselligkeit mit vielen nicht vereint]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil mich Geselligkeit mit vielen nicht vereint,", "tokens": ["Weil", "mich", "Ge\u00b7sel\u00b7lig\u00b7keit", "mit", "vie\u00b7len", "nicht", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "APPR", "PIS", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "H\u00e4lt man mich hie und da f\u00fcr einen Menschenfeind.", "tokens": ["H\u00e4lt", "man", "mich", "hie", "und", "da", "f\u00fcr", "ei\u00b7nen", "Men\u00b7schen\u00b7feind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "ADV", "KON", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Euch flieht nur mein Verstand, mein Herz ist euch geblieben,", "tokens": ["Euch", "flieht", "nur", "mein", "Ver\u00b7stand", ",", "mein", "Herz", "ist", "euch", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ich entferne mich, um f\u00fcrder euch zu lieben.", "tokens": ["Und", "ich", "ent\u00b7fer\u00b7ne", "mich", ",", "um", "f\u00fcr\u00b7der", "euch", "zu", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "KOUI", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}