{"textgrid.poem.43314": {"metadata": {"author": {"name": "Gerstenberg, Heinrich Wilhelm von", "birth": "N.A.", "death": "N.A."}, "title": "1L: G\u00f6ttinn, G\u00f6ttinn zarter Herzen,", "genre": "verse", "period": "N.A.", "pub_year": 1780, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "G\u00f6ttinn, G\u00f6ttinn zarter Herzen,", "tokens": ["G\u00f6t\u00b7tinn", ",", "G\u00f6t\u00b7tinn", "zar\u00b7ter", "Her\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O du bists mit deinen Scherzen!", "tokens": ["O", "du", "bists", "mit", "dei\u00b7nen", "Scher\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dein Gefolg sind L\u00fcsternheit,", "tokens": ["Dein", "Ge\u00b7folg", "sind", "L\u00fcs\u00b7tern\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Muthwill, laute Fr\u00f6hlichkeit,", "tokens": ["Mut\u00b7hwill", ",", "lau\u00b7te", "Fr\u00f6h\u00b7lich\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "$,", "VVFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Anmuthsvolle Sp\u00f6ttereyen,", "tokens": ["An\u00b7muths\u00b7vol\u00b7le", "Sp\u00f6t\u00b7te\u00b7re\u00b7yen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Winke, Blicke, T\u00e4ndeleyen!", "tokens": ["Win\u00b7ke", ",", "Bli\u00b7cke", ",", "T\u00e4n\u00b7de\u00b7le\u00b7yen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Seht, o seht an ihren Wangen", "tokens": ["Seht", ",", "o", "seht", "an", "ih\u00b7ren", "Wan\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "FM", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Alle L\u00e4chelgeister hangen,", "tokens": ["Al\u00b7le", "L\u00e4\u00b7chel\u00b7geis\u00b7ter", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "In der Wangen Gr\u00fcbchen lauschen,", "tokens": ["In", "der", "Wan\u00b7gen", "Gr\u00fcb\u00b7chen", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und in Wonne sich berauschen!", "tokens": ["Und", "in", "Won\u00b7ne", "sich", "be\u00b7rau\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In meines M\u00e4dchens Augenliedern blinken,", "tokens": ["In", "mei\u00b7nes", "M\u00e4d\u00b7chens", "Au\u00b7gen\u00b7lie\u00b7dern", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Geister von zehntausend holden Winken,", "tokens": ["Die", "Geis\u00b7ter", "von", "zehn\u00b7tau\u00b7send", "hol\u00b7den", "Win\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gleich dem Gestirn in heller Nacht.", "tokens": ["Gleich", "dem", "Ge\u00b7stirn", "in", "hel\u00b7ler", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Es dr\u00e4ngt sich, wie auf Knospen junger Rosen,", "tokens": ["Es", "dr\u00e4ngt", "sich", ",", "wie", "auf", "Knos\u00b7pen", "jun\u00b7ger", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "PWAV", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der keuschen Freuden Heer, sie liebzukosen,", "tokens": ["Der", "keu\u00b7schen", "Freu\u00b7den", "Heer", ",", "sie", "lieb\u00b7zu\u00b7ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Auf ihren Lippen, wenn sie lacht.", "tokens": ["Auf", "ih\u00b7ren", "Lip\u00b7pen", ",", "wenn", "sie", "lacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}