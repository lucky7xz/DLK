{"dta.poem.14520": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der W\u00e4chter .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "N\u00e4chtlich macht der Herr die Rund',", "tokens": ["N\u00e4cht\u00b7lich", "macht", "der", "Herr", "die", "Rund'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sucht die Seinen unverdrossen,", "tokens": ["Sucht", "die", "Sei\u00b7nen", "un\u00b7ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PPOSS", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber \u00fcberall verschlossen", "tokens": ["A\u00b7ber", "\u00fc\u00b7be\u00b7rall", "ver\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trifft er Th\u00fcr und Herzensgrund,", "tokens": ["Trifft", "er", "Th\u00fcr", "und", "Her\u00b7zens\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und er wendet sich voll Trauer:", "tokens": ["Und", "er", "wen\u00b7det", "sich", "voll", "Trau\u00b7er", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Niemand ist, der mit mir wacht. \u2014", "tokens": ["Nie\u00b7mand", "ist", ",", "der", "mit", "mir", "wacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "VAFIN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nur der Wald vernimmt's mit Schauer,", "tokens": ["Nur", "der", "Wald", "ver\u00b7nimmt's", "mit", "Schau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Rauschet fromm die ganze Nacht.", "tokens": ["Rau\u00b7schet", "fromm", "die", "gan\u00b7ze", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}