{"textgrid.poem.60633": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Man soll sich m\u00f6glichst alle Welt verpflichten,", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man soll sich m\u00f6glichst alle Welt verpflichten,", "tokens": ["Man", "soll", "sich", "m\u00f6g\u00b7lichst", "al\u00b7le", "Welt", "ver\u00b7pflich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Kleinste selbst kann werten Dank verrichten", "tokens": ["Der", "Kleins\u00b7te", "selbst", "kann", "wer\u00b7ten", "Dank", "ver\u00b7rich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VMFIN", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nichts ahnend kroch", "tokens": ["Nichts", "ah\u00b7nend", "kroch"], "token_info": ["word", "word", "word"], "pos": ["PIS", "ADJD", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Aus ihrem Loch", "tokens": ["Aus", "ih\u00b7rem", "Loch"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Vor des L\u00f6wen Maul eine Ratte hervor", "tokens": ["Vor", "des", "L\u00f6\u00b7wen", "Maul", "ei\u00b7ne", "Rat\u00b7te", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "ART", "NN", "PTKVZ"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und bangte, da\u00df sie ihr Leben verlor.", "tokens": ["Und", "bang\u00b7te", ",", "da\u00df", "sie", "ihr", "Le\u00b7ben", "ver\u00b7lor", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Der K\u00f6nig der Tiere aber bewies", "tokens": ["Der", "K\u00f6\u00b7nig", "der", "Tie\u00b7re", "a\u00b7ber", "be\u00b7wies"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "ADV", "VVFIN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Gro\u00dfmut, indem er sie laufen lie\u00df.", "tokens": ["Gro\u00df\u00b7mut", ",", "in\u00b7dem", "er", "sie", "lau\u00b7fen", "lie\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PPER", "VVINF", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Die Wohltat blieb nicht ohne Lohn.", "tokens": ["Die", "Wohl\u00b7tat", "blieb", "nicht", "oh\u00b7ne", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wer glaubt wohl, da\u00df von jener Ratte", "tokens": ["Wer", "glaubt", "wohl", ",", "da\u00df", "von", "je\u00b7ner", "Rat\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "$,", "KOUS", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Der L\u00f6we einen Nutzen hatte?", "tokens": ["Der", "L\u00f6\u00b7we", "ei\u00b7nen", "Nut\u00b7zen", "hat\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und doch geschah's nach wenig Tagen schon,", "tokens": ["Und", "doch", "ge\u00b7scha\u00b7h's", "nach", "we\u00b7nig", "Ta\u00b7gen", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "APPR", "PIAT", "NN", "ADV", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.11": {"text": "Als er aus seinen sichern W\u00e4ldern ging", "tokens": ["Als", "er", "aus", "sei\u00b7nen", "si\u00b7chern", "W\u00e4l\u00b7dern", "ging"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und unversehens sich in einem Netze fing.", "tokens": ["Und", "un\u00b7ver\u00b7se\u00b7hens", "sich", "in", "ei\u00b7nem", "Net\u00b7ze", "fing", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Er machte drin \u2013 vergeblich \u2013 gro\u00df Geschrei", "tokens": ["Er", "mach\u00b7te", "drin", "\u2013", "ver\u00b7geb\u00b7lich", "\u2013", "gro\u00df", "Ge\u00b7schrei"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$(", "ADJD", "$(", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Und doch: die Ratte eilt darauf herbei \u2013", "tokens": ["Und", "doch", ":", "die", "Rat\u00b7te", "eilt", "da\u00b7rauf", "her\u00b7bei", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "ART", "NN", "VVFIN", "PAV", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Das Maschenwerk zernagen ihre Z\u00e4hne,", "tokens": ["Das", "Ma\u00b7schen\u00b7werk", "zer\u00b7na\u00b7gen", "ih\u00b7re", "Z\u00e4h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Befreit entkommt der K\u00f6nig mit der M\u00e4hne.", "tokens": ["Be\u00b7freit", "ent\u00b7kommt", "der", "K\u00f6\u00b7nig", "mit", "der", "M\u00e4h\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Viel mehr als Wut und gro\u00dfe Kraft", "tokens": ["Viel", "mehr", "als", "Wut", "und", "gro\u00b7\u00dfe", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "KOUS", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Hat hier Geduld und Zeit geschafft.", "tokens": ["Hat", "hier", "Ge\u00b7duld", "und", "Zeit", "ge\u00b7schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}