{"textgrid.poem.38233": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Ich stand an einem Morgen (3)", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich stand an einem Morgen: mein wo?", "tokens": ["Ich", "stand", "an", "ei\u00b7nem", "Mor\u00b7gen", ":", "mein", "wo", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$.", "PPOSAT", "PWAV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Hat dich niemand gesehen? Warum?", "tokens": ["Hat", "dich", "nie\u00b7mand", "ge\u00b7se\u00b7hen", "?", "Wa\u00b7rum", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "PIS", "VVPP", "$.", "PWAV", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Vor wem? von wem? wer war sie dann?", "tokens": ["Vor", "wem", "?", "von", "wem", "?", "wer", "war", "sie", "dann", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "$.", "APPR", "PWS", "$.", "PWS", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ists vielleicht die breite Gretha gewesen?", "tokens": ["Ists", "viel\u00b7leicht", "die", "brei\u00b7te", "Gre\u00b7tha", "ge\u00b7we\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "ADJA", "NE", "VAPP", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Was hat sie dann gesagt? Gl\u00fcck zu,", "tokens": ["Was", "hat", "sie", "dann", "ge\u00b7sagt", "?", "Gl\u00fcck", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "VVPP", "$.", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gl\u00fcck zu, Gl\u00fcck zu wohl auf die Rei\u00df.", "tokens": ["Gl\u00fcck", "zu", ",", "Gl\u00fcck", "zu", "wohl", "auf", "die", "Rei\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "NN", "APPR", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}