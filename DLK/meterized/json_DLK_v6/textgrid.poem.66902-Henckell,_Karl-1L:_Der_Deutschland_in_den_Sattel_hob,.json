{"textgrid.poem.66902": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Deutschland in den Sattel hob,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Deutschland in den Sattel hob,", "tokens": ["Der", "Deutschland", "in", "den", "Sat\u00b7tel", "hob", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ruht jenseits Tadel, jenseits Lob", "tokens": ["Ruht", "jen\u00b7seits", "Ta\u00b7del", ",", "jen\u00b7seits", "Lob"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Sachsenwaldes gr\u00fcnem Haus", "tokens": ["In", "Sach\u00b7sen\u00b7wal\u00b7des", "gr\u00fc\u00b7nem", "Haus"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Heldenkampf des Lebens aus.", "tokens": ["Vom", "Hel\u00b7den\u00b7kampf", "des", "Le\u00b7bens", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da zuckt mit Donnerkrach der Blitz!", "tokens": ["Da", "zuckt", "mit", "Don\u00b7ner\u00b7krach", "der", "Blitz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer klopft ans Grab? Der Alte Fritz.", "tokens": ["Wer", "klopft", "ans", "Grab", "?", "Der", "Al\u00b7te", "Fritz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPRART", "NN", "$.", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf! Beide machen sturmumgellt", "tokens": ["Auf", "!", "Bei\u00b7de", "ma\u00b7chen", "stur\u00b7mum\u00b7gellt"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["APPR", "$.", "PIS", "VVINF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mobil die Tod- und Teufelswelt.", "tokens": ["Mo\u00b7bil", "die", "To\u00b7d", "und", "Teu\u00b7fels\u00b7welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "TRUNC", "KON", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Hoch in gespenstergrauer Wehr", "tokens": ["Hoch", "in", "ge\u00b7spens\u00b7ter\u00b7grau\u00b7er", "Wehr"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ziehn sie voran dem wilden Heer,", "tokens": ["Ziehn", "sie", "vo\u00b7ran", "dem", "wil\u00b7den", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf Wolkenrossen durch die Nacht", "tokens": ["Auf", "Wol\u00b7ken\u00b7ros\u00b7sen", "durch", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hinwogend ob der V\u00f6lkerschlacht.", "tokens": ["Hin\u00b7wo\u00b7gend", "ob", "der", "V\u00f6l\u00b7ker\u00b7schlacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbund wer ist drunten mit dabei?", "tokens": ["\u00bb", "und", "wer", "ist", "drun\u00b7ten", "mit", "da\u00b7bei", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PWS", "VAFIN", "ADV", "APPR", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u203afrei-Deutschland!\u2039 t\u00f6nt ihr Feldgeschrei.", "tokens": ["\u203a", "frei\u00b7Deut\u00b7schland", "!", "\u2039", "t\u00f6nt", "ihr", "Feld\u00b7ge\u00b7schrei", "."], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NN", "$.", "$(", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Reichsfeind?! Nur ein Vaterland!", "tokens": ["Kein", "Reichs\u00b7feind", "?!", "Nur", "ein", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So steh, mein Volk, im Weltenbrand!\u00ab \u2013", "tokens": ["So", "steh", ",", "mein", "Volk", ",", "im", "Wel\u00b7ten\u00b7brand", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPOSAT", "NN", "$,", "APPRART", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wenn stark gesichert Deutschlands Gl\u00fcck,", "tokens": ["Wenn", "stark", "ge\u00b7si\u00b7chert", "Deutschlands", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VVPP", "NE", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Kehrt Bismarck in die Gruft zur\u00fcck", "tokens": ["Kehrt", "Bis\u00b7marck", "in", "die", "Gruft", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "NE", "APPR", "ART", "NN", "PTKVZ"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und ruht vom wilden H\u00f6llenstrau\u00df", "tokens": ["Und", "ruht", "vom", "wil\u00b7den", "H\u00f6l\u00b7len\u00b7strau\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Sachsenwald f\u00fcr ewig aus.", "tokens": ["Im", "Sach\u00b7sen\u00b7wald", "f\u00fcr", "e\u00b7wig", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}