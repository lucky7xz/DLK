{"textgrid.poem.26131": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Mai kommt Freude aufpfropfend herbei", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Mai aus dem h\u00e4rtesten Baum zu dir lacht", "tokens": ["Der", "Mai", "aus", "dem", "h\u00e4r\u00b7tes\u00b7ten", "Baum", "zu", "dir", "lacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "APPR", "PPER", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und alle Menschengesichter zu Vollmonden macht.", "tokens": ["Und", "al\u00b7le", "Men\u00b7schen\u00b7ge\u00b7sich\u00b7ter", "zu", "Voll\u00b7mon\u00b7den", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mai kommt Freude aufpfropfend herbei,", "tokens": ["Mai", "kommt", "Freu\u00b7de", "auf\u00b7pfrop\u00b7fend", "her\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "VVPP", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Maiheiterkeit tanzt \u00fcber Gr\u00e4ser frei.", "tokens": ["Mai\u00b7hei\u00b7ter\u00b7keit", "tanzt", "\u00fc\u00b7ber", "Gr\u00e4\u00b7ser", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mai macht dich verliebt in allen St\u00fccken,", "tokens": ["Mai", "macht", "dich", "ver\u00b7liebt", "in", "al\u00b7len", "St\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Aller Griesgram wird klein wie die spielenden M\u00fccken.", "tokens": ["Al\u00b7ler", "Gries\u00b7gram", "wird", "klein", "wie", "die", "spie\u00b7len\u00b7den", "M\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.7": {"text": "Der Mai l\u00e4\u00dft's Verliebtsein noch niemals mi\u00dfgl\u00fccken,", "tokens": ["Der", "Mai", "l\u00e4\u00dft's", "Ver\u00b7liebt\u00b7sein", "noch", "nie\u00b7mals", "mi\u00df\u00b7gl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Er schl\u00e4gt dich mit Sorgen nicht heute entzwei;", "tokens": ["Er", "schl\u00e4gt", "dich", "mit", "Sor\u00b7gen", "nicht", "heu\u00b7te", "ent\u00b7zwei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.9": {"text": "Der Mai verschiebt's Ungl\u00fcck auf morgen.", "tokens": ["Der", "Mai", "ver\u00b7schiebt's", "Un\u00b7gl\u00fcck", "auf", "mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}