{"textgrid.poem.33237": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "1L: So schliessen wir den Sarg der werthen Henrietten!", "genre": "verse", "period": "N.A.", "pub_year": 1676, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So schliessen wir den Sarg der werthen Henrietten!", "tokens": ["So", "schlies\u00b7sen", "wir", "den", "Sarg", "der", "wert\u00b7hen", "Hen\u00b7riet\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Es konten Ihren Leib nicht Stand noch Jugend retten,", "tokens": ["Es", "kon\u00b7ten", "Ih\u00b7ren", "Leib", "nicht", "Stand", "noch", "Ju\u00b7gend", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "PTKNEG", "NN", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nur Ihrer Gottesfurcht und Tugend wich der Tod,", "tokens": ["Nur", "Ih\u00b7rer", "Got\u00b7tes\u00b7furcht", "und", "Tu\u00b7gend", "wich", "der", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So, da\u00df ihr bester Theil vom Sterben frey geblieben:", "tokens": ["So", ",", "da\u00df", "ihr", "bes\u00b7ter", "Theil", "vom", "Ster\u00b7ben", "frey", "ge\u00b7blie\u00b7ben", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Durch jene lebt Ihr Geist, befreyt von aller Noth,", "tokens": ["Durch", "je\u00b7ne", "lebt", "Ihr", "Geist", ",", "be\u00b7freyt", "von", "al\u00b7ler", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VVFIN", "PPOSAT", "NN", "$,", "VVPP", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Durch diese bleibt Ihr Lob den Hertzen eingeschrieben.", "tokens": ["Durch", "die\u00b7se", "bleibt", "Ihr", "Lob", "den", "Hert\u00b7zen", "ein\u00b7ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VVFIN", "PPOSAT", "NN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}