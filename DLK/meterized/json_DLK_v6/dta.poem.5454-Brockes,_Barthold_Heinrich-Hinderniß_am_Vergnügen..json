{"dta.poem.5454": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Hinderni\u00df am Vergn\u00fcgen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es sehnt sich unser Geist vergn\u00fcgt und froh zu seyn:", "tokens": ["Es", "sehnt", "sich", "un\u00b7ser", "Geist", "ver\u00b7gn\u00fcgt", "und", "froh", "zu", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PPOSAT", "NN", "VVPP", "KON", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hiemit besch\u00e4ftigt sich sein reger Wunsch allein.", "tokens": ["Hie\u00b7mit", "be\u00b7sch\u00e4f\u00b7tigt", "sich", "sein", "re\u00b7ger", "Wunsch", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "ADJA", "NN", "ADV", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Weil er nun keine Lust an GOttes Wercken findet,", "tokens": ["Weil", "er", "nun", "kei\u00b7ne", "Lust", "an", "Got\u00b7tes", "Wer\u00b7cken", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN", "APPR", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Indem er selbige nicht achtet,", "tokens": ["In\u00b7dem", "er", "sel\u00b7bi\u00b7ge", "nicht", "ach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und, durch Exempel blos verf\u00fchrt, sie nicht betrachtet;", "tokens": ["Und", ",", "durch", "Ex\u00b7em\u00b7pel", "blos", "ver\u00b7f\u00fchrt", ",", "sie", "nicht", "be\u00b7trach\u00b7tet", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "NN", "ADV", "VVPP", "$,", "PPER", "PTKNEG", "VVPP", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Was Wunder da\u00df er sich mit eitler Lust verbindet,", "tokens": ["Was", "Wun\u00b7der", "da\u00df", "er", "sich", "mit", "eit\u00b7ler", "Lust", "ver\u00b7bin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KOUS", "PPER", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und in der Leidenschaft ein\u2019 Art von Lust empfindet,", "tokens": ["Und", "in", "der", "Lei\u00b7den\u00b7schaft", "ein'", "Art", "von", "Lust", "emp\u00b7fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die voller Schmertzen steckt, in welcher nimmer Ruh.", "tokens": ["Die", "vol\u00b7ler", "Schmert\u00b7zen", "steckt", ",", "in", "wel\u00b7cher", "nim\u00b7mer", "Ruh", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "APPR", "PRELS", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "K\u00f6mmt, nebst Exempeln nun, Gewohnheit auch dazu;", "tokens": ["K\u00f6mmt", ",", "nebst", "Ex\u00b7em\u00b7peln", "nun", ",", "Ge\u00b7wohn\u00b7heit", "auch", "da\u00b7zu", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "NN", "ADV", "$,", "NN", "ADV", "PAV", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "So hindert die verbotne Frucht,", "tokens": ["So", "hin\u00b7dert", "die", "ver\u00b7bot\u00b7ne", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die Ehren-Geld- und Wollust-Sucht,", "tokens": ["Die", "Eh\u00b7ren\u00b7Geld", "und", "Wol\u00b7lust\u00b7Sucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Durch immer in sich selbst sich mehrende Jdeen,", "tokens": ["Durch", "im\u00b7mer", "in", "sich", "selbst", "sich", "meh\u00b7ren\u00b7de", "Jdeen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "PRF", "ADV", "PRF", "PIAT", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.13": {"text": "Da\u00df wir nicht sehen, was wir sehen;", "tokens": ["Da\u00df", "wir", "nicht", "se\u00b7hen", ",", "was", "wir", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Da\u00df wir, zu unsers Sch\u00f6pfers Ehren,", "tokens": ["Da\u00df", "wir", ",", "zu", "un\u00b7sers", "Sch\u00f6p\u00b7fers", "Eh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Nicht riechen, f\u00fchlen, schmecken, h\u00f6ren;", "tokens": ["Nicht", "rie\u00b7chen", ",", "f\u00fch\u00b7len", ",", "schme\u00b7cken", ",", "h\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Da\u00df alles, was der Sch\u00f6pfer wirckt und schafft,", "tokens": ["Da\u00df", "al\u00b7les", ",", "was", "der", "Sch\u00f6p\u00b7fer", "wirckt", "und", "schafft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PRELS", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Uns nicht des Ansehns wehrt, uns unschmackhaft", "tokens": ["Uns", "nicht", "des", "An\u00b7sehns", "wehrt", ",", "uns", "un\u00b7schmack\u00b7haft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "PTKNEG", "ART", "NN", "VVFIN", "$,", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Und recht ver\u00e4chtlich scheint; das doch, wenn mans erweget,", "tokens": ["Und", "recht", "ver\u00b7\u00e4cht\u00b7lich", "scheint", ";", "das", "doch", ",", "wenn", "mans", "er\u00b7we\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "$.", "PDS", "ADV", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Der Anmuth wahren Kern in sch\u00f6nen Schalen heget.", "tokens": ["Der", "An\u00b7muth", "wah\u00b7ren", "Kern", "in", "sch\u00f6\u00b7nen", "Scha\u00b7len", "he\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}