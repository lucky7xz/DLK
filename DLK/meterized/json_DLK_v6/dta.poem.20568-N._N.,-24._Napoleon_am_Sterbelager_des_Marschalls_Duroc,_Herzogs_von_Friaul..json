{"dta.poem.20568": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "24.  \n  Napoleon am Sterbelager des Marschalls  \n Duroc, Herzogs von Friaul.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1848", "urn": "urn:nbn:de:kobv:b4-25129-1", "language": ["de:0.99"], "booktitle": "[N. N.]: Erinnerungen eines freiwilligen reitenden J\u00e4gers aus den Kriegsjahren 1813\u20131815. Berlin, 1848."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Duroc zeuch denn hin in jene Lande,", "tokens": ["Mein", "Du\u00b7roc", "zeuch", "denn", "hin", "in", "je\u00b7ne", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVIMP", "ADV", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Uns unbekannt, doch glaubt an sie das Herz.", "tokens": ["Uns", "un\u00b7be\u00b7kannt", ",", "doch", "glaubt", "an", "sie", "das", "Herz", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "ADV", "VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zerrei\u00dfen m\u00f6gen Dir der Erde Bande,", "tokens": ["Zer\u00b7rei\u00b7\u00dfen", "m\u00f6\u00b7gen", "Dir", "der", "Er\u00b7de", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu Ende gehn der Wunden bittrer Schmerz.", "tokens": ["Zu", "En\u00b7de", "gehn", "der", "Wun\u00b7den", "bit\u00b7trer", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Freund war\u2019s, den in Dir mir G\u00f6tter brachten,", "tokens": ["Der", "Freund", "wa\u00b7r's", ",", "den", "in", "Dir", "mir", "G\u00f6t\u00b7ter", "brach\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PRELS", "APPR", "PPER", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Mag freundlich Dich, mag schnell Dich Tod umnachten.", "tokens": ["Mag", "freund\u00b7lich", "Dich", ",", "mag", "schnell", "Dich", "Tod", "um\u00b7nach\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "PPER", "$,", "VMFIN", "ADJD", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Duroc sprach: Wollt' noch gern viel' Tage leben,", "tokens": ["Du\u00b7roc", "sprach", ":", "Wollt'", "noch", "gern", "viel'", "Ta\u00b7ge", "le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "VMFIN", "ADV", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zerfleischte mich auch tausendfache Qual,", "tokens": ["Zer\u00b7fleischte", "mich", "auch", "tau\u00b7send\u00b7fa\u00b7che", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Um dringend letzte Mahnung Dir zu geben", "tokens": ["Um", "drin\u00b7gend", "letz\u00b7te", "Mah\u00b7nung", "Dir", "zu", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ADJD", "ADJA", "NN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum Anhalt Dir im ird\u2019schen Maskensaal.", "tokens": ["Zum", "An\u00b7halt", "Dir", "im", "ird'\u00b7schen", "Mas\u00b7ken\u00b7saal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Um Dich noch, Kaiser, kreisen Truggestalten.", "tokens": ["Um", "Dich", "noch", ",", "Kai\u00b7ser", ",", "krei\u00b7sen", "Trug\u00b7ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ADV", "$,", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gott m\u00f6ge sie von jetzt an fern Dir halten!", "tokens": ["Gott", "m\u00f6\u00b7ge", "sie", "von", "jetzt", "an", "fern", "Dir", "hal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "APPR", "ADV", "APPR", "ADJD", "PPER", "VVINF", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}