{"textgrid.poem.24134": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Bilanz", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So geh ich nun, Cylinder in der Stirn,", "tokens": ["So", "geh", "ich", "nun", ",", "Cy\u00b7lin\u00b7der", "in", "der", "Stirn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Schnurrbart aufgestrichen, wie sichs ziemt,", "tokens": ["Den", "Schnurr\u00b7bart", "auf\u00b7ge\u00b7stri\u00b7chen", ",", "wie", "sichs", "ziemt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und setz mein spanisch Rohr altv\u00e4terisch,", "tokens": ["Und", "setz", "mein", "spa\u00b7nisch", "Rohr", "alt\u00b7v\u00e4\u00b7te\u00b7risch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJD", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bed\u00e4chtig, schrittbemessen vor mich bin.", "tokens": ["Be\u00b7d\u00e4ch\u00b7tig", ",", "schritt\u00b7be\u00b7mes\u00b7sen", "vor", "mich", "bin", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "APPR", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Alt wohl noch nicht, doch auch nicht mehr ganz jung,", "tokens": ["Alt", "wohl", "noch", "nicht", ",", "doch", "auch", "nicht", "mehr", "ganz", "jung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADV", "PTKNEG", "$,", "ADV", "ADV", "PTKNEG", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ist man denn in der Mitte angelangt,", "tokens": ["Ist", "man", "denn", "in", "der", "Mit\u00b7te", "an\u00b7ge\u00b7langt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Beim \u00e0 peu pr\u00e8s, und hat Gelegenheit,", "tokens": ["Beim", "\u00e0", "peu", "pr\u00e8s", ",", "und", "hat", "Ge\u00b7le\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "KON", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vorw\u00e4rts und r\u00fcckw\u00e4rts still sich umzusehn.", "tokens": ["Vor\u00b7w\u00e4rts", "und", "r\u00fcck\u00b7w\u00e4rts", "still", "sich", "um\u00b7zu\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "ADJD", "PRF", "VVIZU", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Kopfnicken. Stirnefalten. Hem und hum ...", "tokens": ["Kopf\u00b7ni\u00b7cken", ".", "Stir\u00b7ne\u00b7fal\u00b7ten", ".", "Hem", "und", "hum", "..."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Tja: Manches hat sich nicht so eingestellt,", "tokens": ["Tja", ":", "Man\u00b7ches", "hat", "sich", "nicht", "so", "ein\u00b7ge\u00b7stellt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PIS", "VAFIN", "PRF", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wies einst der \u00dfolze Gymnasiast getr\u00e4umt,", "tokens": ["Wies", "einst", "der", "\u00dfol\u00b7ze", "Gym\u00b7na\u00b7si\u00b7ast", "ge\u00b7tr\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Und dies und das ging unerquicklich aus,", "tokens": ["Und", "dies", "und", "das", "ging", "un\u00b7er\u00b7quick\u00b7lich", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "KON", "PDS", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das sich erst wundersch\u00f6n zu machen schien.", "tokens": ["Das", "sich", "erst", "wun\u00b7der\u00b7sch\u00f6n", "zu", "ma\u00b7chen", "schien", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "ADV", "ADJD", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Zieh die Bilanz, Mann im Cylinderhut!", "tokens": ["Zieh", "die", "Bi\u00b7lanz", ",", "Mann", "im", "Cy\u00b7lin\u00b7der\u00b7hut", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "NN", "APPRART", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Schlag Blatt f\u00fcr Blatt mit Rechnermiene um!", "tokens": ["Schlag", "Blatt", "f\u00fcr", "Blatt", "mit", "Rech\u00b7ner\u00b7mie\u00b7ne", "um", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sieh nach, Freund Ich, was dir noch \u00fcbrig bleibt!", "tokens": ["Sieh", "nach", ",", "Freund", "Ich", ",", "was", "dir", "noch", "\u00fcb\u00b7rig", "bleibt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "NN", "PPER", "$,", "PWS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wer st\u00f6\u00dft mich da? Um Gott!: die Adelheid!", "tokens": ["Wer", "st\u00f6\u00dft", "mich", "da", "?", "Um", "Gott", "!", ":", "die", "A\u00b7del\u00b7heid", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$.", "KOUI", "NN", "$.", "$.", "ART", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nanu, Madam, wo kommst denn du jetzt her?", "tokens": ["Na\u00b7nu", ",", "Ma\u00b7dam", ",", "wo", "kommst", "denn", "du", "jetzt", "her", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "PWAV", "VVFIN", "KON", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Tanzst du denn nicht bei den Ambassadeurs?", "tokens": ["Tanz\u00b7st", "du", "denn", "nicht", "bei", "den", "Am\u00b7bas\u00b7sa\u00b7deurs", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "\u2013 \u00bbIch tanze nicht.\u00ab Sie sagts mit D\u00fcsterheit.", "tokens": ["\u2013", "\u00bb", "Ich", "tan\u00b7ze", "nicht", ".", "\u00ab", "Sie", "sagts", "mit", "D\u00fcs\u00b7ter\u00b7heit", "."], "token_info": ["punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "PPER", "VVFIN", "PTKNEG", "$.", "$(", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "\u2013 Um Gotteswillen, Kind, was ist geschehn?", "tokens": ["\u2013", "Um", "Got\u00b7tes\u00b7wil\u00b7len", ",", "Kind", ",", "was", "ist", "ge\u00b7schehn", "?"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KOUI", "NN", "$,", "NN", "$,", "PWS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "\u2013 \u00bbIch bin ...\u00ab, sie streift den Handschuh sich zur\u00fcck", "tokens": ["\u2013", "\u00bb", "Ich", "bin", "...", "\u00ab", ",", "sie", "streift", "den", "Hand\u00b7schuh", "sich", "zu\u00b7r\u00fcck"], "token_info": ["punct", "punct", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "$(", "PPER", "VAFIN", "$(", "$(", "$,", "PPER", "VVFIN", "ART", "NN", "PRF", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und zeigt mir, herkle! einen Ehering.", "tokens": ["Und", "zeigt", "mir", ",", "her\u00b7kle", "!", "ei\u00b7nen", "E\u00b7he\u00b7ring", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "VVFIN", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Da mu\u00df ich lachen, da\u00df mein Seidenhut", "tokens": ["Da", "mu\u00df", "ich", "la\u00b7chen", ",", "da\u00df", "mein", "Sei\u00b7den\u00b7hut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dem Dampfschiffschlote gleich schwankt, der im Sturm", "tokens": ["Dem", "Dampf\u00b7schiff\u00b7schlo\u00b7te", "gleich", "schwankt", ",", "der", "im", "Sturm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Mit h\u00e4userhohen Wellen trotzig k\u00e4mpft", "tokens": ["Mit", "h\u00e4u\u00b7ser\u00b7ho\u00b7hen", "Wel\u00b7len", "trot\u00b7zig", "k\u00e4mpft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "\u2013 Du bist ... die Welt geht unter ... du ... oh Schreck!", "tokens": ["\u2013", "Du", "bist", "...", "die", "Welt", "geht", "un\u00b7ter", "...", "du", "...", "oh", "Schreck", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "$(", "ART", "NN", "VVFIN", "APPR", "$(", "PPER", "$(", "FM", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dagegen ich! Sie her! \u2013 Und frei und nackt", "tokens": ["Da\u00b7ge\u00b7gen", "ich", "!", "Sie", "her", "!", "\u2013", "Und", "frei", "und", "nackt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "$.", "PPER", "PTKVZ", "$.", "$(", "KON", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Zeigt meine Hand sich ohne goldnen Reif.", "tokens": ["Zeigt", "mei\u00b7ne", "Hand", "sich", "oh\u00b7ne", "gold\u00b7nen", "Reif", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Nun geht ein Fragen nieder auf mein Haupt,", "tokens": ["Nun", "geht", "ein", "Fra\u00b7gen", "nie\u00b7der", "auf", "mein", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df neben ihm ein Donnerwolkenbruch", "tokens": ["Da\u00df", "ne\u00b7ben", "ihm", "ein", "Don\u00b7ner\u00b7wol\u00b7ken\u00b7bruch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein M\u00fcckenniesen ist. Ich halte still", "tokens": ["Ein", "M\u00fc\u00b7cken\u00b7nie\u00b7sen", "ist", ".", "Ich", "hal\u00b7te", "still"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$.", "PPER", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und sage nichts und werde v\u00f6llig stumm,", "tokens": ["Und", "sa\u00b7ge", "nichts", "und", "wer\u00b7de", "v\u00f6l\u00b7lig", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "KON", "VAFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Bin lang schon nicht mehr da \u2013 wo bin ich nur? \u2013", "tokens": ["Bin", "lang", "schon", "nicht", "mehr", "da", "\u2013", "wo", "bin", "ich", "nur", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "PTKNEG", "ADV", "ADV", "$(", "PWAV", "VAFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und schlie\u00dflich dr\u00fcck ich mich von Adelheid.", "tokens": ["Und", "schlie\u00df\u00b7lich", "dr\u00fcck", "ich", "mich", "von", "A\u00b7del\u00b7heid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PRF", "APPR", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Wo war ich doch ...? Ja so, ja: die Bilanz.", "tokens": ["Wo", "war", "ich", "doch", "...", "?", "Ja", "so", ",", "ja", ":", "die", "Bi\u00b7lanz", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "$(", "$.", "PTKANT", "ADV", "$,", "ADV", "$.", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nun gut, was bleibt? Bin ich vielleicht bankrott?", "tokens": ["Nun", "gut", ",", "was", "bleibt", "?", "Bin", "ich", "viel\u00b7leicht", "bank\u00b7rott", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWS", "VVFIN", "$.", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bin ich solvent, wenn mich das Schicksal mahnt", "tokens": ["Bin", "ich", "sol\u00b7vent", ",", "wenn", "mich", "das", "Schick\u00b7sal", "mahnt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und zu mir spricht, der grimme Gl\u00e4ubiger:", "tokens": ["Und", "zu", "mir", "spricht", ",", "der", "grim\u00b7me", "Gl\u00e4u\u00b7bi\u00b7ger", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Her mit dem Darlehn, mit den Zinsen her!", "tokens": ["Her", "mit", "dem", "Dar\u00b7lehn", ",", "mit", "den", "Zin\u00b7sen", "her", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Ist denn nicht alles, alles l\u00e4ngst verthan?", "tokens": ["Ist", "denn", "nicht", "al\u00b7les", ",", "al\u00b7les", "l\u00e4ngst", "ver\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "PIS", "$,", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Kam irgend nur ein kleiner Posten ein?", "tokens": ["Kam", "ir\u00b7gend", "nur", "ein", "klei\u00b7ner", "Pos\u00b7ten", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Mir scheint, mir scheint, Freund Ich, das Ding steht schlimm.", "tokens": ["Mir", "scheint", ",", "mir", "scheint", ",", "Freund", "Ich", ",", "das", "Ding", "steht", "schlimm", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "NN", "PPER", "$,", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bedeppt, beklommen, ein ertappter Dieb,", "tokens": ["Be\u00b7deppt", ",", "be\u00b7klom\u00b7men", ",", "ein", "er\u00b7tapp\u00b7ter", "Dieb", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Steh ich gesenkten Hauptes, schuldbewu\u00dft,", "tokens": ["Steh", "ich"