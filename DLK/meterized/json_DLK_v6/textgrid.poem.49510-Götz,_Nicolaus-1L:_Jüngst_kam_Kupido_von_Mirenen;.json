{"textgrid.poem.49510": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: J\u00fcngst kam Kupido von Mirenen;", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst kam Kupido von Mirenen;", "tokens": ["J\u00fcngst", "kam", "Ku\u00b7pi\u00b7do", "von", "Mi\u00b7re\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Und Venus spricht:", "tokens": ["Und", "Ve\u00b7nus", "spricht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wie fandstu sie? \u2013 \u2013 Er sprach mit Thr\u00e4nen,", "tokens": ["Wie", "fands\u00b7tu", "sie", "?", "\u2013", "\u2013", "Er", "sprach", "mit", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$.", "$(", "$(", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So liebreich nicht!", "tokens": ["So", "lieb\u00b7reich", "nicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "O Mutter, habe doch Erbarmen,", "tokens": ["O", "Mut\u00b7ter", ",", "ha\u00b7be", "doch", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und strafe sie!", "tokens": ["Und", "stra\u00b7fe", "sie", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Sie legt mich andern in die Armen,", "tokens": ["Sie", "legt", "mich", "an\u00b7dern", "in", "die", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und nimmt mich nie!", "tokens": ["Und", "nimmt", "mich", "nie", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}