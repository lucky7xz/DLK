{"textgrid.poem.35365": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nochmals den gro\u00dfen Wassern meinen Gru\u00df,", "tokens": ["Noch\u00b7mals", "den", "gro\u00b7\u00dfen", "Was\u00b7sern", "mei\u00b7nen", "Gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dem Ocean und all den wackern Leuten,", "tokens": ["Dem", "O\u00b7cean", "und", "all", "den", "wa\u00b7ckern", "Leu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Die nur durch ihn ihr armes Brot erbeuten! \u2013", "tokens": ["Die", "nur", "durch", "ihn", "ihr", "ar\u00b7mes", "Brot", "er\u00b7beu\u00b7ten", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf eines Schiffes Planken tritt mein Fu\u00df", "tokens": ["Auf", "ei\u00b7nes", "Schif\u00b7fes", "Plan\u00b7ken", "tritt", "mein", "Fu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nicht ohne Sch\u00fcchternheit; und sch\u00fctteln darf", "tokens": ["Nicht", "oh\u00b7ne", "Sch\u00fcch\u00b7tern\u00b7heit", ";", "und", "sch\u00fct\u00b7teln", "darf"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "NN", "$.", "KON", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Des Seemanns Schwielenhand nicht ohne Scham", "tokens": ["Des", "See\u00b7manns", "Schwie\u00b7len\u00b7hand", "nicht", "oh\u00b7ne", "Scham"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wer, so wie ich, den tr\u00e4umerischen Gram,", "tokens": ["Wer", ",", "so", "wie", "ich", ",", "den", "tr\u00e4u\u00b7me\u00b7ri\u00b7schen", "Gram", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "ADV", "KOKOM", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Das faule Frachtgut nicht zur Seite warf.", "tokens": ["Das", "fau\u00b7le", "Fracht\u00b7gut", "nicht", "zur", "Sei\u00b7te", "warf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Mir war es nicht verg\u00f6nnt, von alten Schlacken", "tokens": ["Mir", "war", "es", "nicht", "ver\u00b7g\u00f6nnt", ",", "von", "al\u00b7ten", "Schla\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Mich zu befrei'n und von ererbtem Staube;", "tokens": ["Mich", "zu", "be\u00b7frei'n", "und", "von", "er\u00b7erb\u00b7tem", "Stau\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "KON", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Ein gutes Beispiel aber fand mein Glaube", "tokens": ["Ein", "gu\u00b7tes", "Bei\u00b7spiel", "a\u00b7ber", "fand", "mein", "Glau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Stets auf der See, bei den beteerten Jacken.", "tokens": ["Stets", "auf", "der", "See", ",", "bei", "den", "be\u00b7teer\u00b7ten", "Ja\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und an dem Anspruchslosen ganz besonders,", "tokens": ["Und", "an", "dem", "An\u00b7spruchs\u00b7lo\u00b7sen", "ganz", "be\u00b7son\u00b7ders", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Der in der gro\u00dfen Menge sich verliert,", "tokens": ["Der", "in", "der", "gro\u00b7\u00dfen", "Men\u00b7ge", "sich", "ver\u00b7liert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Und dessen Aermel keine Schnur verziert,", "tokens": ["Und", "des\u00b7sen", "A\u00b7er\u00b7mel", "kei\u00b7ne", "Schnur", "ver\u00b7ziert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Erlabte sich der Blick des Hypochonders.", "tokens": ["Er\u00b7lab\u00b7te", "sich", "der", "Blick", "des", "Hy\u00b7po\u00b7chon\u00b7ders", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Hei, seht die Handelsschiffe, schwach bemannt,", "tokens": ["Hei", ",", "seht", "die", "Han\u00b7dels\u00b7schif\u00b7fe", ",", "schwach", "be\u00b7mannt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ART", "NN", "$,", "ADJD", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.18": {"text": "Wie sie nach allen Winden sich zerstreu'n!", "tokens": ["Wie", "sie", "nach", "al\u00b7len", "Win\u00b7den", "sich", "zer\u00b7streu'n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PIAT", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Wie viele dort an Bord? \u2013 Acht oder neun,", "tokens": ["Wie", "vie\u00b7le", "dort", "an", "Bord", "?", "\u2013", "Acht", "o\u00b7der", "neun", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "APPR", "NN", "$.", "$(", "CARD", "KON", "CARD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Und dennoch alle Segel aufgespannt.", "tokens": ["Und", "den\u00b7noch", "al\u00b7le", "Se\u00b7gel", "auf\u00b7ge\u00b7spannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Drei Masten tr\u00e4gt das Fahrzeug? \u2013 Gute Reise!", "tokens": ["Drei", "Mas\u00b7ten", "tr\u00e4gt", "das", "Fahr\u00b7zeug", "?", "\u2013", "Gu\u00b7te", "Rei\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ART", "NN", "$.", "$(", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Zieht durch die Meere hin als Friedensboten", "tokens": ["Zieht", "durch", "die", "Mee\u00b7re", "hin", "als", "Frie\u00b7dens\u00b7bo\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "KOUS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Und kehrt zur\u00fcck als gl\u00fcckliche Piloten,", "tokens": ["Und", "kehrt", "zu\u00b7r\u00fcck", "als", "gl\u00fcck\u00b7li\u00b7che", "Pi\u00b7lo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Heim aus Ostindien, heim aus Gr\u00f6nlands Eise!", "tokens": ["Heim", "aus", "Ost\u00b7in\u00b7di\u00b7en", ",", "heim", "aus", "Gr\u00f6n\u00b7lands", "Ei\u00b7se", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$,", "PTKVZ", "APPR", "NE", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.25": {"text": "Mich selber hat die Trope wie entbl\u00e4ttert,", "tokens": ["Mich", "sel\u00b7ber", "hat", "die", "Tro\u00b7pe", "wie", "ent\u00b7bl\u00e4t\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "NN", "KOKOM", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Doch m\u00f6cht' ich einmal noch die Zweige recken", "tokens": ["Doch", "m\u00f6cht'", "ich", "ein\u00b7mal", "noch", "die", "Zwei\u00b7ge", "re\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Dort, wo auf unbegrenzte L\u00e4nderstrecken", "tokens": ["Dort", ",", "wo", "auf", "un\u00b7be\u00b7grenz\u00b7te", "L\u00e4n\u00b7der\u00b7stre\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Die Sonne ihre Strahlen niederschmettert;", "tokens": ["Die", "Son\u00b7ne", "ih\u00b7re", "Strah\u00b7len", "nie\u00b7der\u00b7schmet\u00b7tert", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Dort, wo des Himmels Blau mit Purpurfarben", "tokens": ["Dort", ",", "wo", "des", "Him\u00b7mels", "Blau", "mit", "Pur\u00b7purf\u00b7ar\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Ges\u00e4ttigt auf die Palmen niederlacht,", "tokens": ["Ge\u00b7s\u00e4t\u00b7tigt", "auf", "die", "Pal\u00b7men", "nie\u00b7der\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Wo matte Wellen einst in lichter Nacht", "tokens": ["Wo", "mat\u00b7te", "Wel\u00b7len", "einst", "in", "lich\u00b7ter", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Zu meinen, des Verbannten, F\u00fc\u00dfen starben,", "tokens": ["Zu", "mei\u00b7nen", ",", "des", "Ver\u00b7bann\u00b7ten", ",", "F\u00fc\u00b7\u00dfen", "star\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "$,", "ART", "NN", "$,", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "In goldnen Sand verrinnend; an Gestaden,", "tokens": ["In", "gold\u00b7nen", "Sand", "ver\u00b7rin\u00b7nend", ";", "an", "Ge\u00b7sta\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$.", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Wo ich getr\u00e4umt, gelitten und \u2013 gezaudert,", "tokens": ["Wo", "ich", "ge\u00b7tr\u00e4umt", ",", "ge\u00b7lit\u00b7ten", "und", "\u2013", "ge\u00b7zau\u00b7dert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "$,", "VVPP", "KON", "$(", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Und wo mir holde, schmeichelnde Najaden", "tokens": ["Und", "wo", "mir", "hol\u00b7de", ",", "schmei\u00b7cheln\u00b7de", "Na\u00b7ja\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Von alter Liebe Herrlichkeit geplaudert;", "tokens": ["Von", "al\u00b7ter", "Lie\u00b7be", "Herr\u00b7lich\u00b7keit", "ge\u00b7plau\u00b7dert", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Wo die Natur in nimmerm\u00fcdem Schaffen", "tokens": ["Wo", "die", "Na\u00b7tur", "in", "nim\u00b7mer\u00b7m\u00fc\u00b7dem", "Schaf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Auf Wunder immer neue Wunder baut. \u2013", "tokens": ["Auf", "Wun\u00b7der", "im\u00b7mer", "neu\u00b7e", "Wun\u00b7der", "baut", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "ADV", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Dahin ist meine Zeit! \u2013 O Schmerzenslaut!", "tokens": ["Da\u00b7hin", "ist", "mei\u00b7ne", "Zeit", "!", "\u2013", "O", "Schmer\u00b7zens\u00b7laut", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPOSAT", "NN", "$.", "$(", "NE", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Es ist zu sp\u00e4t, mich wieder aufzuraffen;", "tokens": ["Es", "ist", "zu", "sp\u00e4t", ",", "mich", "wie\u00b7der", "auf\u00b7zu\u00b7raf\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "$,", "PPER", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Nach kurzen Tagen ist es Nacht geworden;", "tokens": ["Nach", "kur\u00b7zen", "Ta\u00b7gen", "ist", "es", "Nacht", "ge\u00b7wor\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPER", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Mir sind besonnte Pfade abgeschnitten.", "tokens": ["Mir", "sind", "be\u00b7sonn\u00b7te", "Pfa\u00b7de", "ab\u00b7ge\u00b7schnit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Ich bin gekommen, um im rauhen Norden,", "tokens": ["Ich", "bin", "ge\u00b7kom\u00b7men", ",", "um", "im", "rau\u00b7hen", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "KOUI", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "Im Schneegest\u00f6ber um ein Grab zu bitten.", "tokens": ["Im", "Schnee\u00b7ge\u00b7st\u00f6\u00b7ber", "um", "ein", "Grab", "zu", "bit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "Und wenn ich noch am stillen Abendfeuer", "tokens": ["Und", "wenn", "ich", "noch", "am", "stil\u00b7len", "A\u00b7bend\u00b7feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.46": {"text": "Mich berge vor dem heimatlichen Schnee,", "tokens": ["Mich", "ber\u00b7ge", "vor", "dem", "hei\u00b7mat\u00b7li\u00b7chen", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.47": {"text": "Gedenk ich deiner, sternbes\u00e4te See,", "tokens": ["Ge\u00b7denk", "ich", "dei\u00b7ner", ",", "stern\u00b7be\u00b7s\u00e4\u00b7te", "See", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.48": {"text": "Ihr k\u00fchnen Schiffer, dann gedenk ich euer!", "tokens": ["Ihr", "k\u00fch\u00b7nen", "Schif\u00b7fer", ",", "dann", "ge\u00b7denk", "ich", "eu\u00b7er", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.49": {"text": "Vergessenheit ist unser Los; doch besser", "tokens": ["Ver\u00b7ges\u00b7sen\u00b7heit", "ist", "un\u00b7ser", "Los", ";", "doch", "bes\u00b7ser"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$.", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.50": {"text": "Das Herbe mit Gehorsam und Geduld", "tokens": ["Das", "Her\u00b7be", "mit", "Ge\u00b7hor\u00b7sam", "und", "Ge\u00b7duld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.51": {"text": "Bezwingen, als der Undankbaren Schuld", "tokens": ["Be\u00b7zwin\u00b7gen", ",", "als", "der", "Un\u00b7dank\u00b7ba\u00b7ren", "Schuld"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.52": {"text": "Blo\u00dflegen, schonungslos, mit spitzem Messer.", "tokens": ["Blo\u00df\u00b7le\u00b7gen", ",", "scho\u00b7nungs\u00b7los", ",", "mit", "spit\u00b7zem", "Mes\u00b7ser", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.53": {"text": "Vorw\u00e4rts gesteuert durch des Lebens Brandung", "tokens": ["Vor\u00b7w\u00e4rts", "ge\u00b7steu\u00b7ert", "durch", "des", "Le\u00b7bens", "Bran\u00b7dung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "APPR", "ART", "NN", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.54": {"text": "Als tapf'rer Lotse, nicht als Menschenhasser \u2013", "tokens": ["Als", "tapf'\u00b7rer", "Lot\u00b7se", ",