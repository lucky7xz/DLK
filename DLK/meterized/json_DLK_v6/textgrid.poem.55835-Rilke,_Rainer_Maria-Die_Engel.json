{"textgrid.poem.55835": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Die Engel", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie haben alle m\u00fcde M\u00fcnde", "tokens": ["Sie", "ha\u00b7ben", "al\u00b7le", "m\u00fc\u00b7de", "M\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "und helle Seelen ohne Saum.", "tokens": ["und", "hel\u00b7le", "See\u00b7len", "oh\u00b7ne", "Saum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und eine Sehnsucht (wie nach S\u00fcnde)", "tokens": ["Und", "ei\u00b7ne", "Sehn\u00b7sucht", "(", "wie", "nach", "S\u00fcn\u00b7de", ")"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$(", "KOKOM", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "geht ihnen manchmal durch den Traum.", "tokens": ["geht", "ih\u00b7nen", "manch\u00b7mal", "durch", "den", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Fast gleichen sie einander alle;", "tokens": ["Fast", "glei\u00b7chen", "sie", "ein\u00b7an\u00b7der", "al\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PIS", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "in Gottes G\u00e4rten schweigen sie,", "tokens": ["in", "Got\u00b7tes", "G\u00e4r\u00b7ten", "schwei\u00b7gen", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "wie viele, viele Intervalle", "tokens": ["wie", "vie\u00b7le", ",", "vie\u00b7le", "In\u00b7ter\u00b7val\u00b7le"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PWAV", "PIS", "$,", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "in seiner Macht und Melodie.", "tokens": ["in", "sei\u00b7ner", "Macht", "und", "Me\u00b7lo\u00b7die", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nur wenn sie ihre Fl\u00fcgel breiten,", "tokens": ["Nur", "wenn", "sie", "ih\u00b7re", "Fl\u00fc\u00b7gel", "brei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PPOSAT", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "sind sie die Wecker eines Winds:", "tokens": ["sind", "sie", "die", "We\u00b7cker", "ei\u00b7nes", "Winds", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "als ginge Gott mit seinen weiten", "tokens": ["als", "gin\u00b7ge", "Gott", "mit", "sei\u00b7nen", "wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "NN", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bildhauerh\u00e4nden durch die Seiten", "tokens": ["Bild\u00b7hau\u00b7er\u00b7h\u00e4n\u00b7den", "durch", "die", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "im dunklen Buch des Anbeginns.", "tokens": ["im", "dunk\u00b7len", "Buch", "des", "An\u00b7be\u00b7ginns", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}