{"dta.poem.2282": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An Doris", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905198996", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nt ich Holz, wie Menschen schnitzen,", "tokens": ["K\u00f6nt", "ich", "Holz", ",", "wie", "Men\u00b7schen", "schnit\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "PWAV", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lauter Nimfen wolt ich schnitzen;", "tokens": ["Lau\u00b7ter", "Nim\u00b7fen", "wolt", "ich", "schnit\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6nt ich Marmorseulen hauen,", "tokens": ["K\u00f6nt", "ich", "Mar\u00b7mor\u00b7seu\u00b7len", "hau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lauter Nimfen wolt ich hauen;", "tokens": ["Lau\u00b7ter", "Nim\u00b7fen", "wolt", "ich", "hau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00f6nt ich nur Tapeten wirken,", "tokens": ["K\u00f6nt", "ich", "nur", "Ta\u00b7pe\u00b7ten", "wir\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "O! so wirkt ich lauter Nimfen;", "tokens": ["O", "!", "so", "wirkt", "ich", "lau\u00b7ter", "Nim\u00b7fen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Lauter z\u00e4rtliche Blondinen,", "tokens": ["Lau\u00b7ter", "z\u00e4rt\u00b7li\u00b7che", "Blon\u00b7di\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lauter willige Brunetten,", "tokens": ["Lau\u00b7ter", "wil\u00b7li\u00b7ge", "Bru\u00b7net\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und die zukkers\u00fcsse Sch\u00f6ne,", "tokens": ["Und", "die", "zuk\u00b7ker\u00b7s\u00fcs\u00b7se", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Die mich itzt so z\u00e4rtlich k\u00fc\u00dfte,", "tokens": ["Die", "mich", "itzt", "so", "z\u00e4rt\u00b7lich", "k\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Solte mir zum Muster dienen.", "tokens": ["Sol\u00b7te", "mir", "zum", "Mus\u00b7ter", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}