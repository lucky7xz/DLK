{"textgrid.poem.57264": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie erhebt sich das Herz, wenn es dich,", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie erhebt sich das Herz, wenn es dich,", "tokens": ["Wie", "er\u00b7hebt", "sich", "das", "Herz", ",", "wenn", "es", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "ART", "NN", "$,", "KOUS", "PPER", "PRF", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Unendlicher, denkt! wie sinkt es,", "tokens": ["Un\u00b7end\u00b7li\u00b7cher", ",", "denkt", "!", "wie", "sinkt", "es", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "$.", "PWAV", "VVFIN", "PPER", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wenns auf sich herunterschaut!", "tokens": ["Wenns", "auf", "sich", "her\u00b7un\u00b7ter\u00b7schaut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Elend schauts wehklagend dann, und Nacht und Tod!", "tokens": ["E\u00b7lend", "schauts", "weh\u00b7kla\u00b7gend", "dann", ",", "und", "Nacht", "und", "Tod", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "ADV", "$,", "KON", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Allein du rufst mich aus meiner Nacht, der im Elend, der im Tod hilft!", "tokens": ["Al\u00b7lein", "du", "rufst", "mich", "aus", "mei\u00b7ner", "Nacht", ",", "der", "im", "E\u00b7lend", ",", "der", "im", "Tod", "hilft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,", "PRELS", "APPRART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+--+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Dann denk ich es ganz, dass du ewig mich schufst,", "tokens": ["Dann", "denk", "ich", "es", "ganz", ",", "dass", "du", "e\u00b7wig", "mich", "schufst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "$,", "KOUS", "PPER", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Herlicher! den kein Preis, unten am Grab', oben am Thron,", "tokens": ["Her\u00b7li\u00b7cher", "!", "den", "kein", "Preis", ",", "un\u00b7ten", "am", "Grab'", ",", "o\u00b7ben", "am", "Thron", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "PIAT", "NN", "$,", "ADV", "APPRART", "NN", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "+---+-+--++--+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Herr Herr Gott! den dankend entflamt, kein Jubel genug besingt.", "tokens": ["Herr", "Herr", "Gott", "!", "den", "dan\u00b7kend", "ent\u00b7flamt", ",", "kein", "Ju\u00b7bel", "ge\u00b7nug", "be\u00b7singt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "$.", "ART", "ADJD", "VVPP", "$,", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+--+-+--+-+", "measure": "trochaic.septa.relaxed"}}, "stanza.3": {"line.1": {"text": "Weht, B\u00e4ume des Lebens, ins Harfenget\u00f6n!", "tokens": ["Weht", ",", "B\u00e4u\u00b7me", "des", "Le\u00b7bens", ",", "ins", "Har\u00b7fen\u00b7ge\u00b7t\u00f6n", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "ART", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Rausche mit ihnen ins Harfenget\u00f6n, krystallner Strom!", "tokens": ["Rau\u00b7sche", "mit", "ih\u00b7nen", "ins", "Har\u00b7fen\u00b7ge\u00b7t\u00f6n", ",", "krys\u00b7tall\u00b7ner", "Strom", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "APPRART", "NN", "$,", "ADJA", "NN", "$."], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Ihr lispelt, und rauscht, und, Harfen, ihr t\u00f6nt", "tokens": ["Ihr", "lis\u00b7pelt", ",", "und", "rauscht", ",", "und", ",", "Har\u00b7fen", ",", "ihr", "t\u00f6nt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "$,", "KON", "$,", "NN", "$,", "PPER", "VVFIN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nie es ganz! Gott ist es, den ihr preist!", "tokens": ["Nie", "es", "ganz", "!", "Gott", "ist", "es", ",", "den", "ihr", "preist", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "$.", "NN", "VAFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Donnert, Welten, in feyerlichem Gang, in der Posaunen Chor!", "tokens": ["Don\u00b7nert", ",", "Wel\u00b7ten", ",", "in", "fey\u00b7er\u00b7li\u00b7chem", "Gang", ",", "in", "der", "Po\u00b7sau\u00b7nen", "Chor", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "APPR", "ADJA", "NN", "$,", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Du Orion, Wage, du auch!", "tokens": ["Du", "O\u00b7rion", ",", "Wa\u00b7ge", ",", "du", "auch", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "NN", "$,", "PPER", "ADV", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "T\u00f6nt all' ihr Sonnen auf der Strasse voll Glanz,", "tokens": ["T\u00f6nt", "all'", "ihr", "Son\u00b7nen", "auf", "der", "Stras\u00b7se", "voll", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPOSAT", "NN", "APPR", "ART", "NN", "ADJD", "NN", "$,"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.4": {"text": "In der Posaunen Chor!", "tokens": ["In", "der", "Po\u00b7sau\u00b7nen", "Chor", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ihr Welten, donnert", "tokens": ["Ihr", "Wel\u00b7ten", ",", "don\u00b7nert"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Und du, der Posaunen Chor, hallest", "tokens": ["Und", "du", ",", "der", "Po\u00b7sau\u00b7nen", "Chor", ",", "hal\u00b7lest"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "$,", "ART", "NN", "NN", "$,", "VVFIN"], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nie es ganz, Gott; nie es ganz, Gott,", "tokens": ["Nie", "es", "ganz", ",", "Gott", ";", "nie", "es", "ganz", ",", "Gott", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "$,", "NN", "$.", "ADV", "PPER", "ADV", "$,", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Gott, Gott ist es, den ihr preist!", "tokens": ["Gott", ",", "Gott", "ist", "es", ",", "den", "ihr", "preist", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VAFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}