{"dta.poem.10037": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das beste Theil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als Gottlieb j\u00fcngst zween Freunden, auf dem Garten,", "tokens": ["Als", "Gott\u00b7lieb", "j\u00fcngst", "zween", "Freun\u00b7den", ",", "auf", "dem", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "VVFIN", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zwo Hyacinthen, voll gef\u00fcllter Glocken, gab;", "tokens": ["Zwo", "Hya\u00b7cin\u00b7then", ",", "voll", "ge\u00b7f\u00fcll\u00b7ter", "Glo\u00b7cken", ",", "gab", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["CARD", "NN", "$,", "ADJD", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Nahm er gar bald an ihrer Handlung ab,", "tokens": ["Nahm", "er", "gar", "bald", "an", "ih\u00b7rer", "Hand\u00b7lung", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-----+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Wie ihr Gem\u00fcth und Sinn so unterschiedner Arten.", "tokens": ["Wie", "ihr", "Ge\u00b7m\u00fcth", "und", "Sinn", "so", "un\u00b7ter\u00b7schied\u00b7ner", "Ar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "KON", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Aurander nahme sie mit tr\u00e4gen H\u00e4nden hin,", "tokens": ["Au\u00b7ran\u00b7der", "nah\u00b7me", "sie", "mit", "tr\u00e4\u00b7gen", "H\u00e4n\u00b7den", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Und steckte sie, nicht lang\u2019 hernach,", "tokens": ["Und", "steck\u00b7te", "sie", ",", "nicht", "lang'", "her\u00b7nach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PTKNEG", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Indem er von vermehrten Renten sprach,", "tokens": ["In\u00b7dem", "er", "von", "ver\u00b7mehr\u00b7ten", "Ren\u00b7ten", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zerdr\u00fccket und geknickt in seine Tasche. Jener", "tokens": ["Zer\u00b7dr\u00fc\u00b7cket", "und", "ge\u00b7knickt", "in", "sei\u00b7ne", "Ta\u00b7sche", ".", "Je\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "KON", "VVPP", "APPR", "PPOSAT", "NN", "$.", "PDAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auf seine Bluhm\u2019, und rieff: Ach! kann wol etwas sch\u00f6ner,", "tokens": ["Auf", "sei\u00b7ne", "Bluhm'", ",", "und", "rieff", ":", "Ach", "!", "kann", "wol", "et\u00b7was", "sch\u00f6\u00b7ner", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "VVFIN", "$.", "ITJ", "$.", "VMFIN", "ADV", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als diese Bluhme, seyn!", "tokens": ["Als", "die\u00b7se", "Bluh\u00b7me", ",", "seyn", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "$,", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Welch einen reinen Silber-Schein,", "tokens": ["Welch", "ei\u00b7nen", "rei\u00b7nen", "Sil\u00b7ber\u00b7Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Welch eine liebliche Figur", "tokens": ["Welch", "ei\u00b7ne", "lieb\u00b7li\u00b7che", "Fi\u00b7gur"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Hat, von den Fingern der Natur,", "tokens": ["Hat", ",", "von", "den", "Fin\u00b7gern", "der", "Na\u00b7tur", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Die\u00df holde Kind des Fr\u00fchlings \u00fcberkommen!", "tokens": ["Die\u00df", "hol\u00b7de", "Kind", "des", "Fr\u00fch\u00b7lings", "\u00fc\u00b7ber\u00b7kom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie klar ist dieses Wei\u00df! wie gl\u00e4utzend! auch die Haut", "tokens": ["Wie", "klar", "ist", "die\u00b7ses", "Wei\u00df", "!", "wie", "gl\u00e4ut\u00b7zend", "!", "auch", "die", "Haut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "PDAT", "NN", "$.", "PWAV", "ADJD", "$.", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der sch\u00f6nsten Sch\u00f6nen auf der Welt", "tokens": ["Der", "sch\u00f6ns\u00b7ten", "Sch\u00f6\u00b7nen", "auf", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verlieret, wenn man sie bey diesem Schimmer h\u00e4lt,", "tokens": ["Ver\u00b7lie\u00b7ret", ",", "wenn", "man", "sie", "bey", "die\u00b7sem", "Schim\u00b7mer", "h\u00e4lt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PIS", "PPER", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und eine bey der andern schaut,", "tokens": ["Und", "ei\u00b7ne", "bey", "der", "an\u00b7dern", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Preis, und scheint beschmutzt. Die r\u00f6thlich weisse", "tokens": ["Den", "Preis", ",", "und", "scheint", "be\u00b7schmutzt", ".", "Die", "r\u00f6th\u00b7lich", "weis\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "VVPP", "$.", "ART", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die, in dem innersten, der Bluhme kleine H\u00f6le", "tokens": ["Die", ",", "in", "dem", "in\u00b7ners\u00b7ten", ",", "der", "Bluh\u00b7me", "klei\u00b7ne", "H\u00f6\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$,", "APPR", "ART", "ADJA", "$,", "ART", "NN", "ADJA", "NN"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Mit s\u00fcsser R\u00f6the f\u00fcllt, f\u00fcllt meine frohe Seele,", "tokens": ["Mit", "s\u00fcs\u00b7ser", "R\u00f6\u00b7the", "f\u00fcllt", ",", "f\u00fcllt", "mei\u00b7ne", "fro\u00b7he", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mit ihrem holden Reitz. Ich f\u00fchle zarte Flammen", "tokens": ["Mit", "ih\u00b7rem", "hol\u00b7den", "Reitz", ".", "Ich", "f\u00fch\u00b7le", "zar\u00b7te", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Aus dieser r\u00f6thlichen und weissen Klarheit stammen.", "tokens": ["Aus", "die\u00b7ser", "r\u00f6th\u00b7li\u00b7chen", "und", "weis\u00b7sen", "Klar\u00b7heit", "stam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Es scheint der Bluhmen reiner Schein", "tokens": ["Es", "scheint", "der", "Bluh\u00b7men", "rei\u00b7ner", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ein Sinn-Bild einer reinen Seele,", "tokens": ["Ein", "Sinn\u00b7Bild", "ei\u00b7ner", "rei\u00b7nen", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Worin ein Feur der Liebe gl\u00fcht, zu seyn.", "tokens": ["Wo\u00b7rin", "ein", "Feur", "der", "Lie\u00b7be", "gl\u00fcht", ",", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Ich w\u00fcnsche wenigstens, da\u00df meine Seele, rein", "tokens": ["Ich", "w\u00fcn\u00b7sche", "we\u00b7nigs\u00b7tens", ",", "da\u00df", "mei\u00b7ne", "See\u00b7le", ",", "rein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPOSAT", "NN", "$,", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Von Lastern, durch die Triebe", "tokens": ["Von", "Las\u00b7tern", ",", "durch", "die", "Trie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Von der aus Danckbarkeit in mir entstandnen Liebe,", "tokens": ["Von", "der", "aus", "Dan\u00b7ck\u00b7bar\u00b7keit", "in", "mir", "ent\u00b7stand\u00b7nen", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "NN", "APPR", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Zu Dem, der Seiner Wercke Pracht", "tokens": ["Zu", "Dem", ",", "der", "Sei\u00b7ner", "Wer\u00b7cke", "Pracht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "$,", "PRELS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "So wunderbar hervor gebracht,", "tokens": ["So", "wun\u00b7der\u00b7bar", "her\u00b7vor", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "In solcher s\u00fcssen Gluht entbrenne;", "tokens": ["In", "sol\u00b7cher", "s\u00fcs\u00b7sen", "Gluht", "ent\u00b7bren\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Und da\u00df mein N\u00e4chster sich an mir,", "tokens": ["Und", "da\u00df", "mein", "N\u00e4chs\u00b7ter", "sich", "an", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "PRF", "APPR", "PPER", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "Wie ich, geliebte Bluhm\u2019, an dir", "tokens": ["Wie", "ich", ",", "ge\u00b7lieb\u00b7te", "Bluhm'", ",", "an", "dir"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PPER", "$,", "ADJA", "NN", "$,", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Mich freue, stets erfreuen k\u00f6nne.", "tokens": ["Mich", "freu\u00b7e", ",", "stets", "er\u00b7freu\u00b7en", "k\u00f6n\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Ja, da die Bluhmen wunderbar", "tokens": ["Ja", ",", "da", "die", "Bluh\u00b7men", "wun\u00b7der\u00b7bar"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Bem\u00fcht sind, ihre Krafft so lieblich auszuhauchen,", "tokens": ["Be\u00b7m\u00fcht", "sind", ",", "ih\u00b7re", "Krafft", "so", "lieb\u00b7lich", "aus\u00b7zu\u00b7hau\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "PPOSAT", "NN", "ADV", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So w\u00fcnsch ich, da\u00df mein Hertz, als wie ein Danck-Altar,", "tokens": ["So", "w\u00fcnsch", "ich", ",", "da\u00df", "mein", "Hertz", ",", "als", "wie", "ein", "Dan\u00b7ck\u00b7Al\u00b7tar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "$,", "KOUS", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.25": {"text": "O Sch\u00f6pfer, Dir zum Ruhm, m\u00f6g\u2019 unaufh\u00f6rlich rauchen!", "tokens": ["O", "Sch\u00f6p\u00b7fer", ",", "Dir", "zum", "Ruhm", ",", "m\u00f6g'", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "rau\u00b7chen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "APPRART", "NN", "$,", "VMFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Was meinst du nun von diesen beiden,", "tokens": ["Was", "meinst", "du", "nun", "von", "die\u00b7sen", "bei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "PDAT", "PIAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geliebter Leser, wer hat GOttes Creatur,", "tokens": ["Ge\u00b7lieb\u00b7ter", "Le\u00b7ser", ",", "wer", "hat", "Got\u00b7tes", "Crea\u00b7tur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWS", "VAFIN", "NN", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Wie sichs geb\u00fchrt, gebraucht? und wessen Spur", "tokens": ["Wie", "sichs", "ge\u00b7b\u00fchrt", ",", "ge\u00b7braucht", "?", "und", "wes\u00b7sen", "Spur"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PIS", "VVPP", "$,", "VVPP", "$.", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "H\u00e4ltst du dich pflichtig zu vermeiden?", "tokens": ["H\u00e4ltst", "du", "dich", "pflich\u00b7tig", "zu", "ver\u00b7mei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hat an dem ersten selbst der Sch\u00f6pffer nicht", "tokens": ["Hat", "an", "dem", "ers\u00b7ten", "selbst", "der", "Sch\u00f6pf\u00b7fer", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "ADV", "ART", "NN", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der Sch\u00f6pfung End-Zwecks gar verfehlet?", "tokens": ["Der", "Sch\u00f6p\u00b7fung", "En\u00b7dZwecks", "gar", "ver\u00b7feh\u00b7let", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und hat Beraldo nicht, zufolge seiner Pflicht,", "tokens": ["Und", "hat", "Be\u00b7ral\u00b7do", "nicht", ",", "zu\u00b7fol\u00b7ge", "sei\u00b7ner", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NE", "PTKNEG", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "In seiner Lust ", "tokens": ["In", "sei\u00b7ner", "Lust"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}}}}}