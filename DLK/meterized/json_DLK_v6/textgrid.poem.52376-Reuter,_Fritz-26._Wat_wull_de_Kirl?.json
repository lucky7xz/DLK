{"textgrid.poem.52376": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "26. Wat wull de Kirl?", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbne, Fiken, denk di, wo't mi gung!", "tokens": ["\u00bb", "ne", ",", "Fi\u00b7ken", ",", "denk", "di", ",", "wo't", "mi", "gung", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$,", "NN", "$,", "VVFIN", "NE", "$,", "PWAV", "NE", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "As't gistern an tau schummern fung,", "tokens": ["As't", "gis\u00b7tern", "an", "tau", "schum\u00b7mern", "fung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dunn gah ick hen nah'n Water halen,", "tokens": ["Dunn", "gah", "ick", "hen", "nah'n", "Wa\u00b7ter", "ha\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "VVFIN", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Un as ick kam nah unsen Sod,", "tokens": ["Un", "as", "ick", "kam", "nah", "un\u00b7sen", "Sod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPER", "VVFIN", "ADJD", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Dunn steiht en Kirl dor rank un grot", "tokens": ["Dunn", "steiht", "en", "Kirl", "dor", "rank", "un", "grot"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "FM", "FM", "FM", "FM", "FM", "FM"], "meter": "-