{"textgrid.poem.60505": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "An die medisierenden Damen", "genre": "verse", "period": "N.A.", "pub_year": 1835, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Spro\u00dft ihr wie des Fr\u00fchlings junge Triebe,", "tokens": ["Spro\u00dft", "ihr", "wie", "des", "Fr\u00fch\u00b7lings", "jun\u00b7ge", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ahmt die Wange seiner Rosen Glut,", "tokens": ["Ahmt", "die", "Wan\u00b7ge", "sei\u00b7ner", "Ro\u00b7sen", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Soll das Herz auch ahmen seine Liebe,", "tokens": ["Soll", "das", "Herz", "auch", "ah\u00b7men", "sei\u00b7ne", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wie das Herz des Fr\u00fchlings \u2013 mild und gut.", "tokens": ["Wie", "das", "Herz", "des", "Fr\u00fch\u00b7lings", "\u2013", "mild", "und", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "$(", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Medisiert das Bl\u00fcmlein auf der Wiese,", "tokens": ["Me\u00b7di\u00b7siert", "das", "Bl\u00fcm\u00b7lein", "auf", "der", "Wie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Seinem un verlernen Paradiese?", "tokens": ["Sei\u00b7nem", "un", "ver\u00b7ler\u00b7nen", "Pa\u00b7ra\u00b7die\u00b7se", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "FM", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Tuns im Wald die jungen, gr\u00fcnen Bl\u00e4tter,", "tokens": ["Tuns", "im", "Wald", "die", "jun\u00b7gen", ",", "gr\u00fc\u00b7nen", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Wenn sie beim Gedr\u00f6hn der Fr\u00fchlingswetter", "tokens": ["Wenn", "sie", "beim", "Ge\u00b7dr\u00f6hn", "der", "Fr\u00fch\u00b7lings\u00b7wet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.9": {"text": "Wonnig rauschen und zusammenschauern?", "tokens": ["Won\u00b7nig", "rau\u00b7schen", "und", "zu\u00b7sam\u00b7men\u00b7schau\u00b7ern", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KON", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Geht und lauscht und lernet euch bedauern!", "tokens": ["Geht", "und", "lauscht", "und", "ler\u00b7net", "euch", "be\u00b7dau\u00b7ern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "KON", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Liebe singt der Vogel von den Zweigen,", "tokens": ["Lie\u00b7be", "singt", "der", "Vo\u00b7gel", "von", "den", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Und im frohen Jugendreigen", "tokens": ["Und", "im", "fro\u00b7hen", "Ju\u00b7gen\u00b7drei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Rauben liebestrunken Maienl\u00fcfte", "tokens": ["Rau\u00b7ben", "lie\u00b7be\u00b7strun\u00b7ken", "Mai\u00b7en\u00b7l\u00fcf\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Aufgebl\u00fchten Blumen ihre D\u00fcfte;", "tokens": ["Auf\u00b7ge\u00b7bl\u00fch\u00b7ten", "Blu\u00b7men", "ih\u00b7re", "D\u00fcf\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Aber keinen guten Namen.", "tokens": ["A\u00b7ber", "kei\u00b7nen", "gu\u00b7ten", "Na\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Medisiert nicht, junge Damen!", "tokens": ["Me\u00b7di\u00b7siert", "nicht", ",", "jun\u00b7ge", "Da\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Sa\u00df ich einst in einem M\u00e4dchenkreise,", "tokens": ["Sa\u00df", "ich", "einst", "in", "ei\u00b7nem", "M\u00e4d\u00b7chen\u00b7krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Da begann in ihrem Bl\u00fctenkranze", "tokens": ["Da", "be\u00b7gann", "in", "ih\u00b7rem", "Bl\u00fc\u00b7ten\u00b7kran\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Erst geheim zu zischeln, klug und leise,", "tokens": ["Erst", "ge\u00b7heim", "zu", "zi\u00b7scheln", ",", "klug", "und", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKZU", "VVINF", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Doch bald laut die Schlange: Medisance.", "tokens": ["Doch", "bald", "laut", "die", "Schlan\u00b7ge", ":", "Me\u00b7di\u00b7san\u00b7ce", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "$.", "NE", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.21": {"text": "Und sie r\u00fcmpften ihre feine Nase,", "tokens": ["Und", "sie", "r\u00fcmpf\u00b7ten", "ih\u00b7re", "fei\u00b7ne", "Na\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Ekel zuckte mancher Rosenmund,", "tokens": ["E\u00b7kel", "zuck\u00b7te", "man\u00b7cher", "Ro\u00b7sen\u00b7mund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.23": {"text": "Weil ein Name, wacker und gesund,", "tokens": ["Weil", "ein", "Na\u00b7me", ",", "wa\u00b7cker", "und", "ge\u00b7sund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.24": {"text": "Von dem Bi\u00df der Schlange ward zum Aase. \u2013", "tokens": ["Von", "dem", "Bi\u00df", "der", "Schlan\u00b7ge", "ward", "zum", "Aa\u00b7se", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "APPR", "ART", "NN", "VAFIN", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.25": {"text": "Ist der Name krank, so la\u00dft den kranken", "tokens": ["Ist", "der", "Na\u00b7me", "krank", ",", "so", "la\u00dft", "den", "kran\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ADJD", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.26": {"text": "Ungeneckt an euch vor\u00fcberwanken;", "tokens": ["Un\u00b7ge\u00b7neckt", "an", "euch", "vor\u00b7\u00fc\u00b7ber\u00b7wan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.27": {"text": "Wollt ihr lindern nicht die Namenswunde", "tokens": ["Wollt", "ihr", "lin\u00b7dern", "nicht", "die", "Na\u00b7mens\u00b7wun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.28": {"text": "Mit des Frauenmitleids weichem \u00d6le,", "tokens": ["Mit", "des", "Frau\u00b7en\u00b7mit\u00b7leids", "wei\u00b7chem", "\u00d6\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.29": {"text": "La\u00dft ihn ziehn; doch nicht in eure Runde", "tokens": ["La\u00dft", "ihn", "ziehn", ";", "doch", "nicht", "in", "eu\u00b7re", "Run\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVINF", "$.", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.30": {"text": "Rei\u00dft ihn als in eine R\u00e4uberh\u00f6hle! \u2013", "tokens": ["Rei\u00dft", "ihn", "als", "in", "ei\u00b7ne", "R\u00e4u\u00b7ber\u00b7h\u00f6h\u00b7le", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.31": {"text": "Wandelt ihr im Herbste eurer Tage,", "tokens": ["Wan\u00b7delt", "ihr", "im", "Herbs\u00b7te", "eu\u00b7rer", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.32": {"text": "Ist in jedem Mienenzug zu lesen", "tokens": ["Ist", "in", "je\u00b7dem", "Mien\u00b7en\u00b7zug", "zu", "le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PIAT", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.33": {"text": "Des Verwelkens untr\u00f6stbare Klage,", "tokens": ["Des", "Ver\u00b7wel\u00b7kens", "un\u00b7tr\u00f6st\u00b7ba\u00b7re", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-++--+-", "measure": "trochaic.penta.relaxed"}, "line.34": {"text": "Da\u00df ihr nimmer seid, was ihr gewesen;", "tokens": ["Da\u00df", "ihr", "nim\u00b7mer", "seid", ",", "was", "ihr", "ge\u00b7we\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "$,", "PWS", "PPER", "VAPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.35": {"text": "Dann, ihr Damen, lernt vom Herbst die Wehmut,", "tokens": ["Dann", ",", "ihr", "Da\u00b7men", ",", "lernt", "vom", "Herbst", "die", "Weh\u00b7mut", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "NN", "$,", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.36": {"text": "Lernet die gedankenvolle Demut,", "tokens": ["Ler\u00b7net", "die", "ge\u00b7dan\u00b7ken\u00b7vol\u00b7le", "De\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.37": {"text": "Nehmet mit Bedacht", "tokens": ["Neh\u00b7met", "mit", "Be\u00b7dacht"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.38": {"text": "Euer Grab in acht,", "tokens": ["Eu\u00b7er", "Grab", "in", "acht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "CARD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.39": {"text": "Statt in andrer Fehler schn\u00f6d zu kramen;", "tokens": ["Statt", "in", "an\u00b7drer", "Feh\u00b7ler", "schn\u00f6d", "zu", "kra\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.40": {"text": "Medisiert nicht, alte Damen!", "tokens": ["Me\u00b7di\u00b7siert", "nicht", ",", "al\u00b7te", "Da\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Fliegt ein schuldlos V\u00f6glein unbewu\u00dft", "tokens": ["Fliegt", "ein", "schuld\u00b7los", "V\u00f6\u00b7glein", "un\u00b7be\u00b7wu\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJD", "NN", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.42": {"text": "\u00dcber Guas-Upas giftgen See,", "tokens": ["\u00dc\u00b7ber", "Gu\u00b7as\u00b7U\u00b7pas", "gift\u00b7gen", "See", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.43": {"text": "St\u00fcrzt es schnell, die liedervolle Brust", "tokens": ["St\u00fcrzt", "es", "schnell", ",", "die", "lie\u00b7der\u00b7vol\u00b7le", "Brust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.44": {"text": "Ist verstummt in bittrem Todesweh.", "tokens": ["Ist", "ver\u00b7stummt", "in", "bitt\u00b7rem", "To\u00b7des\u00b7weh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.45": {"text": "In den Borden eurer Kessel, Kannen", "tokens": ["In", "den", "Bor\u00b7den", "eu\u00b7rer", "Kes\u00b7sel", ",", "Kan\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.46": {"text": "Flutet Guas-Upas: Tee, Kaffee,", "tokens": ["Flu\u00b7tet", "Gu\u00b7as\u00b7U\u00b7pas", ":", "Tee", ",", "Kaf\u00b7fee", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "NE", "$.", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Und es zog kein Name heil von dannen,", "tokens": ["Und", "es", "zog", "kein", "Na\u00b7me", "heil", "von", "dan\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN", "ADJD", "APPR", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.48": {"text": "Dessen Flug verirrt an diesem See;", "tokens": ["Des\u00b7sen", "Flug", "ver\u00b7irrt", "an", "die\u00b7sem", "See", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVPP", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.49": {"text": "Klang der arme Flattrer auch", "tokens": ["Klang", "der", "ar\u00b7me", "Flatt\u00b7rer", "auch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.50": {"text": "Erst im heimatlichen Strauch", "tokens": ["Erst", "im", "hei\u00b7mat\u00b7li\u00b7chen", "Strauch"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.51": {"text": "Wie das Lied des Vogels rein und gut,", "tokens": ["Wie", "das", "Lied", "des", "Vo\u00b7gels", "rein", "und", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.52": {"text": "St\u00fcrzt er tot in eure braune Flut. \u2013", "tokens": ["St\u00fcrzt", "er", "tot", "in", "eu\u00b7re", "brau\u00b7ne", "Flut", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.53": {"text": "Aber, gilt es auch nicht gleich den Namen,", "tokens": ["A\u00b7ber", ",", "gilt", "es", "auch", "nicht", "gleich", "den", "Na\u00b7men", ","], "tok