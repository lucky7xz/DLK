{"textgrid.poem.48969": {"metadata": {"author": {"name": "Kretschmann, Karl Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "9. Das t\u00e4gliche Fallen", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl siebenzigmal an einem einz'gen Tage", "tokens": ["Wohl", "sie\u00b7ben\u00b7zig\u00b7mal", "an", "ei\u00b7nem", "einz'\u00b7gen", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "F\u00e4llt der Gerechteste auf dieser argen Welt.", "tokens": ["F\u00e4llt", "der", "Ge\u00b7rech\u00b7tes\u00b7te", "auf", "die\u00b7ser", "ar\u00b7gen", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Nun m\u00f6cht' ich doch schon wissen, ist die Frage,", "tokens": ["Nun", "m\u00f6cht'", "ich", "doch", "schon", "wis\u00b7sen", ",", "ist", "die", "Fra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie oft des Tages die Gerechte f\u00e4llt?", "tokens": ["Wie", "oft", "des", "Ta\u00b7ges", "die", "Ge\u00b7rech\u00b7te", "f\u00e4llt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}