{"dta.poem.18237": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Ein junger Mensch von achtzehn Jahren,", "tokens": ["Ein", "jun\u00b7ger", "Mensch", "von", "acht\u00b7zehn", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und dieses Buch der Welt schien mir", "tokens": ["Und", "die\u00b7ses", "Buch", "der", "Welt", "schien", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie eines Engels Memoiren.", "tokens": ["Wie", "ei\u00b7nes", "En\u00b7gels", "Me\u00b7moi\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Schon sah ich mich im Fr\u00fchrothschein", "tokens": ["Schon", "sah", "ich", "mich", "im", "Fr\u00fch\u00b7roth\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vor lauter Gl\u00fcck die H\u00e4nde falten,", "tokens": ["Vor", "lau\u00b7ter", "Gl\u00fcck", "die", "H\u00e4n\u00b7de", "fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch heut gesteh ich's traurig ein:", "tokens": ["Doch", "heut", "ge\u00b7steh", "ich's", "trau\u00b7rig", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mein Herz hat mir nicht Wort gehalten!", "tokens": ["Mein", "Herz", "hat", "mir", "nicht", "Wort", "ge\u00b7hal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "PTKNEG", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Auch schrieb ich manchen Liebesbrief", "tokens": ["Auch", "schrieb", "ich", "man\u00b7chen", "Lie\u00b7bes\u00b7brief"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schw\u00e4rmte ", "tokens": ["Und", "schw\u00e4rm\u00b7te"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Doch das war kindisch und naiv,", "tokens": ["Doch", "das", "war", "kin\u00b7disch", "und", "naiv", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Denn statt der Herzen fand ich Steine.", "tokens": ["Denn", "statt", "der", "Her\u00b7zen", "fand", "ich", "Stei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nun h\u00e4ngt am Galgen mein Humor", "tokens": ["Nun", "h\u00e4ngt", "am", "Gal\u00b7gen", "mein", "Hu\u00b7mor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Und macht mein warmes Blut erkalten,", "tokens": ["Und", "macht", "mein", "war\u00b7mes", "Blut", "er\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Denn traurig klingt es mir im Ohr:", "tokens": ["Denn", "trau\u00b7rig", "klingt", "es", "mir", "im", "Ohr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mein Herz hat mir nicht Wort gehalten!", "tokens": ["Mein", "Herz", "hat", "mir", "nicht", "Wort", "ge\u00b7hal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "PTKNEG", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Zwar meiner Kunst ersehnten Kranz,", "tokens": ["Zwar", "mei\u00b7ner", "Kunst", "er\u00b7sehn\u00b7ten", "Kranz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schon streift ihn hie und da mein Scheitel,", "tokens": ["Schon", "streift", "ihn", "hie", "und", "da", "mein", "Schei\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KON", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch denk ich schon wie Meister Hans", "tokens": ["Doch", "denk", "ich", "schon", "wie", "Meis\u00b7ter", "Hans"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "KOKOM", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und deklamire: Alles eitel!", "tokens": ["Und", "de\u00b7kla\u00b7mi\u00b7re", ":", "Al\u00b7les", "ei\u00b7tel", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PIS", "ADJD", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Mir kreist das Hirn, mir wankt das Knie,", "tokens": ["Mir", "kreist", "das", "Hirn", ",", "mir", "wankt", "das", "Knie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Andrer mag mein Amt verwalten!", "tokens": ["Ein", "A\u00b7ndrer", "mag", "mein", "Amt", "ver\u00b7wal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mein Herz hat mir nicht Wort gehalten!", "tokens": ["Mein", "Herz", "hat", "mir", "nicht", "Wort", "ge\u00b7hal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "PTKNEG", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}