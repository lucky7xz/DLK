{"dta.poem.21538": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Gesicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Durch der Vergangenheit Gefild", "tokens": ["Durch", "der", "Ver\u00b7gan\u00b7gen\u00b7heit", "Ge\u00b7fild"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Schwebte mein Geist,", "tokens": ["Schweb\u00b7te", "mein", "Geist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Auf der Geschichte Bl\u00e4ttern", "tokens": ["Auf", "der", "Ge\u00b7schich\u00b7te", "Bl\u00e4t\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weilte mein Auge,", "tokens": ["Weil\u00b7te", "mein", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Als mit dunkelem", "tokens": ["Als", "mit", "dun\u00b7ke\u00b7lem"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.6": {"text": "Sternbes\u00e4eten Fittig", "tokens": ["Stern\u00b7be\u00b7s\u00e4e\u00b7ten", "Fit\u00b7tig"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Ueber die Erde", "tokens": ["Ue\u00b7ber", "die", "Er\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Hinzog die Mitternacht.", "tokens": ["Hin\u00b7zog", "die", "Mit\u00b7ter\u00b7nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Auf meine Schl\u00e4fe", "tokens": ["Auf", "mei\u00b7ne", "Schl\u00e4\u00b7fe"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Legte sich\u2019s pl\u00f6tzlich", "tokens": ["Leg\u00b7te", "sich's", "pl\u00f6tz\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PIS", "ADJD"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Wie beschw\u00f6rende", "tokens": ["Wie", "be\u00b7schw\u00f6\u00b7ren\u00b7de"], "token_info": ["word", "word"], "pos": ["PWAV", "ADJA"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.12": {"text": "Zauberhand,", "tokens": ["Zau\u00b7ber\u00b7hand", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Meine Augen erkannten", "tokens": ["Mei\u00b7ne", "Au\u00b7gen", "er\u00b7kann\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.14": {"text": "Nicht mehr die Lettern", "tokens": ["Nicht", "mehr", "die", "Let\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Und diese dehnten sich aus", "tokens": ["Und", "die\u00b7se", "dehn\u00b7ten", "sich", "aus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "PRF", "APPR"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.16": {"text": "Und wuchsen", "tokens": ["Und", "wuch\u00b7sen"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "Und waren zu fassen", "tokens": ["Und", "wa\u00b7ren", "zu", "fas\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PTKZU", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.18": {"text": "Und wurden zu K\u00f6rpern . . . .", "tokens": ["Und", "wur\u00b7den", "zu", "K\u00f6r\u00b7pern", ".", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "$.", "$.", "$.", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.19": {"text": "Und pl\u00f6tzlich", "tokens": ["Und", "pl\u00f6tz\u00b7lich"], "token_info": ["word", "word"], "pos": ["KON", "ADJD"], "meter": "-+-", "measure": "amphibrach.single"}, "line.20": {"text": "Stand ich auf einem", "tokens": ["Stand", "ich", "auf", "ei\u00b7nem"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.21": {"text": "Unendlichen,", "tokens": ["Un\u00b7end\u00b7li\u00b7chen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+--", "measure": "dactylic.init"}, "line.22": {"text": "Gro\u00dfen Friedhof \u2026", "tokens": ["Gro\u00b7\u00dfen", "Fried\u00b7hof", "\u2026"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.23": {"text": "Von Horizont zu Horizont", "tokens": ["Von", "Ho\u00b7ri\u00b7zont", "zu", "Ho\u00b7ri\u00b7zont"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Reichte die Reihe der Gr\u00e4ber,", "tokens": ["Reich\u00b7te", "die", "Rei\u00b7he", "der", "Gr\u00e4\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.25": {"text": "Und auf ihnen standen", "tokens": ["Und", "auf", "ih\u00b7nen", "stan\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.26": {"text": "Kreuze und Male,", "tokens": ["Kreu\u00b7ze", "und", "Ma\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.27": {"text": "Und dazwischen gl\u00fchten", "tokens": ["Und", "da\u00b7zwi\u00b7schen", "gl\u00fch\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["KON", "PAV", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.28": {"text": "Lichter, als w\u00e4re", "tokens": ["Lich\u00b7ter", ",", "als", "w\u00e4\u00b7re"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "KOKOM", "VAFIN"], "meter": "+-++-", "measure": "unknown.measure.tri"}, "line.29": {"text": "Der Tag aller Seelen.", "tokens": ["Der", "Tag", "al\u00b7ler", "See\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.30": {"text": "Wie ich nun hinblickte", "tokens": ["Wie", "ich", "nun", "hin\u00b7blick\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.31": {"text": "Sah ich, da\u00df aus den Gr\u00e4bern", "tokens": ["Sah", "ich", ",", "da\u00df", "aus", "den", "Gr\u00e4\u00b7bern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Jedem wuchs eine Hand,", "tokens": ["Je\u00b7dem", "wuchs", "ei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.33": {"text": "Eine anklagende Todtenhand,", "tokens": ["Ei\u00b7ne", "an\u00b7kla\u00b7gen\u00b7de", "Tod\u00b7ten\u00b7hand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.34": {"text": "Den Richter anflehend", "tokens": ["Den", "Rich\u00b7ter", "an\u00b7fle\u00b7hend"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.35": {"text": "Um Gerechtigkeit.", "tokens": ["Um", "Ge\u00b7rech\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "punct"], "pos": ["KOUI", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.36": {"text": "Und an den Kreuzen", "tokens": ["Und", "an", "den", "Kreu\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "++-+-", "measure": "iambic.di"}, "line.37": {"text": "Hingen blutige", "tokens": ["Hin\u00b7gen", "blu\u00b7ti\u00b7ge"], "token_info": ["word", "word"], "pos": ["ADV", "ADJA"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.38": {"text": "Leichname", "tokens": ["Leich\u00b7na\u00b7me"], "token_info": ["word"], "pos": ["NN"], "meter": "---", "measure": "unknown.measure.zero"}, "line.39": {"text": "Mit schmerzverzerrten", "tokens": ["Mit", "schmerz\u00b7ver\u00b7zerr\u00b7ten"], "token_info": ["word", "word"], "pos": ["APPR", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.40": {"text": "Gesichtern", "tokens": ["Ge\u00b7sich\u00b7tern"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.41": {"text": "Und gebrochenen Augen", "tokens": ["Und", "ge\u00b7bro\u00b7che\u00b7nen", "Au\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.42": {"text": "Und von den erbla\u00dften,", "tokens": ["Und", "von", "den", "er\u00b7bla\u00df\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.43": {"text": "Wehdurchzuckten Lippen", "tokens": ["Weh\u00b7durch\u00b7zuck\u00b7ten", "Lip\u00b7pen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.44": {"text": "T\u00f6nte die Klage", "tokens": ["T\u00f6n\u00b7te", "die", "Kla\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.45": {"text": "Gegen Tyrannen", "tokens": ["Ge\u00b7gen", "Ty\u00b7ran\u00b7nen"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.46": {"text": "Und alle die Grausamen,", "tokens": ["Und", "al\u00b7le", "die", "Grau\u00b7sa\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "NN", "$,"], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.47": {"text": "Die seit der Welt Beginn", "tokens": ["Die", "seit", "der", "Welt", "Be\u00b7ginn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.48": {"text": "Die Menschheit gepeinigt", "tokens": ["Die", "Menschheit", "ge\u00b7pei\u00b7nigt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.49": {"text": "Und gemartert,", "tokens": ["Und", "ge\u00b7mar\u00b7tert", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.50": {"text": "Die nicht wu\u00dften,", "tokens": ["Die", "nicht", "wu\u00df\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.51": {"text": "Da\u00df die Menschen", "tokens": ["Da\u00df", "die", "Men\u00b7schen"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.52": {"text": "Alle nur Br\u00fcder", "tokens": ["Al\u00b7le", "nur", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADV", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.53": {"text": "Und die der Liebe verga\u00dfen \u2026", "tokens": ["Und", "die", "der", "Lie\u00b7be", "ver\u00b7ga\u00b7\u00dfen", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "VVINF", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.54": {"text": "Die Leuchten aber", "tokens": ["Die", "Leuch\u00b7ten", "a\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.55": {"text": "Waren Scheiterhaufen,", "tokens": ["Wa\u00b7ren", "Schei\u00b7ter\u00b7hau\u00b7fen", ","], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.56": {"text": "In denen", "tokens": ["In", "de\u00b7nen"], "token_info": ["word", "word"], "pos": ["APPR", "PRELS"], "meter": "-+-", "measure": "amphibrach.single"}, "line.57": {"text": "Verdammte st\u00f6hnten,", "tokens": ["Ver\u00b7damm\u00b7te", "st\u00f6hn\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.58": {"text": "Und unter den", "tokens": ["Und", "un\u00b7ter", "den"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "ART"], "meter": "-+--", "measure": "dactylic.init"}, "line.59": {"text": "Steinernen Gr\u00e4bermalen", "tokens": ["Stei\u00b7ner\u00b7nen", "Gr\u00e4\u00b7ber\u00b7ma\u00b7len"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.60": {"text": "Keuchten Schatten,", "tokens": ["Keuch\u00b7ten", "Schat\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.61": {"text": "Als tr\u00fcgen sie", "tokens": ["Als", "tr\u00fc\u00b7gen", "sie"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.62": {"text": "Noch wie dereinst", "tokens": ["Noch", "wie", "de\u00b7reinst"], "token_info": ["word", "word", "word"], "pos": ["ADV", "KOKOM", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.63": {"text": "In grausamem Frohndienst", "tokens": ["In", "grau\u00b7sa\u00b7mem", "Frohn\u00b7dienst"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.64": {"text": "Die Felsenbl\u00f6cke", "tokens": ["Die", "Fel\u00b7sen\u00b7bl\u00f6\u00b7cke"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.65": {"text": "Hin zum Baue", "tokens": ["Hin", "zum", "Bau\u00b7e"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.66": {"text": "Der Pyramiden \u2026", "tokens": ["Der", "Py\u00b7ra\u00b7mi\u00b7den", "\u2026"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Schaudernd stand ich,", "tokens": ["Schau\u00b7dernd", "stand", "ich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Da rief eine Stimme:", "tokens": ["Da", "rief", "ei\u00b7ne", "Stim\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die du hier siehst,", "tokens": ["Die", "du", "hier", "siehst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Es sind die Schatten", "tokens": ["Es", "sind", "die", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Der Armen und Elenden,", "tokens": ["Der", "Ar\u00b7men", "und", "E\u00b7len\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.6": {"text": "Der unschuldig Verdammten.", "tokens": ["Der", "un\u00b7schul\u00b7dig", "Ver\u00b7damm\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Der M\u00e4rtyrer,", "tokens": ["Der", "M\u00e4r\u00b7ty\u00b7rer", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+--", "measure": "dactylic.init"}, "line.8": {"text": "Die Nero mordete,", "tokens": ["Die", "Ne\u00b7ro", "mor\u00b7de\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Und die auf Philipps Weisung", "tokens": ["Und", "die", "auf", "Phi\u00b7lipps", "Wei\u00b7sung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "APPR", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Des Feuers Rachen verschlang.", "tokens": ["Des", "Feu\u00b7ers", "Ra\u00b7chen", "ver\u00b7schlang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "Alle unschuldi