{"textgrid.poem.60544": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Die Rache", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der dunklen Wolken letzte schwand", "tokens": ["Der", "dunk\u00b7len", "Wol\u00b7ken", "letz\u00b7te", "schwand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hinab am glatten Meeresrand,", "tokens": ["Hin\u00b7ab", "am", "glat\u00b7ten", "Mee\u00b7res\u00b7rand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um Schatten fernem Land zu schenken", "tokens": ["Um", "Schat\u00b7ten", "fer\u00b7nem", "Land", "zu", "schen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "NN", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mit Gewittern es zu tr\u00e4nken.", "tokens": ["Und", "mit", "Ge\u00b7wit\u00b7tern", "es", "zu", "tr\u00e4n\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hier regt kein Hauch das durstge Laub,", "tokens": ["Hier", "regt", "kein", "Hauch", "das", "durst\u00b7ge", "Laub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und ruhig liegt der feinste Staub;", "tokens": ["Und", "ru\u00b7hig", "liegt", "der", "feins\u00b7te", "Staub", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Sommerluft ist schw\u00fcl und matt,", "tokens": ["Die", "Som\u00b7mer\u00b7luft", "ist", "schw\u00fcl", "und", "matt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und auf der Wasserfl\u00e4che glatt", "tokens": ["Und", "auf", "der", "Was\u00b7ser\u00b7fl\u00e4\u00b7che", "glatt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mag sicher hin die Spinne schreiten,", "tokens": ["Mag", "si\u00b7cher", "hin", "die", "Spin\u00b7ne", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sie kann in keine Furche gleiten;", "tokens": ["Sie", "kann", "in", "kei\u00b7ne", "Fur\u00b7che", "glei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Die M\u00f6wen taumeln trag und schlagen", "tokens": ["Die", "M\u00f6\u00b7wen", "tau\u00b7meln", "trag", "und", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "VVFIN", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Die schlaffe Luft mit Unbehagen.", "tokens": ["Die", "schlaf\u00b7fe", "Luft", "mit", "Un\u00b7be\u00b7ha\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Matrosen baden dort und singen,", "tokens": ["Mat\u00b7ro\u00b7sen", "ba\u00b7den", "dort", "und", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Um Leben in die Luft zu bringen,", "tokens": ["Um", "Le\u00b7ben", "in", "die", "Luft", "zu", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Denn ist der Seemann m\u00fc\u00dfig auch,", "tokens": ["Denn", "ist", "der", "See\u00b7mann", "m\u00fc\u00b7\u00dfig", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er liebt des Windes frischen Hauch,", "tokens": ["Er", "liebt", "des", "Win\u00b7des", "fri\u00b7schen", "Hauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf seinen Fahrten lernt er hassen", "tokens": ["Auf", "sei\u00b7nen", "Fahr\u00b7ten", "lernt", "er", "has\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das stille Meer, vom Wind verlassen.", "tokens": ["Das", "stil\u00b7le", "Meer", ",", "vom", "Wind", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sie singen froh ein irisch Lied,", "tokens": ["Sie", "sin\u00b7gen", "froh", "ein", "i\u00b7risch", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie dem Matrosen wohl geschieht,", "tokens": ["Wie", "dem", "Mat\u00b7ro\u00b7sen", "wohl", "ge\u00b7schieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wenn er die Fahrt mit M\u00fch vollbracht,", "tokens": ["Wenn", "er", "die", "Fahrt", "mit", "M\u00fch", "voll\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die M\u00fcnze rollt, die Dirne lacht,", "tokens": ["Die", "M\u00fcn\u00b7ze", "rollt", ",", "die", "Dir\u00b7ne", "lacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die Fiedel ... weh! ein banger Schrei!", "tokens": ["Die", "Fie\u00b7del", "...", "weh", "!", "ein", "ban\u00b7ger", "Schrei", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PTKVZ", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Den einen bi\u00df ein Hai entzwei.", "tokens": ["Den", "ei\u00b7nen", "bi\u00df", "ein", "Hai", "ent\u00b7zwei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Dem Kameraden, ders erblickt,", "tokens": ["Dem", "Ka\u00b7me\u00b7ra\u00b7den", ",", "ders", "er\u00b7blickt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Hat Schreck und Wut das Herz durchz\u00fcckt.", "tokens": ["Hat", "Schreck", "und", "Wut", "das", "Herz", "durch\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch hat er schnell sich aufgemannt,", "tokens": ["Doch", "hat", "er", "schnell", "sich", "auf\u00b7ge\u00b7mannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Schreck ist in der Wut verbrannt,", "tokens": ["Sein", "Schreck", "ist", "in", "der", "Wut", "ver\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er springt ans Land und holt sein Messer", "tokens": ["Er", "springt", "ans", "Land", "und", "holt", "sein", "Mes\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und st\u00fcrzt zur Rache ins Gew\u00e4sser,", "tokens": ["Und", "st\u00fcrzt", "zur", "Ra\u00b7che", "ins", "Ge\u00b7w\u00e4s\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die andern starren vom Gestade", "tokens": ["Die", "an\u00b7dern", "star\u00b7ren", "vom", "Ge\u00b7sta\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ihm nach, und flehen Gott um Gnade.", "tokens": ["Ihm", "nach", ",", "und", "fle\u00b7hen", "Gott", "um", "Gna\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "KON", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wo bist? komm an! \u2013 er taucht und dreht", "tokens": ["Wo", "bist", "?", "komm", "an", "!", "\u2013", "er", "taucht", "und", "dreht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "$.", "VVFIN", "PTKVZ", "$.", "$(", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Augen rings und schwimmt und sp\u00e4ht", "tokens": ["Die", "Au\u00b7gen", "rings", "und", "schwimmt", "und", "sp\u00e4ht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "KON", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sucht den grimmen Feind verwegen;", "tokens": ["Und", "sucht", "den", "grim\u00b7men", "Feind", "ver\u00b7we\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da schie\u00dft das Untier ihm entgegen,", "tokens": ["Da", "schie\u00dft", "das", "Un\u00b7tier", "ihm", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Weit g\u00e4hnt ihm zu der Rachenri\u00df", "tokens": ["Weit", "g\u00e4hnt", "ihm", "zu", "der", "Ra\u00b7chen\u00b7ri\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und fletscht nach ihm das Mordgebi\u00df.", "tokens": ["Und", "fletscht", "nach", "ihm", "das", "Mord\u00b7ge\u00b7bi\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch denkt er nicht der eignen Sache,", "tokens": ["Doch", "denkt", "er", "nicht", "der", "eig\u00b7nen", "Sa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nur Rache, seinem Toten Rache.", "tokens": ["Nur", "Ra\u00b7che", ",", "sei\u00b7nem", "To\u00b7ten", "Ra\u00b7che", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Tief in des Meeres Einsamkeit", "tokens": ["Tief", "in", "des", "Mee\u00b7res", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und D\u00e4mmerung beginnt der Streit,", "tokens": ["Und", "D\u00e4m\u00b7me\u00b7rung", "be\u00b7ginnt", "der", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Wild, atemlos, still; wer bezwungen,", "tokens": ["Wild", ",", "a\u00b7tem\u00b7los", ",", "still", ";", "wer", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "ADJD", "$.", "PWS", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.12": {"text": "Wird stiller nicht, als er gerungen,", "tokens": ["Wird", "stil\u00b7ler", "nicht", ",", "als", "er", "ge\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PTKNEG", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Der Dolch, die Z\u00e4hne sind gez\u00fcckt,", "tokens": ["Der", "Dolch", ",", "die", "Z\u00e4h\u00b7ne", "sind", "ge\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Das Auge nah dem Auge r\u00fcckt.", "tokens": ["Das", "Au\u00b7ge", "nah", "dem", "Au\u00b7ge", "r\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Am Strande stehn die andern harrend,", "tokens": ["Am", "Stran\u00b7de", "stehn", "die", "an\u00b7dern", "har\u00b7rend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bang nach dem Ausgang niederstarrend.", "tokens": ["Bang", "nach", "dem", "Aus\u00b7gang", "nie\u00b7ders\u00b7tar\u00b7rend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Wohl manchen mahnts: o spring hinein,", "tokens": ["Wohl", "man\u00b7chen", "mahnts", ":", "o", "spring", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$.", "FM", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00df deinen Bruder nicht allein!", "tokens": ["La\u00df", "dei\u00b7nen", "Bru\u00b7der", "nicht", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch Schrecken hemmt die k\u00fchne Tat", "tokens": ["Doch", "Schre\u00b7cken", "hemmt", "die", "k\u00fch\u00b7ne", "Tat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und raunt ihm zu: Es ist zu spat.", "tokens": ["Und", "raunt", "ihm", "zu", ":", "Es", "ist", "zu", "spat", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VAFIN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da sehn sie rot das Meer sich f\u00e4rben,", "tokens": ["Da", "sehn", "sie", "rot", "das", "Meer", "sich", "f\u00e4r\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Stets r\u00f6ter quillts \u2013 wer mu\u00dfte sterben?", "tokens": ["Stets", "r\u00f6\u00b7ter", "quill