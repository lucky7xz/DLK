{"textgrid.poem.46906": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ob sei eigens mir verh\u00e4nget]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob sei eigens mir verh\u00e4nget,", "tokens": ["Ob", "sei", "ei\u00b7gens", "mir", "ver\u00b7h\u00e4n\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was mich enget und bedr\u00e4nget,", "tokens": ["Was", "mich", "en\u00b7get", "und", "be\u00b7dr\u00e4n\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wei\u00df ich nicht; so weit sind meiner", "tokens": ["Wei\u00df", "ich", "nicht", ";", "so", "weit", "sind", "mei\u00b7ner"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$.", "ADV", "ADJD", "VAFIN", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einsicht F\u00e4den nicht verl\u00e4nget,", "tokens": ["Ein\u00b7sicht", "F\u00e4\u00b7den", "nicht", "ver\u00b7l\u00e4n\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bis dahin, von wo den ersten", "tokens": ["Bis", "da\u00b7hin", ",", "von", "wo", "den", "ers\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PAV", "$,", "APPR", "PWAV", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sto\u00df der Gang der Welt empf\u00e4nget.", "tokens": ["Sto\u00df", "der", "Gang", "der", "Welt", "emp\u00b7f\u00e4n\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mir gen\u00fcgt Gef\u00fchl des Glaubens,", "tokens": ["Mir", "ge\u00b7n\u00fcgt", "Ge\u00b7f\u00fchl", "des", "Glau\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df kein Ungef\u00e4hr mich zw\u00e4nget,", "tokens": ["Da\u00df", "kein", "Un\u00b7ge\u00b7f\u00e4hr", "mich", "zw\u00e4n\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Sondern mein Geschick, das kleine,", "tokens": ["Son\u00b7dern", "mein", "Ge\u00b7schick", ",", "das", "klei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Ab vom gro\u00dfen Ganzen h\u00e4nget.", "tokens": ["Ab", "vom", "gro\u00b7\u00dfen", "Gan\u00b7zen", "h\u00e4n\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}