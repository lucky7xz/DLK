{"textgrid.poem.58324": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "362. Die Ruhe des Geistes", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kann Fleisches Ungemach des Geistes Ruh noch st\u00f6ren,", "tokens": ["Kann", "Flei\u00b7sches", "Un\u00b7ge\u00b7mach", "des", "Geis\u00b7tes", "Ruh", "noch", "st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJA", "NN", "ART", "NN", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ist die Ruhe noch nicht tief, nicht rein und fest;", "tokens": ["So", "ist", "die", "Ru\u00b7he", "noch", "nicht", "tief", ",", "nicht", "rein", "und", "fest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "PTKNEG", "ADJD", "$,", "PTKNEG", "ADJD", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Geist kann freier sich zu seiner Ruhe kehren,", "tokens": ["Der", "Geist", "kann", "frei\u00b7er", "sich", "zu", "sei\u00b7ner", "Ru\u00b7he", "keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADJD", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Je enger man das Fleisch ans Kreuz gebunden l\u00e4\u00dft.", "tokens": ["Je", "en\u00b7ger", "man", "das", "Fleisch", "ans", "Kreuz", "ge\u00b7bun\u00b7den", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PIS", "ART", "NN", "APPRART", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}