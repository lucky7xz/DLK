{"textgrid.poem.31019": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "133.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu Quinsay hat es Birnen, die w\u00e4gen auff zehn Pfund;", "tokens": ["Zu", "Quin\u00b7say", "hat", "es", "Bir\u00b7nen", ",", "die", "w\u00e4\u00b7gen", "auff", "zehn", "Pfund", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "PPER", "NN", "$,", "PRELS", "VVFIN", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Es wird davon geschrieben, hats aber auch wol grund?", "tokens": ["Es", "wird", "da\u00b7von", "ge\u00b7schrie\u00b7ben", ",", "hats", "a\u00b7ber", "auch", "wol", "grund", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "VVPP", "$,", "VAFIN", "ADV", "ADV", "ADV", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die Stadt hat hundert Meilen; da\u00df eine solche Stadt,", "tokens": ["Die", "Stadt", "hat", "hun\u00b7dert", "Mei\u00b7len", ";", "da\u00df", "ei\u00b7ne", "sol\u00b7che", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "CARD", "NN", "$.", "KOUS", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ist billich, nach der gr\u00f6sse so grosse Birnen hat.", "tokens": ["Ist", "bil\u00b7lich", ",", "nach", "der", "gr\u00f6s\u00b7se", "so", "gros\u00b7se", "Bir\u00b7nen", "hat", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "APPR", "ART", "ADJA", "ADV", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}