{"textgrid.poem.45248": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[so ist dir erloschen der Musen Gunst]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbso ist dir erloschen der Musen Gunst,", "tokens": ["\u00bb", "so", "ist", "dir", "er\u00b7lo\u00b7schen", "der", "Mu\u00b7sen", "Gunst", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Erlahmt dein ganzes Streben?\u00ab", "tokens": ["Er\u00b7lahmt", "dein", "gan\u00b7zes", "Stre\u00b7ben", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mein Freund, ich treibe die schwere Kunst,", "tokens": ["Mein", "Freund", ",", "ich", "trei\u00b7be", "die", "schwe\u00b7re", "Kunst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In diesen Zeiten zu leben.", "tokens": ["In", "die\u00b7sen", "Zei\u00b7ten", "zu", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}