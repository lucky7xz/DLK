{"textgrid.poem.34138": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Ein Narr in Folio", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdichter sollte ich sein, doch will es leider die Zeit nicht;", "tokens": ["\u00bb", "dich\u00b7ter", "soll\u00b7te", "ich", "sein", ",", "doch", "will", "es", "lei\u00b7der", "die", "Zeit", "nicht", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PPER", "VAINF", "$,", "ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "PTKNEG", "$."], "meter": "+-+--+-+-+-+++", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "W\u00e4re sie, was sie nicht ist, w\u00e4re ich, was ich nicht bin!\u00ab", "tokens": ["W\u00e4\u00b7re", "sie", ",", "was", "sie", "nicht", "ist", ",", "w\u00e4\u00b7re", "ich", ",", "was", "ich", "nicht", "bin", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "$,", "PRELS", "PPER", "PTKNEG", "VAFIN", "$,", "VAFIN", "PPER", "$,", "PWS", "PPER", "PTKNEG", "VAFIN", "$.", "$("], "meter": "---+-+-+-+-+-+", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Schwanger f\u00fchle ich mich, den Heiland k\u00f6nnt' ich geb\u00e4ren,", "tokens": ["Schwan\u00b7ger", "f\u00fch\u00b7le", "ich", "mich", ",", "den", "Hei\u00b7land", "k\u00f6nnt'", "ich", "ge\u00b7b\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "$,", "ART", "NN", "VMFIN", "PPER", "VVPP", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Aber die Stunde ist schlecht, und ich ersticke das Kind.", "tokens": ["A\u00b7ber", "die", "Stun\u00b7de", "ist", "schlecht", ",", "und", "ich", "er\u00b7sti\u00b7cke", "das", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "$,", "KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Schweig mir, Vettel, denn h\u00e4tte der Himmel dich wirklich gesegnet,", "tokens": ["Schweig", "mir", ",", "Vet\u00b7tel", ",", "denn", "h\u00e4t\u00b7te", "der", "Him\u00b7mel", "dich", "wirk\u00b7lich", "ge\u00b7seg\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "KON", "VAFIN", "ART", "NN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Br\u00e4chtest du's freudig zur Welt, fehlten auch Krippe und Stall.", "tokens": ["Br\u00e4ch\u00b7test", "du's", "freu\u00b7dig", "zur", "Welt", ",", "fehl\u00b7ten", "auch", "Krip\u00b7pe", "und", "Stall", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJD", "APPRART", "NN", "$,", "VVFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}}}}}