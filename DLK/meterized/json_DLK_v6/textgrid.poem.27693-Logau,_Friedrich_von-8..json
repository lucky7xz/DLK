{"textgrid.poem.27693": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie kam es, da\u00df, da Job hatt alles eingeb\u00fcsset,", "tokens": ["Wie", "kam", "es", ",", "da\u00df", ",", "da", "Job", "hatt", "al\u00b7les", "ein\u00b7ge\u00b7b\u00fcs\u00b7set", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "KOUS", "$,", "KOUS", "NE", "VAFIN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was ihm ergetzlich war, da\u00df er sein Weib nicht misset?", "tokens": ["Was", "ihm", "er\u00b7getz\u00b7lich", "war", ",", "da\u00df", "er", "sein", "Weib", "nicht", "mis\u00b7set", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Es steht nicht deutlich da, warumb sie \u00fcbrig blieb;", "tokens": ["Es", "steht", "nicht", "deut\u00b7lich", "da", ",", "wa\u00b7rumb", "sie", "\u00fcb\u00b7rig", "blieb", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJD", "PTKVZ", "$,", "PWAV", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Allein ich schliesse fast, er hatte sie nicht lieb.", "tokens": ["Al\u00b7lein", "ich", "schlies\u00b7se", "fast", ",", "er", "hat\u00b7te", "sie", "nicht", "lieb", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", 