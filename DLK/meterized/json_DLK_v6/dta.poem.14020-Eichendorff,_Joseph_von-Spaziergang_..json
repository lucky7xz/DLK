{"dta.poem.14020": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Spaziergang .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Nelke, du reizendes Kind, wie hast du so gar nichts", "tokens": ["Nel\u00b7ke", ",", "du", "rei\u00b7zen\u00b7des", "Kind", ",", "wie", "hast", "du", "so", "gar", "nichts"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "ADJA", "NN", "$,", "PWAV", "VAFIN", "PPER", "ADV", "ADV", "PIS"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Bescheid'nes!", "tokens": ["Be\u00b7schei\u00b7d'\u00b7nes", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-+--", "measure": "dactylic.init"}, "line.3": {"text": "Jauchzende Farben vor Lust flammst du in's trau-", "tokens": ["Jauch\u00b7zen\u00b7de", "Far\u00b7ben", "vor", "Lust", "flammst", "du", "in's", "trau"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "APPR", "NN", "VVFIN", "PPER", "APPRART", "TRUNC"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "rige Gr\u00fcn,", "tokens": ["ri\u00b7ge", "Gr\u00fcn", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "--+", "measure": "anapaest.init"}, "line.5": {"text": "Tief von den eigenen D\u00fcften du selber lustig berauschet", "tokens": ["Tief", "von", "den", "ei\u00b7ge\u00b7nen", "D\u00fcf\u00b7ten", "du", "sel\u00b7ber", "lus\u00b7tig", "be\u00b7rau\u00b7schet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "PPER", "ADV", "ADJD", "VVFIN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Spiele denn, brenne, von dir la\u00df ich berauschen mich", "tokens": ["Spie\u00b7le", "denn", ",", "bren\u00b7ne", ",", "von", "dir", "la\u00df", "ich", "be\u00b7rau\u00b7schen", "mich"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "VVFIN", "$,", "APPR", "PPER", "VVFIN", "PPER", "VVFIN", "PPER"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "gern!", "tokens": ["gern", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-", "measure": "single.down"}}}}}