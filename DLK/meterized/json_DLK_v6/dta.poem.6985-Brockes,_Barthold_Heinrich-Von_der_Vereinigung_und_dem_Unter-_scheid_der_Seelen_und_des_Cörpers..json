{"dta.poem.6985": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Vereinigung und dem Unter-  \n scheid der Seelen und des C\u00f6rpers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Umsonst ists, wenn man Geist und Leib zu mischen meint.", "tokens": ["Um\u00b7sonst", "ists", ",", "wenn", "man", "Geist", "und", "Leib", "zu", "mi\u00b7schen", "meint", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "KOUS", "PIS", "NN", "KON", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie unterscheiden sich stets, ob sie gleich vereint.", "tokens": ["Sie", "un\u00b7ter\u00b7schei\u00b7den", "sich", "stets", ",", "ob", "sie", "gleich", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "$,", "KOUS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nach viel vergeblichem Bem\u00fchn, mu\u00df der Verstand", "tokens": ["Nach", "viel", "ver\u00b7geb\u00b7li\u00b7chem", "Be\u00b7m\u00fchn", ",", "mu\u00df", "der", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie sehr die Zweifel auch gewohnt, mit ihm zu spielen;", "tokens": ["Wie", "sehr", "die", "Zwei\u00b7fel", "auch", "ge\u00b7wohnt", ",", "mit", "ihm", "zu", "spie\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ADV", "VVPP", "$,", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch seinen eignen Adel f\u00fchlen.", "tokens": ["Doch", "sei\u00b7nen", "eig\u00b7nen", "A\u00b7del", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Empfinden wir denn nicht, und ist uns nicht bekannt", "tokens": ["Emp\u00b7fin\u00b7den", "wir", "denn", "nicht", ",", "und", "ist", "uns", "nicht", "be\u00b7kannt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "PTKNEG", "$,", "KON", "VAFIN", "PPER", "PTKNEG", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Jhr unvergleichlicher Verband?", "tokens": ["Ihr", "un\u00b7ver\u00b7gleich\u00b7li\u00b7cher", "Ver\u00b7band", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wird man die Ordnung, die so sch\u00f6n", "tokens": ["Wird", "man", "die", "Ord\u00b7nung", ",", "die", "so", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PIS", "ART", "NN", "$,", "PRELS", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und wunderbar mit Recht zu nennen,", "tokens": ["Und", "wun\u00b7der\u00b7bar", "mit", "Recht", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die Regeln, die nicht zu ver\u00e4ndern stehn,", "tokens": ["Die", "Re\u00b7geln", ",", "die", "nicht", "zu", "ver\u00b7\u00e4n\u00b7dern", "stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PTKNEG", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Den innerlich-geheimen Band erkennen;", "tokens": ["Den", "in\u00b7ner\u00b7lich\u00b7ge\u00b7hei\u00b7men", "Band", "er\u00b7ken\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Wodurch, ob Seel und Leib sich gleich verbinden,", "tokens": ["Wo\u00b7durch", ",", "ob", "Seel", "und", "Leib", "sich", "gleich", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "NN", "KON", "NN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Wir einen Unterscheid doch zwischen ihnen finden?", "tokens": ["Wir", "ei\u00b7nen", "Un\u00b7ter\u00b7scheid", "doch", "zwi\u00b7schen", "ih\u00b7nen", "fin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}