{"dta.poem.3946": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Vi.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Gleich, da mein Fu\u00df zu Pferde steigt,", "tokens": ["Gleich", ",", "da", "mein", "Fu\u00df", "zu", "Pfer\u00b7de", "steigt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird mir ein Briefgen \u00fcberreicht;", "tokens": ["Wird", "mir", "ein", "Brief\u00b7gen", "\u00fc\u00b7berr\u00b7eicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und eh mein Gaul von dannen lief,", "tokens": ["Und", "eh", "mein", "Gaul", "von", "dan\u00b7nen", "lief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da kriegt ich den Gevatter-Brief.", "tokens": ["Da", "kriegt", "ich", "den", "Ge\u00b7vat\u00b7ter\u00b7Brief", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Antwort kriegst du weiter nicht,", "tokens": ["Die", "Ant\u00b7wort", "kriegst", "du", "wei\u00b7ter", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bis da\u00df die Hand das Siegel bricht,", "tokens": ["Bis", "da\u00df", "die", "Hand", "das", "Sie\u00b7gel", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Denn wirst du allzu deutlich sehen,", "tokens": ["Denn", "wirst", "du", "all\u00b7zu", "deut\u00b7lich", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKA", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Es wird kein Wort darinnen stehen.", "tokens": ["Es", "wird", "kein", "Wort", "da\u00b7rin\u00b7nen", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Bin ich zum schreiben heute faul,", "tokens": ["Bin", "ich", "zum", "schrei\u00b7ben", "heu\u00b7te", "faul", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "So komm ich morgen samt dem Gaul.", "tokens": ["So", "komm", "ich", "mor\u00b7gen", "samt", "dem", "Gaul", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}