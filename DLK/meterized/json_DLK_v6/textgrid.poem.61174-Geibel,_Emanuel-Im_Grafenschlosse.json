{"textgrid.poem.61174": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Im Grafenschlosse", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie waren alle in den Forst hinaus,", "tokens": ["Sie", "wa\u00b7ren", "al\u00b7le", "in", "den", "Forst", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "APPR", "ART", "NN", "APZR", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Hirsch mit B\u00fcchs' und Messer zu erlegen;", "tokens": ["Den", "Hirsch", "mit", "B\u00fcchs'", "und", "Mes\u00b7ser", "zu", "er\u00b7le\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich sa\u00df allein im alten Grafenhaus", "tokens": ["Ich", "sa\u00df", "al\u00b7lein", "im", "al\u00b7ten", "Gra\u00b7fen\u00b7haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und harrt' im Saal der J\u00e4gerschar entgegen.", "tokens": ["Und", "harrt'", "im", "Saal", "der", "J\u00e4\u00b7ger\u00b7schar", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein fahles Sp\u00e4trot flo\u00df ged\u00e4mpften Lichts", "tokens": ["Ein", "fah\u00b7les", "Sp\u00e4t\u00b7rot", "flo\u00df", "ge\u00b7d\u00e4mpf\u00b7ten", "Lichts"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Auf W\u00e4nd' und Hausrat durch die engen Scheiben,", "tokens": ["Auf", "W\u00e4nd'", "und", "Haus\u00b7rat", "durch", "die", "en\u00b7gen", "Schei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Rings Totenstill' umher! Ich h\u00f6rte nichts,", "tokens": ["Rings", "To\u00b7ten\u00b7still'", "um\u00b7her", "!", "Ich", "h\u00f6r\u00b7te", "nichts", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKVZ", "$.", "PPER", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Als vorn im Hof den Zugwind in den Eiben.", "tokens": ["Als", "vorn", "im", "Hof", "den", "Zug\u00b7wind", "in", "den", "Ei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPRART", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Spiegel rings, in dumpfes Gold gefa\u00dft,", "tokens": ["Die", "Spie\u00b7gel", "rings", ",", "in", "dum\u00b7pfes", "Gold", "ge\u00b7fa\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Laubwerk am Gesims, einst vielbewundert,", "tokens": ["Das", "Laub\u00b7werk", "am", "Ge\u00b7sims", ",", "einst", "viel\u00b7be\u00b7wun\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "ADV", "CARD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die d\u00fcstern Samttapeten, halb verbla\u00dft, -", "tokens": ["Die", "d\u00fcs\u00b7tern", "Samt\u00b7ta\u00b7pe\u00b7ten", ",", "halb", "ver\u00b7bla\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "VVPP", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mich mahnt' es an ein anderes Jahrhundert.", "tokens": ["Mich", "mahnt'", "es", "an", "ein", "an\u00b7de\u00b7res", "Jahr\u00b7hun\u00b7dert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Spieluhr sang ein Lied aus alter Zeit,", "tokens": ["Die", "Spie\u00b7luhr", "sang", "ein", "Lied", "aus", "al\u00b7ter", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ein Liebeslied - jetzt lange schon vergessen -", "tokens": ["Ein", "Lie\u00b7bes\u00b7lied", "jetzt", "lan\u00b7ge", "schon", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ADV", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da dacht' ich derer, die in Lust und Leid", "tokens": ["Da", "dacht'", "ich", "de\u00b7rer", ",", "die", "in", "Lust", "und", "Leid"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PDS", "$,", "PRELS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Bei diesem St\u00fcckchen horchend einst gesessen.", "tokens": ["Bei", "die\u00b7sem", "St\u00fcck\u00b7chen", "hor\u00b7chend", "einst", "ge\u00b7ses\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVPP", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und mit Gestalten f\u00fcllt' ich mir den Saal,", "tokens": ["Und", "mit", "Ge\u00b7stal\u00b7ten", "f\u00fcllt'", "ich", "mir", "den", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "PPER", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die dunkeln Bilder rief ich aus den Rahmen;", "tokens": ["Die", "dun\u00b7keln", "Bil\u00b7der", "rief", "ich", "aus", "den", "Rah\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hin durch die D\u00e4mmrung schwebten sie zumal,", "tokens": ["Hin", "durch", "die", "D\u00e4mm\u00b7rung", "schweb\u00b7ten", "sie", "zu\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In Festesputz die alten Herrn und Damen.", "tokens": ["In", "Fes\u00b7tes\u00b7putz", "die", "al\u00b7ten", "Herrn", "und", "Da\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich sah den Reifrock, das Brokatgewand;", "tokens": ["Ich", "sah", "den", "Rei\u00b7frock", ",", "das", "Bro\u00b7kat\u00b7ge\u00b7wand", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Das war ein hastig fl\u00fcsterndes Bewegen,", "tokens": ["Das", "war", "ein", "has\u00b7tig", "fl\u00fcs\u00b7tern\u00b7des", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ein Drehn! - Da f\u00fchlt' ich pl\u00f6tzlich eine Hand", "tokens": ["Ein", "Drehn", "!", "Da", "f\u00fchlt'", "ich", "pl\u00f6tz\u00b7lich", "ei\u00b7ne", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "CARD", "$.", "$(", "ADV", "VVFIN", "PPER", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Sich kalt wie Eis auf meine Schulter legen.", "tokens": ["Sich", "kalt", "wie", "Eis", "auf", "mei\u00b7ne", "Schul\u00b7ter", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "KOKOM", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich wandte mich - bei Gott, das war kein Wahn! -", "tokens": ["Ich", "wand\u00b7te", "mich", "bei", "Gott", ",", "das", "war", "kein", "Wahn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "PPER", "$(", "APPR", "NN", "$,", "PDS", "VAFIN", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da stand ein Weib mit Z\u00fcgen bleich und steinern,", "tokens": ["Da", "stand", "ein", "Weib", "mit", "Z\u00fc\u00b7gen", "bleich", "und", "stei\u00b7nern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "ADJD", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit schwarzverscho\u00dfnem Schleppkleid angetan,", "tokens": ["Mit", "schwarz\u00b7ver\u00b7scho\u00df\u00b7nem", "Schlepp\u00b7kleid", "an\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Draus ihre Hand hervorsah elfenbeinern.", "tokens": ["Draus", "ih\u00b7re", "Hand", "her\u00b7vor\u00b7sah", "el\u00b7fen\u00b7bei\u00b7nern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sie sah mich an - o dieser Blick voll Leid!", "tokens": ["Sie", "sah", "mich", "an", "o", "die\u00b7ser", "Blick", "voll", "Leid", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$(", "FM", "PDAT", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "O dieses Auges halberloschnes Strahlen!", "tokens": ["O", "die\u00b7ses", "Au\u00b7ges", "hal\u00b7ber\u00b7lo\u00b7schnes", "Strah\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Mir war's, als starrt' ich in die Ewigkeit", "tokens": ["Mir", "wa\u00b7r's", ",", "als", "starrt'", "ich", "in", "die", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "Und in den Abgrund bodenloser Qualen.", "tokens": ["Und", "in", "den", "Ab\u00b7grund", "bo\u00b7den\u00b7lo\u00b7ser", "Qua\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Sie winkt' und schritt. Nicht h\u00f6rt' ich ihren Fu\u00df,", "tokens": ["Sie", "winkt'", "und", "schritt", ".", "Nicht", "h\u00f6rt'", "ich", "ih\u00b7ren", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$.", "PTKNEG", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nicht ihrer Schleppe Saum den Teppich r\u00fchren.", "tokens": ["Nicht", "ih\u00b7rer", "Schlep\u00b7pe", "Saum", "den", "Tep\u00b7pich", "r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie sprach kein Wort, sie sagte keinen Gru\u00df;", "tokens": ["Sie", "sprach", "kein", "Wort", ",", "sie", "sag\u00b7te", "kei\u00b7nen", "Gru\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie winkt', und tonlos sprangen auf die T\u00fcren.", "tokens": ["Sie", "winkt'", ",", "und", "ton\u00b7los", "spran\u00b7gen", "auf", "die", "T\u00fc\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "ADJD", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich folgte stumm. Sie schwebte vor mir her", "tokens": ["Ich", "folg\u00b7te", "stumm", ".", "Sie", "schweb\u00b7te", "vor", "mir", "her"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "PPER", "VVFIN", "APPR", "PPER", "APZR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Durch Prunkgem\u00e4cher, Treppen auf und nieder,", "tokens": ["Durch", "Prunk\u00b7ge\u00b7m\u00e4\u00b7cher", ",", "Trep\u00b7pen", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Durch G\u00e4nge dann und S\u00e4le w\u00fcst und leer -", "tokens": ["Durch", "G\u00e4n\u00b7ge", "dann", "und", "S\u00e4\u00b7le", "w\u00fcst", "und", "leer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "KON", "NN", "VVFIN", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Sie schritt und sah sich um und winkte wieder.", "tokens": ["Sie", "schritt", "und", "sah", "sich", "um", "und", "wink\u00b7te", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PRF", "APPR", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Zum Erkerturm! Es war ein eng Gemach,", "tokens": ["Zum", "Er\u00b7ker\u00b7turm", "!", "Es", "war", "ein", "eng", "Ge\u00b7mach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "PPER", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gew\u00f6lbt und dumpfig, eine d\u00fcstre St\u00e4tte;", "tokens": ["Ge\u00b7w\u00f6lbt", "und", "dump\u00b7fig", ",", "ei\u00b7ne", "d\u00fcst\u00b7re", "St\u00e4t\u00b7te", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Tischchen hier, drauf alter Goldschmuck lag,", "tokens": ["Ein", "Tischchen", "hier", ",", "drauf", "al\u00b7ter", "Gold\u00b7schmuck", "lag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PAV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und hoch und faltig dort ein Himmelbette.", "tokens": ["Und", "hoch", "und", "fal\u00b7tig", "dort", "ein", "Him\u00b7mel\u00b7bet\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dort stand sie still und wies mit wei\u00dfer Hand", "tokens": ["Dort", "stand", "sie", "still", "und", "wies", "mit", "wei\u00b7\u00dfer", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Erst auf den Tisch, dann auf die staub'gen Dielen;", "tokens": ["Erst", "auf", "den", "Tisch", ",", "dann", "auf", "die", "staub'\u00b7gen", "Die\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "lin