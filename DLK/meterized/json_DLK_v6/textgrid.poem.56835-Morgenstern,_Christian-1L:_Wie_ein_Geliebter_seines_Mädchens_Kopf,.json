{"textgrid.poem.56835": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie ein Geliebter seines M\u00e4dchens Kopf,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ein Geliebter seines M\u00e4dchens Kopf,", "tokens": ["Wie", "ein", "Ge\u00b7lieb\u00b7ter", "sei\u00b7nes", "M\u00e4d\u00b7chens", "Kopf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "den s\u00fc\u00dfen Kopf mit seiner Welt von Gl\u00fcck,", "tokens": ["den", "s\u00fc\u00b7\u00dfen", "Kopf", "mit", "sei\u00b7ner", "Welt", "von", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "in seine beiden armen H\u00e4nde nimmt,", "tokens": ["in", "sei\u00b7ne", "bei\u00b7den", "ar\u00b7men", "H\u00e4n\u00b7de", "nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "so fa\u00df ich deinen Fr\u00fchlingskopf, Natur,", "tokens": ["so", "fa\u00df", "ich", "dei\u00b7nen", "Fr\u00fch\u00b7lings\u00b7kopf", ",", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "dein \u00fcberschw\u00e4nglich holdes Maienhaupt,", "tokens": ["dein", "\u00fc\u00b7bersc\u00b7hw\u00e4ng\u00b7lich", "hol\u00b7des", "Mai\u00b7en\u00b7haupt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "in meine armen, schlichten Menschenh\u00e4nde,", "tokens": ["in", "mei\u00b7ne", "ar\u00b7men", ",", "schlich\u00b7ten", "Men\u00b7schen\u00b7h\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "und, tief erregt, versink ich stumm in dich,", "tokens": ["und", ",", "tief", "er\u00b7regt", ",", "ver\u00b7sink", "ich", "stumm", "in", "dich", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "VVPP", "$,", "VVFIN", "PPER", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "indes du l\u00e4chelnd mir ins Auge schaust,", "tokens": ["in\u00b7des", "du", "l\u00e4\u00b7chelnd", "mir", "ins", "Au\u00b7ge", "schaust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADJD", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "und stammle leis dir das Bekenntnis zu:", "tokens": ["und", "stamm\u00b7le", "leis", "dir", "das", "Be\u00b7kennt\u00b7nis", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Vor so viel Sch\u00f6nheit schweigt mein tiefstes Lied.", "tokens": ["Vor", "so", "viel", "Sch\u00f6n\u00b7heit", "schweigt", "mein", "tiefs\u00b7tes", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}}}}