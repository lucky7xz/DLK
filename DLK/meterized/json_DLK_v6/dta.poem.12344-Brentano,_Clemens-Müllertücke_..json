{"dta.poem.12344": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "M\u00fcllert\u00fccke .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ging ein M\u00fcller wohl \u00fcbers Feld,               ", "tokens": ["Es", "ging", "ein", "M\u00fcl\u00b7ler", "wohl", "\u00fc\u00b7bers", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NE", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der hatt' einen Beutel und hatt' kein Geld,", "tokens": ["Der", "hatt'", "ei\u00b7nen", "Beu\u00b7tel", "und", "hatt'", "kein", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "KON", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Er wird es wohl bekommen.", "tokens": ["Er", "wird", "es", "wohl", "be\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und als er in den gr\u00fcnen Wald kam,", "tokens": ["Und", "als", "er", "in", "den", "gr\u00fc\u00b7nen", "Wald", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Drey M\u00f6rder unter dem Weidenbaum stahn,", "tokens": ["Drey", "M\u00f6r\u00b7der", "un\u00b7ter", "dem", "Wei\u00b7den\u00b7baum", "stahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die hatten drey gro\u00dfe Messer.", "tokens": ["Die", "hat\u00b7ten", "drey", "gro\u00b7\u00dfe", "Mes\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "CARD", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Der eine zog seinen Beutel heraus,", "tokens": ["Der", "ei\u00b7ne", "zog", "sei\u00b7nen", "Beu\u00b7tel", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Drey hundert Thaler zahlt er draus:", "tokens": ["Drey", "hun\u00b7dert", "Tha\u00b7ler", "zahlt", "er", "draus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "CARD", "NN", "VVFIN", "PPER", "PAV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "\u201enimm hin f\u00fcr Weib und Kinder.\u201c", "tokens": ["\u201e", "nimm", "hin", "f\u00fcr", "Weib", "und", "Kin\u00b7der", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "ADV", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "---+-+-", "measure": "unknown.measure.di"}}, "stanza.4": {"line.1": {"text": "Der M\u00fcller dacht in seinem Sinn,", "tokens": ["Der", "M\u00fcl\u00b7ler", "dacht", "in", "sei\u00b7nem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es w\u00e4r zu wenig f\u00fcr Weib und Kind:", "tokens": ["Es", "w\u00e4r", "zu", "we\u00b7nig", "f\u00fcr", "Weib", "und", "Kind", ":"], "token_info": ["word"