{"textgrid.poem.49422": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "[es ist das selige Bangen]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist das selige Bangen,", "tokens": ["Es", "ist", "das", "se\u00b7li\u00b7ge", "Ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Es ist das m\u00fcde Umfangen,", "tokens": ["Es", "ist", "das", "m\u00fc\u00b7de", "Um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Schauer im d\u00e4mmernden Wald,", "tokens": ["Der", "Schau\u00b7er", "im", "d\u00e4m\u00b7mern\u00b7den", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Der Winde schmeichelnd Umschlingen,", "tokens": ["Der", "Win\u00b7de", "schmei\u00b7chelnd", "Um\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wann vom grauen Gezweige das Singen", "tokens": ["Wann", "vom", "grau\u00b7en", "Ge\u00b7zwei\u00b7ge", "das", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Der kleinen Stimmen erschallt.", "tokens": ["Der", "klei\u00b7nen", "Stim\u00b7men", "er\u00b7schallt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "O dies zarte Zirpen und Girren,", "tokens": ["O", "dies", "zar\u00b7te", "Zir\u00b7pen", "und", "Gir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDS", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Dies junge Gezwitscher und Schwirren,", "tokens": ["Dies", "jun\u00b7ge", "Ge\u00b7zwit\u00b7scher", "und", "Schwir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Klingt hold wie Gr\u00e4ser im Wind,", "tokens": ["Klingt", "hold", "wie", "Gr\u00e4\u00b7ser", "im", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KOKOM", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Als ob \u00fcber blanken Kieseln", "tokens": ["Als", "ob", "\u00fc\u00b7ber", "blan\u00b7ken", "Kie\u00b7seln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit heimlichem Rauschen und Rieseln", "tokens": ["Mit", "heim\u00b7li\u00b7chem", "Rau\u00b7schen", "und", "Rie\u00b7seln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Das murmelnde Wasser verrinnt.", "tokens": ["Das", "mur\u00b7meln\u00b7de", "Was\u00b7ser", "ver\u00b7rinnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Die Seele, die lebt im Zagen", "tokens": ["Die", "See\u00b7le", ",", "die", "lebt", "im", "Za\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der leise schlummernden Klagen,", "tokens": ["Der", "lei\u00b7se", "schlum\u00b7mern\u00b7den", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ist es die unsere? sag!", "tokens": ["Ist", "es", "die", "un\u00b7se\u00b7re", "?", "sag", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "PPOSAT", "$.", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Die meine ja und die deine,", "tokens": ["Die", "mei\u00b7ne", "ja", "und", "die", "dei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "KON", "ART", "PPOSAT", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die so mit stillem Geweine", "tokens": ["Die", "so", "mit", "stil\u00b7lem", "Ge\u00b7wei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Verhaucht im scheidenden Tag.", "tokens": ["Ver\u00b7haucht", "im", "schei\u00b7den\u00b7den", "Tag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}