{"textgrid.poem.32742": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Grablied", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlaf, Schwester, sanft im Erdenscho\u00df!", "tokens": ["Schlaf", ",", "Schwes\u00b7ter", ",", "sanft", "im", "Er\u00b7den\u00b7scho\u00df", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du bist des m\u00fcden Jammers los.", "tokens": ["Du", "bist", "des", "m\u00fc\u00b7den", "Jam\u00b7mers", "los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir sahen deinen Thr\u00e4nen zu,", "tokens": ["Wir", "sa\u00b7hen", "dei\u00b7nen", "Thr\u00e4\u00b7nen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und w\u00fcnschten dir des Grabes Ruh.", "tokens": ["Und", "w\u00fcnschten", "dir", "des", "Gra\u00b7bes", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Nun nahe sich der falsche Mann,", "tokens": ["Nun", "na\u00b7he", "sich", "der", "fal\u00b7sche", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und seh' die blassen Wangen an!", "tokens": ["Und", "seh'", "die", "blas\u00b7sen", "Wan\u00b7gen", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und seh' dies Herz, das ohne Trug", "tokens": ["Und", "seh'", "dies", "Herz", ",", "das", "oh\u00b7ne", "Trug"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PDS", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm noch im Todeskampfe schlug!", "tokens": ["Ihm", "noch", "im", "To\u00b7des\u00b7kamp\u00b7fe", "schlug", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und jeder Falsche m\u00fcss' es schaun,", "tokens": ["Und", "je\u00b7der", "Fal\u00b7sche", "m\u00fcss'", "es", "schaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und f\u00fchlen Seelenangst und Graun!", "tokens": ["Und", "f\u00fch\u00b7len", "See\u00b7len\u00b7angst", "und", "Graun", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und dieser Unschuld L\u00e4cheln sei", "tokens": ["Und", "die\u00b7ser", "Un\u00b7schuld", "L\u00e4\u00b7cheln", "sei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erweckung ihm zur sp\u00e4ten Reu'!", "tokens": ["Er\u00b7we\u00b7ckung", "ihm", "zur", "sp\u00e4\u00b7ten", "Reu'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du aber ruh in deiner Gruft,", "tokens": ["Du", "a\u00b7ber", "ruh", "in", "dei\u00b7ner", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bis dich der letzte Morgen ruft,", "tokens": ["Bis", "dich", "der", "letz\u00b7te", "Mor\u00b7gen", "ruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis sanft, von Thr\u00e4nen unentstellt,", "tokens": ["Bis", "sanft", ",", "von", "Thr\u00e4\u00b7nen", "un\u00b7ent\u00b7stellt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich wieder dein Gesicht erhellt!", "tokens": ["Sich", "wie\u00b7der", "dein", "Ge\u00b7sicht", "er\u00b7hellt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Indes, o Schwester, pflanzen wir", "tokens": ["In\u00b7des", ",", "o", "Schwes\u00b7ter", ",", "pflan\u00b7zen", "wir"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "FM", "NN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zween junge Rosenstr\u00e4uche dir;", "tokens": ["Zween", "jun\u00b7ge", "Ro\u00b7sen\u00b7str\u00e4u\u00b7che", "dir", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und eilen, dulden wir wie du,", "tokens": ["Und", "ei\u00b7len", ",", "dul\u00b7den", "wir", "wie", "du", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "PPER", "PWAV", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Thr\u00e4nen ihren Schatten zu.", "tokens": ["Mit", "Thr\u00e4\u00b7nen", "ih\u00b7ren", "Schat\u00b7ten", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}