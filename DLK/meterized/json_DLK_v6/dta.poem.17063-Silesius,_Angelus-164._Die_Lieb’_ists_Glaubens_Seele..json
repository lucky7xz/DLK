{"dta.poem.17063": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "164.  Die Lieb\u2019 ists Glaubens Seele.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Der Glaub allein ist Todt/ Er kan nicht eher Leben/", "tokens": ["Der", "Glaub", "al\u00b7lein", "ist", "Todt", "/", "Er", "kan", "nicht", "e\u00b7her", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "NN", "$(", "PPER", "VMFIN", "PTKNEG", "ADV", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bi\u00df da\u00df jhm seine Seel die Liebe wird gegeben.", "tokens": ["Bi\u00df", "da\u00df", "jhm", "sei\u00b7ne", "Seel", "die", "Lie\u00b7be", "wird", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "PPOSAT", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}