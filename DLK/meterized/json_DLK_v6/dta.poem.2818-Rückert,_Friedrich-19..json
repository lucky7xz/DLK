{"dta.poem.2818": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hat die Unendlichkeit nicht R\u00e4ume ungeheuer?", "tokens": ["Hat", "die", "Un\u00b7end\u00b7lich\u00b7keit", "nicht", "R\u00e4u\u00b7me", "un\u00b7ge\u00b7heu\u00b7er", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKNEG", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch \u00fcberall ist Raum gespart, als sei er theuer.", "tokens": ["Doch", "\u00fc\u00b7be\u00b7rall", "ist", "Raum", "ge\u00b7spart", ",", "als", "sei", "er", "theu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NN", "VVPP", "$,", "KOKOM", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Drang des Lebens, wenn er sich w\u00e4r' \u00fcberlassen,", "tokens": ["Der", "Drang", "des", "Le\u00b7bens", ",", "wenn", "er", "sich", "w\u00e4r'", "\u00fc\u00b7ber\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "KOUS", "PPER", "PRF", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Selbst die Unendlichkeit verm\u00f6cht' ihn nicht zu fassen.", "tokens": ["Selbst", "die", "Un\u00b7end\u00b7lich\u00b7keit", "ver\u00b7m\u00f6cht'", "ihn", "nicht", "zu", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Drum ist des Lebens F\u00fcll' ins Engeste gezw\u00e4ngt,", "tokens": ["Drum", "ist", "des", "Le\u00b7bens", "F\u00fcll'", "ins", "En\u00b7ges\u00b7te", "ge\u00b7zw\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil \u00fcberall ihr Trieb ins Weitere sie dr\u00e4ngt.", "tokens": ["Weil", "\u00fc\u00b7be\u00b7rall", "ihr", "Trieb", "ins", "Wei\u00b7te\u00b7re", "sie", "dr\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "APPRART", "ADJA", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Zur Raumersparung hat Baumeisterin Natur", "tokens": ["Zur", "Rau\u00b7mer\u00b7spa\u00b7rung", "hat", "Bau\u00b7meis\u00b7te\u00b7rin", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Bienenvolk gelehrt sechseckig bauen nur,", "tokens": ["Das", "Bie\u00b7nen\u00b7volk", "ge\u00b7lehrt", "sechs\u00b7ec\u00b7kig", "bau\u00b7en", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "ADJD", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Da\u00df Zell' an Zelle pa\u00dft und aller Zellen Enge", "tokens": ["Da\u00df", "Zell'", "an", "Zel\u00b7le", "pa\u00dft", "und", "al\u00b7ler", "Zel\u00b7len", "En\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "NN", "VVFIN", "KON", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zur Noth bequem nur fa\u00dft die arbeitselige Menge.", "tokens": ["Zur", "Noth", "be\u00b7quem", "nur", "fa\u00dft", "die", "ar\u00b7beit\u00b7se\u00b7li\u00b7ge", "Men\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Verkr\u00fcppelt zwitterhaft sind drin die flei\u00df'gen Horden,", "tokens": ["Ver\u00b7kr\u00fcp\u00b7pelt", "zwit\u00b7ter\u00b7haft", "sind", "drin", "die", "flei\u00df'\u00b7gen", "Hor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von denen jeder frei sonst w\u00e4r' ein Weisel worden.", "tokens": ["Von", "de\u00b7nen", "je\u00b7der", "frei", "sonst", "w\u00e4r'", "ein", "Wei\u00b7sel", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "ADJD", "ADV", "VAFIN", "ART", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "So w\u00fcrd' ein Bauer, wenn ihn nicht von allen Seiten", "tokens": ["So", "w\u00fcrd'", "ein", "Bau\u00b7er", ",", "wenn", "ihn", "nicht", "von", "al\u00b7len", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "KOUS", "PPER", "PTKNEG", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Nachbarn zw\u00e4ngten, sich als Patriarch ausbreiten.", "tokens": ["Die", "Nach\u00b7barn", "zw\u00e4ng\u00b7ten", ",", "sich", "als", "Pat\u00b7ri\u00b7arch", "aus\u00b7brei\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRF", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Mit rascher Fruchtbarkeit hat er ein Land besetzt,", "tokens": ["Mit", "ra\u00b7scher", "Frucht\u00b7bar\u00b7keit", "hat", "er", "ein", "Land", "be\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Bis die Bev\u00f6lkerung sich selber Schranken setzt.", "tokens": ["Bis", "die", "Be\u00b7v\u00f6l\u00b7ke\u00b7rung", "sich", "sel\u00b7ber", "Schran\u00b7ken", "setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Alswie im dichten Wald von tausend Saamenk\u00f6rnern", "tokens": ["Als\u00b7wie", "im", "dich\u00b7ten", "Wald", "von", "tau\u00b7send", "Saa\u00b7men\u00b7k\u00f6r\u00b7nern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPRART", "ADJA", "NN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur eines sich empor arbeitet aus den D\u00f6rnern;", "tokens": ["Nur", "ei\u00b7nes", "sich", "em\u00b7por", "ar\u00b7bei\u00b7tet", "aus", "den", "D\u00f6r\u00b7nern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "PRF", "PTKVZ", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Doch wird er ausgehaun, mag eine Tanne streun", "tokens": ["Doch", "wird", "er", "aus\u00b7ge\u00b7haun", ",", "mag", "ei\u00b7ne", "Tan\u00b7ne", "streun"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "VVINF", "$,", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Saamen weit umher, und bald den Wald erneun.", "tokens": ["Die", "Saa\u00b7men", "weit", "um\u00b7her", ",", "und", "bald", "den", "Wald", "er\u00b7neun", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$,", "KON", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Der Baum des Lebens ist von Saamen ganz erf\u00fcllt,", "tokens": ["Der", "Baum", "des", "Le\u00b7bens", "ist", "von", "Saa\u00b7men", "ganz", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und \u00fcberall ein Trieb im andern eingeh\u00fcllt.", "tokens": ["Und", "\u00fc\u00b7be\u00b7rall", "ein", "Trieb", "im", "an\u00b7dern", "ein\u00b7ge\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPRART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Die Knospe wartet nur auf Platz hervorzudringen,", "tokens": ["Die", "Knos\u00b7pe", "war\u00b7tet", "nur", "auf", "Platz", "her\u00b7vor\u00b7zu\u00b7drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sobald die alte weicht, wird gleich die neu' entspringen.", "tokens": ["So\u00b7bald", "die", "al\u00b7te", "weicht", ",", "wird", "gleich", "die", "neu'", "ent\u00b7sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "VVFIN", "$,", "VAFIN", "ADV", "ART", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Wie an der Eidechs', ob du Fu\u00df ihr oder Hand", "tokens": ["Wie", "an", "der", "Ei\u00b7dechs'", ",", "ob", "du", "Fu\u00df", "ihr", "o\u00b7der", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "NN", "PPOSAT", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Abhiebest, Hand und Fu\u00df am selben Ort entstand;", "tokens": ["Ab\u00b7hie\u00b7best", ",", "Hand", "und", "Fu\u00df", "am", "sel\u00b7ben", "Ort", "ent\u00b7stand", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Alsob die Glieder schon verborgen fertig lauern,", "tokens": ["Al\u00b7sob", "die", "Glie\u00b7der", "schon", "ver\u00b7bor\u00b7gen", "fer\u00b7tig", "lau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "VVPP", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und k\u00f6nnen nur nicht vor, so lang' die alten dauern.", "tokens": ["Und", "k\u00f6n\u00b7nen", "nur", "nicht", "vor", ",", "so", "lang'", "die", "al\u00b7ten", "dau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PTKNEG", "PTKVZ", "$,", "ADV", "ADV", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.15": {"line.1": {"text": "So \u00fcberquillend ist auch Menschenf\u00e4higkeit;", "tokens": ["So", "\u00fc\u00b7berq\u00b7uil\u00b7lend", "ist", "auch", "Men\u00b7schen\u00b7f\u00e4\u00b7hig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gib Spielraum ihr, sie tritt hervor zu rechter Zeit.", "tokens": ["Gib", "Spiel\u00b7raum", "ihr", ",", "sie", "tritt", "her\u00b7vor", "zu", "rech\u00b7ter", "Zeit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "PPER", "$,", "PPER", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.16": {"line.1": {"text": "Drum f\u00fcge dich der Zeit, erf\u00fclle deinen Platz,", "tokens": ["Drum", "f\u00fc\u00b7ge", "dich", "der", "Zeit", ",", "er\u00b7f\u00fcl\u00b7le", "dei\u00b7nen", "Platz", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und r\u00e4um' ihn auch getrost, es fehlt nicht an Ersatz.", "tokens": ["Und", "r\u00e4um'", "ihn", "auch", "ge\u00b7trost", ",", "es", "fehlt", "nicht", "an", "Er\u00b7satz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "VVPP", "$,", "PPER", "VVFIN", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}