{"textgrid.poem.26844": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: An der Quelle sa\u00df der Knabe,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An der Quelle sa\u00df der Knabe,", "tokens": ["An", "der", "Quel\u00b7le", "sa\u00df", "der", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blumen wand er sich zum Kranz,", "tokens": ["Blu\u00b7men", "wand", "er", "sich", "zum", "Kranz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und er sah sie fortgerissen,", "tokens": ["Und", "er", "sah", "sie", "fort\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Treiben in der Wellen Tanz.", "tokens": ["Trei\u00b7ben", "in", "der", "Wel\u00b7len", "Tanz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbund so fliehen meine Tage", "tokens": ["\u00bb", "und", "so", "flie\u00b7hen", "mei\u00b7ne", "Ta\u00b7ge"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie die Quelle rastlos hin!", "tokens": ["Wie", "die", "Quel\u00b7le", "rast\u00b7los", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und so bleichet meine Jugend,", "tokens": ["Und", "so", "blei\u00b7chet", "mei\u00b7ne", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie die Kr\u00e4nze schnell verbl\u00fchn!", "tokens": ["Wie", "die", "Kr\u00e4n\u00b7ze", "schnell", "ver\u00b7bl\u00fchn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Fraget nicht, warum ich traure", "tokens": ["Fra\u00b7get", "nicht", ",", "wa\u00b7rum", "ich", "trau\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In des Lebens Bl\u00fctenzeit!", "tokens": ["In", "des", "Le\u00b7bens", "Bl\u00fc\u00b7ten\u00b7zeit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles freuet sich und hoffet,", "tokens": ["Al\u00b7les", "freu\u00b7et", "sich", "und", "hof\u00b7fet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn der Fr\u00fchling sich erneut.", "tokens": ["Wenn", "der", "Fr\u00fch\u00b7ling", "sich", "er\u00b7neut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber diese tausend Stimmen", "tokens": ["A\u00b7ber", "die\u00b7se", "tau\u00b7send", "Stim\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDAT", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der erwachenden Natur", "tokens": ["Der", "er\u00b7wa\u00b7chen\u00b7den", "Na\u00b7tur"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wecken in dem tiefen Busen", "tokens": ["We\u00b7cken", "in", "dem", "tie\u00b7fen", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mir den schweren Kummer nur.", "tokens": ["Mir", "den", "schwe\u00b7ren", "Kum\u00b7mer", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Was soll mir die Freude frommen,", "tokens": ["Was", "soll", "mir", "die", "Freu\u00b7de", "from\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ART", "NN", "ADJA", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die der sch\u00f6ne Lenz mir beut?", "tokens": ["Die", "der", "sch\u00f6\u00b7ne", "Lenz", "mir", "beut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine nur ists, die ich suche,", "tokens": ["Ei\u00b7ne", "nur", "ists", ",", "die", "ich", "su\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VAFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie ist nah und ewig weit.", "tokens": ["Sie", "ist", "nah", "und", "e\u00b7wig", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sehnend breit ich meine Arme", "tokens": ["Seh\u00b7nend", "breit", "ich", "mei\u00b7ne", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nach dem teuren Schattenbild,", "tokens": ["Nach", "dem", "teu\u00b7ren", "Schat\u00b7ten\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach, ich kann es nicht erreichen,", "tokens": ["Ach", ",", "ich", "kann", "es", "nicht", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und das Herz bleibt ungestillt!", "tokens": ["Und", "das", "Herz", "bleibt", "un\u00b7ge\u00b7stillt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Komm herab, du sch\u00f6ne Holde,", "tokens": ["Komm", "her\u00b7ab", ",", "du", "sch\u00f6\u00b7ne", "Hol\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und verla\u00df dein stolzes Schlo\u00df!", "tokens": ["Und", "ver\u00b7la\u00df", "dein", "stol\u00b7zes", "Schlo\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Blumen, die der Lenz geboren,", "tokens": ["Blu\u00b7men", ",", "die", "der", "Lenz", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Streu ich dir in deinen Scho\u00df.", "tokens": ["Streu", "ich", "dir", "in", "dei\u00b7nen", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Horch, der Hain erschallt von Liedern,", "tokens": ["Horch", ",", "der", "Hain", "er\u00b7schallt", "von", "Lie\u00b7dern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die Quelle rieselt klar!", "tokens": ["Und", "die", "Quel\u00b7le", "rie\u00b7selt", "klar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Raum ist in der kleinsten H\u00fctte", "tokens": ["Raum", "ist", "in", "der", "kleins\u00b7ten", "H\u00fct\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "F\u00fcr ein gl\u00fccklich liebend Paar.\u00ab", "tokens": ["F\u00fcr", "ein", "gl\u00fcck\u00b7lich", "lie\u00b7bend", "Paar", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJD", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}