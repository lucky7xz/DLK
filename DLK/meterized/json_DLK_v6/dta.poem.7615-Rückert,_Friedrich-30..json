{"dta.poem.7615": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des Mondes Geisterlicht macht fremd auch das Bekannte,", "tokens": ["Des", "Mon\u00b7des", "Geis\u00b7ter\u00b7licht", "macht", "fremd", "auch", "das", "Be\u00b7kann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn fremde Schauder selbst ein Blick der Sonne bannte.", "tokens": ["Wenn", "frem\u00b7de", "Schau\u00b7der", "selbst", "ein", "Blick", "der", "Son\u00b7ne", "bann\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Drum sieh ein fremd Gefild im Licht der Sonne nur,", "tokens": ["Drum", "sieh", "ein", "fremd", "Ge\u00b7fild", "im", "Licht", "der", "Son\u00b7ne", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJD", "NN", "APPRART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und lieblich fremd im Glanz des Monds die eigne Flur.", "tokens": ["Und", "lieb\u00b7lich", "fremd", "im", "Glanz", "des", "Monds", "die", "eig\u00b7ne", "Flur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "APPRART", "NN", "ART", "NN", "ART", "ADJA", "NN", "$."],