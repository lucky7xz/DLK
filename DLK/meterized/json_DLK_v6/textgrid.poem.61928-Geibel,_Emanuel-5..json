{"textgrid.poem.61928": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Endlich hab' ich's \u00fcberwunden,", "tokens": ["End\u00b7lich", "hab'", "ich's", "\u00fc\u00b7berw\u00b7un\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was so wild in mir gegl\u00fcht,", "tokens": ["Was", "so", "wild", "in", "mir", "ge\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die goldnen Fr\u00fchlingsstunden", "tokens": ["Und", "die", "gold\u00b7nen", "Fr\u00fch\u00b7lings\u00b7stun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gr\u00fc\u00dft gel\u00e4utert mein Gem\u00fct.", "tokens": ["Gr\u00fc\u00dft", "ge\u00b7l\u00e4u\u00b7tert", "mein", "Ge\u00b7m\u00fct", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch im freigewordnen Busen", "tokens": ["Doch", "im", "frei\u00b7ge\u00b7word\u00b7nen", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blieb dein Wesen mir gepr\u00e4gt", "tokens": ["Blieb", "dein", "We\u00b7sen", "mir", "ge\u00b7pr\u00e4gt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Heiter, wie das Bild der Musen,", "tokens": ["Hei\u00b7ter", ",", "wie", "das", "Bild", "der", "Mu\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das mich sch\u00f6pferisch bewegt.", "tokens": ["Das", "mich", "sch\u00f6p\u00b7fe\u00b7risch", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "All mein Tag geh\u00f6rt dem Werke", "tokens": ["All", "mein", "Tag", "ge\u00b7h\u00f6rt", "dem", "Wer\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wieder und die Nacht der Ruh',", "tokens": ["Wie\u00b7der", "und", "die", "Nacht", "der", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ART", "NN", "ART", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Doch es quoll mir junge St\u00e4rke", "tokens": ["Doch", "es", "quoll", "mir", "jun\u00b7ge", "St\u00e4r\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus der Brust Gewittern zu.", "tokens": ["Aus", "der", "Brust", "Ge\u00b7wit\u00b7tern", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und so dank' ich dir und lerne", "tokens": ["Und", "so", "dank'", "ich", "dir", "und", "ler\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fromm den G\u00f6tterschlu\u00df verstehn,", "tokens": ["Fromm", "den", "G\u00f6t\u00b7ter\u00b7schlu\u00df", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der dich mir gleich einem Sterne", "tokens": ["Der", "dich", "mir", "gleich", "ei\u00b7nem", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aufgehn lie\u00df und untergehn.", "tokens": ["Auf\u00b7gehn", "lie\u00df", "und", "un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVINF", "VVFIN", "KON", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ach, und doch in manchen Stunden", "tokens": ["Ach", ",", "und", "doch", "in", "man\u00b7chen", "Stun\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KON", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sehnt wie nach verlornem Gl\u00fcck", "tokens": ["Sehnt", "wie", "nach", "ver\u00b7lor\u00b7nem", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich dies Herz nach seinen Wunden,", "tokens": ["Sich", "dies", "Herz", "nach", "sei\u00b7nen", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PDS", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach der s\u00fc\u00dfen Qual zur\u00fcck.", "tokens": ["Nach", "der", "s\u00fc\u00b7\u00dfen", "Qual", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}