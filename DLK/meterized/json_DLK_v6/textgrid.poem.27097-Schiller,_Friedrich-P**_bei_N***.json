{"textgrid.poem.27097": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "P** bei N***", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ganz hypochondrisch bin ich vor Langerweile geworden,", "tokens": ["Ganz", "hy\u00b7po\u00b7chond\u00b7risch", "bin", "ich", "vor", "Lan\u00b7ger\u00b7wei\u00b7le", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "APPR", "NN", "VAPP", "$,"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und ich flie\u00dfe nur fort, weil es so hergebracht ist.", "tokens": ["Und", "ich", "flie\u00b7\u00dfe", "nur", "fort", ",", "weil", "es", "so", "her\u00b7ge\u00b7bracht", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PTKVZ", "$,", "KOUS", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "--+--+---+-+-", "measure": "anapaest.di.plus"}}}}}