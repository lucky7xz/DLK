{"textgrid.poem.25258": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mit lauen Nebeln h\u00fcllt der Sommerabend", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit lauen Nebeln h\u00fcllt der Sommerabend", "tokens": ["Mit", "lau\u00b7en", "Ne\u00b7beln", "h\u00fcllt", "der", "Som\u00b7mer\u00b7a\u00b7bend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des dunkeln Meeres Bucht. Im Hafen tr\u00e4umt", "tokens": ["Des", "dun\u00b7keln", "Mee\u00b7res", "Bucht", ".", "Im", "Ha\u00b7fen", "tr\u00e4umt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "$.", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Kahnkolo\u00df. Die Welle t\u00e4tschelt leise", "tokens": ["Ein", "Kahn\u00b7ko\u00b7lo\u00df", ".", "Die", "Wel\u00b7le", "t\u00e4t\u00b7schelt", "lei\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Geteerte Planken. Ankerketten rasseln.", "tokens": ["Ge\u00b7teer\u00b7te", "Plan\u00b7ken", ".", "An\u00b7ker\u00b7ket\u00b7ten", "ras\u00b7seln", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Verh\u00fcllte Stimmen. An des Kahnes Bug", "tokens": ["Ver\u00b7h\u00fcll\u00b7te", "Stim\u00b7men", ".", "An", "des", "Kah\u00b7nes", "Bug"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Gl\u00fchn zwei Laternen, rot und gr\u00fcn, sie senken", "tokens": ["Gl\u00fchn", "zwei", "La\u00b7ter\u00b7nen", ",", "rot", "und", "gr\u00fcn", ",", "sie", "sen\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "CARD", "NN", "$,", "ADJD", "KON", "ADJD", "$,", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Zwei Feuers\u00e4ulen, zitternd, rot und gr\u00fcn,", "tokens": ["Zwei", "Feu\u00b7er\u00b7s\u00e4u\u00b7len", ",", "zit\u00b7ternd", ",", "rot", "und", "gr\u00fcn", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "VVPP", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Durch schwarze Flut her\u00fcber. Und es summen", "tokens": ["Durch", "schwar\u00b7ze", "Flut", "her\u00b7\u00fc\u00b7ber", ".", "Und", "es", "sum\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADV", "$.", "KON", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Matrosen eine schl\u00e4frig weiche Weise.", "tokens": ["Mat\u00b7ro\u00b7sen", "ei\u00b7ne", "schl\u00e4f\u00b7rig", "wei\u00b7che", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Der Rundreim lautet, wenn ich recht versteh:", "tokens": ["Der", "Rund\u00b7reim", "lau\u00b7tet", ",", "wenn", "ich", "recht", "ver\u00b7steh", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "\u00bbdas Heimatland, das alte Weh", "tokens": ["\u00bb", "das", "Hei\u00b7mat\u00b7land", ",", "das", "al\u00b7te", "Weh"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Versenke du in tiefe See!\u00ab", "tokens": ["Ver\u00b7sen\u00b7ke", "du", "in", "tie\u00b7fe", "See", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du Loderfackel, roter Mars dort oben!", "tokens": ["Du", "Lo\u00b7der\u00b7fa\u00b7ckel", ",", "ro\u00b7ter", "Mars", "dort", "o\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "ADJA", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was winkst du so geheimnisvoll aus Nebeln?", "tokens": ["Was", "winkst", "du", "so", "ge\u00b7heim\u00b7nis\u00b7voll", "aus", "Ne\u00b7beln", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bist du nicht jener Stern, von dem man sagt,", "tokens": ["Bist", "du", "nicht", "je\u00b7ner", "Stern", ",", "von", "dem", "man", "sagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "PDAT", "NN", "$,", "APPR", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein menschengleich Geschlecht bewohne ihn?", "tokens": ["Ein", "men\u00b7schen\u00b7gleich", "Ge\u00b7schlecht", "be\u00b7woh\u00b7ne", "ihn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PPER", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Nur \u00e4lter, weiser, gl\u00fccklicher als wir/", "tokens": ["Nur", "\u00e4l\u00b7ter", ",", "wei\u00b7ser", ",", "gl\u00fcck\u00b7li\u00b7cher", "als", "wir", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJA", "$,", "ADJD", "KOKOM", "PPER", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wir armen, mangelhaften Erdenkinder ...", "tokens": ["Wir", "ar\u00b7men", ",", "man\u00b7gel\u00b7haf\u00b7ten", "Er\u00b7den\u00b7kin\u00b7der", "..."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "\u00bbdas Heimatland, das alte Weh", "tokens": ["\u00bb", "das", "Hei\u00b7mat\u00b7land", ",", "das", "al\u00b7te", "Weh"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Versenke du in tiefe See!\u00ab", "tokens": ["Ver\u00b7sen\u00b7ke", "du", "in", "tie\u00b7fe", "See", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}