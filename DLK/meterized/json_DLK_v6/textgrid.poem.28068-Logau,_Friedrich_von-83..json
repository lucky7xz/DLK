{"textgrid.poem.28068": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "83.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist irgend Tugend wo, ist irgend wo ein Ehre,", "tokens": ["Ist", "ir\u00b7gend", "Tu\u00b7gend", "wo", ",", "ist", "ir\u00b7gend", "wo", "ein", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "PWAV", "$,", "VAFIN", "ADV", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jagt der die Welt frisch nach, bi\u00df da\u00df sie sie zerst\u00f6re;", "tokens": ["Jagt", "der", "die", "Welt", "frisch", "nach", ",", "bi\u00df", "da\u00df", "sie", "sie", "zer\u00b7st\u00f6\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ART", "NN", "ADJD", "PTKVZ", "$,", "KON", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "+--+-++--+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Ist irgend eine Schand, ist irgend eine Schmach,", "tokens": ["Ist", "ir\u00b7gend", "ei\u00b7ne", "Schand", ",", "ist", "ir\u00b7gend", "ei\u00b7ne", "Schmach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die hat bey unsrer Welt hoch Acht und gut Gemach.", "tokens": ["Die", "hat", "bey", "uns\u00b7rer", "Welt", "hoch", "Acht", "und", "gut", "Ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPOSA