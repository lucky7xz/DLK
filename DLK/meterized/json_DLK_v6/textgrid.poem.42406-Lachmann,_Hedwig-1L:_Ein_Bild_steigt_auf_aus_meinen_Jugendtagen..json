{"textgrid.poem.42406": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Bild steigt auf aus meinen Jugendtagen.", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Bild steigt auf aus meinen Jugendtagen.", "tokens": ["Ein", "Bild", "steigt", "auf", "aus", "mei\u00b7nen", "Ju\u00b7gend\u00b7ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Gotteshause stehen Reih an Reihe", "tokens": ["Im", "Got\u00b7tes\u00b7hau\u00b7se", "ste\u00b7hen", "Reih", "an", "Rei\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die M\u00e4nner, Betgew\u00e4nder umgeschlagen,", "tokens": ["Die", "M\u00e4n\u00b7ner", ",", "Bet\u00b7ge\u00b7w\u00e4n\u00b7der", "um\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sich neigend nach geheimnisvollen Riten;", "tokens": ["Sich", "nei\u00b7gend", "nach", "ge\u00b7heim\u00b7nis\u00b7vol\u00b7len", "Ri\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mein Vater unter ihnen singt mit Weihe", "tokens": ["Mein", "Va\u00b7ter", "un\u00b7ter", "ih\u00b7nen", "singt", "mit", "Wei\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Bussgebete der Israeliten.", "tokens": ["Die", "Buss\u00b7ge\u00b7be\u00b7te", "der", "Is\u00b7ra\u00b7e\u00b7li\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sein Haar ist silbern, und das weisse Leinen", "tokens": ["Sein", "Haar", "ist", "sil\u00b7bern", ",", "und", "das", "weis\u00b7se", "Lei\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "VVINF", "$,", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Priesterkleidung fliesst um seine Glieder.", "tokens": ["Der", "Pries\u00b7ter\u00b7klei\u00b7dung", "fliesst", "um", "sei\u00b7ne", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gewaltig dr\u00e4ngen zu dem furchtbar Einen,", "tokens": ["Ge\u00b7wal\u00b7tig", "dr\u00e4n\u00b7gen", "zu", "dem", "furcht\u00b7bar", "Ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "ADJD", "ART", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Licht und Wolken bl\u00e4st vom Firmamente,", "tokens": ["Der", "Licht", "und", "Wol\u00b7ken", "bl\u00e4st", "vom", "Fir\u00b7ma\u00b7men\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und bitten und beschw\u00f6ren immer wieder", "tokens": ["Und", "bit\u00b7ten", "und", "be\u00b7schw\u00f6\u00b7ren", "im\u00b7mer", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "KON", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die klagenden, vertrauenden Akzente.", "tokens": ["Die", "kla\u00b7gen\u00b7den", ",", "ver\u00b7trau\u00b7en\u00b7den", "Ak\u00b7zen\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Dann zu noch reuevollerer Kasteiung", "tokens": ["Dann", "zu", "noch", "reu\u00b7e\u00b7vol\u00b7le\u00b7rer", "Kas\u00b7te\u00b7i\u00b7ung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADV", "ADJA", "NN"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Sinkt in die Knie die ganze Schar der Beter,", "tokens": ["Sinkt", "in", "die", "Knie", "die", "gan\u00b7ze", "Schar", "der", "Be\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um ihrer Busse willen die Befreiung", "tokens": ["Um", "ih\u00b7rer", "Bus\u00b7se", "wil\u00b7len", "die", "Be\u00b7frei\u00b7ung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus S\u00fcnden und dem Joche der Bedr\u00e4nger", "tokens": ["Aus", "S\u00fcn\u00b7den", "und", "dem", "Jo\u00b7che", "der", "Be\u00b7dr\u00e4n\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Erflehend von dem starken Gott der V\u00e4ter.", "tokens": ["Er\u00b7fle\u00b7hend", "von", "dem", "star\u00b7ken", "Gott", "der", "V\u00e4\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Aufrecht steht nur der priesterliche S\u00e4nger.", "tokens": ["Auf\u00b7recht", "steht", "nur", "der", "pries\u00b7ter\u00b7li\u00b7che", "S\u00e4n\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Doch nun mit weit ausgreifender Geb\u00e4rde", "tokens": ["Doch", "nun", "mit", "weit", "aus\u00b7grei\u00b7fen\u00b7der", "Ge\u00b7b\u00e4r\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Beugt er das Knie zum Anruf um Vergebung", "tokens": ["Beugt", "er", "das", "Knie", "zum", "An\u00b7ruf", "um", "Ver\u00b7ge\u00b7bung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und legt sich mit dem Antlitz auf die Erde", "tokens": ["Und", "legt", "sich", "mit", "dem", "Ant\u00b7litz", "auf", "die", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und preist den Namen, der von S\u00fcnden reinigt,", "tokens": ["Und", "preist", "den", "Na\u00b7men", ",", "der", "von", "S\u00fcn\u00b7den", "rei\u00b7nigt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und aus Gebet und Busse und Erhebung", "tokens": ["Und", "aus", "Ge\u00b7bet", "und", "Bus\u00b7se", "und", "Er\u00b7he\u00b7bung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "--+--+--+--", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Klingt seine Stimme wie mit Gott vereinigt.", "tokens": ["Klingt", "sei\u00b7ne", "Stim\u00b7me", "wie", "mit", "Gott", "ver\u00b7ei\u00b7nigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KOKOM", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}