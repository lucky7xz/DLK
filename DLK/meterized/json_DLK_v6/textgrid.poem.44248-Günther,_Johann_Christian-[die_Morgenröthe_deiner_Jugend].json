{"textgrid.poem.44248": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[die Morgenr\u00f6the deiner Jugend]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Morgenr\u00f6the deiner Jugend,", "tokens": ["Die", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "dei\u00b7ner", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du schon so fr\u00fch galantes Kind,", "tokens": ["Du", "schon", "so", "fr\u00fch", "ga\u00b7lan\u00b7tes", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verspricht uns Rosen aller Tugend,", "tokens": ["Ver\u00b7spricht", "uns", "Ro\u00b7sen", "al\u00b7ler", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die jezo noch in Knospen sind,", "tokens": ["Die", "je\u00b7zo", "noch", "in", "Knos\u00b7pen", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "APPR", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und l\u00e4st dich k\u00fcnftig auf der Erden", "tokens": ["Und", "l\u00e4st", "dich", "k\u00fcnf\u00b7tig", "auf", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gewis zur Sch\u00f6nheitssonne werden.", "tokens": ["Ge\u00b7wis", "zur", "Sch\u00f6n\u00b7heits\u00b7son\u00b7ne", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie gl\u00fccklich mag sich der nicht sch\u00e4zen,", "tokens": ["Wie", "gl\u00fcck\u00b7lich", "mag", "sich", "der", "nicht", "sch\u00e4\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VMFIN", "PRF", "ART", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vor de\u00dfen Mund die s\u00fc\u00dfe Frucht", "tokens": ["Vor", "de\u00b7\u00dfen", "Mund", "die", "s\u00fc\u00b7\u00dfe", "Frucht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Der Lippen, die schon jezt erg\u00f6zen,", "tokens": ["Der", "Lip\u00b7pen", ",", "die", "schon", "jezt", "er\u00b7g\u00f6\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Lust des reifen Alters sucht", "tokens": ["Die", "Lust", "des", "rei\u00b7fen", "Al\u00b7ters", "sucht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und welchem einmahl zu Gefallen", "tokens": ["Und", "wel\u00b7chem", "ein\u00b7mahl", "zu", "Ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAT", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die feuerreichen B(r\u00fcste) wallen.", "tokens": ["Die", "feu\u00b7er\u00b7rei\u00b7chen", "B", "(", "r\u00fcs\u00b7te", ")", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "VVFIN", "$(", "VVINF", "$."], "meter": "-+-+-++-+-", "measure": "unknown.measure.penta"}}, "stanza.3": {"line.1": {"text": "Die Reizung an Person und Mienen", "tokens": ["Die", "Rei\u00b7zung", "an", "Per\u00b7son", "und", "Mie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Nimmt wie der Wohlstand t\u00e4glich zu:", "tokens": ["Nimmt", "wie", "der", "Wohl\u00b7stand", "t\u00e4g\u00b7lich", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie manche Freyheit wird dir dienen?", "tokens": ["Wie", "man\u00b7che", "Frey\u00b7heit", "wird", "dir", "die\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie manchem st\u00f6rstu schon die Ruh?", "tokens": ["Wie", "man\u00b7chem", "st\u00f6rs\u00b7tu", "schon", "die", "Ruh", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie manchem wird nach deinen K\u00fc\u00dfen", "tokens": ["Wie", "man\u00b7chem", "wird", "nach", "dei\u00b7nen", "K\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Mund vergebens w\u00e4\u00dfern m\u00fc\u00dfen?", "tokens": ["Der", "Mund", "ver\u00b7ge\u00b7bens", "w\u00e4\u00b7\u00dfern", "m\u00fc\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach sollt ich doch die Lust erleben,", "tokens": ["Ach", "sollt", "ich", "doch", "die", "Lust", "er\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dich, sch\u00f6nstes Kind, als Braut zu sehn!", "tokens": ["Dich", ",", "sch\u00f6ns\u00b7tes", "Kind", ",", "als", "Braut", "zu", "sehn", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "KOUS", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie w\u00fcrde sich mein Geist erheben,", "tokens": ["Wie", "w\u00fcr\u00b7de", "sich", "mein", "Geist", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PRF", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie w\u00fcrden sich die W\u00fcrbel drehn,", "tokens": ["Wie", "w\u00fcr\u00b7den", "sich", "die", "W\u00fcr\u00b7bel", "drehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie wollt ich nicht die Laute zwingen,", "tokens": ["Wie", "wollt", "ich", "nicht", "die", "Lau\u00b7te", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dir nach Verdienst ein Lob zu singen!", "tokens": ["Dir", "nach", "Ver\u00b7dienst", "ein", "Lob", "zu", "sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Inde\u00dfen steige dein Vergn\u00fcgen,", "tokens": ["In\u00b7de\u00b7\u00dfen", "stei\u00b7ge", "dein", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So wie dein Werth und Alter steigt!", "tokens": ["So", "wie", "dein", "Werth", "und", "Al\u00b7ter", "steigt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur las dich nicht den Schein betriegen,", "tokens": ["Nur", "las", "dich", "nicht", "den", "Schein", "be\u00b7trie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der auch die Kl\u00fcgsten oft betreugt,", "tokens": ["Der", "auch", "die", "Kl\u00fcgs\u00b7ten", "oft", "be\u00b7treugt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wenn du lieben wirst, so liebe", "tokens": ["Und", "wenn", "du", "lie\u00b7ben", "wirst", ",", "so", "lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVINF", "VAFIN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit rein- und unverf\u00e4lschtem Triebe.", "tokens": ["Mit", "rein", "und", "un\u00b7ver\u00b7f\u00e4lschtem", "Trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}