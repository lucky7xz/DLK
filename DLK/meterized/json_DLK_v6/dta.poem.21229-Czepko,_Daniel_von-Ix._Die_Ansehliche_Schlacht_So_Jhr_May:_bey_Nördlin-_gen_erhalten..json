{"dta.poem.21229": {"metadata": {"author": {"name": "Czepko, Daniel von", "birth": "N.A.", "death": "N.A."}, "title": "Ix.  \n  Die Ansehliche Schlacht/  \n So Jhr May: bey N\u00f6rdlin-  \n gen erhalten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20871-8", "language": ["de:0.99"], "booktitle": "Czepko, Daniel von: Triumph Bogen. Breslau, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Kein Vogel/ der die Kreil an einen Raub gesatzt/", "tokens": ["Kein", "Vo\u00b7gel", "/", "der", "die", "Kreil", "an", "ei\u00b7nen", "Raub", "ge\u00b7satzt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$(", "ART", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat jemahls seinen Feind so d\u00fcrffen zerrn vnd kr\u00e4ncken/", "tokens": ["Hat", "je\u00b7mahls", "sei\u00b7nen", "Feind", "so", "d\u00fcrf\u00b7fen", "zerrn", "vnd", "kr\u00e4n\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "ADV", "VMFIN", "VVINF", "KON", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Al\u00df in die Fl\u00fcgel sich der ", "tokens": ["Al\u00df", "in", "die", "Fl\u00fc\u00b7gel", "sich", "der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "PRF", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auff die im k\u00fchnen Zug er zu vnd lo\u00df geplatzt.", "tokens": ["Auff", "die", "im", "k\u00fch\u00b7nen", "Zug", "er", "zu", "vnd", "lo\u00df", "ge\u00b7platzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPRART", "ADJA", "NN", "PPER", "PTKVZ", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "O HERR/ Sein grimmer plitz zubrach der Lappen Ei\u00df", "tokens": ["O", "HeRR", "/", "Sein", "grim\u00b7mer", "plitz", "zu\u00b7brach", "der", "Lap\u00b7pen", "Ei\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$(", "PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es schlug die Mitternacht der Mittag ", "tokens": ["Es", "schlug", "die", "Mit\u00b7ter\u00b7nacht", "der", "Mit\u00b7tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dann ob durch strengen Ernst die beyden L\u00e4ger trafen/", "tokens": ["Dann", "ob", "durch", "stren\u00b7gen", "Ernst", "die", "bey\u00b7den", "L\u00e4\u00b7ger", "tra\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "APPR", "ADJA", "NN", "ART", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ward doch jhr Heer sein Aa\u00df/ vnd Er behilt den Prei\u00df.", "tokens": ["Ward", "doch", "jhr", "Heer", "sein", "Aa\u00df", "/", "vnd", "Er", "be\u00b7hilt", "den", "Prei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "PPOSAT", "NN", "$(", "KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Jtzt holt dem ", "tokens": ["Jtzt", "holt", "dem"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.