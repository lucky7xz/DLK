{"dta.poem.11526": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Die Geister am Mummelsee.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Vom Berge, was kommt dort um Mitternacht sp\u00e4t", "tokens": ["Vom", "Ber\u00b7ge", ",", "was", "kommt", "dort", "um", "Mit\u00b7ter\u00b7nacht", "sp\u00e4t"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWS", "VVFIN", "ADV", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Fackeln so pr\u00e4chtig herunter?", "tokens": ["Mit", "Fa\u00b7ckeln", "so", "pr\u00e4ch\u00b7tig", "her\u00b7un\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ob das wohl zum Tanze, zum Feste noch geht?", "tokens": ["Ob", "das", "wohl", "zum", "Tan\u00b7ze", ",", "zum", "Fes\u00b7te", "noch", "geht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ADV", "APPRART", "NN", "$,", "APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mir klingen die Lieder so munter.", "tokens": ["Mir", "klin\u00b7gen", "die", "Lie\u00b7der", "so", "mun\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "O nein!", "tokens": ["O", "nein", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "PTKANT", "$."], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "So sage, was mag es wohl seyn?", "tokens": ["So", "sa\u00b7ge", ",", "was", "mag", "es", "wohl", "seyn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWS", "VMFIN", "PPER", "ADV", "VAINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Das, was du da siehest, ist Todtengeleit,", "tokens": ["Das", ",", "was", "du", "da", "sie\u00b7hest", ",", "ist", "Tod\u00b7ten\u00b7ge\u00b7leit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "ADV", "VVFIN", "$,", "VAFIN", "NN", "$,"], "meter": "-+--++-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und was du da h\u00f6rest, sind Klagen,", "tokens": ["Und", "was", "du", "da", "h\u00f6\u00b7rest", ",", "sind", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVFIN", "$,", "VAFIN", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Dem K\u00f6nig, dem Zauberer, gilt es zu Leid,", "tokens": ["Dem", "K\u00f6\u00b7nig", ",", "dem", "Zau\u00b7be\u00b7rer", ",", "gilt", "es", "zu", "Leid", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und Geister nur sind's, die ihn tragen.", "tokens": ["Und", "Geis\u00b7ter", "nur", "sin\u00b7d's", ",", "die", "ihn", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VAFIN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ach wohl!", "tokens": ["Ach", "wohl", "!"], "token_info": ["word", "word", "punct"], "pos": ["ITJ", "ADV", "$."], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Sie singen so traurig und hohl.", "tokens": ["Sie", "sin\u00b7gen", "so", "trau\u00b7rig", "und", "hohl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Sie schweben hernieder ins Mummelseethal,", "tokens": ["Sie", "schwe\u00b7ben", "her\u00b7nie\u00b7der", "ins", "Mum\u00b7mel\u00b7see\u00b7thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sie haben den See schon betreten,", "tokens": ["Sie", "ha\u00b7ben", "den", "See", "schon", "be\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sie r\u00fchren und netzen den Fu\u00df nicht einmal,", "tokens": ["Sie", "r\u00fch\u00b7ren", "und", "net\u00b7zen", "den", "Fu\u00df", "nicht", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "NN", "ART", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Sie schwirren in leisen Gebeten:", "tokens": ["Sie", "schwir\u00b7ren", "in", "lei\u00b7sen", "Ge\u00b7be\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "O schau,", "tokens": ["O", "schau", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Am Sarge die gl\u00e4nzende Frau!", "tokens": ["Am", "Sar\u00b7ge", "die", "gl\u00e4n\u00b7zen\u00b7de", "Frau", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Jezt \u00f6ffnet der See das gr\u00fcnspiegelnde Thor;", "tokens": ["Jezt", "\u00f6ff\u00b7net", "der", "See", "das", "gr\u00fcn\u00b7spie\u00b7geln\u00b7de", "Thor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+---+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gib Acht, nun tauchen sie nieder!", "tokens": ["Gib", "Acht", ",", "nun", "tau\u00b7chen", "sie", "nie\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "CARD", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es schwankt eine lebende Treppe hervor,", "tokens": ["Es", "schwankt", "ei\u00b7ne", "le\u00b7ben\u00b7de", "Trep\u00b7pe", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und \u2014 drunten schon summen die Lieder.", "tokens": ["Und", "drun\u00b7ten", "schon", "sum\u00b7men", "die", "Lie\u00b7der", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "ADV", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "H\u00f6rst du?", "tokens": ["H\u00f6rst", "du", "?"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Sie singen ihn unten zur Ruh.", "tokens": ["Sie", "sin\u00b7gen", "ihn", "un\u00b7ten", "zur", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Die Wasser, wie lieblich sie brennen und gl\u00fchn!", "tokens": ["Die", "Was\u00b7ser", ",", "wie", "lieb\u00b7lich", "sie", "bren\u00b7nen", "und", "gl\u00fchn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ADJD", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Sie spielen in gr\u00fcnendem Feuer;", "tokens": ["Sie", "spie\u00b7len", "in", "gr\u00fc\u00b7nen\u00b7dem", "Feu\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Es geisten die Nebel am Ufer dahin,", "tokens": ["Es", "geis\u00b7ten", "die", "Ne\u00b7bel", "am", "U\u00b7fer", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "PAV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Zum Meere verzieht sich der Weiher.", "tokens": ["Zum", "Mee\u00b7re", "ver\u00b7zieht", "sich", "der", "Wei\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Nur still:", "tokens": ["Nur", "still", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Ob dort sich nichts r\u00fchren will?", "tokens": ["Ob", "dort", "sich", "nichts", "r\u00fch\u00b7ren", "will", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PRF", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Es zuckt in der Mitten \u2014 o Himmel! ach hilf!", "tokens": ["Es", "zuckt", "in", "der", "Mit\u00b7ten", "o", "Him\u00b7mel", "!", "ach", "hilf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$(", "FM", "NN", "$.", "XY", "XY", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich glaube, sie nahen, sie kommen!", "tokens": ["Ich", "glau\u00b7be", ",", "sie", "na\u00b7hen", ",", "sie", "kom\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "ADJA", "$,", "PPER", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Es orgelt im Rohr und es klirret im Schilf;", "tokens": ["Es", "or\u00b7gelt", "im", "Rohr", "und", "es", "klir\u00b7ret", "im", "Schilf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Nur hurtig, die Flucht nur genommen!", "tokens": ["Nur", "hur\u00b7tig", ",", "die", "Flucht", "nur", "ge\u00b7nom\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Davon!", "tokens": ["Da\u00b7von", "!"], "token_info": ["word", "punct"], "pos": ["PAV", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Sie wittern, sie haschen mich schon!", "tokens": ["Sie", "wit\u00b7tern", ",", "sie", "ha\u00b7schen", "mich", "schon", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}