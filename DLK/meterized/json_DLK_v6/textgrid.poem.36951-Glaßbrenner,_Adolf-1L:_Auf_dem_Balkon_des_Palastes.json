{"textgrid.poem.36951": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf dem Balkon des Palastes", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf dem Balkon des Palastes", "tokens": ["Auf", "dem", "Bal\u00b7kon", "des", "Pa\u00b7las\u00b7tes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steht Seine Excellenz mit dem Topf,", "tokens": ["Steht", "Sei\u00b7ne", "Ex\u00b7cel\u00b7lenz", "mit", "dem", "Topf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und unten harret die Masse", "tokens": ["Und", "un\u00b7ten", "har\u00b7ret", "die", "Mas\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Des Volkes Kopf an Kopf.", "tokens": ["Des", "Vol\u00b7kes", "Kopf", "an", "Kopf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Topf \u2013 wohl \u00fcber die falsche", "tokens": ["Der", "Topf", "\u2013", "wohl", "\u00fc\u00b7ber", "die", "fal\u00b7sche"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ADV", "APPR", "ART", "ADJA"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Bestimmung weinend \u2013 tropft,", "tokens": ["Be\u00b7stim\u00b7mung", "wei\u00b7nend", "\u2013", "tropft", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "VVPP", "$(", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ist mit bunten Orden", "tokens": ["Und", "ist", "mit", "bun\u00b7ten", "Or\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und Sternen vollgestopft.", "tokens": ["Und", "Ster\u00b7nen", "voll\u00b7ge\u00b7stopft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Seine Excellenz der Minister beugt sich", "tokens": ["Sei\u00b7ne", "Ex\u00b7cel\u00b7lenz", "der", "Mi\u00b7nis\u00b7ter", "beugt", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVFIN", "PRF"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Ueber's Gel\u00e4nder hinaus;", "tokens": ["Ue\u00b7ber's", "Ge\u00b7l\u00e4n\u00b7der", "hin\u00b7aus", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Dann ruft er warnend: Kopf weg!", "tokens": ["Dann", "ruft", "er", "war\u00b7nend", ":", "Kopf", "weg", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "$.", "NN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und gie\u00dft den Inhalt aus.", "tokens": ["Und", "gie\u00dft", "den", "In\u00b7halt", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}