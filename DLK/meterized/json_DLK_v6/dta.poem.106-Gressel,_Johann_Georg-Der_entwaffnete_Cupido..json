{"dta.poem.106": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der entwaffnete  Cupido.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Als n\u00e4chst der Chloris Schoo\u00df die k\u00fchlen Fluhten k\u00fc\u00dfte/", "tokens": ["Als", "n\u00e4chst", "der", "Chlo\u00b7ris", "Schoo\u00df", "die", "k\u00fch\u00b7len", "Fluh\u00b7ten", "k\u00fc\u00df\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NE", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Versah\u2019 es ", "tokens": ["Ver\u00b7sah'", "es"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Da\u00df er dis nackte Bild als seine Mutter gr\u00fc\u00dfte", "tokens": ["Da\u00df", "er", "dis", "nack\u00b7te", "Bild", "als", "sei\u00b7ne", "Mut\u00b7ter", "gr\u00fc\u00df\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PDS", "VVFIN", "NN", "KOKOM", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dieweil sie ihn aus Schertz auf ihre Arme nahm.", "tokens": ["Die\u00b7weil", "sie", "ihn", "aus", "Schertz", "auf", "ih\u00b7re", "Ar\u00b7me", "nahm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch sah er den Betrug/ und weil er ohne Waffen/", "tokens": ["Doch", "sah", "er", "den", "Be\u00b7trug", "/", "und", "weil", "er", "oh\u00b7ne", "Waf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$(", "KON", "KOUS", "PPER", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die er am Strand gelegt/ nicht kunte Rache schaffen/", "tokens": ["Die", "er", "am", "Strand", "ge\u00b7legt", "/", "nicht", "kun\u00b7te", "Ra\u00b7che", "schaf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVPP", "$(", "PTKNEG", "VMFIN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So rie\u00df er sich geschwind von ihren Armen lo\u00df/", "tokens": ["So", "rie\u00df", "er", "sich", "ge\u00b7schwind", "von", "ih\u00b7ren", "Ar\u00b7men", "lo\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "APPR", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und flog dem Lande zu; wo er den Bogen blo\u00df", "tokens": ["Und", "flog", "dem", "Lan\u00b7de", "zu", ";", "wo", "er", "den", "Bo\u00b7gen", "blo\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$.", "PWAV", "PPER", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nicht Sehn\u2019 und Pfeile fand/ die ", "tokens": ["Nicht", "Sehn'", "und", "Pfei\u00b7le", "fand", "/", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PTKNEG", "NN", "KON", "NN", "VVFIN", "$(", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.11": {"text": "Und heimlich mit dem Raub war in dem Busch entwichen/", "tokens": ["Und", "heim\u00b7lich", "mit", "dem", "Raub", "war", "in", "dem", "Busch", "ent\u00b7wi\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Daselbsten einen Stock zu seiner Sehn zu hohln.", "tokens": ["Da\u00b7selbs\u00b7ten", "ei\u00b7nen", "Stock", "zu", "sei\u00b7ner", "Sehn", "zu", "hohln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Cupido durffte nicht zu seiner Mutter kommen/", "tokens": ["Cu\u00b7pi\u00b7do", "durff\u00b7te", "nicht", "zu", "sei\u00b7ner", "Mut\u00b7ter", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Weil ihm die Sehn\u2019 und Pfeil so liederlich genommen/", "tokens": ["Weil", "ihm", "die", "Sehn'", "und", "Pfeil", "so", "lie\u00b7der\u00b7lich", "ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "KON", "NN", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Indem er nun betr\u00fcbt bey seinem Bogen sa\u00df", "tokens": ["In\u00b7dem", "er", "nun", "be\u00b7tr\u00fcbt", "bey", "sei\u00b7nem", "Bo\u00b7gen", "sa\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "War er der ", "tokens": ["War", "er", "der"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPER", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.17": {"text": "Er kunte unbewehrt die herbe Schmach nicht r\u00e4chen/", "tokens": ["Er", "kun\u00b7te", "un\u00b7be\u00b7wehrt", "die", "her\u00b7be", "Schmach", "nicht", "r\u00e4\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Damit das nackte Weib ihm stets beschwerlich war/", "tokens": ["Da\u00b7mit", "das", "nack\u00b7te", "Weib", "ihm", "stets", "be\u00b7schwer\u00b7lich", "war", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "PPER", "ADV", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Das Hertze wolte ihm vor Zorn im Leibe brechen/", "tokens": ["Das", "Hert\u00b7ze", "wol\u00b7te", "ihm", "vor", "Zorn", "im", "Lei\u00b7be", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VMFIN", "PPER", "APPR", "NN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Doch war sein Zorn umsonst/ weil Sehn\u2019 und Pfeil nicht dar.", "tokens": ["Doch", "war", "sein", "Zorn", "um\u00b7sonst", "/", "weil", "Sehn'", "und", "Pfeil", "nicht", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "ADV", "$(", "KOUS", "NN", "KON", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Zu diesem Schmertz ward er noch hefftiger vexiret/", "tokens": ["Zu", "die\u00b7sem", "Schmertz", "ward", "er", "noch", "heff\u00b7ti\u00b7ger", "ve\u00b7xi\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "PPER", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Denn ", "tokens": ["Denn"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.23": {"text": "Ein ander mahl sieh dich/ ", "tokens": ["Ein", "an\u00b7der", "mahl", "sieh", "dich", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "VVIMP", "PPER", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "So treibt ein schwaches Weib nicht ihren Spott mit dir.", "tokens": ["So", "treibt", "ein", "schwa\u00b7ches", "Weib", "nicht", "ih\u00b7ren", "Spott", "mit", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}