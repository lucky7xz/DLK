{"textgrid.poem.42691": {"metadata": {"author": {"name": "Ratschky, Joseph Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Freund, willst du, Thoren gleich, die, um vergn\u00fcgt zu seyn,", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund, willst du, Thoren gleich, die, um vergn\u00fcgt zu seyn,", "tokens": ["Freund", ",", "willst", "du", ",", "Tho\u00b7ren", "gleich", ",", "die", ",", "um", "ver\u00b7gn\u00fcgt", "zu", "seyn", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "PPER", "$,", "NN", "ADV", "$,", "PRELS", "$,", "KOUI", "VVPP", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der wandelbaren Gunst des blinden Gl\u00fccks bed\u00fcrfen,", "tokens": ["Der", "wan\u00b7del\u00b7ba\u00b7ren", "Gunst", "des", "blin\u00b7den", "Gl\u00fccks", "be\u00b7d\u00fcr\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ertr\u00e4umter M\u00f6glichkeit und t\u00e4uschenden Entw\u00fcrfen", "tokens": ["Er\u00b7tr\u00e4um\u00b7ter", "M\u00f6g\u00b7lich\u00b7keit", "und", "t\u00e4u\u00b7schen\u00b7den", "Ent\u00b7w\u00fcr\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Zukunft deine Tage weihn?", "tokens": ["Der", "Zu\u00b7kunft", "dei\u00b7ne", "Ta\u00b7ge", "weihn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sey kl\u00fcger, und geniess des Daseyns kurze Frist,", "tokens": ["Sey", "kl\u00fc\u00b7ger", ",", "und", "ge\u00b7niess", "des", "Da\u00b7seyns", "kur\u00b7ze", "Frist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Statt sie mit nichtigen Phantomen zu vertr\u00e4umen!", "tokens": ["Statt", "sie", "mit", "nich\u00b7ti\u00b7gen", "Phan\u00b7to\u00b7men", "zu", "ver\u00b7tr\u00e4u\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O sieh! der Lenz beginnt. Sieh, wie den Ahornb\u00e4umen", "tokens": ["O", "sieh", "!", "der", "Lenz", "be\u00b7ginnt", ".", "Sieh", ",", "wie", "den", "A\u00b7horn\u00b7b\u00e4u\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$.", "ART", "NN", "VVFIN", "$.", "NE", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das jugendliche Laub entspriesst!", "tokens": ["Das", "ju\u00b7gend\u00b7li\u00b7che", "Laub", "ent\u00b7spriesst", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Horch! Lerch' und Nachtigall verk\u00fcnden rings umher", "tokens": ["Horch", "!", "Ler\u00b7ch'", "und", "Nach\u00b7ti\u00b7gall", "ver\u00b7k\u00fcn\u00b7den", "rings", "um\u00b7her"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "NN", "KON", "NN", "VVFIN", "ADV", "PTKVZ"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Den frohen Wonnemond helltrillernd durch die L\u00fcfte:", "tokens": ["Den", "fro\u00b7hen", "Won\u00b7ne\u00b7mond", "hellt\u00b7ril\u00b7lernd", "durch", "die", "L\u00fcf\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Weste lauer Hauch, der Bl\u00fchten Balsamd\u00fcfte", "tokens": ["Der", "Wes\u00b7te", "lau\u00b7er", "Hauch", ",", "der", "Bl\u00fch\u00b7ten", "Bal\u00b7sam\u00b7d\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sind Boten seiner Wiederkehr.", "tokens": ["Sind", "Bo\u00b7ten", "sei\u00b7ner", "Wie\u00b7der\u00b7kehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sieh! alles, was sich regt, was auf bebl\u00fcmter Flur,", "tokens": ["Sieh", "!", "al\u00b7les", ",", "was", "sich", "regt", ",", "was", "auf", "be\u00b7bl\u00fcm\u00b7ter", "Flur", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PIS", "$,", "PRELS", "PRF", "VVFIN", "$,", "PRELS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im hohen Luftrevier, im Wasserreiche lebet,", "tokens": ["Im", "ho\u00b7hen", "Luf\u00b7tre\u00b7vier", ",", "im", "Was\u00b7ser\u00b7rei\u00b7che", "le\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was rings im weiten Raum der Sch\u00f6pfung Odem hebet,", "tokens": ["Was", "rings", "im", "wei\u00b7ten", "Raum", "der", "Sch\u00f6p\u00b7fung", "O\u00b7dem", "he\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPRART", "ADJA", "NN", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Freut sich der Anmuth der Natur.", "tokens": ["Freut", "sich", "der", "An\u00b7muth", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ART", "NN", "$."], "meter": "+---+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Die Freude beut auch dir ihr reiches F\u00fcllhorn dar:", "tokens": ["Die", "Freu\u00b7de", "beut", "auch", "dir", "ihr", "rei\u00b7ches", "F\u00fcll\u00b7horn", "dar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PPER", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lass nach der Gr\u00f6sse Tand des Stolzes Knechte d\u00fcrsten!", "tokens": ["Lass", "nach", "der", "Gr\u00f6s\u00b7se", "Tand", "des", "Stol\u00b7zes", "Knech\u00b7te", "d\u00fcrs\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "NN", "ART", "ADJA", "NN", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vergn\u00fcgen sey dein Ziel, nicht schn\u00f6de Gunst der F\u00fcrsten,", "tokens": ["Ver\u00b7gn\u00fc\u00b7gen", "sey", "dein", "Ziel", ",", "nicht", "schn\u00f6\u00b7de", "Gunst", "der", "F\u00fcrs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,", "PTKNEG", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die st\u00e4ts des Grams Gef\u00e4hrtinn war!", "tokens": ["Die", "st\u00e4ts", "des", "Grams", "Ge\u00b7f\u00e4hr\u00b7tinn", "war", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Sieh jenen H\u00f6fling an! des Sturzes Bild umschwebt", "tokens": ["Sieh", "je\u00b7nen", "H\u00f6f\u00b7ling", "an", "!", "des", "Stur\u00b7zes", "Bild", "um\u00b7schwebt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PDAT", "NN", "PTKVZ", "$.", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Prophetisch seinen Blick: der bangen Ahnung Leiden", "tokens": ["Pro\u00b7phe\u00b7tisch", "sei\u00b7nen", "Blick", ":", "der", "ban\u00b7gen", "Ah\u00b7nung", "Lei\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$.", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verbittern st\u00fcndlich ihm die unbefangnen Freuden,", "tokens": ["Ver\u00b7bit\u00b7tern", "st\u00fcnd\u00b7lich", "ihm", "die", "un\u00b7be\u00b7fang\u00b7nen", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die der nur kennt, der sorglos lebt.", "tokens": ["Die", "der", "nur", "kennt", ",", "der", "sorg\u00b7los", "lebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADV", "VVFIN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Drum z\u00e4hme deinen Wunsch! leb' als ein freyer Mann!", "tokens": ["Drum", "z\u00e4h\u00b7me", "dei\u00b7nen", "Wunsch", "!", "leb'", "als", "ein", "frey\u00b7er", "Mann", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "$.", "VVFIN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was man nicht sehnlich sucht, vermisst man ohne Sorgen.", "tokens": ["Was", "man", "nicht", "sehn\u00b7lich", "sucht", ",", "ver\u00b7misst", "man", "oh\u00b7ne", "Sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "PTKNEG", "ADV", "VVFIN", "$,", "VVFIN", "PIS", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Weise l\u00e4sst durch nichts sich fesseln, was ihm morgen", "tokens": ["Der", "Wei\u00b7se", "l\u00e4sst", "durch", "nichts", "sich", "fes\u00b7seln", ",", "was", "ihm", "mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIS", "PRF", "VVINF", "$,", "PWS", "PPER", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des Zufalls Laune rauben kann.", "tokens": ["Des", "Zu\u00b7falls", "Lau\u00b7ne", "rau\u00b7ben", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}