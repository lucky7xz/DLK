{"textgrid.poem.62755": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: G\u00f6ttliche Zufriedenheit,", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "G\u00f6ttliche Zufriedenheit,", "tokens": ["G\u00f6tt\u00b7li\u00b7che", "Zu\u00b7frie\u00b7den\u00b7heit", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Braut des Weisen,", "tokens": ["Braut", "des", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Nur ein Sohn der goldnen Zeit", "tokens": ["Nur", "ein", "Sohn", "der", "gold\u00b7nen", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Darf dich preisen.", "tokens": ["Darf", "dich", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Aber flehn darf ich zu dir:", "tokens": ["A\u00b7ber", "flehn", "darf", "ich", "zu", "dir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hilf das Thal der Leiden mir", "tokens": ["Hilf", "das", "Thal", "der", "Lei\u00b7den", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "ART", "NN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Still durchreisen.", "tokens": ["Still", "durc\u00b7hrei\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Nur nach dir, du h\u00f6chstes Gut,", "tokens": ["Nur", "nach", "dir", ",", "du", "h\u00f6chs\u00b7tes", "Gut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Geht mein Streben;", "tokens": ["Geht", "mein", "Stre\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Schenke du dem Dulder Muth", "tokens": ["Schen\u00b7ke", "du", "dem", "Dul\u00b7der", "Muth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auszuleben.", "tokens": ["Aus\u00b7zu\u00b7le\u00b7ben", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Selbst dem Kelch des Marterthums", "tokens": ["Selbst", "dem", "Kelch", "des", "Mar\u00b7ter\u00b7thums"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Kannst du des Elysiums", "tokens": ["Kannst", "du", "des", "E\u00b7ly\u00b7si\u00b7ums"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Vorschmack geben.", "tokens": ["Vor\u00b7schmack", "ge\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Was nur Kinder vom Geschick", "tokens": ["Was", "nur", "Kin\u00b7der", "vom", "Ge\u00b7schick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hei\u00df begehren;", "tokens": ["Hei\u00df", "be\u00b7geh\u00b7ren", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Was Geburt und Gunst und Gl\u00fcck", "tokens": ["Was", "Ge\u00b7burt", "und", "Gunst", "und", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "NN", "KON", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Uns gew\u00e4hren;", "tokens": ["Uns", "ge\u00b7w\u00e4h\u00b7ren", ";"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Was ihm nicht zur Weisheit n\u00fctzt,", "tokens": ["Was", "ihm", "nicht", "zur", "Weis\u00b7heit", "n\u00fctzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Lernt ein Herz, das dich besitzt,", "tokens": ["Lernt", "ein", "Herz", ",", "das", "dich", "be\u00b7sitzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Leicht entbehren.", "tokens": ["Leicht", "ent\u00b7beh\u00b7ren", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Gern l\u00e4\u00dft es die Schichten Geld", "tokens": ["Gern", "l\u00e4\u00dft", "es", "die", "Schich\u00b7ten", "Geld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Harpagonen;", "tokens": ["Har\u00b7pa\u00b7go\u00b7nen", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Gern l\u00e4\u00dft es dem stolzen Held", "tokens": ["Gern", "l\u00e4\u00dft", "es", "dem", "stol\u00b7zen", "Held"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Seine Kronen.", "tokens": ["Sei\u00b7ne", "Kro\u00b7nen", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Unter einem Hirtenkleid", "tokens": ["Un\u00b7ter", "ei\u00b7nem", "Hir\u00b7ten\u00b7kleid"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wohnet mehr Zufriedenheit", "tokens": ["Woh\u00b7net", "mehr", "Zu\u00b7frie\u00b7den\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Als auf Thronen.", "tokens": ["Als", "auf", "Thro\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Wank ich gleich an meines Kahns", "tokens": ["Wank", "ich", "gleich", "an", "mei\u00b7nes", "Kahns"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Morschem Steuer;", "tokens": ["Mor\u00b7schem", "Steu\u00b7er", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Eingeh\u00fcllt in O\u00dfians", "tokens": ["Ein\u00b7ge\u00b7h\u00fcllt", "in", "O\u00b7\u00dfians"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "NE"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Augenschleyer;", "tokens": ["Au\u00b7gen\u00b7schle\u00b7yer", ";"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Dennoch bleibt mein Frohsinn mir;", "tokens": ["Den\u00b7noch", "bleibt", "mein", "Froh\u00b7sinn", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Den, o G\u00f6ttin, dank ich dir", "tokens": ["Den", ",", "o", "G\u00f6t\u00b7tin", ",", "dank", "ich", "dir"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "FM", "NN", "$,", "APPR", "PPER", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und der Leyer.", "tokens": ["Und", "der", "Le\u00b7yer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Wenn ich, trautes Himmelskind,", "tokens": ["Wenn", "ich", ",", "trau\u00b7tes", "Him\u00b7mel\u00b7skind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dich nur habe;", "tokens": ["Dich", "nur", "ha\u00b7be", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Dienest du durchs Labyrinth", "tokens": ["Die\u00b7nest", "du", "durchs", "La\u00b7by\u00b7rinth"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mir zum Stabe;", "tokens": ["Mir", "zum", "Sta\u00b7be", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "So beklag ich nie mein Loos,", "tokens": ["So", "be\u00b7klag", "ich", "nie", "mein", "Loos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und ich finde deinen Schoos", "tokens": ["Und", "ich", "fin\u00b7de", "dei\u00b7nen", "Schoos"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Auch im Grabe.", "tokens": ["Auch", "im", "Gra\u00b7be", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}