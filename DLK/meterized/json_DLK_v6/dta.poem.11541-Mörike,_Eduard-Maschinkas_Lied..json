{"dta.poem.11541": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Maschinkas Lied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Was dich dr\u00fcckt und qu\u00e4lt?", "tokens": ["Was", "dich", "dr\u00fcckt", "und", "qu\u00e4lt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Oder kann man so um Nichts verzagen?", "tokens": ["O\u00b7der", "kann", "man", "so", "um", "Nichts", "ver\u00b7za\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "ADV", "APPR", "PIS", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Herz, ich habe schwer an dir zu tragen,", "tokens": ["Herz", ",", "ich", "ha\u00b7be", "schwer", "an", "dir", "zu", "tra\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADJD", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Schwer! Schwer!", "tokens": ["Schwer", "!", "Schwer", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$.", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Da\u00df ich mit dir im Grabe w\u00e4r'!", "tokens": ["Da\u00df", "ich", "mit", "dir", "im", "Gra\u00b7be", "w\u00e4r'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Geschwister kommen mich zu fragen,", "tokens": ["Die", "Ge\u00b7schwis\u00b7ter", "kom\u00b7men", "mich", "zu", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Was mir immer fehlt?", "tokens": ["Was", "mir", "im\u00b7mer", "fehlt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "O ich darf nicht wagen,", "tokens": ["O", "ich", "darf", "nicht", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Die verweinten Augen aufzuschlagen,", "tokens": ["Die", "ver\u00b7wein\u00b7ten", "Au\u00b7gen", "auf\u00b7zu\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wenn ich denke, was du mir verhehlt!", "tokens": ["Wenn", "ich", "den\u00b7ke", ",", "was", "du", "mir", "ver\u00b7hehlt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PWS", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Herz! ich habe schwer an dir zu tragen,", "tokens": ["Herz", "!", "ich", "ha\u00b7be", "schwer", "an", "dir", "zu", "tra\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VAFIN", "ADJD", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Schwer! Schwer!", "tokens": ["Schwer", "!", "Schwer", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$.", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Da\u00df ich im Grabe w\u00e4r'!", "tokens": ["Da\u00df", "ich", "im", "Gra\u00b7be", "w\u00e4r'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}