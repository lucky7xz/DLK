{"textgrid.poem.24291": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Heidelbeeren", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als heut ich durch die Dresdner Haide fuhr,", "tokens": ["Als", "heut", "ich", "durch", "die", "Dresd\u00b7ner", "Hai\u00b7de", "fuhr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "APPR", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Stand meine Kindheit vor mir da: Ein Kind,", "tokens": ["Stand", "mei\u00b7ne", "Kind\u00b7heit", "vor", "mir", "da", ":", "Ein", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "PPER", "ADV", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Bauernm\u00e4delchen im kurzen Rock,", "tokens": ["Ein", "Bau\u00b7ern\u00b7m\u00e4\u00b7del\u00b7chen", "im", "kur\u00b7zen", "Rock", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das bunte Kopftuch \u00fcber dem blonden Haar:", "tokens": ["Das", "bun\u00b7te", "Kopf\u00b7tuch", "\u00fc\u00b7ber", "dem", "blon\u00b7den", "Haar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Die \u00bbGuge\u00ab, die sich so h\u00fcbsch an rote Backen schmiegt", "tokens": ["Die", "\u00bb", "Gu\u00b7ge", "\u00ab", ",", "die", "sich", "so", "h\u00fcbsch", "an", "ro\u00b7te", "Ba\u00b7cken", "schmiegt"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "$(", "NN", "$(", "$,", "PRELS", "PRF", "ADV", "ADJD", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Und unterm Kinne zipfelig geschlungen ist.", "tokens": ["Und", "un\u00b7term", "Kin\u00b7ne", "zip\u00b7fe\u00b7lig", "ge\u00b7schlun\u00b7gen", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u00bbbarbs\u00ab geht sie \u2013 barfu\u00df: was f\u00fcr W\u00e4delchen!", "tokens": ["\u00bb", "barbs", "\u00ab", "geht", "sie", "\u2013", "bar\u00b7fu\u00df", ":", "was", "f\u00fcr", "W\u00e4\u00b7del\u00b7chen", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$(", "VVFIN", "PPER", "$(", "PTKVZ", "$.", "PWS", "APPR", "NN", "$."], "meter": "-+--+--+--", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Wie s\u00fc\u00df die zierlichen Zehen geschnitten sind", "tokens": ["Wie", "s\u00fc\u00df", "die", "zier\u00b7li\u00b7chen", "Ze\u00b7hen", "ge\u00b7schnit\u00b7ten", "sind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "ADJA", "NN", "VVPP", "VAFIN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "(ob auch ein wenig mit Staub gepudert) \u2013, ach und sieh:", "tokens": ["(", "ob", "auch", "ein", "we\u00b7nig", "mit", "Staub", "ge\u00b7pu\u00b7dert", ")", "\u2013", ",", "ach", "und", "sieh", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "ADV", "ART", "PIS", "APPR", "NN", "VVPP", "$(", "$(", "$,", "XY", "KON", "VVIMP", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Wie sich das B\u00e4uchlein leise vorw\u00e4rts w\u00f6lbt", "tokens": ["Wie", "sich", "das", "B\u00e4uch\u00b7lein", "lei\u00b7se", "vor\u00b7w\u00e4rts", "w\u00f6lbt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ART", "NN", "ADJD", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "(grad nur, zu zeigen, da\u00df es da ist), und", "tokens": ["(", "grad", "nur", ",", "zu", "zei\u00b7gen", ",", "da\u00df", "es", "da", "ist", ")", ",", "und"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["$(", "ADV", "ADV", "$,", "PTKZU", "VVINF", "$,", "KOUS", "PPER", "ADV", "VAFIN", "$(", "$,", "KON"], "meter": "-+-+-+-++-", "measure": "unknown.measure.penta"}, "line.12": {"text": "Wie schelmhaft dieses Fr\u00e4ulein l\u00e4cheln kann!", "tokens": ["Wie", "schelm\u00b7haft", "die\u00b7ses", "Fr\u00e4u\u00b7lein", "l\u00e4\u00b7cheln", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PDAT", "NN", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Fr\u00e4ulein von zw\u00f6lf Jahren, ein Kind und doch", "tokens": ["Ein", "Fr\u00e4u\u00b7lein", "von", "zw\u00f6lf", "Jah\u00b7ren", ",", "ein", "Kind", "und", "doch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "CARD", "NN", "$,", "ART", "NN", "KON", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein Frauchen: Allerliebst kokett bereits", "tokens": ["Ein", "Frau\u00b7chen", ":", "Al\u00b7ler\u00b7liebst", "ko\u00b7kett", "be\u00b7reits"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "ADV", "ADJD", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und doch unschuldig, Duft noch ganz und Tau", "tokens": ["Und", "doch", "un\u00b7schul\u00b7dig", ",", "Duft", "noch", "ganz", "und", "Tau"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "$,", "NN", "ADV", "ADV", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des frischen Morgens. In den H\u00e4nden h\u00e4lt", "tokens": ["Des", "fri\u00b7schen", "Mor\u00b7gens", ".", "In", "den", "H\u00e4n\u00b7den", "h\u00e4lt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das Kindchen einen Korb, bis obenan", "tokens": ["Das", "Kind\u00b7chen", "ei\u00b7nen", "Korb", ",", "bis", "o\u00b7be\u00b7nan"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "KOUS", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Gef\u00fcllt mit Heidelbeeren. Und da seh ich nun,", "tokens": ["Ge\u00b7f\u00fcllt", "mit", "Hei\u00b7del\u00b7bee\u00b7ren", ".", "Und", "da", "seh", "ich", "nun", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$.", "KON", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Warum die Lippen ihm ein bi\u00dfchen \u00bbschnuddlich\u00ab sind:", "tokens": ["Wa\u00b7rum", "die", "Lip\u00b7pen", "ihm", "ein", "bi\u00df\u00b7chen", "\u00bb", "schnudd\u00b7lich", "\u00ab", "sind", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "ART", "PIS", "$(", "ADJD", "$(", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Gef\u00e4rbt vom Blaurot unsrer W\u00e4ldlerin,", "tokens": ["Ge\u00b7f\u00e4rbt", "vom", "Blau\u00b7rot", "uns\u00b7rer", "W\u00e4ld\u00b7le\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Der drallen Blauen, die sich den Armen schenkt.", "tokens": ["Der", "dral\u00b7len", "Blau\u00b7en", ",", "die", "sich", "den", "Ar\u00b7men", "schenkt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Ja wohl, so wars: So sah meine Kindheit aus.", "tokens": ["Ja", "wohl", ",", "so", "wars", ":", "So", "sah", "mei\u00b7ne", "Kind\u00b7heit", "aus", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "$,", "ADV", "VAFIN", "$.", "ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Heidelbeere, nicht die Ananas,", "tokens": ["Die", "Hei\u00b7del\u00b7bee\u00b7re", ",", "nicht", "die", "A\u00b7na\u00b7nas", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Seh ich als Sinnbild jener zagen Zeit.", "tokens": ["Seh", "ich", "als", "Sinn\u00b7bild", "je\u00b7ner", "za\u00b7gen", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "NN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Heidelbeere, tief im Wald gesucht,", "tokens": ["Die", "Hei\u00b7del\u00b7bee\u00b7re", ",", "tief", "im", "Wald", "ge\u00b7sucht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die w\u00e4\u00dfrig-s\u00e4uerliche, die so s\u00fc\u00df doch war", "tokens": ["Die", "w\u00e4\u00df\u00b7rig\u00b7s\u00e4u\u00b7er\u00b7li\u00b7che", ",", "die", "so", "s\u00fc\u00df", "doch", "war"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "PRELS", "ADV", "ADJD", "ADV", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dem unverw\u00f6hnt gesunden Kindesmund,", "tokens": ["Dem", "un\u00b7ver\u00b7w\u00f6hnt", "ge\u00b7sun\u00b7den", "Kin\u00b7des\u00b7mund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der damals schon beim S\u00fc\u00dfe-Suchen sang:", "tokens": ["Der", "da\u00b7mals", "schon", "beim", "S\u00fc\u00b7\u00dfe\u00b7Su\u00b7chen", "sang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Heedelbeern, Heedelbeern,", "tokens": ["Hee\u00b7del\u00b7beern", ",", "Hee\u00b7del\u00b7beern", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.9": {"text": "Such ich in der Haide.", "tokens": ["Such", "ich", "in", "der", "Hai\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ART", "NE", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Heedelbeern, Heedelbeern", "tokens": ["Hee\u00b7del\u00b7beern", ",", "Hee\u00b7del\u00b7beern"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.11": {"text": "Suchen macht mir Freide.", "tokens": ["Su\u00b7chen", "macht", "mir", "Frei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Heedelbeern sin scheene,", "tokens": ["Hee\u00b7del\u00b7beern", "sin", "schee\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "In den Kober kommt keene;", "tokens": ["In", "den", "Ko\u00b7ber", "kommt", "kee\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADJA", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "Ich esse alle Heedelbeern, Heedelbeern alleene.", "tokens": ["Ich", "es\u00b7se", "al\u00b7le", "Hee\u00b7del\u00b7beern", ",", "Hee\u00b7del\u00b7beern", "al\u00b7lee\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "NN", "ADJA", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}