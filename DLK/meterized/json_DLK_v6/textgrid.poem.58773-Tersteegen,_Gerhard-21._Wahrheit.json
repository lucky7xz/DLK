{"textgrid.poem.58773": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "21. Wahrheit", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gedicht und Traum ist mir zu schlecht;", "tokens": ["Ge\u00b7dicht", "und", "Traum", "ist", "mir", "zu", "schlecht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "PPER", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du bist und gibst die Wahrheit recht,", "tokens": ["Du", "bist", "und", "gibst", "die", "Wahr\u00b7heit", "recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KON", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Aug' und Herz und all's vergn\u00fcget.", "tokens": ["Die", "Aug'", "und", "Herz", "und", "all's", "ver\u00b7gn\u00fc\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "KON", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Wort und was dein Mund verspricht,", "tokens": ["Dein", "Wort", "und", "was", "dein", "Mund", "ver\u00b7spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PWS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ist Wahrheit und fehlt nimmer nicht;", "tokens": ["Ist", "Wahr\u00b7heit", "und", "fehlt", "nim\u00b7mer", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "VVFIN", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du bist kein Menschenkind, das l\u00fcget.", "tokens": ["Du", "bist", "kein", "Men\u00b7schen\u00b7kind", ",", "das", "l\u00fc\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Viel tausend haben's drauf gewagt;", "tokens": ["Viel", "tau\u00b7send", "ha\u00b7ben's", "drauf", "ge\u00b7wagt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auch mir, auch mir ist's zugesagt.", "tokens": ["Auch", "mir", ",", "auch", "mir", "ist's", "zu\u00b7ge\u00b7sagt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "ADV", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.9": {"text": "Halleluja! Halleluja!", "tokens": ["Hal\u00b7le\u00b7lu\u00b7ja", "!", "Hal\u00b7le\u00b7lu\u00b7ja", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "ITJ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}