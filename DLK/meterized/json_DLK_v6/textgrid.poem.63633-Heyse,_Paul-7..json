{"textgrid.poem.63633": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war im Himmel und auf Erden nichts,", "tokens": ["Es", "war", "im", "Him\u00b7mel", "und", "auf", "Er\u00b7den", "nichts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "KON", "APPR", "NN", "PIS", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was uns nicht h\u00f6her Sinn und Herz entz\u00fcckte,", "tokens": ["Was", "uns", "nicht", "h\u00f6\u00b7her", "Sinn", "und", "Herz", "ent\u00b7z\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "ADJD", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn aus dem Spiegel deines Angesichts,", "tokens": ["Wenn", "aus", "dem", "Spie\u00b7gel", "dei\u00b7nes", "An\u00b7ge\u00b7sichts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Geliebtes Kind, es uns entgegenblickte.", "tokens": ["Ge\u00b7lieb\u00b7tes", "Kind", ",", "es", "uns", "ent\u00b7ge\u00b7gen\u00b7blick\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der klare Spiegel ward so j\u00e4h zerschlagen,", "tokens": ["Der", "kla\u00b7re", "Spie\u00b7gel", "ward", "so", "j\u00e4h", "zer\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nun hat die Welt uns weiter nichts zu sagen.", "tokens": ["Nun", "hat", "die", "Welt", "uns", "wei\u00b7ter", "nichts", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "ADV", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nicht lockt uns mehr der Dinge Widerschein;", "tokens": ["Nicht", "lockt", "uns", "mehr", "der", "Din\u00b7ge", "Wi\u00b7der\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wir starren freudenblind in uns hinein.", "tokens": ["Wir", "star\u00b7ren", "freu\u00b7den\u00b7blind", "in", "uns", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}