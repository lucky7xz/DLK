{"textgrid.poem.25953": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Eine leere Fahnenstange", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine leere Fahnenstange", "tokens": ["Ei\u00b7ne", "lee\u00b7re", "Fah\u00b7nen\u00b7stan\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieht zum Regengrau hinauf,", "tokens": ["Sieht", "zum", "Re\u00b7gen\u00b7grau", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dran z\u00f6g ich als Trauerwimpel", "tokens": ["Dran", "z\u00f6g", "ich", "als", "Trau\u00b7er\u00b7wim\u00b7pel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gern mein nasses Sacktuch auf.", "tokens": ["Gern", "mein", "nas\u00b7ses", "Sack\u00b7tuch", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie 'ne Henne gackst die Seele,", "tokens": ["Wie", "'ne", "Hen\u00b7ne", "gackst", "die", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Laut aussto\u00dfend Schrei um Schrei,", "tokens": ["Laut", "aus\u00b7sto\u00b7\u00dfend", "Schrei", "um", "Schrei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVPP", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und sie legt mir unter Schmerzen", "tokens": ["Und", "sie", "legt", "mir", "un\u00b7ter", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "T\u00e4glich nur ein hohles Ei.", "tokens": ["T\u00e4g\u00b7lich", "nur", "ein", "hoh\u00b7les", "Ei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Welke Rosen in dem Glase", "tokens": ["Wel\u00b7ke", "Ro\u00b7sen", "in", "dem", "Gla\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAT", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Runzelig wie alte Parzen,", "tokens": ["Run\u00b7ze\u00b7lig", "wie", "al\u00b7te", "Par\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Ausgesogen wie an alten", "tokens": ["Aus\u00b7ge\u00b7so\u00b7gen", "wie", "an", "al\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KOKOM", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Mutterbr\u00fcsten welke Warzen.", "tokens": ["Mut\u00b7ter\u00b7br\u00fcs\u00b7ten", "wel\u00b7ke", "War\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PWAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Dieses sind in meinem Zimmer", "tokens": ["Die\u00b7ses", "sind", "in", "mei\u00b7nem", "Zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Von der Sommerseligkeit", "tokens": ["Von", "der", "Som\u00b7mer\u00b7se\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Noch der letzte Rest und Schimmer \u2013", "tokens": ["Noch", "der", "letz\u00b7te", "Rest", "und", "Schim\u00b7mer", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Alles andere fra\u00df die Zeit.", "tokens": ["Al\u00b7les", "an\u00b7de\u00b7re", "fra\u00df", "die", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}