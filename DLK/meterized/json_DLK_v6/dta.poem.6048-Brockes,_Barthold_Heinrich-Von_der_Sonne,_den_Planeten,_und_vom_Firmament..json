{"dta.poem.6048": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.85", "ca:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In einem grossen Crey\u00df Eccliptica genannt,", "tokens": ["In", "ei\u00b7nem", "gros\u00b7sen", "Crey\u00df", "Ec\u00b7clip\u00b7ti\u00b7ca", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der den Aequator schr\u00e4ge theilet,", "tokens": ["Der", "den", "A\u00b7e\u00b7qua\u00b7tor", "schr\u00e4\u00b7ge", "thei\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sieht man, wie sie best\u00e4ndig eilet;", "tokens": ["Sieht", "man", ",", "wie", "sie", "be\u00b7st\u00e4n\u00b7dig", "ei\u00b7let", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PWAV", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df sie der Sonnen Blick, bald seitenwerts gewandt,", "tokens": ["Da\u00df", "sie", "der", "Son\u00b7nen", "Blick", ",", "bald", "sei\u00b7ten\u00b7werts", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "$,", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und bald gerad empfange.", "tokens": ["Und", "bald", "ge\u00b7rad", "emp\u00b7fan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Indem sie gegen sie in ihrem Gange", "tokens": ["In\u00b7dem", "sie", "ge\u00b7gen", "sie", "in", "ih\u00b7rem", "Gan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Verschiedentlich gekehret stehet;", "tokens": ["Ver\u00b7schie\u00b7dent\u00b7lich", "ge\u00b7keh\u00b7ret", "ste\u00b7het", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Durchrennet sie, und drehet", "tokens": ["Durch\u00b7ren\u00b7net", "sie", ",", "und", "dre\u00b7het"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Sich durch die Zw\u00f6lf bestimmten H\u00e4user,", "tokens": ["Sich", "durch", "die", "Zw\u00f6lf", "be\u00b7stimm\u00b7ten", "H\u00e4u\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "(die Zeichen, wo des Tags, zusammt des Jahres Zeiten,", "tokens": ["(", "die", "Zei\u00b7chen", ",", "wo", "des", "Tags", ",", "zu\u00b7sammt", "des", "Jah\u00b7res", "Zei\u00b7ten", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PWAV", "ART", "NN", "$,", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Gezeichnet und gesetzt) zwar an verschiednen Seiten,", "tokens": ["Ge\u00b7zeich\u00b7net", "und", "ge\u00b7setzt", ")", "zwar", "an", "ver\u00b7schied\u00b7nen", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$(", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Doch allezeit im Licht.", "tokens": ["Doch", "al\u00b7le\u00b7zeit", "im", "Licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "So, wie ihr es an einer Kugel sehet,", "tokens": ["So", ",", "wie", "ihr", "es", "an", "ei\u00b7ner", "Ku\u00b7gel", "se\u00b7het", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Da\u00df, wenn die eine Helfft\u2019 im Lichte stehet:", "tokens": ["Da\u00df", ",", "wenn", "die", "ei\u00b7ne", "Helfft'", "im", "Lich\u00b7te", "ste\u00b7het", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "ART", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Die andre Seite stets ein dunckler Schatten deckt;", "tokens": ["Die", "and\u00b7re", "Sei\u00b7te", "stets", "ein", "dunck\u00b7ler", "Schat\u00b7ten", "deckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}