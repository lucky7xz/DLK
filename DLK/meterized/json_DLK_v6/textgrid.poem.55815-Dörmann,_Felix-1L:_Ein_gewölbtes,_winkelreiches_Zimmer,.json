{"textgrid.poem.55815": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein gew\u00f6lbtes, winkelreiches Zimmer,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein gew\u00f6lbtes, winkelreiches Zimmer,", "tokens": ["Ein", "ge\u00b7w\u00f6lb\u00b7tes", ",", "win\u00b7kel\u00b7rei\u00b7ches", "Zim\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Krause M\u00f6bel von verblich'ner Pracht,", "tokens": ["Krau\u00b7se", "M\u00f6\u00b7bel", "von", "ver\u00b7blich'\u00b7ner", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Todtenstille; \u2013 gelber Lampenschimmer", "tokens": ["Tod\u00b7ten\u00b7stil\u00b7le", ";", "\u2013", "gel\u00b7ber", "Lam\u00b7pen\u00b7schim\u00b7mer"], "token_info": ["word", "punct", "punct", "word", "word"], "pos": ["NN", "$.", "$(", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Um ein Schreibpult, weiterhin die Nacht.", "tokens": ["Um", "ein", "Schreib\u00b7pult", ",", "wei\u00b7ter\u00b7hin", "die", "Nacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,", "PWAV", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Halbgeschloss'nen Auges, schlummertrunken,", "tokens": ["Halb\u00b7ge\u00b7schloss'\u00b7nen", "Au\u00b7ges", ",", "schlum\u00b7mer\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Tief im Lehnstuhl, \u00fcberschlank und bleich", "tokens": ["Tief", "im", "Lehn\u00b7stuhl", ",", "\u00fc\u00b7bersc\u00b7hlank", "und", "bleich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "$,", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Ruht ein J\u00fcngling, in sich selbst versunken,", "tokens": ["Ruht", "ein", "J\u00fcng\u00b7ling", ",", "in", "sich", "selbst", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "APPR", "PRF", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Ohne Regung, einem Todten gleich.", "tokens": ["Oh\u00b7ne", "Re\u00b7gung", ",", "ei\u00b7nem", "Tod\u00b7ten", "gleich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Pl\u00f6tzlich aber f\u00e4hrt er aus den Kissen,", "tokens": ["Pl\u00f6tz\u00b7lich", "a\u00b7ber", "f\u00e4hrt", "er", "aus", "den", "Kis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Wie ber\u00fchrt von Geisterhand, empor,", "tokens": ["Wie", "be\u00b7r\u00fchrt", "von", "Geis\u00b7ter\u00b7hand", ",", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVPP", "APPR", "NN", "$,", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Dumpfgebroch'nen Lautes, qualzerrissen,", "tokens": ["Dumpf\u00b7ge\u00b7bro\u00b7ch'\u00b7nen", "Lau\u00b7tes", ",", "qual\u00b7zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Rollt und grollt es j\u00e4h aus ihm hervor:", "tokens": ["Rollt", "und", "grollt", "es", "j\u00e4h", "aus", "ihm", "her\u00b7vor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "\u00bbschwach und hilflos, aller Welt zum Hohne?", "tokens": ["\u00bb", "schwach", "und", "hil\u00b7flos", ",", "al\u00b7ler", "Welt", "zum", "Hoh\u00b7ne", "?"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "KON", "ADJD", "$,", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Alte Puppen im erneuten Kleid?", "tokens": ["Al\u00b7te", "Pup\u00b7pen", "im", "er\u00b7neu\u00b7ten", "Kleid", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "Nur ein lendenlahmer Epigone?!", "tokens": ["Nur", "ein", "len\u00b7den\u00b7lah\u00b7mer", "E\u00b7pi\u00b7go\u00b7ne", "?!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Das mein Schicksal?! Nein! noch ist es Zeit!", "tokens": ["Das", "mein", "Schick\u00b7sal", "?!", "Nein", "!", "noch", "ist", "es", "Zeit", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "$.", "PTKANT", "$.", "ADV", "VAFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.17": {"text": "Sei's durch Gift und Kunst auch, ich will denken,", "tokens": ["Sei's", "durch", "Gift", "und", "Kunst", "auch", ",", "ich", "will", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "KON", "NN", "ADV", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Will unsterblich und ein Dichter sein!\u00ab", "tokens": ["Will", "uns\u00b7terb\u00b7lich", "und", "ein", "Dich\u00b7ter", "sein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "ADJD", "KON", "ART", "NN", "VAINF", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.19": {"text": "Zitternd zuckt die Faust nach tr\u00fcben Tr\u00e4nken,", "tokens": ["Zit\u00b7ternd", "zuckt", "die", "Faust", "nach", "tr\u00fc\u00b7ben", "Tr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Gierig saugen sie die Lippen ein;", "tokens": ["Gie\u00b7rig", "sau\u00b7gen", "sie", "die", "Lip\u00b7pen", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.21": {"text": "Fieberhaft beginnt das Blut zu kreisen,", "tokens": ["Fie\u00b7ber\u00b7haft", "be\u00b7ginnt", "das", "Blut", "zu", "krei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Dunkler seine Wange gl\u00e4nzt und gl\u00fcht,", "tokens": ["Dunk\u00b7ler", "sei\u00b7ne", "Wan\u00b7ge", "gl\u00e4nzt", "und", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.23": {"text": "Seinem Mund entquellen wilde Weisen,", "tokens": ["Sei\u00b7nem", "Mund", "ent\u00b7quel\u00b7len", "wil\u00b7de", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.24": {"text": "Sein entflammtes Auge blitzt und spr\u00fcht.", "tokens": ["Sein", "ent\u00b7flamm\u00b7tes", "Au\u00b7ge", "blitzt", "und", "spr\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.25": {"text": "Raubthierw\u00fcthig jagt er durch das Zimmer,", "tokens": ["Raubt\u00b7hier\u00b7w\u00fct\u00b7hig", "jagt", "er", "durch", "das", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.26": {"text": "Von den Schl\u00e4fen tropft's ihm hei\u00df und kalt,", "tokens": ["Von", "den", "Schl\u00e4\u00b7fen", "tropft's", "ihm", "hei\u00df", "und", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.27": {"text": "Jubel wechselt mit der Qual Gewimmer,", "tokens": ["Ju\u00b7bel", "wech\u00b7selt", "mit", "der", "Qual", "Ge\u00b7wim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.28": {"text": "Und er donnert, s\u00e4uselt, kreischt und lallt.", "tokens": ["Und", "er", "don\u00b7nert", ",", "s\u00e4u\u00b7selt", ",", "kreischt", "und", "lallt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.29": {"text": "Da \u2013 auf einmal steht er traumverloren,", "tokens": ["Da", "\u2013", "auf", "ein\u00b7mal", "steht", "er", "traum\u00b7ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "APPR", "ADV", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.30": {"text": "Nur sein Fieberauge starrt und starrt:", "tokens": ["Nur", "sein", "Fie\u00b7be\u00b7rau\u00b7ge", "starrt", "und", "starrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.31": {"text": "Ist's ein Mensch, gleich ihm in Fleisch geboren,", "tokens": ["Ist's", "ein", "Mensch", ",", "gleich", "ihm", "in", "Fleisch", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "ADV", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.32": {"text": "Ist's ein Trugbild, das die Sinne narrt? \u2013", "tokens": ["Ist's", "ein", "Trug\u00b7bild", ",", "das", "die", "Sin\u00b7ne", "narrt", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.33": {"text": "Majest\u00e4tisch schreitet ihm entgegen", "tokens": ["Ma\u00b7jes\u00b7t\u00e4\u00b7tisch", "schrei\u00b7tet", "ihm", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.34": {"text": "Eines Mannes m\u00e4chtige Gestalt;", "tokens": ["Ei\u00b7nes", "Man\u00b7nes", "m\u00e4ch\u00b7ti\u00b7ge", "Ge\u00b7stalt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.35": {"text": "Um das marmorbleiche Antlitz legen", "tokens": ["Um", "das", "mar\u00b7mor\u00b7blei\u00b7che", "Ant\u00b7litz", "le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.36": {"text": "Sich die Locken schwer und dichtgeballt,", "tokens": ["Sich", "die", "Lo\u00b7cken", "schwer", "und", "dicht\u00b7ge\u00b7ballt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.37": {"text": "Von den Schultern quellen reichgeraffte,", "tokens": ["Von", "den", "Schul\u00b7tern", "quel\u00b7len", "reich\u00b7ge\u00b7raff\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.38": {"text": "K\u00f6nigliche Falten, goldverbr\u00e4mt,", "tokens": ["K\u00f6\u00b7nig\u00b7li\u00b7che", "Fal\u00b7ten", ",", "gold\u00b7ver\u00b7br\u00e4mt", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.39": {"text": "Und das Haupt, das sch\u00f6ne, grausenhafte,", "tokens": ["Und", "das", "Haupt", ",", "das", "sch\u00f6\u00b7ne", ",", "grau\u00b7sen\u00b7haf\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.40": {"text": "R\u00e4thselvolle, ist bediademt.", "tokens": ["R\u00e4th\u00b7sel\u00b7vol\u00b7le", ",", "ist", "be\u00b7di\u00b7a\u00b7demt", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.41": {"text": "Seltsam-starre, dunkle Blicke senken", "tokens": ["Selt\u00b7sam\u00b7star\u00b7re", ",", "dunk\u00b7le", "Bli\u00b7cke", "sen\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.42": {"text": "Sich vom Gaste zu dem Dichter her,", "tokens": ["Sich", "vom", "Gas\u00b7te", "zu", "dem", "Dich\u00b7ter", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.43": {"text": "Sie verl\u00f6schten Dichten ihm und Denken", "tokens": ["Sie", "ver\u00b7l\u00f6schten", "Dich\u00b7ten", "ihm", "und", "Den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "PPER", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "L\u00f6schen, l\u00f6schen alles los und leer.", "tokens": ["L\u00f6\u00b7schen", ",", "l\u00f6\u00b7schen", "al\u00b7les", "los", "und", "leer", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PIS", "PTKVZ", "KON", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.45": {"text": "Schauernd vor dem Gast im Scharlachkleide", "tokens": ["Schau\u00b7ernd", "vor", "dem", "Gast", "im", "Schar\u00b7lach\u00b7klei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.46": {"text": "Mit dem d\u00fcster-sch\u00f6nen Angesicht,", "tokens": ["Mit", "dem", "d\u00fcs\u00b7ter\u00b7sch\u00f6\u00b7nen", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.47": {"text": "Mit dem kronengleichen Hauptgeschmeide,", "tokens": ["Mit", "dem", "kro\u00b7nen\u00b7glei\u00b7chen", "Haupt\u00b7ge\u00b7schmei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.48": {"text": "Schauernd \u2013 der Poet zusammenbricht.", "tokens": ["Schau\u00b7ernd", "\u2013", "der", "Po\u00b7et", "zu\u00b7sam\u00b7men\u00b7bricht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$(", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.49": {"text": "Da, wie sich im Fall die Augen wenden", "tokens": ["Da", ",", "wie", "sich", "im", "Fall", "die", "Au\u00b7gen", "wen\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "PRF", "APPRART", "NN", "ART", "NN", "VVINF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.50": {"text": "Wie sein Blick mit eins zu Boden rollt,", "tokens": ["Wie", "sein", "Blick", "mit", "eins", "zu", "Bo\u00b7den", "rollt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "APPR", "PIS", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.51": {"text": "Sieht er pl\u00f6tzlich in des Gastes H\u00e4nden", "tokens": ["Sieht", "er", "pl\u00f6tz\u00b7lich", "in", "des", "Gas\u00b7tes", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.52": {"text": "Einen Kranz von dunkelrothem Gold.", "tokens": ["Ei\u00b7nen", "Kranz", "von", "dun\u00b7kel\u00b7ro\u00b7them", "Gold", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.53": {"text": "Jubel wird und Lust sein banges St\u00f6hnen", "tokens": ["Ju\u00b7bel", "wird", "und", "Lust", "sein", "ban\u00b7ges", "St\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "KON", "NN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.54": {"text": "Siegestrunken, stolz sein Aug' erglimmt!", "tokens": ["Sie\u00b7ge\u00b7strun\u00b7ken", ",", "stolz", "sein", "Aug'", "er\u00b7glimmt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.55": {"text": "\u00bbach, Du kamst als Dichter mich zu kr\u00f6nen,", "tokens": ["\u00bb", "ach", ",", "Du", "kamst", "als", "Dich\u00b7ter", "mich", "zu", "kr\u00f6\u00b7nen", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "PPER", "VVFIN", "KOUS", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.56": {"text": "Mir zum Preis ist dieser Kranz bestimmt!", "tokens": ["Mir", "zum", "Preis", "ist", "die\u00b7ser", "Kranz", "be\u00b7stimmt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VAFIN", "PDAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.57": {"text": "Lass Dein h\u00e4misch-zages Zaudern enden;", "tokens": ["Lass", "Dein", "h\u00e4\u00b7mischza\u00b7ges", "Zau\u00b7dern", "en\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.58": {"text": "Das der h\u00f6chsten Wonne mich beraubt,", "tokens": ["Das", "der", "h\u00f6chs\u00b7ten", "Won\u00b7ne", "mich", "be\u00b7raubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.59": {"text": "Her den Kranz! Ich selbst will ihn vollenden", "tokens": ["Her", "den", "Kranz", "!", "Ich", "selbst", "will", "ihn", "voll\u00b7en\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "PPER", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.60": {"text": "Meinen Sieg\u00ab \u2013 und er umzinkt sein Haupt. \u2013", "tokens": ["Mei\u00b7nen", "Sieg", "\u00ab", "\u2013", "und", "er", "um\u00b7zinkt", "sein", "Haupt", ".", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$(", "$(", "KON", "PPER", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.61": {"text": "Klagend aber st\u00fcrzt er, schreiend nieder,", "tokens": ["Kla\u00b7gend", "a\u00b7ber", "st\u00fcrzt", "er", ",", "schrei\u00b7end", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "PPER", "$,", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.62": {"text": "Denn die Krone gl\u00fcht auf seiner Stirn,", "tokens": ["Denn", "die", "Kro\u00b7ne", "gl\u00fcht", "auf", "sei\u00b7ner", "Stirn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.63": {"text": "Zuckend w\u00e4lzen sich die schlanken Glieder,", "tokens": ["Zu\u00b7ckend", "w\u00e4l\u00b7zen", "sich", "die", "schlan\u00b7ken", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.64": {"text": "Und er st\u00f6hnt: \u00bbMein Hirn, mein armes Hirn!\u00ab", "tokens": ["Und", "er", "st\u00f6hnt", ":", "\u00bb", "Mein", "Hirn", ",", "mein", "ar\u00b7mes", "Hirn", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "$(", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.65": {"text": "Will vom Haupte sich die Krone rei\u00dfen,", "tokens": ["Will", "vom", "Haup\u00b7te", "sich", "die", "Kro\u00b7ne", "rei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "NN", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.66": {"text": "Immer tiefer frisst sie sich hinein.", "tokens": ["Im\u00b7mer", "tie\u00b7fer", "frisst", "sie", "sich", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.67": {"text": "Roth und r\u00f6ther ihre Zacken glei\u00dfen,", "tokens": ["Roth", "und", "r\u00f6\u00b7ther", "ih\u00b7re", "Za\u00b7cken", "glei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.68": {"text": "Schauerlich umloht ihn Purpurschein.", "tokens": ["Schau\u00b7er\u00b7lich", "um\u00b7loht", "ihn", "Pur\u00b7pur\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.69": {"text": "Im verglasten Aug' ein letztes Schimmern,", "tokens": ["Im", "ver\u00b7glas\u00b7ten", "Aug'", "ein", "letz\u00b7tes", "Schim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.70": {"text": "Halberstickt von rettungsloser Nacht,", "tokens": ["Hal\u00b7ber\u00b7stickt", "von", "ret\u00b7tungs\u00b7lo\u00b7ser", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.71": {"text": "Schaumbedeckt die blauen Lippen wimmern:", "tokens": ["Schaum\u00b7be\u00b7deckt", "die", "blau\u00b7en", "Lip\u00b7pen", "wim\u00b7mern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.72": {"text": "\u00bbdiese Krone, wer hat sie gebracht?", "tokens": ["\u00bb", "die\u00b7se", "Kro\u00b7ne", ",", "wer", "hat", "sie", "ge\u00b7bracht", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDAT", "NN", "$,", "PWS", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.73": {"text": "Mann, wer bist Du, wer hat Dich gesendet?", "tokens": ["Mann", ",", "wer", "bist", "Du", ",", "wer", "hat", "Dich", "ge\u00b7sen\u00b7det", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VAFIN", "PPER", "$,", "PWS", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.74": {"text": "Nein, zuerst die Krone mir vom Haupt,", "tokens": ["Nein", ",", "zu\u00b7erst", "die", "Kro\u00b7ne", "mir", "vom", "Haupt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "ART", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.75": {"text": "Dass die Qual, die Todesqual doch endet,", "tokens": ["Dass", "die", "Qual", ",", "die", "To\u00b7des\u00b7qual", "doch", "en\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.76": {"text": "Die den Sch\u00e4del mir zu Scherben schraubt.", "tokens": ["Die", "den", "Sch\u00e4\u00b7del", "mir", "zu", "Scher\u00b7ben", "schraubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.77": {"text": "Soll ich vor Dir winseln?, soll ich beten? \u2013", "tokens": ["Soll", "ich", "vor", "Dir", "win\u00b7seln", "?", ",", "soll", "ich", "be\u00b7ten", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "VVINF", "$.", "$,", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.78": {"text": "Sag', was Deinen Qualenhunger stillt!", "tokens": ["Sag'", ",", "was", "Dei\u00b7nen", "Qua\u00b7len\u00b7hun\u00b7ger", "stillt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.79": {"text": "Willst Du meine Seele ganz zertreten?", "tokens": ["Willst", "Du", "mei\u00b7ne", "See\u00b7le", "ganz", "zer\u00b7tre\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.80": {"text": "Nicht den Blick, aus dem der Wahnwitz quillt,", "tokens": ["Nicht", "den", "Blick", ",", "aus", "dem", "der", "Wahn\u00b7witz", "quillt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$,", "APPR", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.81": {"text": "Weg mit Dir! Wer trug nach Dir Verlangen?,", "tokens": ["Weg", "mit", "Dir", "!", "Wer", "trug", "nach", "Dir", "Ver\u00b7lan\u00b7gen", "?", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "PPER", "$.", "PWS", "VVFIN", "APPR", "PPER", "NN", "$.", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.82": {"text": "Dein Geschenk ist Untergang und Graus!", "tokens": ["Dein", "Ge\u00b7schenk", "ist", "Un\u00b7ter\u00b7gang", "und", "Graus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.83": {"text": "Deinen Bruder glaubt ich zu empfangen,", "tokens": ["Dei\u00b7nen", "Bru\u00b7der", "glaubt", "ich", "zu", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.84": {"text": "Warum tratest Du zu mir in's Haus?\u00ab", "tokens": ["Wa\u00b7rum", "tra\u00b7test", "Du", "zu", "mir", "in's", "Haus", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "PPER", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.85": {"text": "Und, mit hochgespreizten Geierkrallen,", "tokens": ["Und", ",", "mit", "hoch\u00b7ge\u00b7spreiz\u00b7ten", "Gei\u00b7er\u00b7kral\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.86": {"text": "Springt er br\u00fcllend auf den dunklen Gast,", "tokens": ["Springt", "er", "br\u00fcl\u00b7lend", "auf", "den", "dunk\u00b7len", "Gast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.87": {"text": "Taumelt r\u00fcckw\u00e4rts mit verthiertem Lallen,", "tokens": ["Tau\u00b7melt", "r\u00fcck\u00b7w\u00e4rts", "mit", "ver\u00b7thier\u00b7tem", "Lal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.88": {"text": "St\u00fcrzt zu Boden, von der Wuth erfasst;", "tokens": ["St\u00fcrzt", "zu", "Bo\u00b7den", ",", "von", "der", "Wuth", "er\u00b7fasst", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.89": {"text": "In den Kleidern w\u00fchlt er mit den N\u00e4geln,", "tokens": ["In", "den", "Klei\u00b7dern", "w\u00fchlt", "er", "mit", "den", "N\u00e4\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.90": {"text": "Bis er sich auf Fleisch und Knochen gr\u00e4bt,", "tokens": ["Bis", "er", "sich", "auf", "Fleisch", "und", "Kno\u00b7chen", "gr\u00e4bt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.91": {"text": "Seine F\u00e4uste schleudert er gleich Schl\u00e4geln", "tokens": ["Sei\u00b7ne", "F\u00e4us\u00b7te", "schleu\u00b7dert", "er", "gleich", "Schl\u00e4\u00b7geln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.92": {"text": "Sich in's Antlitz, bis es Blut verklebt.", "tokens": ["Sich", "in's", "Ant\u00b7litz", ",", "bis", "es", "Blut", "ver\u00b7klebt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "$,", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.93": {"text": "Nimmer ahnt er, dass er je gesungen,", "tokens": ["Nim\u00b7mer", "ahnt", "er", ",", "dass", "er", "je", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.94": {"text": "Nimmer ahnt er, dass er je gedacht,", "tokens": ["Nim\u00b7mer", "ahnt", "er", ",", "dass", "er", "je", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.95": {"text": "All sein Wollen hat mit eins verschlungen", "tokens": ["All", "sein", "Wol\u00b7len", "hat", "mit", "eins", "ver\u00b7schlun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "VAFIN", "APPR", "PIS", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.96": {"text": "Rettungslos, f\u00fcr immerdar die Nacht.", "tokens": ["Ret\u00b7tungs\u00b7los", ",", "f\u00fcr", "im\u00b7mer\u00b7dar", "die", "Nacht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}