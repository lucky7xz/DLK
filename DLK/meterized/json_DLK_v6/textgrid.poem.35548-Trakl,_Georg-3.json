{"textgrid.poem.35548": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "3", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "N\u00e4chtens \u00fcbern kahlen Anger", "tokens": ["N\u00e4ch\u00b7tens", "\u00fc\u00b7bern", "kah\u00b7len", "An\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gaukelt sie in Fiebertr\u00e4umen.", "tokens": ["Gau\u00b7kelt", "sie", "in", "Fie\u00b7ber\u00b7tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00fcrrisch greint der Wind im Anger", "tokens": ["M\u00fcr\u00b7risch", "greint", "der", "Wind", "im", "An\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Mond lauscht aus den B\u00e4umen.", "tokens": ["Und", "der", "Mond", "lauscht", "aus", "den", "B\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "--+++-+-", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Balde rings die Sterne bleichen", "tokens": ["Bal\u00b7de", "rings", "die", "Ster\u00b7ne", "blei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ermattet von Beschwerde", "tokens": ["Und", "er\u00b7mat\u00b7tet", "von", "Be\u00b7schwer\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00e4chsern ihre Wangen bleichen.", "tokens": ["W\u00e4ch\u00b7sern", "ih\u00b7re", "Wan\u00b7gen", "blei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00e4ulnis wittert aus der Erde.", "tokens": ["F\u00e4ul\u00b7nis", "wit\u00b7tert", "aus", "der", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Traurig rauscht das Rohr im T\u00fcmpel", "tokens": ["Trau\u00b7rig", "rauscht", "das", "Rohr", "im", "T\u00fcm\u00b7pel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sie friert in sich gekauert.", "tokens": ["Und", "sie", "friert", "in", "sich", "ge\u00b7kau\u00b7ert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PRF", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fern ein Hahn kr\u00e4ht. \u00dcbern T\u00fcmpel", "tokens": ["Fern", "ein", "Hahn", "kr\u00e4ht", ".", "\u00dc\u00b7bern", "T\u00fcm\u00b7pel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "VVFIN", "$.", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hart und grau der Morgen schauert.", "tokens": ["Hart", "und", "grau", "der", "Mor\u00b7gen", "schau\u00b7ert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}