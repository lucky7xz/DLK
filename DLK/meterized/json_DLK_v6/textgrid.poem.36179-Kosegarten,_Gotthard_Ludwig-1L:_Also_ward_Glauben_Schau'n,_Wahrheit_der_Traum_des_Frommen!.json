{"textgrid.poem.36179": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Also ward Glauben Schau'n, Wahrheit der Traum des Frommen!", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Also ward Glauben Schau'n, Wahrheit der Traum des Frommen!", "tokens": ["Al\u00b7so", "ward", "Glau\u00b7ben", "Schau'n", ",", "Wahr\u00b7heit", "der", "Traum", "des", "From\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "NN", "$,", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Reich Gottes, Reich des Heils, du bist mit Pracht gekommen!", "tokens": ["Reich", "Got\u00b7tes", ",", "Reich", "des", "Heils", ",", "du", "bist", "mit", "Pracht", "ge\u00b7kom\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "ART", "NN", "$,", "PPER", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Fr\u00f6hlich gr\u00fcnt rings das Land, ged\u00fcngt durch M\u00e4rt'rerblut!", "tokens": ["Fr\u00f6h\u00b7lich", "gr\u00fcnt", "rings", "das", "Land", ",", "ge\u00b7d\u00fcngt", "durch", "M\u00e4rt'\u00b7rerblut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "NN", "$,", "VVPP", "APPR", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Der Herr Herr hat gerichtet;", "tokens": ["Der", "Herr", "Herr", "hat", "ge\u00b7rich\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das B\u00f6se ward vernichtet;", "tokens": ["Das", "B\u00f6\u00b7se", "ward", "ver\u00b7nich\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Es siegt nach langem Hader,", "tokens": ["Es", "siegt", "nach", "lan\u00b7gem", "Ha\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Was sch\u00f6n, was wahr, was gut!", "tokens": ["Was", "sch\u00f6n", ",", "was", "wahr", ",", "was", "gut", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADJD", "$,", "PWS", "ADJD", "$,", "PRELS", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Germania frohlockt! Aus ihren hundert Gauen,", "tokens": ["Ger\u00b7ma\u00b7nia", "froh\u00b7lockt", "!", "Aus", "ih\u00b7ren", "hun\u00b7dert", "Gau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "APPR", "PPOSAT", "CARD", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Rechts wo die Donau br\u00fcllt, links wo die Belten blauen,", "tokens": ["Rechts", "wo", "die", "Do\u00b7nau", "br\u00fcllt", ",", "links", "wo", "die", "Bel\u00b7ten", "blau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "NE", "VVFIN", "$,", "ADV", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.3": {"text": "St\u00fcrmt himmelan ihr Psalm, lodert die Opferflamm:", "tokens": ["St\u00fcrmt", "him\u00b7me\u00b7lan", "ihr", "Psalm", ",", "lo\u00b7dert", "die", "Op\u00b7fer\u00b7flamm", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u00bbder Herr Herr hat gerichtet,", "tokens": ["\u00bb", "der", "Herr", "Herr", "hat", "ge\u00b7rich\u00b7tet", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Zwietracht ward geschlichtet;", "tokens": ["Die", "Zwiet\u00b7racht", "ward", "ge\u00b7schlich\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Thuiskons hundert St\u00e4mme", "tokens": ["Thu\u00b7is\u00b7kons", "hun\u00b7dert", "St\u00e4m\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["NE", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sind nun Ein Volk, Ein Stamm!\u00ab", "tokens": ["Sind", "nun", "Ein", "Volk", ",", "Ein", "Stamm", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Auf ", "tokens": ["Auf"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Legt ab den Trauerflor! La\u00dft Rosmarin und Raute!", "tokens": ["Legt", "ab", "den", "Trau\u00b7er\u00b7flor", "!", "La\u00dft", "Ros\u00b7ma\u00b7rin", "und", "Rau\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$.", "VVIMP", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "H\u00fcllt euch in Seid' und Sammt; spart der Geschmeide keins!", "tokens": ["H\u00fcllt", "euch", "in", "Seid'", "und", "Sammt", ";", "spart", "der", "Ge\u00b7schmei\u00b7de", "keins", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "KON", "NN", "$.", "VVFIN", "ART", "NN", "PIAT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das Jawort ist gesprochen;", "tokens": ["Das", "Ja\u00b7wort", "ist", "ge\u00b7spro\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Scheidebrief gebrochen;", "tokens": ["Der", "Schei\u00b7de\u00b7brief", "ge\u00b7bro\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Trauring ward gewechselt;", "tokens": ["Der", "Trau\u00b7ring", "ward", "ge\u00b7wech\u00b7selt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Was Zwei gewest, ist Eins.", "tokens": ["Was", "Zwei", "ge\u00b7west", ",", "ist", "Eins", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "CARD", "VVPP", "$,", "VAFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Was Gott zusammenf\u00fcgt, das soll der Mensch nicht scheiden!", "tokens": ["Was", "Gott", "zu\u00b7sam\u00b7men\u00b7f\u00fcgt", ",", "das", "soll", "der", "Mensch", "nicht", "schei\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "$,", "PDS", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hinfort soll Pommern nicht zwiesp\u00e4ltig Pommern neiden;", "tokens": ["Hin\u00b7fort", "soll", "Pom\u00b7mern", "nicht", "zwie\u00b7sp\u00e4l\u00b7tig", "Pom\u00b7mern", "nei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PTKNEG", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ewig sey Eins hinfort, was Sprach' und Abkunft eint!", "tokens": ["E\u00b7wig", "sey", "Eins", "hin\u00b7fort", ",", "was", "Sprach'", "und", "Ab\u00b7kunft", "eint", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "NN", "PTKVZ", "$,", "PWS", "NN", "KON", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Wohl unter den drei Kronen", "tokens": ["Wohl", "un\u00b7ter", "den", "drei", "Kro\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Lie\u00df sich's gem\u00e4chlich wohnen;", "tokens": ["Lie\u00df", "sich's", "ge\u00b7m\u00e4ch\u00b7lich", "woh\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Doch mag das Band nicht dauern,", "tokens": ["Doch", "mag", "das", "Band", "nicht", "dau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Was die Natur verneint.", "tokens": ["Was", "die", "Na\u00b7tur", "ver\u00b7neint", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Fahr', ", "tokens": ["Fahr'", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Funkl' ewig dir dein Stern, der Angelstern der Wahrheit!", "tokens": ["Funkl'", "e\u00b7wig", "dir", "dein", "Stern", ",", "der", "An\u00b7gels\u00b7tern", "der", "Wahr\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "PPOSAT", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wir scheiden ... Fahre wohl ... Wohl thut das Scheiden weh ....", "tokens": ["Wir", "schei\u00b7den", "...", "Fah\u00b7re", "wohl", "...", "Wohl", "thut", "das", "Schei\u00b7den", "weh", "...."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "VVFIN", "ADV", "$(", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch ach, die Br\u00fcder winken;", "tokens": ["Doch", "ach", ",", "die", "Br\u00fc\u00b7der", "win\u00b7ken", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie winken, und wir sinken", "tokens": ["Sie", "win\u00b7ken", ",", "und", "wir", "sin\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "An Hals und Herz den Br\u00fcdern,", "tokens": ["An", "Hals", "und", "Herz", "den", "Br\u00fc\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und Wollust wird das Weh!", "tokens": ["Und", "Wol\u00b7lust", "wird", "das", "Weh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wer wollte mit Vertrau'n nicht in das Lichtreich treten,", "tokens": ["Wer", "woll\u00b7te", "mit", "Ver\u00b7trau'n", "nicht", "in", "das", "Lich\u00b7treich", "tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "APPR", "NN", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo ", "tokens": ["Wo"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "In Mitten seines Volks, das heldisch ernst und mild;", "tokens": ["In", "Mit\u00b7ten", "sei\u00b7nes", "Volks", ",", "das", "hel\u00b7disch", "ernst", "und", "mild", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$,", "PRELS", "ADJD", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo nicht die Willk\u00fchr schaltet,", "tokens": ["Wo", "nicht", "die", "Will\u00b7k\u00fchr", "schal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wo das Gesetz nur waltet,", "tokens": ["Wo", "das", "Ge\u00b7setz", "nur", "wal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wo noch der V\u00e4ter Glaube,", "tokens": ["Wo", "noch", "der", "V\u00e4\u00b7ter", "Glau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der Alten Weisheit gilt.", "tokens": ["Der", "Al\u00b7ten", "Weis\u00b7heit", "gilt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wer ist, wer sagt uns an, was Staaten weckt und decket,", "tokens": ["Wer", "ist", ",", "wer", "sagt", "uns", "an", ",", "was", "Staa\u00b7ten", "weckt", "und", "de\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PWS", "VVFIN", "PPER", "PTKVZ", "$,", "PWS", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was Thronen st\u00fctzt und schm\u00fcckt, was die Tyrannen schrecket?", "tokens": ["Was", "Thro\u00b7nen", "st\u00fctzt", "und", "schm\u00fcckt", ",", "was", "die", "Ty\u00b7ran\u00b7nen", "schre\u00b7cket", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "KON", "VVFIN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist's nicht der Drang, der k\u00fchn ins Mark der Wesen dringt?", "tokens": ["Ist's", "nicht", "der", "Drang", ",", "der", "k\u00fchn", "ins", "Mark", "der", "We\u00b7sen", "dringt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ART", "NN", "$,", "PRELS", "ADJD", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nie mag ein Volk verknechten,", "tokens": ["Nie", "mag", "ein", "Volk", "ver\u00b7knech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nie ein Geschlecht verschlechten,", "tokens": ["Nie", "ein", "Ge\u00b7schlecht", "ver\u00b7schlech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das nach dem Vlie\u00df der Wahrheit", "tokens": ["Das", "nach", "dem", "Vlie\u00df", "der", "Wahr\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und Sch\u00f6nheit rastlos ringt!", "tokens": ["Und", "Sch\u00f6n\u00b7heit", "rast\u00b7los", "ringt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Preis dir Germania! An deinen Stromgestaden", "tokens": ["Preis", "dir", "Ger\u00b7ma\u00b7nia", "!", "An", "dei\u00b7nen", "Strom\u00b7ge\u00b7sta\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "NE", "$.", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Lustwandelt rings der Chor der Helikoniaden;", "tokens": ["Lust\u00b7wan\u00b7delt", "rings", "der", "Chor", "der", "He\u00b7li\u00b7ko\u00b7ni\u00b7a\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "++-+-+-+-+-+-", "