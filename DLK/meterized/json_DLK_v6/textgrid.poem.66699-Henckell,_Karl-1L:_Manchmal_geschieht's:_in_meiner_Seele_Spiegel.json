{"textgrid.poem.66699": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Manchmal geschieht's: in meiner Seele Spiegel", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Manchmal geschieht's: in meiner Seele Spiegel", "tokens": ["Manch\u00b7mal", "ge\u00b7schieht's", ":", "in", "mei\u00b7ner", "See\u00b7le", "Spie\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NE", "$.", "APPR", "PPOSAT", "NN", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "F\u00e4llt j\u00e4h ein lockender Flitterstein der Welt,", "tokens": ["F\u00e4llt", "j\u00e4h", "ein", "lo\u00b7cken\u00b7der", "Flit\u00b7ter\u00b7stein", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Unruhig zittern die gest\u00f6rten Linien ...", "tokens": ["Un\u00b7ru\u00b7hig", "zit\u00b7tern", "die", "ge\u00b7st\u00f6r\u00b7ten", "Li\u00b7ni\u00b7en", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "\u00bbgeh, gib dem Trubel deine Stille preis!\u00ab", "tokens": ["\u00bb", "geh", ",", "gib", "dem", "Tru\u00b7bel", "dei\u00b7ne", "Stil\u00b7le", "preis", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$,", "VVIMP", "ART", "NN", "PPOSAT", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So gurgelt's, wo die Oberfl\u00e4che kr\u00e4uselt,", "tokens": ["So", "gur\u00b7gelt's", ",", "wo", "die", "O\u00b7berf\u00b7l\u00e4\u00b7che", "kr\u00e4u\u00b7selt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbgeh, mische dich dem rauschenden Gepr\u00e4ng!", "tokens": ["\u00bb", "geh", ",", "mi\u00b7sche", "dich", "dem", "rau\u00b7schen\u00b7den", "Ge\u00b7pr\u00e4ng", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich wei\u00df ein Reich, wo rascher Ruhm dir lacht,", "tokens": ["Ich", "wei\u00df", "ein", "Reich", ",", "wo", "ra\u00b7scher", "Ruhm", "dir", "lacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PWAV", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich wei\u00df ein Reich, wo du der Ehren Kranz,", "tokens": ["Ich", "wei\u00df", "ein", "Reich", ",", "wo", "du", "der", "Eh\u00b7ren", "Kranz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PWAV", "PPER", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Den eine goldverbr\u00e4mte Sch\u00f6ne pendelt,", "tokens": ["Den", "ei\u00b7ne", "gold\u00b7ver\u00b7br\u00e4m\u00b7te", "Sch\u00f6\u00b7ne", "pen\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Mit deinem kleinen Finger schon erhaschst", "tokens": ["Mit", "dei\u00b7nem", "klei\u00b7nen", "Fin\u00b7ger", "schon", "er\u00b7haschst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und thronst in Pomp und Pracht vor allen Leuten.", "tokens": ["Und", "thronst", "in", "Pomp", "und", "Pracht", "vor", "al\u00b7len", "Leu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NE", "KON", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Reizt es dich nicht, nach dieses Reiches Macht", "tokens": ["Reizt", "es", "dich", "nicht", ",", "nach", "die\u00b7ses", "Rei\u00b7ches", "Macht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "PTKNEG", "$,", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Den kleinen Finger spielend auszustrecken?\u00ab", "tokens": ["Den", "klei\u00b7nen", "Fin\u00b7ger", "spie\u00b7lend", "aus\u00b7zu\u00b7stre\u00b7cken", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Manchmal geschieht's: in meiner Seele Spiegel", "tokens": ["Manch\u00b7mal", "ge\u00b7schieht's", ":", "in", "mei\u00b7ner", "See\u00b7le", "Spie\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NE", "$.", "APPR", "PPOSAT", "NN", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.12": {"text": "F\u00e4llt j\u00e4h ein lockender Flitterstein der Welt ...", "tokens": ["F\u00e4llt", "j\u00e4h", "ein", "lo\u00b7cken\u00b7der", "Flit\u00b7ter\u00b7stein", "der", "Welt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "ART", "NN", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Aus Tiefen kommt ein Ton, und eine gro\u00dfe", "tokens": ["Aus", "Tie\u00b7fen", "kommt", "ein", "Ton", ",", "und", "ei\u00b7ne", "gro\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,", "KON", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Grundwelle wischt des Spiegels St\u00f6rung fort.", "tokens": ["Grund\u00b7wel\u00b7le", "wischt", "des", "Spie\u00b7gels", "St\u00f6\u00b7rung", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}}}}