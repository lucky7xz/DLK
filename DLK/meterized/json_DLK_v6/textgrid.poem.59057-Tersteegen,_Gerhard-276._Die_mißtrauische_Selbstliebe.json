{"textgrid.poem.59057": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "276. Die mi\u00dftrauische Selbstliebe", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dir selbst zu helfen, nicht auskehr!", "tokens": ["Dir", "selbst", "zu", "hel\u00b7fen", ",", "nicht", "aus\u00b7kehr", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "$,", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du liebst dich, Gott liebt dich weit mehr.", "tokens": ["Du", "liebst", "dich", ",", "Gott", "liebt", "dich", "weit", "mehr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "VVFIN", "PPER", "ADJD", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sei nur ganz Gottes, wie Gott deine,", "tokens": ["Sei", "nur", "ganz", "Got\u00b7tes", ",", "wie", "Gott", "dei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "NN", "$,", "PWAV", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er schaut auf dich, schau ihn alleine!", "tokens": ["Er", "schaut", "auf", "dich", ",", "schau", "ihn", "al\u00b7lei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}