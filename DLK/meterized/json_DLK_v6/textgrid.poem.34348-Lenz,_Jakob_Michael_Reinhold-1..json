{"textgrid.poem.34348": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach ihr jungen Rosen, du bebl\u00fcmtes Gras,", "tokens": ["Ach", "ihr", "jun\u00b7gen", "Ro\u00b7sen", ",", "du", "be\u00b7bl\u00fcm\u00b7tes", "Gras", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "ADJA", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Die sein Blick behauchte, seyd ihr nun so blass!", "tokens": ["Die", "sein", "Blick", "be\u00b7hauch\u00b7te", ",", "seyd", "ihr", "nun", "so", "blass", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Wessen Aug' und Herz nicht rein,", "tokens": ["Wes\u00b7sen", "Aug'", "und", "Herz", "nicht", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kann der euer Maler seyn?", "tokens": ["Kann", "der", "eu\u00b7er", "Ma\u00b7ler", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}