{"dta.poem.19009": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Cartel", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Wjr kommen nicht hieher Vns selbsten vil zu", "tokens": ["Wjr", "kom\u00b7men", "nicht", "hie\u00b7her", "Vns", "selbs\u00b7ten", "vil", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKNEG", "PAV", "PPER", "VVFIN", "ADV", "APPR"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "r\u00fchmen/", "tokens": ["r\u00fch\u00b7men", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Oder durch frembde sprach die warheit zuverbl\u00fcme\u0304/", "tokens": ["O\u00b7der", "durch", "fremb\u00b7de", "sprach", "die", "war\u00b7heit", "zu\u00b7ver\u00b7bl\u00fcm\u0113", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "VVFIN", "ART", "NN", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als ob wir k\u00e4men her au\u00df einem End der Welt/", "tokens": ["Als", "ob", "wir", "k\u00e4\u00b7men", "her", "au\u00df", "ei\u00b7nem", "End", "der", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Oder wider belebt von Elisischem Feld.", "tokens": ["O\u00b7der", "wi\u00b7der", "be\u00b7lebt", "von", "E\u00b7li\u00b7si\u00b7schem", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "VVPP", "APPR", "NE", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Nein. Teufel seind wir nicht/ noch Risen/ noch halb-", "tokens": ["Nein", ".", "Teu\u00b7fel", "seind", "wir", "nicht", "/", "noch", "Ri\u00b7sen", "/", "noch", "halb"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "$.", "NN", "VAFIN", "PPER", "PTKNEG", "$(", "ADV", "NN", "$(", "ADV", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "G\u00f6tter/", "tokens": ["G\u00f6t\u00b7ter", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Noch Helden/ noch Wildleut/ noch vnsers Lands", "tokens": ["Noch", "Hel\u00b7den", "/", "noch", "Wild\u00b7leut", "/", "noch", "vn\u00b7sers", "Lands"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "NN", "$(", "ADV", "NE", "$(", "ADV", "ADJA", "NN"], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "versp\u00f6tter;", "tokens": ["ver\u00b7sp\u00f6t\u00b7ter", ";"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Das edle Teutsche Reich ist vnser Vatterland", "tokens": ["Das", "ed\u00b7le", "Teut\u00b7sche", "Reich", "ist", "vn\u00b7ser", "Vat\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Teutsch seind wir von geburt/ von stammen/ hertz vnd", "tokens": ["Teutsch", "seind", "wir", "von", "ge\u00b7burt", "/", "von", "stam\u00b7men", "/", "hertz", "vnd"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "APPR", "NE", "$(", "APPR", "VVFIN", "$(", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "hand;", "tokens": ["hand", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Was dient es frembden prei\u00df vnd Namen zu ent-", "tokens": ["Was", "dient", "es", "fremb\u00b7den", "prei\u00df", "vnd", "Na\u00b7men", "zu", "ent"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADJA", "NN", "KON", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "lehnen?", "tokens": ["leh\u00b7nen", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Teutschland bedarff sich nicht mit Au\u00dfl\u00e4ndern be-", "tokens": ["Teutschland", "be\u00b7darff", "sich", "nicht", "mit", "Au\u00df\u00b7l\u00e4n\u00b7dern", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PRF", "PTKNEG", "APPR", "NN", "TRUNC"], "meter": "+-+-++-+--", "measure": "unknown.measure.penta"}, "line.16": {"text": "sch\u00f6nen:", "tokens": ["sch\u00f6\u00b7nen", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Wie dan die welt wol weist/ da\u00df Teutschland alle zeit", "tokens": ["Wie", "dan", "die", "welt", "wol", "weist", "/", "da\u00df", "Teutschland", "al\u00b7le", "zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "ADV", "VVFIN", "$(", "KOUS", "NE", "PIAT", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.18": {"text": "Hat leut f\u00fcrtrefflich gnug zum friden vnd zum streit.", "tokens": ["Hat", "leut", "f\u00fcr\u00b7treff\u00b7lich", "gnug", "zum", "fri\u00b7den", "vnd", "zum", "streit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "ADV", "APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Darum\u0303 ob wir wol jung nicht sonders vil erfahre\u0304/", "tokens": ["Dar\u00b7um\u0303", "ob", "wir", "wol", "jung", "nicht", "son\u00b7ders", "vil", "er\u00b7fahr\u0113", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KOUS", "PPER", "ADV", "ADJD", "PTKNEG", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Begehre\u0304 wir doch nicht die haut noch faust zu spahre\u0304:", "tokens": ["Be\u00b7gehr\u0113", "wir", "doch", "nicht", "die", "haut", "noch", "faust", "zu", "spahr\u0113", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PTKNEG", "ART", "VVFIN", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Sondern erscheinen nur in vnser Teutschen tracht", "tokens": ["Son\u00b7dern", "er\u00b7schei\u00b7nen", "nur", "in", "vn\u00b7ser", "Teut\u00b7schen", "tracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.22": {"text": "Mit redlich-Teutschem muht/ vmb vnser erste macht", "tokens": ["Mit", "red\u00b7lich\u00b7Teut\u00b7schem", "muht", "/", "vmb", "vn\u00b7ser", "ers\u00b7te", "macht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "VVFIN", "$(", "APPR", "PPOSAT", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "An disen Rittern hie (die so hoch triumfieren)", "tokens": ["An", "di\u00b7sen", "Rit\u00b7tern", "hie", "(", "die", "so", "hoch", "tri\u00b7um\u00b7fie\u00b7ren", ")"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "$(", "ART", "ADV", "ADJD", "VVINF", "$("], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.24": {"text": "Jhrer begird gem\u00e4\u00df/ gewafnet zu probieren;", "tokens": ["Ih\u00b7rer", "be\u00b7gird", "ge\u00b7m\u00e4\u00df", "/", "ge\u00b7waf\u00b7net", "zu", "pro\u00b7bie\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$(", "VVPP", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.25": {"text": "Verhoffend/ zweifels frey/ da\u00df dise erste prob/", "tokens": ["Ver\u00b7hof\u00b7fend", "/", "zwei\u00b7fels", "frey", "/", "da\u00df", "di\u00b7se", "ers\u00b7te", "prob", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$(", "ADV", "ADJD", "$(", "KOUS", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Vollendend jhren ruhm/ anfangen soll das lob/", "tokens": ["Voll\u00b7en\u00b7dend", "jhren", "ruhm", "/", "an\u00b7fan\u00b7gen", "soll", "das", "lob", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$(", "VVINF", "VMFIN", "ART", "NN", "$("], "meter": "---+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.27": {"text": "So man von nunan wirt durch die straich vnsrer", "tokens": ["So", "man", "von", "nu\u00b7nan", "wirt", "durch", "die", "straich", "vns\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "APPR", "ADV", "VAFIN", "APPR", "ART", "ADJD", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "w\u00f6hren", "tokens": ["w\u00f6h\u00b7ren"], "token_info": ["word"], "pos": ["VVINF"], "meter": "+-", "measure": "trochaic.single"}, "line.29": {"text": "Vnder dem Firmament t\u00e4glich erschallen h\u00f6ren.", "tokens": ["Vn\u00b7der", "dem", "Fir\u00b7ma\u00b7ment", "t\u00e4g\u00b7lich", "er\u00b7schal\u00b7len", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVINF", "VVINF", "$."], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}}}}}