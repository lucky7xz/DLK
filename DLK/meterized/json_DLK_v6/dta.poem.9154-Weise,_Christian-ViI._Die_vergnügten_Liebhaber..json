{"dta.poem.9154": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "ViI.  \n Die vergn\u00fcgten Liebhaber.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "So liegen die neuen verliebten beysammen", "tokens": ["So", "lie\u00b7gen", "die", "neu\u00b7en", "ver\u00b7lieb\u00b7ten", "bey\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "In unverruckter hertzens-lust/", "tokens": ["In", "un\u00b7ver\u00b7ruck\u00b7ter", "hert\u00b7zens\u00b7lust", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und locken die hitze der ehrlichen flammen", "tokens": ["Und", "lo\u00b7cken", "die", "hit\u00b7ze", "der", "ehr\u00b7li\u00b7chen", "flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "ADJA", "VVINF"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Au\u00df ihrer tugendhafften brust/", "tokens": ["Au\u00df", "ih\u00b7rer", "tu\u00b7gend\u00b7haff\u00b7ten", "brust", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So schertzet und spottet das edele paar", "tokens": ["So", "schert\u00b7zet", "und", "spot\u00b7tet", "das", "e\u00b7de\u00b7le", "paar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Der nimmer-vergn\u00fcglichen liebes-gefahr.", "tokens": ["Der", "nim\u00b7mer\u00b7ver\u00b7gn\u00fcg\u00b7li\u00b7chen", "lie\u00b7bes\u00b7ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+--++-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "2. Jhr liechter des Himmels ach sehet sie liegen/", "tokens": ["Ihr", "liech\u00b7ter", "des", "Him\u00b7mels", "ach", "se\u00b7het", "sie", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "ADV", "VVFIN", "PPER", "VVFIN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Seht wie sie durch die sch\u00f6ne nacht", "tokens": ["Seht", "wie", "sie", "durch", "die", "sch\u00f6\u00b7ne", "nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jhr lieben im lieben durch lieben vergn\u00fcgen/", "tokens": ["Ihr", "lie\u00b7ben", "im", "lie\u00b7ben", "durch", "lie\u00b7ben", "ver\u00b7gn\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "APPR", "ADJA", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Bi\u00df Phosphorus den morgen macht:", "tokens": ["Bi\u00df", "Phos\u00b7pho\u00b7rus", "den", "mor\u00b7gen", "macht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ART", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da lernen sie langsam und schl\u00e4fferig seyn", "tokens": ["Da", "ler\u00b7nen", "sie", "lang\u00b7sam", "und", "schl\u00e4f\u00b7fe\u00b7rig", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "VAINF"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und schlaffen in werther zufriedenheit ein.", "tokens": ["Und", "schlaf\u00b7fen", "in", "wert\u00b7her", "zu\u00b7frie\u00b7den\u00b7heit", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "3. Sie k\u00f6nnen sich k\u00fchnlich einander erkl\u00e4ren/", "tokens": ["Sie", "k\u00f6n\u00b7nen", "sich", "k\u00fchn\u00b7lich", "ein\u00b7an\u00b7der", "er\u00b7kl\u00e4\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADJD", "PRF", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Dann dessenwegen sind sie da/", "tokens": ["Dann", "des\u00b7sen\u00b7we\u00b7gen", "sind", "sie", "da", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "VAFIN", "PPER", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auff alles verlangen auff alles begehren", "tokens": ["Auff", "al\u00b7les", "ver\u00b7lan\u00b7gen", "auff", "al\u00b7les", "be\u00b7geh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "VVFIN", "APPR", "PIS", "VVINF"], "meter": "-+--+-++--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Folgt nichts als ein vergn\u00fcgtes ja/", "tokens": ["Folgt", "nichts", "als", "ein", "ver\u00b7gn\u00fcg\u00b7tes", "ja", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "KOKOM", "ART", "ADJA", "PTKANT", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie heisset sein leben/ er heisset ihr liecht/", "tokens": ["Sie", "heis\u00b7set", "sein", "le\u00b7ben", "/", "er", "heis\u00b7set", "ihr", "liecht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "VVINF", "$(", "PPER", "VVFIN", "PPER", "VVFIN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Sie k\u00fcssen einander und f\u00fcrchten sich nicht.", "tokens": ["Sie", "k\u00fcs\u00b7sen", "ein\u00b7an\u00b7der", "und", "f\u00fcrch\u00b7ten", "sich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "KON", "VVFIN", "PRF", "PTKNEG", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "4. Was manchen verliebten im weitesten felde/", "tokens": ["Was", "man\u00b7chen", "ver\u00b7lieb\u00b7ten", "im", "wei\u00b7tes\u00b7ten", "fel\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "APPRART", "ADJA", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wie eine ro\u00df\u2019 im winter bl\u00fcht.", "tokens": ["Wie", "ei\u00b7ne", "ro\u00df'", "im", "win\u00b7ter", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was mancher nur gleichsam in einem gem\u00e4hlde/", "tokens": ["Was", "man\u00b7cher", "nur", "gleich\u00b7sam", "in", "ei\u00b7nem", "ge\u00b7m\u00e4hl\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADV", "ADJD", "APPR", "ART", "ADJA", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Von seiner lust-vergn\u00fcgung siht/", "tokens": ["Von", "sei\u00b7ner", "lust\u00b7ver\u00b7gn\u00fc\u00b7gung", "siht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das f\u00fchlen sie w\u00fcrcklich und ruffen die ruh/", "tokens": ["Das", "f\u00fch\u00b7len", "sie", "w\u00fcrck\u00b7lich", "und", "ruf\u00b7fen", "die", "ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "ART", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Als eine beliebliche zeugin dazu/", "tokens": ["Als", "ei\u00b7ne", "be\u00b7lieb\u00b7li\u00b7che", "zeu\u00b7gin", "da\u00b7zu", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PAV", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "5. Drum kom\u0303et ihr Gratien/ schwinget die lieder/", "tokens": ["Drum", "kom\u0303et", "ihr", "Gra\u00b7ti\u00b7en", "/", "schwin\u00b7get", "die", "lie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "NE", "$(", "VVFIN", "ART", "NN", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Durch die erfreute Linden-lufft/", "tokens": ["Durch", "die", "er\u00b7freu\u00b7te", "Lin\u00b7den\u00b7lufft", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erf\u00fcllet die gassen bi\u00df Echo hinwieder", "tokens": ["Er\u00b7f\u00fcl\u00b7let", "die", "gas\u00b7sen", "bi\u00df", "E\u00b7cho", "hin\u00b7wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "ADV"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Mit ihrer holen stimme rufft:", "tokens": ["Mit", "ih\u00b7rer", "ho\u00b7len", "stim\u00b7me", "rufft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVINF", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die selig verkn\u00fcpfften die haben gesiegt/", "tokens": ["Die", "se\u00b7lig", "ver\u00b7kn\u00fcpff\u00b7ten", "die", "ha\u00b7ben", "ge\u00b7siegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "PDS", "VAFIN", "VVPP", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Sie leben erfreulich/ und lieben vergn\u00fcgt.", "tokens": ["Sie", "le\u00b7ben", "er\u00b7freu\u00b7lich", "/", "und", "lie\u00b7ben", "ver\u00b7gn\u00fcgt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$(", "KON", "ADJA", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}