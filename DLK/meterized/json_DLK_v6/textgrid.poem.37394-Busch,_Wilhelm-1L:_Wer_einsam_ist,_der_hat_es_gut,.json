{"textgrid.poem.37394": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer einsam ist, der hat es gut,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer einsam ist, der hat es gut,", "tokens": ["Wer", "ein\u00b7sam", "ist", ",", "der", "hat", "es", "gut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "PRELS", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil keiner da, der ihm was tut.", "tokens": ["Weil", "kei\u00b7ner", "da", ",", "der", "ihm", "was", "tut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "$,", "PRELS", "PPER", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihn st\u00f6rt in seinem Lustrevier", "tokens": ["Ihn", "st\u00f6rt", "in", "sei\u00b7nem", "Lus\u00b7tre\u00b7vier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Tier, kein Mensch und kein Klavier,", "tokens": ["Kein", "Tier", ",", "kein", "Mensch", "und", "kein", "Kla\u00b7vier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Und niemand gibt ihm weise Lehren,", "tokens": ["Und", "nie\u00b7mand", "gibt", "ihm", "wei\u00b7se", "Leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die gut gemeint und b\u00f6s zu h\u00f6ren.", "tokens": ["Die", "gut", "ge\u00b7meint", "und", "b\u00f6s", "zu", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "KON", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Welt entronnen, geht er still", "tokens": ["Der", "Welt", "ent\u00b7ron\u00b7nen", ",", "geht", "er", "still"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In Filzpantoffeln, wann er will.", "tokens": ["In", "Filz\u00b7pan\u00b7tof\u00b7feln", ",", "wann", "er", "will", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sogar im Schlafrock wandelt er", "tokens": ["So\u00b7gar", "im", "Schla\u00b7frock", "wan\u00b7delt", "er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Bequem den ganzen Tag umher.", "tokens": ["Be\u00b7quem", "den", "gan\u00b7zen", "Tag", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Er kennt kein weibliches Verbot,", "tokens": ["Er", "kennt", "kein", "weib\u00b7li\u00b7ches", "Ver\u00b7bot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Drum raucht und dampft er wie ein Schlot.", "tokens": ["Drum", "raucht", "und", "dampft", "er", "wie", "ein", "Schlot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "KON", "VVFIN", "PPER", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Gesch\u00fctzt vor fremden Sp\u00e4herblicken,", "tokens": ["Ge\u00b7sch\u00fctzt", "vor", "frem\u00b7den", "Sp\u00e4\u00b7her\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Kann er sich selbst die Hose flicken.", "tokens": ["Kann", "er", "sich", "selbst", "die", "Ho\u00b7se", "fli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Liebt er Musik, so darf er fl\u00f6ten,", "tokens": ["Liebt", "er", "Mu\u00b7sik", ",", "so", "darf", "er", "fl\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "ADV", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.16": {"text": "Um angenehm die Zeit zu t\u00f6ten,", "tokens": ["Um", "an\u00b7ge\u00b7nehm", "die", "Zeit", "zu", "t\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Und laut und kr\u00e4ftig darf er prusten,", "tokens": ["Und", "laut", "und", "kr\u00e4f\u00b7tig", "darf", "er", "prus\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Und ohne R\u00fccksicht darf er husten,", "tokens": ["Und", "oh\u00b7ne", "R\u00fcck\u00b7sicht", "darf", "er", "hus\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Und allgemach vergi\u00dft man seiner.", "tokens": ["Und", "all\u00b7ge\u00b7mach", "ver\u00b7gi\u00dft", "man", "sei\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "PPOSAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Nur allerh\u00f6chstens fragt mal einer:", "tokens": ["Nur", "al\u00b7ler\u00b7h\u00f6chs\u00b7tens", "fragt", "mal", "ei\u00b7ner", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ADV", "PIS", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Was, lebt er noch? Ei, Schwerenot,", "tokens": ["Was", ",", "lebt", "er", "noch", "?", "Ei", ",", "Schwe\u00b7re\u00b7not", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "$,", "VVFIN", "PPER", "ADV", "$.", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Ich dachte l\u00e4ngst, er w\u00e4re tot.", "tokens": ["Ich", "dach\u00b7te", "l\u00e4ngst", ",", "er", "w\u00e4\u00b7re", "tot", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Kurz, abgesehn vom Steuerzahlen,", "tokens": ["Kurz", ",", "ab\u00b7ge\u00b7sehn", "vom", "Steu\u00b7er\u00b7zah\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "L\u00e4\u00dft sich das Gl\u00fcck nicht sch\u00f6ner malen.", "tokens": ["L\u00e4\u00dft", "sich", "das", "Gl\u00fcck", "nicht", "sch\u00f6\u00b7ner", "ma\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Worauf denn auch der Satz beruht:", "tokens": ["Wo\u00b7rauf", "denn", "auch", "der", "Satz", "be\u00b7ruht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Wer einsam ist, der hat es gut.", "tokens": ["Wer", "ein\u00b7sam", "ist", ",", "der", "hat", "es", "gut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "PRELS", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}