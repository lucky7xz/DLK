{"textgrid.poem.55262": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Selbstgef\u00fchl", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jeder ist doch auch ein Mensch!! \u2013", "tokens": ["Je\u00b7der", "ist", "doch", "auch", "ein", "Mensch", "!!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "VAFIN", "ADV", "ADV", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn er sich gewahret,", "tokens": ["Wenn", "er", "sich", "ge\u00b7wah\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sieht er, da\u00df Natur an ihm", "tokens": ["Sieht", "er", ",", "da\u00df", "Na\u00b7tur", "an", "ihm"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "NN", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wahrlich nicht gesparet,", "tokens": ["Wahr\u00b7lich", "nicht", "ge\u00b7spa\u00b7ret", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Da\u00df er manche Lust und Pein", "tokens": ["Da\u00df", "er", "man\u00b7che", "Lust", "und", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Tr\u00e4gt als Er und eigen.", "tokens": ["Tr\u00e4gt", "als", "Er", "und", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "PPER", "KON", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Sollt er nicht auch hinterdrein", "tokens": ["Sollt", "er", "nicht", "auch", "hin\u00b7ter\u00b7drein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wohlgemut sich zeigen?", "tokens": ["Wohl\u00b7ge\u00b7mut", "sich", "zei\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}