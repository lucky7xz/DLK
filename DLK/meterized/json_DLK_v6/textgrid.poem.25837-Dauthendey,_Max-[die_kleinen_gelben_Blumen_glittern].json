{"textgrid.poem.25837": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[die kleinen gelben Blumen glittern]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die kleinen gelben Blumen glittern,", "tokens": ["Die", "klei\u00b7nen", "gel\u00b7ben", "Blu\u00b7men", "glit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Wind springt durch den Wiesengrund,", "tokens": ["Der", "Wind", "springt", "durch", "den", "Wie\u00b7sen\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und tausend Silberhalme splittern.", "tokens": ["Und", "tau\u00b7send", "Sil\u00b7ber\u00b7hal\u00b7me", "split\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die tausend Gr\u00fc\u00dfe, die ich k\u00fc\u00dfte", "tokens": ["Die", "tau\u00b7send", "Gr\u00fc\u00b7\u00dfe", ",", "die", "ich", "k\u00fc\u00df\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "CARD", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In all die kleinen gelben Kelche,", "tokens": ["In", "all", "die", "klei\u00b7nen", "gel\u00b7ben", "Kel\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Wind dr\u00fcckt sie auf deinen Mund,", "tokens": ["Der", "Wind", "dr\u00fcckt", "sie", "auf", "dei\u00b7nen", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Meilen zwischen hier und dir", "tokens": ["Die", "Mei\u00b7len", "zwi\u00b7schen", "hier", "und", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADV", "KON", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sollen von meinen K\u00fcssen zittern.", "tokens": ["Sol\u00b7len", "von", "mei\u00b7nen", "K\u00fcs\u00b7sen", "zit\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}}}}