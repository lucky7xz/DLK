{"textgrid.poem.62858": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Graf Hunerich, ein deutscher Mann,", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Graf Hunerich, ein deutscher Mann,", "tokens": ["Graf", "Hu\u00b7ne\u00b7rich", ",", "ein", "deut\u00b7scher", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hielt sich und seinem Weib,", "tokens": ["Hielt", "sich", "und", "sei\u00b7nem", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Frau Hedwig, einen Schlo\u00dfkaplan", "tokens": ["Frau", "Hed\u00b7wig", ",", "ei\u00b7nen", "Schlo\u00df\u00b7kap\u00b7lan"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NE", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum frommen Zeitvertreib.", "tokens": ["Zum", "from\u00b7men", "Zeit\u00b7ver\u00b7treib", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der M\u00f6nch verga\u00df beym leckern Tisch", "tokens": ["Der", "M\u00f6nch", "ver\u00b7ga\u00df", "beym", "le\u00b7ckern", "Tisch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Grafen sein Brevier;", "tokens": ["Des", "Gra\u00b7fen", "sein", "Bre\u00b7vier", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "A\u00df auch am Freytag selten Fisch,", "tokens": ["A\u00df", "auch", "am", "Frey\u00b7tag", "sel\u00b7ten", "Fisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Trank lieber Wein als Bier.", "tokens": ["Trank", "lie\u00b7ber", "Wein", "als", "Bier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "KOUS", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Einst weckt ihn was um Mitternacht;", "tokens": ["Einst", "weckt", "ihn", "was", "um", "Mit\u00b7ter\u00b7nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRELS", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da stand mit stillem Grimm,", "tokens": ["Da", "stand", "mit", "stil\u00b7lem", "Grimm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Geh\u00f6rnt, in schwefelgelber Tracht,", "tokens": ["Ge\u00b7h\u00f6rnt", ",", "in", "schwe\u00b7fel\u00b7gel\u00b7ber", "Tracht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcrst Lucifer vor ihm.", "tokens": ["F\u00fcrst", "Lu\u00b7ci\u00b7fer", "vor", "ihm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "APPR", "PPER", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "W\u00e4hl, sprach er, unter dreyen eins:", "tokens": ["W\u00e4hl", ",", "sprach", "er", ",", "un\u00b7ter", "drey\u00b7en", "eins", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "$,", "APPR", "CARD", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ermorde Hunerich,", "tokens": ["Er\u00b7mor\u00b7de", "Hu\u00b7ne\u00b7rich", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Entehr sein Weib, sauf dich voll Weins,", "tokens": ["E\u00b7ntehr", "sein", "Weib", ",", "sauf", "dich", "voll", "Weins", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sonst hol ich morgen dich.", "tokens": ["Sonst", "hol", "ich", "mor\u00b7gen", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Er w\u00e4hlt die Flasche, treibt berauscht", "tokens": ["Er", "w\u00e4hlt", "die", "Fla\u00b7sche", ",", "treibt", "be\u00b7rauscht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Hedwig frevle Lust,", "tokens": ["Mit", "Hed\u00b7wig", "frev\u00b7le", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und st\u00f6\u00dft dem Mann, der sie belauscht,", "tokens": ["Und", "st\u00f6\u00dft", "dem", "Mann", ",", "der", "sie", "be\u00b7lauscht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Messer in die Brust.", "tokens": ["Ein", "Mes\u00b7ser", "in", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}