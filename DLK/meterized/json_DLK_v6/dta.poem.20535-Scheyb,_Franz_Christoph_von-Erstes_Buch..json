{"dta.poem.20535": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Erstes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20536-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wje sich Theresia, die Frau von", "tokens": ["Wie", "sich", "The\u00b7re\u00b7sia", ",", "die", "Frau", "von"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PRF", "NE", "$,", "ART", "NN", "APPR"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oesterreich,", "tokens": ["O\u00b7es\u00b7ter\u00b7reich", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Dem siebenfachen Schwert, und dessen Wirbel-", "tokens": ["Dem", "sie\u00b7ben\u00b7fa\u00b7chen", "Schwert", ",", "und", "des\u00b7sen", "Wir\u00b7bel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "PDS", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Streich:", "tokens": ["Streich", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Der sich so f\u00fcrchterlich um ihre Scheitel schwunge,", "tokens": ["Der", "sich", "so", "f\u00fcrch\u00b7ter\u00b7lich", "um", "ih\u00b7re", "Schei\u00b7tel", "schwun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df er den Kronen-Schmuck fast von derselben drunge:", "tokens": ["Da\u00df", "er", "den", "Kro\u00b7nen\u00b7Schmuck", "fast", "von", "der\u00b7sel\u00b7ben", "drun\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "APPR", "PDAT", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nur von dem Tugend-Schild und Helden-Muth beschirmt;", "tokens": ["Nur", "von", "dem", "Tu\u00b7gen\u00b7dSchild", "und", "Hel\u00b7den\u00b7Muth", "be\u00b7schirmt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sonst Hilff- und Waffen-blo\u00df entgegen aufgeth\u00fcrmt:", "tokens": ["Sonst", "Hilff", "und", "Waf\u00b7fen\u00b7blo\u00df", "ent\u00b7ge\u00b7gen", "auf\u00b7ge\u00b7th\u00fcrmt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "TRUNC", "KON", "NN", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Will ich, nicht Feinden Zorn, nur Freunden Trost zu bringen;", "tokens": ["Will", "ich", ",", "nicht", "Fein\u00b7den", "Zorn", ",", "nur", "Freun\u00b7den", "Trost", "zu", "brin\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "PTKNEG", "NN", "NN", "$,", "ADV", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sonst aller Absicht frey mit tiefer Ehrfurcht singen.", "tokens": ["Sonst", "al\u00b7ler", "Ab\u00b7sicht", "frey", "mit", "tie\u00b7fer", "Ehr\u00b7furcht", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADJD", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das ist, was ich entschlo\u00df; als ihre Majest\u00e4t", "tokens": ["Das", "ist", ",", "was", "ich", "ent\u00b7schlo\u00df", ";", "als", "ih\u00b7re", "Ma\u00b7jes\u00b7t\u00e4t"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "$,", "PWS", "PPER", "VVFIN", "$.", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "10Sich aus dem Krieges-Sturm in Lorber-Pracht erh\u00f6ht;", "tokens": ["aus", "dem", "Krie\u00b7ge\u00b7sSturm", "in", "Lor\u00b7ber\u00b7Pracht", "er\u00b7h\u00f6ht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.13": {"text": "Da die Verwunderung frohlockend aufgeschrieen:", "tokens": ["Da", "die", "Ver\u00b7wun\u00b7de\u00b7rung", "froh\u00b7lo\u00b7ckend", "auf\u00b7ge\u00b7schri\u00b7een", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201emit diser K\u00f6niginn wird Heil und Wohlfahrt bl\u00fchen!", "tokens": ["\u201e", "mit", "di\u00b7ser", "K\u00f6\u00b7ni\u00b7ginn", "wird", "Heil", "und", "Wohl\u00b7fahrt", "bl\u00fc\u00b7hen", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PDAT", "NN", "VAFIN", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Bald nahm ich meine Leyr, versuchte Thon und Klang;", "tokens": ["Bald", "nahm", "ich", "mei\u00b7ne", "Leyr", ",", "ver\u00b7such\u00b7te", "Thon", "und", "Klang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Bald reitzte mich die Lust zu einem Lob-Gesang:", "tokens": ["Bald", "reitz\u00b7te", "mich", "die", "Lust", "zu", "ei\u00b7nem", "Lob\u00b7Ge\u00b7sang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "15Allein es sprach das Hertz: \u201dWie kann er dir gerathen?", "tokens": ["es", "sprach", "das", "Hertz", ":", "\"", "Wie", "kann", "er", "dir", "ge\u00b7ra\u00b7then", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "$(", "PWAV", "VMFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "\u201eerweg der Sayten Kraft! mi\u00df dieser F\u00fcrstinn Thaten!", "tokens": ["\u201e", "er\u00b7weg", "der", "Say\u00b7ten", "Kraft", "!", "mi\u00df", "die\u00b7ser", "F\u00fcrs\u00b7tinn", "Tha\u00b7ten", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "ADJA", "NN", "$.", "APPR", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "\u201emeinst du, da\u00df dieses Spiel ein klingendes Metall;", "tokens": ["\u201e", "meinst", "du", ",", "da\u00df", "die\u00b7ses", "Spiel", "ein", "klin\u00b7gen\u00b7des", "Me\u00b7tall", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "KOUS", "PDAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "\u201eein heiseres Geth\u00f6n den Welt-Posaunen-Schall", "tokens": ["\u201e", "ein", "hei\u00b7se\u00b7res", "Ge\u00b7th\u00f6n", "den", "Welt\u00b7Po\u00b7sau\u00b7nen\u00b7Schall"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "\u201ein seinem hohen Laut und Widerhall begleite?", "tokens": ["\u201e", "in", "sei\u00b7nem", "ho\u00b7hen", "Laut", "und", "Wi\u00b7der\u00b7hall", "be\u00b7glei\u00b7te", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "ADJA", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "20So legt' ich mit der Leyr fast auch den Muth bey Seite.", "tokens": ["legt'", "ich", "mit", "der", "Leyr", "fast", "auch", "den", "Muth", "bey", "Sei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "ADV", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Welch-unversehner Fall! Ich stunde wanckelbar;", "tokens": ["Welch\u00b7un\u00b7ver\u00b7seh\u00b7ner", "Fall", "!", "Ich", "stun\u00b7de", "wan\u00b7ckel\u00b7bar", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hier rung ich mit mir selbst, dort sche\u00fct\u2019 ich die Gefahr;", "tokens": ["Hier", "rung", "ich", "mit", "mir", "selbst", ",", "dort", "sche\u00b7\u00fct'", "ich", "die", "Ge\u00b7fahr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "ADV", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Die Furcht begunnte sich in meiner Brust zu sch\u00e4rfen;", "tokens": ["Die", "Furcht", "be\u00b7gunn\u00b7te", "sich", "in", "mei\u00b7ner", "Brust", "zu", "sch\u00e4r\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Freud hingegen mir den Unmuth vorzuwerfen.", "tokens": ["Die", "Freud", "hin\u00b7ge\u00b7gen", "mir", "den", "Un\u00b7muth", "vor\u00b7zu\u00b7wer\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "25Inzwischen schien es mir, als schwebt' ich in der Luft;", "tokens": ["schien", "es", "mir", ",", "als", "schwebt'", "ich", "in", "der", "Luft", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "KOUS", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Als h\u00f6rt\u2019 ich ein Ger\u00e4usch, aus welchem jemand ruft:", "tokens": ["Als", "h\u00f6rt'", "ich", "ein", "Ge\u00b7r\u00e4usch", ",", "aus", "wel\u00b7chem", "je\u00b7mand", "ruft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ART", "NN", "$,", "APPR", "PRELS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201enur fort! verzage nicht! ermuntre deine Sinnen!", "tokens": ["\u201e", "nur", "fort", "!", "ver\u00b7za\u00b7ge", "nicht", "!", "er\u00b7munt\u00b7re", "dei\u00b7ne", "Sin\u00b7nen", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PTKVZ", "$.", "VVFIN", "PTKNEG", "$.", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201estimm deine Leyr, und sing! verfolge dein Beginnen!", "tokens": ["\u201e", "stimm", "dei\u00b7ne", "Leyr", ",", "und", "sing", "!", "ver\u00b7fol\u00b7ge", "dein", "Be\u00b7gin\u00b7nen", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPOSAT", "NN", "$,", "KON", "VVFIN", "$.", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201ean nichts gebrach es dir, als an dergleichen Sprung;", "tokens": ["\u201e", "an", "nichts", "ge\u00b7brach", "es", "dir", ",", "als", "an", "derg\u00b7lei\u00b7chen", "Sprung", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PIS", "VVFIN", "PPER", "PPER", "$,", "KOUS", "APPR", "PIS", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "30\u201dNun aber ligt es noch an einem Feder-Schwung.", "tokens": ["\"", "Nun", "a\u00b7ber", "ligt", "es", "noch", "an", "ei\u00b7nem", "Fe\u00b7der\u00b7Schwung", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201eauf auf dann! zittre nicht! der Flug wird uns gelingen;", "tokens": ["\u201e", "auf", "auf", "dann", "!", "zitt\u00b7re", "nicht", "!", "der", "Flug", "wird", "uns", "ge\u00b7lin\u00b7gen", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "APPR", "ADV", "$.", "VVFIN", "PTKNEG", "$.", "ART", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201ewir werden ihn begl\u00fcckt, nicht ohne Lust, vollbringen.", "tokens": ["\u201e", "wir", "wer\u00b7den", "ihn", "be\u00b7gl\u00fcckt", ",", "nicht", "oh\u00b7ne", "Lust", ",", "voll\u00b7brin\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPER", "VVPP", "$,", "PTKNEG", "APPR", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Stimme, dieser Flug, der Luft verborgne Macht", "tokens": ["Die", "Stim\u00b7me", ",", "die\u00b7ser", "Flug", ",", "der", "Luft", "ver\u00b7borg\u00b7ne", "Macht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PDAT", "NN", "$,", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "War, was mich fast in Angst und in Verstummung bracht.", "tokens": ["War", ",", "was", "mich", "fast", "in", "Angst", "und", "in", "Ver\u00b7stum\u00b7mung", "bracht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWS", "PPER", "ADV", "APPR", "NN", "KON", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "35\u201dFort\u201d, wiederhohlte sie, Thalia wird dich leiten!", "tokens": ["\"", "Fort", "\"", ",", "wie\u00b7der\u00b7hohl\u00b7te", "sie", ",", "Tha\u00b7lia", "wird", "dich", "lei\u00b7ten", "!"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$(", "$,", "VVFIN", "PPER", "$,", "NE", "VAFIN", "PPER", "VVINF", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Der Nahme machte mich mit Furcht und Hoffnung streiten.", "tokens": ["Der", "Nah\u00b7me", "mach\u00b7te", "mich", "mit", "Furcht", "und", "Hoff\u00b7nung", "strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die beyde nebst der Lust bemeisterten mein Hertz;", "tokens": ["Die", "bey\u00b7de", "nebst", "der", "Lust", "be\u00b7meis\u00b7ter\u00b7ten", "mein", "Hertz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was, dacht\u2019 ich, ist es Ernst, Verblendung, oder Schertz?", "tokens": ["Was", ",", "dacht'", "ich", ",", "ist", "es", "Ernst", ",", "Ver\u00b7blen\u00b7dung", ",", "o\u00b7der", "Schertz", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "$,", "VVFIN", "PPER", "$,", "VAFIN", "PPER", "NE", "$,", "NN", "$,", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "O spr\u00e4nge sie mir bey! O k\u00f6nnt\u2019 ich sie schon sehen!", "tokens": ["O", "spr\u00e4n\u00b7ge", "sie", "mir", "bey", "!", "O", "k\u00f6nnt'", "ich", "sie", "schon", "se\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPER", "PTKVZ", "$.", "NE", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "40So h\u00e4tt ich die Gewalt vielleicht nicht auszustehen.", "tokens": ["h\u00e4tt", "ich", "die", "Ge\u00b7walt", "viel\u00b7leicht", "nicht", "aus\u00b7zu\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Sonne ziehet zwar den Morgen-Thau zu sich:", "tokens": ["Die", "Son\u00b7ne", "zie\u00b7het", "zwar", "den", "Mor\u00b7gen\u00b7\u00b7T\u00b7hau", "zu", "sich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "APPR", "PRF", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Mir aber war der Zug zu fremd und wunderlich.", "tokens": ["Mir", "a\u00b7ber", "war", "der", "Zug", "zu", "fremd", "und", "wun\u00b7der\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "NN", "PTKA", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ob mich der Grund verlie\u00df, ob ich ihn selbst vertriebe,", "tokens": ["Ob", "mich", "der", "Grund", "ver\u00b7lie\u00df", ",", "ob", "ich", "ihn", "selbst", "ver\u00b7trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das unterschied\u2019 ich nicht, nicht was den Zwang ver\u00fcbe.", "tokens": ["Das", "un\u00b7ter\u00b7schied'", "ich", "nicht", ",", "nicht", "was", "den", "Zwang", "ver\u00b7\u00fc\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "$,", "PTKNEG", "PWS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "45Sie rieff mir wiederum: \u201dWas n\u00fctzt dich Witz und Geist,", "tokens": ["rieff", "mir", "wie\u00b7de\u00b7rum", ":", "\"", "Was", "n\u00fctzt", "dich", "Witz", "und", "Geist", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$.", "$(", "PWS", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "\u201ewann er nicht aus dem Krei\u00df zaghafter Sinnen rei\u00dft?", "tokens": ["\u201e", "wann", "er", "nicht", "aus", "dem", "Krei\u00df", "zag\u00b7haf\u00b7ter", "Sin\u00b7nen", "rei\u00dft", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PPER", "PTKNEG", "APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Mein Thun und Lassen war nur Flattern, Zittern, Beben:", "tokens": ["Mein", "Thun", "und", "Las\u00b7sen", "war", "nur", "Flat\u00b7tern", ",", "Zit\u00b7tern", ",", "Be\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VAFIN", "ADV", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So wu\u00dft\u2019 ich diesem Ruf nicht viel Geh\u00f6r zu geben.", "tokens": ["So", "wu\u00dft'", "ich", "die\u00b7sem", "Ruf", "nicht", "viel", "Ge\u00b7h\u00f6r", "zu", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", 