{"textgrid.poem.64403": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "21. Charaktere", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Janhageln gleich an H\u00f6flichkeit,", "tokens": ["Jan\u00b7ha\u00b7geln", "gleich", "an", "H\u00f6f\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch stolzer, als die reichsten Briten,", "tokens": ["Noch", "stol\u00b7zer", ",", "als", "die", "reichs\u00b7ten", "Bri\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und eitel, wie ihr Franzen seyd:", "tokens": ["Und", "ei\u00b7tel", ",", "wie", "ihr", "Fran\u00b7zen", "seyd", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So zeigt er aller V\u00f6lker Sitten.", "tokens": ["So", "zeigt", "er", "al\u00b7ler", "V\u00f6l\u00b7ker", "Sit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das alte Rom ist ihm bekannt,", "tokens": ["Das", "al\u00b7te", "Rom", "ist", "ihm", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die heut'gen Sch\u00f6nen mu\u00df er kennen,", "tokens": ["Die", "heut'\u00b7gen", "Sch\u00f6\u00b7nen", "mu\u00df", "er", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als Doctor ist er sehr galant,", "tokens": ["Als", "Doc\u00b7tor", "ist", "er", "sehr", "ga\u00b7lant", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als Stutzer sehr gelehrt zu nennen.", "tokens": ["Als", "Stut\u00b7zer", "sehr", "ge\u00b7lehrt", "zu", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die zweyer V\u00f6lker Reiz mir wies,", "tokens": ["Die", "zwey\u00b7er", "V\u00f6l\u00b7ker", "Reiz", "mir", "wies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die mu\u00dfte wohl mein Herz gewinnen;", "tokens": ["Die", "mu\u00df\u00b7te", "wohl", "mein", "Herz", "ge\u00b7win\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Frech, wie die Damen in Paris,", "tokens": ["Frech", ",", "wie", "die", "Da\u00b7men", "in", "Pa\u00b7ris", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "APPR", "NE", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Plump, wie die deutschen Sch\u00e4ferinnen.", "tokens": ["Plump", ",", "wie", "die", "deut\u00b7schen", "Sch\u00e4\u00b7fe\u00b7rin\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}