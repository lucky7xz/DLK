{"textgrid.poem.33528": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Kientoppbildchen", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein St\u00e4dtchen liegt da wo im Land,", "tokens": ["Ein", "St\u00e4dt\u00b7chen", "liegt", "da", "wo", "im", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PWAV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie \u00fcblich: altert\u00fcmlich.", "tokens": ["Wie", "\u00fcb\u00b7lich", ":", "al\u00b7ter\u00b7t\u00fcm\u00b7lich", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADJD", "$.", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und B\u00e4ume stehn am Stra\u00dfenrand,", "tokens": ["Und", "B\u00e4u\u00b7me", "stehn", "am", "Stra\u00b7\u00dfen\u00b7rand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die wackeln manchmal ziemlich.", "tokens": ["Die", "wa\u00b7ckeln", "manch\u00b7mal", "ziem\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und Kinder laufen ungek\u00e4mmt.", "tokens": ["Und", "Kin\u00b7der", "lau\u00b7fen", "un\u00b7ge\u00b7k\u00e4mmt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie haben nackte Beine.", "tokens": ["Sie", "ha\u00b7ben", "nack\u00b7te", "Bei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zufrieden schaut ein schmutzges Hemd", "tokens": ["Zu\u00b7frie\u00b7den", "schaut", "ein", "schmutz\u00b7ges", "Hemd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von einer W\u00e4scheleine.", "tokens": ["Von", "ei\u00b7ner", "W\u00e4\u00b7sche\u00b7lei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Abend bringt den Zeitvertreib,", "tokens": ["Der", "A\u00b7bend", "bringt", "den", "Zeit\u00b7ver\u00b7treib", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Laternen, Mond, Gespenster.", "tokens": ["La\u00b7ter\u00b7nen", ",", "Mond", ",", "Ge\u00b7spens\u00b7ter", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$."], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.3": {"text": "Recht h\u00e4ufig h\u00e4ngt ein altes Weib", "tokens": ["Recht", "h\u00e4u\u00b7fig", "h\u00e4ngt", "ein", "al\u00b7tes", "Weib"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In einem kleinen Fenster.", "tokens": ["In", "ei\u00b7nem", "klei\u00b7nen", "Fens\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}