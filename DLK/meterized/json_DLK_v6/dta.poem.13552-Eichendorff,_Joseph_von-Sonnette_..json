{"dta.poem.13552": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnette .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Es w\u00e4chst sehns\u00fcchtig, st\u00fcrzt und leuchtet trunken", "tokens": ["Es", "w\u00e4chst", "sehn\u00b7s\u00fcch\u00b7tig", ",", "st\u00fcrzt", "und", "leuch\u00b7tet", "trun\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "VVFIN", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Jauchzend im Innersten die heil'ge Quelle,", "tokens": ["Jauch\u00b7zend", "im", "In\u00b7ners\u00b7ten", "die", "heil'\u00b7ge", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+---+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Bald Bahn sich brechend durch die Kluft zur Helle,", "tokens": ["Bald", "Bahn", "sich", "bre\u00b7chend", "durch", "die", "Kluft", "zur", "Hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PRF", "ADJD", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bald k\u00fchle rauschend dann in Nacht versunken.", "tokens": ["Bald", "k\u00fch\u00b7le", "rau\u00b7schend", "dann", "in", "Nacht", "ver\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}