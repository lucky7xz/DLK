{"dta.poem.7110": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn wir den C\u00f6rper nur sam\u0303t seinen Gliedern sehn,", "tokens": ["Wenn", "wir", "den", "C\u00f6r\u00b7per", "nur", "sam\u0303t", "sei\u00b7nen", "Glie\u00b7dern", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie gro\u00df ist ihre Meng\u2019, und wie so lang die Liste,", "tokens": ["Wie", "gro\u00df", "ist", "ih\u00b7re", "Meng'", ",", "und", "wie", "so", "lang", "die", "Lis\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,", "KON", "PWAV", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wen\u0303 man sie nach der Reih\u2019 und Ordnung z\u00e4hlen wollte,", "tokens": ["We\u00f1", "man", "sie", "nach", "der", "Reih'", "und", "Ord\u00b7nung", "z\u00e4h\u00b7len", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "APPR", "ART", "NN", "KON", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wovon das kleinste, wenns uns fehlen sollte,", "tokens": ["Wo\u00b7von", "das", "kleins\u00b7te", ",", "wenns", "uns", "feh\u00b7len", "soll\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Uns elend machen w\u00fcrd\u2019. Ich mu\u00df absonderlich", "tokens": ["Uns", "e\u00b7lend", "ma\u00b7chen", "w\u00fcrd'", ".", "Ich", "mu\u00df", "ab\u00b7son\u00b7der\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJD", "VVINF", "VAFIN", "$.", "PPER", "VMFIN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Denn Dir, o Vater! noch um so viel mehr Lob singen,", "tokens": ["Denn", "Dir", ",", "o", "Va\u00b7ter", "!", "noch", "um", "so", "viel", "mehr", "Lob", "sin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "NN", "$.", "ADV", "APPR", "ADV", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Da aller Meinigen gesunde C\u00f6rper ich", "tokens": ["Da", "al\u00b7ler", "Mei\u00b7ni\u00b7gen", "ge\u00b7sun\u00b7de", "C\u00f6r\u00b7per", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "PPOSAT", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mit auch gesundem Geist, (o seltnes Gl\u00fccke!)", "tokens": ["Mit", "auch", "ge\u00b7sun\u00b7dem", "Geist", ",", "(", "o", "selt\u00b7nes", "Gl\u00fc\u00b7cke", "!", ")"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,", "$(", "FM", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Durch so viel Augenblicke,", "tokens": ["Durch", "so", "viel", "Au\u00b7gen\u00b7bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.10": {"text": "Vergn\u00fcgt erhalten seh\u2019. Denn jegliche Minute", "tokens": ["Ver\u00b7gn\u00fcgt", "er\u00b7hal\u00b7ten", "seh'", ".", "Denn", "jeg\u00b7li\u00b7che", "Mi\u00b7nu\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVPP", "VAINF", "$.", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ist ja ein Theil, ein St\u00fcck von unsrer Zeit,", "tokens": ["Ist", "ja", "ein", "Theil", ",", "ein", "St\u00fcck", "von", "uns\u00b7rer", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und Du hast uns, o HERR! mit Deinem Gute,", "tokens": ["Und", "Du", "hast", "uns", ",", "o", "HeRR", "!", "mit", "Dei\u00b7nem", "Gu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "$,", "FM", "NN", "$.", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "In einem jeglichen so wunderbar erfreut,", "tokens": ["In", "ei\u00b7nem", "jeg\u00b7li\u00b7chen", "so", "wun\u00b7der\u00b7bar", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Erhalten und bewahrt. Hab ewig Danck daf\u00fcr!", "tokens": ["Er\u00b7hal\u00b7ten", "und", "be\u00b7wahrt", ".", "Hab", "e\u00b7wig", "Danck", "da\u00b7f\u00fcr", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$.", "NN", "ADJD", "NN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Sey aber auch insoderheit gepriesen,", "tokens": ["Sey", "a\u00b7ber", "auch", "in\u00b7so\u00b7der\u00b7heit", "ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Da\u00df Du, nebst tausend andern, mir", "tokens": ["Da\u00df", "Du", ",", "nebst", "tau\u00b7send", "an\u00b7dern", ",", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "$,", "APPR", "CARD", "ADJA", "$,", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Solch eine seltne Gab\u2019 erwiesen,", "tokens": ["Solch", "ei\u00b7ne", "selt\u00b7ne", "Gab'", "er\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Da\u00df ich den ", "tokens": ["Da\u00df", "ich", "den"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "gen,", "tokens": ["gen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-", "measure": "single.down"}, "line.20": {"text": "Jm abgewichnen Jahr, ", "tokens": ["Jm", "ab\u00b7ge\u00b7wich\u00b7nen", "Jahr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Und da\u00df es allbereit,", "tokens": ["Und", "da\u00df", "es", "all\u00b7be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "So wie ich h\u00f6ren sagen,", "tokens": ["So", "wie", "ich", "h\u00f6\u00b7ren", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Zum Preise Deiner Herrlichkeit,", "tokens": ["Zum", "Prei\u00b7se", "Dei\u00b7ner", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Schon hin und wieder Frucht getragen.", "tokens": ["Schon", "hin", "und", "wie\u00b7der", "Frucht", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "KON", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Es freuet sich in mir das Innerste der Seelen,", "tokens": ["Es", "freu\u00b7et", "sich", "in", "mir", "das", "In\u00b7ners\u00b7te", "der", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.26": {"text": "Da\u00df, zur Verherrlichung von Deiner Wunder-Pracht", "tokens": ["Da\u00df", ",", "zur", "Ver\u00b7herr\u00b7li\u00b7chung", "von", "Dei\u00b7ner", "Wun\u00b7der\u00b7Pracht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "APPRART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Du mich, aus Gnaden blo\u00df, zum Werckzeug hast ge-", "tokens": ["Du", "mich", ",", "aus", "Gna\u00b7den", "blo\u00df", ",", "zum", "Wer\u00b7ck\u00b7zeug", "hast", "ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PRF", "$,", "APPR", "NN", "ADV", "$,", "APPRART", "NN", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.28": {"text": "macht.", "tokens": ["macht", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.29": {"text": "Ach la\u00df mich fernerhin, o HERR, noch mehr erz\u00e4hlen,", "tokens": ["Ach", "la\u00df", "mich", "fer\u00b7ner\u00b7hin", ",", "o", "HeRR", ",", "noch", "mehr", "er\u00b7z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "VVIMP", "PPER", "PTKVZ", "$,", "FM", "NN", "$,", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Zu Deines Nahmens Ehr und Preise,", "tokens": ["Zu", "Dei\u00b7nes", "Nah\u00b7mens", "Ehr", "und", "Prei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Wie Gegenw\u00e4rtig, Gro\u00df und Weise", "tokens": ["Wie", "Ge\u00b7gen\u00b7w\u00e4r\u00b7tig", ",", "Gro\u00df", "und", "Wei\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "$,", "NE", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Dein G\u00f6ttlich Wesen sey. Was ich mir vorgenommen,", "tokens": ["Dein", "G\u00f6tt\u00b7lich", "We\u00b7sen", "sey", ".", "Was", "ich", "mir", "vor\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VAFIN", "$.", "PWS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Vom ", "tokens": ["Vom"], "token_info": ["word"], "pos": ["APPRART"], "meter": "+", "measure": "single.up"}, "line.34": {"text": "bringen,", "tokens": ["brin\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.35": {"text": "Das la\u00df, wo Dirs gef\u00e4llt, o HERR! gelingen,", "tokens": ["Das", "la\u00df", ",", "wo", "Dirs", "ge\u00b7f\u00e4llt", ",", "o", "HeRR", "!", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PWAV", "NN", "VVPP", "$,", "FM", "NN", "$.", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Und auch in diesem Jahr zu Stande kommen!", "tokens": ["Und", "auch", "in", "die\u00b7sem", "Jahr", "zu", "Stan\u00b7de", "kom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PDAT", "NN", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Dieweil im k\u00fcnfftigen des Richter-Amtes B\u00fcrde", "tokens": ["Die\u00b7weil", "im", "k\u00fcnff\u00b7ti\u00b7gen", "des", "Rich\u00b7ter\u00b7Am\u00b7tes", "B\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "ADJA", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Mich sonst vielleicht daran verhindern w\u00fcrde.", "tokens": ["Mich", "sonst", "viel\u00b7leicht", "da\u00b7ran", "ver\u00b7hin\u00b7dern", "w\u00fcr\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "PAV", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Gesegne meinen Stand, la\u00df meine Kinder-Zucht", "tokens": ["Ge\u00b7seg\u00b7ne", "mei\u00b7nen", "Stand", ",", "la\u00df", "mei\u00b7ne", "Kin\u00b7der\u00b7Zucht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$,", "VVIMP", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Zugleich gesegnet seyn.", "tokens": ["Zu\u00b7gleich", "ge\u00b7seg\u00b7net", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}