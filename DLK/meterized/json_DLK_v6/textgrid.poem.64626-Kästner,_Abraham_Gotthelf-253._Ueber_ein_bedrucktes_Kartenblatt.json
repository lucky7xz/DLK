{"textgrid.poem.64626": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "253. Ueber ein bedrucktes Kartenblatt", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft w\u00fcnscht' ich, was sie schrieb, der Welt zum Unterricht;", "tokens": ["Oft", "w\u00fcnscht'", "ich", ",", "was", "sie", "schrieb", ",", "der", "Welt", "zum", "Un\u00b7ter\u00b7richt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$,", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nein, sprach sie, drucken la\u00df ich nicht.", "tokens": ["Nein", ",", "sprach", "sie", ",", "dru\u00b7cken", "la\u00df", "ich", "nicht", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "$,", "VVFIN", "VVIMP", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und, eh' man sich's versieht,", "tokens": ["Und", ",", "eh'", "man", "sich's", "ver\u00b7sieht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PIS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Gibt sie gedruckt: da\u00df sie von uns entflieht.", "tokens": ["Gibt", "sie", "ge\u00b7druckt", ":", "da\u00df", "sie", "von", "uns", "ent\u00b7flieht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "$.", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}