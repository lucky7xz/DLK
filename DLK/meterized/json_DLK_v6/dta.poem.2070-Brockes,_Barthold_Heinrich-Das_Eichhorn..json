{"dta.poem.2070": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Eichhorn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dem, der darauf achtet, zeigt, so, da\u00df es sich fast nicht", "tokens": ["Dem", ",", "der", "da\u00b7rauf", "ach\u00b7tet", ",", "zeigt", ",", "so", ",", "da\u00df", "es", "sich", "fast", "nicht"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "$,", "PRELS", "PAV", "VVFIN", "$,", "VVFIN", "$,", "ADV", "$,", "KOUS", "PPER", "PRF", "ADV", "PTKNEG"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "regt,", "tokens": ["regt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Da es nicht durch seine Stellung uns zur Munterkeit be-", "tokens": ["Da", "es", "nicht", "durch", "sei\u00b7ne", "Stel\u00b7lung", "uns", "zur", "Mun\u00b7ter\u00b7keit", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "PPER", "APPRART", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "wegt.", "tokens": ["wegt", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Da es, nebst der Zierlichkeit, auch zugleich sehr gut", "tokens": ["Da", "es", ",", "nebst", "der", "Zier\u00b7lich\u00b7keit", ",", "auch", "zu\u00b7gleich", "sehr", "gut"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "APPR", "ART", "NN", "$,", "ADV", "ADV", "ADV", "ADJD"], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "zu essen,", "tokens": ["zu", "es\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Ist es billig, da\u00df wir es auch als ein Gesch\u00f6pf er-", "tokens": ["Ist", "es", "bil\u00b7lig", ",", "da\u00df", "wir", "es", "auch", "als", "ein", "Ge\u00b7sch\u00f6pf", "er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "PPER", "ADV", "KOUS", "ART", "NN", "TRUNC"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "messen", "tokens": ["mes\u00b7sen"], "token_info": ["word"], "pos": ["VVINF"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Eines wunderbaren Sch\u00f6pfers, der auch hier sein\u2019 Allmacht", "tokens": ["Ei\u00b7nes", "wun\u00b7der\u00b7ba\u00b7ren", "Sch\u00f6p\u00b7fers", ",", "der", "auch", "hier", "sein'", "All\u00b7macht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "zeigt,", "tokens": ["zeigt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.11": {"text": "Und von dessen Lieb und Weisheit nichts, was er geschaffen,", "tokens": ["Und", "von", "des\u00b7sen", "Lieb", "und", "Weis\u00b7heit", "nichts", ",", "was", "er", "ge\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRELAT", "NN", "KON", "NN", "PIS", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.12": {"text": "schweigt.", "tokens": ["schweigt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}}}}}