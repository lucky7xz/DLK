{"textgrid.poem.47686": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[auf des Olympos H\u00f6he senkt]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf des Olympos H\u00f6he senkt", "tokens": ["Auf", "des", "O\u00b7lym\u00b7pos", "H\u00f6\u00b7he", "senkt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein m\u00e4chtiger Aar die Schwinge,", "tokens": ["Ein", "m\u00e4ch\u00b7ti\u00b7ger", "Aar", "die", "Schwin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ein abgehauenes Menschenhaupt", "tokens": ["Ein", "ab\u00b7ge\u00b7hau\u00b7e\u00b7nes", "Men\u00b7schen\u00b7haupt"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In seiner R\u00e4uberkralle,", "tokens": ["In", "sei\u00b7ner", "R\u00e4u\u00b7ber\u00b7kral\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein trotziges im Tode noch,", "tokens": ["Ein", "trot\u00b7zi\u00b7ges", "im", "To\u00b7de", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Haupt von einem Braven.", "tokens": ["Das", "Haupt", "von", "ei\u00b7nem", "Bra\u00b7ven", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und wie er sitzt und wie er schaut", "tokens": ["Und", "wie", "er", "sitzt", "und", "wie", "er", "schaut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "KON", "PWAV", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auf seine Beute, fragt er:", "tokens": ["Auf", "sei\u00b7ne", "Beu\u00b7te", ",", "fragt", "er", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "\u00bbda du noch Eins mit deinem Rumpf,", "tokens": ["\u00bb", "da", "du", "noch", "Eins", "mit", "dei\u00b7nem", "Rumpf", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ADV", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wer du gewesen, sage!\u00ab \u2013", "tokens": ["Wer", "du", "ge\u00b7we\u00b7sen", ",", "sa\u00b7ge", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["PWS", "PPER", "VAPP", "$,", "VVFIN", "$.", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "\u00bbfri\u00df, Vogel, meine wilde Kraft,", "tokens": ["\u00bb", "fri\u00df", ",", "Vo\u00b7gel", ",", "mei\u00b7ne", "wil\u00b7de", "Kraft", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "$,", "NE", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Verzehre meine Mannheit!", "tokens": ["Ver\u00b7zeh\u00b7re", "mei\u00b7ne", "Mann\u00b7heit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Noch eins so lang, noch eins so breit", "tokens": ["Noch", "eins", "so", "lang", ",", "noch", "eins", "so", "breit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "ADV", "ADJD", "$,", "ADV", "PIS", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Wird Schwinge dir und Kralle.", "tokens": ["Wird", "Schwin\u00b7ge", "dir", "und", "Kral\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPER", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "In Luros und Xeromeros", "tokens": ["In", "Lu\u00b7ros", "und", "Xe\u00b7ro\u00b7me\u00b7ros"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NE"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Da war ich Armatole;", "tokens": ["Da", "war", "ich", "Ar\u00b7ma\u00b7to\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Zw\u00f6lf Jahre lang behorstet' ich", "tokens": ["Zw\u00f6lf", "Jah\u00b7re", "lang", "be\u00b7hors\u00b7tet'", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "ADJD", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Als Klephte den Olympos.", "tokens": ["Als", "Kleph\u00b7te", "den", "O\u00b7lym\u00b7pos", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Ich brachte sechzig Agas um", "tokens": ["Ich", "brach\u00b7te", "sech\u00b7zig", "A\u00b7gas", "um"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "CARD", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und z\u00fcndet' ihre D\u00f6rfer,", "tokens": ["Und", "z\u00fcn\u00b7det'", "ih\u00b7re", "D\u00f6r\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Unz\u00e4hlige T\u00fcrken au\u00dferdem", "tokens": ["Un\u00b7z\u00e4h\u00b7li\u00b7ge", "T\u00fcr\u00b7ken", "au\u00b7\u00dfer\u00b7dem"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "PAV"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.22": {"text": "Und Albanesen w\u00fcrgt' ich.", "tokens": ["Und", "Al\u00b7ba\u00b7ne\u00b7sen", "w\u00fcrgt'", "ich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Nun stille deinen Hunger!", "tokens": ["Nun", "stil\u00b7le", "dei\u00b7nen", "Hun\u00b7ger", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Nicht deiner unwerth ist der Raub,", "tokens": ["Nicht", "dei\u00b7ner", "un\u00b7werth", "ist", "der", "Raub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Gewaltiger Vogelklephte!\u00ab", "tokens": ["Ge\u00b7wal\u00b7ti\u00b7ger", "Vo\u00b7gel\u00b7kleph\u00b7te", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}