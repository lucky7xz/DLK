{"textgrid.poem.55497": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "77", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohin er auch die Blicke kehrt und wendet,", "tokens": ["Wo\u00b7hin", "er", "auch", "die", "Bli\u00b7cke", "kehrt", "und", "wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Je mehr erstaunt er \u00fcber Kunst und Pracht,", "tokens": ["Je", "mehr", "er\u00b7staunt", "er", "\u00fc\u00b7ber", "Kunst", "und", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit Vorsatz scheint der Reichtum hier verschwendet,", "tokens": ["Mit", "Vor\u00b7satz", "scheint", "der", "Reich\u00b7tum", "hier", "ver\u00b7schwen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es scheint, als habe sich nur alles selbst gemacht.", "tokens": ["Es", "scheint", ",", "als", "ha\u00b7be", "sich", "nur", "al\u00b7les", "selbst", "ge\u00b7macht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOKOM", "VAFIN", "PRF", "ADV", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Soll er sich wundern, da\u00df das Werk vollendet?", "tokens": ["Soll", "er", "sich", "wun\u00b7dern", ",", "da\u00df", "das", "Werk", "voll\u00b7en\u00b7det", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "$,", "KOUS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Soll er sich wundern, da\u00df es so erdacht?", "tokens": ["Soll", "er", "sich", "wun\u00b7dern", ",", "da\u00df", "es", "so", "er\u00b7dacht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "$,", "KOUS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ihn d\u00fcnkt, als fang er erst, mit himmlischem Entz\u00fccken,", "tokens": ["Ihn", "d\u00fcnkt", ",", "als", "fang", "er", "erst", ",", "mit", "himm\u00b7li\u00b7schem", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "VVFIN", "PPER", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Zu leben an in diesen Augenblicken.", "tokens": ["Zu", "le\u00b7ben", "an", "in", "die\u00b7sen", "Au\u00b7gen\u00b7bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}