{"dta.poem.9973": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "82.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Weil ich in den w\u00e4ldern blieb/ war ich todt und ohne leben/", "tokens": ["Weil", "ich", "in", "den", "w\u00e4l\u00b7dern", "blieb", "/", "war", "ich", "todt", "und", "oh\u00b7ne", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$(", "VAFIN", "PPER", "ADJD", "KON", "APPR", "VVINF", "$("], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Konte damals keinen schall und auch keinen klang nicht geben.", "tokens": ["Kon\u00b7te", "da\u00b7mals", "kei\u00b7nen", "schall", "und", "auch", "kei\u00b7nen", "klang", "nicht", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PIAT", "NN", "KON", "ADV", "PIAT", "VVFIN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+----+-+-+-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Jtzt nun ich gestorben bin/ so kan ich durch s\u00fcsses klingen", "tokens": ["Jtzt", "nun", "ich", "ge\u00b7stor\u00b7ben", "bin", "/", "so", "kan", "ich", "durch", "s\u00fcs\u00b7ses", "klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "VVPP", "VAFIN", "$(", "ADV", "VMFIN", "PPER", "APPR", "ADJA", "VVINF"], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Nicht die ohren nur allein/ sondern selbst die hertzen zwingen.", "tokens": ["Nicht", "die", "oh\u00b7ren", "nur", "al\u00b7lein", "/", "son\u