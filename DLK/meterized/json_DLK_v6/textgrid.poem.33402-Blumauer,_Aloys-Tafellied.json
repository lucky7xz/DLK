{"textgrid.poem.33402": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Tafellied", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Br\u00fcder, freuet euch zur Wette,", "tokens": ["Br\u00fc\u00b7der", ",", "freu\u00b7et", "euch", "zur", "Wet\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlingt die grosse Bruderkette", "tokens": ["Schlingt", "die", "gros\u00b7se", "Bru\u00b7der\u00b7ket\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Um die Freude, eh' sie flieht;", "tokens": ["Um", "die", "Freu\u00b7de", ",", "eh'", "sie", "flieht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eingebannt nach Zaub'rerweise,", "tokens": ["Ein\u00b7ge\u00b7bannt", "nach", "Zaub'\u00b7rer\u00b7wei\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schwebt sie dann in unserm Kreise", "tokens": ["Schwebt", "sie", "dann", "in", "un\u00b7serm", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Rund herum von Glied zu Glied.", "tokens": ["Rund", "he\u00b7rum", "von", "Glied", "zu", "Glied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "APPR", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aus der Kette, die wir kn\u00fcpfen,", "tokens": ["Aus", "der", "Ket\u00b7te", ",", "die", "wir", "kn\u00fcp\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Soll die Lose nicht entschl\u00fcpfen,", "tokens": ["Soll", "die", "Lo\u00b7se", "nicht", "ent\u00b7schl\u00fcp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis die Mitternacht anbricht;", "tokens": ["Bis", "die", "Mit\u00b7ter\u00b7nacht", "an\u00b7bricht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn um d'raus sich loszuwinden,", "tokens": ["Denn", "um", "d'raus", "sich", "los\u00b7zu\u00b7win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUI", "PAV", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "M\u00fc\u00dfte sie das Ende finden,", "tokens": ["M\u00fc\u00df\u00b7te", "sie", "das", "En\u00b7de", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und das hat die Kette nicht.", "tokens": ["Und", "das", "hat", "die", "Ket\u00b7te", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}