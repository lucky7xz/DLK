{"dta.poem.20806": {"metadata": {"author": {"name": "Candidus, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Canzone   XV .", "genre": "Epos", "period": "N.A.", "pub_year": "1854", "urn": "urn:nbn:de:kobv:b4-20090519783", "language": ["de:0.99"], "booktitle": "Candidus, Karl: Der deutsche Christus. F\u00fcnfzehn Canzonen. Leipzig, 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Eins wei\u00df ich, Andres glaub' ich nur zu wissen.", "tokens": ["Eins", "wei\u00df", "ich", ",", "And\u00b7res", "glaub'", "ich", "nur", "zu", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "PIS", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich wei\u00df das mein Erl\u00f6ser lebt und treu ist.", "tokens": ["Ich", "wei\u00df", "das", "mein", "Er\u00b7l\u00f6\u00b7ser", "lebt", "und", "treu", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "PPOSAT", "NN", "VVFIN", "KON", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Lachen wei\u00df ich dies und unter Thr\u00e4nen", "tokens": ["Im", "La\u00b7chen", "wei\u00df", "ich", "dies", "und", "un\u00b7ter", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PDS", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weil er in Wol und Weh mir immer neu ist.", "tokens": ["Weil", "er", "in", "Wol", "und", "Weh", "mir", "im\u00b7mer", "neu", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NE", "KON", "NN", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich habe redlich mich vor ihm beflissen,", "tokens": ["Ich", "ha\u00b7be", "red\u00b7lich", "mich", "vor", "ihm", "be\u00b7flis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "PRF", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Als unter seinen Augen, alles W\u00e4hnen", "tokens": ["Als", "un\u00b7ter", "sei\u00b7nen", "Au\u00b7gen", ",", "al\u00b7les", "W\u00e4h\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und mi\u00dfverstand'ne Sehnen", "tokens": ["Und", "mi\u00df\u00b7ver\u00b7stan\u00b7d'\u00b7ne", "Seh\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Zu ihm zu bringen als zum Vollgen\u00fcgen", "tokens": ["Zu", "ihm", "zu", "brin\u00b7gen", "als", "zum", "Voll\u00b7ge\u00b7n\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der Heiligkeit und Wahrheit, und ich glaube", "tokens": ["Der", "Hei\u00b7lig\u00b7keit", "und", "Wahr\u00b7heit", ",", "und", "ich", "glau\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "$,", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Nicht da\u00df mein Thun zum Raube", "tokens": ["Nicht", "da\u00df", "mein", "Thun", "zum", "Rau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "KOUS", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Dem Wahn, zur Beute ward dem Selbstbetr\u00fcgen.", "tokens": ["Dem", "Wahn", ",", "zur", "Beu\u00b7te", "ward", "dem", "Selbst\u00b7be\u00b7tr\u00fc\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Doch nur auf Eins mag leben ich und sterben:", "tokens": ["Doch", "nur", "auf", "Eins", "mag", "le\u00b7ben", "ich", "und", "ster\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VMFIN", "VVFIN", "PPER", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Mein Jesus lebt und l\u00e4\u00dft mich nicht verderben.", "tokens": ["Mein", "Je\u00b7sus", "lebt", "und", "l\u00e4\u00dft", "mich", "nicht", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "VVFIN", "KON", "VVFIN", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So lang ich lebe, mu\u00df ich denkend leben,", "tokens": ["So", "lang", "ich", "le\u00b7be", ",", "mu\u00df", "ich", "den\u00b7kend", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVFIN", "$,", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und soll ich anders denken als ich denke,", "tokens": ["Und", "soll", "ich", "an\u00b7ders", "den\u00b7ken", "als", "ich", "den\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVINF", "KOKOM", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So wirst du, mein Erl\u00f6ser, mich es lehren,", "tokens": ["So", "wirst", "du", ",", "mein", "Er\u00b7l\u00f6\u00b7ser", ",", "mich", "es", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wenn ich hier der Welt nur Irrwahn sckenke,", "tokens": ["Und", "wenn", "ich", "hier", "der", "Welt", "nur", "Irr\u00b7wahn", "scken\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So wirst du ihr daf\u00fcr die Wahrheit geben", "tokens": ["So", "wirst", "du", "ihr", "da\u00b7f\u00fcr", "die", "Wahr\u00b7heit", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "PAV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und wirst, die ich verf\u00fchrte, selbst bekehren.", "tokens": ["Und", "wirst", ",", "die", "ich", "ver\u00b7f\u00fchr\u00b7te", ",", "selbst", "be\u00b7keh\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "PRELS", "PPER", "VVFIN", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ich wollte dich ja ehren,", "tokens": ["Ich", "woll\u00b7te", "dich", "ja", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mein Heiland, und du wei\u00dft da\u00df ich dich liebe.", "tokens": ["Mein", "Hei\u00b7land", ",", "und", "du", "wei\u00dft", "da\u00df", "ich", "dich", "lie\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "PPER", "VVFIN", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Auch glaub' ich nicht da\u00df ich mich selbst beth\u00f6re,", "tokens": ["Auch", "glaub'", "ich", "nicht", "da\u00df", "ich", "mich", "selbst", "be\u00b7th\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Denn wenn ich dich verl\u00f6re,", "tokens": ["Denn", "wenn", "ich", "dich", "ver\u00b7l\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "So w\u00fc\u00dft' ich nicht was von dem Allen bliebe", "tokens": ["So", "w\u00fc\u00dft'", "ich", "nicht", "was", "von", "dem", "Al\u00b7len", "blie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "PWS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Was betend ich und weinend dir gesungen.", "tokens": ["Was", "be\u00b7tend", "ich", "und", "wei\u00b7nend", "dir", "ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVPP", "PPER", "KON", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Warst du Beherrscher doch auch meiner Zungen!", "tokens": ["Warst", "du", "Be\u00b7herr\u00b7scher", "doch", "auch", "mei\u00b7ner", "Zun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie jegliches vollbrachte Werk, so mahnet", "tokens": ["Wie", "jeg\u00b7li\u00b7ches", "voll\u00b7brach\u00b7te", "Werk", ",", "so", "mah\u00b7net"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auch dieses an den Tod mich, drum erscheinet", "tokens": ["Auch", "die\u00b7ses", "an", "den", "Tod", "mich", ",", "drum", "er\u00b7schei\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PDAT", "APPR", "ART", "NN", "PPER", "$,", "PAV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mir so unwesentlich was ich vollbrachte.", "tokens": ["Mir", "so", "un\u00b7we\u00b7sent\u00b7lich", "was", "ich", "voll\u00b7brach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und freilich wenn mir Alles was da scheinet,", "tokens": ["Und", "frei\u00b7lich", "wenn", "mir", "Al\u00b7les", "was", "da", "schei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "PPER", "PIS", "PWS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wenn diese Welt, die du mir hast gebahnet,", "tokens": ["Wenn", "die\u00b7se", "Welt", ",", "die", "du", "mir", "hast", "ge\u00b7bah\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "$,", "PRELS", "PPER", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wenn dieses Denken, das in dir ich dachte,", "tokens": ["Wenn", "die\u00b7ses", "Den\u00b7ken", ",", "das", "in", "dir", "ich", "dach\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "$,", "PRELS", "APPR", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Das sich in dir entfachte,", "tokens": ["Das", "sich", "in", "dir", "ent\u00b7fach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dereinst zu kleinstem Durchma\u00df in sich sinket.", "tokens": ["De\u00b7reinst", "zu", "kleins\u00b7tem", "Durch\u00b7ma\u00df", "in", "sich", "sin\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "So wie die Glieder m\u00e4hlig mir erkalten", "tokens": ["So", "wie", "die", "Glie\u00b7der", "m\u00e4h\u00b7lig", "mir", "er\u00b7kal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "ADJD", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und nur im Herzen walten", "tokens": ["Und", "nur", "im", "Her\u00b7zen", "wal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Noch mag was ew'gen Lebens Abglanz blinket,", "tokens": ["Noch", "mag", "was", "ew'\u00b7gen", "Le\u00b7bens", "Ab\u00b7glanz", "blin\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Dann ist mein Alles, bei dem Allverschwinden,", "tokens": ["Dann", "ist", "mein", "Al\u00b7les", ",", "bei", "dem", "All\u00b7ver\u00b7schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "PIS", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "In meines Herzens Herzen dich zu finden.", "tokens": ["In", "mei\u00b7nes", "Her\u00b7zens", "Her\u00b7zen", "dich", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}