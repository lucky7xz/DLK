{"dta.poem.14733": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Schnee .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wann die Lerchen wieder schwirren,", "tokens": ["Wann", "die", "Ler\u00b7chen", "wie\u00b7der", "schwir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trittst Du drau\u00dfen vor das Haus,", "tokens": ["Trittst", "Du", "drau\u00b7\u00dfen", "vor", "das", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch er mag nicht mit Dir irren,", "tokens": ["Doch", "er", "mag", "nicht", "mit", "Dir", "ir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PTKNEG", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zog weit in das Land hinaus;", "tokens": ["Zog", "weit", "in", "das", "Land", "hin\u00b7aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die Gedanken sich verwirren,", "tokens": ["Die", "Ge\u00b7dan\u00b7ken", "sich", "ver\u00b7wir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie Du siehst den Morgen roth, \u2014", "tokens": ["Wie", "Du", "siehst", "den", "Mor\u00b7gen", "roth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ART", "NN", "ADJD", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wann die Lerchen wieder schwirren,", "tokens": ["Wann", "die", "Ler\u00b7chen", "wie\u00b7der", "schwir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Armes Kind, ach w\u00e4rst Du todt!", "tokens": ["Ar\u00b7mes", "Kind", ",", "ach", "w\u00e4rst", "Du", "todt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "XY", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}