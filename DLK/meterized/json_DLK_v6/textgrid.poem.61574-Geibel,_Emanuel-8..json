{"textgrid.poem.61574": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf den gr\u00fcnen Auen", "tokens": ["Auf", "den", "gr\u00fc\u00b7nen", "Au\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wallt der Sonnenschein;", "tokens": ["Wallt", "der", "Son\u00b7nen\u00b7schein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Berg' und Burgen schauen", "tokens": ["Ber\u00b7g'", "und", "Bur\u00b7gen", "schau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KON", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Winkend in den Rhein.", "tokens": ["Win\u00b7kend", "in", "den", "Rhein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wei\u00df vom Bl\u00fctensegen", "tokens": ["Wei\u00df", "vom", "Bl\u00fc\u00b7ten\u00b7se\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NE", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Liegt mein Pfad bestreut,", "tokens": ["Liegt", "mein", "Pfad", "be\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Durch das Tal entgegen", "tokens": ["Durch", "das", "Tal", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PTKVZ"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Schwebt mir Festgel\u00e4ut.", "tokens": ["Schwebt", "mir", "Fest\u00b7ge\u00b7l\u00e4ut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Wie mir da im Innern", "tokens": ["Wie", "mir", "da", "im", "In\u00b7nern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Jeder Schatten weicht", "tokens": ["Je\u00b7der", "Schat\u00b7ten", "weicht"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und ein hold Erinnern", "tokens": ["Und", "ein", "hold", "E\u00b7rin\u00b7nern"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Wonnig mich beschleicht!", "tokens": ["Won\u00b7nig", "mich", "be\u00b7schleicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Lieblichste der Frauen,", "tokens": ["Lieb\u00b7lichs\u00b7te", "der", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+---+-", "measure": "dactylic.init"}, "line.2": {"text": "Still gedenk' ich dein!", "tokens": ["Still", "ge\u00b7denk'", "ich", "dein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Auf den gr\u00fcnen Auen", "tokens": ["Auf", "den", "gr\u00fc\u00b7nen", "Au\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wallt der Sonnenschein.", "tokens": ["Wallt", "der", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}