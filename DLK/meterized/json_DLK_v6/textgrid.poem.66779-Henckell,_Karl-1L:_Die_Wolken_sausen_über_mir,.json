{"textgrid.poem.66779": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Wolken sausen \u00fcber mir,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Wolken sausen \u00fcber mir,", "tokens": ["Die", "Wol\u00b7ken", "sau\u00b7sen", "\u00fc\u00b7ber", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "durch kahle Heide schreit' ich zu,", "tokens": ["durch", "kah\u00b7le", "Hei\u00b7de", "schreit'", "ich", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Sturmgew\u00f6lk die Seele saust,", "tokens": ["Wie", "Sturm\u00b7ge\u00b7w\u00f6lk", "die", "See\u00b7le", "saust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "die Seele kennt nicht Rast noch Ruh;", "tokens": ["die", "See\u00b7le", "kennt", "nicht", "Rast", "noch", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein m\u00e4chtiger Schauer hob sich auf", "tokens": ["Ein", "m\u00e4ch\u00b7ti\u00b7ger", "Schau\u00b7er", "hob", "sich", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "APPR"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "aus ihren Tiefen \u00fcber Nacht,", "tokens": ["aus", "ih\u00b7ren", "Tie\u00b7fen", "\u00fc\u00b7ber", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und schwere, bittre Herzensnot", "tokens": ["Und", "schwe\u00b7re", ",", "bitt\u00b7re", "Her\u00b7zens\u00b7not"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "ist in ihr pl\u00f6tzlich aufgewacht.", "tokens": ["ist", "in", "ihr", "pl\u00f6tz\u00b7lich", "auf\u00b7ge\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Nur weiter, weiter durch das Feld,", "tokens": ["Nur", "wei\u00b7ter", ",", "wei\u00b7ter", "durch", "das", "Feld", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "dem Sturme gib, was dich beengt,", "tokens": ["dem", "Stur\u00b7me", "gib", ",", "was", "dich", "be\u00b7engt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIMP", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und zu den Wolken wirf empor,", "tokens": ["Und", "zu", "den", "Wol\u00b7ken", "wirf", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "was furchtbar qu\u00e4lend dich bedr\u00e4ngt,", "tokens": ["was", "furcht\u00b7bar", "qu\u00e4\u00b7lend", "dich", "be\u00b7dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVPP", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und regt der Sp\u00f6tter Hauf sich rings", "tokens": ["Und", "regt", "der", "Sp\u00f6t\u00b7ter", "Hauf", "sich", "rings"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "PRF", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "und lacht dich aus in blindem Wahn,", "tokens": ["und", "lacht", "dich", "aus", "in", "blin\u00b7dem", "Wahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Verk\u00fcnde, was dein Ohr geh\u00f6rt,", "tokens": ["Ver\u00b7k\u00fcn\u00b7de", ",", "was", "dein", "Ohr", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "ruf aus, was deine Augen sahn!", "tokens": ["ruf", "aus", ",", "was", "dei\u00b7ne", "Au\u00b7gen", "sahn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Es kommt die Zeit, der Spott verstummt,", "tokens": ["Es", "kommt", "die", "Zeit", ",", "der", "Spott", "ver\u00b7stummt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "und sie verh\u00fcllen scheu ihr Haupt,", "tokens": ["und", "sie", "ver\u00b7h\u00fcl\u00b7len", "scheu", "ihr", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Und alle sinken gl\u00e4ubig hin,", "tokens": ["Und", "al\u00b7le", "sin\u00b7ken", "gl\u00e4u\u00b7big", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "die heute nimmermehr geglaubt.", "tokens": ["die", "heu\u00b7te", "nim\u00b7mer\u00b7mehr", "ge\u00b7glaubt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Verflucht die Nacht, verflucht die Nacht,", "tokens": ["Ver\u00b7flucht", "die", "Nacht", ",", "ver\u00b7flucht", "die", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "die eure Herzen ganz bedeckt,", "tokens": ["die", "eu\u00b7re", "Her\u00b7zen", "ganz", "be\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Da\u00df ihr den Strahl nicht sehen wollt,", "tokens": ["Da\u00df", "ihr", "den", "Strahl", "nicht", "se\u00b7hen", "wollt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "der euch zu sch\u00f6nerm Leben weckt!", "tokens": ["der", "euch", "zu", "sch\u00f6\u00b7nerm", "Le\u00b7ben", "weckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "In finsterm Wandel schreitet ihr,", "tokens": ["In", "fins\u00b7term", "Wan\u00b7del", "schrei\u00b7tet", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "ein bl\u00f6des Volk, tagaus, tagein,", "tokens": ["ein", "bl\u00f6\u00b7des", "Volk", ",", "ta\u00b7gaus", ",", "ta\u00b7gein", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Und eure Seele fror zu Eis,", "tokens": ["Und", "eu\u00b7re", "See\u00b7le", "fror", "zu", "Eis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "und euer Gl\u00fcck ist Angst und Pein.", "tokens": ["und", "eu\u00b7er", "Gl\u00fcck", "ist", "Angst", "und", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Wie morsche Bl\u00e4tter, die der Herbst", "tokens": ["Wie", "mor\u00b7sche", "Bl\u00e4t\u00b7ter", ",", "die", "der", "Herbst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "den Winden gibt zu losem Spiel,", "tokens": ["den", "Win\u00b7den", "gibt", "zu", "lo\u00b7sem", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Treibt ihr am Boden wirr und welk,", "tokens": ["Treibt", "ihr", "am", "Bo\u00b7den", "wirr", "und", "welk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "und euer Jagen kennt kein Ziel.", "tokens": ["und", "eu\u00b7er", "Ja\u00b7gen", "kennt", "kein", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Das aber ist der Hauch der Pest,", "tokens": ["Das", "a\u00b7ber", "ist", "der", "Hauch", "der", "Pest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "die grausig schwirrt durch eure Nacht,", "tokens": ["die", "grau\u00b7sig", "schwirrt", "durch", "eu\u00b7re", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Da\u00df eurer Herzen Glanz verblich", "tokens": ["Da\u00df", "eu\u00b7rer", "Her\u00b7zen", "Glanz", "ver\u00b7blich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "in eitel Gold und Goldespracht;", "tokens": ["in", "ei\u00b7tel", "Gold", "und", "Gol\u00b7de\u00b7spracht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Das aber ist das Leichentuch,", "tokens": ["Das", "a\u00b7ber", "ist", "das", "Lei\u00b7chen\u00b7tuch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "begrabend Lieb und Lust der Zeit,", "tokens": ["be\u00b7gra\u00b7bend", "Lieb", "und", "Lust", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Da\u00df unbarmherzig wie Basalt", "tokens": ["Da\u00df", "un\u00b7barm\u00b7her\u00b7zig", "wie", "Ba\u00b7salt"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "KOKOM", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "ihr starrt in Herzensh\u00e4rtigkeit.", "tokens": ["ihr", "starrt", "in", "Her\u00b7zens\u00b7h\u00e4r\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Wohl w\u00fchlt ihr in der Erde Herz", "tokens": ["Wohl", "w\u00fchlt", "ihr", "in", "der", "Er\u00b7de", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "mit Maulwurfsemsigkeit euch ein,", "tokens": ["mit", "Maul\u00b7wurf\u00b7sem\u00b7sig\u00b7keit", "euch", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Wohl wi\u00dft ihr der Gestirne Gang", "tokens": ["Wohl", "wi\u00dft", "ihr", "der", "Ge\u00b7stir\u00b7ne", "Gang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "und spaltet selbst den Sonnenschein;", "tokens": ["und", "spal\u00b7tet", "selbst", "den", "Son\u00b7nen\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Wohl reiht das St\u00e4ubchen ihr zum Staub", "tokens": ["Wohl", "reiht", "das", "St\u00e4ub\u00b7chen", "ihr", "zum", "Staub"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "und baut das All aus Stoff und Kraft,", "tokens": ["und", "baut", "das", "All", "aus", "Stoff", "und", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Und seid f\u00fcrwahr ein klug Geschlecht", "tokens": ["Und", "seid", "f\u00fcr\u00b7wahr", "ein", "klug", "Ge\u00b7schlecht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "an Hirnverstand und Wissenschaft.", "tokens": ["an", "Hirn\u00b7ver\u00b7stand", "und", "Wis\u00b7sen\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Und doch unselig seid ihr ganz,", "tokens": ["Und", "doch", "un\u00b7se\u00b7lig", "seid", "ihr", "ganz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.50": {"text": "und euer Wissen all ist hohl,", "tokens": ["und", "eu\u00b7er", "Wis\u00b7sen", "all", "ist", "hohl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PIAT", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Ihr habt ermordet euer Herz,", "tokens": ["Ihr", "habt", "er\u00b7mor\u00b7det", "eu\u00b7er", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "der Gottheit leuchtendes Symbol.", "tokens": ["der", "Got\u00b7theit", "leuch\u00b7ten\u00b7des", "Sym\u00b7bol", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "Verbrechen paart ihr dem Genu\u00df", "tokens": ["Ver\u00b7bre\u00b7chen", "paart", "ihr", "dem", "Ge\u00b7nu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "und reizt des Fleisches rohen Sinn:", "tokens": ["und", "reizt", "des", "Flei\u00b7sches", "ro\u00b7hen", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.55": {"text": "\u00bbnach uns die S\u00fcndflut \u2013 Halali!\u00ab \u2013", "tokens": ["\u00bb", "nach", "uns", "die", "S\u00fcnd\u00b7flut", "\u2013", "Ha\u00b7la\u00b7li", "!", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["$(", "APPR", "PPER", "ART", "NN", "$(", "NE", "$.", "$(", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.56": {"text": "und taumelt siech und l\u00fcstern hin.", 