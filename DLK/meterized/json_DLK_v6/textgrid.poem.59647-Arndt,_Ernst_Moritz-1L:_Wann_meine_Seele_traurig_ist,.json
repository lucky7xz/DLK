{"textgrid.poem.59647": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wann meine Seele traurig ist,", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann meine Seele traurig ist,", "tokens": ["Wann", "mei\u00b7ne", "See\u00b7le", "trau\u00b7rig", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Mut und Lust in mir verzagen,", "tokens": ["Und", "Mut", "und", "Lust", "in", "mir", "ver\u00b7za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wann wankend zwischen Wahn und List", "tokens": ["Wann", "wan\u00b7kend", "zwi\u00b7schen", "Wahn", "und", "List"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich Welt und S\u00fcnde hart verklagen,", "tokens": ["Sich", "Welt", "und", "S\u00fcn\u00b7de", "hart", "ver\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "KON", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wann auf der Zweifel wildem Meer", "tokens": ["Wann", "auf", "der", "Zwei\u00b7fel", "wil\u00b7dem", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Schifflein steuerlos mu\u00df treiben,", "tokens": ["Mein", "Schif\u00b7flein", "steu\u00b7er\u00b7los", "mu\u00df", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wo scheint der Stern der Rettung her?", "tokens": ["Wo", "scheint", "der", "Stern", "der", "Ret\u00b7tung", "her", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Was l\u00e4\u00dft mich dennoch oben bleiben?", "tokens": ["Was", "l\u00e4\u00dft", "mich", "den\u00b7noch", "o\u00b7ben", "blei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wann um mich alles finster wird,", "tokens": ["Wann", "um", "mich", "al\u00b7les", "fins\u00b7ter", "wird", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPER", "PIS", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als s\u00e4\u00df' ich in der dunklen H\u00f6lle,", "tokens": ["Als", "s\u00e4\u00df'", "ich", "in", "der", "dunk\u00b7len", "H\u00f6l\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wann's in mir bangt und zagt und irrt,", "tokens": ["Wann's", "in", "mir", "bangt", "und", "zagt", "und", "irrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als wenn die S\u00fcndflut um mich schw\u00f6lle,", "tokens": ["Als", "wenn", "die", "S\u00fcnd\u00b7flut", "um", "mich", "schw\u00f6l\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wann diese tiefste Seelennot", "tokens": ["Wann", "die\u00b7se", "tiefs\u00b7te", "See\u00b7len\u00b7not"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Fast will am ew'gen Heil verzagen,", "tokens": ["Fast", "will", "am", "ew'\u00b7gen", "Heil", "ver\u00b7za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wo d\u00e4mmert dann das Morgenrot,", "tokens": ["Wo", "d\u00e4m\u00b7mert", "dann", "das", "Mor\u00b7gen\u00b7rot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Sonne Zukunft anzusagen?", "tokens": ["Der", "Son\u00b7ne", "Zu\u00b7kunft", "an\u00b7zu\u00b7sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Aus dir! Aus dir! Du bist der Stern,", "tokens": ["Aus", "dir", "!", "Aus", "dir", "!", "Du", "bist", "der", "Stern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$.", "APPR", "PPER", "$.", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du bist der Hoffnung lichte Sonne,", "tokens": ["Du", "bist", "der", "Hoff\u00b7nung", "lich\u00b7te", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Knechte Knecht, der Herr der Herrn,", "tokens": ["Der", "Knech\u00b7te", "Knecht", ",", "der", "Herr", "der", "Herrn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Kranken Arzt, der Schwachen Wonne,", "tokens": ["Der", "Kran\u00b7ken", "Arzt", ",", "der", "Schwa\u00b7chen", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Armen Schatz, der Biestern Licht,", "tokens": ["Der", "Ar\u00b7men", "Schatz", ",", "der", "Bies\u00b7tern", "Licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vers\u00f6hner aller, die verloren,", "tokens": ["Ver\u00b7s\u00f6h\u00b7ner", "al\u00b7ler", ",", "die", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PIAT", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Erl\u00f6ser von des Zorns Gericht,", "tokens": ["Er\u00b7l\u00f6\u00b7ser", "von", "des", "Zorns", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der ganzen Welt zum Heil geboren.", "tokens": ["Der", "gan\u00b7zen", "Welt", "zum", "Heil", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du bist's allein, Herr Jesu Christ,", "tokens": ["Du", "bist's", "al\u00b7lein", ",", "Herr", "Je\u00b7su", "Christ", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "$,", "NN", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du bist die Hoffnung, du der Glaube,", "tokens": ["Du", "bist", "die", "Hoff\u00b7nung", ",", "du", "der", "Glau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du rettest von des B\u00f6sen List", "tokens": ["Du", "ret\u00b7test", "von", "des", "B\u00f6\u00b7sen", "List"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und von der eitlen Lust am Staube,", "tokens": ["Und", "von", "der", "eit\u00b7len", "Lust", "am", "Stau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du richtest uns das Angesicht", "tokens": ["Du", "rich\u00b7test", "uns", "das", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hin, wo die ew'gen Sterne funkeln,", "tokens": ["Hin", ",", "wo", "die", "ew'\u00b7gen", "Ster\u00b7ne", "fun\u00b7keln", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Du sprichst: ", "tokens": ["Du", "sprichst", ":"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.5": {"line.1": {"text": "Du bist's allein, du s\u00fc\u00dfer Hort,", "tokens": ["Du", "bist's", "al\u00b7lein", ",", "du", "s\u00fc\u00b7\u00dfer", "Hort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du milder Tr\u00f6ster aller Schmerzen,", "tokens": ["Du", "mil\u00b7der", "Tr\u00f6s\u00b7ter", "al\u00b7ler", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dein ist die Wonne, dein das Wort,", "tokens": ["Dein", "ist", "die", "Won\u00b7ne", ",", "dein", "das", "Wort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "NN", "$,", "PPOSAT", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein ist die Kindschaft frommer Herzen:", "tokens": ["Dein", "ist", "die", "Kind\u00b7schaft", "from\u00b7mer", "Her\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wir sollen alle Kinder sein,", "tokens": ["Wir", "sol\u00b7len", "al\u00b7le", "Kin\u00b7der", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Einfalt Kinder und im Glauben,", "tokens": ["In", "Ein\u00b7falt", "Kin\u00b7der", "und", "im", "Glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}