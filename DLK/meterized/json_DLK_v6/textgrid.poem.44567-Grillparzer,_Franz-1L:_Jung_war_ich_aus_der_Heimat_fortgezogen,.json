{"textgrid.poem.44567": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Jung war ich aus der Heimat fortgezogen,", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jung war ich aus der Heimat fortgezogen,", "tokens": ["Jung", "war", "ich", "aus", "der", "Hei\u00b7mat", "fort\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es lockte mich ein Bild, das, hell und reich,", "tokens": ["Es", "lock\u00b7te", "mich", "ein", "Bild", ",", "das", ",", "hell", "und", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,", "PDS", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf ferner Berge himmelnahen Bogen", "tokens": ["Auf", "fer\u00b7ner", "Ber\u00b7ge", "him\u00b7mel\u00b7na\u00b7hen", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Halb Sternbild gl\u00e4nzte und halb Menschen-gleich.", "tokens": ["Halb", "Stern\u00b7bild", "gl\u00e4nz\u00b7te", "und", "halb", "Men\u00b7schen\u00b7gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Entgegen schien es winkend selbst zu kommen,", "tokens": ["Ent\u00b7ge\u00b7gen", "schien", "es", "win\u00b7kend", "selbst", "zu", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erreichbar schiens dem K\u00fchnen, der mit Mut", "tokens": ["Er\u00b7reich\u00b7bar", "schiens", "dem", "K\u00fch\u00b7nen", ",", "der", "mit", "Mut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "ADV", "ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den Gipfel erst des Berges nur erklommen,", "tokens": ["Den", "Gip\u00b7fel", "erst", "des", "Ber\u00b7ges", "nur", "er\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und also zog ich fort in Gottes Hut.", "tokens": ["Und", "al\u00b7so", "zog", "ich", "fort", "in", "Got\u00b7tes", "Hut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKVZ", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch auf dem Gipfel angelangt der H\u00f6hen,", "tokens": ["Doch", "auf", "dem", "Gip\u00b7fel", "an\u00b7ge\u00b7langt", "der", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zerflo\u00df das Bild wie leichter Heiderauch,", "tokens": ["Zer\u00b7flo\u00df", "das", "Bild", "wie", "leich\u00b7ter", "Hei\u00b7de\u00b7rauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KOKOM", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In gleicher Ferne sah ichs wieder stehen,", "tokens": ["In", "glei\u00b7cher", "Fer\u00b7ne", "sah", "ichs", "wie\u00b7der", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf Bergen thronend, so wie fr\u00fcher auch.", "tokens": ["Auf", "Ber\u00b7gen", "thro\u00b7nend", ",", "so", "wie", "fr\u00fc\u00b7her", "auch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "ADV", "KOKOM", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "War T\u00e4uschung nun die erstgeglaubte N\u00e4he,", "tokens": ["War", "T\u00e4u\u00b7schung", "nun", "die", "erst\u00b7ge\u00b7glaub\u00b7te", "N\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So war doch Wahrheit Mut und Lust und Kraft,", "tokens": ["So", "war", "doch", "Wahr\u00b7heit", "Mut", "und", "Lust", "und", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NN", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auch schien ja wirklich, was ich deutlich sehe,", "tokens": ["Auch", "schien", "ja", "wirk\u00b7lich", ",", "was", "ich", "deut\u00b7lich", "se\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "$,", "PWS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und also hatt ich neu mich aufgerafft.", "tokens": ["Und", "al\u00b7so", "hatt", "ich", "neu", "mich", "auf\u00b7ge\u00b7rafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Doch wie ich eifrig klomm und wie ich strebte,", "tokens": ["Doch", "wie", "ich", "eif\u00b7rig", "klomm", "und", "wie", "ich", "streb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "VVFIN", "KON", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es blieb der Abstand immerdar sich gleich,", "tokens": ["Es", "blieb", "der", "Ab\u00b7stand", "im\u00b7mer\u00b7dar", "sich", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "PRF", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dasselbe Bild, das k\u00f6rperlos entschwebte,", "tokens": ["Das\u00b7sel\u00b7be", "Bild", ",", "das", "k\u00f6r\u00b7per\u00b7los", "ent\u00b7schweb\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In Fernen gl\u00e4nzend, in der N\u00e4he bleich.", "tokens": ["In", "Fer\u00b7nen", "gl\u00e4n\u00b7zend", ",", "in", "der", "N\u00e4\u00b7he", "bleich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJD", "$,", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Da ward ich m\u00fcd wie alle Staubgebornen,", "tokens": ["Da", "ward", "ich", "m\u00fcd", "wie", "al\u00b7le", "Staub\u00b7ge\u00b7bor\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KOKOM", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auch war der Weg von Steinen rauh und scharf,", "tokens": ["Auch", "war", "der", "Weg", "von", "Stei\u00b7nen", "rauh", "und", "scharf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "NN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bis auf das Leben ritzten spitze Dornen", "tokens": ["Bis", "auf", "das", "Le\u00b7ben", "ritz\u00b7ten", "spit\u00b7ze", "Dor\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und alles fehlte, was der Mensch bedarf.", "tokens": ["Und", "al\u00b7les", "fehl\u00b7te", ",", "was", "der", "Mensch", "be\u00b7darf", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Zugleich im Gegensatz des luftgen Bildes", "tokens": ["Zu\u00b7gleich", "im", "Ge\u00b7gen\u00b7satz", "des", "luft\u00b7gen", "Bil\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kam mir ein andres vor den wachen Sinn:", "tokens": ["Kam", "mir", "ein", "and\u00b7res", "vor", "den", "wa\u00b7chen", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "PIS", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erinnerung des heimischen Gefildes,", "tokens": ["E\u00b7rin\u00b7ne\u00b7rung", "des", "hei\u00b7mi\u00b7schen", "Ge\u00b7fil\u00b7des", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In dem ich ward, was ich doch endlich bin;", "tokens": ["In", "dem", "ich", "ward", ",", "was", "ich", "doch", "end\u00b7lich", "bin", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VAFIN", "$,", "PWS", "PPER", "ADV", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Wo mir des Vaters Grab zur\u00fcckgeblieben,", "tokens": ["Wo", "mir", "des", "Va\u00b7ters", "Grab", "zu\u00b7r\u00fcck\u00b7ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo die Genossen froh im nahen Gl\u00fcck,", "tokens": ["Wo", "die", "Ge\u00b7nos\u00b7sen", "froh", "im", "na\u00b7hen", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Atem weht von schwerverla\u00dfnen Lieben;", "tokens": ["Der", "A\u00b7tem", "weht", "von", "schwer\u00b7ver\u00b7la\u00df\u00b7nen", "Lie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und also kehrt ich wegersch\u00f6pft zur\u00fcck.", "tokens": ["Und", "al\u00b7so", "kehrt", "ich", "we\u00b7ger\u00b7sch\u00f6pft", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Nur ruhen wollt ich und dann neu beginnen.", "tokens": ["Nur", "ru\u00b7hen", "wollt", "ich", "und", "dann", "neu", "be\u00b7gin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "PPER", "KON", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch sah ich kaum den heimatlichen Herd,", "tokens": ["Doch", "sah", "ich", "kaum", "den", "hei\u00b7mat\u00b7li\u00b7chen", "Herd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da ward als Frucht ich der Vers\u00e4umung innen,", "tokens": ["Da", "ward", "als", "Frucht", "ich", "der", "Ver\u00b7s\u00e4u\u00b7mung", "in\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "KOUS", "NN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie alles dort verfallen und verkehrt.", "tokens": ["Wie", "al\u00b7les", "dort", "ver\u00b7fal\u00b7len", "und", "ver\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "VVINF", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Die Fenster blind, verquollen T\u00fcr und Schwelle,", "tokens": ["Die", "Fens\u00b7ter", "blind", ",", "ver\u00b7quol\u00b7len", "T\u00fcr", "und", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie \u00f6ffnete dem Freundestritt sich nicht,", "tokens": ["Sie", "\u00f6ff\u00b7ne\u00b7te", "dem", "Freun\u00b7de\u00b7stritt", "sich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von dem Ger\u00e4te nichts an seiner Stelle,", "tokens": ["Von", "dem", "Ge\u00b7r\u00e4\u00b7te", "nichts", "an", "sei\u00b7ner", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIS", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Dach gab statt der Fenster Luft und Licht.", "tokens": ["Das", "Dach", "gab", "statt", "der", "Fens\u00b7ter", "Luft", "und", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Im kleinen G\u00e4rtchen, l\u00e4ngst entwohnt der Pflege,", "tokens": ["Im", "klei\u00b7nen", "G\u00e4rt\u00b7chen", ",", "l\u00e4ngst", "ent\u00b7wohnt", "der", "Pfle\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wuchs Unkraut, wo Gew\u00e4chse sonst in Reihn,", "tokens": ["Wuchs", "Un\u00b7kraut", ",", "wo", "Ge\u00b7w\u00e4ch\u00b7se", "sonst", "in", "Reihn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PWAV", "NN", "ADV", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit wucherndem Gestr\u00fcpp bedeckt die Wege,", "tokens": ["Mit", "wu\u00b7chern\u00b7dem", "Ge\u00b7str\u00fcpp", "be\u00b7deckt", "die", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und nur im wilden Anflug schien Gedeihn.", "tokens": ["Und", "nur", "im", "wil\u00b7den", "An\u00b7flug", "schien", "Ge\u00b7deihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Da fiels mich an: die n\u00f6tigste der Taten", "tokens": ["Da", "fiels", "mich", "an", ":", "die", "n\u00f6\u00b7tigs\u00b7te", "der", "Ta\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$.", "ART", "ADJA", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sei doch, da\u00df erst die Heimat wohl bestellt,", "tokens": ["Sei", "doch", ",", "da\u00df", "erst", "die", "Hei\u00b7mat", "wohl", "be\u00b7stellt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "KOUS", "ADV", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und also nahm ich Haue, Karst und Spaten", "tokens": ["Und", "al\u00b7so", "nahm", "ich", "Hau\u00b7e", ",", "Karst", "und", "Spa\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "NN", "$,", "NN", "