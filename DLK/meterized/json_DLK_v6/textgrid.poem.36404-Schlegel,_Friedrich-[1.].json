{"textgrid.poem.36404": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[1.]", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geschritten in die Welt kam Schiller,", "tokens": ["Ge\u00b7schrit\u00b7ten", "in", "die", "Welt", "kam", "Schil\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und da ward's still und immer stiller.", "tokens": ["Und", "da", "ward's", "still", "und", "im\u00b7mer", "stil\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Erstaunt frug die Natur: \u00bbWas will er?\u00ab", "tokens": ["Er\u00b7staunt", "frug", "die", "Na\u00b7tur", ":", "\u00bb", "Was", "will", "er", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$.", "$(", "PWS", "VMFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und dreimal t\u00f6nte laut der h\u00f6chste Triller.", "tokens": ["Und", "drei\u00b7mal", "t\u00f6n\u00b7te", "laut", "der", "h\u00f6chs\u00b7te", "Tril\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}