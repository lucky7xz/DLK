{"dta.poem.23637": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Sie und ich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr seid die Herrn der Schl\u00f6sser und Pall\u00e4ste,", "tokens": ["Ihr", "seid", "die", "Herrn", "der", "Schl\u00f6s\u00b7ser", "und", "Pal\u00b7l\u00e4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu Haus bei Gold und Edelstein:", "tokens": ["Zu", "Haus", "bei", "Gold", "und", "E\u00b7del\u00b7stein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich bin ein Fremdling bin ein Gast der G\u00e4ste,", "tokens": ["Ich", "bin", "ein", "Fremd\u00b7ling", "bin", "ein", "Gast", "der", "G\u00e4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht einen Grashalm nenn' ich mein.", "tokens": ["Nicht", "ei\u00b7nen", "Gras\u00b7halm", "nenn'", "ich", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch mir geh\u00f6rt die hohe Himmelsveste,", "tokens": ["Doch", "mir", "ge\u00b7h\u00f6rt", "die", "ho\u00b7he", "Him\u00b7mels\u00b7ves\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Fr\u00fchling und der Sonnenschein:", "tokens": ["Der", "Fr\u00fch\u00b7ling", "und", "der", "Son\u00b7nen\u00b7schein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Behaltet eure Schl\u00f6sser und Pall\u00e4ste!", "tokens": ["Be\u00b7hal\u00b7tet", "eu\u00b7re", "Schl\u00f6s\u00b7ser", "und", "Pal\u00b7l\u00e4s\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich singe \u2014 und die Welt ist mein.", "tokens": ["Ich", "sin\u00b7ge", "und", "die", "Welt", "ist", "mein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KON", "ART", "NN", "VAFIN", "PPOSAT", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}