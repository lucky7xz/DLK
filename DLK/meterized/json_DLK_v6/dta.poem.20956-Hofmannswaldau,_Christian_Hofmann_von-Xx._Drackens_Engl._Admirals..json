{"dta.poem.20956": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Xx.  \n Drackens Engl.  Admirals.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "In ungest\u00fcmer See erwarb ich Ruhm und Gut/", "tokens": ["In", "un\u00b7ge\u00b7st\u00fc\u00b7mer", "See", "er\u00b7warb", "ich", "Ruhm", "und", "Gut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In ungest\u00fcmer See verlohr ich Geist und Blut.", "tokens": ["In", "un\u00b7ge\u00b7st\u00fc\u00b7mer", "See", "ver\u00b7lohr", "ich", "Geist", "und", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein K\u00f6nig mag ein Grab von Gold und Perlen ha-", "tokens": ["Ein", "K\u00f6\u00b7nig", "mag", "ein", "Grab", "von", "Gold", "und", "Per\u00b7len", "ha"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ben/", "tokens": ["ben", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Mich hat ", "tokens": ["Mich", "hat"], "token_info": ["word", "word"], "pos": ["PPER", "VAFIN"], "meter": "-+", "measure": "iambic.single"}}}}}