{"textgrid.poem.39784": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "An Fouqu\u00e9", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O willkommen, lieber S\u00e4nger,", "tokens": ["O", "will\u00b7kom\u00b7men", ",", "lie\u00b7ber", "S\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trittst du unverletzt herein?", "tokens": ["Trittst", "du", "un\u00b7ver\u00b7letzt", "her\u00b7ein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Darf es uns hinfort nicht l\u00e4nger", "tokens": ["Darf", "es", "uns", "hin\u00b7fort", "nicht", "l\u00e4n\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "PTKNEG", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bang um dich im Herzen sein?", "tokens": ["Bang", "um", "dich", "im", "Her\u00b7zen", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "APPRART", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ja, wir wissen's, hei\u00df in Liedern", "tokens": ["Ja", ",", "wir", "wis\u00b7sen's", ",", "hei\u00df", "in", "Lie\u00b7dern"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "$,", "ADJD", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hast du nach dem Tod begehrt;", "tokens": ["Hast", "du", "nach", "dem", "Tod", "be\u00b7gehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch der Himmel, wohl uns Br\u00fcdern!", "tokens": ["Doch", "der", "Him\u00b7mel", ",", "wohl", "uns", "Br\u00fc\u00b7dern", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADV", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat den ersten Wunsch verwehrt.", "tokens": ["Hat", "den", "ers\u00b7ten", "Wunsch", "ver\u00b7wehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Freilich, sch\u00f6n bed\u00e4ucht es Allen,", "tokens": ["Frei\u00b7lich", ",", "sch\u00f6n", "be\u00b7d\u00e4ucht", "es", "Al\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "VVFIN", "PPER", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00e4re nach der deutschen Schlacht", "tokens": ["W\u00e4\u00b7re", "nach", "der", "deut\u00b7schen", "Schlacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jeder Held, der drin gefallen,", "tokens": ["Je\u00b7der", "Held", ",", "der", "drin", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dort bei ", "tokens": ["Dort", "bei"], "token_info": ["word", "word"], "pos": ["ADV", "APPR"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.4": {"line.1": {"text": "Freilich, einen edlern Toten", "tokens": ["Frei\u00b7lich", ",", "ei\u00b7nen", "ed\u00b7lern", "To\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kann der V\u00e4ter sel'ge Hand", "tokens": ["Kann", "der", "V\u00e4\u00b7ter", "sel'\u00b7ge", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht erwinken sich zum Boten", "tokens": ["Nicht", "er\u00b7win\u00b7ken", "sich", "zum", "Bo\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "PRF", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vom befreiten Vaterland.", "tokens": ["Vom", "be\u00b7frei\u00b7ten", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Droben st\u00e4nd'st du, aus der Wunde", "tokens": ["Dro\u00b7ben", "st\u00e4n\u00b7d'st", "du", ",", "aus", "der", "Wun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "ART", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Str\u00f6mt' ein heil'ger Stral von Blut,", "tokens": ["Str\u00f6mt'", "ein", "heil'\u00b7ger", "Stral", "von", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und aus deinem Liedermunde", "tokens": ["Und", "aus", "dei\u00b7nem", "Lie\u00b7der\u00b7mun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lobgesang auf deutschen Mut.", "tokens": ["Lob\u00b7ge\u00b7sang", "auf", "deut\u00b7schen", "Mut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch sie wissen's lange droben,", "tokens": ["Doch", "sie", "wis\u00b7sen's", "lan\u00b7ge", "dro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Engel thaten's ihnen kund,", "tokens": ["En\u00b7gel", "tha\u00b7ten's", "ih\u00b7nen", "kund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein Jauchzen und ein Loben", "tokens": ["Und", "ein", "Jauch\u00b7zen", "und", "ein", "Lo\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00fcttert durch des Himmels Rund.", "tokens": ["Sch\u00fct\u00b7tert", "durch", "des", "Him\u00b7mels", "Rund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und die alten Preu\u00dfendichter,", "tokens": ["Und", "die", "al\u00b7ten", "Preu\u00b7\u00dfen\u00b7dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Voller, als sie's hier gethan,", "tokens": ["Vol\u00b7ler", ",", "als", "sie's", "hier", "ge\u00b7than", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schlagen jubelnd vor dem Richter", "tokens": ["Schla\u00b7gen", "ju\u00b7belnd", "vor", "dem", "Rich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihre deutschen Harfen an.", "tokens": ["Ih\u00b7re", "deut\u00b7schen", "Har\u00b7fen", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Bleibe du noch der Lebend'gen,", "tokens": ["Blei\u00b7be", "du", "noch", "der", "Le\u00b7ben\u00b7d'\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Edler Freiherr, Trost und Licht!", "tokens": ["Ed\u00b7ler", "Frei\u00b7herr", ",", "Trost", "und", "Licht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hilf den Argen ferner b\u00e4nd'gen,", "tokens": ["Hilf", "den", "Ar\u00b7gen", "fer\u00b7ner", "b\u00e4n\u00b7d'\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wie durchs Schwert einst, durchs Gedicht!", "tokens": ["Wie", "durchs", "Schwert", "einst", ",", "durchs", "Ge\u00b7dicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ADV", "$,", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Warte, bis der goldne Friede", "tokens": ["War\u00b7te", ",", "bis", "der", "gold\u00b7ne", "Frie\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unser deutsches Land begr\u00fc\u00dft,", "tokens": ["Un\u00b7ser", "deut\u00b7sches", "Land", "be\u00b7gr\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Feire den erst noch im Liede,", "tokens": ["Fei\u00b7re", "den", "erst", "noch", "im", "Lie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis dich sp\u00e4ter Schlummer k\u00fc\u00dft.", "tokens": ["Bis", "dich", "sp\u00e4\u00b7ter", "Schlum\u00b7mer", "k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Fr\u00fch genug, den du gesungen,", "tokens": ["Fr\u00fch", "ge\u00b7nug", ",", "den", "du", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weckt der gro\u00dfe Karl dich dort,", "tokens": ["Weckt", "der", "gro\u00b7\u00dfe", "Karl", "dich", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NE", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Folko h\u00e4lt dich treu umschlungen,", "tokens": ["Fol\u00b7ko", "h\u00e4lt", "dich", "treu", "um\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sigurd beut den reichen Hort.", "tokens": ["Si\u00b7gurd", "beut", "den", "rei\u00b7chen", "Hort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Alt und junge deutsche Br\u00fcder", "tokens": ["Alt", "und", "jun\u00b7ge", "deut\u00b7sche", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Freun sich dort bei deinem Sang;", "tokens": ["Freun", "sich", "dort", "bei", "dei\u00b7nem", "Sang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle sehn wir da dich wieder;", "tokens": ["Al\u00b7le", "sehn", "wir", "da", "dich", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "KOUS", "PPER", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bleibe nur \u2013 es ist nicht lang!", "tokens": ["Blei\u00b7be", "nur", "\u2013", "es", "ist", "nicht", "lang", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$(", "PPER", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}