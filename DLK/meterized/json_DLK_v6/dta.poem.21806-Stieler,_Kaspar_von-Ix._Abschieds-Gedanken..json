{"dta.poem.21806": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "Ix.  \n  Abschieds-Gedanken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Oherber Todes-stich! O Dornen-wort!", "tokens": ["O\u00b7her\u00b7ber", "To\u00b7des\u00b7stich", "!", "O", "Dor\u00b7nen\u00b7wort", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "NE", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Rosille/ lebe wol/ ich reise fort.", "tokens": ["Ro\u00b7sil\u00b7le", "/", "le\u00b7be", "wol", "/", "ich", "rei\u00b7se", "fort", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VVFIN", "ADV", "$(", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O Elend sonder End! O Zentner-Pein!", "tokens": ["O", "E\u00b7lend", "son\u00b7der", "End", "!", "O", "Zent\u00b7ner\u00b7Pein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$.", "NE", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "wird meine Seel\u2019 alsdenn auch bey mir", "tokens": ["wird", "mei\u00b7ne", "Seel'", "als\u00b7denn", "auch", "bey", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "ADV", "APPR", "PPER"], "meter": "+--+--++-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Lebt doch mein Leben so wie todt/ in mir", "tokens": ["Lebt", "doch", "mein", "Le\u00b7ben", "so", "wie", "todt", "/", "in", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "ADV", "KOKOM", "ADJD", "$(", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "da ich/ mein Rosen-kind/ bin neben dir.", "tokens": ["da", "ich", "/", "mein", "Ro\u00b7sen\u00b7kind", "/", "bin", "ne\u00b7ben", "dir", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "PPOSAT", "NN", "$(", "VAFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "wo meinen matten Geist dein Geist nicht", "tokens": ["wo", "mei\u00b7nen", "mat\u00b7ten", "Geist", "dein", "Geist", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "PPOSAT", "NN", "PTKNEG"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "bin ich ein kalter Leib und unbewegt.", "tokens": ["bin", "ich", "ein", "kal\u00b7ter", "Leib", "und", "un\u00b7be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "KON", "ADJD", "$."], "meter": "---+-+-+-+", "measure": "zehnsilber"}}, "stanza.3": {"line.1": {"text": "Zerrei\u00df/ verwirrtes Herz/ und weiche hin", "tokens": ["Zer\u00b7rei\u00df", "/", "ver\u00b7wirr\u00b7tes", "Herz", "/", "und", "wei\u00b7che", "hin"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "ADJA", "NN", "$(", "KON", "ADJA", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "indehm ich noch bey ihr/ der Sch\u00f6nen/ bin.", "tokens": ["in\u00b7dehm", "ich", "noch", "bey", "ihr", "/", "der", "Sch\u00f6\u00b7nen", "/", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPOSAT", "$(", "ART", "NN", "$(", "VAFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "der Trauer-seuffzer/ den sie dr\u00fcber l\u00e4st", "tokens": ["der", "Trau\u00b7e\u00b7rseuff\u00b7zer", "/", "den", "sie", "dr\u00fc\u00b7ber", "l\u00e4st"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ART", "PPER", "PAV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "ist der des Charons Schiff bald \u00fcberbl\u00e4st.", "tokens": ["ist", "der", "des", "Cha\u00b7rons", "Schiff", "bald", "\u00fc\u00b7ber\u00b7bl\u00e4st", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ART", "NN", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Gew\u00fcnschte Sterbligkeit! bes\u00fc\u00dfte Ruh!", "tokens": ["Ge\u00b7w\u00fcnschte", "Ster\u00b7blig\u00b7keit", "!", "be\u00b7s\u00fc\u00df\u00b7te", "Ruh", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADJA", "NN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "dr\u00fckkt Sie/ Rosille mir die Augen zu.", "tokens": ["dr\u00fckkt", "Sie", "/", "Ro\u00b7sil\u00b7le", "mir", "die", "Au\u00b7gen", "zu", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "NN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-++-+-+-+", "measure": "zehnsilber"}, "line.3": {"text": "der Liebe lezter Dienst/ ein kalter Ku\u00df", "tokens": ["der", "Lie\u00b7be", "lez\u00b7ter", "Dienst", "/", "ein", "kal\u00b7ter", "Ku\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "wird machen/ da\u00df ich todt auch leben mu\u00df.", "tokens": ["wird", "ma\u00b7chen", "/", "da\u00df", "ich", "todt", "auch", "le\u00b7ben", "mu\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVINF", "$(", "KOUS", "PPER", "ADJD", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Was w\u00fcntschest aber/ du Armer/ so?", "tokens": ["Was", "w\u00fcnt\u00b7schest", "a\u00b7ber", "/", "du", "Ar\u00b7mer", "/", "so", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "$(", "PPER", "NN", "$(", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "wird Rosilis dardurch auch werden froh?", "tokens": ["wird", "Ro\u00b7si\u00b7lis", "dar\u00b7durch", "auch", "wer\u00b7den", "froh", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "PAV", "ADV", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "wird ihrer Augen Brunn denn stille stehn/", "tokens": ["wird", "ih\u00b7rer", "Au\u00b7gen", "Brunn", "denn", "stil\u00b7le", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NE", "KON", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und ihr dein Sterben nicht zu Herzen gehn.", "tokens": ["und", "ihr", "dein", "Ster\u00b7ben", "nicht", "zu", "Her\u00b7zen", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "NN", "PTKNEG", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "So lebe nu vielmehr/ denn bleibt noch Trost", "tokens": ["So", "le\u00b7be", "nu", "viel\u00b7mehr", "/", "denn", "bleibt", "noch", "Trost"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "$(", "KON", "VVFIN", "ADV", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "da\u00df einsten Wiederkehr das bring\u2019 herein/", "tokens": ["da\u00df", "eins\u00b7ten", "Wie\u00b7der\u00b7kehr", "das", "bring'", "her\u00b7ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PDS", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "mas dich vor Schmerzen nicht l\u00e4st deine", "tokens": ["mas", "dich", "vor", "Schmer\u00b7zen", "nicht", "l\u00e4st", "dei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "NN", "PTKNEG", "VVFIN", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}