{"textgrid.poem.26857": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf einer gro\u00dfen Weide ...", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf einer gro\u00dfen Weide gehen", "tokens": ["Auf", "ei\u00b7ner", "gro\u00b7\u00dfen", "Wei\u00b7de", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Viel tausend Schafe silberwei\u00df,", "tokens": ["Viel", "tau\u00b7send", "Scha\u00b7fe", "sil\u00b7ber\u00b7wei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie wir sie heute wandeln sehen,", "tokens": ["Wie", "wir", "sie", "heu\u00b7te", "wan\u00b7deln", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sah sie der aller\u00e4ltste Greis.", "tokens": ["Sah", "sie", "der", "al\u00b7le\u00b7r\u00e4lts\u00b7te", "Greis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie altern nie und trinken Leben", "tokens": ["Sie", "al\u00b7tern", "nie", "und", "trin\u00b7ken", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "KON", "VVFIN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus einem unersch\u00f6pften Born,", "tokens": ["Aus", "ei\u00b7nem", "un\u00b7er\u00b7sch\u00f6pf\u00b7ten", "Born", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Hirt ist ihnen zugegeben", "tokens": ["Ein", "Hirt", "ist", "ih\u00b7nen", "zu\u00b7ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit sch\u00f6n gebognem Silberhorn.", "tokens": ["Mit", "sch\u00f6n", "ge\u00b7bog\u00b7nem", "Sil\u00b7ber\u00b7horn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er treibt sie aus zu goldnen Toren,", "tokens": ["Er", "treibt", "sie", "aus", "zu", "gold\u00b7nen", "To\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er \u00fcberz\u00e4hlt sie jede Nacht,", "tokens": ["Er", "\u00fc\u00b7berz\u00b7\u00e4hlt", "sie", "je\u00b7de", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und hat der L\u00e4mmer keins verloren,", "tokens": ["Und", "hat", "der", "L\u00e4m\u00b7mer", "keins", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PIAT", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sooft er auch den Weg vollbracht.", "tokens": ["Sooft", "er", "auch", "den", "Weg", "voll\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein treuer ", "tokens": ["Ein", "treu\u00b7er"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Ein muntrer ", "tokens": ["Ein", "mun\u00b7trer"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Und auch den Hirten zeig mir an.", "tokens": ["Und", "auch", "den", "Hir\u00b7ten", "zeig", "mir", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}