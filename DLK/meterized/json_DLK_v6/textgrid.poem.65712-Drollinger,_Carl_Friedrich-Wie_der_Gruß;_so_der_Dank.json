{"textgrid.poem.65712": {"metadata": {"author": {"name": "Drollinger, Carl Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Wie der Gru\u00df; so der Dank", "genre": "verse", "period": "N.A.", "pub_year": 1715, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Ordensmann sprach: Friede sey mit dir!", "tokens": ["Ein", "Or\u00b7dens\u00b7mann", "sprach", ":", "Frie\u00b7de", "sey", "mit", "dir", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "NN", "VAFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als ein Husar bey ihm vor\u00fcber rennte.", "tokens": ["Als", "ein", "Hu\u00b7sar", "bey", "ihm", "vor\u00b7\u00fc\u00b7ber", "renn\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPER", "ADV", "VVFIN", "$."], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Was? sagte der, so w\u00f6llt ich, da\u00df hinf\u00fcr", "tokens": ["Was", "?", "sag\u00b7te", "der", ",", "so", "w\u00f6llt", "ich", ",", "da\u00df", "hin\u00b7f\u00fcr"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "$.", "VVFIN", "ART", "$,", "ADV", "VVFIN", "PPER", "$,", "KOUS", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kein Funke mehr an deinem Fegfe\u00fcr brennte!", "tokens": ["Kein", "Fun\u00b7ke", "mehr", "an", "dei\u00b7nem", "Feg\u00b7fe\u00fcr", "brenn\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "O frecher Mensch! Mein Fre\u00fcnd, verzeih dirs Gott!", "tokens": ["O", "fre\u00b7cher", "Mensch", "!", "Mein", "Fre\u00fcnd", ",", "ver\u00b7zeih", "dirs", "Gott", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "PPOSAT", "NN", "$,", "VVIMP", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ist dies ein Wunsch f\u00fcr fromme Christenseelen?", "tokens": ["Ist", "dies", "ein", "Wunsch", "f\u00fcr", "from\u00b7me", "Chris\u00b7ten\u00b7see\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "O tummer Pfaff! was hast du viel zu schm\u00e4hlen;", "tokens": ["O", "tum\u00b7mer", "Pfaff", "!", "was", "hast", "du", "viel", "zu", "schm\u00e4h\u00b7len", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$.", "PWS", "VAFIN", "PPER", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "War deiner nicht von eben diesem Schrot?", "tokens": ["War", "dei\u00b7ner", "nicht", "von", "e\u00b7ben", "die\u00b7sem", "Schrot", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "PTKNEG", "APPR", "ADV", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Denn, merk es doch, wenn Krieg und Fegfe\u00fcr fehlen,", "tokens": ["Denn", ",", "merk", "es", "doch", ",", "wenn", "Krieg", "und", "Feg\u00b7fe\u00fcr", "feh\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "ADV", "$,", "KOUS", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wer Henkers gibt uns Beiden denn das Brot?", "tokens": ["Wer", "Hen\u00b7kers", "gibt", "uns", "Bei\u00b7den", "denn", "das", "Brot", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "VVFIN", "PPER", "PIS", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}