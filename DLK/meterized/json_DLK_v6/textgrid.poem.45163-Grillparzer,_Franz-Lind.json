{"textgrid.poem.45163": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Lind", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wollt ihr mit andern K\u00fcnstlern sie vergleichen", "tokens": ["Wollt", "ihr", "mit", "an\u00b7dern", "K\u00fcnst\u00b7lern", "sie", "ver\u00b7glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und tadeln ihr Benehmen f\u00fcr die Welt?", "tokens": ["Und", "ta\u00b7deln", "ihr", "Be\u00b7neh\u00b7men", "f\u00fcr", "die", "Welt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es geht ihr eben wie andern Reichen,", "tokens": ["Es", "geht", "ihr", "e\u00b7ben", "wie", "an\u00b7dern", "Rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie hat nicht immer kleines Geld.", "tokens": ["Sie", "hat", "nicht", "im\u00b7mer", "klei\u00b7nes", "Geld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}