{"textgrid.poem.59028": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "247. Aufger\u00e4umt", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tu, was du tust, nicht weil du's mu\u00dft,", "tokens": ["Tu", ",", "was", "du", "tust", ",", "nicht", "weil", "du's", "mu\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "PPER", "VVFIN", "$,", "PTKNEG", "KOUS", "PIS", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nein, was du mu\u00dft, tu, weil dir's Lust!", "tokens": ["Nein", ",", "was", "du", "mu\u00dft", ",", "tu", ",", "weil", "dir's", "Lust", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWS", "PPER", "VMFIN", "$,", "NE", "$,", "KOUS", "PIS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gott sei's getan das mind'st' und meiste", "tokens": ["Gott", "sei's", "ge\u00b7tan", "das", "min\u00b7d'\u00b7st'", "und", "meis\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "VVPP", "ART", "ADJA", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit aufger\u00e4umt' und stillem Geiste!", "tokens": ["Mit", "auf\u00b7ge\u00b7r\u00e4umt'", "und", "stil\u00b7lem", "Geis\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}