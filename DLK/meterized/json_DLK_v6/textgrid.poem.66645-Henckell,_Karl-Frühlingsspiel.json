{"textgrid.poem.66645": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingsspiel", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Sprengstoff flog. Doch ich flog nicht dabei.", "tokens": ["Der", "Spreng\u00b7stoff", "flog", ".", "Doch", "ich", "flog", "nicht", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "KON", "PPER", "VVFIN", "PTKNEG", "PAV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Froh f\u00fchl' ich nur: von \u00bbJugend\u00ab ward ich frei.", "tokens": ["Froh", "f\u00fchl'", "ich", "nur", ":", "von", "\u00bb", "Ju\u00b7gend", "\u00ab", "ward", "ich", "frei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "$.", "APPR", "$(", "NN", "$(", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erst scheint es L\u00e4hmung einen Augenblick,", "tokens": ["Erst", "scheint", "es", "L\u00e4h\u00b7mung", "ei\u00b7nen", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Man wirft den Arm, man sch\u00fcttelt das Genick,", "tokens": ["Man", "wirft", "den", "Arm", ",", "man", "sch\u00fct\u00b7telt", "das", "Ge\u00b7nick", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "$,", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Man springt behutsam \u00fcber einen Graben", "tokens": ["Man", "springt", "be\u00b7hut\u00b7sam", "\u00fc\u00b7ber", "ei\u00b7nen", "Gra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und merkt, man ist noch Herr von seinen Gaben,", "tokens": ["Und", "merkt", ",", "man", "ist", "noch", "Herr", "von", "sei\u00b7nen", "Ga\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PIS", "VAFIN", "ADV", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Setzt sich vergn\u00fcgt auf einen Stein und denkt:", "tokens": ["Setzt", "sich", "ver\u00b7gn\u00fcgt", "auf", "ei\u00b7nen", "Stein", "und", "denkt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "VVPP", "APPR", "ART", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der klare Wein wird langsam eingeschenkt.", "tokens": ["Der", "kla\u00b7re", "Wein", "wird", "lang\u00b7sam", "ein\u00b7ge\u00b7schenkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O diese Lust, sein Leibliches zu strecken,", "tokens": ["O", "die\u00b7se", "Lust", ",", "sein", "Leib\u00b7li\u00b7ches", "zu", "stre\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von ungef\u00e4hr sich selber zu entdecken,", "tokens": ["Von", "un\u00b7ge\u00b7f\u00e4hr", "sich", "sel\u00b7ber", "zu", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "PRF", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den echten Kerl mit seinem Rattenkaiser", "tokens": ["Den", "ech\u00b7ten", "Kerl", "mit", "sei\u00b7nem", "Rat\u00b7ten\u00b7kai\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Riesenw\u00fcnschen, die nun leiser, leiser", "tokens": ["Von", "Rie\u00b7sen\u00b7w\u00fcn\u00b7schen", ",", "die", "nun", "lei\u00b7ser", ",", "lei\u00b7ser"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "ADV", "ADJD", "$,", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und immer leiser pfeifen, flau zu h\u00f6ren,", "tokens": ["Und", "im\u00b7mer", "lei\u00b7ser", "pfei\u00b7fen", ",", "flau", "zu", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "$,", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Doch nimmer mein gewitzigt Ohr bet\u00f6ren.", "tokens": ["Doch", "nim\u00b7mer", "mein", "ge\u00b7wit\u00b7zigt", "Ohr", "be\u00b7t\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "VVPP", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ihr schlauer Pfiff aus jeder Tonart zog", "tokens": ["Ihr", "schlau\u00b7er", "Pfiff", "aus", "je\u00b7der", "To\u00b7nart", "zog"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "In Spalt und Schlund mich, und er log, er log!", "tokens": ["In", "Spalt", "und", "Schlund", "mich", ",", "und", "er", "log", ",", "er", "log", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PPER", "$,", "KON", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich lebe noch. 's ist nicht so schlicht begriffen.", "tokens": ["Ich", "le\u00b7be", "noch", ".", "'s", "ist", "nicht", "so", "schlicht", "be\u00b7grif\u00b7fen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "PPER", "VAFIN", "PTKNEG", "ADV", "ADJD", "VVPP", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Rattenpfiff hat mir nun ausgepfiffen.", "tokens": ["Der", "Rat\u00b7ten\u00b7pfiff", "hat", "mir", "nun", "aus\u00b7ge\u00b7pfif\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ob sie noch so majest\u00e4tisch prahlen,", "tokens": ["Und", "ob", "sie", "noch", "so", "ma\u00b7jes\u00b7t\u00e4\u00b7tisch", "prah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bin doch der Herr von meinen Idealen.", "tokens": ["Bin", "doch", "der", "Herr", "von", "mei\u00b7nen", "I\u00b7dea\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und ob sie strahlen noch so souver\u00e4n,", "tokens": ["Und", "ob", "sie", "strah\u00b7len", "noch", "so", "sou\u00b7ve\u00b7r\u00e4n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Bin doch der Herr der Banner, die sich bl\u00e4hn,", "tokens": ["Bin", "doch", "der", "Herr", "der", "Ban\u00b7ner", ",", "die", "sich", "bl\u00e4hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ART", "NN", "$,", "PRELS", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Als w\u00e4ren sie die Weiser meiner Ziele,", "tokens": ["Als", "w\u00e4\u00b7ren", "sie", "die", "Wei\u00b7ser", "mei\u00b7ner", "Zie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und sind doch meiner Finger Fr\u00fchlingsspiele.", "tokens": ["Und", "sind", "doch", "mei\u00b7ner", "Fin\u00b7ger", "Fr\u00fch\u00b7lings\u00b7spie\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ja, Fr\u00fchlingsspiel, mit sichrer Hand ge\u00fcbt,", "tokens": ["Ja", ",", "Fr\u00fch\u00b7lings\u00b7spiel", ",", "mit", "sich\u00b7rer", "Hand", "ge\u00b7\u00fcbt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein Leben sei, wie sich der Tag auch tr\u00fcbt!", "tokens": ["Mein", "Le\u00b7ben", "sei", ",", "wie", "sich", "der", "Tag", "auch", "tr\u00fcbt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$,", "PWAV", "PRF", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nur einem treu im heiter-ernsten Spiele,", "tokens": ["Nur", "ei\u00b7nem", "treu", "im", "hei\u00b7ter\u00b7erns\u00b7ten", "Spie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht Volk, nicht Menschheit, keinem \u00bbheiligen Ziele\u00ab,", "tokens": ["Nicht", "Volk", ",", "nicht", "Menschheit", ",", "kei\u00b7nem", "\u00bb", "hei\u00b7li\u00b7gen", "Zie\u00b7le", "\u00ab", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "NN", "$,", "PTKNEG", "NN", "$,", "PIAT", "$(", "ADJA", "NN", "$(", "$,"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Nur mir treu, mir, in dessen Hirn sich zeugt", "tokens": ["Nur", "mir", "treu", ",", "mir", ",", "in", "des\u00b7sen", "Hirn", "sich", "zeugt"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "ADJD", "$,", "PPER", "$,", "APPR", "PRELAT", "NN", "PRF", "VVFIN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Das Ziel selbst, das so gern sich Knechte beugt ...", "tokens": ["Das", "Ziel", "selbst", ",", "das", "so", "gern", "sich", "Knech\u00b7te", "beugt", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "ADV", "ADV", "PRF", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Frei wie ein Lichtf\u00fcrst \u00fcber Meer und Land", "tokens": ["Frei", "wie", "ein", "Licht\u00b7f\u00fcrst", "\u00fc\u00b7ber", "Meer", "und", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Mit Sternengold und Hermelingewand.", "tokens": ["Mit", "Ster\u00b7nen\u00b7gold", "und", "Her\u00b7me\u00b7lin\u00b7ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}