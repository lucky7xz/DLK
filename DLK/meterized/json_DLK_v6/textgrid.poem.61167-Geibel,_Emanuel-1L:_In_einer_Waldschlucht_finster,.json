{"textgrid.poem.61167": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: In einer Waldschlucht finster,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In einer Waldschlucht finster,", "tokens": ["In", "ei\u00b7ner", "Wald\u00b7schlucht", "fins\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo heimlich baut der Fuchs,", "tokens": ["Wo", "heim\u00b7lich", "baut", "der", "Fuchs", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo Farrenkraut und Ginster", "tokens": ["Wo", "Far\u00b7ren\u00b7kraut", "und", "Gins\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich rangt in \u00fcpp'gem Wuchs,", "tokens": ["Sich", "rangt", "in", "\u00fcpp'\u00b7gem", "Wuchs", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Lag ich, vom Gr\u00fcn umwoben,", "tokens": ["Lag", "ich", ",", "vom", "Gr\u00fcn", "um\u00b7wo\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "An einem dunklen Bach;", "tokens": ["An", "ei\u00b7nem", "dunk\u00b7len", "Bach", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es lugte kaum von oben", "tokens": ["Es", "lug\u00b7te", "kaum", "von", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Sonn' ins Laubgemach.", "tokens": ["Die", "Sonn'", "ins", "Laub\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich hatte Moos zum Pf\u00fchle,", "tokens": ["Ich", "hat\u00b7te", "Moos", "zum", "Pf\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gestr\u00fcpp zur Lagerstatt,", "tokens": ["Ge\u00b7str\u00fcpp", "zur", "La\u00b7ger\u00b7statt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom Fels kam eine K\u00fchle", "tokens": ["Vom", "Fels", "kam", "ei\u00b7ne", "K\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und ging durch Busch und Blatt;", "tokens": ["Und", "ging", "durch", "Busch", "und", "Blatt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und k\u00fchle quoll der Sprudel", "tokens": ["Und", "k\u00fch\u00b7le", "quoll", "der", "Spru\u00b7del"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und murrt' am schroffen Hang,", "tokens": ["Und", "murrt'", "am", "schrof\u00b7fen", "Hang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den oft bei Nacht im Rudel", "tokens": ["Den", "oft", "bei", "Nacht", "im", "Ru\u00b7del"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Hindin \u00fcbersprang.", "tokens": ["Die", "Hin\u00b7din", "\u00fc\u00b7bers\u00b7prang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mit rotem Auge schaute", "tokens": ["Mit", "ro\u00b7tem", "Au\u00b7ge", "schau\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vom Baum der Auerhahn,", "tokens": ["Vom", "Baum", "der", "Au\u00b7er\u00b7hahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es zog mit heisrem Laute", "tokens": ["Es", "zog", "mit", "heis\u00b7rem", "Lau\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der H\u00e4her seine Bahn;", "tokens": ["Der", "H\u00e4\u00b7her", "sei\u00b7ne", "Bahn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dann h\u00e4mmert' abgebrochen", "tokens": ["Dann", "h\u00e4m\u00b7mert'", "ab\u00b7ge\u00b7bro\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Specht von Zeit zu Zeit -", "tokens": ["Der", "Specht", "von", "Zeit", "zu", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "APPR", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mir war's, als h\u00f6rt' ich pochen", "tokens": ["Mir", "wa\u00b7r's", ",", "als", "h\u00f6rt'", "ich", "po\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Das Herz der Einsamkeit.", "tokens": ["Das", "Herz", "der", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da pl\u00f6tzlich sah ich lehnen", "tokens": ["Da", "pl\u00f6tz\u00b7lich", "sah", "ich", "leh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am Stamm ein hohes Weib,", "tokens": ["Am", "Stamm", "ein", "ho\u00b7hes", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Umwallt von lockigen Str\u00e4hnen", "tokens": ["Um\u00b7wallt", "von", "lo\u00b7cki\u00b7gen", "Str\u00e4h\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den wundersch\u00f6nen Leib;", "tokens": ["Den", "wun\u00b7der\u00b7sch\u00f6\u00b7nen", "Leib", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wem ward zum Eigentume", "tokens": ["Wem", "ward", "zum", "Ei\u00b7gen\u00b7tu\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Je solch ein Goldgewand!", "tokens": ["Je", "solch", "ein", "Gold\u00b7ge\u00b7wand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sie trug eine blaue Blume", "tokens": ["Sie", "trug", "ei\u00b7ne", "blau\u00b7e", "Blu\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "In ihrer wei\u00dfen Hand.", "tokens": ["In", "ih\u00b7rer", "wei\u00b7\u00dfen", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sie sprach: \u00bbSei mir willkommen!", "tokens": ["Sie", "sprach", ":", "\u00bb", "Sei", "mir", "will\u00b7kom\u00b7men", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du bist ein seltner Gast,", "tokens": ["Du", "bist", "ein", "selt\u00b7ner", "Gast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch hast du dir zum Frommen", "tokens": ["Doch", "hast", "du", "dir", "zum", "From\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erkoren hier die Rast;", "tokens": ["Er\u00b7ko\u00b7ren", "hier", "die", "Rast", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von allen K\u00f6niginnen", "tokens": ["Von", "al\u00b7len", "K\u00f6\u00b7ni\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die reichste bin ich bald;", "tokens": ["Die", "reichs\u00b7te", "bin", "ich", "bald", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mein Schlo\u00df mit gr\u00fcnen Zinnen,", "tokens": ["Mein", "Schlo\u00df", "mit", "gr\u00fc\u00b7nen", "Zin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das ist der lust'ge Wald.", "tokens": ["Das", "ist", "der", "lust'\u00b7ge", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sonst macht' ich wohl hinunter", "tokens": ["Sonst", "macht'", "ich", "wohl", "hin\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ins offne Land den Ritt,", "tokens": ["Ins", "off\u00b7ne", "Land", "den", "Ritt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Blumen spro\u00dften munter,", "tokens": ["Und", "Blu\u00b7men", "spro\u00df\u00b7ten", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wohin mein Zelter schritt;", "tokens": ["Wo\u00b7hin", "mein", "Zel\u00b7ter", "schritt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zu bringen Lust und Minne,", "tokens": ["Zu", "brin\u00b7gen", "Lust", "und", "Min\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das war mein fr\u00f6hlich Recht;", "tokens": ["Das", "war", "mein", "fr\u00f6h\u00b7lich", "Recht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch ist von anderm Sinne", "tokens": ["Doch", "ist", "von", "an\u00b7derm", "Sin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "PIS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das heurige Geschlecht.", "tokens": ["Das", "heu\u00b7ri\u00b7ge", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Das tr\u00e4umt von Klingenhieben,", "tokens": ["Das", "tr\u00e4umt", "von", "Klin\u00b7gen\u00b7hie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Schlacht nur und Gescho\u00df;", "tokens": ["Von", "Schlacht", "nur", "und", "Ge\u00b7scho\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da bin ich heimgeblieben", "tokens": ["Da", "bin", "ich", "heim\u00b7ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In meinem Zauberschlo\u00df.", "tokens": ["In", "mei\u00b7nem", "Zau\u00b7ber\u00b7schlo\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nun lehr' ich singend wallen", "tokens": ["Nun", "lehr'", "ich", "sin\u00b7gend", "wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Bach durch Fels und Ried,", "tokens": ["Den", "Bach", "durch", "Fels", "und", "Ried", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nun lehr' ich die Nachtigallen", "tokens": ["Nun", "lehr'", "ich", "die", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Im Lenz ihr s\u00fc\u00dfestes Lied.", "tokens": ["Im", "Lenz", "ihr", "s\u00fc\u00b7\u00dfes\u00b7tes", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Ich wei\u00df, auch du mu\u00dft fechten,", "tokens": ["Ich", "wei\u00df", ",", "auch", "du", "mu\u00dft", "fech\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auch du geh\u00f6rst der Zeit;", "tokens": ["Auch", "du", "ge\u00b7h\u00f6rst", "der", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So steh' zu deinen Rechten", "tokens": ["So", "steh'", "zu", "dei\u00b7nen", "Rech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und f\u00fchre wackern Streit!", "tokens": ["Und", "f\u00fch\u00b7re", "wa\u00b7ckern", "Streit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch will dein Arm erm\u00fcden,", "tokens": ["Doch", "will", "dein", "Arm", "er\u00b7m\u00fc\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bei mir dann kehre du ein,", "tokens": ["Bei", "mir", "dann", "keh\u00b7re", "du", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Im s\u00e4uselnden Waldfrieden", "tokens": ["Im", "s\u00e4u\u00b7seln\u00b7den", "Wald\u00b7frie\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.8": {"text": "Sollst du gekr\u00e4ftigt sein.", "tokens": ["Sollst", "du", "ge\u00b7kr\u00e4f\u00b7tigt", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Da sollst du Frische saugen", "tokens": ["Da", "sollst", "du", "Fri\u00b7sche", "sau\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im harz'gen Duft vom Tann,", "tokens": ["Im", "ha\u00b7rz'\u00b7gen", "Duft", "vom", "Tann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Da schaut aus Blumenaugen", "tokens": ["Da", "schaut", "aus", "Blu\u00b7men\u00b7au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das M\u00e4rchen froh dich an;", "tokens": ["Das", "M\u00e4r\u00b7chen", "froh", "dich", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und macht der Forst dich singen:", "tokens": ["Und", "macht", "der", "Forst", "dich", "sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Es wird in der Zeiten Gang", "tokens": ["Es", "wird", "in", "der", "Zei\u00b7ten", "Gang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Auf solche Weise dringen", "tokens": ["Auf", "sol\u00b7che", "Wei\u00b7se", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie gr\u00fcner Waldhornklang.\u00ab", "tokens": ["Wie", "gr\u00fc\u00b7ner", "Wald\u00b7horn\u00b7klang", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Sie sprach's; ich stand erschrocken", "tokens": ["Sie", "sprach's", ";", "ich", "stand", "er\u00b7schro\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und wu\u00dfte nicht ein Wort,", "tokens": ["Und", "wu\u00df\u00b7te", "nicht", "ein", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da sch\u00fcttelte sie die Locken", "tokens": ["Da", "sch\u00fct\u00b7tel\u00b7te", "sie", "die", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und schwand ins Dickicht fort.", "tokens": ["Und", "schwand", "ins", "Di\u00b7ckicht", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Noch glaubt' ich fern das Wallen", "tokens": ["Noch", "glaubt'", "ich", "fern", "das", "Wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zu sehn des goldnen Haars,", "tokens": ["Zu", "sehn", "des", "gold\u00b7nen", "Haars", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch in den Buchenhallen", "tokens": ["Doch", "in", "den", "Bu\u00b7chen\u00b7hal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein Strahl der Sonne war's.", "tokens": ["Ein", "Strahl", "der", "Son\u00b7ne", "wa\u00b7r'", "s."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "NE"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.11": {"line.1": {"text": "Und wieder schrie der H\u00e4her,", "tokens": ["Und", "wie\u00b7der", "schrie", "der", "H\u00e4\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und wieder quoll die Flut;", "tokens": ["Und", "wie\u00b7der", "quoll", "die", "Flut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch mir entz\u00fccktem Seher", "tokens": ["Doch", "mir", "ent\u00b7z\u00fcck\u00b7tem", "Se\u00b7her"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "War gro\u00df und still zumut.", "tokens": ["War", "gro\u00df", "und", "still", "zu\u00b7mut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und zeihn sie mir's als S\u00fcnde:", "tokens": ["Und", "zeihn", "sie", "mir's", "als", "S\u00fcn\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NE", "KOUS", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich lasse dich dennoch nie,", "tokens": ["Ich", "las\u00b7se", "dich", "den\u00b7noch", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "O Fei der Waldesgr\u00fcnde,", "tokens": ["O", "Fei", "der", "Wal\u00b7des\u00b7gr\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "O Sagenpoesie!", "tokens": ["O", "Sa\u00b7gen\u00b7po\u00b7e\u00b7sie", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}