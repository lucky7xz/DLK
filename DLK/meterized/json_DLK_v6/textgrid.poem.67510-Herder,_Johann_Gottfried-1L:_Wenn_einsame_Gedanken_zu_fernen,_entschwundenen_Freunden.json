{"textgrid.poem.67510": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn einsame Gedanken zu fernen, entschwundenen Freunden", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn einsame Gedanken zu fernen, entschwundenen Freunden", "tokens": ["Wenn", "ein\u00b7sa\u00b7me", "Ge\u00b7dan\u00b7ken", "zu", "fer\u00b7nen", ",", "ent\u00b7schwun\u00b7de\u00b7nen", "Freun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "PTKZU", "VVINF", "$,", "ADJA", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Flie\u00dfen, so ist's vielleicht nur durch der T\u00f6ne Gewalt.", "tokens": ["Flie\u00b7\u00dfen", ",", "so", "ist's", "viel\u00b7leicht", "nur", "durch", "der", "T\u00f6\u00b7ne", "Ge\u00b7walt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VAFIN", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Diese finden den Weg zu Elysiums gl\u00fccklichen Inseln;", "tokens": ["Die\u00b7se", "fin\u00b7den", "den", "Weg", "zu", "E\u00b7ly\u00b7si\u00b7ums", "gl\u00fcck\u00b7li\u00b7chen", "In\u00b7seln", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPR", "NE", "ADJA", "NN", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Auf der Empfindungen Strom gleitet die Seele dahin,", "tokens": ["Auf", "der", "Emp\u00b7fin\u00b7dun\u00b7gen", "Strom", "glei\u00b7tet", "die", "See\u00b7le", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "ART", "NN", "PAV", "$,"], "meter": "+-+-+-++--+--+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Schaffet sich Harmonien, wo sonst Mi\u00dfkl\u00e4nge betr\u00fcbten,", "tokens": ["Schaf\u00b7fet", "sich", "Har\u00b7mo\u00b7ni\u00b7en", ",", "wo", "sonst", "Mi\u00df\u00b7kl\u00e4n\u00b7ge", "be\u00b7tr\u00fcb\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NE", "$,", "PWAV", "ADV", "NN", "VVFIN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "F\u00fchlet in Einem das All, f\u00fchlet in Allem nur Eins!", "tokens": ["F\u00fch\u00b7let", "in", "Ei\u00b7nem", "das", "All", ",", "f\u00fch\u00b7let", "in", "Al\u00b7lem", "nur", "Eins", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIS", "ART", "NN", "$,", "VVFIN", "APPR", "PIS", "ADV", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Auf denn! heilige Seele, la\u00df in der einsamen Zelle", "tokens": ["Auf", "denn", "!", "hei\u00b7li\u00b7ge", "See\u00b7le", ",", "la\u00df", "in", "der", "ein\u00b7sa\u00b7men", "Zel\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "$.", "ADJA", "NN", "$,", "VVIMP", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Widerklingen Dein Herz und sich in T\u00f6nen erfreun!", "tokens": ["Wi\u00b7der\u00b7klin\u00b7gen", "Dein", "Herz", "und", "sich", "in", "T\u00f6\u00b7nen", "er\u00b7freun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "KON", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Locke die Geister hervor, die im Saitenspiele Dir harren!", "tokens": ["Lo\u00b7cke", "die", "Geis\u00b7ter", "her\u00b7vor", ",", "die", "im", "Sai\u00b7ten\u00b7spie\u00b7le", "Dir", "har\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$,", "PRELS", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Sende, wohin Du sie willst, send auch zuweilen sie uns!", "tokens": ["Sen\u00b7de", ",", "wo\u00b7hin", "Du", "sie", "willst", ",", "send", "auch", "zu\u00b7wei\u00b7len", "sie", "uns", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "PPER", "VMFIN", "$,", "ADJD", "ADV", "ADV", "PPER", "PPER", "$."], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}}}}}