{"textgrid.poem.26215": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Ich steh' geblendet", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Bambus schaukeln rot und blaue Papageien,", "tokens": ["Im", "Bam\u00b7bus", "schau\u00b7keln", "rot", "und", "blau\u00b7e", "Pa\u00b7pa\u00b7gei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und gl\u00e4nzend in der lila Sonne wehen der Kokospalmen gr\u00fcne Reihen.", "tokens": ["Und", "gl\u00e4n\u00b7zend", "in", "der", "li\u00b7la", "Son\u00b7ne", "we\u00b7hen", "der", "Ko\u00b7ko\u00b7spal\u00b7men", "gr\u00fc\u00b7ne", "Rei\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+-+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Darunter gehen, bunt wie Edelsteine, die gelbgesichtigen Malaien.", "tokens": ["Da\u00b7run\u00b7ter", "ge\u00b7hen", ",", "bunt", "wie", "E\u00b7del\u00b7stei\u00b7ne", ",", "die", "gelb\u00b7ge\u00b7sich\u00b7ti\u00b7gen", "Ma\u00b7lai\u00b7en", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVINF", "$,", "ADJD", "KOKOM", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Ich steh' geblendet im perlmutterwei\u00dfen Sand im Meergeruch, im freien,", "tokens": ["Ich", "steh'", "ge\u00b7blen\u00b7det", "im", "perl\u00b7mut\u00b7ter\u00b7wei\u00b7\u00dfen", "Sand", "im", "Meer\u00b7ge\u00b7ruch", ",", "im", "frei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "APPRART", "ADJA", "NN", "APPRART", "NN", "$,", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.5": {"text": "Und seufze: \u00bbAll die schnellen S\u00fcdseefarben, sie k\u00f6nnen nicht das Trauerschwarz", "tokens": ["Und", "seuf\u00b7ze", ":", "\u00bb", "All", "die", "schnel\u00b7len", "S\u00fcd\u00b7see\u00b7far\u00b7ben", ",", "sie", "k\u00f6n\u00b7nen", "nicht", "das", "Trau\u00b7er\u00b7schwarz"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "$(", "NN", "ART", "ADJA", "NN", "$,", "PPER", "VMFIN", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.6": {"text": "In meinem Herzen \u00fcberschreien.\u00ab", "tokens": ["In", "mei\u00b7nem", "Her\u00b7zen", "\u00fc\u00b7bersc\u00b7hrei\u00b7en", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}