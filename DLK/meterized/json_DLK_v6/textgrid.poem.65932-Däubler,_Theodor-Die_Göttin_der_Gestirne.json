{"textgrid.poem.65932": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Die G\u00f6ttin der Gestirne", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir m\u00fcssen, als Mond, zu der Sternen-Vertrauten,", "tokens": ["Wir", "m\u00fcs\u00b7sen", ",", "als", "Mond", ",", "zu", "der", "Ster\u00b7nen\u00b7Ver\u00b7trau\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "\u2013 Uns funkelnde G\u00f6ttin \u2013 den Aufschwung befl\u00fcgeln,", "tokens": ["\u2013", "Uns", "fun\u00b7keln\u00b7de", "G\u00f6t\u00b7tin", "\u2013", "den", "Auf\u00b7schwung", "be\u00b7fl\u00fc\u00b7geln", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADJA", "NN", "$(", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Gehimmelter Weltwunsch entwand sich aus Z\u00fcgeln,", "tokens": ["Ge\u00b7him\u00b7mel\u00b7ter", "Welt\u00b7wunsch", "ent\u00b7wand", "sich", "aus", "Z\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die blitzlich in schlingernde Schiefen zerblauten.", "tokens": ["Die", "blitz\u00b7lich", "in", "schlin\u00b7gern\u00b7de", "Schie\u00b7fen", "zer\u00b7blau\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Erblick du dich, Schmachter, bei Perlen und Rauten!", "tokens": ["Er\u00b7blick", "du", "dich", ",", "Schmach\u00b7ter", ",", "bei", "Per\u00b7len", "und", "Rau\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "$,", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wenn b\u00e4ndernde B\u00e4che den Heimschlich erkl\u00fcgeln,", "tokens": ["Wenn", "b\u00e4n\u00b7dern\u00b7de", "B\u00e4\u00b7che", "den", "Heim\u00b7schlich", "er\u00b7kl\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--++-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Entgeister, mit glei\u00dfendem Mondglanz, den H\u00fcgeln", "tokens": ["Ent\u00b7geis\u00b7ter", ",", "mit", "glei\u00b7\u00dfen\u00b7dem", "Mond\u00b7glanz", ",", "den", "H\u00fc\u00b7geln"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "APPR", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "\u2013 Wohl M\u00fcndungen zu \u2013 wo sich Sehnsuchten stauten!", "tokens": ["\u2013", "Wohl", "M\u00fcn\u00b7dun\u00b7gen", "zu", "\u2013", "wo", "sich", "Sehn\u00b7such\u00b7ten", "stau\u00b7ten", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "NN", "PTKZU", "$(", "PWAV", "PRF", "NN", "VVFIN", "$."], "meter": "-+--+--++-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Wir m\u00fcssen, als Mond, zu der Sternengeliebten,", "tokens": ["Wir", "m\u00fcs\u00b7sen", ",", "als", "Mond", ",", "zu", "der", "Ster\u00b7nen\u00b7ge\u00b7lieb\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die Urbrunst zur kommenden Sonne beschwichtigt:", "tokens": ["Die", "Ur\u00b7brunst", "zur", "kom\u00b7men\u00b7den", "Son\u00b7ne", "be\u00b7schwich\u00b7tigt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "So sp\u00fcr, wie ihr kr\u00e4nkelnde M\u00f6ndchen entstiebten!", "tokens": ["So", "sp\u00fcr", ",", "wie", "ihr", "kr\u00e4n\u00b7keln\u00b7de", "M\u00f6nd\u00b7chen", "ent\u00b7stieb\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn blo\u00df uns ein Herz junge Weisung berichtigt,", "tokens": ["Wenn", "blo\u00df", "uns", "ein", "Herz", "jun\u00b7ge", "Wei\u00b7sung", "be\u00b7rich\u00b7tigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "So schweift mich Beruf durch die sternsteilen Bauten,", "tokens": ["So", "schweift", "mich", "Be\u00b7ruf", "durch", "die", "stern\u00b7stei\u00b7len", "Bau\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wo ernst wir die Braut, vor den M\u00fcttern, erschauten!", "tokens": ["Wo", "ernst", "wir", "die", "Braut", ",", "vor", "den", "M\u00fct\u00b7tern", ",", "er\u00b7schau\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$,", "APPR", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}}}}