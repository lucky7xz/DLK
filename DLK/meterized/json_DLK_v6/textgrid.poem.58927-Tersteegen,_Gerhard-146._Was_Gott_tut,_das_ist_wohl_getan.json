{"textgrid.poem.58927": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "146. Was Gott tut, das ist wohl getan", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Preis Gottes Wege und Gericht,", "tokens": ["Preis", "Got\u00b7tes", "We\u00b7ge", "und", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kannst du sie gleich begreifen nicht;", "tokens": ["Kannst", "du", "sie", "gleich", "be\u00b7grei\u00b7fen", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "VVINF", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "W\u00fcnsch nichts, als da\u00df er seinen Willen", "tokens": ["W\u00fcnsch", "nichts", ",", "als", "da\u00df", "er", "sei\u00b7nen", "Wil\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PIS", "$,", "KOKOM", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In dir und allen mag erf\u00fcllen!", "tokens": ["In", "dir", "und", "al\u00b7len", "mag", "er\u00b7f\u00fcl\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}