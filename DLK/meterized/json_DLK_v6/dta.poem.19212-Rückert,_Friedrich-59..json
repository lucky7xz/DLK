{"dta.poem.19212": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "59.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nie der Erscheinung siehst du v\u00f6llig auf den Grund,", "tokens": ["Nie", "der", "Er\u00b7schei\u00b7nung", "siehst", "du", "v\u00f6l\u00b7lig", "auf", "den", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Dinge thun sich nur durch ihre Wirkung kund.", "tokens": ["Die", "Din\u00b7ge", "thun", "sich", "nur", "durch", "ih\u00b7re", "Wir\u00b7kung", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Erkl\u00e4ren magst du sie dir, wie du willst, mein Kind,", "tokens": ["Er\u00b7kl\u00e4\u00b7ren", "magst", "du", "sie", "dir", ",", "wie", "du", "willst", ",", "mein", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "PPER", "$,", "PWAV", "PPER", "VMFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Hauptsach' aber ist: sie brauchen, wie sie sind.", "tokens": ["Die", "Haupt\u00b7sach'", "a\u00b7ber", "ist", ":", "sie", "brau\u00b7chen", ",", "wie", "sie", "sind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "$.", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du siehst: damit dis Na\u00df vom Heber flie\u00dfe, mu\u00df", "tokens": ["Du", "siehst", ":", "da\u00b7mit", "dis", "Na\u00df", "vom", "He\u00b7ber", "flie\u00b7\u00dfe", ",", "mu\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$.", "KOUS", "PDS", "NN", "APPRART", "NN", "VVFIN", "$,", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "L\u00e4nger sein \u00e4u\u00dfrer seyn, k\u00fcrzer sein innrer Fu\u00df.", "tokens": ["L\u00e4n\u00b7ger", "sein", "\u00e4u\u00df\u00b7rer", "seyn", ",", "k\u00fcr\u00b7zer", "sein", "inn\u00b7rer", "Fu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "VAINF", "$,", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Dann gn\u00fcgt ein Mundeshauch, so steigts von selbst im Rohr", "tokens": ["Dann", "gn\u00fcgt", "ein", "Mun\u00b7des\u00b7hauch", ",", "so", "steigts", "von", "selbst", "im", "Rohr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "APPR", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im k\u00fcrzeren, und flie\u00dft im l\u00e4ngeren hervor.", "tokens": ["Im", "k\u00fcr\u00b7ze\u00b7ren", ",", "und", "flie\u00dft", "im", "l\u00e4n\u00b7ge\u00b7ren", "her\u00b7vor", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "KON", "VVFIN", "APPRART", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Warum? ob du's begreifst, das ist nicht von Gewicht;", "tokens": ["Wa\u00b7rum", "?", "ob", "du's", "be\u00b7greifst", ",", "das", "ist", "nicht", "von", "Ge\u00b7wicht", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KOUS", "PIS", "VVFIN", "$,", "PDS", "VAFIN", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+++-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Doch nimmst du ihn verkehrt, so flie\u00dft der Heber nicht.", "tokens": ["Doch", "nimmst", "du", "ihn", "ver\u00b7kehrt", ",", "so", "flie\u00dft", "der", "He\u00b7ber", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "VVPP", "$,", "ADV", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die Dinge der Natur stehn unter Zauberbann,", "tokens": ["Die", "Din\u00b7ge", "der", "Na\u00b7tur", "stehn", "un\u00b7ter", "Zau\u00b7ber\u00b7bann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und der beherrscht sie, wer das Wort aussprechen kann.", "tokens": ["Und", "der", "be\u00b7herrscht", "sie", ",", "wer", "das", "Wort", "aus\u00b7spre\u00b7chen", "kann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "PPER", "$,", "PWS", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Es auszusprechen gn\u00fcgt, ob oder nicht verstanden;", "tokens": ["Es", "aus\u00b7zu\u00b7spre\u00b7chen", "gn\u00fcgt", ",", "ob", "o\u00b7der", "nicht", "ver\u00b7stan\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVIZU", "VVFIN", "$,", "KOUS", "KON", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ganz es zu verstehn, ist noch nicht Zeit vorhanden.", "tokens": ["Und", "ganz", "es", "zu", "ver\u00b7stehn", ",", "ist", "noch", "nicht", "Zeit", "vor\u00b7han\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "PTKZU", "VVINF", "$,", "VAFIN", "ADV", "PTKNEG", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}