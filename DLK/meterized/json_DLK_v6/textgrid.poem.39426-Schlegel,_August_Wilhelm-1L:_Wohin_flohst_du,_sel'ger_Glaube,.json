{"textgrid.poem.39426": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wohin flohst du, sel'ger Glaube,", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohin flohst du, sel'ger Glaube,", "tokens": ["Wo\u00b7hin", "flohst", "du", ",", "sel'\u00b7ger", "Glau\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Aus der Menschen Sinn und Muth?", "tokens": ["Aus", "der", "Men\u00b7schen", "Sinn", "und", "Muth", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wurdest schn\u00f6dem Spott zum Raube,", "tokens": ["Wur\u00b7dest", "schn\u00f6\u00b7dem", "Spott", "zum", "Rau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohne Ruhstatt irrt die Taube", "tokens": ["Oh\u00b7ne", "Ruh\u00b7statt", "irrt", "die", "Tau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ob der gro\u00dfen S\u00fcndenflut.", "tokens": ["Ob", "der", "gro\u00b7\u00dfen", "S\u00fcn\u00b7den\u00b7flut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Du, o Glaub' an reine Liebe,", "tokens": ["Du", ",", "o", "Glaub'", "an", "rei\u00b7ne", "Lie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "FM", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die das Herz in F\u00fclle n\u00e4hrt,", "tokens": ["Die", "das", "Herz", "in", "F\u00fcl\u00b7le", "n\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die, wenn keine Jugend bliebe,", "tokens": ["Die", ",", "wenn", "kei\u00b7ne", "Ju\u00b7gend", "blie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Keine Sch\u00f6nheit, inn'ge Triebe", "tokens": ["Kei\u00b7ne", "Sch\u00f6n\u00b7heit", ",", "inn'\u00b7ge", "Trie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bis zum letzten Hauch gew\u00e4hrt!", "tokens": ["Bis", "zum", "letz\u00b7ten", "Hauch", "ge\u00b7w\u00e4hrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Glaub' an eines Freundes Treue,", "tokens": ["Glaub'", "an", "ei\u00b7nes", "Freun\u00b7des", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welcher mit uns steht und f\u00e4llt,", "tokens": ["Wel\u00b7cher", "mit", "uns", "steht", "und", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "APPR", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Welcher ohne Scheu und Reue,", "tokens": ["Wel\u00b7cher", "oh\u00b7ne", "Scheu", "und", "Reu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie auch Leumund ihn bedr\u00e4ue,", "tokens": ["Wie", "auch", "Leu\u00b7mund", "ihn", "be\u00b7dr\u00e4u\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "PPER", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Uns bekennt vor aller Welt!", "tokens": ["Uns", "be\u00b7kennt", "vor", "al\u00b7ler", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Glaub' an die Gewalt der Ehre,", "tokens": ["Glaub'", "an", "die", "Ge\u00b7walt", "der", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Alles Thuns Geleit und Hort,", "tokens": ["Al\u00b7les", "Thuns", "Ge\u00b7leit", "und", "Hort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df kein Schwur sich je verkehre,", "tokens": ["Da\u00df", "kein", "Schwur", "sich", "je", "ver\u00b7keh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "VVFIN", "PRF", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Felsenfest die biedre Lehre", "tokens": ["Fel\u00b7sen\u00b7fest", "die", "bied\u00b7re", "Leh\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Immer steh': ein Mann, ein Wort!", "tokens": ["Im\u00b7mer", "steh'", ":", "ein", "Mann", ",", "ein", "Wort", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Glaub' an unsers Volkes Weise,", "tokens": ["Glaub'", "an", "un\u00b7sers", "Vol\u00b7kes", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An ein heimisch Vaterland,", "tokens": ["An", "ein", "hei\u00b7misch", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo im schlichten alten Kreise", "tokens": ["Wo", "im", "schlich\u00b7ten", "al\u00b7ten", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jeder still beharrt, und weise", "tokens": ["Je\u00b7der", "still", "be\u00b7harrt", ",", "und", "wei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "ADJD", "VVFIN", "$,", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Fremde L\u00fcst' und Sitten bannt!", "tokens": ["Frem\u00b7de", "L\u00fcst'", "und", "Sit\u00b7ten", "bannt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Glaub' an Kunde von den hohen", "tokens": ["Glaub'", "an", "Kun\u00b7de", "von", "den", "ho\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Thaten k\u00fchner alter Zeit,", "tokens": ["Tha\u00b7ten", "k\u00fch\u00b7ner", "al\u00b7ter", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An die W\u00fcrde der Heroen,", "tokens": ["An", "die", "W\u00fcr\u00b7de", "der", "He\u00b7roen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deren Geist der Welt entflohen,", "tokens": ["De\u00b7ren", "Geist", "der", "Welt", "ent\u00b7flo\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Deren Namen sie entweiht!", "tokens": ["De\u00b7ren", "Na\u00b7men", "sie", "ent\u00b7weiht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Glaub' an hehrer Freiheit Dauer,", "tokens": ["Glaub'", "an", "heh\u00b7rer", "Frei\u00b7heit", "Dau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf Gesetz erbaut und Recht,", "tokens": ["Auf", "Ge\u00b7setz", "er\u00b7baut", "und", "Recht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schirmend in der Bundesmauer", "tokens": ["Schir\u00b7mend", "in", "der", "Bun\u00b7des\u00b7mau\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00f6nig, Ritter, B\u00fcrger, Bauer,", "tokens": ["K\u00f6\u00b7nig", ",", "Rit\u00b7ter", ",", "B\u00fcr\u00b7ger", ",", "Bau\u00b7er", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "All' ein br\u00fcderlich Geschlecht!", "tokens": ["All'", "ein", "br\u00fc\u00b7der\u00b7lich", "Ge\u00b7schlecht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Glaub' an milder Vorsicht Wache,", "tokens": ["Glaub'", "an", "mil\u00b7der", "Vor\u00b7sicht", "Wa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie es sei um uns bestellt;", "tokens": ["Wie", "es", "sei", "um", "uns", "be\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df Er denk' an unsre Sache,", "tokens": ["Da\u00df", "Er", "denk'", "an", "uns\u00b7re", "Sa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dem kein Sperling f\u00e4llt vom Dache,", "tokens": ["Dem", "kein", "Sper\u00b7ling", "f\u00e4llt", "vom", "Da\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gleichwie er das Ganze h\u00e4lt!", "tokens": ["Gleich\u00b7wie", "er", "das", "Gan\u00b7ze", "h\u00e4lt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Glaub' an jenes Licht von oben,", "tokens": ["Glaub'", "an", "je\u00b7nes", "Licht", "von", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "APPR", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das so glorreich niederstrahlt,", "tokens": ["Das", "so", "glor\u00b7reich", "nie\u00b7der\u00b7strahlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und am Vorhang, blau gewoben", "tokens": ["Und", "am", "Vor\u00b7hang", ",", "blau", "ge\u00b7wo\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPRART", "NN", "$,", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor dem Heiligsten da droben,", "tokens": ["Vor", "dem", "Hei\u00b7ligs\u00b7ten", "da", "dro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADV", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Ew'ger Wahrheit Bilder mahlt!", "tokens": ["Ew'\u00b7ger", "Wahr\u00b7heit", "Bil\u00b7der", "mahlt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Glaub' an aller Liebe Bronnen,", "tokens": ["Glaub'", "an", "al\u00b7ler", "Lie\u00b7be", "Bron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der die Gottheit selbst ergo\u00df,", "tokens": ["Der", "die", "Got\u00b7theit", "selbst", "er\u00b7go\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In des Opfers Glut zerronnen,", "tokens": ["In", "des", "Op\u00b7fers", "Glut", "zer\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welches, s\u00fchnend, Friedenswonnen", "tokens": ["Wel\u00b7ches", ",", "s\u00fch\u00b7nend", ",", "Frie\u00b7dens\u00b7won\u00b7nen"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["PWS", "$,", "VVPP", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der Wesen Heil erschlo\u00df! \u2013", "tokens": ["Und", "der", "We\u00b7sen", "Heil", "er\u00b7schlo\u00df", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Was die H\u00e4nd' und Augen greifen,", "tokens": ["Was", "die", "H\u00e4nd'", "und", "Au\u00b7gen", "grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist ein tr\u00fcglich eitles Gut.", "tokens": ["Ist", "ein", "tr\u00fcg\u00b7lich", "eit\u00b7les", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie die klugen Sinn' auch schweifen,", "tokens": ["Wie", "die", "klu\u00b7gen", "Sinn'", "auch", "schwei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Niemals wird ein Segen reifen,", "tokens": ["Nie\u00b7mals", "wird", "ein", "Se\u00b7gen", "rei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Strebet h\u00f6her nicht der Muth.", "tokens": ["Stre\u00b7bet", "h\u00f6\u00b7her", "nicht", "der", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKNEG", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Vor dem Glauben Berge schwanden,", "tokens": ["Vor", "dem", "Glau\u00b7ben", "Ber\u00b7ge", "schwan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Glaube macht die Schwachen stark.", "tokens": ["Glau\u00b7be", "macht", "die", "Schwa\u00b7chen", "stark", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja, aus Erd' und Todes-Banden", "tokens": ["Ja", ",", "aus", "Erd'", "und", "To\u00b7des\u00b7Ban\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist der Gl\u00e4ub'ge schon erstanden:", "tokens": ["Ist", "der", "Gl\u00e4ub'\u00b7ge", "schon", "er\u00b7stan\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Glaub' ist unsers Lebens Mark.", "tokens": ["Glaub'", "ist", "un\u00b7sers", "Le\u00b7bens", "Mark", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Komm denn, himmlisches Vertrauen,", "tokens": ["Komm", "denn", ",", "himm\u00b7li\u00b7sches", "Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Komm zur\u00fcck in meine Brust!", "tokens": ["Komm", "zu\u00b7r\u00fcck", "in", "mei\u00b7ne", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wolle linde mich bethauen,", "tokens": ["Wol\u00b7le", "lin\u00b7de", "mich", "be\u00b7thau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie die winterlichen Auen", "tokens": ["Wie", "die", "win\u00b7ter\u00b7li\u00b7chen", "Au\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Linde Luft und Fr\u00fchlingsluft.", "tokens": ["Lin\u00b7de", "Luft", "und", "Fr\u00fch\u00b7lings\u00b7luft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Scheuche du das tr\u00fcbe Zagen!", "tokens": ["Scheu\u00b7che", "du", "das", "tr\u00fc\u00b7be", "Za\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was verschuldet' ich so schwer,", "tokens": ["Was", "ver\u00b7schul\u00b7det'", "ich", "so", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich nie mich soll entschlagen", "tokens": ["Da\u00df", "ich", "nie", "mich", "soll", "ent\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der Gedanken und der Fragen,", "tokens": ["Der", "Ge\u00b7dan\u00b7ken", "und", "der", "Fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die sich streiten hin und her?", "tokens": ["Die", "sich", "strei\u00b7ten", "hin", "und", "her", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Zwar ich habe mit den Blinden", "tokens": ["Zwar", "ich", "ha\u00b7be", "mit", "den", "Blin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Falscher Weisheit auch gefr\u00f6hnt,", "tokens": ["Fal\u00b7scher", "Weis\u00b7heit", "auch", "ge\u00b7fr\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch gesucht den Weg zu finden", "tokens": ["Doch", "ge\u00b7sucht", "den", "Weg", "zu", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "ART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus des Irrthums Labyrinthen,", "tokens": ["Aus", "des", "Irr\u00b7thums", "La\u00b7by\u00b7rin\u00b7then", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und das Edle nie geh\u00f6hnt.", "tokens": ["Und", "das", "Ed\u00b7le", "nie", "ge\u00b7h\u00f6hnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Kann Gehorsam dich erwerben,", "tokens": ["Kann", "Ge\u00b7hor\u00b7sam", "dich", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Giebst du dich der Einfalt kund:", "tokens": ["Giebst", "du", "dich", "der", "Ein\u00b7falt", "kund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieh in Demuth mich ersterben,", "tokens": ["Sieh", "in", "De\u00b7muth", "mich", "ers\u00b7ter\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "PPER", "VVPP", "$,"], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.4": {"text": "Sieh die Wehmuth mich entf\u00e4rben.", "tokens": ["Sieh", "die", "Weh\u00b7muth", "mich", "ent\u00b7f\u00e4r\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Thu' mir auf der Geister Bund!", "tokens": ["Thu'", "mir", "auf", "der", "Geis\u00b7ter", "Bund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "O wie hat mich oft erhoben,", "tokens": ["O", "wie", "hat", "mich", "oft", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was du halb mir nur enth\u00fcllst!", "tokens": ["Was", "du", "halb", "mir", "nur", "ent\u00b7h\u00fcllst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df mich deine Kraft erproben,", "tokens": ["La\u00df", "mich", "dei\u00b7ne", "Kraft", "er\u00b7pro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jubeln will ich und Gott loben,", "tokens": ["Ju\u00b7beln", "will", "ich", "und", "Gott", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn du ganz die Seele f\u00fcllst.", "tokens": ["Wenn", "du", "ganz", "die", "See\u00b7le", "f\u00fcllst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Ob dann soll der Boden schwanken,", "tokens": ["Ob", "dann", "soll", "der", "Bo\u00b7den", "schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob die H\u00f6lle scheinbar siegt,", "tokens": ["Ob", "die", "H\u00f6l\u00b7le", "schein\u00b7bar", "siegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Will als Reb' ich ohne Wanken", "tokens": ["Will", "als", "Reb'", "ich", "oh\u00b7ne", "Wan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "KOKOM", "VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf am Lebensbaum mich ranken,", "tokens": ["Auf", "am", "Le\u00b7bens\u00b7baum", "mich", "ran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Welcher keinem Blitz erliegt.", "tokens": ["Wel\u00b7cher", "kei\u00b7nem", "Blitz", "er\u00b7liegt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}