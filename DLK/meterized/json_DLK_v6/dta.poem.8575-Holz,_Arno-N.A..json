{"dta.poem.8575": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1899", "urn": "urn:nbn:de:kobv:b4-200905192690", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Im Hause, wo die bunten Ampeln brennen,", "tokens": ["Im", "Hau\u00b7se", ",", "wo", "die", "bun\u00b7ten", "Am\u00b7peln", "bren\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "gl\u00e4nzen auf demselben B\u00fccherspind,", "tokens": ["gl\u00e4n\u00b7zen", "auf", "dem\u00b7sel\u00b7ben", "B\u00fc\u00b7cher\u00b7spind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "\u00fcber George Ohnet, Stinde und Dante,", "tokens": ["\u00fc\u00b7ber", "Ge\u00b7or\u00b7ge", "Oh\u00b7net", ",", "Stin\u00b7de", "und", "Dan\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "$,", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Schiller und Goethe:", "tokens": ["Schil\u00b7ler", "und", "Goe\u00b7the", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "beide beteiligt an ein und demselben Gypskranz!", "tokens": ["bei\u00b7de", "be\u00b7tei\u00b7ligt", "an", "ein", "und", "dem\u00b7sel\u00b7ben", "Gyp\u00b7skranz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ART", "KON", "PDAT", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}}, "stanza.2": {"line.1": {"text": "Im Hause, wo die bunten Ampeln brennen,", "tokens": ["Im", "Hau\u00b7se", ",", "wo", "die", "bun\u00b7ten", "Am\u00b7peln", "bren\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "h\u00e4ngt an derselben Wedgwoodtapete, \u00fcber demselben Rokokoschirm,", "tokens": ["h\u00e4ngt", "an", "der\u00b7sel\u00b7ben", "Wedg\u00b7wood\u00b7ta\u00b7pe\u00b7te", ",", "\u00fc\u00b7ber", "dem\u00b7sel\u00b7ben", "Ro\u00b7ko\u00b7ko\u00b7schirm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PDAT", "NN", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "+--+--+-+-+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "zwischen Klinger und Hokusai,", "tokens": ["zwi\u00b7schen", "Klin\u00b7ger", "und", "Ho\u00b7ku\u00b7sai", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Anton von Werner.", "tokens": ["An\u00b7ton", "von", "Wer\u00b7ner", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPR", "NE", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Im Hause, wo die bunten Ampeln brennen,", "tokens": ["Im", "Hau\u00b7se", ",", "wo", "die", "bun\u00b7ten", "Am\u00b7peln", "bren\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "spielen dieselben schlanken H\u00e4nde, auf demselben Ebenholzfl\u00fcgel,", "tokens": ["spie\u00b7len", "die\u00b7sel\u00b7ben", "schlan\u00b7ken", "H\u00e4n\u00b7de", ",", "auf", "dem\u00b7sel\u00b7ben", "E\u00b7ben\u00b7holz\u00b7fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "ADJA", "NN", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "+--+-+-+-+-+-+--+-", "measure": "iambic.octa.plus.invert"}, "line.3": {"text": "mit demselben Charm und Chic", "tokens": ["mit", "dem\u00b7sel\u00b7ben", "Charm", "und", "Chic"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "KON", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Fr\u00e9d\u00e9ric Fran\u00e7ois Chopin und Ludolf Waldmann.", "tokens": ["Fr\u00e9d\u00e9ric", "Fran\u00e7ois", "Cho\u00b7pin", "und", "Lu\u00b7dolf", "Wald\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "KON", "NE", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Im Hause, wo die bunten Ampeln brennen,", "tokens": ["Im", "Hau\u00b7se", ",", "wo", "die", "bun\u00b7ten", "Am\u00b7peln", "bren\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "auf vergoldeten St\u00fchlchen sitzend,", "tokens": ["auf", "ver\u00b7gol\u00b7de\u00b7ten", "St\u00fchl\u00b7chen", "sit\u00b7zend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "trinkt man Chablis, Pilsner und Sect,", "tokens": ["trinkt", "man", "Cha\u00b7blis", ",", "Pils\u00b7ner", "und", "Sect", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "NE", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "kommt dann peu-\u00e0-peu auf Nietzsche,", "tokens": ["kommt", "dann", "peu\u00b7\u00e0\u00b7peu", "auf", "Nietz\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "APPR", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "zuletzt wird getanzt.", "tokens": ["zu\u00b7letzt", "wird", "ge\u00b7tanzt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Ich k\u00fcsse entz\u00fcckt der Hausfrau die Hand,", "tokens": ["Ich", "k\u00fcs\u00b7se", "ent\u00b7z\u00fcckt", "der", "Haus\u00b7frau", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "entt\u00e4usche einen \u00e4lteren, glattrasirten Herrn", "tokens": ["ent\u00b7t\u00e4u\u00b7sche", "ei\u00b7nen", "\u00e4l\u00b7te\u00b7ren", ",", "glatt\u00b7ra\u00b7sir\u00b7ten", "Herrn"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "mit baumwollnen Handschuhen und Wadenstr\u00fcmpfen", "tokens": ["mit", "baum\u00b7woll\u00b7nen", "Hand\u00b7schu\u00b7hen", "und", "Wa\u00b7den\u00b7str\u00fcmp\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+--++--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "durch eine Mark Trinkgeld", "tokens": ["durch", "ei\u00b7ne", "Mark", "Trink\u00b7geld"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "und verschwinde.", "tokens": ["und", "ver\u00b7schwin\u00b7de", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJA", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}