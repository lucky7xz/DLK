{"textgrid.poem.39976": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "4. Die Schlacht am Speicher", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In dem gr\u00fcnen Speicherwald,", "tokens": ["In", "dem", "gr\u00fc\u00b7nen", "Spei\u00b7cher\u00b7wald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Drunter schmucke H\u00e4user liegen,", "tokens": ["Drun\u00b7ter", "schmu\u00b7cke", "H\u00e4u\u00b7ser", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Werden freie M\u00e4nner bald", "tokens": ["Wer\u00b7den", "frei\u00b7e", "M\u00e4n\u00b7ner", "bald"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADJA", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Fr\u00f6hlich sterben oder siegen.", "tokens": ["Fr\u00f6h\u00b7lich", "ster\u00b7ben", "o\u00b7der", "sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "KON", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Von dem Sternenhimmel sieht", "tokens": ["Von", "dem", "Ster\u00b7nen\u00b7him\u00b7mel", "sieht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gott auf sie, der Herr der Schlachten,", "tokens": ["Gott", "auf", "sie", ",", "der", "Herr", "der", "Schlach\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo das fromme H\u00e4uflein kniet,", "tokens": ["Wo", "das", "from\u00b7me", "H\u00e4uf\u00b7lein", "kniet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Betend hier zu \u00fcbernachten.", "tokens": ["Be\u00b7tend", "hier", "zu", "\u00fc\u00b7bern\u00b7ach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbwenn es sein mag,\u00ab flehen sie,", "tokens": ["\u00bb", "wenn", "es", "sein", "mag", ",", "\u00ab", "fle\u00b7hen", "sie", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "VAINF", "VMFIN", "$,", "$(", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbla\u00df, o Herr! uns hier genesen!", "tokens": ["\u00bb", "la\u00df", ",", "o", "Herr", "!", "uns", "hier", "ge\u00b7ne\u00b7sen", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "$,", "FM", "NN", "$.", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder sei der Boden hie", "tokens": ["O\u00b7der", "sei", "der", "Bo\u00b7den", "hie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Uns zum Kirchhof auserlesen", "tokens": ["Uns", "zum", "Kirch\u00b7hof", "au\u00b7ser\u00b7le\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer sich fliehend umgewandt,", "tokens": ["Wer", "sich", "flie\u00b7hend", "um\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Werd' auf fremder Erd' erschlagen,", "tokens": ["Werd'", "auf", "frem\u00b7der", "Erd'", "er\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nicht das freie Vaterland", "tokens": ["Nicht", "das", "frei\u00b7e", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Soll im Schoose solchen tragen!\u00ab", "tokens": ["Soll", "im", "Schoo\u00b7se", "sol\u00b7chen", "tra\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "APPRART", "NN", "PIAT", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und der erste Sonnenstral", "tokens": ["Und", "der", "ers\u00b7te", "Son\u00b7nen\u00b7stral"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "L\u00e4chelt, wie sie sprechen Amen,", "tokens": ["L\u00e4\u00b7chelt", ",", "wie", "sie", "spre\u00b7chen", "A\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als die Feinde von dem Thal", "tokens": ["Als", "die", "Fein\u00b7de", "von", "dem", "Thal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach den H\u00f6hn gestiegen kamen;", "tokens": ["Nach", "den", "H\u00f6hn", "ge\u00b7stie\u00b7gen", "ka\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Vorn die Edeln, hoch zu Ro\u00df,", "tokens": ["Vorn", "die", "E\u00b7deln", ",", "hoch", "zu", "Ro\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die im Sattel st\u00e4hlern sitzen,", "tokens": ["Die", "im", "Sat\u00b7tel", "st\u00e4h\u00b7lern", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ihnen folgt ein kecker Tro\u00df", "tokens": ["Ih\u00b7nen", "folgt", "ein", "ke\u00b7cker", "Tro\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Leichtbewehrter Bogensch\u00fctzen.", "tokens": ["Leicht\u00b7be\u00b7wehr\u00b7ter", "Bo\u00b7gen\u00b7sch\u00fct\u00b7zen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch sie sind die letzten nicht,", "tokens": ["Doch", "sie", "sind", "die", "letz\u00b7ten", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJA", "PTKNEG", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Die bergan behende laufen:", "tokens": ["Die", "ber\u00b7gan", "be\u00b7hen\u00b7de", "lau\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hinten erst im Sonnenlicht", "tokens": ["Hin\u00b7ten", "erst", "im", "Son\u00b7nen\u00b7licht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gl\u00e4nzen die gewalt'gen Haufen:", "tokens": ["Gl\u00e4n\u00b7zen", "die", "ge\u00b7walt'\u00b7gen", "Hau\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dicht, wie Blumen in dem Lenz,", "tokens": ["Dicht", ",", "wie", "Blu\u00b7men", "in", "dem", "Lenz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Funkeln Helme, winken H\u00fcte;", "tokens": ["Fun\u00b7keln", "Hel\u00b7me", ",", "win\u00b7ken", "H\u00fc\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Constanz, Ravenspurg, Bregenz", "tokens": ["Con\u00b7stanz", ",", "Ra\u00b7ven\u00b7spurg", ",", "Bre\u00b7genz"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NE", "$,", "NE", "$,", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sendet seiner M\u00e4nner Bl\u00fcte.", "tokens": ["Sen\u00b7det", "sei\u00b7ner", "M\u00e4n\u00b7ner", "Bl\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und die Kirche schickt den Bann", "tokens": ["Und", "die", "Kir\u00b7che", "schickt", "den", "Bann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fluchend in des Hirten Ohren,", "tokens": ["Flu\u00b7chend", "in", "des", "Hir\u00b7ten", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Pfaffe, B\u00fcrger, Edelmann", "tokens": ["Pfaf\u00b7fe", ",", "B\u00fcr\u00b7ger", ",", "E\u00b7del\u00b7mann"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Haben Schmach ihm heut geschworen.", "tokens": ["Ha\u00b7ben", "Schmach", "ihm", "heut", "ge\u00b7schwo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbwill der Bauer,\u00ab sprechen sie,", "tokens": ["\u00bb", "will", "der", "Bau\u00b7er", ",", "\u00ab", "spre\u00b7chen", "sie", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "VMFIN", "ART", "NN", "$,", "$(", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00bbgegen uns sein Haupt erheben?", "tokens": ["\u00bb", "ge\u00b7gen", "uns", "sein", "Haupt", "er\u00b7he\u00b7ben", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nieder mu\u00df er auf das Knie,", "tokens": ["Nie\u00b7der", "mu\u00df", "er", "auf", "das", "Knie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Mu\u00df erst betteln um sein Leben!\u00ab", "tokens": ["Mu\u00df", "erst", "bet\u00b7teln", "um", "sein", "Le\u00b7ben", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "H\u00e4ttet ihr geschauet ihn,", "tokens": ["H\u00e4t\u00b7tet", "ihr", "ge\u00b7schau\u00b7et", "ihn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ei, wie w\u00fcrdet ihr ihn loben,", "tokens": ["Ei", ",", "wie", "w\u00fcr\u00b7det", "ihr", "ihn", "lo\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VAFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn er lag schon auf den Knie'n,", "tokens": ["Denn", "er", "lag", "schon", "auf", "den", "Knie'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jetzt erst hat er sich erhoben.", "tokens": ["Jetzt", "erst", "hat", "er", "sich", "er\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PRF", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja, vor Gott hat er gekniet,", "tokens": ["Ja", ",", "vor", "Gott", "hat", "er", "ge\u00b7kniet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "APPR", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Doch vor euch denkt er zu stehen,", "tokens": ["Doch", "vor", "euch", "denkt", "er", "zu", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Ob er schon zur\u00fcck sich zieht,", "tokens": ["Ob", "er", "schon", "zu\u00b7r\u00fcck", "sich", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKVZ", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Klug verborgen auf den H\u00f6hen.", "tokens": ["Klug", "ver\u00b7bor\u00b7gen", "auf", "den", "H\u00f6\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Einsam trifft der Feind den Wald,", "tokens": ["Ein\u00b7sam", "trifft", "der", "Feind", "den", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein Verhau von wenig St\u00e4mmen", "tokens": ["Ein", "Ver\u00b7hau", "von", "we\u00b7nig", "St\u00e4m\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Macht ihm keinen Aufenthalt,", "tokens": ["Macht", "ihm", "kei\u00b7nen", "Auf\u00b7ent\u00b7halt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kann den raschen Zug nicht hemmen.", "tokens": ["Kann", "den", "ra\u00b7schen", "Zug", "nicht", "hem\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus der St\u00e4dter r\u00fcst'gen Reih'n", "tokens": ["Aus", "der", "St\u00e4d\u00b7ter", "r\u00fcst'\u00b7gen", "Reih'n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Treten vor die Zimmerleute,", "tokens": ["Tre\u00b7ten", "vor", "die", "Zim\u00b7mer\u00b7leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sto\u00dfen ihn mit Lachen ein:", "tokens": ["Sto\u00b7\u00dfen", "ihn", "mit", "La\u00b7chen", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "\u00bbappenzell, bist unsre Beute!\u00ab", "tokens": ["\u00bb", "ap\u00b7pen\u00b7zell", ",", "bist", "uns\u00b7re", "Beu\u00b7te", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$,", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Sieh da! von den h\u00f6chsten H\u00f6h'n", "tokens": ["Sieh", "da", "!", "von", "den", "h\u00f6chs\u00b7ten", "H\u00f6h'n"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Rasselt es mit Steinen nieder,", "tokens": ["Ras\u00b7selt", "es", "mit", "Stei\u00b7nen", "nie\u