oken_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Den besieht vom Berge sich", "tokens": ["Den", "be\u00b7sieht", "vom", "Ber\u00b7ge", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPRART", "NN", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch zuletzt ein Hirtenjunge:", "tokens": ["Doch", "zu\u00b7letzt", "ein", "Hir\u00b7ten\u00b7jun\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbhilft mir Gott, so f\u00e4ll' ich dich!\u00ab", "tokens": ["\u00bb", "hilft", "mir", "Gott", ",", "so", "f\u00e4ll'", "ich", "dich", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "NN", "$,", "ADV", "VVFIN", "PPER", "PRF", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hebt die Schleuder dann zum Schwunge", "tokens": ["Hebt", "die", "Schleu\u00b7der", "dann", "zum", "Schwun\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Einen spitzen Stein er schie\u00dft", "tokens": ["Ei\u00b7nen", "spit\u00b7zen", "Stein", "er", "schie\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihm so flink durch's Helmesgitter,", "tokens": ["Ihm", "so", "flink", "durch's", "Hel\u00b7mes\u00b7git\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df das Blut sich draus ergie\u00dft,", "tokens": ["Da\u00df", "das", "Blut", "sich", "draus", "er\u00b7gie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "PAV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und zu Boden st\u00fcrzt der Ritter.", "tokens": ["Und", "zu", "Bo\u00b7den", "st\u00fcrzt", "der", "Rit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Drauf herab hat sich die Flucht", "tokens": ["Drauf", "her\u00b7ab", "hat", "sich", "die", "Flucht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "VAFIN", "PRF", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In Sankt Gallens Thal gezogen,", "tokens": ["In", "Sankt", "Gal\u00b7lens", "Thal", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "NE", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zwanzig Hirten in die Schlucht", "tokens": ["Zwan\u00b7zig", "Hir\u00b7ten", "in", "die", "Schlucht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind ihr k\u00fchnlich nachgeflogen;", "tokens": ["Sind", "ihr", "k\u00fchn\u00b7lich", "nach\u00b7ge\u00b7flo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Werfen einen Feuerbrand", "tokens": ["Wer\u00b7fen", "ei\u00b7nen", "Feu\u00b7er\u0