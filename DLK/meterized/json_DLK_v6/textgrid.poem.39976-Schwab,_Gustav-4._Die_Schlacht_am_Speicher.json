 ["KON", "ADV", "VMFIN", "PPER", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wehrt sich einer aus dem Schwarme.", "tokens": ["Wehrt", "sich", "ei\u00b7ner", "aus", "dem", "Schwar\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Denn es fliegt der Alpenhirt", "tokens": ["Denn", "es", "fliegt", "der", "Al\u00b7pen\u00b7hirt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00fcpfend auf die Felsenst\u00fccke,", "tokens": ["H\u00fcp\u00b7fend", "auf", "die", "Fel\u00b7sen\u00b7st\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df kein Streich, kein Schu\u00df verirrt", "tokens": ["Da\u00df", "kein", "Streich", ",", "kein", "Schu\u00df", "ver\u00b7irrt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "$,", "PIAT", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unter seinem sichern Blicke,", "tokens": ["Un\u00b7ter", "sei\u00b7nem", "si\u00b7chern", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bis des Klosters Knechte fliehn,", "tokens": ["Bis", "des", "Klos\u00b7ters", "Knech\u00b7te", "fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die zuerst, wie feige Weiber,", "tokens": ["Di