 "tokens": ["Schaut", "in", "bei\u00b7de", "Th\u00e4\u00b7ler", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "H\u00f6ret aus dem Appenzell", "tokens": ["H\u00f6\u00b7ret", "aus", "dem", "Ap\u00b7pen\u00b7zell"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Freien Volkes Jubel schallen,", "tokens": ["Frei\u00b7en", "Vol\u00b7kes", "Ju\u00b7bel", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ein Totengl\u00f6cklein hell", "tokens": ["Und", "ein", "To\u00b7ten\u00b7gl\u00f6c\u00b7klein", "hell"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "T\u00f6nt her\u00fcber aus Sankt Gallen.", "tokens": ["T\u00f6nt", "her\u00b7\u00fc\u00b7ber", "aus", "Sankt", "Gal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "VVFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}