{"textgrid.poem.57723": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Ein Lied zum Weine", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es schl\u00e4gt ein fremder Fink im Land,", "tokens": ["Es", "schl\u00e4gt", "ein", "frem\u00b7der", "Fink", "im", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Radibimmel, radibammel, radibumm,", "tokens": ["Ra\u00b7di\u00b7bim\u00b7mel", ",", "ra\u00b7di\u00b7bam\u00b7mel", ",", "ra\u00b7di\u00b7bumm", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Die Luft die riecht wie angebrannt,", "tokens": ["Die", "Luft", "die", "riecht", "wie", "an\u00b7ge\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVFIN", "KOKOM", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Tilly, der zieht um;", "tokens": ["Der", "Til\u00b7ly", ",", "der", "zieht", "um", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PRELS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es klingt so fein, radibimmbummbamm", "tokens": ["Es", "klingt", "so", "fein", ",", "ra\u00b7di\u00b7bimm\u00b7bumm\u00b7bamm"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "XY"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die Pfeife und die Trumm.", "tokens": ["Die", "Pfei\u00b7fe", "und", "die", "Trumm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Rose bl\u00fcht, der Dorn der sticht,", "tokens": ["Die", "Ro\u00b7se", "bl\u00fcht", ",", "der", "Dorn", "der", "sticht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das steht in jedem Krug,", "tokens": ["Das", "steht", "in", "je\u00b7dem", "Krug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer gleich bezahlt, vergi\u00dft es nicht,", "tokens": ["Wer", "gleich", "be\u00b7zahlt", ",", "ver\u00b7gi\u00dft", "es", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "$,", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Z\u00f6gerns ist genug;", "tokens": ["Des", "Z\u00f6\u00b7gerns", "ist", "ge\u00b7nug", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Lutherschen die m\u00fcssen dran", "tokens": ["Die", "Lu\u00b7ther\u00b7schen", "die", "m\u00fcs\u00b7sen", "dran"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "VMFIN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Haus und Hof, mit Maus und Mann,", "tokens": ["Mit", "Haus", "und", "Hof", ",", "mit", "Maus", "und", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Denselben gilt der Zug.", "tokens": ["Den\u00b7sel\u00b7ben", "gilt", "der", "Zug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Wind der weht, der Hahn der kr\u00e4ht,", "tokens": ["Der", "Wind", "der", "weht", ",", "der", "Hahn", "der", "kr\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVFIN", "$,", "ART", "NN", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die rote Flamme loht,", "tokens": ["Die", "ro\u00b7te", "Flam\u00b7me", "loht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Tod uns treu zur Seite steht,", "tokens": ["Der", "Tod", "uns", "treu", "zur", "Sei\u00b7te", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und auch die schwere Not;", "tokens": ["Und", "auch", "die", "schwe\u00b7re", "Not", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein falsches Wort geht um im Land,", "tokens": ["Ein", "fal\u00b7sches", "Wort", "geht", "um", "im", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So mancher, welcher zu ihm stand,", "tokens": ["So", "man\u00b7cher", ",", "wel\u00b7cher", "zu", "ihm", "stand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den fra\u00df der bitt're Tod.", "tokens": ["Den", "fra\u00df", "der", "bitt'\u00b7re", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Tilly ist von Leibe klein,", "tokens": ["Der", "Til\u00b7ly", "ist", "von", "Lei\u00b7be", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Schwert ist meilenlang,", "tokens": ["Sein", "Schwert", "ist", "mei\u00b7len\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn es blitzt, dann schl\u00e4gt es ein,", "tokens": ["Und", "wenn", "es", "blitzt", ",", "dann", "schl\u00e4gt", "es", "ein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann setzt es Brand und Stank;", "tokens": ["Dann", "setzt", "es", "Brand", "und", "Stank", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hinunter mu\u00df die L\u00fcgenbrut,", "tokens": ["Hin\u00b7un\u00b7ter", "mu\u00df", "die", "L\u00fc\u00b7gen\u00b7brut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was einer gegen diese tut,", "tokens": ["Was", "ei\u00b7ner", "ge\u00b7gen", "die\u00b7se", "tut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Herrgott wei\u00df ihm Dank.", "tokens": ["Der", "Herr\u00b7gott", "wei\u00df", "ihm", "Dank", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Das Liedlein ist zu End' gebracht,", "tokens": ["Das", "Lied\u00b7lein", "ist", "zu", "End'", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und der's gesungen hat,", "tokens": ["Und", "der's", "ge\u00b7sun\u00b7gen", "hat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der hat der Beute viel gemacht", "tokens": ["Der", "hat", "der", "Beu\u00b7te", "viel", "ge\u00b7macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und trank an Wein sich satt;", "tokens": ["Und", "trank", "an", "Wein", "sich", "satt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PRF", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er nennt sich T\u00f6nnes Tielemann", "tokens": ["Er", "nennt", "sich", "T\u00f6n\u00b7nes", "Tie\u00b7le\u00b7mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und steckte drei\u00dfig D\u00f6rfer an,", "tokens": ["Und", "steck\u00b7te", "drei\u00b7\u00dfig", "D\u00f6r\u00b7fer", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "CARD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Des wurde er nicht matt.", "tokens": ["Des", "wur\u00b7de", "er", "nicht", "matt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VAFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}