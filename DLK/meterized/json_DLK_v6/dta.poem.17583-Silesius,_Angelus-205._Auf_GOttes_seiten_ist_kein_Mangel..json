{"dta.poem.17583": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "205.  Auf GOttes seiten ist kein Mangel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Gott wirkt ohn unterla\u00df: Er g\u00f6sse tausend Freuden", "tokens": ["Gott", "wirkt", "ohn", "un\u00b7ter\u00b7la\u00df", ":", "Er", "g\u00f6s\u00b7se", "tau\u00b7send", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "NN", "$.", "PPER", "VVFIN", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In dich auf einmal ein/ wo du Jhn k\u00f6ntest leyden.", "tokens": ["In", "dich", "auf", "ein\u00b7mal", "ein", "/", "wo", "du", "Jhn", "k\u00f6n\u00b7test", "ley\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "APPR", "ADV", "ART", "$(", "PWAV", "PPER", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}}}}}