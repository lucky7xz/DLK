{"textgrid.poem.32337": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "14. An die Herren X und Y", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welch Feuer mu\u00df in eurem Busen lodern!", "tokens": ["Welch", "Feu\u00b7er", "mu\u00df", "in", "eu\u00b7rem", "Bu\u00b7sen", "lo\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr habt den Mut, euch k\u00fchn herauszufodern.", "tokens": ["Ihr", "habt", "den", "Mut", ",", "euch", "k\u00fchn", "her\u00b7aus\u00b7zu\u00b7fo\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch eure Klugheit h\u00e4lt dem Mute das Gewicht:", "tokens": ["Doch", "eu\u00b7re", "Klug\u00b7heit", "h\u00e4lt", "dem", "Mu\u00b7te", "das", "Ge\u00b7wicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ihr fodert euch, und stellt euch nicht.", "tokens": ["Ihr", "fo\u00b7dert", "euch", ",", "und", "stellt", "euch", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}