{"dta.poem.984": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "K. W.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Wir seligen! die gottentsandten sprecher                 ", "tokens": ["Wir", "se\u00b7li\u00b7gen", "!", "die", "got\u00b7tent\u00b7sand\u00b7ten", "spre\u00b7cher"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur wagen diesen laut &#183; auf deinen f&#228;hren                 ", "tokens": ["Nur", "wa\u00b7gen", "die\u00b7sen", "laut", "&#183;", "auf", "dei\u00b7nen", "f", "&#228;", "hren"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word", "XML_entity", "word"], "pos": ["ADV", "VVFIN", "PDAT", "ADJD", "$(", "APPR", "PPOSAT", "NN", "$(", "VVINF"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Erklang er t&#228;glich aus umkr&#228;nztem becher                 ", "tokens": ["Er\u00b7klang", "er", "t", "&#228;", "glich", "aus", "um\u00b7kr", "&#228;", "nz\u00b7tem", "be\u00b7cher"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN", "$(", "VVFIN", "APPR", "NE", "$(", "ADJA", "NN"], "meter": "-+--+--++-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und dennoch f&#252;hl ich reue in mir g&#228;hren               ", "tokens": ["Und", "den\u00b7noch", "f", "&#252;", "hl", "ich", "reu\u00b7e", "in", "mir", "g", "&#228;", "hren"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["KON", "ADV", "VVFIN", "$(", "VVFIN", "PPER", "VVFIN", "APPR", "PPER", "ADJD", "$(", "VVINF"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Dein leben ehrend muss ich es vermeiden                 ", "tokens": ["Dein", "le\u00b7ben", "eh\u00b7rend", "muss", "ich", "es", "ver\u00b7mei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "VVFIN", "ADJD", "VMFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein l&#228;cheln und das gl&#252;ck (f&#252;r dich das wahre)                 ", "tokens": ["Dein", "l", "&#228;", "cheln", "und", "das", "gl", "&#252;", "ck", "(", "f", "&#252;", "r", "dich", "das", "wah\u00b7re", ")"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "XML_entity", "word", "punct", "word", "XML_entity", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "VVINF", "KON", "ART", "NN", "$(", "NE", "$(", "NE", "$(", "VVIMP", "PPER", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich muss zur&#252;ck auf meere dumpfer leiden                 ", "tokens": ["Ich", "muss", "zur", "&#252;", "ck", "auf", "mee\u00b7re", "dum\u00b7pfer", "lei\u00b7den"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPRART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In meine wunderbaren wehmutjahre.               ", "tokens": ["In", "mei\u00b7ne", "wun\u00b7der\u00b7ba\u00b7ren", "we\u00b7hmut\u00b7jah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}