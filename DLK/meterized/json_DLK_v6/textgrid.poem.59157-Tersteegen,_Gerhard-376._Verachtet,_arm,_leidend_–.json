{"textgrid.poem.59157": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "376. Verachtet, arm, leidend \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kommt, Kinder, die ihr seid verlassen,", "tokens": ["Kommt", ",", "Kin\u00b7der", ",", "die", "ihr", "seid", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "PRELS", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Veracht't und arm, in Kreuz und Pein!", "tokens": ["Ver\u00b7acht't", "und", "arm", ",", "in", "Kreuz", "und", "Pein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich lasse mich so gern umfassen", "tokens": ["Ich", "las\u00b7se", "mich", "so", "gern", "um\u00b7fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von solchen, die mir \u00e4hnlich sein.", "tokens": ["Von", "sol\u00b7chen", ",", "die", "mir", "\u00e4hn\u00b7lich", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "$,", "PRELS", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}