{"dta.poem.10697": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Eines Kochs.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Wie wird die Welt doch vberal verkehret/", "tokens": ["Wie", "wird", "die", "Welt", "doch", "vbe\u00b7ral", "ver\u00b7keh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hie hat ein Koch im grabe seine ruh/", "tokens": ["Hie", "hat", "ein", "Koch", "im", "gra\u00b7be", "sei\u00b7ne", "ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NE", "APPRART", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dermancherley von Speissen richtet zu/", "tokens": ["Der\u00b7man\u00b7cher\u00b7ley", "von", "Speis\u00b7sen", "rich\u00b7tet", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "PTKZU", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Jetzt haben jhn die W\u00fcrme roh verzehret.", "tokens": ["Jetzt", "ha\u00b7ben", "jhn", "die", "W\u00fcr\u00b7me", "roh", "ver\u00b7zeh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}