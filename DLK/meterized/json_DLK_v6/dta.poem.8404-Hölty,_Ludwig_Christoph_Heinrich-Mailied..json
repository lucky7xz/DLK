{"dta.poem.8404": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Mailied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Der, in seiner Herlichkeit", "tokens": ["Der", ",", "in", "sei\u00b7ner", "Her\u00b7lich\u00b7keit"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wiederkehrend, Reiz und Segen", "tokens": ["Wie\u00b7der\u00b7keh\u00b7rend", ",", "Reiz", "und", "Se\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ueber Thal und H\u00fcgel streut!", "tokens": ["Ue\u00b7ber", "Thal", "und", "H\u00fc\u00b7gel", "streut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine Macht verj\u00fcngt und gattet", "tokens": ["Sei\u00b7ne", "Macht", "ver\u00b7j\u00fcngt", "und", "gat\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVPP", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles, was der gr\u00fcne Wald,", "tokens": ["Al\u00b7les", ",", "was", "der", "gr\u00fc\u00b7ne", "Wald", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Was der zarte Halm beschattet,", "tokens": ["Was", "der", "zar\u00b7te", "Halm", "be\u00b7schat\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die laue Wog' umwallt.", "tokens": ["Und", "die", "lau\u00b7e", "Wo\u00b7g'", "um\u00b7wallt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Tanz, o J\u00fcngling, tanz, o Sch\u00f6ne,", "tokens": ["Tanz", ",", "o", "J\u00fcng\u00b7ling", ",", "tanz", ",", "o", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$,", "VVFIN", "$,", "FM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die des Maies Hauch versch\u00f6nt!", "tokens": ["Die", "des", "Mai\u00b7es", "Hauch", "ver\u00b7sch\u00f6nt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Menget Lieder ins Get\u00f6ne,", "tokens": ["Men\u00b7get", "Lie\u00b7der", "ins", "Ge\u00b7t\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das die Morgenklocke t\u00f6nt,", "tokens": ["Das", "die", "Mor\u00b7gen\u00b7klo\u00b7cke", "t\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ins Ges\u00e4usel junger Bl\u00e4tter,", "tokens": ["Ins", "Ge\u00b7s\u00e4u\u00b7sel", "jun\u00b7ger", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und der holden Nachtigall", "tokens": ["Und", "der", "hol\u00b7den", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Liebejauchzendes Geschmetter;", "tokens": ["Lie\u00b7be\u00b7jauch\u00b7zen\u00b7des", "Ge\u00b7schmet\u00b7ter", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und erweckt den Wiederhall.", "tokens": ["Und", "er\u00b7weckt", "den", "Wie\u00b7der\u00b7hall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Flieht der Stadt umw\u00f6lkte Zinnen!", "tokens": ["Flieht", "der", "Stadt", "um\u00b7w\u00f6lk\u00b7te", "Zin\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier, wo Mai und Lieb' euch ruft,", "tokens": ["Hier", ",", "wo", "Mai", "und", "Lieb'", "euch", "ruft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "NN", "KON", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Athmet, sch\u00f6ne St\u00e4dterinnen,", "tokens": ["Ath\u00b7met", ",", "sch\u00f6\u00b7ne", "St\u00e4d\u00b7te\u00b7rin\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Athmet frische Maienluft!", "tokens": ["Ath\u00b7met", "fri\u00b7sche", "Mai\u00b7en\u00b7luft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Irrt mit eurem Sonnenh\u00fctchen,", "tokens": ["Irrt", "mit", "eu\u00b7rem", "Son\u00b7nen\u00b7h\u00fct\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf die Fr\u00fchlingsflur hinaus,", "tokens": ["Auf", "die", "Fr\u00fch\u00b7lings\u00b7flur", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APZR", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Singt ein fr\u00f6hlich Maienliedchen,", "tokens": ["Singt", "ein", "fr\u00f6h\u00b7lich", "Mai\u00b7en\u00b7lied\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Pfl\u00fccket einen Busenstrauss!", "tokens": ["Pfl\u00fc\u00b7cket", "ei\u00b7nen", "Bu\u00b7sen\u00b7strauss", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Schm\u00fcckt mit Kirschenbl\u00fctenzweigen", "tokens": ["Schm\u00fcckt", "mit", "Kir\u00b7schen\u00b7bl\u00fc\u00b7ten\u00b7zwei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Euch den gr\u00fcnen Sonnenhut,", "tokens": ["Euch", "den", "gr\u00fc\u00b7nen", "Son\u00b7nen\u00b7hut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00fcrzt das R\u00f6ckchen, tanzet Reigen.", "tokens": ["Sch\u00fcrzt", "das", "R\u00f6ck\u00b7chen", ",", "tan\u00b7zet", "Rei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie die Sch\u00e4ferjugend thut!", "tokens": ["Wie", "die", "Sch\u00e4\u00b7fer\u00b7ju\u00b7gend", "thut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bienen sumsen um die Bl\u00fcte,", "tokens": ["Bie\u00b7nen", "sum\u00b7sen", "um", "die", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und der Westwind schw\u00e4rmt sich matt,", "tokens": ["Und", "der", "West\u00b7wind", "schw\u00e4rmt", "sich", "matt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "ADJD", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Schw\u00e4rmt, und haucht auf eure H\u00fcte", "tokens": ["Schw\u00e4rmt", ",", "und", "haucht", "auf", "eu\u00b7re", "H\u00fc\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Manches weisse Bl\u00fctenblatt.", "tokens": ["Man\u00b7ches", "weis\u00b7se", "Bl\u00fc\u00b7ten\u00b7blatt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}