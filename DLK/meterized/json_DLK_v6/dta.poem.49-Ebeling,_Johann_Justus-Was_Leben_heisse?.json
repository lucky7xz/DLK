{"dta.poem.49": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Was Leben heisse?", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht alle leben auf der Erden,", "tokens": ["Nicht", "al\u00b7le", "le\u00b7ben", "auf", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die in die Welt gebohren werden,", "tokens": ["Die", "in", "die", "Welt", "ge\u00b7boh\u00b7ren", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die auf derselben Schauplaz stehn,", "tokens": ["Die", "auf", "der\u00b7sel\u00b7ben", "Schau\u00b7plaz", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wiederum von hinnen gehn.", "tokens": ["Und", "wie\u00b7de\u00b7rum", "von", "hin\u00b7nen", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Ausspruch, wird wol mancher", "tokens": ["Der", "Aus\u00b7spruch", ",", "wird", "wol", "man\u00b7cher"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VAFIN", "ADV", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ist ungegr\u00fcndet, zu verneinen.", "tokens": ["Ist", "un\u00b7ge\u00b7gr\u00fcn\u00b7det", ",", "zu", "ver\u00b7nei\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Allein, die ihr also urtheilt", "tokens": ["Al\u00b7lein", ",", "die", "ihr", "al\u00b7so", "ur\u00b7theilt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PRELS", "PPER", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Jhr schliesset warlich \u00fcbereilt.", "tokens": ["Ihr", "schlies\u00b7set", "war\u00b7lich", "\u00fc\u00b7be\u00b7reilt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ich will euch von den Menschen Leben,", "tokens": ["Ich", "will", "euch", "von", "den", "Men\u00b7schen", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die richtige Beschreibung geben:", "tokens": ["Die", "rich\u00b7ti\u00b7ge", "Be\u00b7schrei\u00b7bung", "ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Der lebet wer sich auf der Welt,", "tokens": ["Der", "le\u00b7bet", "wer", "sich", "auf", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PWS", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "So wie sein Sch\u00f6pfer will, verh\u00e4lt,", "tokens": ["So", "wie", "sein", "Sch\u00f6p\u00b7fer", "will", ",", "ver\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "KOKOM", "PPOSAT", "NN", "VMFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und die ihm aufgetragnen Pflichten,", "tokens": ["Und", "die", "ihm", "auf\u00b7ge\u00b7trag\u00b7nen", "Pflich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "In Em\u00dfiakeit sucht zu verrichten.", "tokens": ["In", "E\u00b7m\u00dfia\u00b7keit", "sucht", "zu", "ver\u00b7rich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Wenn man des Sch\u00f6pfers Zwek bedenkt,", "tokens": ["Wenn", "man", "des", "Sch\u00f6p\u00b7fers", "Zwek", "be\u00b7denkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "So ist das Leben uns geschenkt,", "tokens": ["So", "ist", "das", "Le\u00b7ben", "uns", "ge\u00b7schenkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Da\u00df wir ihn auf der Welt betrachten,", "tokens": ["Da\u00df", "wir", "ihn", "auf", "der", "Welt", "be\u00b7trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Und in Erkenntnis herrlich achten,", "tokens": ["Und", "in", "Er\u00b7kennt\u00b7nis", "herr\u00b7lich", "ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Wir stehen im Gesellschafts-Band", "tokens": ["Wir", "ste\u00b7hen", "im", "Ge\u00b7sell\u00b7schafts\u00b7Band"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und jeder mu\u00df nach seinem Stand,", "tokens": ["Und", "je\u00b7der", "mu\u00df", "nach", "sei\u00b7nem", "Stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Den Neben-Menschen redlich lieben,", "tokens": ["Den", "Ne\u00b7ben\u00b7Men\u00b7schen", "red\u00b7lich", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Und sich im wahren Guten \u00fcben.", "tokens": ["Und", "sich", "im", "wah\u00b7ren", "Gu\u00b7ten", "\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Wir leben wenn wir uns bem\u00fchn,", "tokens": ["Wir", "le\u00b7ben", "wenn", "wir", "uns", "be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Den Hindernissen zu entfliehn,", "tokens": ["Den", "Hin\u00b7der\u00b7nis\u00b7sen", "zu", "ent\u00b7fliehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Die uns allhie im Wege stehen,", "tokens": ["Die", "uns", "all\u00b7hie", "im", "We\u00b7ge", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Zum ewgen Leben einzugehen.", "tokens": ["Zum", "ew\u00b7gen", "Le\u00b7ben", "ein\u00b7zu\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Bedenket dies und saget mir,", "tokens": ["Be\u00b7den\u00b7ket", "dies", "und", "sa\u00b7get", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Wie viele sind im Welt-Revier,", "tokens": ["Wie", "vie\u00b7le", "sind", "im", "Welt\u00b7Re\u00b7vier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Die keine Lebenspflicht beweisen,", "tokens": ["Die", "kei\u00b7ne", "Le\u00b7bens\u00b7pflicht", "be\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Und als umsonst von dannen reisen.", "tokens": ["Und", "als", "um\u00b7sonst", "von", "dan\u00b7nen", "rei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Sie sehen diese Welt nur an,", "tokens": ["Sie", "se\u00b7hen", "die\u00b7se", "Welt", "nur", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Und haben darin nichts gethan:", "tokens": ["Und", "ha\u00b7ben", "da\u00b7rin", "nichts", "ge\u00b7than", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PAV", "PIS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Drum wird das eine Warheit bleiben,", "tokens": ["Drum", "wird", "das", "ei\u00b7ne", "War\u00b7heit", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PDS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Man kan auf ihre Gr\u00e4ber schreiben:", "tokens": ["Man", "kan", "auf", "ih\u00b7re", "Gr\u00e4\u00b7ber", "schrei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Seht Leser! todte Menschen an,", "tokens": ["Seht", "Le\u00b7ser", "!", "tod\u00b7te", "Men\u00b7schen", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$.", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Die wenig auf der Welt gethan,", "tokens": ["Die", "we\u00b7nig", "auf", "der", "Welt", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Und nichts mehr k\u00f6nt ihr allhie lesen,", "tokens": ["Und", "nichts", "mehr", "k\u00f6nt", "ihr", "all\u00b7hie", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Als da\u00df sie lange da gewesen.", "tokens": ["Als", "da\u00df", "sie", "lan\u00b7ge", "da", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "ADV", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Von der Geburt bis an die Bahr,", "tokens": ["Von", "der", "Ge\u00b7burt", "bis", "an", "die", "Bahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Sind sechszig, siebzig, achzig Jahr", "tokens": ["Sind", "sechs\u00b7zig", ",", "sieb\u00b7zig", ",", "ach\u00b7zig", "Jahr"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VAFIN", "CARD", "$,", "ADJD", "$,", "CARD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "In ihren Lauffe fast verstrichen;", "tokens": ["In", "ih\u00b7ren", "Lauf\u00b7fe", "fast", "ver\u00b7stri\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Und da sie wiederum verblichen,", "tokens": ["Und", "da", "sie", "wie\u00b7de\u00b7rum", "ver\u00b7bli\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "So ist wenn man gleich rechnet viel,", "tokens": ["So", "ist", "wenn", "man", "gleich", "rech\u00b7net", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "KOUS", "PIS", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Ein Tag ihr ganzes Lebens-Ziel.", "tokens": ["Ein", "Tag", "ihr", "gan\u00b7zes", "Le\u00b7bens\u00b7Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}