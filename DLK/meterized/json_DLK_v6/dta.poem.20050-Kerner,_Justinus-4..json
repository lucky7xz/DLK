{"dta.poem.20050": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bin ich wie ein Kind, das seine Mutter", "tokens": ["Bin", "ich", "wie", "ein", "Kind", ",", "das", "sei\u00b7ne", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "KOKOM", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Erst verloren, weinend in der Nacht steht:", "tokens": ["Erst", "ver\u00b7lo\u00b7ren", ",", "wei\u00b7nend", "in", "der", "Nacht", "steht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "VVPP", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sieh! so bin ich seit ich fern gezogen.", "tokens": ["Sieh", "!", "so", "bin", "ich", "seit", "ich", "fern", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VAFIN", "PPER", "APPR", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Stund im Traum' ich heut' auf unsrem Berge,", "tokens": ["Stund", "im", "Traum'", "ich", "heut'", "auf", "uns\u00b7rem", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Blick' ich in das tiefe Thal hernieder.", "tokens": ["Blick'", "ich", "in", "das", "tie\u00b7fe", "Thal", "her\u00b7nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Such' dein Haus ich, aber find' es nimmer.", "tokens": ["Such'", "dein", "Haus", "ich", ",", "a\u00b7ber", "find'", "es", "nim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "$,", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Seh' ich eine einsame Kapelle", "tokens": ["Seh'", "ich", "ei\u00b7ne", "ein\u00b7sa\u00b7me", "Ka\u00b7pel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Auf der Stelle, wo's gestanden, stehen,", "tokens": ["Auf", "der", "Stel\u00b7le", ",", "wo's", "ge\u00b7stan\u00b7den", ",", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "VVPP", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Tret' ich in die heilige Kapelle.", "tokens": ["Tret'", "ich", "in", "die", "hei\u00b7li\u00b7ge", "Ka\u00b7pel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Hallet lange jeder meiner Tritte", "tokens": ["Hal\u00b7let", "lan\u00b7ge", "je\u00b7der", "mei\u00b7ner", "Trit\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "PIAT", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Im verlassenen Gew\u00f6lbe wieder;", "tokens": ["Im", "ver\u00b7las\u00b7se\u00b7nen", "Ge\u00b7w\u00f6l\u00b7be", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Blicken ernst und fragend mich die heil'gen", "tokens": ["Bli\u00b7cken", "ernst", "und", "fra\u00b7gend", "mich", "die", "heil'\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "KON", "ADJD", "PPER", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Bilder an von den geweihten W\u00e4nden.", "tokens": ["Bil\u00b7der", "an", "von", "den", "ge\u00b7weih\u00b7ten", "W\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Tret' ich vor den Hochaltar, zu beten.", "tokens": ["Tret'", "ich", "vor", "den", "Hoc\u00b7hal\u00b7tar", ",", "zu", "be\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Knieest du in einem wei\u00dfen Kleide", "tokens": ["Kni\u00b7eest", "du", "in", "ei\u00b7nem", "wei\u00b7\u00dfen", "Klei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Bleich auf schwarzem Teppich vor'm Altare,", "tokens": ["Bleich", "auf", "schwar\u00b7zem", "Tep\u00b7pich", "vor'm", "Al\u00b7ta\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Lilien und Tulpen um dich her.", "tokens": ["Li\u00b7li\u00b7en", "und", "Tul\u00b7pen", "um", "dich", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Steht der Rosenstock zu deinen F\u00fc\u00dfen,", "tokens": ["Steht", "der", "Ro\u00b7sen\u00b7stock", "zu", "dei\u00b7nen", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Bl\u00fcthenreich vom Lorbeer sch\u00f6n umwunden,", "tokens": ["Bl\u00fc\u00b7then\u00b7reich", "vom", "Lor\u00b7beer", "sch\u00f6n", "um\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Kehr' ich nie aus der Kapelle wieder.", "tokens": ["Kehr'", "ich", "nie", "aus", "der", "Ka\u00b7pel\u00b7le", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}