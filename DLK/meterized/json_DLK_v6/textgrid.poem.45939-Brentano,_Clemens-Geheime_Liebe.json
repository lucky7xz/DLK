{"textgrid.poem.45939": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Geheime Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1811, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unbegl\u00fcckt mu\u00df ich durchs Leben gehen,", "tokens": ["Un\u00b7be\u00b7gl\u00fcckt", "mu\u00df", "ich", "durchs", "Le\u00b7ben", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Meine Rechte sind nicht anerkannt;", "tokens": ["Mei\u00b7ne", "Rech\u00b7te", "sind", "nicht", "an\u00b7er\u00b7kannt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Aus der Liebe sch\u00f6nem Reich verbannt,", "tokens": ["Aus", "der", "Lie\u00b7be", "sch\u00f6\u00b7nem", "Reich", "ver\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Mu\u00df ich dennoch stets ihr Sch\u00f6nstes sehen!", "tokens": ["Mu\u00df", "ich", "den\u00b7noch", "stets", "ihr", "Sch\u00f6ns\u00b7tes", "se\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Nicht die schwache Zunge darf's gestehen,", "tokens": ["Nicht", "die", "schwa\u00b7che", "Zun\u00b7ge", "dar\u00b7f's", "ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "PAV", "VVPP", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Nicht der Blick verstohlen zugesandt,", "tokens": ["Nicht", "der", "Blick", "ver\u00b7stoh\u00b7len", "zu\u00b7ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VVINF", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Was sich eigen hat das Herz ernannt,", "tokens": ["Was", "sich", "ei\u00b7gen", "hat", "das", "Herz", "er\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ADJD", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Nicht im Seufzer darf's der Brust entwehen!", "tokens": ["Nicht", "im", "Seuf\u00b7zer", "dar\u00b7f's", "der", "Brust", "ent\u00b7we\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "PAV", "ART", "NN", "VVINF", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Tr\u00f6stung such' ich bei der fremden Nacht,", "tokens": ["Tr\u00f6s\u00b7tung", "such'", "ich", "bei", "der", "frem\u00b7den", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Wenn der leere lange Tag vergangen,", "tokens": ["Wenn", "der", "lee\u00b7re", "lan\u00b7ge", "Tag", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ihr vertrau' ich mein geheim Verlangen;", "tokens": ["Ihr", "ver\u00b7trau'", "ich", "mein", "ge\u00b7heim", "Ver\u00b7lan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ist in Tr\u00e4nen meine Nacht durchwacht,", "tokens": ["Ist", "in", "Tr\u00e4\u00b7nen", "mei\u00b7ne", "Nacht", "durch\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Und der lange leere Tag kommt wieder,", "tokens": ["Und", "der", "lan\u00b7ge", "lee\u00b7re", "Tag", "kommt", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Still ins Herz steigt meine Liebe nieder.", "tokens": ["Still", "ins", "Herz", "steigt", "mei\u00b7ne", "Lie\u00b7be", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}