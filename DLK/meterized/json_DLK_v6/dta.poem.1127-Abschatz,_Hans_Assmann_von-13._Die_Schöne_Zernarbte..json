{"dta.poem.1127": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "13. Die Sch\u00f6ne Zernarbte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4r' auch ohn Unterscheid und Theilung sch\u00f6n zu nennen ", "tokens": ["W\u00e4r'", "auch", "ohn", "Un\u00b7ter\u00b7scheid", "und", "Thei\u00b7lung", "sch\u00f6n", "zu", "nen\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "NN", "KON", "NN", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der blau-gew\u00f6lbten Last geraumes Wunder-Feld?", "tokens": ["Der", "blau\u00b7ge\u00b7w\u00f6lb\u00b7ten", "Last", "ge\u00b7rau\u00b7mes", "Wun\u00b7der\u00b7Feld", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und w\u00fcrde Phebus nicht in steter Irre rennen/", "tokens": ["Und", "w\u00fcr\u00b7de", "Phe\u00b7bus", "nicht", "in", "ste\u00b7ter", "Ir\u00b7re", "ren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NE", "PTKNEG", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Im fall ihm keine Bahn und Strasse vorgestellt?", "tokens": ["Im", "fall", "ihm", "kei\u00b7ne", "Bahn", "und", "Stras\u00b7se", "vor\u00b7ge\u00b7stellt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PIAT", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Des Himmels Antlitz l\u00e4st sich eine Narbe trennen/", "tokens": ["Des", "Him\u00b7mels", "Ant\u00b7litz", "l\u00e4st", "sich", "ei\u00b7ne", "Nar\u00b7be", "tren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PRF", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Tag und Nacht/ die Hitz und Frost in Wage h\u00e4lt/", "tokens": ["Die", "Tag", "und", "Nacht", "/", "die", "Hitz", "und", "Frost", "in", "Wa\u00b7ge", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$(", "ART", "NN", "KON", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Sonnen-Lauff umbschr\u00e4nckt/ und ieder mu\u00df bekennen/", "tokens": ["Der", "Son\u00b7nen\u00b7Lauff", "umbsc\u00b7hr\u00e4nckt", "/", "und", "ie\u00b7der", "mu\u00df", "be\u00b7ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$(", "KON", "PIS", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Auff diesem Striche ruh der Wohlstand aller Welt.", "tokens": ["Auff", "die\u00b7sem", "Stri\u00b7che", "ruh", "der", "Wohl\u00b7stand", "al\u00b7ler", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "In G\u00e4rten liebet man der Felder Unterscheid:", "tokens": ["In", "G\u00e4r\u00b7ten", "lie\u00b7bet", "man", "der", "Fel\u00b7der", "Un\u00b7ter\u00b7scheid", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIS", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ein unbefurchtes Land ist ohne Fruchtbarkeit;", "tokens": ["Ein", "un\u00b7be\u00b7furch\u00b7tes", "Land", "ist", "oh\u00b7ne", "Frucht\u00b7bar\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ein unzertheiltes Haar der Wildni\u00df zu vergleichen:", "tokens": ["Ein", "un\u00b7zer\u00b7theil\u00b7tes", "Haar", "der", "Wild\u00b7ni\u00df", "zu", "ver\u00b7glei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Drum f\u00fchrestu mit Recht/ als einen Pol/ di\u00df Zeichen.", "tokens": ["Drum", "f\u00fch\u00b7re\u00b7stu", "mit", "Recht", "/", "als", "ei\u00b7nen", "Pol", "/", "di\u00df", "Zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "NN", "$(", "KOUS", "ART", "NN", "$(", "PDS", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ja weil dein Angesicht hegt zweyer Sonnen Schein/", "tokens": ["Ja", "weil", "dein", "An\u00b7ge\u00b7sicht", "hegt", "zwey\u00b7er", "Son\u00b7nen", "Schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "KOUS", "PPOSAT", "NN", "VVFIN", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So solten billich auch zwey Strassen drinnen seyn.", "tokens": ["So", "sol\u00b7ten", "bil\u00b7lich", "auch", "zwey", "Stras\u00b7sen", "drin\u00b7nen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADJD", "ADV", "CARD", "NN", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}