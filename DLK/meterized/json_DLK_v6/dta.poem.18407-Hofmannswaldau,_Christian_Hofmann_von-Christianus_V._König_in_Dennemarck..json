{"dta.poem.18407": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Christianus  V.  K\u00f6nig in Dennemarck.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du siehst/ wie Hercules/ anitzo unter zweyen/", "tokens": ["Du", "siehst", "/", "wie", "Her\u00b7cu\u00b7les", "/", "a\u00b7nit\u00b7zo", "un\u00b7ter", "zwe\u00b7yen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOKOM", "NN", "$(", "ADV", "APPR", "VVFIN", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Es lockt dich Leopold und Franckreichs schmeicheleyen;", "tokens": ["Es", "lockt", "dich", "Leo\u00b7pold", "und", "Fran\u00b7ck\u00b7reichs", "schmei\u00b7che\u00b7le\u00b7yen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NE", "KON", "NN", "VVINF", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Doch l\u00e4st du diese stehn/ und folgest jener bahn/", "tokens": ["Doch", "l\u00e4st", "du", "die\u00b7se", "stehn", "/", "und", "fol\u00b7gest", "je\u00b7ner", "bahn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PDS", "VVINF", "$(", "KON", "VVFIN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So bast du hier so viel als Hercules gethan.", "tokens": ["So", "bast", "du", "hier", "so", "viel", "als", "Her\u00b7cu\u00b7les", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ADV", "KOUS", "NN