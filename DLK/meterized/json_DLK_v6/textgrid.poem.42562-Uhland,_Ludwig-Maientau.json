{"textgrid.poem.42562": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Maientau", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf den Wald und auf die Wiese,", "tokens": ["Auf", "den", "Wald", "und", "auf", "die", "Wie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit dem ersten Morgengrau,", "tokens": ["Mit", "dem", "ers\u00b7ten", "Mor\u00b7gen\u00b7grau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tr\u00e4uft ein Quell vom Paradiese,", "tokens": ["Tr\u00e4uft", "ein", "Quell", "vom", "Pa\u00b7ra\u00b7die\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leiser, frischer Maientau;", "tokens": ["Lei\u00b7ser", ",", "fri\u00b7scher", "Mai\u00b7en\u00b7tau", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was den Mai zum Heiligtume", "tokens": ["Was", "den", "Mai", "zum", "Hei\u00b7lig\u00b7tu\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jeder s\u00fc\u00dfen Wonne schafft,", "tokens": ["Je\u00b7der", "s\u00fc\u00b7\u00dfen", "Won\u00b7ne", "schafft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schmelz der Bl\u00e4tter, Glanz der Blume,", "tokens": ["Schmelz", "der", "Bl\u00e4t\u00b7ter", ",", "Glanz", "der", "Blu\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "W\u00fcrz und Duft, ist seine Kraft.", "tokens": ["W\u00fcrz", "und", "Duft", ",", "ist", "sei\u00b7ne", "Kraft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn den Tau die Muschel trinket,", "tokens": ["Wenn", "den", "Tau", "die", "Mu\u00b7schel", "trin\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird in ihr ein Perlenstrau\u00df;", "tokens": ["Wird", "in", "ihr", "ein", "Per\u00b7len\u00b7strau\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn er in den Eichstamm sinket,", "tokens": ["Wenn", "er", "in", "den", "Eichs\u00b7tamm", "sin\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werden Honigbienen draus;", "tokens": ["Wer\u00b7den", "Ho\u00b7nig\u00b7bie\u00b7nen", "draus", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn der Vogel auf dem Reise", "tokens": ["Wenn", "der", "Vo\u00b7gel", "auf", "dem", "Rei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kaum damit den Schnabel netzt,", "tokens": ["Kaum", "da\u00b7mit", "den", "Schna\u00b7bel", "netzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Lernet er die helle Weise,", "tokens": ["Ler\u00b7net", "er", "die", "hel\u00b7le", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die den ernsten Wald ergetzt.", "tokens": ["Die", "den", "erns\u00b7ten", "Wald", "er\u00b7getzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Mit dem Tau der Maienglocken", "tokens": ["Mit", "dem", "Tau", "der", "Mai\u00b7en\u00b7glo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wascht die Jungfrau ihr Gesicht,", "tokens": ["Wascht", "die", "Jung\u00b7frau", "ihr", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Badet sie die goldnen Locken,", "tokens": ["Ba\u00b7det", "sie", "die", "gold\u00b7nen", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sie gl\u00e4nzt von Himmelslicht;", "tokens": ["Und", "sie", "gl\u00e4nzt", "von", "Him\u00b7mels\u00b7licht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Selbst ein Auge, rot geweinet,", "tokens": ["Selbst", "ein", "Au\u00b7ge", ",", "rot", "ge\u00b7wei\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Labt sich mit den Tropfen gern,", "tokens": ["Labt", "sich", "mit", "den", "Trop\u00b7fen", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Bis ihm freundlich niederscheinet,", "tokens": ["Bis", "ihm", "freund\u00b7lich", "nie\u00b7der\u00b7schei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Taugetr\u00e4nkt, der Morgenstern.", "tokens": ["Tau\u00b7ge\u00b7tr\u00e4nkt", ",", "der", "Mor\u00b7gens\u00b7tern", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.4": {"line.1": {"text": "Sink denn auch auf mich hernieder,", "tokens": ["Sink", "denn", "auch", "auf", "mich", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADV", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Balsam du f\u00fcr jeden Schmerz!", "tokens": ["Bal\u00b7sam", "du", "f\u00fcr", "je\u00b7den", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Netz auch mir die Augenlider!", "tokens": ["Netz", "auch", "mir", "die", "Au\u00b7gen\u00b7li\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00e4nke mir mein d\u00fcrstend Herz!", "tokens": ["Tr\u00e4n\u00b7ke", "mir", "mein", "d\u00fcrs\u00b7tend", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gib mir Jugend, Sangeswonne,", "tokens": ["Gib", "mir", "Ju\u00b7gend", ",", "San\u00b7ges\u00b7won\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Himmlischer Gebilde Schau,", "tokens": ["Himm\u00b7li\u00b7scher", "Ge\u00b7bil\u00b7de", "Schau", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "St\u00e4rke mir den Blick zur Sonne,", "tokens": ["St\u00e4r\u00b7ke", "mir", "den", "Blick", "zur", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Leiser, frischer Maientau!", "tokens": ["Lei\u00b7ser", ",", "fri\u00b7scher", "Mai\u00b7en\u00b7tau", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}