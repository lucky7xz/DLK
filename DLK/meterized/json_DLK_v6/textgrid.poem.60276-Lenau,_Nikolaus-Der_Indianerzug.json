{"textgrid.poem.60276": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Der Indianerzug", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wehklage hallt am Susquehannaufer,", "tokens": ["Weh\u00b7kla\u00b7ge", "hallt", "am", "Sus\u00b7que\u00b7han\u00b7nau\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Wandrer f\u00fchlt sie tief sein Herz durchschneiden;", "tokens": ["Der", "Wand\u00b7rer", "f\u00fchlt", "sie", "tief", "sein", "Herz", "durch\u00b7schnei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer sind die lauten, wildbewegten Rufer?", "tokens": ["Wer", "sind", "die", "lau\u00b7ten", ",", "wild\u00b7be\u00b7weg\u00b7ten", "Ru\u00b7fer", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Indianer sinds, die von der Heimat scheiden.", "tokens": ["In\u00b7di\u00b7a\u00b7ner", "sinds", ",", "die", "von", "der", "Hei\u00b7mat", "schei\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "PRELS", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch pl\u00f6tzlich ihre lauten Klagen stocken.", "tokens": ["Doch", "pl\u00f6tz\u00b7lich", "ih\u00b7re", "lau\u00b7ten", "Kla\u00b7gen", "sto\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der H\u00e4uptling naht mit heftig raschem Tritte,", "tokens": ["Der", "H\u00e4upt\u00b7ling", "naht", "mit", "hef\u00b7tig", "ra\u00b7schem", "Trit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Greis von finstern Augen, bleichen Locken,", "tokens": ["Ein", "Greis", "von", "fins\u00b7tern", "Au\u00b7gen", ",", "blei\u00b7chen", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und also t\u00f6nt sein Wort in ihrer Mitte:", "tokens": ["Und", "al\u00b7so", "t\u00f6nt", "sein", "Wort", "in", "ih\u00b7rer", "Mit\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "\u00bbstets weiter dr\u00e4ngen uns, als ihre Herde,", "tokens": ["\u00bb", "stets", "wei\u00b7ter", "dr\u00e4n\u00b7gen", "uns", ",", "als", "ih\u00b7re", "Her\u00b7de", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Stets weiter, weiter die verfluchten Wei\u00dfen,", "tokens": ["Stets", "wei\u00b7ter", ",", "wei\u00b7ter", "die", "ver\u00b7fluch\u00b7ten", "Wei\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die kommen sind, uns von der Muttererde", "tokens": ["Die", "kom\u00b7men", "sind", ",", "uns", "von", "der", "Mut\u00b7ter\u00b7er\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "VVINF", "VAFIN", "$,", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und von den alten G\u00f6ttern fortzurei\u00dfen.", "tokens": ["Und", "von", "den", "al\u00b7ten", "G\u00f6t\u00b7tern", "fort\u00b7zu\u00b7rei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mir ist es klar, ich sehs im Licht der Flamme,", "tokens": ["Mir", "ist", "es", "klar", ",", "ich", "sehs", "im", "Licht", "der", "Flam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$,", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die mir das Herz verbrennt mit wildem Nagen:", "tokens": ["Die", "mir", "das", "Herz", "ver\u00b7brennt", "mit", "wil\u00b7dem", "Na\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie brachten uns das Heil am Kreuzesstamme,", "tokens": ["Sie", "brach\u00b7ten", "uns", "das", "Heil", "am", "Kreu\u00b7zes\u00b7stam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Mut zur Rache an das Kreuz zu schlagen.", "tokens": ["Den", "Mut", "zur", "Ra\u00b7che", "an", "das", "Kreuz", "zu", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Den Wald, wo wir den Kindesschlaf genossen,", "tokens": ["Den", "Wald", ",", "wo", "wir", "den", "Kin\u00b7des\u00b7schlaf", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verlassen wir; der uns sein Wild geboten;", "tokens": ["Ver\u00b7las\u00b7sen", "wir", ";", "der", "uns", "sein", "Wild", "ge\u00b7bo\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "ART", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo liebend wir ein teures Weib umschlossen;", "tokens": ["Wo", "lie\u00b7bend", "wir", "ein", "teu\u00b7res", "Weib", "um\u00b7schlos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Wald, wo wir begraben unsre Toten.", "tokens": ["Den", "Wald", ",", "wo", "wir", "be\u00b7gra\u00b7ben", "uns\u00b7re", "To\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Naht ihr den Gr\u00e4bern euch von euren Ahnen,", "tokens": ["Naht", "ihr", "den", "Gr\u00e4\u00b7bern", "euch", "von", "eu\u00b7ren", "Ah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sei still von euch die H\u00fcgelschar beschlichen,", "tokens": ["Sei", "still", "von", "euch", "die", "H\u00fc\u00b7gel\u00b7schar", "be\u00b7schli\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "APPR", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Toten nicht zu wecken und zu mahnen,", "tokens": ["Die", "To\u00b7ten", "nicht", "zu", "we\u00b7cken", "und", "zu", "mah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df wir von ihrem Glauben sind gewichen.", "tokens": ["Da\u00df", "wir", "von", "ih\u00b7rem", "Glau\u00b7ben", "sind", "ge\u00b7wi\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Der Hohn wird kommen, fr\u00fcher oder sp\u00e4ter,", "tokens": ["Der", "Hohn", "wird", "kom\u00b7men", ",", "fr\u00fc\u00b7her", "o\u00b7der", "sp\u00e4\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der gier'ge Pflug wird in die Gr\u00e4ber dringen;", "tokens": ["Der", "gier'\u00b7ge", "Pflug", "wird", "in", "die", "Gr\u00e4\u00b7ber", "drin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann mu\u00df die heilge Asche unsrer V\u00e4ter", "tokens": ["Dann", "mu\u00df", "die", "heil\u00b7ge", "A\u00b7sche", "uns\u00b7rer", "V\u00e4\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des tiefverha\u00dften Feindes Saaten d\u00fcngen!\u00ab \u2013", "tokens": ["Des", "tief\u00b7ver\u00b7ha\u00df\u00b7ten", "Fein\u00b7des", "Saa\u00b7ten", "d\u00fcn\u00b7gen", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVINF", "$.", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Nun feiern sie der Toten Angedenken;", "tokens": ["Nun", "fei\u00b7ern", "sie", "der", "To\u00b7ten", "An\u00b7ge\u00b7den\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Sonn im Westen wandelt ihre Neige,", "tokens": ["Die", "Sonn", "im", "Wes\u00b7ten", "wan\u00b7delt", "ih\u00b7re", "Nei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Gr\u00e4ber noch bestrahlend, und sie senken", "tokens": ["Die", "Gr\u00e4\u00b7ber", "noch", "be\u00b7strah\u00b7lend", ",", "und", "sie", "sen\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVPP", "$,", "KON", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Viel Tr\u00e4nen drauf und gr\u00fcne Tannenzweige.", "tokens": ["Viel", "Tr\u00e4\u00b7nen", "drauf", "und", "gr\u00fc\u00b7ne", "Tan\u00b7nen\u00b7zwei\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKVZ", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Da bricht die Wehmut pl\u00f6tzlich ihre Hemmung,", "tokens": ["Da", "bricht", "die", "Weh\u00b7mut", "pl\u00f6tz\u00b7lich", "ih\u00b7re", "Hem\u00b7mung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie str\u00f6met laut und lauter in die L\u00fcfte,", "tokens": ["Sie", "str\u00f6\u00b7met", "laut", "und", "lau\u00b7ter", "in", "die", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schon braust des Schmerzes volle \u00dcberschwemmung", "tokens": ["Schon", "braust", "des", "Schmer\u00b7zes", "vol\u00b7le", "\u00dc\u00b7bersc\u00b7hwem\u00b7mung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In wilden Klagen um die stillen Gr\u00fcfte.", "tokens": ["In", "wil\u00b7den", "Kla\u00b7gen", "um", "die", "stil\u00b7len", "Gr\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Nun wenden sich zur Wandrung die Vertriebnen,", "tokens": ["Nun", "wen\u00b7den", "sich", "zur", "Wand\u00b7rung", "die", "Ver\u00b7trieb\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Oft gr\u00fc\u00dfend noch zur\u00fcck mit finsterm Sehnen", "tokens": ["Oft", "gr\u00fc\u00b7\u00dfend", "noch", "zu\u00b7r\u00fcck", "mit", "fins\u00b7term", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADV", "PTKVZ", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die teuren H\u00fcgel der Zur\u00fcckgebliebnen,", "tokens": ["Die", "teu\u00b7ren", "H\u00fc\u00b7gel", "der", "Zu\u00b7r\u00fcck\u00b7ge\u00b7blieb\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bestreuend ihre Bahn mit Fl\u00fcchen, Tr\u00e4nen.", "tokens": ["Be\u00b7streu\u00b7end", "ih\u00b7re", "Bahn", "mit", "Fl\u00fc\u00b7chen", ",", "Tr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "APPR", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Wie sie vor\u00fcberwandern an den B\u00e4umen,", "tokens": ["Wie", "sie", "vor\u00b7\u00fc\u00b7ber\u00b7wan\u00b7dern", "an", "den", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Umarmend viele an die St\u00e4mme fallen,", "tokens": ["Um\u00b7ar\u00b7mend", "vie\u00b7le", "an", "die", "St\u00e4m\u00b7me", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PIS", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zum Scheidegru\u00df den trauten Waldesr\u00e4umen", "tokens": ["Zum", "Schei\u00b7de\u00b7gru\u00df", "den", "trau\u00b7ten", "Wal\u00b7des\u00b7r\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "L\u00e4\u00dft jeder einmal noch die Flinte knallen. \u2013", "tokens": ["L\u00e4\u00dft", "je\u00b7der", "ein\u00b7mal", "noch", "die", "Flin\u00b7te", "knal\u00b7len", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADV", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Der Flintenruf, der Ruf ger\u00fchrter Kehlen", "tokens": ["Der", "Flin\u00b7ten\u00b7ruf", ",", "der", "Ruf", "ge\u00b7r\u00fchr\u00b7ter", "Keh\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist an den H\u00fcgeln allgemach verrauschet,", "tokens": ["Ist", "an", "den", "H\u00fc\u00b7geln", "all\u00b7ge\u00b7mach", "ver\u00b7rau\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"t