{"textgrid.poem.45755": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Luxemburg Frage", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Flickt euer Deutschland nur wieder zusammen,", "tokens": ["Flickt", "eu\u00b7er", "Deutschland", "nur", "wie\u00b7der", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Was n\u00fctzt, von denselben Eltern zu stammen!", "tokens": ["Was", "n\u00fctzt", ",", "von", "den\u00b7sel\u00b7ben", "El\u00b7tern", "zu", "stam\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "APPR", "PDAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Seit eure Bundesverfassung entschlief,", "tokens": ["Seit", "eu\u00b7re", "Bun\u00b7des\u00b7ver\u00b7fas\u00b7sung", "ent\u00b7schlief", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Seid ihr nur ein geographischer Begriff.", "tokens": ["Seid", "ihr", "nur", "ein", "geo\u00b7gra\u00b7phi\u00b7scher", "Be\u00b7griff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}}}}