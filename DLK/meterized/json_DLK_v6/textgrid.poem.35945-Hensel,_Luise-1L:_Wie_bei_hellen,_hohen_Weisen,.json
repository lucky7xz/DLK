{"textgrid.poem.35945": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie bei hellen, hohen Weisen,", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie bei hellen, hohen Weisen,", "tokens": ["Wie", "bei", "hel\u00b7len", ",", "ho\u00b7hen", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo nur Geig' und Fl\u00f6te t\u00f6nen,", "tokens": ["Wo", "nur", "Geig'", "und", "Fl\u00f6\u00b7te", "t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Man sich sehnt nach Cello's Dr\u00f6hnen:", "tokens": ["Man", "sich", "sehnt", "nach", "Cel\u00b7lo's", "Dr\u00f6h\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PRF", "VVFIN", "APPR", "NE", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So zu all den s\u00fc\u00dfen Speisen", "tokens": ["So", "zu", "all", "den", "s\u00fc\u00b7\u00dfen", "Spei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PIAT", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und den lichten, reichen Spenden,", "tokens": ["Und", "den", "lich\u00b7ten", ",", "rei\u00b7chen", "Spen\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dargebracht von zarten H\u00e4nden,", "tokens": ["Dar\u00b7ge\u00b7bracht", "von", "zar\u00b7ten", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Soll mein kr\u00e4ftig Schwarzbrod reisen.", "tokens": ["Soll", "mein", "kr\u00e4f\u00b7tig", "Schwarz\u00b7brod", "rei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJD", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wollt ihm Eure Huld beweisen;", "tokens": ["Wollt", "ihm", "Eu\u00b7re", "Huld", "be\u00b7wei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Kann Euch nimmer Bess'res senden.", "tokens": ["Kann", "Euch", "nim\u00b7mer", "Bess'\u00b7res", "sen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}