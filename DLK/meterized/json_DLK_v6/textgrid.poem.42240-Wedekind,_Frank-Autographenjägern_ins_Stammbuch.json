{"textgrid.poem.42240": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Autographenj\u00e4gern ins Stammbuch", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer der Kunst sich weiht, gilt oft als Misset\u00e4ter,", "tokens": ["Wer", "der", "Kunst", "sich", "weiht", ",", "gilt", "oft", "als", "Mis\u00b7se\u00b7t\u00e4\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PRF", "VVFIN", "$,", "VVFIN", "ADV", "KOUS", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Und die Welt empf\u00e4ngt ihn vielfach mit Geheul.", "tokens": ["Und", "die", "Welt", "emp\u00b7f\u00e4ngt", "ihn", "viel\u00b7fach", "mit", "Ge\u00b7heul", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Autographensammler aber sind Erfolgsanbeter,", "tokens": ["Au\u00b7to\u00b7gra\u00b7phen\u00b7samm\u00b7ler", "a\u00b7ber", "sind", "Er\u00b7folgs\u00b7an\u00b7be\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "NN", "$,"], "meter": "+-+-+-+--++-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Und Erfolgsanbeter sind der Kunst ein Greul!", "tokens": ["Und", "Er\u00b7folgs\u00b7an\u00b7be\u00b7ter", "sind", "der", "Kunst", "ein", "Greul", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}}}}}