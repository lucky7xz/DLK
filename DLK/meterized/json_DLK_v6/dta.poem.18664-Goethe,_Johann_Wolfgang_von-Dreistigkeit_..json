{"dta.poem.18664": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Dreistigkeit .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Worauf kommt es \u00fcberall an", "tokens": ["Wo\u00b7rauf", "kommt", "es", "\u00fc\u00b7be\u00b7rall", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "APPR"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Dass der Mensch gesundet?", "tokens": ["Dass", "der", "Mensch", "ge\u00b7sun\u00b7det", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Jeder h\u00f6ret gern den Schall an", "tokens": ["Je\u00b7der", "h\u00f6\u00b7ret", "gern", "den", "Schall", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "ART", "NN", "APPR"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der zum Ton sich rundet.", "tokens": ["Der", "zum", "Ton", "sich", "run\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Alles weg! was deinen Lauf st\u00f6rt!", "tokens": ["Al\u00b7les", "weg", "!", "was", "dei\u00b7nen", "Lauf", "st\u00f6rt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PTKVZ", "$.", "PWS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur kein d\u00fcster Streben!", "tokens": ["Nur", "kein", "d\u00fcs\u00b7ter", "Stre\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Eh er singt und eh er aufh\u00f6rt", "tokens": ["Eh", "er", "singt", "und", "eh", "er", "auf\u00b7h\u00f6rt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "KOUS", "PPER", "VVFIN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Muss der Dichter leben.", "tokens": ["Muss", "der", "Dich\u00b7ter", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Und so mag des Lebens Erzklang", "tokens": ["Und", "so", "mag", "des", "Le\u00b7bens", "Erz\u00b7klang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die Seele dr\u00f6hnen!", "tokens": ["Durch", "die", "See\u00b7le", "dr\u00f6h\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "F\u00fchlt der Dichter sich das Herz bang", "tokens": ["F\u00fchlt", "der", "Dich\u00b7ter", "sich", "das", "Herz", "bang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PRF", "ART", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird sich selbst vers\u00f6hnen.", "tokens": ["Wird", "sich", "selbst", "ver\u00b7s\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}