{"textgrid.poem.52864": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Das Lied vom gerochenen Hausknecht", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Sobbe und der Putzky,", "tokens": ["Der", "Sob\u00b7be", "und", "der", "Putz\u00b7ky", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Putzky und der Sobb',", "tokens": ["Der", "Putz\u00b7ky", "und", "der", "Sobb'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie haben abgemurxky", "tokens": ["Sie", "ha\u00b7ben", "ab\u00b7ge\u00b7murx\u00b7ky"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "NE"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.4": {"text": "Wohl einen Hausknecht grob.", "tokens": ["Wohl", "ei\u00b7nen", "Haus\u00b7knecht", "grob", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Hausknecht war betrunken,", "tokens": ["Der", "Haus\u00b7knecht", "war", "be\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie waren n\u00fcchtern ganz,", "tokens": ["Sie", "wa\u00b7ren", "n\u00fcch\u00b7tern", "ganz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie waren edle Junker,", "tokens": ["Sie", "wa\u00b7ren", "ed\u00b7le", "Jun\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zwei Gardeleutenants.", "tokens": ["Zwei", "Gar\u00b7de\u00b7leu\u00b7ten\u00b7ants", "."], "token_info": ["word", "word", "punct"], "pos": ["CARD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie kamen auf die Festung", "tokens": ["Sie", "ka\u00b7men", "auf", "die", "Fes\u00b7tung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ob dieses Hausknechts Mord;", "tokens": ["Ob", "die\u00b7ses", "Haus\u00b7knechts", "Mord", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie flohen vor der L\u00e4st'rung", "tokens": ["Sie", "flo\u00b7hen", "vor", "der", "L\u00e4st'\u00b7rung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der b\u00f6sen Menschen fort.", "tokens": ["Der", "b\u00f6\u00b7sen", "Men\u00b7schen", "fort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie brannten durch, auf Ehre,", "tokens": ["Sie", "brann\u00b7ten", "durch", ",", "auf", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sind weit umhergeirrt,", "tokens": ["Sind", "weit", "um\u00b7her\u00b7geirrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Als wie zwei Ahasvere,", "tokens": ["Als", "wie", "zwei", "A\u00b7has\u00b7ve\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und waren sehr lakirt.", "tokens": ["Und", "wa\u00b7ren", "sehr", "la\u00b7kirt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sie waren nirgends sicher,", "tokens": ["Sie", "wa\u00b7ren", "nir\u00b7gends", "si\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verfolgt hat sie gar dreist", "tokens": ["Ver\u00b7folgt", "hat", "sie", "gar", "dreist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPER", "ADV", "CARD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Hausknechts unsterblicher", "tokens": ["Des", "Haus\u00b7knechts", "uns\u00b7terb\u00b7li\u00b7cher"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADJA"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Und ungerochner Geist.", "tokens": ["Und", "un\u00b7ge\u00b7roch\u00b7ner", "Geist", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O h\u00f6ret jetzt die Kunde", "tokens": ["O", "h\u00f6\u00b7ret", "jetzt", "die", "Kun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die aus Australien t\u00f6nt,", "tokens": ["Die", "aus", "Aust\u00b7ra\u00b7li\u00b7en", "t\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie haben Ruh' gefunden,", "tokens": ["Sie", "ha\u00b7ben", "Ruh'", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Hausknecht ist vers\u00f6hnt.", "tokens": ["Der", "Haus\u00b7knecht", "ist", "ver\u00b7s\u00f6hnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Der Putzky, Freund des Sobben,", "tokens": ["Der", "Putz\u00b7ky", ",", "Freund", "des", "Sob\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Sobbe, Freund Putzky's,", "tokens": ["Der", "Sob\u00b7be", ",", "Freund", "Putz\u00b7ky's", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sind selber Hausknecht worden,", "tokens": ["Sind", "sel\u00b7ber", "Haus\u00b7knecht", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "VAPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das ist die Nemesis.", "tokens": ["Das", "ist", "die", "Ne\u00b7me\u00b7sis", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}