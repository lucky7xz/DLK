{"dta.poem.8466": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Laube.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "K\u00fchle gr\u00fcne Dunkelheit,", "tokens": ["K\u00fch\u00b7le", "gr\u00fc\u00b7ne", "Dun\u00b7kel\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo mein liebes M\u00e4dchen oft gesessen,", "tokens": ["Wo", "mein", "lie\u00b7bes", "M\u00e4d\u00b7chen", "oft", "ge\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und des Fr\u00fchlings sich gefreut.", "tokens": ["Und", "des", "Fr\u00fch\u00b7lings", "sich", "ge\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schauer wird durch meine Nerven beben,", "tokens": ["Schau\u00b7er", "wird", "durch", "mei\u00b7ne", "Ner\u00b7ven", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Werd' ich deine Bl\u00fcten sehn,", "tokens": ["Werd'", "ich", "dei\u00b7ne", "Bl\u00fc\u00b7ten", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ihr Bildniss mir entgegen schweben,", "tokens": ["Und", "ihr", "Bild\u00b7niss", "mir", "ent\u00b7ge\u00b7gen", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPER", "PTKVZ", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ihre Gottheit mich umwehn.", "tokens": ["Ih\u00b7re", "Got\u00b7theit", "mich", "um\u00b7wehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Thr\u00e4nenvoll werd' ich beim Mondenlichte,", "tokens": ["Thr\u00e4\u00b7nen\u00b7voll", "werd'", "ich", "beim", "Mon\u00b7den\u00b7lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In der Geisterstunde Graun,", "tokens": ["In", "der", "Geis\u00b7ter\u00b7stun\u00b7de", "Graun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dir entgegen zittern, und Gesichte", "tokens": ["Dir", "ent\u00b7ge\u00b7gen", "zit\u00b7tern", ",", "und", "Ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "PTKVZ", "VVFIN", "$,", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Auf Gesichte werd' ich schaun;", "tokens": ["Auf", "Ge\u00b7sich\u00b7te", "werd'", "ich", "schaun", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Mich in manchen G\u00f6ttertraum verirren,", "tokens": ["Mich", "in", "man\u00b7chen", "G\u00f6t\u00b7ter\u00b7traum", "ver\u00b7ir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Bis Entz\u00fcckung mich durchbebt,", "tokens": ["Bis", "Ent\u00b7z\u00fc\u00b7ckung", "mich", "durch\u00b7bebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und nach meinem s\u00fcssen T\u00e4ubchen girren,", "tokens": ["Und", "nach", "mei\u00b7nem", "s\u00fcs\u00b7sen", "T\u00e4ub\u00b7chen", "gir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dessen Abschied vor mir schwebt.", "tokens": ["Des\u00b7sen", "Ab\u00b7schied", "vor", "mir", "schwebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wenn ich auf der Bahn der Tugend wanke,", "tokens": ["Wenn", "ich", "auf", "der", "Bahn", "der", "Tu\u00b7gend", "wan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Weltvergn\u00fcgen mich bestrickt;", "tokens": ["Welt\u00b7ver\u00b7gn\u00fc\u00b7gen", "mich", "be\u00b7strickt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dann durchgl\u00fche mich der Feurgedanke,", "tokens": ["Dann", "durch\u00b7gl\u00fc\u00b7he", "mich", "der", "Feur\u00b7ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Was in dir ich einst erblickt:", "tokens": ["Was", "in", "dir", "ich", "einst", "er\u00b7blickt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und, als str\u00f6mt' aus Gottes offnem Himmel", "tokens": ["Und", ",", "als", "str\u00f6mt'", "aus", "Got\u00b7tes", "off\u00b7nem", "Him\u00b7mel"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "VVFIN", "APPR", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Tugendkraft auf mich herab,", "tokens": ["Tu\u00b7gend\u00b7kraft", "auf", "mich", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Werd\u2019 ich fliehen, und vom Erdgewimmel", "tokens": ["Werd'", "ich", "flie\u00b7hen", ",", "und", "vom", "Erd\u00b7ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVINF", "$,", "KON", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Fernen meinen Pilgerstab.", "tokens": ["Fer\u00b7nen", "mei\u00b7nen", "Pil\u00b7ger\u00b7stab", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}