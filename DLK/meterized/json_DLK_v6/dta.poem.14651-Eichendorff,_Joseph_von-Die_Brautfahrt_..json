{"dta.poem.14651": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Brautfahrt .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ich kann hier nicht m\u00fc\u00dfig lauern,", "tokens": ["Ich", "kann", "hier", "nicht", "m\u00fc\u00b7\u00dfig", "lau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Treiben auf dem flachen Sand,", "tokens": ["Trei\u00b7ben", "auf", "dem", "fla\u00b7chen", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieser Kreis von Felsenmauern", "tokens": ["Die\u00b7ser", "Kreis", "von", "Fel\u00b7sen\u00b7mau\u00b7ern"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4lt mein Leben nicht umspannt;", "tokens": ["H\u00e4lt", "mein", "Le\u00b7ben", "nicht", "um\u00b7spannt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sch\u00f6n're L\u00e4nder bl\u00fchen ferne,", "tokens": ["Sch\u00f6n'\u00b7re", "L\u00e4n\u00b7der", "bl\u00fc\u00b7hen", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das verk\u00fcnden mir die Sterne.", "tokens": ["Das", "ver\u00b7k\u00fcn\u00b7den", "mir", "die", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}