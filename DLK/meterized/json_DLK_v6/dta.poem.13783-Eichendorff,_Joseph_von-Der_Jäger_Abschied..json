{"dta.poem.13783": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der J\u00e4ger Abschied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "K\u00fchle auf dem sch\u00f6nen Rheine,", "tokens": ["K\u00fch\u00b7le", "auf", "dem", "sch\u00f6\u00b7nen", "Rhei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fuhren wir vereinte Br\u00fcder,", "tokens": ["Fuh\u00b7ren", "wir", "ver\u00b7ein\u00b7te", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tranken von dem goldnen Weine,", "tokens": ["Tran\u00b7ken", "von", "dem", "gold\u00b7nen", "Wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Singend gute deutsche Lieder.", "tokens": ["Sin\u00b7gend", "gu\u00b7te", "deut\u00b7sche", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was uns dort erf\u00fcllt die Brust,", "tokens": ["Was", "uns", "dort", "er\u00b7f\u00fcllt", "die", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sollen wir halten,", "tokens": ["Sol\u00b7len", "wir", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Niemals erkalten", "tokens": ["Nie\u00b7mals", "er\u00b7kal\u00b7ten"], "token_info": ["word", "word"], "pos": ["ADV", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Und vollbringen treu mit Lust!", "tokens": ["Und", "voll\u00b7brin\u00b7gen", "treu", "mit", "Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Und so wollen wir uns theilen,", "tokens": ["Und", "so", "wol\u00b7len", "wir", "uns", "thei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Eines Fels verschied'ne Quellen,", "tokens": ["Ei\u00b7nes", "Fels", "ver\u00b7schie\u00b7d'\u00b7ne", "Quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "Bleiben so auf hundert Meilen", "tokens": ["Blei\u00b7ben", "so", "auf", "hun\u00b7dert", "Mei\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Ewig redliche Gesellen!", "tokens": ["E\u00b7wig", "red\u00b7li\u00b7che", "Ge\u00b7sel\u00b7len", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}