{"textgrid.poem.48603": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "13. Auf Godfried Wilhelms seinen Namenstag", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ph\u00f6bus hatte seinen Wagen", "tokens": ["Ph\u00f6\u00b7bus", "hat\u00b7te", "sei\u00b7nen", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "kaum recht halb hervor gef\u00fchrt", "tokens": ["kaum", "recht", "halb", "her\u00b7vor", "ge\u00b7f\u00fchrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADJD", "PTKVZ", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "und die muntre Welt geziert", "tokens": ["und", "die", "mun\u00b7tre", "Welt", "ge\u00b7ziert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "in den warmen Sommertagen,", "tokens": ["in", "den", "war\u00b7men", "Som\u00b7mer\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "als der gute ", "tokens": ["als", "der", "gu\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "denen kaum erwachten Schl\u00e4fern,", "tokens": ["de\u00b7nen", "kaum", "er\u00b7wach\u00b7ten", "Schl\u00e4\u00b7fern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "seinen treuen dreien Sch\u00e4fern,", "tokens": ["sei\u00b7nen", "treu\u00b7en", "drei\u00b7en", "Sch\u00e4\u00b7fern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "brachte seinen lieben Gru\u00df.", "tokens": ["brach\u00b7te", "sei\u00b7nen", "lie\u00b7ben", "Gru\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf, auf, auf, sprach er, ihr Br\u00fcder!", "tokens": ["Auf", ",", "auf", ",", "auf", ",", "sprach", "er", ",", "ihr", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "$,", "PTKVZ", "$,", "PTKVZ", "$,", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Auf, auf, auf! Itzt ist es Zeit,", "tokens": ["Auf", ",", "auf", ",", "auf", "!", "Itzt", "ist", "es", "Zeit", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PTKVZ", "$,", "PTKVZ", "$.", "ADV", "VAFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "da\u00df wir alle Sorg' und Leid", "tokens": ["da\u00df", "wir", "al\u00b7le", "Sor\u00b7g'", "und", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIAT", "NN", "KON", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "schlagen zu den F\u00fc\u00dfen nieder.", "tokens": ["schla\u00b7gen", "zu", "den", "F\u00fc\u00b7\u00dfen", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Heute, heute scheint der Schein,", "tokens": ["Heu\u00b7te", ",", "heu\u00b7te", "scheint", "der", "Schein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "des sich meine Mutter freute.", "tokens": ["des", "sich", "mei\u00b7ne", "Mut\u00b7ter", "freu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mein Geburtstag, der ist heute.", "tokens": ["Mein", "Ge\u00b7burts\u00b7tag", ",", "der", "ist", "heu\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "VAFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dieser Tag soll unser sein.", "tokens": ["Die\u00b7ser", "Tag", "soll", "un\u00b7ser", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VMFIN", "PPOSAT", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Was war Liebers diesen Dreien?", "tokens": ["Was", "war", "Lie\u00b7bers", "die\u00b7sen", "Drei\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "sprungen auf mit lauten Schreien:", "tokens": ["sprun\u00b7gen", "auf", "mit", "lau\u00b7ten", "Schrei\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lange lebe ", "tokens": ["Lan\u00b7ge", "le\u00b7be"], "token_info": ["word", "word"], "pos": ["NE", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Und so ward mit Lust und K\u00fcssen", "tokens": ["Und", "so", "ward", "mit", "Lust", "und", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Tag und halbe Nacht verschlissen", "tokens": ["Tag", "und", "hal\u00b7be", "Nacht", "ver\u00b7schlis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "um der ", "tokens": ["um", "der"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}}}}}