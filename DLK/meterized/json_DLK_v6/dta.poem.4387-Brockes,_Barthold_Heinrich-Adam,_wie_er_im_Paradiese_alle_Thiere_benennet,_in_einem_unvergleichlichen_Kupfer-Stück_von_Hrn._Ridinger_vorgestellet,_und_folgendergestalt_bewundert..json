{"dta.poem.4387": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Adam,  \n wie er im Paradiese alle Thiere benennet,  \n in einem  \n unvergleichlichen Kupfer-St\u00fcck  \n  von Hrn. Ridinger vorgestellet,  \n und folgendergestalt bewundert.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Welch ein gebieterischer Anstand herrscht in den freundli-", "tokens": ["Welch", "ein", "ge\u00b7bie\u00b7te\u00b7ri\u00b7scher", "An\u00b7stand", "herrscht", "in", "den", "freund\u00b7li"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "chen Gebehrden,", "tokens": ["chen", "Ge\u00b7behr\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.3": {"text": "Und welch ein herrisches Betragen in dem Monarchen", "tokens": ["Und", "welch", "ein", "her\u00b7ri\u00b7sches", "Be\u00b7tra\u00b7gen", "in", "dem", "Mon\u00b7ar\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAT", "ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-++-+-", "measure": "unknown.measure.septa"}, "line.4": {"text": "dieser Erden!", "tokens": ["die\u00b7ser", "Er\u00b7den", "!"], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Es zeigt die Kunst auf diesem Kupfer, in dem so sch\u00f6nen", "tokens": ["Es", "zeigt", "die", "Kunst", "auf", "die\u00b7sem", "Kup\u00b7fer", ",", "in", "dem", "so", "sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PDAT", "NN", "$,", "APPR", "ART", "ADV", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Lust-Revier", "tokens": ["Lust\u00b7Re\u00b7vier"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Der Anmuht-reichen Edens-Auen, in einer fast verkl\u00e4rten", "tokens": ["Der", "Anm\u00b7uht\u00b7rei\u00b7chen", "E\u00b7dens\u00b7Au\u00b7en", ",", "in", "ei\u00b7ner", "fast", "ver\u00b7kl\u00e4r\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "APPR", "ART", "ADV", "VVFIN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "Zier,", "tokens": ["Zier", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Was wir f\u00fcr Herrlichkeit verlohren. Doch zeiget uns", "tokens": ["Was", "wir", "f\u00fcr", "Herr\u00b7lich\u00b7keit", "ver\u00b7loh\u00b7ren", ".", "Doch", "zei\u00b7get", "uns"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "NN", "VVPP", "$.", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "zugleich dabey", "tokens": ["zu\u00b7gleich", "da\u00b7bey"], "token_info": ["word", "word"], "pos": ["ADV", "PAV"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Der Z\u00fcge Klugheit, was uns doch, auch nach dem Fall,", "tokens": ["Der", "Z\u00fc\u00b7ge", "Klug\u00b7heit", ",", "was", "uns", "doch", ",", "auch", "nach", "dem", "Fall", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "PPER", "ADV", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "gelassen sey.", "tokens": ["ge\u00b7las\u00b7sen", "sey", "."], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Wenn wir ein billiges Bewundern dem Geist des K\u00fcnst-", "tokens": ["Wenn", "wir", "ein", "bil\u00b7li\u00b7ges", "Be\u00b7wun\u00b7dern", "dem", "Geist", "des", "K\u00fcnst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "lers hier gewehren;", "tokens": ["lers", "hier", "ge\u00b7weh\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "So werden wir des Sch\u00f6pfers Gr\u00f6sse, (Der ihn gegeben,", "tokens": ["So", "wer\u00b7den", "wir", "des", "Sch\u00f6p\u00b7fers", "Gr\u00f6s\u00b7se", ",", "(", "Der", "ihn", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "NN", "$,", "$(", "ART", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+----+-", "measure": "unknown.measure.penta"}, "line.16": {"text": "Der, durch ihn,", "tokens": ["Der", ",", "durch", "ihn", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.17": {"text": "Sein Werk so sch\u00f6n copieren lie\u00df, wozu nur Er die Kraft", "tokens": ["Sein", "Werk", "so", "sch\u00f6n", "co\u00b7pie\u00b7ren", "lie\u00df", ",", "wo\u00b7zu", "nur", "Er", "die", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "VVINF", "VVFIN", "$,", "PWAV", "ADV", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.18": {"text": "verliehn,)", "tokens": ["ver\u00b7liehn", ",", ")"], "token_info": ["word", "punct", "punct"], "pos": ["VVPP", "$,", "$("], "meter": "-+", "measure": "iambic.single"}, "line.19": {"text": "In einer fr\u00f6hlichen Betrachtung, mit Lust und Andacht,", "tokens": ["In", "ei\u00b7ner", "fr\u00f6h\u00b7li\u00b7chen", "Be\u00b7trach\u00b7tung", ",", "mit", "Lust", "und", "An\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Selber ehren.", "tokens": ["Sel\u00b7ber", "eh\u00b7ren", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}