{"textgrid.poem.50759": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "7", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von hei\u00dfer Lebenslust entgl\u00fcht,", "tokens": ["Von", "hei\u00b7\u00dfer", "Le\u00b7bens\u00b7lust", "ent\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hab ich das Sommerland durchstreift;", "tokens": ["Hab", "ich", "das", "Som\u00b7mer\u00b7land", "durch\u00b7streift", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drob ist der Tag sch\u00f6n abgebl\u00fcht", "tokens": ["Drob", "ist", "der", "Tag", "sch\u00f6n", "ab\u00b7ge\u00b7bl\u00fcht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und zu der sch\u00f6nsten Nacht gereift.", "tokens": ["Und", "zu", "der", "sch\u00f6ns\u00b7ten", "Nacht", "ge\u00b7reift", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich trete auf des Berges R\u00fccken", "tokens": ["Ich", "tre\u00b7te", "auf", "des", "Ber\u00b7ges", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Einsam ins offne Waldestor", "tokens": ["Ein\u00b7sam", "ins", "off\u00b7ne", "Wal\u00b7des\u00b7tor"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Und beuge mich mit trunknen Blicken", "tokens": ["Und", "beu\u00b7ge", "mich", "mit", "trunk\u00b7nen", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Hoch in die stille Landschaft vor.", "tokens": ["Hoch", "in", "die", "stil\u00b7le", "Land\u00b7schaft", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Am andern H\u00fcgel dr\u00fcben steht", "tokens": ["Am", "an\u00b7dern", "H\u00fc\u00b7gel", "dr\u00fc\u00b7ben", "steht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Sternenschein das liebe Haus;", "tokens": ["Im", "Ster\u00b7nen\u00b7schein", "das", "lie\u00b7be", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus seinem offnen Fenster weht", "tokens": ["Aus", "sei\u00b7nem", "off\u00b7nen", "Fens\u00b7ter", "weht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Vorhang in die Nacht hinaus.", "tokens": ["Ein", "Vor\u00b7hang", "in", "die", "Nacht", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das ist f\u00fcrwahr ein luftig Gitter,", "tokens": ["Das", "ist", "f\u00fcr\u00b7wahr", "ein", "luf\u00b7tig", "Git\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das mir mein Fr\u00e4ulein dort verschlie\u00dft!", "tokens": ["Das", "mir", "mein", "Fr\u00e4u\u00b7lein", "dort", "ver\u00b7schlie\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nur schade, da\u00df mir armem Ritter", "tokens": ["Nur", "scha\u00b7de", ",", "da\u00df", "mir", "ar\u00b7mem", "Rit\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Talstrom noch dazwischen flie\u00dft!", "tokens": ["Der", "Tal\u00b7strom", "noch", "da\u00b7zwi\u00b7schen", "flie\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Zieh du f\u00fcr mich, mein leichter Sang,", "tokens": ["Zieh", "du", "f\u00fcr", "mich", ",", "mein", "leich\u00b7ter", "Sang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hin\u00fcber an der Liebsten Brust!", "tokens": ["Hin\u00b7\u00fc\u00b7ber", "an", "der", "Liebs\u00b7ten", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vielleicht tr\u00e4gt ihr dein ferner Klang", "tokens": ["Viel\u00b7leicht", "tr\u00e4gt", "ihr", "dein", "fer\u00b7ner", "Klang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zu Herzen meine Dichterlust!", "tokens": ["Zu", "Her\u00b7zen", "mei\u00b7ne", "Dich\u00b7ter\u00b7lust", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ja, ich will ihr ein St\u00e4ndchen bringen,", "tokens": ["Ja", ",", "ich", "will", "ihr", "ein", "St\u00e4nd\u00b7chen", "brin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das weithin durch die L\u00fcfte schallt:", "tokens": ["Das", "weit\u00b7hin", "durch", "die", "L\u00fcf\u00b7te", "schallt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So spiele du zu meinem Singen,", "tokens": ["So", "spie\u00b7le", "du", "zu", "mei\u00b7nem", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "O Sommernacht, auf Tal und Wald!", "tokens": ["O", "Som\u00b7mer\u00b7nacht", ",", "auf", "Tal", "und", "Wald", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dein Saitenspiel im Tale liegt,", "tokens": ["Dein", "Sai\u00b7ten\u00b7spiel", "im", "Ta\u00b7le", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die feinen Silberbr\u00fcnnlein all;", "tokens": ["Die", "fei\u00b7nen", "Sil\u00b7ber\u00b7br\u00fcnn\u00b7lein", "all", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Tann, der auf den H\u00f6hn sich wiegt,", "tokens": ["Den", "Tann", ",", "der", "auf", "den", "H\u00f6hn", "sich", "wiegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00df rauschen drein, wie Orgelschall!", "tokens": ["La\u00df", "rau\u00b7schen", "drein", ",", "wie", "Or\u00b7gel\u00b7schall", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PTKVZ", "$,", "PWAV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Elfensummen und das Kosen,", "tokens": ["Das", "El\u00b7fen\u00b7sum\u00b7men", "und", "das", "Ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das schwellend alle Kelche regt,", "tokens": ["Das", "schwel\u00b7lend", "al\u00b7le", "Kel\u00b7che", "regt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vereine mit des Stromes Tosen,", "tokens": ["Ver\u00b7ei\u00b7ne", "mit", "des", "Stro\u00b7mes", "To\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der seine Wogen talw\u00e4rts tr\u00e4gt!", "tokens": ["Der", "sei\u00b7ne", "Wo\u00b7gen", "tal\u00b7w\u00e4rts", "tr\u00e4gt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Im S\u00fcden zieht ein Wetter auf,", "tokens": ["Im", "S\u00fc\u00b7den", "zieht", "ein", "Wet\u00b7ter", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schnell werb ich's f\u00fcr mein St\u00e4ndchen an;", "tokens": ["Schnell", "werb", "ich's", "f\u00fcr", "mein", "St\u00e4nd\u00b7chen", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIS", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch nehm es fernhin seinen Lauf,", "tokens": ["Doch", "nehm", "es", "fern\u00b7hin", "sei\u00b7nen", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich es \u00fcbert\u00f6nen kann!", "tokens": ["Da\u00df", "ich", "es", "\u00fc\u00b7ber\u00b7t\u00f6\u00b7nen", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die M\u00fchlen sind die Hackbrettschl\u00e4ger", "tokens": ["Die", "M\u00fch\u00b7len", "sind", "die", "Hack\u00b7brett\u00b7schl\u00e4\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zuhinterst in des Tales Grund,", "tokens": ["Zu\u00b7hin\u00b7terst", "in", "des", "Ta\u00b7les", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Sterne meine Fackeltr\u00e4ger,", "tokens": ["Die", "Ster\u00b7ne", "mei\u00b7ne", "Fa\u00b7ckel\u00b7tr\u00e4\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sie leuchten mir im weiten Rund!", "tokens": ["Sie", "leuch\u00b7ten", "mir", "im", "wei\u00b7ten", "Rund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Nun will ich singen \u00fcberlaut", "tokens": ["Nun", "will", "ich", "sin\u00b7gen", "\u00fc\u00b7berl\u00b7aut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor allem Land, das gr\u00fcnt und bl\u00fcht!", "tokens": ["Vor", "al\u00b7lem", "Land", ",", "das", "gr\u00fcnt", "und", "bl\u00fcht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "$,", "PDS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es ist kein Baum so hoch gebaut,", "tokens": ["Es", "ist", "kein", "Baum", "so", "hoch", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dar\u00fcberhin mein Sang nicht zieht;", "tokens": ["Da\u00b7r\u00fc\u00b7berh\u00b7in", "mein", "Sang", "nicht", "zieht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Will eine Liederbr\u00fccke schlagen", "tokens": ["Will", "ei\u00b7ne", "Lie\u00b7der\u00b7br\u00fc\u00b7cke", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Aus meiner Brust in ihre Brust:", "tokens": ["Aus", "mei\u00b7ner", "Brust", "in", "ih\u00b7re", "Brust", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Herz! wandle drauf, bis es will tagen,", "tokens": ["Herz", "!", "wand\u00b7le", "drauf", ",", "bis", "es", "will", "ta\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PTKVZ", "$,", "KOUS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Und wecke sie zu gleicher Lust!", "tokens": ["Und", "we\u00b7cke", "sie", "zu", "glei\u00b7cher", "Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}