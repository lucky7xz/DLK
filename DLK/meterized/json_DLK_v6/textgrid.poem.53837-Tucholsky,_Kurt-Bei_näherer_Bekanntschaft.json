{"textgrid.poem.53837": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Bei n\u00e4herer Bekanntschaft", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von ferne gleichen die Gro\u00dfen im Geist", "tokens": ["Von", "fer\u00b7ne", "glei\u00b7chen", "die", "Gro\u00b7\u00dfen", "im", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "ART", "NN", "APPRART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "den G\u00f6ttern, den hehren.", "tokens": ["den", "G\u00f6t\u00b7tern", ",", "den", "heh\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Solange du nichts von ihnen wei\u00dft,", "tokens": ["So\u00b7lan\u00b7ge", "du", "nichts", "von", "ih\u00b7nen", "wei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "kannst du sie verehren.", "tokens": ["kannst", "du", "sie", "ver\u00b7eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Doch hast du mit Deutschlands Musenpracht", "tokens": ["Doch", "hast", "du", "mit", "Deutschlands", "Mu\u00b7sen\u00b7pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "APPR", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "erst n\u00e4here Bekanntschaft gemacht,", "tokens": ["erst", "n\u00e4\u00b7he\u00b7re", "Be\u00b7kannt\u00b7schaft", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "dann schick deine Illusionen man pennen:", "tokens": ["dann", "schick", "dei\u00b7ne", "Il\u00b7lu\u00b7si\u00b7o\u00b7nen", "man", "pen\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "PIS", "VVINF", "$."], "meter": "-+---+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Du mu\u00dft sie nicht kennen! Du mu\u00dft sie nicht kennen!", "tokens": ["Du", "mu\u00dft", "sie", "nicht", "ken\u00b7nen", "!", "Du", "mu\u00dft", "sie", "nicht", "ken\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$.", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Der flicht an der alten Griechen Statt", "tokens": ["Der", "flicht", "an", "der", "al\u00b7ten", "Grie\u00b7chen", "Statt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "die tragische Kette \u2013", "tokens": ["die", "tra\u00b7gi\u00b7sche", "Ket\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "doch verrei\u00dft ihn das ", "tokens": ["doch", "ver\u00b7rei\u00dft", "ihn", "das"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "dann f\u00e4llt er aus't Bette.", "tokens": ["dann", "f\u00e4llt", "er", "aus't", "Bet\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Der meckert im Alter wie ein Bock", "tokens": ["Der", "me\u00b7ckert", "im", "Al\u00b7ter", "wie", "ein", "Bock"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "KOKOM", "ART", "NE"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "und kriecht einer T\u00e4nzerin unter den Rock.", "tokens": ["und", "kriecht", "ei\u00b7ner", "T\u00e4n\u00b7ze\u00b7rin", "un\u00b7ter", "den", "Rock", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Und was sie an Damen ihr Eigen nennen:", "tokens": ["Und", "was", "sie", "an", "Da\u00b7men", "ihr", "Ei\u00b7gen", "nen\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "APPR", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Du mu\u00dft sie nicht kennen! Du mu\u00dft sie nicht kennen!", "tokens": ["Du", "mu\u00dft", "sie", "nicht", "ken\u00b7nen", "!", "Du", "mu\u00dft", "sie", "nicht", "ken\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$.", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Denn mit etwas hat Gott sie sch\u00f6n angeschmiert:", "tokens": ["Denn", "mit", "et\u00b7was", "hat", "Gott", "sie", "sch\u00f6n", "an\u00b7ge\u00b7schmiert", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "VAFIN", "NN", "PPER", "ADJD", "VVPP", "$."], "meter": "--+-+---+-+", "measure": "anapaest.init"}, "line.2": {"text": "mit ihren Frauen.", "tokens": ["mit", "ih\u00b7ren", "Frau\u00b7en", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "\u00bbmein Mann, mein Mann!\u00ab", "tokens": ["\u00bb", "mein", "Mann", ",", "mein", "Mann", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Dergleichen blamiert:", "tokens": ["Derg\u00b7lei\u00b7chen", "bla\u00b7miert", ":"], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "ein Weibst\u00fcck, scheel\u00e4ugig und verschmiert,", "tokens": ["ein", "Weib\u00b7st\u00fcck", ",", "schee\u00b7l\u00e4u\u00b7gig", "und", "ver\u00b7schmiert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "in den himmlischen Gauen.", "tokens": ["in", "den", "himm\u00b7li\u00b7schen", "Gau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Der sitzt in der H\u00f6hle, ein kr\u00f6tiger Greis,", "tokens": ["Der", "sitzt", "in", "der", "H\u00f6h\u00b7le", ",", "ein", "kr\u00f6\u00b7ti\u00b7ger", "Greis", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "der spricht nur von sich, weil er sonst nichts wei\u00df . . .", "tokens": ["der", "spricht", "nur", "von", "sich", ",", "weil", "er", "sonst", "nichts", "wei\u00df", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "VVFIN", "ADV", "APPR", "PRF", "$,", "KOUS", "PPER", "ADV", "PIS", "VVFIN", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Von weitem! La\u00df sie am Himmel brennen!", "tokens": ["Von", "wei\u00b7tem", "!", "La\u00df", "sie", "am", "Him\u00b7mel", "bren\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$.", "VVIMP", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "In B\u00fcchern und an Rundfunkantennen . . .", "tokens": ["In", "B\u00fc\u00b7chern", "und", "an", "Rund\u00b7fun\u00b7kan\u00b7ten\u00b7nen", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.11": {"text": "Aber: Du mu\u00dft sie nicht kennen. Du mu\u00dft sie nicht kennen!", "tokens": ["A\u00b7ber", ":", "Du", "mu\u00dft", "sie", "nicht", "ken\u00b7nen", ".", "Du", "mu\u00dft", "sie", "nicht", "ken\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$.", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$.", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}}}}}