{"textgrid.poem.38065": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Maria, Gnadenmutter zu Freyberg", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wundersch\u00f6n Pr\u00e4chtige,", "tokens": ["Wun\u00b7der\u00b7sch\u00f6n", "Pr\u00e4ch\u00b7ti\u00b7ge", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Gro\u00dfe und M\u00e4chtige,", "tokens": ["Gro\u00b7\u00dfe", "und", "M\u00e4ch\u00b7ti\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "KON", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Liebreich holdselige, himmlische Frau,", "tokens": ["Lieb\u00b7reich", "hold\u00b7se\u00b7li\u00b7ge", ",", "himm\u00b7li\u00b7sche", "Frau", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+---+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Welcher auf ewiglich,", "tokens": ["Wel\u00b7cher", "auf", "e\u00b7wig\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "APPR", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Kindlich verbinde mich,", "tokens": ["Kind\u00b7lich", "ver\u00b7bin\u00b7de", "mich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Ja mit Leib und Seel g\u00e4nzlich vertrau.", "tokens": ["Ja", "mit", "Leib", "und", "Seel", "g\u00e4nz\u00b7lich", "ver\u00b7trau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "NN", "KON", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Billig mein Leben,", "tokens": ["Bil\u00b7lig", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Alles beyneben,", "tokens": ["Al\u00b7les", "bey\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVIZU", "$,"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.9": {"text": "Alles, ja alles, was immer ich bin,", "tokens": ["Al\u00b7les", ",", "ja", "al\u00b7les", ",", "was", "im\u00b7mer", "ich", "bin", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "ADV", "PIS", "$,", "PRELS", "ADV", "PPER", "VAFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.10": {"text": "Geb ich mit Freuden, Maria, dir hin.", "tokens": ["Geb", "ich", "mit", "Freu\u00b7den", ",", "Ma\u00b7ria", ",", "dir", "hin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,", "NE", "$,", "PPER", "PTKVZ", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Weil du ganz mackellos", "tokens": ["Weil", "du", "ganz", "ma\u00b7ckel\u00b7los"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Hat Gott dich Gnaden-Ros,", "tokens": ["Hat", "Gott", "dich", "Gna\u00b7den\u00b7Ros", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPER", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der himmlisch Vater sein Tochter genannt,", "tokens": ["Der", "himm\u00b7lisch", "Va\u00b7ter", "sein", "Toch\u00b7ter", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ja auch der g\u00f6ttlich Sohn,", "tokens": ["Ja", "auch", "der", "g\u00f6tt\u00b7lich", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In seinem h\u00f6chsten Thron", "tokens": ["In", "sei\u00b7nem", "h\u00f6chs\u00b7ten", "Thron"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sich zu dir, dich als Mutter bekannt,", "tokens": ["Sich", "zu", "dir", ",", "dich", "als", "Mut\u00b7ter", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "$,", "PPER", "KOUS", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Endlich die Ehren", "tokens": ["End\u00b7lich", "die", "Eh\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Noch zu vermehren", "tokens": ["Noch", "zu", "ver\u00b7meh\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PTKZU", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Als dir als seiner erw\u00e4hlten Braut,", "tokens": ["Als", "dir", "als", "sei\u00b7ner", "er\u00b7w\u00e4hl\u00b7ten", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOUS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Heiliger Geist sich dir selber vertraut.", "tokens": ["Hei\u00b7li\u00b7ger", "Geist", "sich", "dir", "sel\u00b7ber", "ver\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "PPER", "ADV", "ADJD", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Sonn begleitet dich,", "tokens": ["Die", "Sonn", "be\u00b7glei\u00b7tet", "dich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Es unterwirfet sich,", "tokens": ["Es", "un\u00b7ter\u00b7wir\u00b7fet", "sich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Zu deinen F\u00fc\u00dfen der silberne Mond,", "tokens": ["Zu", "dei\u00b7nen", "F\u00fc\u00b7\u00dfen", "der", "sil\u00b7ber\u00b7ne", "Mond", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Kein Unvollkommenheit", "tokens": ["Kein", "Un\u00b7voll\u00b7kom\u00b7men\u00b7heit"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mindert dein Herrlichkeit,", "tokens": ["Min\u00b7dert", "dein", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Um dein Haupt machen die Sterne ein Kron;", "tokens": ["Um", "dein", "Haupt", "ma\u00b7chen", "die", "Ster\u00b7ne", "ein", "Kron", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "--++--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Alles was lebet,", "tokens": ["Al\u00b7les", "was", "le\u00b7bet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "PWS", "VVFIN", "$,"], "meter": "+----", "measure": "dactylic.init"}, "line.8": {"text": "Alles was schwebet,", "tokens": ["Al\u00b7les", "was", "schwe\u00b7bet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "PWS", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Alles was Himmel und Erde schr\u00e4nkt ein,", "tokens": ["Al\u00b7les", "was", "Him\u00b7mel", "und", "Er\u00b7de", "schr\u00e4nkt", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PWS", "NN", "KON", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.10": {"text": "Mu\u00df deiner Majest\u00e4t unterthan seyn.", "tokens": ["Mu\u00df", "dei\u00b7ner", "Ma\u00b7jes\u00b7t\u00e4t", "un\u00b7ter\u00b7than", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVPP", "VAINF", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "In diesem Jammerthal", "tokens": ["In", "die\u00b7sem", "Jam\u00b7mer\u00b7thal"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Seufzen wir allzumahl,", "tokens": ["Seuf\u00b7zen", "wir", "all\u00b7zu\u00b7mahl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Zu dir, o Jungfrau, in Elend und Noth;", "tokens": ["Zu", "dir", ",", "o", "Jung\u00b7frau", ",", "in", "E\u00b7lend", "und", "Noth", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "FM", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Maria du allein,", "tokens": ["Ma\u00b7ria", "du", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Wollst unsre Mutter seyn,", "tokens": ["Wollst", "uns\u00b7re", "Mut\u00b7ter", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wenn die Seel scheidet vom Leib der Tod,", "tokens": ["Wenn", "die", "Seel", "schei\u00b7det", "vom", "Leib", "der", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-++--+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Wenn wir hinreisen,", "tokens": ["Wenn", "wir", "hin\u00b7rei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Thu uns erweisen", "tokens": ["Thu", "uns", "er\u00b7wei\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Gnad und Barmherzigkeit bey deinem Thron,", "tokens": ["Gnad", "und", "Barm\u00b7her\u00b7zig\u00b7keit", "bey", "dei\u00b7nem", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.10": {"text": "Bitt f\u00fcr uns Jesum dein g\u00f6ttlichen Sohn.", "tokens": ["Bitt", "f\u00fcr", "uns", "Je\u00b7sum", "dein", "g\u00f6tt\u00b7li\u00b7chen", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}