{"textgrid.poem.47213": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die drei Sterne auf Erden", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drei Sterne fielen von Himmelsh\u00f6h'n:", "tokens": ["Drei", "Ster\u00b7ne", "fie\u00b7len", "von", "Him\u00b7mels\u00b7h\u00f6h'n", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Was wollen wir thun auf Erden?", "tokens": ["Was", "wol\u00b7len", "wir", "thun", "auf", "Er\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich will als Ros' auf den Auen stehn,", "tokens": ["Ich", "will", "als", "Ros'", "auf", "den", "Au\u00b7en", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KOKOM", "NE", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich will zur Nachtigall werden.", "tokens": ["Ich", "will", "zur", "Nach\u00b7ti\u00b7gall", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "VAINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ich will versenken mein himmlisch Licht", "tokens": ["Ich", "will", "ver\u00b7sen\u00b7ken", "mein", "himm\u00b7lisch", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF", "PPOSAT", "NN", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "In ein holdl\u00e4chelndes Angesicht,", "tokens": ["In", "ein", "hold\u00b7l\u00e4\u00b7cheln\u00b7des", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Als M\u00e4gdlein will ich wandeln.", "tokens": ["Als", "M\u00e4gd\u00b7lein", "will", "ich", "wan\u00b7deln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VMFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Rose bl\u00fchte, die Nachtigall sang,", "tokens": ["Die", "Ro\u00b7se", "bl\u00fch\u00b7te", ",", "die", "Nach\u00b7ti\u00b7gall", "sang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das M\u00e4gdlein horchte und schaute.", "tokens": ["Das", "M\u00e4gd\u00b7lein", "horch\u00b7te", "und", "schau\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Rose den Tau des Himmels trank,", "tokens": ["Die", "Ro\u00b7se", "den", "Tau", "des", "Him\u00b7mels", "trank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Nachtigall Himmelslaute;", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "Him\u00b7mels\u00b7lau\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Das M\u00e4gdlein sog den Himmelsschein", "tokens": ["Das", "M\u00e4gd\u00b7lein", "sog", "den", "Him\u00b7mels\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In ihre lebenden Augen hinein", "tokens": ["In", "ih\u00b7re", "le\u00b7ben\u00b7den", "Au\u00b7gen", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKVZ"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und str\u00f6mt' es aus in ein Blicken.", "tokens": ["Und", "str\u00f6mt'", "es", "aus", "in", "ein", "Bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Und als der Fr\u00fchling beschlo\u00df den Lauf,", "tokens": ["Und", "als", "der", "Fr\u00fch\u00b7ling", "be\u00b7schlo\u00df", "den", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da f\u00fchlte die Ros' ein Schauern,", "tokens": ["Da", "f\u00fchl\u00b7te", "die", "Ros'", "ein", "Schau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Nachtigall h\u00f6rte zu singen auf,", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "h\u00f6r\u00b7te", "zu", "sin\u00b7gen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKZU", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Das M\u00e4gdlein begann zu trauern.", "tokens": ["Das", "M\u00e4gd\u00b7lein", "be\u00b7gann", "zu", "trau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Willst, Fr\u00fchling, du l\u00e4nger nicht weilen allhier,", "tokens": ["Willst", ",", "Fr\u00fch\u00b7ling", ",", "du", "l\u00e4n\u00b7ger", "nicht", "wei\u00b7len", "all\u00b7hier", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "NN", "$,", "PPER", "ADJD", "PTKNEG", "VVFIN", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "So nimm uns trauernde Schwestern mit dir", "tokens": ["So", "nimm", "uns", "trau\u00b7ern\u00b7de", "Schwes\u00b7tern", "mit", "dir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "ADJA", "NN", "APPR", "PPER"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Zu unsern heimischen Reichen.", "tokens": ["Zu", "un\u00b7sern", "hei\u00b7mi\u00b7schen", "Rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Er nahm die Ros' in die rechte Hand,", "tokens": ["Er", "nahm", "die", "Ros'", "in", "die", "rech\u00b7te", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Nachtigall in die Linken,", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "in", "die", "Lin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das M\u00e4gdlein h\u00fcllt' er in sein Gewand,", "tokens": ["Das", "M\u00e4gd\u00b7lein", "h\u00fcllt'", "er", "in", "sein", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "An den Busen lie\u00df er es sinken.", "tokens": ["An", "den", "Bu\u00b7sen", "lie\u00df", "er", "es", "sin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Die Geister zogen den Sternen zu,", "tokens": ["Die", "Geis\u00b7ter", "zo\u00b7gen", "den", "Ster\u00b7nen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ihr Staub fiel nieder zu irdischer Ruh',", "tokens": ["Ihr", "Staub", "fiel", "nie\u00b7der", "zu", "ir\u00b7di\u00b7scher", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Wer wird von neuem ihn wecken?", "tokens": ["Wer", "wird", "von", "neu\u00b7em", "ihn", "we\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "ADJA", "PPER", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}