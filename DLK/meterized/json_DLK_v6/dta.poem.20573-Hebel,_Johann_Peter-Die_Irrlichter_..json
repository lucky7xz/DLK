{"dta.poem.20573": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Die Irrlichter .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1803", "urn": "urn:nbn:de:kobv:b4-200905192133", "language": ["de:0.99"], "booktitle": "[Hebel, Johann Peter]: Allemannische Gedichte. Karlsruhe, 1803."}, "poem": {"stanza.1": {"line.1": {"text": "Nei, wart e wenig, \u2019s chunnt e guti Lehr!", "tokens": ["Nei", ",", "wart", "e", "we\u00b7nig", ",", "'s", "chunnt", "e", "gu\u00b7ti", "Lehr", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "NE", "ADV", "$,", "PPER", "VVFIN", "NE", "ADJA", "NN", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Vergi\u00df mers nit, schribs lieber in e Buch!", "tokens": ["Ver\u00b7gi\u00df", "mers", "nit", ",", "schribs", "lie\u00b7ber", "in", "e", "Buch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PTKNEG", "$,", "VVFIN", "ADV", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zum Erste sagi: Das walt Gott der Her,", "tokens": ["Zum", "Ers\u00b7te", "sa\u00b7gi", ":", "Das", "walt", "Gott", "der", "Her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "ART", "NN", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "isch alliwil no besser, a\u00df e Fluch.", "tokens": ["isch", "al\u00b7li\u00b7wil", "no", "bes\u00b7ser", ",", "a\u00df", "e", "Fluch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "VVFIN", "NE", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Fluch jagt d\u2019Engel mittem Heil dervo;", "tokens": ["Der", "Fluch", "jagt", "d'\u00b7En\u00b7gel", "mit\u00b7tem", "Heil", "der\u00b7vo", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "NE", "$."], "meter": "-++-+-+-+-+", "measure": "unknown.measure.hexa"}, "line.2": {"text": "e christli Gm\u00fceth und \u2019s Bette zieht sie a;", "tokens": ["e", "christ\u00b7li", "Gm\u00fceth", "und", "'s", "Bet\u00b7te", "zieht", "sie", "a", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "KON", "PPER", "NN", "VVFIN", "PPER", "NE", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "und wemme meint, me seh ne Marcher cho,", "tokens": ["und", "wem\u00b7me", "meint", ",", "me", "seh", "ne", "Mar\u00b7cher", "cho", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "$,", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "\u2019s isch numme so d\u2019Laterne vorne dra.", "tokens": ["'s", "isch", "num\u00b7me", "so", "d'\u00b7La\u00b7ter\u00b7ne", "vor\u00b7ne", "dra", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Zum Anderen, und wenn en Ehre-Ma", "tokens": ["Zum", "An\u00b7de\u00b7ren", ",", "und", "wenn", "en", "Eh\u00b7re\u00b7Ma"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "$,", "KON", "KOUS", "NE", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "ne Gsch\u00e4ft f\u00fcr ihn ellei z\u2019verrichte het,", "tokens": ["ne", "Gsch\u00e4ft", "f\u00fcr", "ihn", "el\u00b7lei", "z'\u00b7ver\u00b7rich\u00b7te", "het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPER", "PIAT", "ADJA", "VAFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "se lo\u00df en mache! Was gohts di denn a?", "tokens": ["se", "lo\u00df", "en", "ma\u00b7che", "!", "Was", "gohts", "di", "denn", "a", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "VVFIN", "$.", "PWS", "VVFIN", "NE", "KON", "NE", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "und los nit, wemme mittem Nochber redt!", "tokens": ["und", "los", "nit", ",", "wem\u00b7me", "mit\u00b7tem", "Noch\u00b7ber", "redt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKNEG", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und goht me der us Weg, se lauf nit no!", "tokens": ["Und", "goht", "me", "der", "us", "Weg", ",", "se", "lauf", "nit", "no", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "ADJA", "NN", "$,", "ADJA", "NN", "PTKNEG", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gang diner Wege furt in Gottis Gleit!", "tokens": ["Gang", "di\u00b7ner", "We\u00b7ge", "furt", "in", "Got\u00b7tis", "Gleit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "VVFIN", "APPR", "NE", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "\u2019s isch Uhverstand, me merkts enanderno,", "tokens": ["'s", "isch", "Uh\u00b7ver\u00b7stand", ",", "me", "merkts", "en\u00b7an\u00b7der\u00b7no", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "$,", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "und \u2019s git en Unehr; sag i heig ders gseit!", "tokens": ["und", "'s", "git", "en", "Un\u00b7ehr", ";", "sag", "i", "heig", "ders", "gseit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "FM.la", "FM.la", "FM.la", "$.", "VVFIN", "NE", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}