{"dta.poem.19683": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Fuhrmannslied auf der Weinstrasse .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zieh, Schimmel, zieh!               ", "tokens": ["Zieh", ",", "Schim\u00b7mel", ",", "zieh", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Im Dreck bis an die Knie;", "tokens": ["Im", "Dreck", "bis", "an", "die", "Knie", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schieb dich fein in diesen Karren,", "tokens": ["Schieb", "dich", "fein", "in", "die\u00b7sen", "Kar\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wir wollen an den Neckar fahren.", "tokens": ["Wir", "wol\u00b7len", "an", "den", "Ne\u00b7ckar", "fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zieh, Schimmel, zieh!", "tokens": ["Zieh", ",", "Schim\u00b7mel", ",", "zieh", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Mein lieber Schimmel mein,", "tokens": ["Mein", "lie\u00b7ber", "Schim\u00b7mel", "mein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dort lad ich lauter Wein,", "tokens": ["Dort", "lad", "ich", "lau\u00b7ter", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Schimmel geht die Weinstra\u00df gern,", "tokens": ["Mein", "Schim\u00b7mel", "geht", "die", "Wein\u00b7stra\u00df", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hat's g'wi\u00df von seinem Herrn gelernt.", "tokens": ["Hat's", "g'\u00b7wi\u00df", "von", "sei\u00b7nem", "Herrn", "ge\u00b7lernt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Zieh, Schimmel, zieh!", "tokens": ["Zieh", ",", "Schim\u00b7mel", ",", "zieh", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Hot, Schimmel, hot, fein flugs!", "tokens": ["Hot", ",", "Schim\u00b7mel", ",", "hot", ",", "fein", "flugs", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "VVFIN", "$,", "ADJD", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mein Schimmel nicht zuruks,", "tokens": ["Mein", "Schim\u00b7mel", "nicht", "zu\u00b7ruks", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir m\u00fc\u00dfen durch den Strudel setzen,", "tokens": ["Wir", "m\u00fc\u00b7\u00dfen", "durch", "den", "Stru\u00b7del", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Schimmel d'mu\u00dft d'F\u00fc\u00df einnetzen.", "tokens": ["Mein", "Schim\u00b7mel", "d'\u00b7mu\u00dft", "d'\u00b7F\u00fc\u00df", "ein\u00b7net\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PDS", "VVIZU", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Zieh, Schimmel, zieh!", "tokens": ["Zieh", ",", "Schim\u00b7mel", ",", "zieh", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Setz an, Schimmel, setz an!", "tokens": ["Setz", "an", ",", "Schim\u00b7mel", ",", "setz", "an", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "NN", "$,", "VVIMP", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Spann alle Kr\u00e4ften d'ran!", "tokens": ["Spann", "al\u00b7le", "Kr\u00e4f\u00b7ten", "d'\u00b7ran", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Da giebts ein'n steinigen Holzweg 'nauf,", "tokens": ["Da", "giebts", "ein'n", "stei\u00b7ni\u00b7gen", "Holz\u00b7weg", "'nauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mein Schimmel da gilt's schnauffen d'rauf.", "tokens": ["Mein", "Schim\u00b7mel", "da", "gilt's", "schnauf\u00b7fen", "d'\u00b7rauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Zieh, Schimmel, zieh!", "tokens": ["Zieh", ",", "Schim\u00b7mel", ",", "zieh", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Adelich ist sein Natur,", "tokens": ["A\u00b7de\u00b7lich", "ist", "sein", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Er ist kein Bauern Gurr,", "tokens": ["Er", "ist", "kein", "Bau\u00b7ern", "Gurr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er ist nit l\u00e4ngst im Krieg g'wesen,", "tokens": ["Er", "ist", "nit", "l\u00e4ngst", "im", "Krieg", "g'\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ist auf ihm ein Hauptmann g'sessen;", "tokens": ["Und", "ist", "auf", "ihm", "ein", "Haupt\u00b7mann", "g'\u00b7ses\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Zieh, Schimmel, zieh!", "tokens": ["Zieh", ",", "Schim\u00b7mel", ",", "zieh", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Er war ein Kyrrisir,", "tokens": ["Er", "war", "ein", "Kyr\u00b7ri\u00b7sir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Bey Gott ein stolzes Thier,", "tokens": ["Bey", "Gott", "ein", "stol\u00b7zes", "Thier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Am Haupt trug er ein Federbuschen,", "tokens": ["Am", "Haupt", "trug", "er", "ein", "Fe\u00b7der\u00b7bu\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nahm ein, theilt aus viel guter Huschen,", "tokens": ["Nahm", "ein", ",", "theilt", "aus", "viel", "gu\u00b7ter", "Hu\u00b7schen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "VVFIN", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zieh, Schimmel, zieh!", "tokens": ["Zieh", ",", "Schim\u00b7mel", ",", "zieh", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Wenn es gab ein Gefecht,", "tokens": ["Wenn", "es", "gab", "ein", "Ge\u00b7fecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Zum Fliehen war er recht,", "tokens": ["Zum", "Flie\u00b7hen", "war", "er", "recht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wann man sich recht wollte wehren,", "tokens": ["Und", "wann", "man", "sich", "recht", "woll\u00b7te", "weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "PRF", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da ri\u00df er aus mit seinem Herren.", "tokens": ["Da", "ri\u00df", "er", "aus", "mit", "sei\u00b7nem", "Her\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zieh, Schimmel, zieh!", "tokens": ["Zieh", ",", "Schim\u00b7mel", ",", "zieh", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Mein Schimmel ist kein Narr,", "tokens": ["Mein", "Schim\u00b7mel", "ist", "kein", "Narr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wust wohl f\u00fcr wen er war,", "tokens": ["Wust", "wohl", "f\u00fcr", "wen", "er", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PWS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "W\u00e4r er nit l\u00e4ngst davon geflogen,", "tokens": ["W\u00e4r", "er", "nit", "l\u00e4ngst", "da\u00b7von", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So h\u00e4t man ihm den Pelz abzogen,", "tokens": ["So", "h\u00e4t", "man", "ihm", "den", "Pelz", "ab\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PPER", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zieh, Schimmel, zieh!", "tokens": ["Zieh", ",", "Schim\u00b7mel", ",", "zieh", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Truz allen Schimmeln truz,", "tokens": ["Truz", "al\u00b7len", "Schim\u00b7meln", "truz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "An ihm ist alles nutz,", "tokens": ["An", "ihm", "ist", "al\u00b7les", "nutz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich kann ihm alle Rippen z\u00e4hlen,", "tokens": ["Ich", "kann", "ihm", "al\u00b7le", "Rip\u00b7pen", "z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sehen wann ihm eins will zerschnellen,", "tokens": ["Und", "se\u00b7hen", "wann", "ihm", "eins", "will", "zer\u00b7schnel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "PWAV", "PPER", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Zieh, Schimmel, zieh!", "tokens": ["Zieh", ",", "Schim\u00b7mel", ",", "zieh", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "Er hat ein gleichen Schritt,", "tokens": ["Er", "hat", "ein", "glei\u00b7chen", "Schritt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "F\u00e4llt nur den vierten Tritt,", "tokens": ["F\u00e4llt", "nur", "den", "vier\u00b7ten", "Tritt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn er stolz will gallopiren", "tokens": ["Und", "wenn", "er", "stolz", "will", "gal\u00b7lo\u00b7pi\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VMFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So geht er auf dem Maul spazieren;", "tokens": ["So", "geht", "er", "auf", "dem", "Maul", "spa\u00b7zie\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zieh, Schimmel, zieh!", "tokens": ["Zieh", ",", "Schim\u00b7mel", ",", "zieh", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.11": {"line.1": {"text": "Ein recht dem\u00fcthig Pferd,", "tokens": ["Ein", "recht", "de\u00b7m\u00fct\u00b7hig", "Pferd", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "K\u00fc\u00dft oftermal die Erd,", "tokens": ["K\u00fc\u00dft", "of\u00b7ter\u00b7mal", "die", "Erd", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er taugt gar wohl zu Rittert\u00e4nzen", "tokens": ["Er", "taugt", "gar", "wohl", "zu", "Rit\u00b7ter\u00b7t\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ist gut zu den Reverenzen,", "tokens": ["Und", "ist", "gut", "zu", "den", "Re\u00b7ve\u00b7ren\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zieh, Schimmel, zieh!", "tokens": ["Zieh", ",", "Schim\u00b7mel", ",", "zieh", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.12": {"line.1": {"text": "Jezt wird er allgemach,", "tokens": ["Jezt", "wird", "er", "all\u00b7ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein kleines R\u00f6\u00dflein schwach,", "tokens": ["Ein", "klei\u00b7nes", "R\u00f6\u00df\u00b7lein", "schwach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er kann kein Offizier mehr tragen,", "tokens": ["Er", "kann", "kein", "Of\u00b7fi\u00b7zier", "mehr", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch ist er recht in meinem Wagen.", "tokens": ["Doch", "ist", "er", "recht", "in", "mei\u00b7nem", "Wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zieh, Schimmel, zieh!", "tokens": ["Zieh", ",", "Schim\u00b7mel", ",", "zieh", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.13": {"line.1": {"text": "Er ist noch wohlgestalt,", "tokens": ["Er", "ist", "noch", "wohl\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ist nit zu jung noch zu alt,", "tokens": ["Ist", "nit", "zu", "jung", "noch", "zu", "alt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PTKA", "ADJD", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Er ist mit meinem Weib geboren,", "tokens": ["Er", "ist", "mit", "mei\u00b7nem", "Weib", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hat erst den zehnten Zahn verlohren.", "tokens": ["Hat", "erst", "den", "zehn\u00b7ten", "Zahn", "ver\u00b7loh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zieh, Schimmel, zieh!", "tokens": ["Zieh", ",", "Schim\u00b7mel", ",", "zieh", "!"], "token_info": ["word", "punct", "wo