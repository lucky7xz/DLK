{"textgrid.poem.48221": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Butterstullenwerfen", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es fliegt ein Stein (die Hand warf ihn gut)", "tokens": ["Es", "fliegt", "ein", "Stein", "(", "die", "Hand", "warf", "ihn", "gut", ")"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "ART", "NN", "VVFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Kr\u00e4ftig, waagrecht \u00fcber die Flut.", "tokens": ["Kr\u00e4f\u00b7tig", ",", "waag\u00b7recht", "\u00fc\u00b7ber", "die", "Flut", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Eine S\u00e4ule steigt auf, und der Sonne Schein", "tokens": ["Ei\u00b7ne", "S\u00e4u\u00b7le", "steigt", "auf", ",", "und", "der", "Son\u00b7ne", "Schein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "KON", "ART", "NN", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Malt einen Regenbogen hinein.", "tokens": ["Malt", "ei\u00b7nen", "Re\u00b7gen\u00b7bo\u00b7gen", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Und weiter, ein zweites und drittes Mal,", "tokens": ["Und", "wei\u00b7ter", ",", "ein", "zwei\u00b7tes", "und", "drit\u00b7tes", "Mal", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ART", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Erhebt sich der siebenfarbige Strahl.", "tokens": ["Er\u00b7hebt", "sich", "der", "sie\u00b7ben\u00b7far\u00b7bi\u00b7ge", "Strahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Aber je weiter vom Ufer entfernt,", "tokens": ["A\u00b7ber", "je", "wei\u00b7ter", "vom", "U\u00b7fer", "ent\u00b7fernt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Der Stein im Fluge das Fliegen verlernt.", "tokens": ["Der", "Stein", "im", "Flu\u00b7ge", "das", "Flie\u00b7gen", "ver\u00b7lernt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Eine Schwere zieht ihn, es ebbt seine Kraft,", "tokens": ["Ei\u00b7ne", "Schwe\u00b7re", "zieht", "ihn", ",", "es", "ebbt", "sei\u00b7ne", "Kraft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Der Strahl ermattet und erschlafft.", "tokens": ["Der", "Strahl", "er\u00b7mat\u00b7tet", "und", "er\u00b7schlafft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ein Kr\u00e4useln noch einmal, ein Tropfen blinkt,", "tokens": ["Ein", "Kr\u00e4u\u00b7seln", "noch", "ein\u00b7mal", ",", "ein", "Trop\u00b7fen", "blinkt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und dann Ruh' und Stille \u2013 der Stein versinkt.", "tokens": ["Und", "dann", "Ruh'", "und", "Stil\u00b7le", "\u2013", "der", "Stein", "ver\u00b7sinkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "KON", "NN", "$(", "ART", "NN", "VVPP", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}