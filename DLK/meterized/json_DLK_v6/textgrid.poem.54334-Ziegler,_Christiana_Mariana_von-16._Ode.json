{"textgrid.poem.54334": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "16. Ode", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie fehlet doch die blinde Welt,", "tokens": ["Wie", "feh\u00b7let", "doch", "die", "blin\u00b7de", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In ihren abgeschmackten Schl\u00fcssen,", "tokens": ["In", "ih\u00b7ren", "ab\u00b7ge\u00b7schmack\u00b7ten", "Schl\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn sie die vor verschwendrisch h\u00e4lt,", "tokens": ["Wenn", "sie", "die", "vor", "ver\u00b7schwend\u00b7risch", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "APPR", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die nett und gro\u00df zu leben wissen?", "tokens": ["Die", "nett", "und", "gro\u00df", "zu", "le\u00b7ben", "wis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie liebt die Knicker nur allein,", "tokens": ["Sie", "liebt", "die", "Kni\u00b7cker", "nur", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und ehrt den greuelhaften Narren,", "tokens": ["Und", "ehrt", "den", "greu\u00b7el\u00b7haf\u00b7ten", "Nar\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der in den tiefsten Sand und Stein", "tokens": ["Der", "in", "den", "tiefs\u00b7ten", "Sand", "und", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Pflegt seinen Mammon zu verscharren.", "tokens": ["Pflegt", "sei\u00b7nen", "Mam\u00b7mon", "zu", "ver\u00b7schar\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach dummer P\u00f6bel! glaube nicht,", "tokens": ["Ach", "dum\u00b7mer", "P\u00f6\u00b7bel", "!", "glau\u00b7be", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$.", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df der sich Huld und Gunst erwecket,", "tokens": ["Da\u00df", "der", "sich", "Huld", "und", "Gunst", "er\u00b7we\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PRF", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der, auf das schn\u00f6de Geld erpicht,", "tokens": ["Der", ",", "auf", "das", "schn\u00f6\u00b7de", "Geld", "er\u00b7picht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In d\u00fcstre Winckel es verstecket;", "tokens": ["In", "d\u00fcst\u00b7re", "Win\u00b7ckel", "es", "ver\u00b7ste\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gr\u00e4bt man das Gold und Silber wohl", "tokens": ["Gr\u00e4bt", "man", "das", "Gold", "und", "Sil\u00b7ber", "wohl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ART", "NN", "KON", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Deswegen aus dem Schacht der Erden,", "tokens": ["Des\u00b7we\u00b7gen", "aus", "dem", "Schacht", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Damit es wieder heimlich soll", "tokens": ["Da\u00b7mit", "es", "wie\u00b7der", "heim\u00b7lich", "soll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In selbigen vergraben werden?", "tokens": ["In", "sel\u00b7bi\u00b7gen", "ver\u00b7gra\u00b7ben", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVPP", "VAINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Gar nicht; es l\u00e4\u00dft es die Natur", "tokens": ["Gar", "nicht", ";", "es", "l\u00e4\u00dft", "es", "die", "Na\u00b7tur"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "$.", "PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus den verborgnen Adern blitzen,", "tokens": ["Aus", "den", "ver\u00b7borg\u00b7nen", "A\u00b7dern", "blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Damit es nach entdeckter Spur,", "tokens": ["Da\u00b7mit", "es", "nach", "ent\u00b7deck\u00b7ter", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den B\u00fcrgern unsrer Welt soll n\u00fctzen.", "tokens": ["Den", "B\u00fcr\u00b7gern", "uns\u00b7rer", "Welt", "soll", "n\u00fct\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Thore hei\u00dft zwar Herr darvon;", "tokens": ["Der", "Tho\u00b7re", "hei\u00dft", "zwar", "Herr", "dar\u00b7von", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und doch wird ihm die Herrschaft mangeln,", "tokens": ["Und", "doch", "wird", "ihm", "die", "Herr\u00b7schaft", "man\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "So viel man auch von ferne schon", "tokens": ["So", "viel", "man", "auch", "von", "fer\u00b7ne", "schon"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PIS", "ADV", "APPR", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ihn t\u00e4glich sieht zusammen angeln.", "tokens": ["Ihn", "t\u00e4g\u00b7lich", "sieht", "zu\u00b7sam\u00b7men", "an\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ihr, die ihr fett und niedlich lebt;", "tokens": ["Ihr", ",", "die", "ihr", "fett", "und", "nied\u00b7lich", "lebt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So bald ihr aus den Federn schleichet,", "tokens": ["So", "bald", "ihr", "aus", "den", "Fe\u00b7dern", "schlei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Kehle s\u00fc\u00df Getr\u00e4nke gebt,", "tokens": ["Der", "Keh\u00b7le", "s\u00fc\u00df", "Ge\u00b7tr\u00e4n\u00b7ke", "gebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Gaumen Leckerbi\u00dfchen reichet;", "tokens": ["Dem", "Gau\u00b7men", "Le\u00b7cker\u00b7bi\u00df\u00b7chen", "rei\u00b7chet", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die ihr in weichen Kleidern stutzt,", "tokens": ["Die", "ihr", "in", "wei\u00b7chen", "Klei\u00b7dern", "stutzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kehrt euch nicht an der Welt ihr L\u00e4stern,", "tokens": ["Kehrt", "euch", "nicht", "an", "der", "Welt", "ihr", "L\u00e4s\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zeigt, da\u00df ihr euch weit besser putzt,", "tokens": ["Zeigt", ",", "da\u00df", "ihr", "euch", "weit", "bes\u00b7ser", "putzt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PPER", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Weit besser e\u00dft und trinkt als gestern!", "tokens": ["Weit", "bes\u00b7ser", "e\u00dft", "und", "trinkt", "als", "ge\u00b7stern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVFIN", "KON", "VVFIN", "KOKOM", "ADV", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Nur immer wacker drauf gezehrt!", "tokens": ["Nur", "im\u00b7mer", "wa\u00b7cker", "drauf", "ge\u00b7zehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "PAV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Fach wird noch in langen Jahren", "tokens": ["Das", "Fach", "wird", "noch", "in", "lan\u00b7gen", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht auf ein Drittheil ausgeleert,", "tokens": ["Nicht", "auf", "ein", "Dritt\u00b7heil", "aus\u00b7ge\u00b7leert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr k\u00f6nnt nicht auf den Boden fahren;", "tokens": ["Ihr", "k\u00f6nnt", "nicht", "auf", "den", "Bo\u00b7den", "fah\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Kasten ist zu tief und gro\u00df;", "tokens": ["Der", "Kas\u00b7ten", "ist", "zu", "tief", "und", "gro\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKA", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wollt ihr gleich alle Stunden greifen;", "tokens": ["Wollt", "ihr", "gleich", "al\u00b7le", "Stun\u00b7den", "grei\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ihr werdet doch das Geld nicht los,", "tokens": ["Ihr", "wer\u00b7det", "doch", "das", "Geld", "nicht", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Es wird sich immer wieder h\u00e4ufen.", "tokens": ["Es", "wird", "sich", "im\u00b7mer", "wie\u00b7der", "h\u00e4u\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Es kam euch ja nicht sauer an:", "tokens": ["Es", "kam", "euch", "ja", "nicht", "sau\u00b7er", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr habt es nicht durch M\u00fch erworben;", "tokens": ["Ihr", "habt", "es", "nicht", "durch", "M\u00fch", "er\u00b7wor\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das, was bisher euch sanft gethan,", "tokens": ["Das", ",", "was", "bis\u00b7her", "euch", "sanft", "ge\u00b7than", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "ADV", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist euch von andern zugestorben.", "tokens": ["Ist", "euch", "von", "an\u00b7dern", "zu\u00b7ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Entschlafner, theurer, lieber Greis!", "tokens": ["Ent\u00b7schlaf\u00b7ner", ",", "theu\u00b7rer", ",", "lie\u00b7ber", "Greis", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du w\u00fcrdest selbst im Grabe lachen,", "tokens": ["Du", "w\u00fcr\u00b7dest", "selbst", "im", "Gra\u00b7be", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn sich dein Erb auf deinen Schwei\u00df", "tokens": ["Wenn", "sich", "dein", "Erb", "auf", "dei\u00b7nen", "Schwei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nicht sollt ein frohes St\u00fcndchen machen.", "tokens": ["Nicht", "sollt", "ein", "fro\u00b7hes", "St\u00fcnd\u00b7chen", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Gar recht; wozu soll das Metall", "tokens": ["Gar", "recht", ";", "wo\u00b7zu", "soll", "das", "Me\u00b7tall"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$.", "PWAV", "VMFIN", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Im Kasten schimmeln, und verrosten?", "tokens": ["Im", "Kas\u00b7ten", "schim\u00b7meln", ",", "und", "ver\u00b7ros\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was w\u00fcrde dies auf solchen Fall", "tokens": ["Was", "w\u00fcr\u00b7de", "dies", "auf", "sol\u00b7chen", "Fall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PDS", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht wieder auszuputzen kosten?", "tokens": ["Nicht", "wie\u00b7der", "aus\u00b7zu\u00b7put\u00b7zen", "kos\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVIZU", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die M\u00e4nner schnappen nach der Luft;", "tokens": ["Die", "M\u00e4n\u00b7ner", "schnap\u00b7pen", "nach", "der", "Luft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr Harnisch dr\u00fcckt zu sehr die H\u00fcften.", "tokens": ["Ihr", "Har\u00b7nisch", "dr\u00fcckt", "zu", "sehr", "die", "H\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Entrei\u00dft sie Moder Qualm, und Duft,", "tokens": ["Ent\u00b7rei\u00dft", "sie", "Mo\u00b7der", "Qualm", ",", "und", "Duft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Was k\u00f6nntet ihr wohl bessers stiften?", "tokens": ["Was", "k\u00f6nn\u00b7tet", "ihr", "wohl", "bes\u00b7sers", "stif\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Erl\u00f6set, die so lange Zeit", "tokens": ["Er\u00b7l\u00f6\u00b7set", ",", "die", "so", "lan\u00b7ge", "Zeit"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PRELS", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Kerker eingeschlossen waren,", "tokens": ["Im", "Ker\u00b7ker", "ein\u00b7ge\u00b7schlos\u00b7sen", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00dft, wie der Himmel selbst gebeuth,", "tokens": ["La\u00dft", ",", "wie", "der", "Him\u00b7mel", "selbst", "ge\u00b7beuth", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PWAV", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Brodt stets \u00fcbers Wasser fahren.", "tokens": ["Das", "Brodt", "stets", "\u00fc\u00b7bers", "Was\u00b7ser", "fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wohl euch, da\u00df ihr das Herze nicht", "tokens": ["Wohl", "euch", ",", "da\u00df", "ihr", "das", "Her\u00b7ze", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "$,", "KOUS", "PPER", "PDS", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "An solchen Schlamm und Koth la\u00dft kleben,", "tokens": ["An", "sol\u00b7chen", "Schlamm", "und", "Koth", "la\u00dft", "kle\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Man kann euch, wenn das Auge bricht,", "tokens": ["Man", "kann", "euch", ",", "wenn", "das", "Au\u00b7ge", "bricht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Doch nichts davon ins Grab mitgeben.", "tokens": ["Doch", "nichts", "da\u00b7von", "ins", "Grab", "mit\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PAV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Seht andre lockre Seelen an,", "tokens": ["Seht", "and\u00b7re", "lock\u00b7re", "See\u00b7len", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie die sich wissen aufzuf\u00fchren,", "tokens": ["Wie", "die", "sich", "wis\u00b7sen", "auf\u00b7zu\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRELS", "PRF", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hier wahrlich ist vom reichen Mann", "tokens": ["Hier", "wahr\u00b7lich", "ist", "vom", "rei\u00b7chen", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein rechtes Ebenbild zu sp\u00fchren.", "tokens": ["Ein", "rech\u00b7tes", "E\u00b7ben\u00b7bild", "zu", "sp\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie herrlich lebet nicht Sempron,", "tokens": ["Wie", "herr\u00b7lich", "le\u00b7bet", "nicht", "Semp\u00b7ron", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PTKNEG", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Der aller Augen auf sich wendet?", "tokens": ["Der", "al\u00b7ler", "Au\u00b7gen", "auf", "sich", "wen\u00b7det", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Was schm\u00e4ht ihr? Meynt ihr, da\u00df er schon", "tokens": ["Was", "schm\u00e4ht", "ihr", "?", "Meynt", "ihr", ",", "da\u00df", "er", "schon"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$.", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sein Haab und Guth bereits verschwendet?", "tokens": ["Sein", "Ha\u00b7ab", "und", "Guth", "be\u00b7reits", "ver\u00b7schwen\u00b7det", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Ihr thut ihm Unrecht; irr ich nicht,", "tokens": ["Ihr", "thut", "ihm", "Un\u00b7recht", ";", "irr", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$.", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So lebet er ganz eingezogen,", "tokens": ["So", "le\u00b7bet", "er", "ganz", "ein\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was man von seinem Prassen spricht,", "tokens": ["Was", "man", "von", "sei\u00b7nem", "Pras\u00b7sen", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist, mit Erlaubni\u00df, gar erlogen.", "tokens": ["Ist", ",", "mit", "Er\u00b7laub\u00b7ni\u00df", ",", "gar", "er\u00b7lo\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "APPR", "NN", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hier ist kein Aufgang, wie man sieht,", "tokens": ["Hier", "ist", "kein", "Auf\u00b7gang", ",", "wie", "man", "sieht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr k\u00f6nnt aus Tracht und Speise schliessen,", "tokens": ["Ihr", "k\u00f6nnt", "aus", "Tracht", "und", "Spei\u00b7se", "schlies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wie sehr er Pracht und Aufwand flieht,", "tokens": ["Wie", "sehr", "er", "Pracht", "und", "Auf\u00b7wand", "flieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sein Gut vern\u00fcnftig zu geniessen.", "tokens": ["Sein", "Gut", "ver\u00b7n\u00fcnf\u00b7tig", "zu", "ge\u00b7nies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Betr\u00fcgt mich nicht ein falscher Schein,", "tokens": ["Be\u00b7tr\u00fcgt", "mich", "nicht", "ein", "fal\u00b7scher", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So seh ich ihn vor Flei\u00df recht schwitzen,", "tokens": ["So", "seh", "ich", "ihn", "vor", "Flei\u00df", "recht", "schwit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und in dem Zimmer ganz allein", "tokens": ["Und", "in", "dem", "Zim\u00b7mer", "ganz", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit unterst\u00fctztem Haupte sitzen.", "tokens": ["Mit", "un\u00b7ter\u00b7st\u00fctz\u00b7tem", "Haup\u00b7te", "sit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was sinnt er doch? was liest er wohl?", "tokens": ["Was", "sinnt", "er", "doch", "?", "was", "liest", "er", "wohl", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$.", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ach! es sich des Plutarchus Lehren,", "tokens": ["Ach", "!", "es", "sich", "des", "Plu\u00b7tar\u00b7chus", "Leh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "PRF", "ART", "NE", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Wie man recht sparsam leben soll,", "tokens": ["Wie", "man", "recht", "spar\u00b7sam", "le\u00b7ben", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Um die Verschwendung abzuwehren.", "tokens": ["Um", "die", "Ver\u00b7schwen\u00b7dung", "ab\u00b7zu\u00b7weh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Ja, ja, er liest; doch nun zu sp\u00e4t,", "tokens": ["Ja", ",", "ja", ",", "er", "liest", ";", "doch", "nun", "zu", "sp\u00e4t", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PPER", "VVFIN", "$.", "ADV", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O, h\u00e4tt er doch vor langen Tagen", "tokens": ["O", ",", "h\u00e4tt", "er", "doch", "vor", "lan\u00b7gen", "Ta\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das, was hier aufgezeichnet steht,", "tokens": ["Das", ",", "was", "hier", "auf\u00b7ge\u00b7zeich\u00b7net", "steht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "ADV", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Statt einer Vorschrift aufgeschlagen.", "tokens": ["Statt", "ei\u00b7ner", "Vor\u00b7schrift", "auf\u00b7ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Des Weisen Rath hilft ihm nicht viel;", "tokens": ["Des", "Wei\u00b7sen", "Rath", "hilft", "ihm", "nicht", "viel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da nun die V\u00f6gel ausgeflogen,", "tokens": ["Da", "nun", "die", "V\u00f6\u00b7gel", "aus\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So wird bey diesem Trauerspiel", "tokens": ["So", "wird", "bey", "die\u00b7sem", "Trau\u00b7er\u00b7spiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Umsonst der Keficht zugezogen.", "tokens": ["Um\u00b7sonst", "der", "Ke\u00b7ficht", "zu\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Du, der du noch bey guter Zeit", "tokens": ["Du", ",", "der", "du", "noch", "bey", "gu\u00b7ter", "Zeit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stets herrlich und in Freuden sassest,", "tokens": ["Stets", "herr\u00b7lich", "und", "in", "Freu\u00b7den", "sas\u00b7sest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch bey dergleichen Herrlichkeit", "tokens": ["Doch", "bey", "derg\u00b7lei\u00b7chen", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Schlu\u00df der Rechnung gar vergassest,", "tokens": ["Den", "Schlu\u00df", "der", "Rech\u00b7nung", "gar", "ver\u00b7gas\u00b7sest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sag uns, was denkst du wohl bey dir,", "tokens": ["Sag", "uns", ",", "was", "denkst", "du", "wohl", "bey", "dir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PWS", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da sich die Scenen so verdrehen,", "tokens": ["Da", "sich", "die", "Sce\u00b7nen", "so", "ver\u00b7dre\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und man dich kahl und nackend hier", "tokens": ["Und", "man", "dich", "kahl", "und", "na\u00b7ckend", "hier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "PRF", "ADJD", "KON", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So, wie des Plato Hahn sieht stehen.", "tokens": ["So", ",", "wie", "des", "Pla\u00b7to", "Hahn", "sieht", "ste\u00b7hen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NE", "NE", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Wie schmecken dir, Verschwender, nun", "tokens": ["Wie", "schme\u00b7cken", "dir", ",", "Ver\u00b7schwen\u00b7der", ",", "nun"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "NN", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die schmahl und mehr als magern Bissen,", "tokens": ["Die", "schmahl", "und", "mehr", "als", "ma\u00b7gern", "Bis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PIAT", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auf ein gespicktes Haselhuhn,", "tokens": ["Auf", "ein", "ge\u00b7spick\u00b7tes", "Ha\u00b7sel\u00b7huhn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das deinen Z\u00e4hnen wird entrissen?", "tokens": ["Das", "dei\u00b7nen", "Z\u00e4h\u00b7nen", "wird", "ent\u00b7ris\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Schleicht nach dem zuckers\u00fcssen Wein,", "tokens": ["Schleicht", "nach", "dem", "zu\u00b7cker\u00b7s\u00fcs\u00b7sen", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bey dem man dich sah t\u00e4glich lachen,", "tokens": ["Bey", "dem", "man", "dich", "sah", "t\u00e4g\u00b7lich", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIS", "PPER", "VVFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der strenge Trank auch glatt hinein,", "tokens": ["Der", "stren\u00b7ge", "Trank", "auch", "glatt", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der deine Kehle rauh will machen?", "tokens": ["Der", "dei\u00b7ne", "Keh\u00b7le", "rauh", "will", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "O herber Wechsel, welcher dich", "tokens": ["O", "her\u00b7ber", "Wech\u00b7sel", ",", "wel\u00b7cher", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "PRELS", "PPER"], "meter": "-+-+-+-+", "