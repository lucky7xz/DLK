{"textgrid.poem.29741": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "156.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bella ist ein schwartz Magnet, der das Eisen an sich zeucht;", "tokens": ["Be\u00b7lla", "ist", "ein", "schwartz", "Mag\u00b7net", ",", "der", "das", "Ei\u00b7sen", "an", "sich", "zeucht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJD", "NN", "$,", "PRELS", "ART", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Varna ist ein wei\u00df Magnet, der das Eisen immer fleucht.", "tokens": ["Var\u00b7na", "ist", "ein", "wei\u00df", "Mag\u00b7net", ",", "der", "das", "Ei\u00b7sen", "im\u00b7mer", "fleucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "VVFIN", "NE", "$,", "PRELS", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Bella liebt nicht, wird geliebt; Varna liebt, wird nicht geliebt;", "tokens": ["Be\u00b7lla", "liebt", "nicht", ",", "wird", "ge\u00b7liebt", ";", "Var\u00b7na", "liebt", ",", "wird", "nicht", "ge\u00b7liebt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "$,", "VAFIN", "VVPP", "$.", "NE", "VVFIN", "$,", "VAFIN", "PTKNEG", "VVPP", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Jene gibt nicht, wann sie nimmt; diese nimmt nicht, wann sie gibt.", "tokens": ["Je\u00b7ne", "gibt", "nicht", ",", "wann", "sie", "nimmt", ";", "die\u00b7se", "nimmt", "nicht", ",", "wann", "sie", "gibt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$.", "PDS", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+------+-+-+", "measure": "unknown.measure.penta"}}}}}