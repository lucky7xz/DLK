{"textgrid.poem.48427": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "5. Auf Herrn Garlef L\u00fcders und Jungfrau Margarethen Brauns Hochzeit in Mo\u00dfkow", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr, die ihr Nacht und Tag auf Lieben zu gedenken", "tokens": ["Ihr", ",", "die", "ihr", "Nacht", "und", "Tag", "auf", "Lie\u00b7ben", "zu", "ge\u00b7den\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PPOSAT", "NN", "KON", "NN", "APPR", "ADJA", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und euren matten Sinn mit Sehnen pflegt zu kr\u00e4nken", "tokens": ["und", "eu\u00b7ren", "mat\u00b7ten", "Sinn", "mit", "Seh\u00b7nen", "pflegt", "zu", "kr\u00e4n\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VVFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "nach jener sch\u00f6nen Zeit, bis da\u00df die s\u00fc\u00dfe Lust,", "tokens": ["nach", "je\u00b7ner", "sch\u00f6\u00b7nen", "Zeit", ",", "bis", "da\u00df", "die", "s\u00fc\u00b7\u00dfe", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,", "KON", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "von der ihr mir erz\u00e4hlt, als das euch ist bewust,", "tokens": ["von", "der", "ihr", "mir", "er\u00b7z\u00e4hlt", ",", "als", "das", "euch", "ist", "be\u00b7wust", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPER", "VVFIN", "$,", "KOUS", "ART", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "auch euch einst stehe frei; setzt, ihr verwirrten Leute,", "tokens": ["auch", "euch", "einst", "ste\u00b7he", "frei", ";", "setzt", ",", "ihr", "ver\u00b7wirr\u00b7ten", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VVFIN", "ADJD", "$.", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "di\u00df, was ihr doch nicht habt, ein wenig auf die Seite,", "tokens": ["di\u00df", ",", "was", "ihr", "doch", "nicht", "habt", ",", "ein", "we\u00b7nig", "auf", "die", "Sei\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "ADV", "PTKNEG", "VAFIN", "$,", "ART", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "vergn\u00fcgt euch selbsten euch! La\u00dft euren eiteln Wahn,", "tokens": ["ver\u00b7gn\u00fcgt", "euch", "selbs\u00b7ten", "euch", "!", "La\u00dft", "eu\u00b7ren", "ei\u00b7teln", "Wahn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "PPER", "$.", "VVIMP", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "und seht um so viel mehr die beiden Lieben an!", "tokens": ["und", "seht", "um", "so", "viel", "mehr", "die", "bei\u00b7den", "Lie\u00b7ben", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADV", "ADV", "ADV", "ART", "PIAT", "ADJA", "PTKVZ", "$."], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Schaut an di\u00df fromme Paar, di\u00df Paar, so zwei an Namen", "tokens": ["Schaut", "an", "di\u00df", "from\u00b7me", "Paar", ",", "di\u00df", "Paar", ",", "so", "zwei", "an", "Na\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PDS", "ADJA", "NN", "$,", "PDS", "NN", "$,", "ADV", "CARD", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "und eins an Herzen ist, das wahrer Liebe Samen", "tokens": ["und", "eins", "an", "Her\u00b7zen", "ist", ",", "das", "wah\u00b7rer", "Lie\u00b7be", "Sa\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PIS", "APPR", "NN", "VAFIN", "$,", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "aus seinen Augen streut und in zwei Herzen s\u00e4et,", "tokens": ["aus", "sei\u00b7nen", "Au\u00b7gen", "streut", "und", "in", "zwei", "Her\u00b7zen", "s\u00e4et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "KON", "APPR", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "darin es allezeit in voller Bl\u00fcte steht", "tokens": ["da\u00b7rin", "es", "al\u00b7le\u00b7zeit", "in", "vol\u00b7ler", "Bl\u00fc\u00b7te", "steht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "ADV", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "und tausent Fr\u00fcchte tr\u00e4gt! Die Furcht, die Qual der Sinnen,", "tokens": ["und", "tau\u00b7sent", "Fr\u00fcch\u00b7te", "tr\u00e4gt", "!", "Die", "Furcht", ",", "die", "Qual", "der", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "VVFIN", "$.", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "das angstgef\u00fcllte Kind der b\u00f6sen Erebinnen,", "tokens": ["das", "angst\u00b7ge\u00b7f\u00fcll\u00b7te", "Kind", "der", "b\u00f6\u00b7sen", "E\u00b7re\u00b7bin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "hat ihren Tod erlebt. Die seufzende Begier,", "tokens": ["hat", "ih\u00b7ren", "Tod", "er\u00b7lebt", ".", "Die", "seuf\u00b7zen\u00b7de", "Be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "die Tochter des Averns, die hat ein Ende hier.", "tokens": ["die", "Toch\u00b7ter", "des", "A\u00b7verns", ",", "die", "hat", "ein", "En\u00b7de", "hier", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die m\u00fcde Hoffnung stirbt, das reizende Verlangen,", "tokens": ["Die", "m\u00fc\u00b7de", "Hoff\u00b7nung", "stirbt", ",", "das", "rei\u00b7zen\u00b7de", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.18": {"text": "das hat auf diesen Tag ganz seinen Rest empfangen.", "tokens": ["das", "hat", "auf", "die\u00b7sen", "Tag", "ganz", "sei\u00b7nen", "Rest", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PDAT", "NN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Kein Harren harret mehr. Das matte Sehnen liegt", "tokens": ["Kein", "Har\u00b7ren", "har\u00b7ret", "mehr", ".", "Das", "mat\u00b7te", "Seh\u00b7nen", "liegt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "und tut den letzten Zug. Sie haben obgesiegt,", "tokens": ["und", "tut", "den", "letz\u00b7ten", "Zug", ".", "Sie", "ha\u00b7ben", "ob\u00b7ge\u00b7siegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$.", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "die Beide, wie ihr seht. Lauft aus, ihr kleinen G\u00f6tter,", "tokens": ["die", "Bei\u00b7de", ",", "wie", "ihr", "seht", ".", "Lauft", "aus", ",", "ihr", "klei\u00b7nen", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$.", "NN", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "ihr Eivolk, Paphos Ruhm, bringt frische Myrtenbl\u00e4tter!", "tokens": ["ihr", "Ei\u00b7volk", ",", "Pa\u00b7phos", "Ruhm", ",", "bringt", "fri\u00b7sche", "Myr\u00b7ten\u00b7bl\u00e4t\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NE", "NN", "$,", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Du Amathunta, flecht in ihr w\u00fcrdiges Haar", "tokens": ["Du", "A\u00b7mat\u00b7hun\u00b7ta", ",", "flecht", "in", "ihr", "w\u00fcr\u00b7di\u00b7ges", "Haar"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "NE", "$,", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.24": {"text": "die Krone, die du machst f\u00fcr deiner Fechter Schar!", "tokens": ["die", "Kro\u00b7ne", ",", "die", "du", "machst", "f\u00fcr", "dei\u00b7ner", "Fech\u00b7ter", "Schar", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Der Ruhm folgt auf den Sieg. Ihr weichen Najadinnen,", "tokens": ["Der", "Ruhm", "folgt", "auf", "den", "Sieg", ".", "Ihr", "wei\u00b7chen", "Na\u00b7ja\u00b7din\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$.", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "eilt, was ihr eilen k\u00f6nnt, zu euren k\u00fchlen Br\u00fcnnen,", "tokens": ["eilt", ",", "was", "ihr", "ei\u00b7len", "k\u00f6nnt", ",", "zu", "eu\u00b7ren", "k\u00fch\u00b7len", "Br\u00fcn\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "PPER", "VVFIN", "VVFIN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "holt frische Schmergel her, pflockt Quendel, Pol und Klee!", "tokens": ["holt", "fri\u00b7sche", "Schmer\u00b7gel", "her", ",", "pflockt", "Quen\u00b7del", ",", "Pol", "und", "Klee", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "PTKVZ", "$,", "VVFIN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Ihr, Nap\u00e4innen, bringt, was her komt \u00fcber See,", "tokens": ["Ihr", ",", "Na\u00b7p\u00e4in\u00b7nen", ",", "bringt", ",", "was", "her", "komt", "\u00fc\u00b7ber", "See", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "$,", "PRELS", "APZR", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.29": {"text": "gebt fremde Tulpen her! Geh, Klio, mit Melposen,", "tokens": ["gebt", "frem\u00b7de", "Tul\u00b7pen", "her", "!", "Geh", ",", "Klio", ",", "mit", "Mel\u00b7po\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "PTKVZ", "$.", "NE", "$,", "NE", "$,", "APPR", "NE", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "reuft Hiazynthen aus, brecht volle Zuckerrosen", "tokens": ["reuft", "Hia\u00b7zyn\u00b7then", "aus", ",", "brecht", "vol\u00b7le", "Zu\u00b7cker\u00b7ro\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NN", "PTKVZ", "$,", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.31": {"text": "und keusche Lilgen ab! Lest, was nach Pestum reucht", "tokens": ["und", "keu\u00b7sche", "Lil\u00b7gen", "ab", "!", "Lest", ",", "was", "nach", "Pes\u00b7tum", "reucht"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "PTKVZ", "$.", "NE", "$,", "PRELS", "APPR", "NE", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "und was Alcinous und Flora Sch\u00f6nes zeugt!", "tokens": ["und", "was", "Al\u00b7ci\u00b7nous", "und", "Flo\u00b7ra", "Sch\u00f6\u00b7nes", "zeugt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "NE", "KON", "NE", "NE", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.33": {"text": "Pflockt alle K\u00f6rbe voll! Eilt, laufet um die Wette,", "tokens": ["Pflockt", "al\u00b7le", "K\u00f6r\u00b7be", "voll", "!", "Eilt", ",", "lau\u00b7fet", "um", "die", "Wet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADJD", "$.", "VVFIN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "komt, bringt und sch\u00fcttet aus! Hier wollen wir ein Bette", "tokens": ["komt", ",", "bringt", "und", "sch\u00fct\u00b7tet", "aus", "!", "Hier", "wol\u00b7len", "wir", "ein", "Bet\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "KON", "VVFIN", "PTKVZ", "$.", "ADV", "VMFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "von Farben bauen auf. Sie, aller Blumen Schein,", "tokens": ["von", "Far\u00b7ben", "bau\u00b7en", "auf", ".", "Sie", ",", "al\u00b7ler", "Blu\u00b7men", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PTKVZ", "$.", "PPER", "$,", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "die sollen ganz und gar verdeckt mit Blumen sein.", "tokens": ["die", "sol\u00b7len", "ganz", "und", "gar", "ver\u00b7deckt", "mit", "Blu\u00b7men", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "KON", "ADV", "ADJD", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Di\u00df ist des Br\u00e4utgams Lohn f\u00fcr seine hohe Gaben,", "tokens": ["Di\u00df", "ist", "des", "Br\u00e4ut\u00b7gams", "Lohn", "f\u00fcr", "sei\u00b7ne", "ho\u00b7he", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NE", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "er will sonst keinen Dank, als von der Liebsten haben.", "tokens": ["er", "will", "sonst", "kei\u00b7nen", "Dank", ",", "als", "von", "der", "Liebs\u00b7ten", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIAT", "NN", "$,", "KOUS", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Sein ausgelehrter Mund, der redet, was er will,", "tokens": ["Sein", "aus\u00b7ge\u00b7lehr\u00b7ter", "Mund", ",", "der", "re\u00b7det", ",", "was", "er", "will", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PRELS", "VVFIN", "$,", "PWS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "