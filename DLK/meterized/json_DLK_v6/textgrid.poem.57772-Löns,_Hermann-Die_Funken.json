{"textgrid.poem.57772": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Die Funken", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wenn das Feuer brennt,", "tokens": ["Und", "wenn", "das", "Feu\u00b7er", "brennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dann fliegen Funken,", "tokens": ["Dann", "flie\u00b7gen", "Fun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ich hatte einen Stern,", "tokens": ["Ich", "hat\u00b7te", "ei\u00b7nen", "Stern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Er ist versunken;", "tokens": ["Er", "ist", "ver\u00b7sun\u00b7ken", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Er ist versunken in der dunklen Nacht,", "tokens": ["Er", "ist", "ver\u00b7sun\u00b7ken", "in", "der", "dunk\u00b7len", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und ich mu\u00df weinen, weil kein Stern mir lacht.", "tokens": ["Und", "ich", "mu\u00df", "wei\u00b7nen", ",", "weil", "kein", "Stern", "mir", "lacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "VVINF", "$,", "KOUS", "PIAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das rote Feuer brennt,", "tokens": ["Das", "ro\u00b7te", "Feu\u00b7er", "brennt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Funken stieben,", "tokens": ["Die", "Fun\u00b7ken", "stie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und dann verl\u00f6schen sie,", "tokens": ["Und", "dann", "ver\u00b7l\u00f6\u00b7schen", "sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So wie mein Lieben;", "tokens": ["So", "wie", "mein", "Lie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPOSAT", "ADJA", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Mein Lieben ist dahin in Nacht und Leid,", "tokens": ["Mein", "Lie\u00b7ben", "ist", "da\u00b7hin", "in", "Nacht", "und", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VAFIN", "PAV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Als wie ein Funken in der Dunkelheit.", "tokens": ["Als", "wie", "ein", "Fun\u00b7ken", "in", "der", "Dun\u00b7kel\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das Feuer brennt nicht mehr,", "tokens": ["Das", "Feu\u00b7er", "brennt", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Es ist gestorben,", "tokens": ["Es", "ist", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ich hatte einen Traum,", "tokens": ["Ich", "hat\u00b7te", "ei\u00b7nen", "Traum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Er ist verdorben;", "tokens": ["Er", "ist", "ver\u00b7dor\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Er ist verdorben und er ist verbl\u00fcht,", "tokens": ["Er", "ist", "ver\u00b7dor\u00b7ben", "und", "er", "ist", "ver\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das Feuer brennt nicht mehr, es ist vergl\u00fcht.", "tokens": ["Das", "Feu\u00b7er", "brennt", "nicht", "mehr", ",", "es", "ist", "ver\u00b7gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}