{"textgrid.poem.34518": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fort denn! Hinaus! Und all das Spiel", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fort denn! Hinaus! Und all das Spiel", "tokens": ["Fort", "denn", "!", "Hin\u00b7aus", "!", "Und", "all", "das", "Spiel"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "KON", "$.", "ADV", "$.", "KON", "PIAT", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "es hab mit Eins sein End!", "tokens": ["es", "hab", "mit", "Eins", "sein", "End", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und z\u00fcngeln noch der Fl\u00e4mmchen viel,", "tokens": ["Und", "z\u00fcn\u00b7geln", "noch", "der", "Fl\u00e4mm\u00b7chen", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "gel\u00f6scht wird, was da brennt.", "tokens": ["ge\u00b7l\u00f6scht", "wird", ",", "was", "da", "brennt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ja, wie ein Dieb, der schon zuviel", "tokens": ["Ja", ",", "wie", "ein", "Dieb", ",", "der", "schon", "zu\u00b7viel"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PWAV", "ART", "NN", "$,", "PRELS", "ADV", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und allzu frech sich stahl,", "tokens": ["und", "all\u00b7zu", "frech", "sich", "stahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKA", "ADJD", "PRF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "berg ich mich in der Welt Gew\u00fchl", "tokens": ["berg", "ich", "mich", "in", "der", "Welt", "Ge\u00b7w\u00fchl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "vor jener Augen Strahl.", "tokens": ["vor", "je\u00b7ner", "Au\u00b7gen", "Strahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Vor deinen Augen, Jessica,", "tokens": ["Vor", "dei\u00b7nen", "Au\u00b7gen", ",", "Jes\u00b7si\u00b7ca", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "die mich zu heiss umloht ...", "tokens": ["die", "mich", "zu", "heiss", "um\u00b7loht", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKA", "ADJD", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Fahr wohl! Jetzt ist ein Ende da:", "tokens": ["Fahr", "wohl", "!", "Jetzt", "ist", "ein", "En\u00b7de", "da", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "ADV", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "f\u00fcr dich bin ich nun tot.", "tokens": ["f\u00fcr", "dich", "bin", "ich", "nun", "tot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}