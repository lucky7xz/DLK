{"textgrid.poem.36844": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: La\u00dft mich schw\u00e4rmen und qu\u00e4let mich nicht; im Lande der Fabel", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00dft mich schw\u00e4rmen und qu\u00e4let mich nicht; im Lande der Fabel", "tokens": ["La\u00dft", "mich", "schw\u00e4r\u00b7men", "und", "qu\u00e4\u00b7let", "mich", "nicht", ";", "im", "Lan\u00b7de", "der", "Fa\u00b7bel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVFIN", "KON", "VVFIN", "PPER", "PTKNEG", "$.", "APPRART", "NN", "ART", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Leb' ich, so sei auch mein Herz, sei auch mein Lied ihr geweiht.", "tokens": ["Leb'", "ich", ",", "so", "sei", "auch", "mein", "Herz", ",", "sei", "auch", "mein", "Lied", "ihr", "ge\u00b7weiht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "ADV", "VAFIN", "ADV", "PPOSAT", "NN", "$,", "VAFIN", "ADV", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Bleibt in den Fesseln und glaubt was euch die Amme gelehret;", "tokens": ["Bleibt", "in", "den", "Fes\u00b7seln", "und", "glaubt", "was", "euch", "die", "Am\u00b7me", "ge\u00b7leh\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "PWS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Anderes aber bewegt mir den entbundenen Geist.", "tokens": ["An\u00b7de\u00b7res", "a\u00b7ber", "be\u00b7wegt", "mir", "den", "ent\u00b7bun\u00b7de\u00b7nen", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.5": {"text": "Dieser Boden, er trug der Offenbarungen jede;", "tokens": ["Die\u00b7ser", "Bo\u00b7den", ",", "er", "trug", "der", "Of\u00b7fen\u00b7ba\u00b7run\u00b7gen", "je\u00b7de", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "PIAT", "$."], "meter": "--+--+-+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Jupiter, Mahom und Christ glaubt' und verehrte man hier.", "tokens": ["Ju\u00b7pi\u00b7ter", ",", "Ma\u00b7hom", "und", "Christ", "glaubt'", "und", "ver\u00b7ehr\u00b7te", "man", "hier", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "KON", "NN", "VVFIN", "KON", "VVFIN", "PIS", "ADV", "$."], "meter": "+---+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.7": {"text": "Drum verarget mir nicht, wenn mir der Tempel Girgentis", "tokens": ["Drum", "ver\u00b7ar\u00b7get", "mir", "nicht", ",", "wenn", "mir", "der", "Tem\u00b7pel", "Gir\u00b7gen\u00b7tis"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "ART", "NN", "NE"], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Mehr als der maurische Dom Opfer und Andacht verdient;", "tokens": ["Mehr", "als", "der", "mau\u00b7ri\u00b7sche", "Dom", "Op\u00b7fer", "und", "An\u00b7dacht", "ver\u00b7dient", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "ADJA", "NN", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Wenn dein uranisches Wundergebild, Syrakus, wenn die G\u00f6ttin", "tokens": ["Wenn", "dein", "u\u00b7ra\u00b7ni\u00b7sches", "Wun\u00b7der\u00b7ge\u00b7bild", ",", "Sy\u00b7ra\u00b7kus", ",", "wenn", "die", "G\u00f6t\u00b7tin"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$,", "NE", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+--++-+--+-", "measure": "trochaic.octa.plus.relaxed"}, "line.10": {"text": "Mehr als das heilige Holz heute dem Auge gef\u00e4llt.", "tokens": ["Mehr", "als", "das", "hei\u00b7li\u00b7ge", "Holz", "heu\u00b7te", "dem", "Au\u00b7ge", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "ADJA", "NN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Dir gestatt' ich daf\u00fcr, da\u00df du deutschth\u00fcmlicher Salbung", "tokens": ["Dir", "ge\u00b7statt'", "ich", "da\u00b7f\u00fcr", ",", "da\u00df", "du", "deutscht\u00b7h\u00fcm\u00b7li\u00b7cher", "Sal\u00b7bung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PAV", "$,", "KOUS", "PPER", "ADJA", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Lebest f\u00fcr gothische Kunst, gothischen Glaubens erstirbst.", "tokens": ["Le\u00b7best", "f\u00fcr", "go\u00b7thi\u00b7sche", "Kunst", ",", "go\u00b7thi\u00b7schen", "Glau\u00b7bens", "er\u00b7stirbst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.13": {"text": "Erst ein Jude, dann Christ, erst Protestant, dann katholisch,", "tokens": ["Erst", "ein", "Ju\u00b7de", ",", "dann", "Christ", ",", "erst", "Pro\u00b7tes\u00b7tant", ",", "dann", "ka\u00b7tho\u00b7lisch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "NN", "$,", "ADV", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Wahrlich ein Heiland, doch erst will ich am Kreuze dich sehn.", "tokens": ["Wahr\u00b7lich", "ein", "Hei\u00b7land", ",", "doch", "erst", "will", "ich", "am", "Kreu\u00b7ze", "dich", "sehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "ADV", "VMFIN", "PPER", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}}}}}