{"dta.poem.579": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Allerhand sch\u00f6ne Rarit\u00e4ten und sch\u00f6ne  \n Spiele - Wercke/  \n Welche auf dem  \n Binnisch - und Gr\u00e4ntzischen  \n Hochzeit-Feste vorgestellet  \n der Teutsche Welsche Mann.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Seht welche Rarit\u00e4t! Hier liegt Hans Sachsens Hut/", "tokens": ["Seht", "wel\u00b7che", "Ra\u00b7ri\u00b7t\u00e4t", "!", "Hier", "liegt", "Hans", "Sach\u00b7sens", "Hut", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PWAT", "NN", "$.", "ADV", "VVFIN", "NE", "NE", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dort Eulenspiegels Rock und abgetragne Hosen/", "tokens": ["Dort", "Eu\u00b7len\u00b7spie\u00b7gels", "Rock", "und", "ab\u00b7ge\u00b7trag\u00b7ne", "Ho\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Marcolphi Narren-Kleid/ so annoch neu und gut/", "tokens": ["Mar\u00b7col\u00b7phi", "Nar\u00b7ren\u00b7Kleid", "/", "so", "an\u00b7noch", "neu", "und", "gut", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$(", "ADV", "ADV", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zehn Erben sind dabey so um dasselbe losen.", "tokens": ["Zehn", "Er\u00b7ben", "sind", "da\u00b7bey", "so", "um", "das\u00b7sel\u00b7be", "lo\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PAV", "ADV", "APPR", "PDAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nechst dem kommt ", "tokens": ["Nechst", "dem", "kommt"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PDS", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Mit mehr als einem Esels-Ohr.", "tokens": ["Mit", "mehr", "als", "ei\u00b7nem", "E\u00b7sels\u00b7Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sch\u00f6ne Rarit\u00e4t/ sch\u00f6ne Spiele-Werckerc.", "tokens": ["Sch\u00f6\u00b7ne", "Ra\u00b7ri\u00b7t\u00e4t", "/", "sch\u00f6\u00b7ne", "Spie\u00b7le\u00b7\u00b7Wer\u00b7ckerc", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "ADJA", "NN", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}}}}}