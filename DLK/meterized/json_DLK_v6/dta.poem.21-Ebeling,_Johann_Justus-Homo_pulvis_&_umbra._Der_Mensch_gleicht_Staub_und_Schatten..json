{"dta.poem.21": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Homo pulvis & umbra.  \n  Der Mensch gleicht Staub und  \n Schatten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin ein Mensch, ein d\u00fcnn\ngewebter Schatten,", "tokens": ["Ich", "bin", "ein", "Mensch", ",", "ein", "d\u00fcnn", "ge\u00b7web\u00b7ter", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ohn allen Wiederspruch kan ich", "tokens": ["Ohn", "al\u00b7len", "Wie\u00b7der\u00b7spruch", "kan", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VMFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Heist es nicht in der Schrift,", "tokens": ["Heist", "es", "nicht", "in", "der", "Schrift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "da\u00df ", "tokens": ["da\u00df"], "token_info": ["word"], "pos": ["KOUS"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Aus einer rothen Erd den Leib", "tokens": ["Aus", "ei\u00b7ner", "ro\u00b7then", "Erd", "den", "Leib"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht Adam nur allein, wir sind ja allzusammen,", "tokens": ["Nicht", "A\u00b7dam", "nur", "al\u00b7lein", ",", "wir", "sind", "ja", "all\u00b7zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ADV", "ADV", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nichts als ein eitler Staub; weil wir daraus her-", "tokens": ["Nichts", "als", "ein", "eit\u00b7ler", "Staub", ";", "weil", "wir", "da\u00b7raus", "her"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "KOKOM", "ART", "ADJA", "NN", "$.", "KOUS", "PPER", "PAV", "TRUNC"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Hebt sich die Sonn hinweg; so fliegt der Schat-", "tokens": ["Hebt", "sich", "die", "Sonn", "hin\u00b7weg", ";", "so", "fliegt", "der", "Schat"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN", "PTKVZ", "$.", "ADV", "VVFIN", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ein sch\u00f6nes Ebenbild! so gehts den Menschen auch;", "tokens": ["Ein", "sch\u00f6\u00b7nes", "E\u00b7ben\u00b7bild", "!", "so", "gehts", "den", "Men\u00b7schen", "auch", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ist er zuvor aus Erd, durchs Sch\u00f6pfers Macht", "tokens": ["Ist", "er", "zu\u00b7vor", "aus", "Erd", ",", "durchs", "Sch\u00f6p\u00b7fers", "Macht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "NN", "$,", "APPRART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Nun stirbt er, und scheint uns, als wie ein Dunst", "tokens": ["Nun", "stirbt", "er", ",", "und", "scheint", "uns", ",", "als", "wie", "ein", "Dunst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "VVFIN", "PPER", "$,", "KOUS", "KOKOM", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "Regt sich der Menschen Leib, der Seelen leimern", "tokens": ["Regt", "sich", "der", "Men\u00b7schen", "Leib", ",", "der", "See\u00b7len", "lei\u00b7mern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN", "NN", "$,", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "In einem Augenblik bl\u00e4st er den Odem aus.", "tokens": ["In", "ei\u00b7nem", "Au\u00b7gen\u00b7blik", "bl\u00e4st", "er", "den", "O\u00b7dem", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Kr\u00f6nt dich o Mensch! ein Gl\u00fck; so siehe zu der", "tokens": ["Kr\u00f6nt", "dich", "o", "Mensch", "!", "ein", "Gl\u00fck", ";", "so", "sie\u00b7he", "zu", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "FM", "NN", "$.", "ART", "NN", "$.", "ADV", "VVFIN", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Hier siehst du, was du bist und was du bald must", "tokens": ["Hier", "siehst", "du", ",", "was", "du", "bist", "und", "was", "du", "bald", "must"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWS", "PPER", "VAFIN", "KON", "PWS", "PPER", "ADV", "VMFIN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Sieh stets die Erde an, die deine Mutter ist;", "tokens": ["Sieh", "stets", "die", "Er\u00b7de", "an", ",", "die", "dei\u00b7ne", "Mut\u00b7ter", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "PTKVZ", "$,", "PRELS", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Tr\u00e4umt dir ein Schattenbild: Denk da\u00df du solches", "tokens": ["Tr\u00e4umt", "dir", "ein", "Schat\u00b7ten\u00b7bild", ":", "Denk", "da\u00df", "du", "sol\u00b7ches"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "NN", "KOUS", "PPER", "PIAT"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.18": {"text": "Und \u00fcberlege stets; so leicht ein Staub verwehet,", "tokens": ["Und", "\u00fc\u00b7berl\u00b7e\u00b7ge", "stets", ";", "so", "leicht", "ein", "Staub", "ver\u00b7we\u00b7het", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$.", "ADV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Reist auch dein leimern Haus, das wie ein Staub", "tokens": ["Reist", "auch", "dein", "lei\u00b7mern", "Haus", ",", "das", "wie", "ein", "Staub"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADV", "PPOSAT", "ADJA", "NN", "$,", "PRELS", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Traum, Schatten fliegen fort; Es l\u00e4st dies Sin-", "tokens": ["Traum", ",", "Schat\u00b7ten", "flie\u00b7gen", "fort", ";", "Es", "l\u00e4st", "dies", "Sin"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "VVFIN", "PTKVZ", "$.", "PPER", "VVFIN", "PDS", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Zur Lehre dir zur\u00fck: ", "tokens": ["Zur", "Leh\u00b7re", "dir", "zu\u00b7r\u00fck", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}