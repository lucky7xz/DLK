{"textgrid.poem.33045": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hier will ich sitzen und ruhen", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier will ich sitzen und ruhen", "tokens": ["Hier", "will", "ich", "sit\u00b7zen", "und", "ru\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "KON", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "An diesem lieblichen Ort,", "tokens": ["An", "die\u00b7sem", "lieb\u00b7li\u00b7chen", "Ort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Will schweifen lassen das Auge", "tokens": ["Will", "schwei\u00b7fen", "las\u00b7sen", "das", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "VVINF", "VVFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In's Weite von Ort zu Ort.", "tokens": ["In's", "Wei\u00b7te", "von", "Ort", "zu", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Will stille sitzen und denken", "tokens": ["Will", "stil\u00b7le", "sit\u00b7zen", "und", "den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "VVINF", "KON", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "An Alles was ich geliebt,", "tokens": ["An", "Al\u00b7les", "was", "ich", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PWS", "PPER", "VVPP", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Will Alles, Alles vergessen,", "tokens": ["Will", "Al\u00b7les", ",", "Al\u00b7les", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "$,", "PIS", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Was mich verletzt und betr\u00fcbt.", "tokens": ["Was", "mich", "ver\u00b7letzt", "und", "be\u00b7tr\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Und kann ich es denn verbannen,", "tokens": ["Und", "kann", "ich", "es", "denn", "ver\u00b7ban\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Woran ich nicht denken will?", "tokens": ["Wo\u00b7ran", "ich", "nicht", "den\u00b7ken", "will", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie bleibt es beim frohen Erinnern", "tokens": ["Wie", "bleibt", "es", "beim", "fro\u00b7hen", "E\u00b7rin\u00b7nern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Im Herzen so \u00f6d' und so still!", "tokens": ["Im", "Her\u00b7zen", "so", "\u00f6d'", "und", "so", "still", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Es sind so innig verbunden", "tokens": ["Es", "sind", "so", "in\u00b7nig", "ver\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In mir die Freuden und Weh'n,", "tokens": ["In", "mir", "die", "Freu\u00b7den", "und", "Weh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "KON", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Da\u00df nur ", "tokens": ["Da\u00df", "nur"], "token_info": ["word", "word"], "pos": ["KOUS", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Vereinigt nur aufersteh'n!", "tokens": ["Ver\u00b7ei\u00b7nigt", "nur", "auf\u00b7er\u00b7steh'n", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}