{"textgrid.poem.39876": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: In dem Dome trennt ein Ritter", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In dem Dome trennt ein Ritter", "tokens": ["In", "dem", "Do\u00b7me", "trennt", "ein", "Rit\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Des Gefolges Scharlachflut:", "tokens": ["Des", "Ge\u00b7fol\u00b7ges", "Schar\u00b7lach\u00b7flut", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbherr und K\u00f6nig, den man kr\u00f6net,", "tokens": ["\u00bb", "herr", "und", "K\u00f6\u00b7nig", ",", "den", "man", "kr\u00f6\u00b7net", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NN", "KON", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gieb mir das erborgte Gut!", "tokens": ["Gieb", "mir", "das", "er\u00b7borg\u00b7te", "Gut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Deines Leibes Purpurlinnen", "tokens": ["Dei\u00b7nes", "Lei\u00b7bes", "Pur\u00b7pur\u00b7lin\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War einst ", "tokens": ["War", "einst"], "token_info": ["word", "word"], "pos": ["VAFIN", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Deiner Krone Diamanten", "tokens": ["Dei\u00b7ner", "Kro\u00b7ne", "Di\u00b7a\u00b7man\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schlummerten in ", "tokens": ["Schlum\u00b7mer\u00b7ten", "in"], "token_info": ["word", "word"], "pos": ["NN", "APPR"], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Dir dein Erbe zu erk\u00e4mpfen", "tokens": ["Dir", "dein", "Er\u00b7be", "zu", "er\u00b7k\u00e4mp\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lieh der alte Freund sie dar.", "tokens": ["Lieh", "der", "al\u00b7te", "Freund", "sie", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An die Stirne dir geheftet", "tokens": ["An", "die", "Stir\u00b7ne", "dir", "ge\u00b7hef\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird mein Auge sie gewahr.", "tokens": ["Wird", "mein", "Au\u00b7ge", "sie", "ge\u00b7wahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nun, bei Gottes Flammenblicke,", "tokens": ["Nun", ",", "bei", "Got\u00b7tes", "Flam\u00b7men\u00b7bli\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der sie alle funkeln sieht:", "tokens": ["Der", "sie", "al\u00b7le", "fun\u00b7keln", "sieht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIS", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Leist' Ersatz mir, da\u00df dich strafend", "tokens": ["Leist'", "Er\u00b7satz", "mir", ",", "da\u00df", "dich", "stra\u00b7fend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "PPER", "$,", "KOUS", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht die Last zu Boden zieht!\u00ab", "tokens": ["Nicht", "die", "Last", "zu", "Bo\u00b7den", "zieht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "ART", "NN", "APPR", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Vor des hohen Thrones Staffel", "tokens": ["Vor", "des", "ho\u00b7hen", "Thro\u00b7nes", "Staf\u00b7fel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heischet er sein Recht vom Gl\u00fcck;", "tokens": ["Hei\u00b7schet", "er", "sein", "Recht", "vom", "Gl\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch ein Fu\u00dftritt seines Schuldners", "tokens": ["Doch", "ein", "Fu\u00df\u00b7tritt", "sei\u00b7nes", "Schuld\u00b7ners"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "St\u00f6\u00dft das graue Haupt zur\u00fcck.", "tokens": ["St\u00f6\u00dft", "das", "grau\u00b7e", "Haupt", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Zu des Feindes braunen Locken", "tokens": ["Zu", "des", "Fein\u00b7des", "brau\u00b7nen", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blitzt empor sein Blick und spricht:", "tokens": ["Blitzt", "em\u00b7por", "sein", "Blick", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "PPOSAT", "NN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbh\u00e4lt nur noch ", "tokens": ["\u00bb", "h\u00e4lt", "nur", "noch"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "ADV", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Junger, ", "tokens": ["Jun\u00b7ger", ","], "token_info": ["word", "punct"], "pos": ["ADJA", "$,"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.7": {"line.1": {"text": "Doch den Fluch verschlingt das Jauchzen,", "tokens": ["Doch", "den", "Fluch", "ver\u00b7schlingt", "das", "Jauch\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Goldner Glanz die Gramgestalt,", "tokens": ["Gold\u00b7ner", "Glanz", "die", "Gram\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ungef\u00fcrchtet, ungesehen", "tokens": ["Un\u00b7ge\u00b7f\u00fcrch\u00b7tet", ",", "un\u00b7ge\u00b7se\u00b7hen"], "token_info": ["word", "punct", "word"], "pos": ["ADJD", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Flieht er in den schwarzen Wald;", "tokens": ["Flieht", "er", "in", "den", "schwar\u00b7zen", "Wald", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Z\u00e4hlet dort die d\u00fcnnen Haare,", "tokens": ["Z\u00e4h\u00b7let", "dort", "die", "d\u00fcn\u00b7nen", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rechnet mit der Lebenszeit:", "tokens": ["Rech\u00b7net", "mit", "der", "Le\u00b7bens\u00b7zeit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn er haush\u00e4lt mit Minuten,", "tokens": ["Wenn", "er", "haus\u00b7h\u00e4lt", "mit", "Mi\u00b7nu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wohnt die Rache nicht zu weit.", "tokens": ["Wohnt", "die", "Ra\u00b7che", "nicht", "zu", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "PTKA", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Bei dem Neide der Vasallen,", "tokens": ["Bei", "dem", "Nei\u00b7de", "der", "Va\u00b7sal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bei der Bauern Qual und Schwei\u00df,", "tokens": ["Bei", "der", "Bau\u00b7ern", "Qual", "und", "Schwei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bei dem Groll versto\u00dfner Knechte", "tokens": ["Bei", "dem", "Groll", "ver\u00b7sto\u00df\u00b7ner", "Knech\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sucht er sie und haucht sie hei\u00df.", "tokens": ["Sucht", "er", "sie", "und", "haucht", "sie", "hei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Und er hat ein Heer gesammelt,", "tokens": ["Und", "er", "hat", "ein", "Heer", "ge\u00b7sam\u00b7melt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eh' ein Haar vom Haupt ihm fiel,", "tokens": ["Eh'", "ein", "Haar", "vom", "Haupt", "ihm", "fiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie im Sturmwind frischer Jugend", "tokens": ["Wie", "im", "Sturm\u00b7wind", "fri\u00b7scher", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wehet ihn die Wut an's Ziel.", "tokens": ["We\u00b7het", "ihn", "die", "Wut", "an's", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Aber aus der Stadt entgegen", "tokens": ["A\u00b7ber", "aus", "der", "Stadt", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwingt sich klagendes Gel\u00e4ut;", "tokens": ["Schwingt", "sich", "kla\u00b7gen\u00b7des", "Ge\u00b7l\u00e4ut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seines K\u00f6nigs junge Leiche", "tokens": ["Sei\u00b7nes", "K\u00f6\u00b7nigs", "jun\u00b7ge", "Lei\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trug das Volk zum Dome heut.", "tokens": ["Trug", "das", "Volk", "zum", "Do\u00b7me", "heut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Vor der offnen Kirchenpforte", "tokens": ["Vor", "der", "off\u00b7nen", "Kir\u00b7ch\u00b7en\u00b7pfor\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Staunt gehobnen Schwerts der Greis,", "tokens": ["Staunt", "ge\u00b7hob\u00b7nen", "Schwerts", "der", "Greis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die hei\u00dfe Rach' im Herzen", "tokens": ["Und", "die", "hei\u00b7\u00dfe", "Rach'", "im", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ballt sich pl\u00f6tzlich ihm zu Eis;", "tokens": ["Ballt", "sich", "pl\u00f6tz\u00b7lich", "ihm", "zu", "Eis", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Wie der J\u00e4ger mit geschwungnem", "tokens": ["Wie", "der", "J\u00e4\u00b7ger", "mit", "ge\u00b7schwung\u00b7nem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Speere vor dem Schlund verzagt,", "tokens": ["Spee\u00b7re", "vor", "dem", "Schlund", "ver\u00b7zagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo den Hirsch, den langgehetzten,", "tokens": ["Wo", "den", "Hirsch", ",", "den", "lang\u00b7ge\u00b7hetz\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Todesangst hinabgejagt.", "tokens": ["To\u00b7de\u00b7sangst", "hin\u00b7ab\u00b7ge\u00b7jagt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Ueber seines Feindes Bahre", "tokens": ["Ue\u00b7ber", "sei\u00b7nes", "Fein\u00b7des", "Bah\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liegt ein weinend Kind gestreckt,", "tokens": ["Liegt", "ein", "wei\u00b7nend", "Kind", "ge\u00b7streckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das der Purpur heilig kleidet,", "tokens": ["Das", "der", "Pur\u00b7pur", "hei\u00b7lig", "klei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das die Demantkrone deckt.", "tokens": ["Das", "die", "De\u00b7mant\u00b7kro\u00b7ne", "deckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Und sein Schwert gesenkt zu Boden,", "tokens": ["Und", "sein", "Schwert", "ge\u00b7senkt", "zu", "Bo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf den Knauf gest\u00fctzt sein Haupt,", "tokens": ["Auf", "den", "Knauf", "ge\u00b7st\u00fctzt", "sein", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ueberblickt der m\u00fcde R\u00e4cher,", "tokens": ["Ue\u00b7berb\u00b7lickt", "der", "m\u00fc\u00b7de", "R\u00e4\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was das Schicksal ihm geraubt:", "tokens": ["Was", "das", "Schick\u00b7sal", "ihm", "ge\u00b7raubt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "\u00bbnein! aus dieses Knaben H\u00e4nden", "tokens": ["\u00bb", "nein", "!", "aus", "die\u00b7ses", "Kna\u00b7ben", "H\u00e4n\u00b7den"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PTKANT", "$.", "APPR", "PDAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fodr' ich nicht des Vaters Schuld!", "tokens": ["Fodr'", "ich", "nicht", "des", "Va\u00b7ters", "Schuld", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hab' Erbarmen, meine Seele,", "tokens": ["Hab'", "Er\u00b7bar\u00b7men", ",", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fasse noch dich in Geduld!", "tokens": ["Fas\u00b7se", "noch", "dich", "in", "Ge\u00b7duld", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PRF", "APPR", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.17": {"line.1": {"text": "Keinen Erben lass' ich scheidend,", "tokens": ["Kei\u00b7nen", "Er\u00b7ben", "lass'", "ich", "schei\u00b7dend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sei denn Alles ihm vermacht!", "tokens": ["Sei", "denn", "Al\u00b7les", "ihm", "ver\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Sorge sei, zu finden", "tokens": ["Mei\u00b7ne", "Sor\u00b7ge", "sei", ",", "zu", "fin\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "$,", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den dort in des Grabes Nacht!\u00ab", "tokens": ["Den", "dort", "in", "des", "Gra\u00b7bes", "Nacht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.18": {"line.1": {"text": "Und mit straffem Fu\u00dfe stampft er,", "tokens": ["Und", "mit", "straf\u00b7fem", "Fu\u00b7\u00dfe", "stampft", "er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df es dr\u00f6hnet, auf den Stein:", "tokens": ["Da\u00df", "es", "dr\u00f6h\u00b7net", ",", "auf", "den", "Stein", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbunter dieser Tempelschwelle", "tokens": ["\u00bb", "un\u00b7ter", "die\u00b7ser", "Tem\u00b7pel\u00b7schwel\u00b7le"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Will ich einst begraben sein!", "tokens": ["Will", "ich", "einst", "be\u00b7gra\u00b7ben", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "M\u00e4nner, die ihr grimmig kamet,", "tokens": ["M\u00e4n\u00b7ner", ",", "die", "ihr", "grim\u00b7mig", "ka\u00b7met", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Neigt euch vor dem Herrscherstab;", "tokens": ["Neigt", "euch", "vor", "dem", "Herr\u00b7scher\u00b7stab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch die Axt, f\u00fcr mich gehoben,", "tokens": ["Doch", "die", "Axt", ",", "f\u00fcr", "mich", "ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Grab' und sch\u00fctze mir dies Grab!", "tokens": ["Grab'", "und", "sch\u00fct\u00b7ze", "mir", "dies", "Grab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "PDS", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Dann, beim Hallen der Posaune,", "tokens": ["Dann", ",", "beim", "Hal\u00b7len", "der", "Po\u00b7sau\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn sich drin der Falsche dehnt,", "tokens": ["Wenn", "sich", "drin", "der", "Fal\u00b7sche", "dehnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Harr' ich, fr\u00fcher auferstanden,", "tokens": ["Harr'", "ich", ",", "fr\u00fc\u00b7her", "auf\u00b7er\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An das Kirchenthor gelehnt.", "tokens": ["An", "das", "Kir\u00b7chen\u00b7thor", "ge\u00b7lehnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.21": {"line.1": {"text": "Schreitet er heraus zum Dome,", "tokens": ["Schrei\u00b7tet", "er", "he\u00b7raus", "zum", "Do\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trifft er mich zum Gang bereit", "tokens": ["Trifft", "er", "mich", "zum", "Gang", "be\u00b7reit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "APPRART", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vor den Thron der schlummerlosen", "tokens": ["Vor", "den", "Thron", "der", "schlum\u00b7mer\u00b7lo\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ewigen Gerechtigkeit!\u00ab", "tokens": ["E\u00b7wi\u00b7gen", "Ge\u00b7rech\u00b7tig\u00b7keit", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$.", "$("], "meter": "+---+-+", "measure": "dactylic.init"}}}}}