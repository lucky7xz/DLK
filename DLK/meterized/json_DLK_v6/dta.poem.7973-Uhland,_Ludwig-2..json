{"dta.poem.7973": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht schamroth weichen soll der S\u00e4ngerorden,", "tokens": ["Nicht", "scham\u00b7roth", "wei\u00b7chen", "soll", "der", "S\u00e4n\u00b7ge\u00b7ror\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVINF", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wann Kriegerscharen ziehn im Waffenglanze;", "tokens": ["Wann", "Krie\u00b7ger\u00b7scha\u00b7ren", "ziehn", "im", "Waf\u00b7fen\u00b7glan\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Noch ist sein Lied kein schn\u00f6des Spiel geworden,", "tokens": ["Noch", "ist", "sein", "Lied", "kein", "schn\u00f6\u00b7des", "Spiel", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PIAT", "ADJA", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch ziert auch ihn der Degen und die Lanze;", "tokens": ["Doch", "ziert", "auch", "ihn", "der", "De\u00b7gen", "und", "die", "Lan\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPER", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wohl schauervoll ist jener Sturm aus Norden,", "tokens": ["Wohl", "schau\u00b7er\u00b7voll", "ist", "je\u00b7ner", "Sturm", "aus", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PDAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Doch weht er frisch und st\u00e4rkt zum Schwerdtertanze.", "tokens": ["Doch", "weht", "er", "frisch", "und", "st\u00e4rkt", "zum", "Schwerd\u00b7ter\u00b7tan\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wollt, Harfner, ihr durch Feindeslager schreiten,", "tokens": ["Wollt", ",", "Harf\u00b7ner", ",", "ihr", "durch", "Fein\u00b7des\u00b7la\u00b7ger", "schrei\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "NN", "$,", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Noch steht\u2019s euch frei \u2014 den Eingang zu erstreiten.", "tokens": ["Noch", "steht's", "euch", "frei", "den", "Ein\u00b7gang", "zu", "er\u00b7strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$(", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Wann: Freiheit! Vaterland! ringsum erschallet,", "tokens": ["Wann", ":", "Frei\u00b7heit", "!", "Va\u00b7ter\u00b7land", "!", "ring\u00b7sum", "er\u00b7schal\u00b7let", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "$.", "NN", "$.", "NN", "$.", "ADV", "VVFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kein Sang t\u00f6nt sch\u00f6ner in der M\u00e4nner Ohren,", "tokens": ["Kein", "Sang", "t\u00f6nt", "sch\u00f6\u00b7ner", "in", "der", "M\u00e4n\u00b7ner", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Kampfe, wo solch heilig Banner wallet,", "tokens": ["Im", "Kamp\u00b7fe", ",", "wo", "solch", "hei\u00b7lig", "Ban\u00b7ner", "wal\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da wird der S\u00e4nger kr\u00e4ftig neugeboren.", "tokens": ["Da", "wird", "der", "S\u00e4n\u00b7ger", "kr\u00e4f\u00b7tig", "neu\u00b7ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Hat Aeschylos, de\u00df Lied vom Siege hallet,", "tokens": ["Hat", "A\u00b7esc\u00b7hy\u00b7los", ",", "de\u00df", "Lied", "vom", "Sie\u00b7ge", "hal\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "$,", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Hat Dante nicht die\u00df sch\u00f6nste Loos erkoren?", "tokens": ["Hat", "Dan\u00b7te", "nicht", "die\u00df", "sch\u00f6ns\u00b7te", "Loos", "er\u00b7ko\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "PTKNEG", "PDS", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Cervantes lie\u00df, gel\u00e4hmt, die Rechte sinken", "tokens": ["Cer\u00b7van\u00b7tes", "lie\u00df", ",", "ge\u00b7l\u00e4hmt", ",", "die", "Rech\u00b7te", "sin\u00b7ken"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "VVPP", "$,", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und schrieb den Don Quixote mit der Linken. ", "tokens": ["Und", "schrieb", "den", "Don", "Qui\u00b7xo\u00b7te", "mit", "der", "Lin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Auch unsres deutschen Liedertempels Pfleger,", "tokens": ["Auch", "uns\u00b7res", "deut\u00b7schen", "Lie\u00b7der\u00b7tem\u00b7pels", "Pfle\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie sind dem Kriegesgeiste nicht verdorben,", "tokens": ["Sie", "sind", "dem", "Krie\u00b7ges\u00b7geis\u00b7te", "nicht", "ver\u00b7dor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Man h\u00f6rt sie wohl die frend\u2019gen Telynschl\u00e4ger,", "tokens": ["Man", "h\u00f6rt", "sie", "wohl", "die", "fren\u00b7d'\u00b7gen", "Te\u00b7lynsc\u00b7hl\u00e4\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und mancher hat sich blut\u2019gen Kranz erworben.", "tokens": ["Und", "man\u00b7cher", "hat", "sich", "blut'\u00b7gen", "Kranz", "er\u00b7wor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PRF", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du, Wehrmann Leo, du, o schwarzer J\u00e4ger,", "tokens": ["Du", ",", "Wehr\u00b7mann", "Leo", ",", "du", ",", "o", "schwar\u00b7zer", "J\u00e4\u00b7ger", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PWAV", "NE", "$,", "PPER", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wohl seyd ihr ritterlichen Tods gestorben!", "tokens": ["Wohl", "seyd", "ihr", "rit\u00b7ter\u00b7li\u00b7chen", "Tods", "ge\u00b7stor\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und Fouqu", "tokens": ["Und", "Fou\u00b7qu"], "token_info": ["word", "word"], "pos": ["KON", "NE"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Du wagtest, k\u00e4mpftest \u2014 doch du lebst und singest.", "tokens": ["Du", "wag\u00b7test", ",", "k\u00e4mpf\u00b7test", "doch", "du", "lebst", "und", "sin\u00b7gest", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$(", "KON", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Den Fr\u00fchling k\u00fcndet der Orkane Sausen,", "tokens": ["Den", "Fr\u00fch\u00b7ling", "k\u00fcn\u00b7det", "der", "Or\u00b7ka\u00b7ne", "Sau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Heere Vorschritt macht die Erde dr\u00f6hnen,", "tokens": ["Der", "Hee\u00b7re", "Vor\u00b7schritt", "macht", "die", "Er\u00b7de", "dr\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wie die Str\u00f6m\u2019 aus ihren Ufern brausen,", "tokens": ["Und", "wie", "die", "Str\u00f6m'", "aus", "ih\u00b7ren", "U\u00b7fern", "brau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So wogt es weit von Deutschlands Heldens\u00f6hnen;", "tokens": ["So", "wogt", "es", "weit", "von", "Deutschlands", "Hel\u00b7den\u00b7s\u00f6h\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "NE", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Der S\u00e4nger folgt durch alles wilde Grausen,", "tokens": ["Der", "S\u00e4n\u00b7ger", "folgt", "durch", "al\u00b7les", "wil\u00b7de", "Grau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "L\u00e4\u00dft Sturm und Wogen gleich sein Lied ert\u00f6nen.", "tokens": ["L\u00e4\u00dft", "Sturm", "und", "Wo\u00b7gen", "gleich", "sein", "Lied", "er\u00b7t\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Bald bl\u00fcht der Fr\u00fchling, bald der goldne Friede,", "tokens": ["Bald", "bl\u00fcht", "der", "Fr\u00fch\u00b7ling", ",", "bald", "der", "gold\u00b7ne", "Frie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mit mildern L\u00fcften und mit sanftrem Liede.", "tokens": ["Mit", "mil\u00b7dern", "L\u00fcf\u00b7ten", "und", "mit", "sanf\u00b7trem", "Lie\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}