{"textgrid.poem.50931": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "7", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weise nicht von dir mein schlichtes Herz,", "tokens": ["Wei\u00b7se", "nicht", "von", "dir", "mein", "schlich\u00b7tes", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Weil es schon so viel geliebet!", "tokens": ["Weil", "es", "schon", "so", "viel", "ge\u00b7lie\u00b7bet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Einer Geige gleicht es, die ge\u00fcbet", "tokens": ["Ei\u00b7ner", "Gei\u00b7ge", "gleicht", "es", ",", "die", "ge\u00b7\u00fc\u00b7bet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Lang ein Meister unter Lust und Schmerz!", "tokens": ["Lang", "ein", "Meis\u00b7ter", "un\u00b7ter", "Lust", "und", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Und je l\u00e4nger er darauf gespielt,", "tokens": ["Und", "je", "l\u00e4n\u00b7ger", "er", "da\u00b7rauf", "ge\u00b7spielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "PPER", "PAV", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Stieg ihr Wert zum h\u00f6chsten Preise;", "tokens": ["Stieg", "ihr", "Wert", "zum", "h\u00f6chs\u00b7ten", "Prei\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn sie gibt mit sichrer Kraft die Weise,", "tokens": ["Denn", "sie", "gibt", "mit", "sich\u00b7rer", "Kraft", "die", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die ein Kund'ger ihren Saiten stiehlt.", "tokens": ["Die", "ein", "Kun\u00b7d'\u00b7ger", "ih\u00b7ren", "Sai\u00b7ten", "stiehlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Also spielte manche Meisterin", "tokens": ["Al\u00b7so", "spiel\u00b7te", "man\u00b7che", "Meis\u00b7te\u00b7rin"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "In mein Herz die rechte Seele!", "tokens": ["In", "mein", "Herz", "die", "rech\u00b7te", "See\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nun ist's wert, da\u00df man es dir empfehle,", "tokens": ["Nun", "ist's", "wert", ",", "da\u00df", "man", "es", "dir", "emp\u00b7feh\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "$,", "KOUS", "PIS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Lasse nicht den k\u00f6stlichen Gewinn!", "tokens": ["Las\u00b7se", "nicht", "den", "k\u00f6st\u00b7li\u00b7chen", "Ge\u00b7winn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}