{"textgrid.poem.39012": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hier vernimmt mein Ohr die T\u00f6ne", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier vernimmt mein Ohr die T\u00f6ne", "tokens": ["Hier", "ver\u00b7nimmt", "mein", "Ohr", "die", "T\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Voller und gewaltiger", "tokens": ["Vol\u00b7ler", "und", "ge\u00b7wal\u00b7ti\u00b7ger"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "ADJA"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von euch, ihr Hochgeweihten,", "tokens": ["Von", "euch", ",", "ihr", "Hoch\u00b7ge\u00b7weih\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die mir dr\u00fcben nur als Echo klangen", "tokens": ["Die", "mir", "dr\u00fc\u00b7ben", "nur", "als", "E\u00b7cho", "klan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADV", "KOUS", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Matt und schwach, fast ohne Farbe.", "tokens": ["Matt", "und", "schwach", ",", "fast", "oh\u00b7ne", "Far\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schon der fr\u00fche Morgen", "tokens": ["Schon", "der", "fr\u00fc\u00b7he", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Findet mich bei Dantes Reimen", "tokens": ["Fin\u00b7det", "mich", "bei", "Dan\u00b7tes", "Rei\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und Ariostos Zauberspielen,", "tokens": ["Und", "A\u00b7rios\u00b7tos", "Zau\u00b7ber\u00b7spie\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Jetzt versteh ich dich, Petrarka,", "tokens": ["Jetzt", "ver\u00b7steh", "ich", "dich", ",", "Pe\u00b7trar\u00b7ka", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "NE", "$,"], "meter": "--+-++-+", "measure": "anapaest.init"}, "line.10": {"text": "Und die zartgeflochtne Rede", "tokens": ["Und", "die", "zart\u00b7ge\u00b7flocht\u00b7ne", "Re\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Des k\u00fchnen Boccacio.", "tokens": ["Des", "k\u00fch\u00b7nen", "Boc\u00b7ca\u00b7cio", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Tasso, Tassani,", "tokens": ["Tas\u00b7so", ",", "Tas\u00b7sa\u00b7ni", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.13": {"text": "Bojardo, und Lorenz der Medic\u00e4er,", "tokens": ["Bo\u00b7jar\u00b7do", ",", "und", "Lo\u00b7renz", "der", "Me\u00b7di\u00b7c\u00e4\u00b7er", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "Lascen, und alle die frohen Zeitgenossen", "tokens": ["Las\u00b7cen", ",", "und", "al\u00b7le", "die", "fro\u00b7hen", "Zeit\u00b7ge\u00b7nos\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KON", "PIS", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Warten schon auf meine Mu\u00dfe.", "tokens": ["War\u00b7ten", "schon", "auf", "mei\u00b7ne", "Mu\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Und drum find' ich kaum die Stunde,", "tokens": ["Und", "drum", "find'", "ich", "kaum", "die", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Was die Landsleute dachten", "tokens": ["Was", "die", "Lands\u00b7leu\u00b7te", "dach\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Zu pr\u00fcfen und mir anzueignen.", "tokens": ["Zu", "pr\u00fc\u00b7fen", "und", "mir", "an\u00b7zu\u00b7eig\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Komm' ich doch zu euch zur\u00fcck,", "tokens": ["Komm'", "ich", "doch", "zu", "euch", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "G\u00f6nnt mir diese Feierstunden,", "tokens": ["G\u00f6nnt", "mir", "die\u00b7se", "Fei\u00b7er\u00b7stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Nie kann ich euch vergessen.", "tokens": ["Nie", "kann", "ich", "euch", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Oft schon hat man belacht,", "tokens": ["Oft", "schon", "hat", "man", "be\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PIS", "VVPP", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Da\u00df der Engelsmann reisend", "tokens": ["Da\u00df", "der", "En\u00b7gels\u00b7mann", "rei\u00b7send"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVPP"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Allen seinen l\u00e4stigen Trost mit sich f\u00fchrt,", "tokens": ["Al\u00b7len", "sei\u00b7nen", "l\u00e4s\u00b7ti\u00b7gen", "Trost", "mit", "sich", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und zum Aetna hinauf", "tokens": ["Und", "zum", "A\u00b7et\u00b7na", "hin\u00b7auf"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "NE", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Den Theekessel schleppt,", "tokens": ["Den", "Thee\u00b7kes\u00b7sel", "schleppt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Um am Krater", "tokens": ["Um", "am", "Kra\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["KOUI", "APPRART", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.7": {"text": "Wie an Londons Kamin", "tokens": ["Wie", "an", "Lon\u00b7dons", "Ka\u00b7min"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NE", "NE"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Den chinesischen Trank zu schl\u00fcrfen.", "tokens": ["Den", "chi\u00b7ne\u00b7si\u00b7schen", "Trank", "zu", "schl\u00fcr\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "L\u00e4chle doch keiner,", "tokens": ["L\u00e4ch\u00b7le", "doch", "kei\u00b7ner", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "PIS", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Denn schlimmer als diese", "tokens": ["Denn", "schlim\u00b7mer", "als", "die\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "KOKOM", "PDS"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Treiben's die Deutschen.", "tokens": ["Trei\u00b7ben's", "die", "Deut\u00b7schen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Wandeln doch oft mit mir", "tokens": ["Wan\u00b7deln", "doch", "oft", "mit", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ADV", "APPR", "PPER"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.13": {"text": "Hochgebildete, feine,", "tokens": ["Hoch\u00b7ge\u00b7bil\u00b7de\u00b7te", ",", "fei\u00b7ne", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.14": {"text": "Fast gelehrte Edelleute,", "tokens": ["Fast", "ge\u00b7lehr\u00b7te", "E\u00b7del\u00b7leu\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Die nur weniges der Italischen Sprache", "tokens": ["Die", "nur", "we\u00b7ni\u00b7ges", "der", "I\u00b7ta\u00b7li\u00b7schen", "Spra\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PIS", "ART", "NN", "NN"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Lesend entwenden,", "tokens": ["Le\u00b7send", "ent\u00b7wen\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Doch alle Meisterwerke", "tokens": ["Doch", "al\u00b7le", "Meis\u00b7ter\u00b7wer\u00b7ke"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Tragisch und komisch", "tokens": ["Tra\u00b7gisch", "und", "ko\u00b7misch"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.19": {"text": "Unsers Kotzebue, Lafontaine,", "tokens": ["Un\u00b7sers", "Kot\u00b7ze\u00b7bue", ",", "La\u00b7fon\u00b7tai\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "$,", "NE", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.20": {"text": "In gro\u00dfen Kisten mit sich f\u00fchren,", "tokens": ["In", "gro\u00b7\u00dfen", "Kis\u00b7ten", "mit", "sich", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Und schwer und theuer", "tokens": ["Und", "schwer", "und", "theu\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.22": {"text": "Die heimathlichen Gef\u00fchle zahlen.", "tokens": ["Die", "hei\u00b7math\u00b7li\u00b7chen", "Ge\u00b7f\u00fch\u00b7le", "zah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "J\u00fcngst fragte mich einer", "tokens": ["J\u00fcngst", "frag\u00b7te", "mich", "ei\u00b7ner"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Neugierig forschend,", "tokens": ["Neu\u00b7gie\u00b7rig", "for\u00b7schend", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ob ich vielleicht ganz unbedingt", "tokens": ["Ob", "ich", "viel\u00b7leicht", "ganz", "un\u00b7be\u00b7dingt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "(was ihm unbillig schien)", "tokens": ["(", "was", "ihm", "un\u00b7bil\u00b7lig", "schien", ")"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PPER", "ADJD", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "G\u00f6the's Fragment vom Faust", "tokens": ["G\u00f6\u00b7the's", "Frag\u00b7ment", "vom", "Faust"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "APPRART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Der Dichtung Schinks", "tokens": ["Der", "Dich\u00b7tung", "Schinks"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Den Vorzug g\u00e4be.", "tokens": ["Den", "Vor\u00b7zug", "g\u00e4\u00b7be", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Er sch\u00fcttelte ungl\u00e4ubig", "tokens": ["Er", "sch\u00fct\u00b7tel\u00b7te", "un\u00b7gl\u00e4u\u00b7big"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Das denkende Haupt,", "tokens": ["Das", "den\u00b7ken\u00b7de", "Haupt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Als ich ihm betheuert,", "tokens": ["Als", "ich", "ihm", "be\u00b7theu\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Da\u00df mir die zweite unbekannt,", "tokens": ["Da\u00df", "mir", "die", "zwei\u00b7te", "un\u00b7be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und ich auch ohne Trieb mich f\u00fchle", "tokens": ["Und", "ich", "auch", "oh\u00b7ne", "Trieb", "mich", "f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "APPR", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Sie zu genie\u00dfen.", "tokens": ["Sie", "zu", "ge\u00b7nie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Ja wohl hei\u00df' ich ihm unpatriotisch,", "tokens": ["Ja", "wohl", "hei\u00df'", "ich", "ihm", "un\u00b7pat\u00b7ri\u00b7o\u00b7tisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "VVFIN", "PPER", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Einseitig in die Erfindungen", "tokens": ["Ein\u00b7sei\u00b7tig", "in", "die", "Er\u00b7fin\u00b7dun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Der W\u00e4lschen vergafft.", "tokens": ["Der", "W\u00e4l\u00b7schen", "ver\u00b7gafft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.17": {"text": "Mit gutm\u00fcthigem Eifer", "tokens": ["Mit", "gut\u00b7m\u00fct\u00b7hi\u00b7gem", "Ei\u00b7fer"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.18": {"text": "Wird mir von Enthusiasten", "tokens": ["Wird", "mir", "von", "En\u00b7thu\u00b7si\u00b7as\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.19": {"text": "Oft aufgedr\u00e4ngt,", "tokens": ["Oft", "auf\u00b7ge\u00b7dr\u00e4ngt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.20": {"text": "Dem ich schon jenseit der Alpen", "tokens": ["Dem", "ich", "schon", "jen\u00b7seit", "der", "Al\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.21": {"text": "Gern entfloh.", "tokens": ["Gern", "ent\u00b7floh", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}