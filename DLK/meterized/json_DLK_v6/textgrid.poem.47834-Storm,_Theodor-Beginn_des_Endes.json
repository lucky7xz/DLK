{"textgrid.poem.47834": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Beginn des Endes", "genre": "verse", "period": "N.A.", "pub_year": 1864, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Punkt nur ist es, kaum ein Schmerz,", "tokens": ["Ein", "Punkt", "nur", "ist", "es", ",", "kaum", "ein", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "PPER", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur ein Gef\u00fchl, empfunden eben;", "tokens": ["Nur", "ein", "Ge\u00b7f\u00fchl", ",", "emp\u00b7fun\u00b7den", "e\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und dennoch spricht es stets darein,", "tokens": ["Und", "den\u00b7noch", "spricht", "es", "stets", "da\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und dennoch st\u00f6rt es dich zu leben.", "tokens": ["Und", "den\u00b7noch", "st\u00f6rt", "es", "dich", "zu", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn du es andern klagen willst,", "tokens": ["Wenn", "du", "es", "an\u00b7dern", "kla\u00b7gen", "willst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So kannst du's nicht in Worte fassen.", "tokens": ["So", "kannst", "du's", "nicht", "in", "Wor\u00b7te", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PTKNEG", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du sagst dir selber: \u00bbEs ist nichts!\u00ab", "tokens": ["Du", "sagst", "dir", "sel\u00b7ber", ":", "\u00bb", "Es", "ist", "nichts", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$.", "$(", "PPER", "VAFIN", "PIS", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und dennoch will es dich nicht lassen.", "tokens": ["Und", "den\u00b7noch", "will", "es", "dich", "nicht", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So seltsam fremd wird dir die Welt,", "tokens": ["So", "selt\u00b7sam", "fremd", "wird", "dir", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und leis verl\u00e4\u00dft dich alles Hoffen,", "tokens": ["Und", "leis", "ver\u00b7l\u00e4\u00dft", "dich", "al\u00b7les", "Hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bis du es endlich, endlich wei\u00dft,", "tokens": ["Bis", "du", "es", "end\u00b7lich", ",", "end\u00b7lich", "wei\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df dich des Todes Pfeil getroffen.", "tokens": ["Da\u00df", "dich", "des", "To\u00b7des", "Pfeil", "ge\u00b7trof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}