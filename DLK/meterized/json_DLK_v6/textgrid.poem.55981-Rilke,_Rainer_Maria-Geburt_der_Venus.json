{"textgrid.poem.55981": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Geburt der Venus", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An diesem Morgen nach der Nacht, die bang", "tokens": ["An", "die\u00b7sem", "Mor\u00b7gen", "nach", "der", "Nacht", ",", "die", "bang"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PDAT", "NN", "APPR", "ART", "NN", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "vergangen war mit Rufen, Unruh, Aufruhr, \u2013", "tokens": ["ver\u00b7gan\u00b7gen", "war", "mit", "Ru\u00b7fen", ",", "Un\u00b7ruh", ",", "Auf\u00b7ruhr", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["VVPP", "VAFIN", "APPR", "NN", "$,", "NN", "$,", "NN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "brach alles Meer noch einmal auf und schrie.", "tokens": ["brach", "al\u00b7les", "Meer", "noch", "ein\u00b7mal", "auf", "und", "schrie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADV", "ADV", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und als der Schrei sich langsam wieder schlo\u00df", "tokens": ["Und", "als", "der", "Schrei", "sich", "lang\u00b7sam", "wie\u00b7der", "schlo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "PRF", "ADJD", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "und von der Himmel blassem Tag und Anfang", "tokens": ["und", "von", "der", "Him\u00b7mel", "blas\u00b7sem", "Tag", "und", "An\u00b7fang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "herabfiel in der stummen Fische Abgrund \u2013:", "tokens": ["her\u00b7ab\u00b7fiel", "in", "der", "stum\u00b7men", "Fi\u00b7sche", "Ab\u00b7grund", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "NN", "$(", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "gebar das Meer.", "tokens": ["ge\u00b7bar", "das", "Meer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Von erster Sonne schimmerte der Haarschaum", "tokens": ["Von", "ers\u00b7ter", "Son\u00b7ne", "schim\u00b7mer\u00b7te", "der", "Haar\u00b7schaum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "der weiten Wogenscham, an deren Rand", "tokens": ["der", "wei\u00b7ten", "Wo\u00b7gen\u00b7scham", ",", "an", "de\u00b7ren", "Rand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PRELAT", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "das M\u00e4dchen aufstand, wei\u00df, verwirrt und feucht.", "tokens": ["das", "M\u00e4d\u00b7chen", "auf\u00b7stand", ",", "wei\u00df", ",", "ver\u00b7wirrt", "und", "feucht", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So wie ein junges gr\u00fcnes Blatt sich r\u00fchrt,", "tokens": ["So", "wie", "ein", "jun\u00b7ges", "gr\u00fc\u00b7nes", "Blatt", "sich", "r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "sich reckt und Eingerolltes langsam aufschl\u00e4gt,", "tokens": ["sich", "reckt", "und", "Ein\u00b7ge\u00b7roll\u00b7tes", "lang\u00b7sam", "auf\u00b7schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "KON", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "entfaltete ihr Leib sich in die K\u00fchle", "tokens": ["ent\u00b7fal\u00b7te\u00b7te", "ihr", "Leib", "sich", "in", "die", "K\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "hinein und in den unber\u00fchrten Fr\u00fchwind.", "tokens": ["hin\u00b7ein", "und", "in", "den", "un\u00b7be\u00b7r\u00fchr\u00b7ten", "Fr\u00fch\u00b7wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie Monde stiegen klar die Kniee auf", "tokens": ["Wie", "Mon\u00b7de", "stie\u00b7gen", "klar", "die", "Kni\u00b7ee", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "VVFIN", "ADJD", "ART", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und tauchten in der Schenkel Wolkenr\u00e4nder;", "tokens": ["und", "tauch\u00b7ten", "in", "der", "Schen\u00b7kel", "Wol\u00b7ken\u00b7r\u00e4n\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "der Waden schmaler Schatten wich zur\u00fcck,", "tokens": ["der", "Wa\u00b7den", "schma\u00b7ler", "Schat\u00b7ten", "wich", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "die F\u00fc\u00dfe spannten sich und wurden licht,", "tokens": ["die", "F\u00fc\u00b7\u00dfe", "spann\u00b7ten", "sich", "und", "wur\u00b7den", "licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "KON", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "und die Gelenke lebten wie die Kehlen", "tokens": ["und", "die", "Ge\u00b7len\u00b7ke", "leb\u00b7ten", "wie", "die", "Keh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "von Trinkenden.", "tokens": ["von", "Trin\u00b7ken\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+--", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Und in dem Kelch des Beckens lag der Leib", "tokens": ["Und", "in", "dem", "Kelch", "des", "Be\u00b7ckens", "lag", "der", "Leib"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "wie eine junge Frucht in eines Kindes Hand.", "tokens": ["wie", "ei\u00b7ne", "jun\u00b7ge", "Frucht", "in", "ei\u00b7nes", "Kin\u00b7des", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In seines Nabels engem Becher war", "tokens": ["In", "sei\u00b7nes", "Na\u00b7bels", "en\u00b7gem", "Be\u00b7cher", "war"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "das ganze Dunkel dieses hellen Lebens.", "tokens": ["das", "gan\u00b7ze", "Dun\u00b7kel", "die\u00b7ses", "hel\u00b7len", "Le\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Darunter hob sich licht die kleine Welle", "tokens": ["Da\u00b7run\u00b7ter", "hob", "sich", "licht", "die", "klei\u00b7ne", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PRF", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "und flo\u00df best\u00e4ndig \u00fcber nach den Lenden,", "tokens": ["und", "flo\u00df", "be\u00b7st\u00e4n\u00b7dig", "\u00fc\u00b7ber", "nach", "den", "Len\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "wo dann und wann ein stilles Rieseln war.", "tokens": ["wo", "dann", "und", "wann", "ein", "stil\u00b7les", "Rie\u00b7seln", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "KON", "PWAV", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Durchschienen aber und noch ohne Schatten,", "tokens": ["Durch\u00b7schie\u00b7nen", "a\u00b7ber", "und", "noch", "oh\u00b7ne", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "wie ein Bestand von Birken im April,", "tokens": ["wie", "ein", "Be\u00b7stand", "von", "Bir\u00b7ken", "im", "Ap\u00b7ril", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "warm, leer und unverborgen, lag die Scham.", "tokens": ["warm", ",", "leer", "und", "un\u00b7ver\u00b7bor\u00b7gen", ",", "lag", "die", "Scham", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Jetzt stand der Schultern rege Waage schon", "tokens": ["Jetzt", "stand", "der", "Schul\u00b7tern", "re\u00b7ge", "Waa\u00b7ge", "schon"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "im Gleichgewichte auf dem graden K\u00f6rper,", "tokens": ["im", "Gleich\u00b7ge\u00b7wich\u00b7te", "auf", "dem", "gra\u00b7den", "K\u00f6r\u00b7per", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "der aus dem Becken wie ein Springbrunn aufstieg", "tokens": ["der", "aus", "dem", "Be\u00b7cken", "wie", "ein", "Spring\u00b7brunn", "auf\u00b7stieg"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "KOKOM", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und z\u00f6gernd in den langen Armen abfiel", "tokens": ["und", "z\u00f6\u00b7gernd", "in", "den", "lan\u00b7gen", "Ar\u00b7men", "ab\u00b7fiel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "und rascher in dem vollen Fall des Haars.", "tokens": ["und", "ra\u00b7scher", "in", "dem", "vol\u00b7len", "Fall", "des", "Haars", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Dann ging sehr langsam das Gesicht vorbei:", "tokens": ["Dann", "ging", "sehr", "lang\u00b7sam", "das", "Ge\u00b7sicht", "vor\u00b7bei", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "aus dem verk\u00fcrzten Dunkel seiner Neigung", "tokens": ["aus", "dem", "ver\u00b7k\u00fcrz\u00b7ten", "Dun\u00b7kel", "sei\u00b7ner", "Nei\u00b7gung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "in klares, waagrechtes Erhobensein.", "tokens": ["in", "kla\u00b7res", ",", "waag\u00b7rech\u00b7tes", "Er\u00b7ho\u00b7ben\u00b7sein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und hinter ihm verschlo\u00df sich steil das Kinn.", "tokens": ["Und", "hin\u00b7ter", "ihm", "ver\u00b7schlo\u00df", "sich", "steil", "das", "Kinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PRF", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Jetzt, da der Hals gestreckt war wie ein Strahl", "tokens": ["Jetzt", ",", "da", "der", "Hals", "ge\u00b7streckt", "war", "wie", "ein", "Strahl"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "VVPP", "VAFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und wie ein Blumenstiel, darin der Saft steigt,", "tokens": ["und", "wie", "ein", "Blu\u00b7men\u00b7stiel", ",", "da\u00b7rin", "der", "Saft", "steigt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "PAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "streckten sich auch die Arme aus wie H\u00e4lse", "tokens": ["streck\u00b7ten", "sich", "auch", "die", "Ar\u00b7me", "aus", "wie", "H\u00e4l\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADV", "ART", "NN", "PTKVZ", "KOKOM", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "von Schw\u00e4nen, wenn sie nach dem Ufer suchen.", "tokens": ["von", "Schw\u00e4\u00b7nen", ",", "wenn", "sie", "nach", "dem", "U\u00b7fer", "su\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Dann kam in dieses Leibes dunkle Fr\u00fche", "tokens": ["Dann", "kam", "in", "die\u00b7ses", "Lei\u00b7bes", "dunk\u00b7le", "Fr\u00fc\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PDAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "wie Morgenwind der erste Atemzug.", "tokens": ["wie", "Mor\u00b7gen\u00b7wind", "der", "ers\u00b7te", "A\u00b7tem\u00b7zug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im zartesten Ge\u00e4st der Aderb\u00e4ume", "tokens": ["Im", "zar\u00b7tes\u00b7ten", "Ge\u00b7\u00e4st", "der", "A\u00b7der\u00b7b\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "entstand ein Fl\u00fcstern, und das Blut begann", "tokens": ["ent\u00b7stand", "ein", "Fl\u00fcs\u00b7tern", ",", "und", "das", "Blut", "be\u00b7gann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "zu rauschen \u00fcber seinen tiefen Stellen.", "tokens": ["zu", "rau\u00b7schen", "\u00fc\u00b7ber", "sei\u00b7nen", "tie\u00b7fen", "Stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und dieser Wind wuchs an: nun warf er sich", "tokens": ["Und", "die\u00b7ser", "Wind", "wuchs", "an", ":", "nun", "warf", "er", "sich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "VVFIN", "PTKVZ", "$.", "ADV", "VV