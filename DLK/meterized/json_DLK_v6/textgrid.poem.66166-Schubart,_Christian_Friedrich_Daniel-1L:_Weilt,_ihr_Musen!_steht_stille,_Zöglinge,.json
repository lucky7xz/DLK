{"textgrid.poem.66166": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Weilt, ihr Musen! steht stille, Z\u00f6glinge,", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weilt, ihr Musen! steht stille, Z\u00f6glinge,", "tokens": ["Weilt", ",", "ihr", "Mu\u00b7sen", "!", "steht", "stil\u00b7le", ",", "Z\u00f6g\u00b7lin\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$.", "VVFIN", "VVFIN", "$,", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Vor diesem heiligen Bilde!", "tokens": ["Vor", "die\u00b7sem", "hei\u00b7li\u00b7gen", "Bil\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Im Drange hohes Gef\u00fchls", "tokens": ["Im", "Dran\u00b7ge", "ho\u00b7hes", "Ge\u00b7f\u00fchls"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Springt mir der Busen!", "tokens": ["Springt", "mir", "der", "Bu\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Zu schwach ist die Mimik", "tokens": ["Zu", "schwach", "ist", "die", "Mi\u00b7mik"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "VAFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Aufzuhalten des Gef\u00fchls", "tokens": ["Auf\u00b7zu\u00b7hal\u00b7ten", "des", "Ge\u00b7f\u00fchls"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Donnernden Wogenschlag.", "tokens": ["Don\u00b7nern\u00b7den", "Wo\u00b7gen\u00b7schlag", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "Ich mu\u00df sagen, laut mu\u00df ich sagen,", "tokens": ["Ich", "mu\u00df", "sa\u00b7gen", ",", "laut", "mu\u00df", "ich", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "ADJD", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.9": {"text": "Was ihr verschweigt.", "tokens": ["Was", "ihr", "ver\u00b7schweigt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Mit Sternengold geschrieben", "tokens": ["Mit", "Ster\u00b7nen\u00b7gold", "ge\u00b7schrie\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Am Olymp \u2013 der Name ", "tokens": ["Am", "O\u00b7lymp", "\u2013", "der", "Na\u00b7me"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$(", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Ha! mit welcher Wonne sprech' ich ihn aus,", "tokens": ["Ha", "!", "mit", "wel\u00b7cher", "Won\u00b7ne", "sprech'", "ich", "ihn", "aus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "APPR", "PWAT", "NN", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.13": {"text": "Deinen Namen, ", "tokens": ["Dei\u00b7nen", "Na\u00b7men", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}}}}}