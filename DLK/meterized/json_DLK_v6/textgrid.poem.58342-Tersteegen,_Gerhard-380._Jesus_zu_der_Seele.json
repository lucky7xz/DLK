{"textgrid.poem.58342": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "380. Jesus zu der Seele", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Seel', ich wohn' und ruh so gern in dir,", "tokens": ["O", "Seel'", ",", "ich", "wohn'", "und", "ruh", "so", "gern", "in", "dir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "ADJD", "KON", "NN", "ADV", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ach wohne doch auch unverr\u00fcckt in mir!", "tokens": ["Ach", "woh\u00b7ne", "doch", "auch", "un\u00b7ver\u00b7r\u00fcckt", "in", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "ADV", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein liebend Herze steht dir offen,", "tokens": ["Mein", "lie\u00b7bend", "Her\u00b7ze", "steht", "dir", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VVFIN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kannst du wohl etwas Bessers hoffen?", "tokens": ["Kannst", "du", "wohl", "et\u00b7was", "Bes\u00b7sers", "hof\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}