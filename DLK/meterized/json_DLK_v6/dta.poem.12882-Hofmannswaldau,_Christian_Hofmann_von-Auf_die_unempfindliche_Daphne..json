{"dta.poem.12882": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf die unempfindliche Daphne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Geliebtes Licht, doch allzukaltes Kind!", "tokens": ["Ge\u00b7lieb\u00b7tes", "Licht", ",", "doch", "all\u00b7zu\u00b7kal\u00b7tes", "Kind", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du gleichest recht den diamanten steinen,", "tokens": ["Du", "glei\u00b7chest", "recht", "den", "di\u00b7a\u00b7man\u00b7ten", "stei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die in der glut fast zu verbrennen scheinen,", "tokens": ["Die", "in", "der", "glut", "fast", "zu", "ver\u00b7bren\u00b7nen", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und dennoch kalt und unempfindlich sind.", "tokens": ["Und", "den\u00b7noch", "kalt", "und", "un\u00b7emp\u00b7find\u00b7lich", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "KON", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}