{"textgrid.poem.43776": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vor meinem Fenster sa\u00dfen sie,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor meinem Fenster sa\u00dfen sie,", "tokens": ["Vor", "mei\u00b7nem", "Fens\u00b7ter", "sa\u00b7\u00dfen", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die lieben T\u00e4ubchen beide;", "tokens": ["Die", "lie\u00b7ben", "T\u00e4ub\u00b7chen", "bei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie flogen aus, sie kehrten heim", "tokens": ["Sie", "flo\u00b7gen", "aus", ",", "sie", "kehr\u00b7ten", "heim"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu meinem Fenster beide.", "tokens": ["Zu", "mei\u00b7nem", "Fens\u00b7ter", "bei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PIS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein Iltis schlich zum Schlag hinein", "tokens": ["Ein", "Il\u00b7tis", "schlich", "zum", "Schlag", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "ADJD", "APPRART", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und w\u00fcrgte mir das eine;", "tokens": ["Und", "w\u00fcrg\u00b7te", "mir", "das", "ei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PDS", "ART", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das andre nun am Fenster sitzt,", "tokens": ["Das", "and\u00b7re", "nun", "am", "Fens\u00b7ter", "sitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich seh' es an und weine.", "tokens": ["Ich", "seh'", "es", "an", "und", "wei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich hol' ihm Wasser, hol' ihm Korn,", "tokens": ["Ich", "hol'", "ihm", "Was\u00b7ser", ",", "hol'", "ihm", "Korn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$,", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Alles will's nicht haben.", "tokens": ["Das", "Al\u00b7les", "will's", "nicht", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VMFIN", "PTKNEG", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es thut, als wollt' es sagen mir:", "tokens": ["Es", "thut", ",", "als", "wollt'", "es", "sa\u00b7gen", "mir", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "VMFIN", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich sollt' es nur begraben.", "tokens": ["Ich", "sollt'", "es", "nur", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es schlo\u00df sein Aug', und ich begrub's", "tokens": ["Es", "schlo\u00df", "sein", "Aug'", ",", "und", "ich", "be\u00b7grub's"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "KON", "PPER", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dort unterm gr\u00fcnen Flieder.", "tokens": ["Dort", "un\u00b7term", "gr\u00fc\u00b7nen", "Flie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich sah's und seh' es immer noch", "tokens": ["Ich", "sah's", "und", "seh'", "es", "im\u00b7mer", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wein' auch immer wieder.", "tokens": ["Und", "wein'", "auch", "im\u00b7mer", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}