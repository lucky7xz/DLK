{"dta.poem.13258": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201enun ists, Gro\u00dfm\u00e4chtigste! vielleicht der Tugend-Krei\u00df,", "tokens": ["\u201e", "nun", "ists", ",", "Gro\u00df\u00b7m\u00e4ch\u00b7tigs\u00b7te", "!", "viel\u00b7leicht", "der", "Tu\u00b7gen\u00b7dKrei\u00df", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "$,", "ADJA", "$.", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eder die Bedeutungen von solchen Dingen wei\u00df.", "tokens": ["\u201e", "der", "die", "Be\u00b7deu\u00b7tun\u00b7gen", "von", "sol\u00b7chen", "Din\u00b7gen", "wei\u00df", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eich glaubte meines Orts, in solcher Pflicht zu leben,", "tokens": ["\u201e", "ich", "glaub\u00b7te", "mei\u00b7nes", "Orts", ",", "in", "sol\u00b7cher", "Pflicht", "zu", "le\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPOSAT", "NN", "$,", "APPR", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eda\u00df ich sollt unverweilt dir diese Nachricht geben.", "tokens": ["\u201e", "da\u00df", "ich", "sollt", "un\u00b7ver\u00b7weilt", "dir", "die\u00b7se", "Nach\u00b7richt", "ge\u00b7ben", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "VMFIN", "ADJD", "PPER", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "345\u201dDie Sach ist viel zu fremd und ausserordentlich;", "tokens": ["\"", "Die", "Sach", "ist", "viel", "zu", "fremd", "und", "aus\u00b7ser\u00b7or\u00b7dent\u00b7lich", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201evielleicht auch deinem Volck zum Gl\u00fcck bef\u00f6rderlich.", "tokens": ["\u201e", "viel\u00b7leicht", "auch", "dei\u00b7nem", "Volck", "zum", "Gl\u00fcck", "be\u00b7f\u00f6r\u00b7der\u00b7lich", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "PPOSAT", "NN", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201ees ist mir wohl bewu\u00dft, was hier ein Adler heisse;", "tokens": ["\u201e", "es", "ist", "mir", "wohl", "be\u00b7wu\u00dft", ",", "was", "hier", "ein", "Ad\u00b7ler", "heis\u00b7se", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPER", "ADV", "ADJD", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201ewie man um den Besiz desselben sich befleisse.", "tokens": ["\u201e", "wie", "man", "um", "den", "Be\u00b7siz", "des\u00b7sel\u00b7ben", "sich", "be\u00b7fleis\u00b7se", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PIS", "APPR", "ART", "NN", "PDS", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}