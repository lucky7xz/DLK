{"textgrid.poem.41843": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fast mahnt es mich gleich eines Traumes Weben,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fast mahnt es mich gleich eines Traumes Weben,", "tokens": ["Fast", "mahnt", "es", "mich", "gleich", "ei\u00b7nes", "Trau\u00b7mes", "We\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df siebzig Jahre mir vorbeigegangen;", "tokens": ["Da\u00df", "sieb\u00b7zig", "Jah\u00b7re", "mir", "vor\u00b7bei\u00b7ge\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "CARD", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Voll Leid, voll Gl\u00fcck, doch immer voll Verlangen", "tokens": ["Voll", "Leid", ",", "voll", "Gl\u00fcck", ",", "doch", "im\u00b7mer", "voll", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "$,", "ADJD", "NN", "$,", "ADV", "ADV", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit meinen Zeitgenossen fortzustreben.", "tokens": ["Mit", "mei\u00b7nen", "Zeit\u00b7ge\u00b7nos\u00b7sen", "fort\u00b7zu\u00b7stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch ach! wie viele schieden aus dem Leben,", "tokens": ["Doch", "ach", "!", "wie", "vie\u00b7le", "schie\u00b7den", "aus", "dem", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PWAV", "PIS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die mir vereint die gleichen Lieder sangen,", "tokens": ["Die", "mir", "ver\u00b7eint", "die", "glei\u00b7chen", "Lie\u00b7der", "san\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Ideales heilge Fahnen schwangen \u2013!", "tokens": ["Des", "I\u00b7dea\u00b7les", "heil\u00b7ge", "Fah\u00b7nen", "schwan\u00b7gen", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$(", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "So dacht ich heut in schmerzlichem Erbeben.", "tokens": ["So", "dacht", "ich", "heut", "in", "schmerz\u00b7li\u00b7chem", "Er\u00b7be\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-----", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "Da aber kamen mir so holde Zeichen:", "tokens": ["Da", "a\u00b7ber", "ka\u00b7men", "mir", "so", "hol\u00b7de", "Zei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbnoch viele sind Dir treu gesinnt geblieben!\u00ab", "tokens": ["\u00bb", "noch", "vie\u00b7le", "sind", "Dir", "treu", "ge\u00b7sinnt", "ge\u00b7blie\u00b7ben", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "PIS", "VAFIN", "PPER", "ADJD", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So sprachs aus Liedern, Bl\u00fcten, Lorberzweigen.", "tokens": ["So", "sprachs", "aus", "Lie\u00b7dern", ",", "Bl\u00fc\u00b7ten", ",", "Lor\u00b7ber\u00b7zwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nun f\u00fchl ich mich ermutigt und getrieben", "tokens": ["Nun", "f\u00fchl", "ich", "mich", "er\u00b7mu\u00b7tigt", "und", "ge\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVPP", "KON", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Begeisterungsfroh Euch meine Hand zu reichen", "tokens": ["Be\u00b7geis\u00b7te\u00b7rungs\u00b7froh", "Euch", "mei\u00b7ne", "Hand", "zu", "rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und warmen Dank zu sagen allen Lieben!\u00ab", "tokens": ["Und", "war\u00b7men", "Dank", "zu", "sa\u00b7gen", "al\u00b7len", "Lie\u00b7ben", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJA", "NN", "PTKZU", "VVINF", "PIAT", "ADJA", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}