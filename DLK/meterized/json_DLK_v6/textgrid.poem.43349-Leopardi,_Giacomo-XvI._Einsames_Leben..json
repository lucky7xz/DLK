{"textgrid.poem.43349": {"metadata": {"author": {"name": "Leopardi, Giacomo", "birth": "N.A.", "death": "N.A."}, "title": "XvI. Einsames Leben.", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am fr\u00fchen Tage, wenn mit Fl\u00fcgelschlagen", "tokens": ["Am", "fr\u00fc\u00b7hen", "Ta\u00b7ge", ",", "wenn", "mit", "Fl\u00fc\u00b7gel\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "KOUS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Henne munter im verschlossnen Hause", "tokens": ["Die", "Hen\u00b7ne", "mun\u00b7ter", "im", "ver\u00b7schloss\u00b7nen", "Hau\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sich regt und gackert und der Landbewohner", "tokens": ["Sich", "regt", "und", "ga\u00b7ckert", "und", "der", "Land\u00b7be\u00b7woh\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "VVFIN", "KON", "VVFIN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf den Altan hinaustritt, w\u00e4hrend zitternd", "tokens": ["Auf", "den", "Al\u00b7tan", "hin\u00b7aus\u00b7tritt", ",", "w\u00e4h\u00b7rend", "zit\u00b7ternd"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "KOUS", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Sonnenpfeile durch den Tropfenfall", "tokens": ["Die", "Son\u00b7nen\u00b7pfei\u00b7le", "durch", "den", "Tro\u00b7pfen\u00b7fall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Des Nebels dringen, weckt der Regen mich,", "tokens": ["Des", "Ne\u00b7bels", "drin\u00b7gen", ",", "weckt", "der", "Re\u00b7gen", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sacht an das Fenster meiner H\u00fctte klopfend.", "tokens": ["Sacht", "an", "das", "Fens\u00b7ter", "mei\u00b7ner", "H\u00fct\u00b7te", "klop\u00b7fend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da steh' ich auf, und jene leichten W\u00f6lkchen,", "tokens": ["Da", "steh'", "ich", "auf", ",", "und", "je\u00b7ne", "leich\u00b7ten", "W\u00f6lk\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der V\u00f6gel erstes Zwitschern und die Frische", "tokens": ["Der", "V\u00f6\u00b7gel", "ers\u00b7tes", "Zwit\u00b7schern", "und", "die", "Fri\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Der L\u00fcfte segn' ich und die heitren Fluren.", "tokens": ["Der", "L\u00fcf\u00b7te", "segn'", "ich", "und", "die", "hei\u00b7tren", "Flu\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Denn euch, der Stadt unsel'ge Mauern, sah ich", "tokens": ["Denn", "euch", ",", "der", "Stadt", "un\u00b7sel'\u00b7ge", "Mau\u00b7ern", ",", "sah", "ich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "$,", "ART", "NN", "ADJA", "NN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Nun lang genug und wei\u00df: in euch ist immer", "tokens": ["Nun", "lang", "ge\u00b7nug", "und", "wei\u00df", ":", "in", "euch", "ist", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADV", "KON", "VVFIN", "$.", "APPR", "PPER", "VAFIN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Dem Schmerz der Ha\u00df gesellt; ach, und in Schmerzen", "tokens": ["Dem", "Schmerz", "der", "Ha\u00df", "ge\u00b7sellt", ";", "ach", ",", "und", "in", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$.", "XY", "$,", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Leb' ich und sterbe so \u2013 wohl bald! Nur hier,", "tokens": ["Leb'", "ich", "und", "ster\u00b7be", "so", "\u2013", "wohl", "bald", "!", "Nur", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "KON", "VVFIN", "ADV", "$(", "ADV", "ADV", "$.", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "In diesen St\u00e4tten, g\u00f6nnt Natur, wie karg auch,", "tokens": ["In", "die\u00b7sen", "St\u00e4t\u00b7ten", ",", "g\u00f6nnt", "Na\u00b7tur", ",", "wie", "karg", "auch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "VVFIN", "NN", "$,", "PWAV", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Ein stilles Mitleid mir, dem sie dereinst", "tokens": ["Ein", "stil\u00b7les", "Mit\u00b7leid", "mir", ",", "dem", "sie", "de\u00b7reinst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPER", "$,", "PRELS", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "So huldvoll sich bewies! Und du auch wendest", "tokens": ["So", "huld\u00b7voll", "sich", "be\u00b7wies", "!", "Und", "du", "auch", "wen\u00b7dest"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PRF", "VVFIN", "$.", "KON", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Vom Ungl\u00fcck ab den Blick; auch du verschm\u00e4hst", "tokens": ["Vom", "Un\u00b7gl\u00fcck", "ab", "den", "Blick", ";", "auch", "du", "ver\u00b7schm\u00e4hst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$.", "ADV", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Die Armen und Beladnen, o Natur,", "tokens": ["Die", "Ar\u00b7men", "und", "Be\u00b7lad\u00b7nen", ",", "o", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Und huldigst nur dem Gl\u00fcck. So bleibt im Himmel", "tokens": ["Und", "hul\u00b7digst", "nur", "dem", "Gl\u00fcck", ".", "So", "bleibt", "im", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$.", "ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Kein Freund und auf der Erde keine Zuflucht", "tokens": ["Kein", "Freund", "und", "auf", "der", "Er\u00b7de", "kei\u00b7ne", "Zu\u00b7flucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "APPR", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.22": {"text": "Dem Ungl\u00fccksel'gen als ein scharfer Stahl.", "tokens": ["Dem", "Un\u00b7gl\u00fcck\u00b7sel'\u00b7gen", "als", "ein", "schar\u00b7fer", "Stahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zuweilen rast' ich einsam irgendwo", "tokens": ["Zu\u00b7wei\u00b7len", "rast'", "ich", "ein\u00b7sam", "ir\u00b7gend\u00b7wo"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf einem H\u00fcgel, an des Weihers Saum,", "tokens": ["Auf", "ei\u00b7nem", "H\u00fc\u00b7gel", ",", "an", "des", "Wei\u00b7hers", "Saum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von traurigstummen Pflanzen rings umkr\u00e4nzt.", "tokens": ["Von", "trau\u00b7rigs\u00b7tum\u00b7men", "Pflan\u00b7zen", "rings", "um\u00b7kr\u00e4nzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dort, wenn der Mittag sich an Himmel neigt,", "tokens": ["Dort", ",", "wenn", "der", "Mit\u00b7tag", "sich", "an", "Him\u00b7mel", "neigt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Spiegelt ihr ruhig Bild die hohe Sonne,", "tokens": ["Spie\u00b7gelt", "ihr", "ru\u00b7hig", "Bild", "die", "ho\u00b7he", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Im Winde regt sich weder Halm noch Blatt,", "tokens": ["Im", "Win\u00b7de", "regt", "sich", "we\u00b7der", "Halm", "noch", "Blatt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PRF", "KON", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Kein Wellchen kr\u00e4uselt sich, kein Heimchen h\u00f6rst du,", "tokens": ["Kein", "Well\u00b7chen", "kr\u00e4u\u00b7selt", "sich", ",", "kein", "Heim\u00b7chen", "h\u00f6rst", "du", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "$,", "PIAT", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nicht einen Vogel schwirren im Gezweig;", "tokens": ["Nicht", "ei\u00b7nen", "Vo\u00b7gel", "schwir\u00b7ren", "im", "Ge\u00b7zweig", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NE", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Kein Falter flattert, weit und breit, vernimmst", "tokens": ["Kein", "Fal\u00b7ter", "flat\u00b7tert", ",", "weit", "und", "breit", ",", "ver\u00b7nimmst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PIAT", "NN", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und siehst du Nichts, was t\u00f6nt und sich bewegt.", "tokens": ["Und", "siehst", "du", "Nichts", ",", "was", "t\u00f6nt", "und", "sich", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "$,", "PWS", "VVFIN", "KON", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Um diese Ufer webt die tiefste Ruhe,", "tokens": ["Um", "die\u00b7se", "U\u00b7fer", "webt", "die", "tiefs\u00b7te", "Ru\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PDAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Da\u00df fast der Welt und meiner selbst vergessend", "tokens": ["Da\u00df", "fast", "der", "Welt", "und", "mei\u00b7ner", "selbst", "ver\u00b7ges\u00b7send"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "KON", "PPOSAT", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Ich reglos sitze, ja mir ist, als w\u00e4ren", "tokens": ["Ich", "reg\u00b7los", "sit\u00b7ze", ",", "ja", "mir", "ist", ",", "als", "w\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADJD", "VVFIN", "$,", "ADV", "PPER", "VAFIN", "$,", "KOKOM", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Die Glieder mir gel\u00f6s't, kein Hauch, kein F\u00fchlen", "tokens": ["Die", "Glie\u00b7der", "mir", "ge\u00b7l\u00f6s't", ",", "kein", "Hauch", ",", "kein", "F\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PPER", "VVPP", "$,", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Bewegte sie, und ihre alte Ruhe", "tokens": ["Be\u00b7weg\u00b7te", "sie", ",", "und", "ih\u00b7re", "al\u00b7te", "Ru\u00b7he"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Verschm\u00f6lze mit der Stille dieses Orts.", "tokens": ["Ver\u00b7schm\u00f6l\u00b7ze", "mit", "der", "Stil\u00b7le", "die\u00b7ses", "Orts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O Lieb', o Liebe, wie so weit entflohst du", "tokens": ["O", "Lieb'", ",", "o", "Lie\u00b7be", ",", "wie", "so", "weit", "ent\u00b7flohst", "du"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "FM", "NN", "$,", "PWAV", "ADV", "ADJD", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von dieser Brust, die einst so warm gef\u00fchlt,", "tokens": ["Von", "die\u00b7ser", "Brust", ",", "die", "einst", "so", "warm", "ge\u00b7f\u00fchlt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ja, gl\u00fchend hei\u00df! Mit seiner kalten Hand", "tokens": ["Ja", ",", "gl\u00fc\u00b7hend", "hei\u00df", "!", "Mit", "sei\u00b7ner", "kal\u00b7ten", "Hand"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADJD", "ADJD", "$.", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ergriff das Ungl\u00fcck sie, bis sie vereis'te", "tokens": ["Er\u00b7griff", "das", "Un\u00b7gl\u00fcck", "sie", ",", "bis", "sie", "ver\u00b7eis'\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PPER", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Im Flor der Jahre. Jener Zeit gedenk' ich,", "tokens": ["Im", "Flor", "der", "Jah\u00b7re", ".", "Je\u00b7ner", "Zeit", "ge\u00b7denk'", "ich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$.", "PDAT", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da du mein Herz durchbebtest, jener s\u00fc\u00dfen,", "tokens": ["Da", "du", "mein", "Herz", "durch\u00b7beb\u00b7test", ",", "je\u00b7ner", "s\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ewig verlornen Zeit, wo sich zuerst", "tokens": ["E\u00b7wig", "ver\u00b7lor\u00b7nen", "Zeit", ",", "wo", "sich", "zu\u00b7erst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "$,", "PWAV", "PRF", "ADV"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Dem jungen Blick der Schauplatz dieser armen,", "tokens": ["Dem", "jun\u00b7gen", "Blick", "der", "Schau\u00b7platz", "die\u00b7ser", "ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PDAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Unsel'gen Welt er\