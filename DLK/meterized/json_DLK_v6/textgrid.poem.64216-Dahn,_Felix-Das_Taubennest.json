{"textgrid.poem.64216": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Das Taubennest", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Geschatt von dichten Zweigen", "tokens": ["Im", "Ge\u00b7schatt", "von", "dich\u00b7ten", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lag ich tief im Eichenhag,", "tokens": ["Lag", "ich", "tief", "im", "Ei\u00b7chen\u00b7hag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ringsum Waldesmittagschweigen:", "tokens": ["Ring\u00b7sum", "Wal\u00b7des\u00b7mit\u00b7tag\u00b7schwei\u00b7gen", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fern nur Spechtes Schnabelschlag.", "tokens": ["Fern", "nur", "Spech\u00b7tes", "Schna\u00b7bel\u00b7schlag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und ganz leise mir zur Seiten", "tokens": ["Und", "ganz", "lei\u00b7se", "mir", "zur", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rann der Moosquell wispernd hin:", "tokens": ["Rann", "der", "Moos\u00b7quell", "wis\u00b7pernd", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dr\u00fcber der Libelle Gleiten,", "tokens": ["Dr\u00fc\u00b7ber", "der", "Li\u00b7bel\u00b7le", "Glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Der beschwingten Schweberin.", "tokens": ["Der", "be\u00b7schwing\u00b7ten", "Schwe\u00b7be\u00b7rin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und ich dachte: \u00bbSch\u00f6n ist's einsam:", "tokens": ["Und", "ich", "dach\u00b7te", ":", "\u00bb", "Sch\u00f6n", "ist's", "ein\u00b7sam", ":"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "$(", "NE", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sang und Traum naht keinem Paar:", "tokens": ["Sang", "und", "Traum", "naht", "kei\u00b7nem", "Paar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber sch\u00f6ner ist's gemeinsam:", "tokens": ["A\u00b7ber", "sch\u00f6\u00b7ner", "ist's", "ge\u00b7mein\u00b7sam", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da wird Sang und Traum erst wahr.", "tokens": ["Da", "wird", "Sang", "und", "Traum", "erst", "wahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "KON", "NN", "ADV", "PTKVZ", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Walther, war es dir zum Besten,", "tokens": ["Walt\u00b7her", ",", "war", "es", "dir", "zum", "Bes\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "PPER", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df stets einsam bliebest du?\u00ab \u2013 \u2013 \u2013", "tokens": ["Da\u00df", "stets", "ein\u00b7sam", "blie\u00b7best", "du", "?", "\u00ab", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct", "punct", "punct"], "pos": ["KOUS", "ADV", "ADJD", "VVFIN", "PPER", "$.", "$(", "$(", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Horch, da hoch aus gr\u00fcnen \u00c4sten", "tokens": ["Horch", ",", "da", "hoch", "aus", "gr\u00fc\u00b7nen", "\u00c4s\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Scholl's hernieder: \u00bbRukuruh!\u00ab", "tokens": ["Scholl's", "her\u00b7nie\u00b7der", ":", "\u00bb", "Ru\u00b7ku\u00b7ruh", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["NE", "PTKVZ", "$.", "$(", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Oben in den Wipfellauben,", "tokens": ["O\u00b7ben", "in", "den", "Wip\u00b7fel\u00b7lau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tief im lauschigsten Versteck,", "tokens": ["Tief", "im", "lau\u00b7schigs\u00b7ten", "Ver\u00b7steck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Lag ein Nest von wilden Tauben", "tokens": ["Lag", "ein", "Nest", "von", "wil\u00b7den", "Tau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sie \u00e4tzten das Geheck.", "tokens": ["Und", "sie", "\u00e4tz\u00b7ten", "das", "Ge\u00b7heck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und ich sah \u2013 ich sah's mit Neiden,", "tokens": ["Und", "ich", "sah", "\u2013", "ich", "sah's", "mit", "Nei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$(", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich, der ungeweibte Mann, \u2013", "tokens": ["Ich", ",", "der", "un\u00b7ge\u00b7weib\u00b7te", "Mann", ",", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "$,", "ART", "ADJA", "NN", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie so eifrig da von beiden", "tokens": ["Wie", "so", "eif\u00b7rig", "da", "von", "bei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJD", "ADV", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Liebgetreues Werk begann.", "tokens": ["Lieb\u00b7ge\u00b7treu\u00b7es", "Werk", "be\u00b7gann", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wie die T\u00e4ubin, nimmer s\u00e4umig,", "tokens": ["Wie", "die", "T\u00e4u\u00b7bin", ",", "nim\u00b7mer", "s\u00e4u\u00b7mig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Flog zu Nest, gef\u00fcllt den Kropf,", "tokens": ["Flog", "zu", "Nest", ",", "ge\u00b7f\u00fcllt", "den", "Kropf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$,", "VVPP", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie der Nestling, wollefl\u00e4umig,", "tokens": ["Wie", "der", "Nest\u00b7ling", ",", "wol\u00b7le\u00b7fl\u00e4u\u00b7mig", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Reckte Fittich, Schopf und Kopf.", "tokens": ["Reck\u00b7te", "Fit\u00b7tich", ",", "Schopf", "und", "Kopf", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Wie dann auch der Tauber kehrte,", "tokens": ["Wie", "dann", "auch", "der", "Tau\u00b7ber", "kehr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "F\u00fctternd wechselnd mit dem Weib,", "tokens": ["F\u00fct\u00b7ternd", "wech\u00b7selnd", "mit", "dem", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dazwischen gurrend lehrte", "tokens": ["Und", "da\u00b7zwi\u00b7schen", "gur\u00b7rend", "lehr\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PAV", "ADJD", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "S\u00fc\u00dfer Weisen Zeitvertreib. \u2013 \u2013", "tokens": ["S\u00fc\u00b7\u00dfer", "Wei\u00b7sen", "Zeit\u00b7ver\u00b7treib", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["ADJA", "NN", "NN", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Herrin, ach von stolzem Sinne!", "tokens": ["Her\u00b7rin", ",", "ach", "von", "stol\u00b7zem", "Sin\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War der S\u00e4nger dir zu arm?", "tokens": ["War", "der", "S\u00e4n\u00b7ger", "dir", "zu", "arm", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "PTKA", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine Treue, seine Minne", "tokens": ["Sei\u00b7ne", "Treu\u00b7e", ",", "sei\u00b7ne", "Min\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "War wie keine treu und warm! \u2013", "tokens": ["War", "wie", "kei\u00b7ne", "treu", "und", "warm", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "KOKOM", "PIAT", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Walther auf! \u2013 Es neigt die Helle,", "tokens": ["Walt\u00b7her", "auf", "!", "\u2013", "Es", "neigt", "die", "Hel\u00b7le", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "$(", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tiefre Schatten fallen ein,", "tokens": ["Tief\u00b7re", "Schat\u00b7ten", "fal\u00b7len", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Walther, heimw\u00e4rts! Deine Zelle,", "tokens": ["Walt\u00b7her", ",", "heim\u00b7w\u00e4rts", "!", "Dei\u00b7ne", "Zel\u00b7le", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "$.", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, die leere, harret dein.", "tokens": ["Ach", ",", "die", "lee\u00b7re", ",", "har\u00b7ret", "dein", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "ADJA", "$,", "VVFIN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Nicht ganz leer! \u2013 Zum Notbedarfe", "tokens": ["Nicht", "ganz", "leer", "!", "\u2013", "Zum", "Not\u00b7be\u00b7dar\u00b7fe"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["PTKNEG", "ADV", "ADJD", "$.", "$(", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00f6stung dir dein Stern beschied:", "tokens": ["Tr\u00f6s\u00b7tung", "dir", "dein", "Stern", "be\u00b7schied", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine Hausfrau ist die Harfe,", "tokens": ["Dei\u00b7ne", "Haus\u00b7frau", "ist", "die", "Har\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dein Kind dein ewig Lied.", "tokens": ["Und", "dein", "Kind", "dein", "e\u00b7wig", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPOSAT", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}