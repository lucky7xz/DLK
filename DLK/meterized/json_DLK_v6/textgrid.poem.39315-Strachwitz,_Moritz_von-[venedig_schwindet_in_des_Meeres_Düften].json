{"textgrid.poem.39315": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "[venedig schwindet in des Meeres D\u00fcften]", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Venedig schwindet in des Meeres D\u00fcften,", "tokens": ["Ve\u00b7ne\u00b7dig", "schwin\u00b7det", "in", "des", "Mee\u00b7res", "D\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Schon rankt sich farbig in Gewind' und Lauben", "tokens": ["Schon", "rankt", "sich", "far\u00b7big", "in", "Ge\u00b7wind'", "und", "Lau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Herbstes Rebe \u00fcber samtnen Triften.", "tokens": ["Des", "Herbs\u00b7tes", "Re\u00b7be", "\u00fc\u00b7ber", "samt\u00b7nen", "Trif\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der erste Staub beginnt am Weg zu stauben,", "tokens": ["Der", "ers\u00b7te", "Staub", "be\u00b7ginnt", "am", "Weg", "zu", "stau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und fast verwundert h\u00f6r' ich wieder Pferde", "tokens": ["Und", "fast", "ver\u00b7wun\u00b7dert", "h\u00f6r'", "ich", "wie\u00b7der", "Pfer\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "PPER", "ADV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nach langer Zeit im Morgenwinde schnauben.", "tokens": ["Nach", "lan\u00b7ger", "Zeit", "im", "Mor\u00b7gen\u00b7win\u00b7de", "schnau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch fast erbittert mich die gr\u00fcne Erde! \u2013", "tokens": ["Doch", "fast", "er\u00b7bit\u00b7tert", "mich", "die", "gr\u00fc\u00b7ne", "Er\u00b7de", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du edle Stadt aus Wasser und aus Steinen,", "tokens": ["Du", "ed\u00b7le", "Stadt", "aus", "Was\u00b7ser", "und", "aus", "Stei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wei\u00df Gott, wann ich Dich wiedersehen werde.", "tokens": ["Wei\u00df", "Gott", ",", "wann", "ich", "Dich", "wie\u00b7der\u00b7se\u00b7hen", "wer\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "PWAV", "PPER", "PRF", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Als wie ein Traumbild willst Du uns erscheinen,", "tokens": ["Als", "wie", "ein", "Traum\u00b7bild", "willst", "Du", "uns", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wie ein Traumbild seh' ich Dich verwehen,", "tokens": ["Und", "wie", "ein", "Traum\u00b7bild", "seh'", "ich", "Dich", "ver\u00b7we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und schaue nach und m\u00f6cht' am liebsten weinen!", "tokens": ["Und", "schau\u00b7e", "nach", "und", "m\u00f6cht'", "am", "liebs\u00b7ten", "wei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "KON", "VMFIN", "PTKA", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Denn wer geh\u00f6rt das Lied der Meeresfeen,", "tokens": ["Denn", "wer", "ge\u00b7h\u00f6rt", "das", "Lied", "der", "Mee\u00b7res\u00b7feen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der starrt so lang' ins feuchte Aug' der Tiefe,", "tokens": ["Der", "starrt", "so", "lang'", "ins", "feuch\u00b7te", "Aug'", "der", "Tie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bis er versinkt \u2013 und wird nicht mehr gesehen!", "tokens": ["Bis", "er", "ver\u00b7sinkt", "\u2013", "und", "wird", "nicht", "mehr", "ge\u00b7se\u00b7hen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$(", "KON", "VAFIN", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und wenn mich nicht ein andres Auge riefe,", "tokens": ["Und", "wenn", "mich", "nicht", "ein", "and\u00b7res", "Au\u00b7ge", "rie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So scheu und tief, wie Adria's Gew\u00e4sser,", "tokens": ["So", "scheu", "und", "tief", ",", "wie", "A\u00b7dria's", "Ge\u00b7w\u00e4s\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,", "PWAV", "NE", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Geblieben w\u00e4r' ich, bis mein Herz entschliefe \u2013", "tokens": ["Ge\u00b7blie\u00b7ben", "w\u00e4r'", "ich", ",", "bis", "mein", "Herz", "ent\u00b7schlie\u00b7fe", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Und nirgend schl\u00e4ft ein m\u00fcdes Herze besser!", "tokens": ["Und", "nir\u00b7gend", "schl\u00e4ft", "ein", "m\u00fc\u00b7des", "Her\u00b7ze", "bes\u00b7ser", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}