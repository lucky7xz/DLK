{"dta.poem.2983": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "29.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Mutter gibt zum Fest den lieben Kindern Gaben,", "tokens": ["Die", "Mut\u00b7ter", "gibt", "zum", "Fest", "den", "lie\u00b7ben", "Kin\u00b7dern", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und alle danken ihr, was sie empfangen haben.", "tokens": ["Und", "al\u00b7le", "dan\u00b7ken", "ihr", ",", "was", "sie", "emp\u00b7fan\u00b7gen", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "$,", "PRELS", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie dr\u00e4ngen sich mit Dank um sie, und sagen nichts", "tokens": ["Sie", "dr\u00e4n\u00b7gen", "sich", "mit", "Dank", "um", "sie", ",", "und", "sa\u00b7gen", "nichts"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "APPR", "PPER", "$,", "KON", "VVFIN", "PIS"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Vater, der dabei steht ernsten Angesichts.", "tokens": ["Dem", "Va\u00b7ter", ",", "der", "da\u00b7bei", "steht", "erns\u00b7ten", "An\u00b7ge\u00b7sichts", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PAV", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Den Vater wird es wol verdrie\u00dfen, da\u00df die Kinder", "tokens": ["Den", "Va\u00b7ter", "wird", "es", "wol", "ver\u00b7drie\u00b7\u00dfen", ",", "da\u00df", "die", "Kin\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVINF", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur auf die Mutter schaun, und nicht auf ihn? Nichts minder.", "tokens": ["Nur", "auf", "die", "Mut\u00b7ter", "schaun", ",", "und", "nicht", "auf", "ihn", "?", "Nichts", "min\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "$,", "KON", "PTKNEG", "APPR", "PPER", "$.", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ihn freut die kindische, die gl\u00fcckliche Beschr\u00e4nkung,", "tokens": ["Ihn", "freut", "die", "kin\u00b7di\u00b7sche", ",", "die", "gl\u00fcck\u00b7li\u00b7che", "Be\u00b7schr\u00e4n\u00b7kung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und was die Mutter ehrt, gereicht ihm nicht zur Kr\u00e4nkung.", "tokens": ["Und", "was", "die", "Mut\u00b7ter", "ehrt", ",", "ge\u00b7reicht", "ihm", "nicht", "zur", "Kr\u00e4n\u00b7kung", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ihn freut die gl\u00fcckliche, die kindische Beschr\u00e4nkung,", "tokens": ["Ihn", "freut", "die", "gl\u00fcck\u00b7li\u00b7che", ",", "die", "kin\u00b7di\u00b7sche", "Be\u00b7schr\u00e4n\u00b7kung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die nach dem ersten Grund nicht fragt der Festbeschenkung,", "tokens": ["Die", "nach", "dem", "ers\u00b7ten", "Grund", "nicht", "fragt", "der", "Fest\u00b7be\u00b7schen\u00b7kung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "PTKNEG", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Nicht nachdenkt, da\u00df dazu, was unter ihrem Titel", "tokens": ["Nicht", "nach\u00b7denkt", ",", "da\u00df", "da\u00b7zu", ",", "was", "un\u00b7ter", "ih\u00b7rem", "Ti\u00b7tel"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVPP", "$,", "KOUS", "PAV", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Die Mutter gibt, ihr selbst der Vater gab die Mittel.", "tokens": ["Die", "Mut\u00b7ter", "gibt", ",", "ihr", "selbst", "der", "Va\u00b7ter", "gab", "die", "Mit\u00b7tel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "ADV", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wer sind die Kinder? wer die Mutter? und wer ist", "tokens": ["Wer", "sind", "die", "Kin\u00b7der", "?", "wer", "die", "Mut\u00b7ter", "?", "und", "wer", "ist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "PWS", "ART", "NN", "$.", "KON", "PWS", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Vater? rathe das, wenn du ein Rather bist.", "tokens": ["Der", "Va\u00b7ter", "?", "ra\u00b7the", "das", ",", "wenn", "du", "ein", "Ra\u00b7ther", "bist", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "VVFIN", "PDS", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}