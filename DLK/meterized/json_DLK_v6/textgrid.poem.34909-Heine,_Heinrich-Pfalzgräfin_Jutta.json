{"textgrid.poem.34909": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Pfalzgr\u00e4fin Jutta", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Pfalzgr\u00e4fin Jutta fuhr \u00fcber den Rhein,", "tokens": ["Pfalz\u00b7gr\u00e4\u00b7fin", "Jut\u00b7ta", "fuhr", "\u00fc\u00b7ber", "den", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "APPR", "ART", "NE", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Im leichten Kahn, bei Mondenschein.", "tokens": ["Im", "leich\u00b7ten", "Kahn", ",", "bei", "Mon\u00b7den\u00b7schein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Zofe rudert, die Gr\u00e4fin spricht:", "tokens": ["Die", "Zo\u00b7fe", "ru\u00b7dert", ",", "die", "Gr\u00e4\u00b7fin", "spricht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u00bbsiehst du die sieben Leichen nicht,", "tokens": ["\u00bb", "siehst", "du", "die", "sie\u00b7ben", "Lei\u00b7chen", "nicht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ART", "CARD", "NN", "PTKNEG", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Die hinter uns kommen", "tokens": ["Die", "hin\u00b7ter", "uns", "kom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Einhergeschwommen \u2013", "tokens": ["Ein\u00b7her\u00b7ge\u00b7schwom\u00b7men", "\u2013"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "So traurig schwimmen die Toten!", "tokens": ["So", "trau\u00b7rig", "schwim\u00b7men", "die", "To\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Das waren Ritter voll Jugendlust \u2013", "tokens": ["Das", "wa\u00b7ren", "Rit\u00b7ter", "voll", "Ju\u00b7gend\u00b7lust", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "ADJD", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie sanken z\u00e4rtlich an meine Brust", "tokens": ["Sie", "san\u00b7ken", "z\u00e4rt\u00b7lich", "an", "mei\u00b7ne", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und schwuren mir Treue \u2013 Zur Sicherheit,", "tokens": ["Und", "schwu\u00b7ren", "mir", "Treu\u00b7e", "\u2013", "Zur", "Si\u00b7cher\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "$(", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Da\u00df sie nicht br\u00e4chen ihren Eid,", "tokens": ["Da\u00df", "sie", "nicht", "br\u00e4\u00b7chen", "ih\u00b7ren", "Eid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Lie\u00df ich sie ergreifen", "tokens": ["Lie\u00df", "ich", "sie", "er\u00b7grei\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Sogleich und ers\u00e4ufen \u2013", "tokens": ["Sog\u00b7leich", "und", "er\u00b7s\u00e4u\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "KON", "VVPP", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "So traurig schwimmen die Toten!\u00ab", "tokens": ["So", "trau\u00b7rig", "schwim\u00b7men", "die", "To\u00b7ten", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Die Zofe rudert, die Gr\u00e4fin lacht.", "tokens": ["Die", "Zo\u00b7fe", "ru\u00b7dert", ",", "die", "Gr\u00e4\u00b7fin", "lacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das hallt so h\u00f6hnisch durch die Nacht!", "tokens": ["Das", "hallt", "so", "h\u00f6h\u00b7nisch", "durch", "die", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis an die H\u00fcfte tauchen hervor", "tokens": ["Bis", "an", "die", "H\u00fcf\u00b7te", "tau\u00b7chen", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Die Leichen und strecken die Finger empor,", "tokens": ["Die", "Lei\u00b7chen", "und", "stre\u00b7cken", "die", "Fin\u00b7ger", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Wie schw\u00f6rend \u2013 Sie nicken", "tokens": ["Wie", "schw\u00f6\u00b7rend", "\u2013", "Sie", "ni\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADJD", "$(", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Mit gl\u00e4sernen Blicken \u2013", "tokens": ["Mit", "gl\u00e4\u00b7ser\u00b7nen", "Bli\u00b7cken", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "So traurig schwimmen die Toten!", "tokens": ["So", "trau\u00b7rig", "schwim\u00b7men", "die", "To\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}