{"dta.poem.3836": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen Vater/  \n  Paul Spindler.  1731.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6chstgeliebtes Vater-Hertz!", "tokens": ["H\u00f6chst\u00b7ge\u00b7lieb\u00b7tes", "Va\u00b7ter\u00b7Hertz", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der du mich so treu erzogen,", "tokens": ["Der", "du", "mich", "so", "treu", "er\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Macht dein Sohn, o banger Schmertz!", "tokens": ["Macht", "dein", "Sohn", ",", "o", "ban\u00b7ger", "Schmertz", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "FM", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir die Schrifft zum Ehren-Bogen?", "tokens": ["Dir", "die", "Schrifft", "zum", "Eh\u00b7ren\u00b7Bo\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "O! hier starret Kiel und Geist,", "tokens": ["O", "!", "hier", "star\u00b7ret", "Kiel", "und", "Geist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Meinen Vorsatz hemmt der Jammer,", "tokens": ["Mei\u00b7nen", "Vor\u00b7satz", "hemmt", "der", "Jam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da der Tod dich uns entrei\u00dft,", "tokens": ["Da", "der", "Tod", "dich", "uns", "ent\u00b7rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "PPER", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "Ruhe sanfft in deiner Kammer.", "tokens": ["Ru\u00b7he", "sanfft", "in", "dei\u00b7ner", "Kam\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Du gehst in das Zion ein,", "tokens": ["Du", "gehst", "in", "das", "Zi\u00b7on", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Gott sey k\u00fcnfftig mein Berather,", "tokens": ["Gott", "sey", "k\u00fcnff\u00b7tig", "mein", "Be\u00b7ra\u00b7ther", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Dieses soll die Grabschrifft seyn:", "tokens": ["Die\u00b7ses", "soll", "die", "Grab\u00b7schrifft", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Ach! hier liegt mein ", "tokens": ["Ach", "!", "hier", "liegt", "mein"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPOSAT"], "meter": "-+-+", "measure": "iambic.di"}}}}}