{"textgrid.poem.66403": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Toleranz", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der dicke Franz nahm eine Hur' ins Haus.", "tokens": ["Der", "di\u00b7cke", "Franz", "nahm", "ei\u00b7ne", "Hur'", "ins", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sein Nachbar Melcher sprach:", "tokens": ["Sein", "Nach\u00b7bar", "Melc\u00b7her", "sprach", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ei Franz, jag doch das Mensch hinaus!", "tokens": ["Ei", "Franz", ",", "jag", "doch", "das", "Mensch", "hin\u00b7aus", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im ganzen Dorf spricht man dir Uebels nach.", "tokens": ["Im", "gan\u00b7zen", "Dorf", "spricht", "man", "dir", "Ue\u00b7bels", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PIS", "PPER", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Hm, sprach der aufgekl\u00e4rte Franz,", "tokens": ["Hm", ",", "sprach", "der", "auf\u00b7ge\u00b7kl\u00e4r\u00b7te", "Franz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "'s ist dummes Volk, wei\u00df nichts von Toleranz.", "tokens": ["'s", "ist", "dum\u00b7mes", "Volk", ",", "wei\u00df", "nichts", "von", "To\u00b7le\u00b7ranz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "$,", "VVFIN", "PIS", "APPR", "NE", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}