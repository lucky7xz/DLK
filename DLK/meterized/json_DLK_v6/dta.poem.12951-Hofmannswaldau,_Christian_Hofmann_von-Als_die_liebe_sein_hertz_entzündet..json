{"dta.poem.12951": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als die liebe sein hertz entz\u00fcndet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hast du mein hertze dir zur herberg ausersehn;", "tokens": ["Hast", "du", "mein", "hert\u00b7ze", "dir", "zur", "her\u00b7berg", "au\u00b7ser\u00b7sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So sage mir, o blinder Cypripor!", "tokens": ["So", "sa\u00b7ge", "mir", ",", "o", "blin\u00b7der", "Cyp\u00b7ri\u00b7por", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "We\u00dfwegen nimmst du denn so tolle streiche vor,", "tokens": ["We\u00df\u00b7we\u00b7gen", "nimmst", "du", "denn", "so", "tol\u00b7le", "strei\u00b7che", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "ADJA", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df du mit eigner hand dein eigen haus verbrennest.", "tokens": ["Da\u00df", "du", "mit", "eig\u00b7ner", "hand", "dein", "ei\u00b7gen", "haus", "ver\u00b7bren\u00b7nest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "PPOSAT", "NN", "APZR", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nun seh ich, da\u00df du auch", "tokens": ["Nun", "seh", "ich", ",", "da\u00df", "du", "auch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Selbst deinen nutzen nicht erkennest;", "tokens": ["Selbst", "dei\u00b7nen", "nut\u00b7zen", "nicht", "er\u00b7ken\u00b7nest", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Denn dencke doch, ist dis so dein gebrauch,", "tokens": ["Denn", "den\u00b7cke", "doch", ",", "ist", "dis", "so", "dein", "ge\u00b7brauch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "VAFIN", "PDS", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wer wird sich wol hinfort beqvemen,", "tokens": ["Wer", "wird", "sich", "wol", "hin\u00b7fort", "be\u00b7qve\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PRF", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Dich in sein haus, ja gar ins hertz zu nehmen?", "tokens": ["Dich", "in", "sein", "haus", ",", "ja", "gar", "ins", "hertz", "zu", "neh\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "$,", "ADV", "ADV", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}