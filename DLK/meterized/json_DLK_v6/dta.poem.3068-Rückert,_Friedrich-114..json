{"dta.poem.3068": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "114.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Sehnen strebet vor, und strebet nicht zur\u00fcck;", "tokens": ["Mein", "Seh\u00b7nen", "stre\u00b7bet", "vor", ",", "und", "stre\u00b7bet", "nicht", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht die Vergangenheit, die Zukunft ist mein Gl\u00fcck.", "tokens": ["Nicht", "die", "Ver\u00b7gan\u00b7gen\u00b7heit", ",", "die", "Zu\u00b7kunft", "ist", "mein", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$,", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mein Sehnen strebet vor und eilet mir voraus,", "tokens": ["Mein", "Seh\u00b7nen", "stre\u00b7bet", "vor", "und", "ei\u00b7let", "mir", "vo\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es schwebet dort empor, und ist schon dort zuhaus.", "tokens": ["Es", "schwe\u00b7bet", "dort", "em\u00b7por", ",", "und", "ist", "schon", "dort", "zu\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$,", "KON", "VAFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es ist schon dort zuhaus, wann ich ihm komme nach,", "tokens": ["Es", "ist", "schon", "dort", "zu\u00b7haus", ",", "wann", "ich", "ihm", "kom\u00b7me", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PTKVZ", "$,", "PWAV", "PPER", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann zeigt es dort mir das, was hier es mir versprach.", "tokens": ["Dann", "zeigt", "es", "dort", "mir", "das", ",", "was", "hier", "es", "mir", "ver\u00b7sprach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "PDS", "$,", "PRELS", "ADV", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}