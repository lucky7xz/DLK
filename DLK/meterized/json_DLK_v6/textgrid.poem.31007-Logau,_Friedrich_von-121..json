{"textgrid.poem.31007": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "121.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcrstin, hier ist nur der Wille; hier ist, F\u00fcrstin, kein Verm\u00fcgen,", "tokens": ["F\u00fcrs\u00b7tin", ",", "hier", "ist", "nur", "der", "Wil\u00b7le", ";", "hier", "ist", ",", "F\u00fcrs\u00b7tin", ",", "kein", "Ver\u00b7m\u00fc\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VAFIN", "ADV", "ART", "NN", "$.", "ADV", "VAFIN", "$,", "NN", "$,", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Das in etwas Euch zu Ehren meine Reime solten t\u00fcgen.", "tokens": ["Das", "in", "et\u00b7was", "Euch", "zu", "Eh\u00b7ren", "mei\u00b7ne", "Rei\u00b7me", "sol\u00b7ten", "t\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PIS", "PPER", "APPR", "NN", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Nehmet hin den schlechten Willen, gebet nur ein klein Belieben,", "tokens": ["Neh\u00b7met", "hin", "den", "schlech\u00b7ten", "Wil\u00b7len", ",", "ge\u00b7bet", "nur", "ein", "klein", "Be\u00b7lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,", "VVFIN", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+---+-+-", "measure": "unknown.measure.septa"}, "line.4": {"text": "Ey, so wird ein ieder glauben, da\u00df ich k\u00f6stlich Ding geschrieben.", "tokens": ["Ey", ",", "so", "wird", "ein", "ie\u00b7der", "glau\u00b7ben", ",", "da\u00df", "ich", "k\u00f6st\u00b7lich", "Ding", "ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VAFIN", "ART", "PIS", "VVINF", "$,", "KOUS", "PPER", "ADJD", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}