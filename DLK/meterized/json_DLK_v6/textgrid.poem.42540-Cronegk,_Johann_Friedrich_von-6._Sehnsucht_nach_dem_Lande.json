{"textgrid.poem.42540": {"metadata": {"author": {"name": "Cronegk, Johann Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "6. Sehnsucht nach dem Lande", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Feld, wo mein Geist, von L\u00e4rm entfernet,", "tokens": ["Feld", ",", "wo", "mein", "Geist", ",", "von", "L\u00e4rm", "ent\u00b7fer\u00b7net", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPOSAT", "NN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Das Gl\u00fcck der Ruhe f\u00fchlen lernet,", "tokens": ["Das", "Gl\u00fcck", "der", "Ru\u00b7he", "f\u00fch\u00b7len", "ler\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Klein wie mein Wunsch, still wie mein Herz!", "tokens": ["Klein", "wie", "mein", "Wunsch", ",", "still", "wie", "mein", "Herz", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PPOSAT", "NN", "$,", "ADJD", "KOKOM", "PPOSAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Wann f\u00fchl' ich einst, der Welt verborgen,", "tokens": ["Wann", "f\u00fchl'", "ich", "einst", ",", "der", "Welt", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In dir den Fr\u00fchling und den Morgen,", "tokens": ["In", "dir", "den", "Fr\u00fch\u00b7ling", "und", "den", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zwar ohne Lust, doch ohne Schmerz,", "tokens": ["Zwar", "oh\u00b7ne", "Lust", ",", "doch", "oh\u00b7ne", "Schmerz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zwar ohne Ruhm, doch ohne Sorgen?", "tokens": ["Zwar", "oh\u00b7ne", "Ruhm", ",", "doch", "oh\u00b7ne", "Sor\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wann k\u00f6mmt die Zeit, geliebtes Feld,", "tokens": ["Wann", "k\u00f6mmt", "die", "Zeit", ",", "ge\u00b7lieb\u00b7tes", "Feld", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da\u00df ich zufrieden in dir wohne?", "tokens": ["Da\u00df", "ich", "zu\u00b7frie\u00b7den", "in", "dir", "woh\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die Rosen sind mir eine Krone,", "tokens": ["Die", "Ro\u00b7sen", "sind", "mir", "ei\u00b7ne", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und diese T\u00e4ler eine Welt.", "tokens": ["Und", "die\u00b7se", "T\u00e4\u00b7ler", "ei\u00b7ne", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "T\u00f6nt freudig in dem Haine wieder!", "tokens": ["T\u00f6nt", "freu\u00b7dig", "in", "dem", "Hai\u00b7ne", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "O w\u00e4rt ihr, ungezwungne Lieder,", "tokens": ["O", "w\u00e4rt", "ihr", ",", "un\u00b7ge\u00b7zwung\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Sch\u00f6n ohne Kunst wie dieses Feld!", "tokens": ["Sch\u00f6n", "oh\u00b7ne", "Kunst", "wie", "die\u00b7ses", "Feld", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "KOKOM", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Nach Ruhm und Geld will ich nicht streben;", "tokens": ["Nach", "Ruhm", "und", "Geld", "will", "ich", "nicht", "stre\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VMFIN", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Mich reizen Freuden ohne M\u00fch':", "tokens": ["Mich", "rei\u00b7zen", "Freu\u00b7den", "oh\u00b7ne", "M\u00fch'", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Die stille Weisheit kann sie geben.", "tokens": ["Die", "stil\u00b7le", "Weis\u00b7heit", "kann", "sie", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Mein Lied sei reich an Harmonie,", "tokens": ["Mein", "Lied", "sei", "reich", "an", "Har\u00b7mo\u00b7nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Doch noch harmonischer mein Leben!", "tokens": ["Doch", "noch", "har\u00b7mo\u00b7ni\u00b7scher", "mein", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}}}}