{"textgrid.poem.57350": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbselmar, dein Wort: Du erscheinst, stirbst du vor mir,", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbselmar, dein Wort: Du erscheinst, stirbst du vor mir,", "tokens": ["\u00bb", "sel\u00b7mar", ",", "dein", "Wort", ":", "Du", "er\u00b7scheinst", ",", "stirbst", "du", "vor", "mir", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Deiner Selma! O geuss den Balsam", "tokens": ["Dei\u00b7ner", "Sel\u00b7ma", "!", "O", "geuss", "den", "Bal\u00b7sam"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NE", "$.", "NE", "NE", "ART", "NN"], "meter": "+----+-+-", "measure": "dactylic.init"}, "line.3": {"text": "In die Wunde der verlassnen,", "tokens": ["In", "die", "Wun\u00b7de", "der", "ver\u00b7lass\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Selmar, dein heiliges Wort!\u00ab", "tokens": ["Sel\u00b7mar", ",", "dein", "hei\u00b7li\u00b7ges", "Wort", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Selma, dein Wort: Du erscheinst, stirbst du vor mir,", "tokens": ["Sel\u00b7ma", ",", "dein", "Wort", ":", "Du", "er\u00b7scheinst", ",", "stirbst", "du", "vor", "mir", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "+---+-+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Deinem Selmar! O geuss den Balsam", "tokens": ["Dei\u00b7nem", "Sel\u00b7mar", "!", "O", "geuss", "den", "Bal\u00b7sam"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "NE", "NE", "ART", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "In die Wunde des verlassnen,", "tokens": ["In", "die", "Wun\u00b7de", "des", "ver\u00b7lass\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Selma, dein heiliges Wort!", "tokens": ["Sel\u00b7ma", ",", "dein", "hei\u00b7li\u00b7ges", "Wort", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Aber kann es, wer schied, kann er sein Bild", "tokens": ["A\u00b7ber", "kann", "es", ",", "wer", "schied", ",", "kann", "er", "sein", "Bild"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "$,", "PWS", "VVFIN", "$,", "VMFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Schaffen dem wartenden Blick des Freundes,", "tokens": ["Schaf\u00b7fen", "dem", "war\u00b7ten\u00b7den", "Blick", "des", "Freun\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Der verstummend ihm zur\u00fcckblieb", "tokens": ["Der", "ver\u00b7stum\u00b7mend", "ihm", "zu\u00b7r\u00fcck\u00b7blieb"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "VVPP", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An der trennenden Gruft?", "tokens": ["An", "der", "tren\u00b7nen\u00b7den", "Gruft", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Zeigen kann ich vielleicht, dass ich dir nah,", "tokens": ["Zei\u00b7gen", "kann", "ich", "viel\u00b7leicht", ",", "dass", "ich", "dir", "nah", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "PPER", "ADJD", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Dass ich dein Selmar noch hin! durch Zeichen,", "tokens": ["Dass", "ich", "dein", "Sel\u00b7mar", "noch", "hin", "!", "durch", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "PTKVZ", "$.", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Die gewiss dir, wie Erscheinung,", "tokens": ["Die", "ge\u00b7wiss", "dir", ",", "wie", "Er\u00b7schei\u00b7nung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPER", "$,", "PWAV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und nicht schrecklich dir sind.", "tokens": ["Und", "nicht", "schreck\u00b7lich", "dir", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADJD", "PPER", "VAFIN", "$."], "meter": "--+-++", "measure": "anapaest.init"}}, "stanza.5": {"line.1": {"text": "\u00bbwenn einst, Selmar, im Lenz unter dem Baum", "tokens": ["\u00bb", "wenn", "einst", ",", "Sel\u00b7mar", ",", "im", "Lenz", "un\u00b7ter", "dem", "Baum"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "KOUS", "ADV", "$,", "NE", "$,", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.2": {"text": "Junge Bl\u00fcthe dich labt; dann giess' ich,", "tokens": ["Jun\u00b7ge", "Bl\u00fc\u00b7the", "dich", "labt", ";", "dann", "giess'", "ich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Wie den Regen, der nicht tr\u00e4ufelt,", "tokens": ["Wie", "den", "Re\u00b7gen", ",", "der", "nicht", "tr\u00e4u\u00b7felt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zeigend, auf dich sie herab.\u00ab", "tokens": ["Zei\u00b7gend", ",", "auf", "dich", "sie", "her\u00b7ab", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "$,", "APPR", "PPER", "PPER", "ADV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Weilst du der Nachtigall einst, Selma, im Lenz;", "tokens": ["Weilst", "du", "der", "Nach\u00b7ti\u00b7gall", "einst", ",", "Sel\u00b7ma", ",", "im", "Lenz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$,", "NE", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Send' ich zu dir sie herab; sie fliegt dir", "tokens": ["Send'", "ich", "zu", "dir", "sie", "her\u00b7ab", ";", "sie", "fliegt", "dir"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "PPER", "ADV", "$.", "PPER", "VVFIN", "PPER"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Auf die Schulter, und sie singt da", "tokens": ["Auf", "die", "Schul\u00b7ter", ",", "und", "sie", "singt", "da"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Neuer als jemals, und stirbt.", "tokens": ["Neu\u00b7er", "als", "je\u00b7mals", ",", "und", "stirbt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADV", "$,", "KON", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbnein, nicht Zerst\u00f6rung! vom Baum l\u00f6s' ich die Frucht", "tokens": ["\u00bb", "nein", ",", "nicht", "Zer\u00b7st\u00f6\u00b7rung", "!", "vom", "Baum", "l\u00f6s'", "ich", "die", "Frucht"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PTKANT", "$,", "PTKNEG", "NN", "$.", "APPRART", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit der Bl\u00fcthe nicht ab; den Liebling,", "tokens": ["Mit", "der", "Bl\u00fc\u00b7the", "nicht", "ab", ";", "den", "Lieb\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKNEG", "PTKVZ", "$.", "ART", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Der noch wach ist, mir au fl\u00f6ten,", "tokens": ["Der", "noch", "wach", "ist", ",", "mir", "au", "fl\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VAFIN", "$,", "PPER", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Selmar, den t\u00f6dtest du nicht!", "tokens": ["Sel\u00b7mar", ",", "den", "t\u00f6d\u00b7test", "du", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Wenn kaum rege das Laub, leise der Bach", "tokens": ["Wenn", "kaum", "re\u00b7ge", "das", "Laub", ",", "lei\u00b7se", "der", "Bach"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADJA", "ART", "NN", "$,", "ADJD", "ART", "NN"], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Einst dir rauschen; du h\u00f6rst dann lautre", "tokens": ["Einst", "dir", "rau\u00b7schen", ";", "du", "h\u00f6rst", "dann", "laut\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVINF", "$.", "PPER", "VVFIN", "ADV", "VVFIN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Melodicen, die du kennest,", "tokens": ["Me\u00b7lo\u00b7di\u00b7cen", ",", "die", "du", "ken\u00b7nest", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "T\u00f6ne, wie Selma's Gesang.", "tokens": ["T\u00f6\u00b7ne", ",", "wie", "Sel\u00b7ma's", "Ge\u00b7sang", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Wenn nach Wettern mein Blick zu des Olymps", "tokens": ["Wenn", "nach", "Wet\u00b7tern", "mein", "Blick", "zu", "des", "O\u00b7lymps"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Hohem Bogen sich hebt; dann seh' ich,", "tokens": ["Ho\u00b7hem", "Bo\u00b7gen", "sich", "hebt", ";", "dann", "seh'", "ich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "An dem Rande des Gem\u00e4hldes,", "tokens": ["An", "dem", "Ran\u00b7de", "des", "Ge\u00b7m\u00e4hl\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fl\u00e4mchen erwachen, und wehn.", "tokens": ["Fl\u00e4m\u00b7chen", "er\u00b7wa\u00b7chen", ",", "und", "wehn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "KON", "VVINF", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.10": {"line.1": {"text": "Selma, mein Wort: Du erblickst, sterb' ich vor dir,", "tokens": ["Sel\u00b7ma", ",", "mein", "Wort", ":", "Du", "er\u00b7blickst", ",", "sterb'", "ich", "vor", "dir", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Wehende Fl\u00e4mchen! \u00bbMein Wort: Du h\u00f6rest,", "tokens": ["We\u00b7hen\u00b7de", "Fl\u00e4m\u00b7chen", "!", "\u00bb", "Mein", "Wort", ":", "Du", "h\u00f6\u00b7rest", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "$(", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Mit den Bl\u00e4ttern, und dem Bache,", "tokens": ["Mit", "den", "Bl\u00e4t\u00b7tern", ",", "und", "dem", "Ba\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "T\u00f6ne, wie Selma's Gesang!", "tokens": ["T\u00f6\u00b7ne", ",", "wie", "Sel\u00b7ma's", "Ge\u00b7sang", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}