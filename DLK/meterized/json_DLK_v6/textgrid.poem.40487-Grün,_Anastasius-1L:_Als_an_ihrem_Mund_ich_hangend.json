{"textgrid.poem.40487": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als an ihrem Mund ich hangend", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als an ihrem Mund ich hangend", "tokens": ["Als", "an", "ih\u00b7rem", "Mund", "ich", "han\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sog noch ihren Odem ein,", "tokens": ["Sog", "noch", "ih\u00b7ren", "O\u00b7dem", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tr\u00e4umt' ich viel von Tod und Trennung", "tokens": ["Tr\u00e4umt'", "ich", "viel", "von", "Tod", "und", "Tren\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und von Sarg und Leichenstein.", "tokens": ["Und", "von", "Sarg", "und", "Lei\u00b7chen\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nun ich steh' an ihrem Grabe,", "tokens": ["Nun", "ich", "steh'", "an", "ih\u00b7rem", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00e4um' ich nur von Liebesgru\u00df,", "tokens": ["Tr\u00e4um'", "ich", "nur", "von", "Lie\u00b7bes\u00b7gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wie ihre Wangen gl\u00fchten,", "tokens": ["Und", "wie", "ih\u00b7re", "Wan\u00b7gen", "gl\u00fch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und von ihrem ersten Ku\u00df.", "tokens": ["Und", "von", "ih\u00b7rem", "ers\u00b7ten", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}