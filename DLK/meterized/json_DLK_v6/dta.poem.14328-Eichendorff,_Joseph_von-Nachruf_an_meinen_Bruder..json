{"dta.poem.14328": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Nachruf an meinen Bruder.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "K\u00fchl wird's auf den G\u00e4ngen,", "tokens": ["K\u00fchl", "wird's", "auf", "den", "G\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "+---+-", "measure": "dactylic.init"}, "line.2": {"text": "Vor alten Ges\u00e4ngen", "tokens": ["Vor", "al\u00b7ten", "Ge\u00b7s\u00e4n\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "M\u00f6cht's Herz mir zerspringen.", "tokens": ["M\u00f6cht's", "Herz", "mir", "zer\u00b7sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PPER", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "So will ich denn singen!", "tokens": ["So", "will", "ich", "denn", "sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Schmerz fliegt ja auf Kl\u00e4ngen", "tokens": ["Schmerz", "fliegt", "ja", "auf", "Kl\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Zu himmlischer Lust,", "tokens": ["Zu", "himm\u00b7li\u00b7scher", "Lust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Und still wird die Brust", "tokens": ["Und", "still", "wird", "die", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Auf k\u00fchlgr\u00fcnen G\u00e4ngen.", "tokens": ["Auf", "k\u00fchl\u00b7gr\u00fc\u00b7nen", "G\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}