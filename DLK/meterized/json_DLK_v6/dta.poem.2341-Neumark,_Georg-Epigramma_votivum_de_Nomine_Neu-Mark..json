{"dta.poem.2341": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Epigramma votivum de Nomine  \n  Neu-Mark.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr \u00e4dlen Geister auf/ auff die ihr die Gedanken/", "tokens": ["Ihr", "\u00e4d\u00b7len", "Geis\u00b7ter", "auf", "/", "auff", "die", "ihr", "die", "Ge\u00b7dan\u00b7ken", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "$(", "APPR", "PRELS", "PPER", "ART", "NN", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Voll himmlischer Begier nicht in die engen Schranken", "tokens": ["Voll", "himm\u00b7li\u00b7scher", "Be\u00b7gier", "nicht", "in", "die", "en\u00b7gen", "Schran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "PTKNEG", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der schn\u00f6den Eitelkeit euch lasset schliessen ein/", "tokens": ["Der", "schn\u00f6\u00b7den", "Ei\u00b7tel\u00b7keit", "euch", "las\u00b7set", "schlies\u00b7sen", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "VVFIN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf/ ich beschwer\u2019 euch itzt/ l\u00e4st h\u00f6ren was soll sein/", "tokens": ["Auf", "/", "ich", "be\u00b7schwer'", "euch", "itzt", "/", "l\u00e4st", "h\u00f6\u00b7ren", "was", "soll", "sein", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$(", "PPER", "VVFIN", "PPER", "ADV", "$(", "ADJD", "VVINF", "PWS", "VMFIN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Zweg/ das rechte Ziehl des vorgesetzten Lebens", "tokens": ["Der", "Zweg", "/", "das", "rech\u00b7te", "Ziehl", "des", "vor\u00b7ge\u00b7setz\u00b7ten", "Le\u00b7bens"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So euch im Sinne liegt? Ich such\u2019 es nicht vergebens.", "tokens": ["So", "euch", "im", "Sin\u00b7ne", "liegt", "?", "Ich", "such'", "es", "nicht", "ver\u00b7ge\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPRART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Denn hier ist solch ein Sinn der noch im suchen ist/", "tokens": ["Denn", "hier", "ist", "solch", "ein", "Sinn", "der", "noch", "im", "su\u00b7chen", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIAT", "ART", "NN", "ART", "ADV", "APPRART", "ADJA", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und g\u00e4ntzlich euer Ziehl zu seinem Zwek erkiest;", "tokens": ["Und", "g\u00e4ntz\u00b7lich", "eu\u00b7er", "Ziehl", "zu", "sei\u00b7nem", "Zwek", "er\u00b7kiest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der sich dem Tugendzwang in allem untergiebet", "tokens": ["Der", "sich", "dem", "Tu\u00b7gend\u00b7zwang", "in", "al\u00b7lem", "un\u00b7ter\u00b7gie\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "ART", "NN", "APPR", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und nichtes ausser ihr/ und ihrer Hoffstadt liebet;", "tokens": ["Und", "nich\u00b7tes", "aus\u00b7ser", "ihr", "/", "und", "ih\u00b7rer", "Hoff\u00b7stadt", "lie\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "PPOSAT", "$(", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ein Geist da Feuer ist/ da sich der Himmel regt/", "tokens": ["Ein", "Geist", "da", "Feu\u00b7er", "ist", "/", "da", "sich", "der", "Him\u00b7mel", "regt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "NN", "VAFIN", "$(", "KOUS", "PRF", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der/ was ihm wiedrig f\u00e4llt/ zu grund und bode\u0303 schl\u00e4gt.", "tokens": ["Der", "/", "was", "ihm", "wied\u00b7rig", "f\u00e4llt", "/", "zu", "grund", "und", "bo\u00b7d\u1ebd", "schl\u00e4gt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PWS", "PPER", "ADJD", "VVFIN", "$(", "PTKA", "ADJD", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mir dencht ich h\u00f6re schon mit gleichen Stimmen sagen;", "tokens": ["Mir", "dencht", "ich", "h\u00f6\u00b7re", "schon", "mit", "glei\u00b7chen", "Stim\u00b7men", "sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der Berge reiches Mark/ das v\u00f6llige Behagen/", "tokens": ["Der", "Ber\u00b7ge", "rei\u00b7ches", "Mark", "/", "das", "v\u00f6l\u00b7li\u00b7ge", "Be\u00b7ha\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Des der nur irrdisch ist/ wird nicht von Euch erw\u00e4hlt/", "tokens": ["Des", "der", "nur", "irr\u00b7disch", "ist", "/", "wird", "nicht", "von", "Euch", "er\u00b7w\u00e4hlt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADV", "ADJD", "VAFIN", "$(", "VAFIN", "PTKNEG", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Auch nicht die Herrengunst/ die mancher Mensche z\u00e4hlt", "tokens": ["Auch", "nicht", "die", "Her\u00b7ren\u00b7gunst", "/", "die", "man\u00b7cher", "Men\u00b7sche", "z\u00e4hlt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "ART", "NN", "$(", "ART", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Bey sein gantz h\u00f6chstes Gl\u00fck; Wiewol es solche Gaben", "tokens": ["Bey", "sein", "gantz", "h\u00f6chs\u00b7tes", "Gl\u00fck", ";", "Wie\u00b7wol", "es", "sol\u00b7che", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADV", "ADJA", "NN", "$.", "KOUS", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die wehrt und theuer sind/ die sie auch gerne haben.", "tokens": ["Die", "wehrt", "und", "theu\u00b7er", "sind", "/", "die", "sie", "auch", "ger\u00b7ne", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "ADJD", "VAFIN", "$(", "PRELS", "PPER", "ADV", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die Ehre thut es nicht/ darnach so mancher strebt", "tokens": ["Die", "Eh\u00b7re", "thut", "es", "nicht", "/", "dar\u00b7nach", "so", "man\u00b7cher", "strebt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$(", "PAV", "ADV", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Der lieber ohne Gott als grosses Ansehn lebt.", "tokens": ["Der", "lie\u00b7ber", "oh\u00b7ne", "Gott", "als", "gros\u00b7ses", "An\u00b7sehn", "lebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "KOUS", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Es ist was bessers noch das kluge Sinnen lieben/", "tokens": ["Es", "ist", "was", "bes\u00b7sers", "noch", "das", "klu\u00b7ge", "Sin\u00b7nen", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Um welches sie vorau\u00df sich in der Tugend \u00fcben:", "tokens": ["Um", "wel\u00b7ches", "sie", "vor\u00b7au\u00df", "sich", "in", "der", "Tu\u00b7gend", "\u00fc\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die Ruhe des Gem\u00fchts; Dle\u00df ist das rechte Ziehl", "tokens": ["Die", "Ru\u00b7he", "des", "Ge\u00b7m\u00fchts", ";", "Dle\u00df", "ist", "das", "rech\u00b7te", "Ziehl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$.", "PDS", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Worauf ein \u00e4dler Geist sich endlich legen wil.", "tokens": ["Wo\u00b7rauf", "ein", "\u00e4d\u00b7ler", "Geist", "sich", "end\u00b7lich", "le\u00b7gen", "wil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "PRF", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Worauf Er t\u00e4glich denkt \u00fcm solches zuerlangen.", "tokens": ["Wo\u00b7rauf", "Er", "t\u00e4g\u00b7lich", "denkt", "\u00fcm", "sol\u00b7ches", "zu\u00b7er\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADJD", "VVFIN", "APPRART", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wie zeitig habt ihr nun/ Herr Neumark/ angefangen", "tokens": ["Wie", "zei\u00b7tig", "habt", "ihr", "nun", "/", "Herr", "Neu\u00b7mark", "/", "an\u00b7ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ADV", "$(", "NN", "NE", "$(", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Recht nach der klugen Ahrt zu suchen diesen Stein/", "tokens": ["Recht", "nach", "der", "klu\u00b7gen", "Ahrt", "zu", "su\u00b7chen", "die\u00b7sen", "Stein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Der nicht verweltzet wird/ der ewig fest kan seyn.", "tokens": ["Der", "nicht", "ver\u00b7welt\u00b7zet", "wird", "/", "der", "e\u00b7wig", "fest", "kan", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "VVPP", "VAFIN", "$(", "ART", "ADJD", "PTKVZ", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Der Jugend zartes Theil ist schon vorbey geschlichen", "tokens": ["Der", "Ju\u00b7gend", "zar\u00b7tes", "Theil", "ist", "schon", "vor\u00b7bey", "ge\u00b7schli\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Mit der die Thorheit auch zu weichen sich verglichen/", "tokens": ["Mit", "der", "die", "Thor\u00b7heit", "auch", "zu", "wei\u00b7chen", "sich", "ver\u00b7gli\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "ADV", "PTKZU", "VVINF", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Viel F\u00e4lle bleiben nach die stets ein kluger Sinn/", "tokens": ["Viel", "F\u00e4l\u00b7le", "blei\u00b7ben", "nach", "die", "stets", "ein", "klu\u00b7ger", "Sinn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "ART", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Bedenkt und \u00fcberlegt bi\u00df in das Alter hin.", "tokens": ["Be\u00b7denkt", "und", "\u00fc\u00b7ber\u00b7legt", "bi\u00df", "in", "das", "Al\u00b7ter", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Der unersch\u00f6pffte Flei\u00df den K\u00fcnsten obzuliegen/", "tokens": ["Der", "un\u00b7er\u00b7sch\u00f6pff\u00b7te", "Flei\u00df", "den", "K\u00fcns\u00b7ten", "ob\u00b7zu\u00b7lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die angewandte M\u00fch lie\u00df schon die Jahre siegen", "tokens": ["Die", "an\u00b7ge\u00b7wand\u00b7te", "M\u00fch", "lie\u00df", "schon", "die", "Jah\u00b7re", "sie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "die minder j\u00e4hrig sind. Du dachtest in die Welt", "tokens": ["die", "min\u00b7der", "j\u00e4h\u00b7rig", "sind", ".", "Du", "dach\u00b7test", "in", "die", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "VAFIN", "$.", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und wagtest es getrost. Wie manche Noht bef\u00e4llt", "tokens": ["Und", "wag\u00b7test", "es", "ge\u00b7trost", ".", "Wie", "man\u00b7che", "Noht", "be\u00b7f\u00e4llt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "VVPP", "$.", "PWAV", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Den Fremdling hier und dar/ die ihn nicht wenig schrekket!", "tokens": ["Den", "Fremd\u00b7ling", "hier", "und", "dar", "/", "die", "ihn", "nicht", "we\u00b7nig", "schrek\u00b7ket", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KON", "PTKVZ", "$(", "PRELS", "PPER", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Doch Lust zur Wissenschafft hat alles \u00fcberdekket/", "tokens": ["Doch", "Lust", "zur", "Wis\u00b7sen\u00b7schafft", "hat", "al\u00b7les", "\u00fc\u00b7ber\u00b7dek\u00b7ket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "NN", "VAFIN", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Bey dir auch wehrter Freund. Es siel zum \u00f6fftern ein-", "tokens": ["Bey", "dir", "auch", "wehr\u00b7ter", "Freund", ".", "Es", "siel", "zum", "\u00f6ff\u00b7tern", "ein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "ADJA", "NN", "$.", "PPER", "VVFIN", "APPRART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wie kan das wilde Meer doch ohn Gereusche seyn?", "tokens": ["Wie", "kan", "das", "wil\u00b7de", "Meer", "doch", "ohn", "Ge\u00b7reu\u00b7sche", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "ADJA", "NN", "ADV", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Ja auch die Quellen selbst? den B\u00fcchern sein ergeben/", "tokens": ["Ja", "auch", "die", "Quel\u00b7len", "selbst", "?", "den", "B\u00fc\u00b7chern", "sein", "er\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "NN", "ADV", "$.", "ART", "NN", "PPOSAT", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "War ja von Anbeginn dein Hertz und gantzes Leben.", "tokens": ["War", "ja", "von", "An\u00b7be\u00b7ginn", "dein", "Hertz", "und", "gant\u00b7zes", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "PPOSAT", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Ein muhtiger Soldat acht keine M\u00fche nicht/", "tokens": ["Ein", "muh\u00b7ti\u00b7ger", "Sol\u00b7dat", "acht", "kei\u00b7ne", "M\u00fc\u00b7he", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "CARD", "PIAT", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Noch Noht die sonsten leicht die schwachen Sinnen bricht/", "tokens": ["Noch", "Noht", "die", "sons\u00b7ten", "leicht", "die", "schwa\u00b7chen", "Sin\u00b7nen", "bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ART", "ADJA", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Jmfalt er Sieg und Ruhm und Ruhe wil erlangen.", "tokens": ["Jm\u00b7falt", "er", "Sieg", "und", "Ruhm", "und", "Ru\u00b7he", "wil", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "KON", "NN", "KON", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Du bist den steilen Berg zwar schwitzend angegangen", "tokens": ["Du", "bist", "den", "stei\u00b7len", "Berg", "zwar", "schwit\u00b7zend", "an\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Doch voller Hoffnungstrost; der gr\u00fcne Helikon", "tokens": ["Doch", "vol\u00b7ler", "Hoff\u00b7nungs\u00b7trost", ";", "der", "gr\u00fc\u00b7ne", "He\u00b7li\u00b7kon"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Und der neun Schwestern Chor hat vielmals deine\u0303 Thon", "tokens": ["Und", "der", "neun", "Schwes\u00b7tern", "Chor", "hat", "viel\u00b7mals", "dein\u1ebd", "Thon"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "CARD", "NN", "NN", "VAFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.49": {"text": "Mit grosser Luft geh\u00f6rt/ nach dem du vorgekommen", "tokens": ["Mit", "gros\u00b7ser", "Luft", "ge\u00b7h\u00f6rt", "/", "nach", "dem", "du", "vor\u00b7ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$(", "APPR", "PRELS", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Und deinen sichren Sitz nun unter Sie genommen/", "tokens": ["Und", "dei\u00b7nen", "sic