 "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "PIAT", "NN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Die werden hochgeneigt vor deine Wolfahrt stehn.", "tokens": ["Die", "wer\u00b7den", "hoch\u00b7ge\u00b7neigt", "vor", "dei\u00b7ne", "Wol\u00b7fahrt", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Der grosse Wilhelm wird/ wie sonst pflegt zu bemahlen", "tokens": ["Der", "gros\u00b7se", "Wil\u00b7helm", "wird", "/", "wie", "sonst", "pflegt", "zu", "be\u00b7mah\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "VAFIN", "$(", "KOKOM", "ADV", "VVFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Die Sonne Wald und Feld/ mit Gnaden dich bestrahlen-", "tokens": ["Die", "Son\u00b7ne", "Wald", "und", "Feld", "/", "mit", "Gna\u00b7den", "dich", "be\u00b7strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "NN", "$(", "APPR", "NN", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Und schenken Ehr\u2019 und Ruh/ die auch dein steter Flei\u00df", "tokens": ["Und", "schen\u00b7ken", "Ehr'", "und", "Ruh", "/", "die", "auch", "dein", "ste\u00b7ter", "Flei\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "$(", "PRELS", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Und unverdrosne M\u00fch nach b