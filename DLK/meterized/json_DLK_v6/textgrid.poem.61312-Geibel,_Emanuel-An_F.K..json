{"textgrid.poem.61312": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "An F.K.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbtrag\u00f6dien dichte; la\u00df das Liederfeilen!\u00ab", "tokens": ["\u00bb", "tra\u00b7g\u00f6\u00b7di\u00b7en", "dich\u00b7te", ";", "la\u00df", "das", "Lie\u00b7der\u00b7fei\u00b7len", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "VVFIN", "$.", "VVIMP", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "So schiltst du und ermahnst du mich voll G\u00fcte,", "tokens": ["So", "schiltst", "du", "und", "er\u00b7mahnst", "du", "mich", "voll", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "PRF", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch sieh, mir steckt ein Fieber im Gebl\u00fcte,", "tokens": ["Doch", "sieh", ",", "mir", "steckt", "ein", "Fie\u00b7ber", "im", "Ge\u00b7bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Fieber der Sonette, schwer zu heilen.", "tokens": ["Das", "Fie\u00b7ber", "der", "So\u00b7net\u00b7te", ",", "schwer", "zu", "hei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dies ist der Krankheit Merkmal, da\u00df mit Eilen,", "tokens": ["Dies", "ist", "der", "Krank\u00b7heit", "Merk\u00b7mal", ",", "da\u00df", "mit", "Ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NE", "$,", "KOUS", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was immer nur ber\u00fchret mein Gem\u00fcte,", "tokens": ["Was", "im\u00b7mer", "nur", "be\u00b7r\u00fch\u00b7ret", "mein", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verschlungen durch vierfachen Reimes Bl\u00fcte", "tokens": ["Ver\u00b7schlun\u00b7gen", "durch", "vier\u00b7fa\u00b7chen", "Rei\u00b7mes", "Bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Mir unbewu\u00dft sich f\u00fcgt in vierzehn Zeilen.", "tokens": ["Mir", "un\u00b7be\u00b7wu\u00dft", "sich", "f\u00fcgt", "in", "vier\u00b7zehn", "Zei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PRF", "VVFIN", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zwar f\u00fcrcht' ich nicht, da\u00df sie ins Grab mich treibe,", "tokens": ["Zwar", "f\u00fcrcht'", "ich", "nicht", ",", "da\u00df", "sie", "ins", "Grab", "mich", "trei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da ja Petrark, den sie geplagt wie keinen,", "tokens": ["Da", "ja", "Pe\u00b7trark", ",", "den", "sie", "ge\u00b7plagt", "wie", "kei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NE", "$,", "PRELS", "PPER", "VVPP", "KOKOM", "PIAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Alt dabei ward und wohl gedieh am Leibe.", "tokens": ["Alt", "da\u00b7bei", "ward", "und", "wohl", "ge\u00b7dieh", "am", "Lei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PAV", "VAFIN", "KON", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch l\u00e4\u00dft sie sich so wenigste verneinen,", "tokens": ["Doch", "l\u00e4\u00dft", "sie", "sich", "so", "we\u00b7nigs\u00b7te", "ver\u00b7nei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df selbst dies Brieflein, das ich rasch dir schreibe,", "tokens": ["Da\u00df", "selbst", "dies", "Brie\u00b7flein", ",", "das", "ich", "rasch", "dir", "schrei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PDS", "NN", "$,", "PRELS", "PPER", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mir zum Sonett wird wider Wunsch und Meinen.", "tokens": ["Mir", "zum", "So\u00b7nett", "wird", "wi\u00b7der", "Wunsch", "und", "Mei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VAFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}