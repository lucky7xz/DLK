{"textgrid.poem.28358": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "73.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es m\u00fcht sich mancher hoch, zu sitzen oben an,", "tokens": ["Es", "m\u00fcht", "sich", "man\u00b7cher", "hoch", ",", "zu", "sit\u00b7zen", "o\u00b7ben", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PIAT", "ADJD", "$,", "PTKZU", "VVINF", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da doch der Mann den Ort ziert, nicht der Ort den Mann;", "tokens": ["Da", "doch", "der", "Mann", "den", "Ort", "ziert", ",", "nicht", "der", "Ort", "den", "Mann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$,", "PTKNEG", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es ist ein schlechter Ruhm, der sitzt; ein Ruhm, der geht", "tokens": ["Es", "ist", "ein", "schlech\u00b7ter", "Ruhm", ",", "der", "sitzt", ";", "ein", "Ruhm", ",", "der", "geht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "VVFIN", "$.", "ART", "NN", "$,", "PRELS", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch tapffrer Leute Mund in alle Welt, besteht.", "tokens": ["Durch", "tapf\u00b7frer", "Leu\u00b7te", "Mund", "in", "al\u00b7le", "Welt", ",", "be\u00b7steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "APPR", "PIAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}