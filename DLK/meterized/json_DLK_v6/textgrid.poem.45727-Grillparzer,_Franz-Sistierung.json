{"textgrid.poem.45727": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Sistierung", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Staatsmaschine mu\u00df jetzt bremsen,", "tokens": ["Die", "Staats\u00b7ma\u00b7schi\u00b7ne", "mu\u00df", "jetzt", "brem\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und der F\u00fchrer selbst h\u00e4lt an,", "tokens": ["Und", "der", "F\u00fch\u00b7rer", "selbst", "h\u00e4lt", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn ein Rudel schlanker Gemsen", "tokens": ["Denn", "ein", "Ru\u00b7del", "schlan\u00b7ker", "Gem\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Laufen schu\u00dfrecht in die Bahn.", "tokens": ["Lau\u00b7fen", "schu\u00df\u00b7recht", "in", "die", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}