{"textgrid.poem.61041": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "35.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und gestern Not und heute Wein,", "tokens": ["Und", "ge\u00b7stern", "Not", "und", "heu\u00b7te", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das ist's, was mir gef\u00e4llt;", "tokens": ["Das", "ist's", ",", "was", "mir", "ge\u00b7f\u00e4llt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und morgen ein Ro\u00df, ein schnelles Ro\u00df,", "tokens": ["Und", "mor\u00b7gen", "ein", "Ro\u00df", ",", "ein", "schnel\u00b7les", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zu reiten in die Welt.", "tokens": ["Zu", "rei\u00b7ten", "in", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Vergangnes Leid ist kaum ein Leid,", "tokens": ["Ver\u00b7gang\u00b7nes", "Leid", "ist", "kaum", "ein", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und s\u00fc\u00df ist Jubel im Haus,", "tokens": ["Und", "s\u00fc\u00df", "ist", "Ju\u00b7bel", "im", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und dazu ein Blick, ein heller Blick", "tokens": ["Und", "da\u00b7zu", "ein", "Blick", ",", "ein", "hel\u00b7ler", "Blick"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PAV", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "In lust'ge Zeit hinaus.", "tokens": ["In", "lust'\u00b7ge", "Zeit", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Welt ist jetzt so fr\u00fchlingsgr\u00fcn", "tokens": ["Die", "Welt", "ist", "jetzt", "so", "fr\u00fch\u00b7lings\u00b7gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und hat der Blumen zu viel,", "tokens": ["Und", "hat", "der", "Blu\u00b7men", "zu", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "APPR", "PIS", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Hat M\u00e4gdlein sch\u00f6n wohl nah und fern", "tokens": ["Hat", "M\u00e4gd\u00b7lein", "sch\u00f6n", "wohl", "nah", "und", "fern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "ADJD", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und klingend Saitenspiel.", "tokens": ["Und", "klin\u00b7gend", "Sai\u00b7ten\u00b7spiel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und bist du nur der rechte Mann", "tokens": ["Und", "bist", "du", "nur", "der", "rech\u00b7te", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und greifest fr\u00f6hlich drein,", "tokens": ["Und", "grei\u00b7fest", "fr\u00f6h\u00b7lich", "drein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So Ros' als Maid, so Lieb' als Lied", "tokens": ["So", "Ros'", "als", "Maid", ",", "so", "Lieb'", "als", "Lied"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NE", "KOUS", "NN", "$,", "ADV", "NN", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist alles, alles dein.", "tokens": ["Ist", "al\u00b7les", ",", "al\u00b7les", "dein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "PIS", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Drum gestern Not und heute Wein,", "tokens": ["Drum", "ge\u00b7stern", "Not", "und", "heu\u00b7te", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "NN", "KON", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das ist's, was mir gef\u00e4llt;", "tokens": ["Das", "ist's", ",", "was", "mir", "ge\u00b7f\u00e4llt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und morgen zu Ro\u00df, wohl hoch zu Ro\u00df", "tokens": ["Und", "mor\u00b7gen", "zu", "Ro\u00df", ",", "wohl", "hoch", "zu", "Ro\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN", "$,", "ADV", "ADJD", "APPR", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Reit' ich in alle Welt.", "tokens": ["Reit'", "ich", "in", "al\u00b7le", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}