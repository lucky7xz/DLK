{"textgrid.poem.43310": {"metadata": {"author": {"name": "Gerstenberg, Heinrich Wilhelm von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dann konnt ich Knabe mich erfreun!", "genre": "verse", "period": "N.A.", "pub_year": 1780, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dann konnt ich Knabe mich erfreun!", "tokens": ["Dann", "konnt", "ich", "Kna\u00b7be", "mich", "er\u00b7freun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ganz Paphos schien mir Tanz zu seyn.", "tokens": ["Ganz", "Pa\u00b7phos", "schien", "mir", "Tanz", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PPER", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn auf mir tanzten Liebesg\u00f6tter,", "tokens": ["Denn", "auf", "mir", "tanz\u00b7ten", "Lie\u00b7bes\u00b7g\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und unter mir die Blumenbl\u00e4tter.", "tokens": ["Und", "un\u00b7ter", "mir", "die", "Blu\u00b7men\u00b7bl\u00e4t\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Bald \u00fcberrascht ich sie in Str\u00e4uchen,", "tokens": ["Bald", "\u00fc\u00b7berr\u00b7ascht", "ich", "sie", "in", "Str\u00e4u\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo sie, entdeckt zu seyn, sanft in das Laub gerauscht;", "tokens": ["Wo", "sie", ",", "ent\u00b7deckt", "zu", "seyn", ",", "sanft", "in", "das", "Laub", "ge\u00b7rauscht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "VVPP", "PTKZU", "VAINF", "$,", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bald, wenn ich mich verbarg, ward ich von ihr belauscht,", "tokens": ["Bald", ",", "wenn", "ich", "mich", "ver\u00b7barg", ",", "ward", "ich", "von", "ihr", "be\u00b7lauscht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,", "VAFIN", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dann floh sie, wenn sie mich belauscht,", "tokens": ["Dann", "floh", "sie", ",", "wenn", "sie", "mich", "be\u00b7lauscht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und ich ihr nach, sie zu erreichen.", "tokens": ["Und", "ich", "ihr", "nach", ",", "sie", "zu", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPER", "PTKVZ", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch schnell verschlo\u00df sie sich in Eichen,", "tokens": ["Doch", "schnell", "ver\u00b7schlo\u00df", "sie", "sich", "in", "Ei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und wehrte mir, sie zu erreichen.", "tokens": ["Und", "wehr\u00b7te", "mir", ",", "sie", "zu", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dann klettert ich auf manchen Baum empor,", "tokens": ["Dann", "klet\u00b7tert", "ich", "auf", "man\u00b7chen", "Baum", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und h\u00f6rte sie verr\u00e4thrisch lachen,", "tokens": ["Und", "h\u00f6r\u00b7te", "sie", "ver\u00b7r\u00e4t\u00b7hrisch", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und bat, ihr Eichenhaus mir Knaben aufzumachen,", "tokens": ["Und", "bat", ",", "ihr", "Ei\u00b7chen\u00b7haus", "mir", "Kna\u00b7ben", "auf\u00b7zu\u00b7ma\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPOSAT", "NN", "PPER", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dann sprang sie froh aus ihrer Eich hervor, \u2013", "tokens": ["Dann", "sprang", "sie", "froh", "aus", "ih\u00b7rer", "Eich", "her\u00b7vor", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "PTKVZ", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So s\u00fc\u00df ist Honig nicht, der vom Hymettus flie\u00dft;", "tokens": ["So", "s\u00fc\u00df", "ist", "Ho\u00b7nig", "nicht", ",", "der", "vom", "Hy\u00b7met\u00b7tus", "flie\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "NN", "PTKNEG", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So s\u00fc\u00df ist nicht die Frucht von Surentiner Reben:", "tokens": ["So", "s\u00fc\u00df", "ist", "nicht", "die", "Frucht", "von", "Su\u00b7ren\u00b7ti\u00b7ner", "Re\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PTKNEG", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So s\u00fc\u00df der Nektar nicht, durch den unsterblich Leben", "tokens": ["So", "s\u00fc\u00df", "der", "Nek\u00b7tar", "nicht", ",", "durch", "den", "uns\u00b7terb\u00b7lich", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "PTKNEG", "$,", "APPR", "ART", "ADJD", "NN"], "meter": "-+-+-----+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Den G\u00f6ttern Ganymed in g\u00fcldne Schaalen gie\u00dft.", "tokens": ["Den", "G\u00f6t\u00b7tern", "Ga\u00b7ny\u00b7med", "in", "g\u00fcld\u00b7ne", "Schaa\u00b7len", "gie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}