{"textgrid.poem.44344": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "An einen Freund", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Schiffer irrt, durch Sturmesnacht getrieben,", "tokens": ["Ein", "Schif\u00b7fer", "irrt", ",", "durch", "Stur\u00b7mes\u00b7nacht", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Wogen und der Winde, leichtes Spiel,", "tokens": ["Der", "Wo\u00b7gen", "und", "der", "Win\u00b7de", ",", "leich\u00b7tes", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wohl sind ihm Mast und Ruder noch geblieben,", "tokens": ["Wohl", "sind", "ihm", "Mast", "und", "Ru\u00b7der", "noch", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "KON", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch fehlt der Reise Wichtigstes: ein Ziel!", "tokens": ["Doch", "fehlt", "der", "Rei\u00b7se", "Wich\u00b7tigs\u00b7tes", ":", "ein", "Ziel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NE", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da sieht er einen Stern durchs Dunkel blinken,", "tokens": ["Da", "sieht", "er", "ei\u00b7nen", "Stern", "durchs", "Dun\u00b7kel", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Froh ordnet er darnach den irren Lauf,", "tokens": ["Froh", "ord\u00b7net", "er", "dar\u00b7nach", "den", "ir\u00b7ren", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und jetzt, da schon die Kr\u00e4fte schwindend sinken,", "tokens": ["Und", "jetzt", ",", "da", "schon", "die", "Kr\u00e4f\u00b7te", "schwin\u00b7dend", "sin\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "ADV", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Tut sich ein Hafen dem Verirrten auf.", "tokens": ["Tut", "sich", "ein", "Ha\u00b7fen", "dem", "Ver\u00b7irr\u00b7ten", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}}, "stanza.3": {"line.1": {"text": "Wie er das hohe Ufer nun beschreitet,", "tokens": ["Wie", "er", "das", "ho\u00b7he", "U\u00b7fer", "nun", "be\u00b7schrei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weiht opfernd er dem Leistern in der Nacht,", "tokens": ["Weiht", "op\u00b7fernd", "er", "dem", "Leis\u00b7tern", "in", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der ihm der Irrfahrt frohes Ziel bereitet,", "tokens": ["Der", "ihm", "der", "Irr\u00b7fahrt", "fro\u00b7hes", "Ziel", "be\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Erstlinge von dem, was er gebracht.", "tokens": ["Die", "Erst\u00b7lin\u00b7ge", "von", "dem", ",", "was", "er", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "--+--+---+", "measure": "anapaest.di.plus"}}}}}