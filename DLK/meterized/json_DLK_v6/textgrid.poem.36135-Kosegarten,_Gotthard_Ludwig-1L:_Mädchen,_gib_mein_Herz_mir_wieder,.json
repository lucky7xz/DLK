{"textgrid.poem.36135": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: M\u00e4dchen, gib mein Herz mir wieder,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00e4dchen, gib mein Herz mir wieder,", "tokens": ["M\u00e4d\u00b7chen", ",", "gib", "mein", "Herz", "mir", "wie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPOSAT", "NN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oder schenk' das deine mir,", "tokens": ["O\u00b7der", "schenk'", "das", "dei\u00b7ne", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "PPOSAT", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn mein Herz und meine Lieder,", "tokens": ["Denn", "mein", "Herz", "und", "mei\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles, alles raubst du mir,", "tokens": ["Al\u00b7les", ",", "al\u00b7les", "raubst", "du", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PIS", "VVFIN", "PPER", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "B\u00e4ndigest mit Einem Blicke", "tokens": ["B\u00e4n\u00b7di\u00b7gest", "mit", "Ei\u00b7nem", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ganz mein stolzes J\u00fcnglings-Herz,", "tokens": ["Ganz", "mein", "stol\u00b7zes", "J\u00fcng\u00b7lings\u00b7Herz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lenkst gebietend mein Geschicke,", "tokens": ["Lenkst", "ge\u00b7bie\u00b7tend", "mein", "Ge\u00b7schi\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Wonna, meinen Schmerz \u2013 \u2013", "tokens": ["Mei\u00b7ne", "Won\u00b7na", ",", "mei\u00b7nen", "Schmerz", "\u2013", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn in s\u00fc\u00dfen Augenblicken", "tokens": ["Wenn", "in", "s\u00fc\u00b7\u00dfen", "Au\u00b7gen\u00b7bli\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich an deinen Locken spielt',", "tokens": ["Ich", "an", "dei\u00b7nen", "Lo\u00b7cken", "spielt'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit innigem Entz\u00fccken", "tokens": ["Und", "mit", "in\u00b7ni\u00b7gem", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Deine warmen H\u00e4nde hielt,", "tokens": ["Dei\u00b7ne", "war\u00b7men", "H\u00e4n\u00b7de", "hielt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn ich dich an meinen Busen", "tokens": ["Wenn", "ich", "dich", "an", "mei\u00b7nen", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Liebe Taumel zog,", "tokens": ["In", "der", "Lie\u00b7be", "Tau\u00b7mel", "zog", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dein junger reiner Busen", "tokens": ["Und", "dein", "jun\u00b7ger", "rei\u00b7ner", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Athmend mir entgegen flog;", "tokens": ["Ath\u00b7mend", "mir", "ent\u00b7ge\u00b7gen", "flog", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "PTKVZ", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Dann, wie Strahl der Abendsonne,", "tokens": ["Dann", ",", "wie", "Strahl", "der", "A\u00b7bend\u00b7son\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "D\u00e4mmerte mir Seelen-rein,", "tokens": ["D\u00e4m\u00b7mer\u00b7te", "mir", "See\u00b7len\u00b7rein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Und ich tr\u00e4umte mir die Wonne,", "tokens": ["Und", "ich", "tr\u00e4um\u00b7te", "mir", "die", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00e4dchen, dir geliebt zu seyn.", "tokens": ["M\u00e4d\u00b7chen", ",", "dir", "ge\u00b7liebt", "zu", "seyn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVPP", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Aber wenn, so wild wie Winde,", "tokens": ["A\u00b7ber", "wenn", ",", "so", "wild", "wie", "Win\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$,", "ADV", "ADJD", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du mir andrer Zeit entflohst,", "tokens": ["Du", "mir", "an\u00b7drer", "Zeit", "ent\u00b7flohst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und, damit mein Traum verschwinde,", "tokens": ["Und", ",", "da\u00b7mit", "mein", "Traum", "ver\u00b7schwin\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir die warme Hand entzogst;", "tokens": ["Mir", "die", "war\u00b7me", "Hand", "ent\u00b7zogst", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Dann, ach! schwand in Wintertr\u00fcbe", "tokens": ["Dann", ",", "ach", "!", "schwand", "in", "Win\u00b7ter\u00b7tr\u00fc\u00b7be"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "ITJ", "$.", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meiner Hoffnung Fr\u00fchlingschein,", "tokens": ["Mei\u00b7ner", "Hoff\u00b7nung", "Fr\u00fch\u00b7ling\u00b7schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Stolz verschm\u00e4hter Liebe", "tokens": ["Und", "der", "Stolz", "ver\u00b7schm\u00e4h\u00b7ter", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "St\u00fcrmet m\u00e4chtig auf mich ein.", "tokens": ["St\u00fcr\u00b7met", "m\u00e4ch\u00b7tig", "auf", "mich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "J\u00fcngling, dacht' ich, frei von Stirnen", "tokens": ["J\u00fcng\u00b7ling", ",", "dacht'", "ich", ",", "frei", "von", "Stir\u00b7nen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "ADJD", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bist du, und dein mildes Herz", "tokens": ["Bist", "du", ",", "und", "dein", "mil\u00b7des", "Herz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "$,", "KON", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Beugte so viel stolze Dirnen,", "tokens": ["Beug\u00b7te", "so", "viel", "stol\u00b7ze", "Dir\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und hier diese schm\u00e4ht dein Herz? \u2013", "tokens": ["Und", "hier", "die\u00b7se", "schm\u00e4ht", "dein", "Herz", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "PDS", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Und dann schwor ich, nicht zu lieben,", "tokens": ["Und", "dann", "schwor", "ich", ",", "nicht", "zu", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00e4hnte auch, mein Herz sei kalt,", "tokens": ["W\u00e4hn\u00b7te", "auch", ",", "mein", "Herz", "sei", "kalt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber, o, den Frost vertrieben", "tokens": ["A\u00b7ber", ",", "o", ",", "den", "Frost", "ver\u00b7trie\u00b7ben"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "FM", "$,", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine Feuerblicke bald,", "tokens": ["Dei\u00b7ne", "Feu\u00b7er\u00b7bli\u00b7cke", "bald", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Und ich kann nicht von dir lassen,", "tokens": ["Und", "ich", "kann", "nicht", "von", "dir", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PTKNEG", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ich mu\u00df mein Herz dir weihn,", "tokens": ["Und", "ich", "mu\u00df", "mein", "Herz", "dir", "weihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mu\u00df bei deinem Lieben, Hassen,", "tokens": ["Mu\u00df", "bei", "dei\u00b7nem", "Lie\u00b7ben", ",", "Has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "APPR", "PPOSAT", "ADJA", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ewig dein Getreuer sein.", "tokens": ["E\u00b7wig", "dein", "Ge\u00b7treu\u00b7er", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Aber, M\u00e4dchen, deutsch und bider", "tokens": ["A\u00b7ber", ",", "M\u00e4d\u00b7chen", ",", "deutsch", "und", "bi\u00b7der"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "NN", "$,", "ADJD", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bin ich, und nicht ungleich dir \u2013", "tokens": ["Bin", "ich", ",", "und", "nicht", "un\u00b7gleich", "dir", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "KON", "PTKNEG", "ADJD", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gib dann, gib mein Herz mir wieder,", "tokens": ["Gib", "dann", ",", "gib", "mein", "Herz", "mir", "wie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "VVIMP", "PPOSAT", "NN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder schenk' das deine mir!", "tokens": ["O\u00b7der", "schenk'", "das", "dei\u00b7ne", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "PPOSAT", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}