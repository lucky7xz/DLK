{"textgrid.poem.45431": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[hier sitz ich unter Faszikeln dicht]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier sitz ich unter Faszikeln dicht,", "tokens": ["Hier", "sitz", "ich", "un\u00b7ter", "Fas\u00b7zi\u00b7keln", "dicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ihr glaubt, verdrossen und einsam \u2013", "tokens": ["Ihr", "glaubt", ",", "ver\u00b7dros\u00b7sen", "und", "ein\u00b7sam", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVPP", "KON", "ADJD", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und doch vielleicht \u2013 das glaubt ihr nicht:", "tokens": ["Und", "doch", "viel\u00b7leicht", "\u2013", "das", "glaubt", "ihr", "nicht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$(", "PDS", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit den ewigen G\u00f6ttern gemeinsam.", "tokens": ["Mit", "den", "e\u00b7wi\u00b7gen", "G\u00f6t\u00b7tern", "ge\u00b7mein\u00b7sam", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}}}}