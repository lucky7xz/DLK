{"textgrid.poem.46643": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ach, mit euerm frohen Lachen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, mit euerm frohen Lachen", "tokens": ["Ach", ",", "mit", "eu\u00b7erm", "fro\u00b7hen", "La\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00f6nnet ihr mich weinen machen,", "tokens": ["K\u00f6n\u00b7net", "ihr", "mich", "wei\u00b7nen", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weinen nicht zum Scherz.", "tokens": ["Wei\u00b7nen", "nicht", "zum", "Scherz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn ich so euch lachen sah,", "tokens": ["Wenn", "ich", "so", "euch", "la\u00b7chen", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lachte mir das Mutterherz,", "tokens": ["Lach\u00b7te", "mir", "das", "Mut\u00b7ter\u00b7herz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Thr\u00e4nen traten nah,", "tokens": ["Und", "die", "Thr\u00e4\u00b7nen", "tra\u00b7ten", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber Thr\u00e4nen nicht aus Schmerz.", "tokens": ["A\u00b7ber", "Thr\u00e4\u00b7nen", "nicht", "aus", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKNEG", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, mit euerm frohen Lachen", "tokens": ["Ach", ",", "mit", "eu\u00b7erm", "fro\u00b7hen", "La\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "K\u00f6nnet ihr mich weinen machen,", "tokens": ["K\u00f6n\u00b7net", "ihr", "mich", "wei\u00b7nen", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Weinen nicht zum Scherz.", "tokens": ["Wei\u00b7nen", "nicht", "zum", "Scherz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Als ich eure Augen sah", "tokens": ["Als", "ich", "eu\u00b7re", "Au\u00b7gen", "sah"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Brechen unter Todesschmerz,", "tokens": ["Bre\u00b7chen", "un\u00b7ter", "To\u00b7des\u00b7schmerz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Saget mir, wie es geschah,", "tokens": ["Sa\u00b7get", "mir", ",", "wie", "es", "ge\u00b7schah", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": [