{"textgrid.poem.48061": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Eisnacht", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie in Seide ein K\u00f6nigskind", "tokens": ["Wie", "in", "Sei\u00b7de", "ein", "K\u00f6\u00b7nigs\u00b7kind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NE", "ART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "schl\u00e4ft die Erde in lauter Schnee,", "tokens": ["schl\u00e4ft", "die", "Er\u00b7de", "in", "lau\u00b7ter", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "blauer Mondscheinzauber spinnt", "tokens": ["blau\u00b7er", "Mond\u00b7schein\u00b7zau\u00b7ber", "spinnt"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "schimmernd \u00fcber der See.", "tokens": ["schim\u00b7mernd", "\u00fc\u00b7ber", "der", "See", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Aus den Wassern der Rauhreif steigt,", "tokens": ["Aus", "den", "Was\u00b7sern", "der", "Rauh\u00b7reif", "steigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "B\u00fcsche und B\u00e4ume atmen kaum:", "tokens": ["B\u00fc\u00b7sche", "und", "B\u00e4u\u00b7me", "at\u00b7men", "kaum", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ADV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "durch die Nacht, die erschauernd schweigt,", "tokens": ["durch", "die", "Nacht", ",", "die", "er\u00b7schau\u00b7ernd", "schweigt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "VVPP", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "schreitet ein glitzernder Traum.", "tokens": ["schrei\u00b7tet", "ein", "glit\u00b7zern\u00b7der", "Traum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}