{"textgrid.poem.61310": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "An Ernst Curtius", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn im f\u00fcrstlichen Palaste", "tokens": ["Wenn", "im", "f\u00fcrst\u00b7li\u00b7chen", "Pa\u00b7las\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Strenger Ernst nicht ganz dich fa\u00dfte,", "tokens": ["Stren\u00b7ger", "Ernst", "nicht", "ganz", "dich", "fa\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKNEG", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und so froh sich noch die Muse", "tokens": ["Und", "so", "froh", "sich", "noch", "die", "Mu\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "PRF", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bitten darf bei dir zu Gaste,", "tokens": ["Bit\u00b7ten", "darf", "bei", "dir", "zu", "Gas\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie dereinst auf \u00c4geus' Fluten", "tokens": ["Wie", "de\u00b7reinst", "auf", "\u00c4\u00b7geus'", "Flu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "An des Hydrioten Maste:", "tokens": ["An", "des", "Hyd\u00b7rio\u00b7ten", "Mas\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Nenne, Freund, mir Tag und Stunde,", "tokens": ["Nen\u00b7ne", ",", "Freund", ",", "mir", "Tag", "und", "Stun\u00b7de", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da ich schw\u00e4rmend bei dir raste,", "tokens": ["Da", "ich", "schw\u00e4r\u00b7mend", "bei", "dir", "ras\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Da\u00df du sp\u00fcrest, wie ich k\u00fchner,", "tokens": ["Da\u00df", "du", "sp\u00fc\u00b7rest", ",", "wie", "ich", "k\u00fch\u00b7ner", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PWAV", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Der ich einst in Farben pra\u00dfte,", "tokens": ["Der", "ich", "einst", "in", "Far\u00b7ben", "pra\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Jetzt nach m\u00e4chtigen Stoffen greife,", "tokens": ["Jetzt", "nach", "m\u00e4ch\u00b7ti\u00b7gen", "Stof\u00b7fen", "grei\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.12": {"text": "Nach gediegnen Formen taste.", "tokens": ["Nach", "ge\u00b7dieg\u00b7nen", "For\u00b7men", "tas\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Brechen will ich dann die reifste", "tokens": ["Bre\u00b7chen", "will", "ich", "dann", "die", "reifs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "ADV", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Meiner Fr\u00fcchte dir vom Aste.", "tokens": ["Mei\u00b7ner", "Fr\u00fcch\u00b7te", "dir", "vom", "As\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}