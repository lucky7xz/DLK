{"textgrid.poem.67906": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1. Einige Hochzeitlieder", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schm\u00fcck dich, M\u00e4dchen, eile, M\u00e4dchen,", "tokens": ["Schm\u00fcck", "dich", ",", "M\u00e4d\u00b7chen", ",", "ei\u00b7le", ",", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "$,", "VVFIN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schm\u00fccke dich mit jenem Schmucke,", "tokens": ["Schm\u00fc\u00b7cke", "dich", "mit", "je\u00b7nem", "Schmu\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der einst deine Mutter schm\u00fcckte.", "tokens": ["Der", "einst", "dei\u00b7ne", "Mut\u00b7ter", "schm\u00fcck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lege an dir jene B\u00e4nder,", "tokens": ["Le\u00b7ge", "an", "dir", "je\u00b7ne", "B\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die die Mutter einst anlegte.", "tokens": ["Die", "die", "Mut\u00b7ter", "einst", "an\u00b7leg\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf den Kopf das Band des Kummers,", "tokens": ["Auf", "den", "Kopf", "das", "Band", "des", "Kum\u00b7mers", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor die Stirn das Band der Sorge,", "tokens": ["Vor", "die", "Stirn", "das", "Band", "der", "Sor\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sitze auf den Sitz der Mutter:", "tokens": ["Sit\u00b7ze", "auf", "den", "Sitz", "der", "Mut\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tritt auf deiner Mutter Fu\u00dftritt:", "tokens": ["Tritt", "auf", "dei\u00b7ner", "Mut\u00b7ter", "Fu\u00df\u00b7tritt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Weine, weine nicht, o M\u00e4dchen,", "tokens": ["Wei\u00b7ne", ",", "wei\u00b7ne", "nicht", ",", "o", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKNEG", "$,", "FM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn du bei dem Brautschmuck weinest,", "tokens": ["Wenn", "du", "bei", "dem", "Brautsc\u00b7hmuck", "wei\u00b7nest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Weinest du dein ganzes Lebend", "tokens": ["Wei\u00b7nest", "du", "dein", "gan\u00b7zes", "Le\u00b7bend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}