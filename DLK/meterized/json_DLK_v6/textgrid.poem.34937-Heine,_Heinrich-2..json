{"textgrid.poem.34937": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe gerochen alle Ger\u00fcche", "tokens": ["Ich", "ha\u00b7be", "ge\u00b7ro\u00b7chen", "al\u00b7le", "Ge\u00b7r\u00fc\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "PIAT", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In dieser holden Erdenk\u00fcche;", "tokens": ["In", "die\u00b7ser", "hol\u00b7den", "Er\u00b7den\u00b7k\u00fc\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was man genie\u00dfen kann in der Welt,", "tokens": ["Was", "man", "ge\u00b7nie\u00b7\u00dfen", "kann", "in", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVINF", "VMFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Das hab ich genossen wie je ein Held!", "tokens": ["Das", "hab", "ich", "ge\u00b7nos\u00b7sen", "wie", "je", "ein", "Held", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "VVPP", "KOKOM", "ADV", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Hab Kaffee getrunken, hab Kuchen gegessen,", "tokens": ["Hab", "Kaf\u00b7fee", "ge\u00b7trun\u00b7ken", ",", "hab", "Ku\u00b7chen", "ge\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVPP", "$,", "VAFIN", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Hab manche sch\u00f6ne Puppe besessen;", "tokens": ["Hab", "man\u00b7che", "sch\u00f6\u00b7ne", "Pup\u00b7pe", "be\u00b7ses\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "---+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Trug seidne Westen, den feinsten Frack,", "tokens": ["Trug", "seid\u00b7ne", "Wes\u00b7ten", ",", "den", "feins\u00b7ten", "Frack", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Mir klingelten auch Dukaten im Sack.", "tokens": ["Mir", "klin\u00b7gel\u00b7ten", "auch", "Du\u00b7ka\u00b7ten", "im", "Sack", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Wie Gellert ritt ich auf hohem Ro\u00df;", "tokens": ["Wie", "Gel\u00b7lert", "ritt", "ich", "auf", "ho\u00b7hem", "Ro\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Ich hatte ein Haus, ich hatte ein Schlo\u00df.", "tokens": ["Ich", "hat\u00b7te", "ein", "Haus", ",", "ich", "hat\u00b7te", "ein", "Schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Ich lag auf der gr\u00fcnen Wiese des Gl\u00fccks,", "tokens": ["Ich", "lag", "auf", "der", "gr\u00fc\u00b7nen", "Wie\u00b7se", "des", "Gl\u00fccks", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Die Sonne gr\u00fc\u00dfte goldigsten Blicks;", "tokens": ["Die", "Son\u00b7ne", "gr\u00fc\u00df\u00b7te", "gol\u00b7digs\u00b7ten", "Blicks", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.13": {"text": "Ein Lorbeerkranz umschlo\u00df die Stirn,", "tokens": ["Ein", "Lor\u00b7beer\u00b7kranz", "um\u00b7schlo\u00df", "die", "Stirn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Er duftete Tr\u00e4ume mir ins Gehirn,", "tokens": ["Er", "duf\u00b7te\u00b7te", "Tr\u00e4u\u00b7me", "mir", "ins", "Ge\u00b7hirn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Tr\u00e4ume von Rosen und ewigem Mai \u2013", "tokens": ["Tr\u00e4u\u00b7me", "von", "Ro\u00b7sen", "und", "e\u00b7wi\u00b7gem", "Mai", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "ADJA", "NN", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.16": {"text": "Es ward mir so selig zu Sinne dabei,", "tokens": ["Es", "ward", "mir", "so", "se\u00b7lig", "zu", "Sin\u00b7ne", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADJD", "APPR", "NN", "PAV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.17": {"text": "So d\u00e4mmers\u00fcchtig, so sterbefaul \u2013", "tokens": ["So", "d\u00e4m\u00b7mer\u00b7s\u00fcch\u00b7tig", ",", "so", "ster\u00b7be\u00b7faul", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Mir flogen gebratne Tauben ins Maul,", "tokens": ["Mir", "flo\u00b7gen", "ge\u00b7brat\u00b7ne", "Tau\u00b7ben", "ins", "Maul", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Und Englein kamen, und aus den Taschen", "tokens": ["Und", "En\u00b7glein", "ka\u00b7men", ",", "und", "aus", "den", "Ta\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "$,", "KON", "APPR", "ART", "NN"], "meter": "-+-+-++-+-", "measure": "unknown.measure.penta"}, "line.20": {"text": "Sie zogen hervor Champagnerflaschen \u2013", "tokens": ["Sie", "zo\u00b7gen", "her\u00b7vor", "Cham\u00b7pag\u00b7ner\u00b7fla\u00b7schen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "NN", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Das waren Visionen, Seifenblasen \u2013", "tokens": ["Das", "wa\u00b7ren", "Vi\u00b7si\u00b7o\u00b7nen", ",", "Sei\u00b7fen\u00b7bla\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$,", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Sie platzten \u2013 Jetzt lieg ich auf feuchtem Rasen,", "tokens": ["Sie", "platz\u00b7ten", "\u2013", "Jetzt", "lieg", "ich", "auf", "feuch\u00b7tem", "Ra\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.23": {"text": "Die Glieder sind mir rheumatisch gel\u00e4hmt,", "tokens": ["Die", "Glie\u00b7der", "sind", "mir", "rheu\u00b7ma\u00b7tisch", "ge\u00b7l\u00e4hmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Und meine Seele ist tief besch\u00e4mt.", "tokens": ["Und", "mei\u00b7ne", "See\u00b7le", "ist", "tief", "be\u00b7sch\u00e4mt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "Ach, jede Lust, ach, jeden Genu\u00df", "tokens": ["Ach", ",", "je\u00b7de", "Lust", ",", "ach", ",", "je\u00b7den", "Ge\u00b7nu\u00df"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ITJ", "$,", "PIAT", "NN", "$,", "ITJ", "$,", "PIAT", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.26": {"text": "Hab ich erkauft durch herben Verdru\u00df;", "tokens": ["Hab", "ich", "er\u00b7kauft", "durch", "her\u00b7ben", "Ver\u00b7dru\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.27": {"text": "Ich ward getr\u00e4nkt mit Bitternissen", "tokens": ["Ich", "ward", "ge\u00b7tr\u00e4nkt", "mit", "Bit\u00b7ter\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Und grausam von den Wanzen gebissen;", "tokens": ["Und", "grau\u00b7sam", "von", "den", "Wan\u00b7zen", "ge\u00b7bis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.29": {"text": "Ich ward bedr\u00e4ngt von schwarzen Sorgen,", "tokens": ["Ich", "ward", "be\u00b7dr\u00e4ngt", "von", "schwar\u00b7zen", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Ich mu\u00dfte l\u00fcgen, ich mu\u00dfte borgen", "tokens": ["Ich", "mu\u00df\u00b7te", "l\u00fc\u00b7gen", ",", "ich", "mu\u00df\u00b7te", "bor\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "PPER", "VMFIN", "VVINF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.31": {"text": "Bei reichen Buben und alten Vetteln \u2013", "tokens": ["Bei", "rei\u00b7chen", "Bu\u00b7ben", "und", "al\u00b7ten", "Vet\u00b7teln", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.32": {"text": "Ich glaube sogar, ich mu\u00dfte betteln.", "tokens": ["Ich", "glau\u00b7be", "so\u00b7gar", ",", "ich", "mu\u00df\u00b7te", "bet\u00b7teln", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.33": {"text": "Jetzt bin ich m\u00fcd' vom Rennen und Laufen,", "tokens": ["Jetzt", "bin", "ich", "m\u00fcd'", "vom", "Ren\u00b7nen", "und", "Lau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VMFIN", "APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.34": {"text": "Jetzt will ich mich im Grabe verschnaufen.", "tokens": ["Jetzt", "will", "ich", "mich", "im", "Gra\u00b7be", "ver\u00b7schnau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.35": {"text": "Lebt wohl! Dort oben, ihr christlichen Br\u00fcder,", "tokens": ["Lebt", "wohl", "!", "Dort", "o\u00b7ben", ",", "ihr", "christ\u00b7li\u00b7chen", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$.", "ADV", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.36": {"text": "Ja, das versteht sich, dort sehn wir uns wieder.", "tokens": ["Ja", ",", "das", "ver\u00b7steht", "sich", ",", "dort", "sehn", "wir", "uns", "wie\u00b7der", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VVFIN", "PRF", "$,", "ADV", "VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}