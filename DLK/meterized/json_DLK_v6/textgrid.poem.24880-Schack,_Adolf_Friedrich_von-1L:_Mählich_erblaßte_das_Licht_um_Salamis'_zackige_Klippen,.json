easure": "dactylic.di.plus"}, "line.47": {"text": "Eine die andre zu wecken; mit Grimm und w\u00fcstem Geheule,", "tokens": ["Ei\u00b7ne", "die", "and\u00b7re", "zu", "we\u00b7cken", ";", "mit", "Grimm", "und", "w\u00fcs\u00b7tem", "Ge\u00b7heu\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "PIS", "PTKZU", "VVINF", "$.", "APPR", "NE", "KON", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.48": {"text": "Gei\u00dfeln in H\u00e4nden, empor st\u00fcrmt die entsetzliche Schar.", "tokens": ["Gei\u00b7\u00dfeln", "in", "H\u00e4n\u00b7den", ",", "em\u00b7por", "st\u00fcrmt", "die", "ent\u00b7setz\u00b7li\u00b7che", "Schar", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "PTKVZ", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.49": {"text": "\u00bbauf, ihn zu jagen, ihr Schwestern! Wohin mordtriefend er fliehn mag,", "tokens": ["\u00bb", "auf", ",", "ihn", "zu", "ja\u00b7gen", ",", "ihr", "Schwes\u00b7tern", "!", "Wo\u00b7hin", "mord\u00b7trie\u00b7fend", "er", "fliehn", "mag", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "PPER", "PTKZU", "VVINF", "$,", "PPOSAT", "NN", "$.", "PWAV", "ADJD", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+--+--+--++-+-+-", "measure": "dactylic.tri.plus"}, "line.50": {"text": "Ueber die L\u00e4nder, das Meer folgt ihm in hastigem Sprung!\u00ab", "tokens": ["Ue\u00b7ber", "die", "L\u00e4n\u00b7der", ",", "das", "Meer", "folgt", "ihm", "in", "has\u00b7ti\u00b7gem", "Sprung", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$.", "$("], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.51": {"text": "Und, sich die Br\u00fcste zerschlagend, mit weit aufstarrenden Blicken,", "tokens": ["Und", ",", "sich", "die", "Br\u00fcs\u00b7te", "zer\u00b7schla\u00b7gend", ",", "mit", "weit", "auf\u00b7star\u00b7ren\u00b7den", "Bli\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRF", "ART", "NN", "VVPP", "$,", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.52": {"text": "W\u00e4lzt sich in Beutebegier fort der m\u00e4nadische Chor \u2013", "tokens": ["W\u00e4lzt", "sich", "in", "Beu\u00b7te\u00b7be\u00b7gier", "fort", "der", "m\u00e4\u00b7na\u00b7di\u00b7sche", "Chor", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "PTKVZ", "ART", "ADJA", "NN", "$("], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.53": {"text": "Irrend, das Haupt umnachtet von Wahnsinn, naht sich indessen", "tokens": ["Ir\u00b7rend", ",", "das", "Haupt", "um\u00b7nach\u00b7tet", "von", "Wahn\u00b7sinn", ",", "naht", "sich", "in\u00b7des\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "ART", "NN", "VVPP", "APPR", "NN", "$,", "VVFIN", "PRF", "ADV"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.54": {"text": "Schwankenden Schrittes Orest Attikas gl\u00fccklichen Aun.", "tokens": ["Schwan\u00b7ken\u00b7den", "Schrit\u00b7tes", "O\u00b7rest", "At\u00b7ti\u00b7kas", "gl\u00fcck\u00b7li\u00b7chen", "Aun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NE", "NE", "ADJA", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.55": {"text": "Leuchtend im Fr\u00fchlicht steigen aus lachendem Gr\u00fcn der Olive", "tokens": ["Leuch\u00b7tend", "im", "Fr\u00fch\u00b7licht", "stei\u00b7gen", "aus", "la\u00b7chen\u00b7dem", "Gr\u00fcn", "der", "O\u00b7li\u00b7ve"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "+--+-+--+--+-+-+", "measure": "iambic.septa.invert"}, "line.56": {"text": "Heilige Tempel vor ihm, Bilder der G\u00f6tter empor.", "tokens": ["Hei\u00b7li\u00b7ge", "Tem\u00b7pel", "vor", "ihm", ",", "Bil\u00b7der", "der", "G\u00f6t\u00b7ter", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPER", "$,", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.57": {"text": "Mild schon lichtet ein Strahl ihm die n\u00e4chtig umdunkelte Seele;", "tokens": ["Mild", "schon", "lich\u00b7tet", "ein", "Strahl", "ihm", "die", "n\u00e4ch\u00b7tig", "um\u00b7dun\u00b7kel\u00b7te", "See\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "ART", "NN", "PPER", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.58": {"text": "Doch wie die Meute dem Wild, st\u00fcrmen die Furien ihm nach,", "tokens": ["Doch", "wie", "die", "Meu\u00b7te", "dem", "Wild", ",", "st\u00fcr\u00b7men", "die", "Fu\u00b7ri\u00b7en", "ihm", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+---+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.59": {"text": "Murmeln ins Ohr ihm den Fluch der e