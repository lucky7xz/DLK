{"textgrid.poem.24880": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: M\u00e4hlich erbla\u00dfte das Licht um Salamis' zackige Klippen,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00e4hlich erbla\u00dfte das Licht um Salamis' zackige Klippen,", "tokens": ["M\u00e4h\u00b7lich", "er\u00b7bla\u00df\u00b7te", "das", "Licht", "um", "Sa\u00b7la\u00b7mis'", "za\u00b7cki\u00b7ge", "Klip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "W\u00e4hrend die Sonne versank in das Aeg\u00e4ische Meer;", "tokens": ["W\u00e4h\u00b7rend", "die", "Son\u00b7ne", "ver\u00b7sank", "in", "das", "A\u00b7e\u00b7g\u00e4i\u00b7sche", "Meer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Hell nur leuchtete noch der honigber\u00fchmte Hymettus", "tokens": ["Hell", "nur", "leuch\u00b7te\u00b7te", "noch", "der", "ho\u00b7nig\u00b7be\u00b7r\u00fchm\u00b7te", "Hy\u00b7met\u00b7tus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Und die Cekropische Burg hoch auf dem Felsengestein.", "tokens": ["Und", "die", "Cek\u00b7ro\u00b7pi\u00b7sche", "Burg", "hoch", "auf", "dem", "Fel\u00b7sen\u00b7ge\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "--+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.5": {"text": "Um mich lagen verwirrt zerbr\u00f6ckelnde Tempelgesimse,", "tokens": ["Um", "mich", "la\u00b7gen", "ver\u00b7wirrt", "zer\u00b7br\u00f6\u00b7ckeln\u00b7de", "Tem\u00b7pel\u00b7ge\u00b7sim\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VVFIN", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.6": {"text": "S\u00e4ulen von dorischer Pracht, Tr\u00fcmmer auf Tr\u00fcmmer geh\u00e4uft.", "tokens": ["S\u00e4u\u00b7len", "von", "do\u00b7ri\u00b7scher", "Pracht", ",", "Tr\u00fcm\u00b7mer", "auf", "Tr\u00fcm\u00b7mer", "ge\u00b7h\u00e4uft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,", "NN", "APPR", "NN", "VVPP", "$."], "meter": "+---+-++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Kaum zu erkennen vermochte der Blick in dem Schutte die Stufen,", "tokens": ["Kaum", "zu", "er\u00b7ken\u00b7nen", "ver\u00b7moch\u00b7te", "der", "Blick", "in", "dem", "Schut\u00b7te", "die", "Stu\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Drauf das Athenische Volk Haupt sich zum Haupte gedr\u00e4ngt,", "tokens": ["Drauf", "das", "A\u00b7the\u00b7ni\u00b7sche", "Volk", "Haupt", "sich", "zum", "Haup\u00b7te", "ge\u00b7dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "NN", "PRF", "APPRART", "NN", "VVPP", "$,"], "meter": "--+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Wenn das Theater dem Donner von Aeschylus' Worten erdr\u00f6hnte,", "tokens": ["Wenn", "das", "The\u00b7a\u00b7ter", "dem", "Don\u00b7ner", "von", "A\u00b7e\u00b7schy\u00b7lus'", "Wor\u00b7ten", "er\u00b7dr\u00f6hn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "APPR", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+---+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Wenn es wie Weihrauchduft Sophokles' Odem durchzog.", "tokens": ["Wenn", "es", "wie", "Weih\u00b7rauch\u00b7duft", "So\u00b7pho\u00b7kles'", "O\u00b7dem", "durch\u00b7zog", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOKOM", "NN", "NE", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.11": {"text": "O wie sind sie verklungen, die herrlichen Ch\u00f6re der Meister!", "tokens": ["O", "wie", "sind", "sie", "ver\u00b7klun\u00b7gen", ",", "die", "herr\u00b7li\u00b7chen", "Ch\u00f6\u00b7re", "der", "Meis\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VAFIN", "PPER", "VVPP", "$,", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "O wie liegst du gest\u00fcrzt, heiligster Tempel der Kunst!", "tokens": ["O", "wie", "liegst", "du", "ge\u00b7st\u00fcrzt", ",", "hei\u00b7ligs\u00b7ter", "Tem\u00b7pel", "der", "Kunst", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VVFIN", "PPER", "VVPP", "$,", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Wo sich die Thymele hob, nicht wei\u00df ich die St\u00e4tte; es haben", "tokens": ["Wo", "sich", "die", "Thy\u00b7me\u00b7le", "hob", ",", "nicht", "wei\u00df", "ich", "die", "St\u00e4t\u00b7te", ";", "es", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PRF", "ART", "NN", "VVFIN", "$,", "PTKNEG", "VVFIN", "PPER", "ART", "NN", "$.", "PPER", "VAFIN"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Zwei Jahrtausende Staub auf die Orchestra geh\u00e4uft. \u2013", "tokens": ["Zwei", "Jahr\u00b7tau\u00b7sen\u00b7de", "Staub", "auf", "die", "Or\u00b7chest\u00b7ra", "ge\u00b7h\u00e4uft", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["CARD", "NN", "NN", "APPR", "ART", "NN", "VVPP", "$.", "$("], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.15": {"text": "W\u00e4hrend ich sa\u00df und das Auge bethr\u00e4nt auf den Tr\u00fcmmern mir ruhte,", "tokens": ["W\u00e4h\u00b7rend", "ich", "sa\u00df", "und", "das", "Au\u00b7ge", "be\u00b7thr\u00e4nt", "auf", "den", "Tr\u00fcm\u00b7mern", "mir", "ruh\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.16": {"text": "Schweifte die Seele zur\u00fcck in Perikleische Zeit.", "tokens": ["Schweif\u00b7te", "die", "See\u00b7le", "zu\u00b7r\u00fcck", "in", "Pe\u00b7rik\u00b7lei\u00b7sche", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.17": {"text": "Wechselnd schwebten vor mir die erhabnen Gestalten der Dichter,", "tokens": ["Wech\u00b7selnd", "schweb\u00b7ten", "vor", "mir", "die", "er\u00b7hab\u00b7nen", "Ge\u00b7stal\u00b7ten", "der", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "Welche zu Thr\u00e4nen wie Lust hier die Athener bewegt;", "tokens": ["Wel\u00b7che", "zu", "Thr\u00e4\u00b7nen", "wie", "Lust", "hier", "die", "A\u00b7the\u00b7ner", "be\u00b7wegt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "APPR", "NN", "KOKOM", "NN", "ADV", "ART", "ADJA", "VVFIN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.19": {"text": "Bald in unsterblichem Weh den titanischen Dulder mir malt' ich", "tokens": ["Bald", "in", "uns\u00b7terb\u00b7li\u00b7chem", "Weh", "den", "ti\u00b7ta\u00b7ni\u00b7schen", "Dul\u00b7der", "mir", "malt'", "ich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "PPER", "VVFIN", "PPER"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.20": {"text": "Ueber dem Weltabgrund ringend am Scythischen Fels,", "tokens": ["Ue\u00b7ber", "dem", "Welt\u00b7ab\u00b7grund", "rin\u00b7gend", "am", "Scyt\u00b7hi\u00b7schen", "Fels", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "+--++-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.21": {"text": "Bald den theb\u00e4ischen K\u00f6nig, wie blind er am Arme der Tochter,", "tokens": ["Bald", "den", "the\u00b7b\u00e4\u00b7i\u00b7schen", "K\u00f6\u00b7nig", ",", "wie", "blind", "er", "am", "Ar\u00b7me", "der", "Toch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "PWAV", "ADJD", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Thronlos, heimatlos L\u00e4nder und St\u00e4dte durchirrt.", "tokens": ["Thron\u00b7los", ",", "hei\u00b7mat\u00b7los", "L\u00e4n\u00b7der", "und", "St\u00e4d\u00b7te", "durch\u00b7irrt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.23": {"text": "Also sann ich und pre\u00dfte die Stirn auf verwitterten Marmor;", "tokens": ["Al\u00b7so", "sann", "ich", "und", "pre\u00df\u00b7te", "die", "Stirn", "auf", "ver\u00b7wit\u00b7ter\u00b7ten", "Mar\u00b7mor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.24": {"text": "Einzig die Seele noch sah, aber das Auge nicht mehr.", "tokens": ["Ein\u00b7zig", "die", "See\u00b7le", "noch", "sah", ",", "a\u00b7ber", "das", "Au\u00b7ge", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ADV", "VVFIN", "$,", "ADV", "ART", "NN", "PTKNEG", "ADV", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.25": {"text": "Horch, auf einmal, da was h\u00f6r' ich? Ein Rauschen, dem Sturm gleich,", "tokens": ["Horch", ",", "auf", "ein\u00b7mal", ",", "da", "was", "h\u00f6r'", "ich", "?", "Ein", "Rau\u00b7schen", ",", "dem", "Sturm", "gleich", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ADV", "$,", "KOUS", "PIS", "VVFIN", "PPER", "$.", "ART", "NN", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+----++-+--+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Wenn er im Pinienwald Wipfel und Aeste durchsaust!", "tokens": ["Wenn", "er", "im", "Pi\u00b7ni\u00b7en\u00b7wald", "Wip\u00b7fel", "und", "A\u00b7es\u00b7te", "durch\u00b7saust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.27": {"text": "Schnell mich raff' ich empor, und siehe! verwandelt ist alles;", "tokens": ["Schnell", "mich", "raff'", "ich", "em\u00b7por", ",", "und", "sie\u00b7he", "!", "ver\u00b7wan\u00b7delt", "ist", "al\u00b7les", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVIMP", "$.", "VVPP", "VAFIN", "PIS", "$."], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.28": {"text": "Statt der Tr\u00fcmmer umher ragt ein unendlicher Bau;", "tokens": ["Statt", "der", "Tr\u00fcm\u00b7mer", "um\u00b7her", "ragt", "ein", "un\u00b7end\u00b7li\u00b7cher", "Bau", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.29": {"text": "Hallen und fliegende Treppen und rings in den Nischen gewahr' ich", "tokens": ["Hal\u00b7len", "und", "flie\u00b7gen\u00b7de", "Trep\u00b7pen", "und", "rings", "in", "den", "Ni\u00b7schen", "ge\u00b7wahr'", "ich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN", "KON", "ADV", "APPR", "ART", "NN", "VVFIN", "PPER"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.30": {"text": "Bilder, wie Phidias sie parischem Marmor entlockt.", "tokens": ["Bil\u00b7der", ",", "wie", "Phi\u00b7di\u00b7as", "sie", "pa\u00b7ri\u00b7schem", "Mar\u00b7mor", "ent\u00b7lockt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NE", "PPER", "ADJA", "NN", "VVPP", "$."], "meter": "+-------+-+--+", "measure": "iambic.tetra.chol"}, "line.31": {"text": "Aufw\u00e4rts steigen zu Seiten mir Sitzreihn, Stufen an Stufen,", "tokens": ["Auf\u00b7w\u00e4rts", "stei\u00b7gen", "zu", "Sei\u00b7ten", "mir", "Sitz\u00b7reihn", ",", "Stu\u00b7fen", "an", "Stu\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "PPER", "NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.32": {"text": "Tausende dr\u00e4ngen sich drauf in der hellenischen Tracht;", "tokens": ["Tau\u00b7sen\u00b7de", "dr\u00e4n\u00b7gen", "sich", "drauf", "in", "der", "hel\u00b7le\u00b7ni\u00b7schen", "Tracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "PAV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.33": {"text": "Weihrauch quillt vom Altar, im Festschmuck leuchtet die Scene,", "tokens": ["Weih\u00b7rauch", "quillt", "vom", "Al\u00b7tar", ",", "im", "Fest\u00b7schmuck", "leuch\u00b7tet", "die", "Sce\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.34": {"text": "Und zu dem Chorlied schallt lieblich der Fl\u00f6ten Get\u00f6n.", "tokens": ["Und", "zu", "dem", "Chor\u