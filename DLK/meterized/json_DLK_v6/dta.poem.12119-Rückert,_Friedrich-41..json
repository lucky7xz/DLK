{"dta.poem.12119": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "41.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lebt oder starb der Mann, der den Verrath begieng,", "tokens": ["Lebt", "o\u00b7der", "starb", "der", "Mann", ",", "der", "den", "Ver\u00b7rath", "be\u00b7gieng", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wodurch des Feindes Macht den theuern Helden fieng?", "tokens": ["Wo\u00b7durch", "des", "Fein\u00b7des", "Macht", "den", "theu\u00b7ern", "Hel\u00b7den", "fi\u00b7eng", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.2": {"line.1": {"text": "\u201eer lebt.\u201c Gelobt sei Gott, da\u00df er noch b\u00fc\u00dfen kann,", "tokens": ["\u201e", "er", "lebt", ".", "\u201c", "Ge\u00b7lobt", "sei", "Gott", ",", "da\u00df", "er", "noch", "b\u00fc\u00b7\u00dfen", "kann", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$.", "$(", "VVPP", "VAFIN", "NN", "$,", "KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was er am Vaterland verbrach und an dem Mann.", "tokens": ["Was", "er", "am", "Va\u00b7ter\u00b7land", "ver\u00b7brach", "und", "an", "dem", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "VVFIN", "KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ist er reich oder arm? \u201ereich!\u201c ihm o desto schlimmer,", "tokens": ["Ist", "er", "reich", "o\u00b7der", "arm", "?", "\u201e", "reich", "!", "\u201c", "ihm", "o", "des\u00b7to", "schlim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KON", "ADJD", "$.", "$(", "ADJD", "$.", "$(", "PPER", "FM", "ADV", "ADJD", "$,"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Zur Reue wird er sp\u00e4t gelangen oder nimmer.", "tokens": ["Zur", "Reu\u00b7e", "wird", "er", "sp\u00e4t", "ge\u00b7lan\u00b7gen", "o\u00b7der", "nim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADJD", "VVINF", "KON", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch hat er Kinder? \u201eNein!\u201c Nun gut, so mag er sterben,", "tokens": ["Doch", "hat", "er", "Kin\u00b7der", "?", "\u201e", "Nein", "!", "\u201c", "Nun", "gut", ",", "so", "mag", "er", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "NN", "$.", "$(", "PTKANT", "$.", "$(", "ADV", "ADJD", "$,", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ohn' auf Unschuldige den Schuldfluch zu vererben.", "tokens": ["Ohn'", "auf", "Un\u00b7schul\u00b7di\u00b7ge", "den", "Schuld\u00b7fluch", "zu", "ver\u00b7er\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}