{"textgrid.poem.54904": {"metadata": {"author": {"name": "Scheerbart, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Alte Knaben sitzen auf den leersten Tonnen,", "genre": "verse", "period": "N.A.", "pub_year": 1889, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alte Knaben sitzen auf den leersten Tonnen,", "tokens": ["Al\u00b7te", "Kna\u00b7ben", "sit\u00b7zen", "auf", "den", "leers\u00b7ten", "Ton\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Und die N\u00e4chte siegen \u00fcber alle Sonnen.", "tokens": ["Und", "die", "N\u00e4ch\u00b7te", "sie\u00b7gen", "\u00fc\u00b7ber", "al\u00b7le", "Son\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Hinten nagen unsichtbare wei\u00dfe M\u00e4use", "tokens": ["Hin\u00b7ten", "na\u00b7gen", "un\u00b7sicht\u00b7ba\u00b7re", "wei\u00b7\u00dfe", "M\u00e4u\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "An dem b\u00f6s zerbeulten gro\u00dfen Hirngeh\u00e4use.", "tokens": ["An", "dem", "b\u00f6s", "zer\u00b7beul\u00b7ten", "gro\u00b7\u00dfen", "Hirn\u00b7ge\u00b7h\u00e4u\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "H\u00f6r doch, wie die ganze Sch\u00e4delh\u00f6hle quarrt!", "tokens": ["H\u00f6r", "doch", ",", "wie", "die", "gan\u00b7ze", "Sch\u00e4\u00b7del\u00b7h\u00f6h\u00b7le", "quarrt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Ist die alte Rinde \u00bbwirklich\u00ab noch so hart?", "tokens": ["Ist", "die", "al\u00b7te", "Rin\u00b7de", "\u00bb", "wirk\u00b7lich", "\u00ab", "noch", "so", "hart", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$(", "ADJD", "$(", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "Alles geht zu Ende \u2013 auch der dickste Kopf!", "tokens": ["Al\u00b7les", "geht", "zu", "En\u00b7de", "\u2013", "auch", "der", "dicks\u00b7te", "Kopf", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "$(", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "Ach, die wei\u00dfen M\u00e4use haben dich am Schopf!", "tokens": ["Ach", ",", "die", "wei\u00b7\u00dfen", "M\u00e4u\u00b7se", "ha\u00b7ben", "dich", "am", "Schopf", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "ADJA", "NN", "VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.9": {"text": "Glaubst du, L\u00e4use sitzen blo\u00df in deinem Puder?", "tokens": ["Glaubst", "du", ",", "L\u00e4u\u00b7se", "sit\u00b7zen", "blo\u00df", "in", "dei\u00b7nem", "Pu\u00b7der", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.10": {"text": "Nein, du bist ein unversch\u00e4mtes dummes Luder,", "tokens": ["Nein", ",", "du", "bist", "ein", "un\u00b7ver\u00b7sch\u00e4m\u00b7tes", "dum\u00b7mes", "Lu\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.11": {"text": "Und die Frechheit kommt in erster Reihe ran.", "tokens": ["Und", "die", "Frech\u00b7heit", "kommt", "in", "ers\u00b7ter", "Rei\u00b7he", "ran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}