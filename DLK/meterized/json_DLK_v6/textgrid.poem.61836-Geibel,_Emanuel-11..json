{"textgrid.poem.61836": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Ja sei Ja, dein Nein sei Nein,", "tokens": ["Dein", "Ja", "sei", "Ja", ",", "dein", "Nein", "sei", "Nein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKANT", "$,", "PPOSAT", "NN", "VAFIN", "PTKANT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und scharf das Schwert an deiner Lende;", "tokens": ["Und", "scharf", "das", "Schwert", "an", "dei\u00b7ner", "Len\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die beste Staatskunst bleibt's am Ende", "tokens": ["Die", "bes\u00b7te", "Staats\u00b7kunst", "bleibt's", "am", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch, tapfer und gerecht zu sein.", "tokens": ["Doch", ",", "tap\u00b7fer", "und", "ge\u00b7recht", "zu", "sein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "KON", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}