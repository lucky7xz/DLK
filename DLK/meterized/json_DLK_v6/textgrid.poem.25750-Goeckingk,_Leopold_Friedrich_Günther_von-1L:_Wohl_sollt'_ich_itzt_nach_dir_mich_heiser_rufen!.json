{"textgrid.poem.25750": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wohl sollt' ich itzt nach dir mich heiser rufen!", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl sollt' ich itzt nach dir mich heiser rufen!", "tokens": ["Wohl", "sollt'", "ich", "itzt", "nach", "dir", "mich", "hei\u00b7ser", "ru\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "PPER", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Traurigen bist du ein Gut!", "tokens": ["Dem", "Trau\u00b7ri\u00b7gen", "bist", "du", "ein", "Gut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Allein hinan des Lebens letzte Stufen!", "tokens": ["Al\u00b7lein", "hi\u00b7nan", "des", "Le\u00b7bens", "letz\u00b7te", "Stu\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn die\u00df will gr\u00f6\u00dfern Muth.", "tokens": ["Denn", "die\u00df", "will", "gr\u00f6\u00b7\u00dfern", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was schwerer ist, war immer meine Sache.", "tokens": ["Was", "schwe\u00b7rer", "ist", ",", "war", "im\u00b7mer", "mei\u00b7ne", "Sa\u00b7che", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie? dennoch wollt' ich schon hinab,", "tokens": ["Wie", "?", "den\u00b7noch", "wollt'", "ich", "schon", "hin\u00b7ab", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "ADV", "VMFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gleich einem Weib', auf meinem Thr\u00e4nenbache", "tokens": ["Gleich", "ei\u00b7nem", "Weib'", ",", "auf", "mei\u00b7nem", "Thr\u00e4\u00b7nen\u00b7ba\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mich schwemmen an das Grab?", "tokens": ["Mich", "schwem\u00b7men", "an", "das", "Grab", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Und wollte \u2013 denn von ihrem Todesschlummer", "tokens": ["Und", "woll\u00b7te", "\u2013", "denn", "von", "ih\u00b7rem", "To\u00b7des\u00b7schlum\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "$(", "KON", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Sind beide sicher wieder wach! \u2013", "tokens": ["Sind", "bei\u00b7de", "si\u00b7cher", "wie\u00b7der", "wach", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PIS", "ADJD", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Durch meinen bleichen, eingefallnen Kummer", "tokens": ["Durch", "mei\u00b7nen", "blei\u00b7chen", ",", "ein\u00b7ge\u00b7fall\u00b7nen", "Kum\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Erpressen noch ihr Ach? \u2013", "tokens": ["Er\u00b7pres\u00b7sen", "noch", "ihr", "Ach", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Ist ihnen wohl: wer kann, als ich, sie st\u00f6ren?", "tokens": ["Ist", "ih\u00b7nen", "wohl", ":", "wer", "kann", ",", "als", "ich", ",", "sie", "st\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$.", "PWS", "VMFIN", "$,", "KOUS", "PPER", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Im Fall' sie noch mich weinen sehn!", "tokens": ["Im", "Fall'", "sie", "noch", "mich", "wei\u00b7nen", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "ADV", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Ha! kann mein Dulden ihre Ruhe mehren:", "tokens": ["Ha", "!", "kann", "mein", "Dul\u00b7den", "ih\u00b7re", "Ru\u00b7he", "meh\u00b7ren", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VMFIN", "PPOSAT", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "So soll die Trauer gehn!", "tokens": ["So", "soll", "die", "Trau\u00b7er", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Wir mu\u00dften einst, (doch schon so fr\u00fch?) uns trennen!", "tokens": ["Wir", "mu\u00df\u00b7ten", "einst", ",", "(", "doch", "schon", "so", "fr\u00fch", "?", ")", "uns", "tren\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$,", "$(", "ADV", "ADV", "ADV", "ADJD", "$.", "$(", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "W\u00e4r' ich vorauf gegangen: ach!", "tokens": ["W\u00e4r'", "ich", "vor\u00b7auf", "ge\u00b7gan\u00b7gen", ":", "ach", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$.", "XY", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Wie h\u00e4tten sie die B\u00fcrde schleppen k\u00f6nnen?", "tokens": ["Wie", "h\u00e4t\u00b7ten", "sie", "die", "B\u00fcr\u00b7de", "schlep\u00b7pen", "k\u00f6n\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ART", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Bin ich nicht fast zu schwach?", "tokens": ["Bin", "ich", "nicht", "fast", "zu", "schwach", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Hier war es gut, dort, hoff' ich, ist es besser!", "tokens": ["Hier", "war", "es", "gut", ",", "dort", ",", "hoff'", "ich", ",", "ist", "es", "bes\u00b7ser", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "ADV", "$,", "VVFIN", "PPER", "$,", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Sie sind vom Sehen nun zum Schaun!", "tokens": ["Sie", "sind", "vom", "Se\u00b7hen", "nun", "zum", "Schaun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Ich aber werde ruhig meine Schl\u00f6sser", "tokens": ["Ich", "a\u00b7ber", "wer\u00b7de", "ru\u00b7hig", "mei\u00b7ne", "Schl\u00f6s\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Hier in die Luft noch baun.", "tokens": ["Hier", "in", "die", "Luft", "noch", "baun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Mein Tagewerk ist noch nicht ganz geendet.", "tokens": ["Mein", "Ta\u00b7ge\u00b7werk", "ist", "noch", "nicht", "ganz", "ge\u00b7en\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "La\u00df mich geschwind noch ab es thun,", "tokens": ["La\u00df", "mich", "ge\u00b7schwind", "noch", "ab", "es", "thun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "ADV", "PTKVZ", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Um dreimal sanfter, wann ich's nun vollendet,", "tokens": ["Um", "drei\u00b7mal", "sanf\u00b7ter", ",", "wann", "ich's", "nun", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADJD", "$,", "PWAV", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "In ihrem Schoo\u00df' zu ruhn.", "tokens": ["In", "ih\u00b7rem", "Schoo\u00df'", "zu", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "Dann f\u00fchre mich, o Tod! durch dich verkl\u00e4ret", "tokens": ["Dann", "f\u00fch\u00b7re", "mich", ",", "o", "Tod", "!", "durch", "dich", "ver\u00b7kl\u00e4\u00b7ret"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "FM", "NN", "$.", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Zum leichtern Geist', zu ihnen hin,", "tokens": ["Zum", "leich\u00b7tern", "Geist'", ",", "zu", "ih\u00b7nen", "hin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Und wer mich liebt, frohlocke, da\u00df gew\u00e4hret", "tokens": ["Und", "wer", "mich", "liebt", ",", "froh\u00b7lo\u00b7cke", ",", "da\u00df", "ge\u00b7w\u00e4h\u00b7ret"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "$,", "KOUS", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Ich meines Wunsches bin!", "tokens": ["Ich", "mei\u00b7nes", "Wun\u00b7sches", "bin", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}