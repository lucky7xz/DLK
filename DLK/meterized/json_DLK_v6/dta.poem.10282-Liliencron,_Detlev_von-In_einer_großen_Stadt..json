{"dta.poem.10282": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "In einer gro\u00dfen Stadt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Es treibt vor&#252;ber mir im Meer der Stadt             ", "tokens": ["Es", "treibt", "vor", "&#252;", "ber", "mir", "im", "Meer", "der", "Stadt"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "$(", "APPR", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bald Der, bald Jener, Einer nach dem Andern.", "tokens": ["Bald", "Der", ",", "bald", "Je\u00b7ner", ",", "Ei\u00b7ner", "nach", "dem", "An\u00b7dern", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "ADV", "PDAT", "$,", "PIS", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Blick ins Auge, und vor\u00fcber schon.", "tokens": ["Ein", "Blick", "ins", "Au\u00b7ge", ",", "und", "vor\u00b7\u00fc\u00b7ber", "schon", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "KON", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es tropft vor\u00fcber mir ins Meer des Nichts", "tokens": ["Es", "tropft", "vor\u00b7\u00fc\u00b7ber", "mir", "ins", "Meer", "des", "Nichts"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bald Der, bald Jener, Einer nach dem Andern.", "tokens": ["Bald", "Der", ",", "bald", "Je\u00b7ner", ",", "Ei\u00b7ner", "nach", "dem", "An\u00b7dern", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "ADV", "PDAT", "$,", "PIS", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Blick auf seinen Sarg, vor\u00fcber schon.", "tokens": ["Ein", "Blick", "auf", "sei\u00b7nen", "Sarg", ",", "vor\u00b7\u00fc\u00b7ber", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es schwimmt ein Leichenzug im Meer der Stadt,", "tokens": ["Es", "schwimmt", "ein", "Lei\u00b7chen\u00b7zug", "im", "Meer", "der", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Querweg die Menschen, Einer nach dem Andern.", "tokens": ["Quer\u00b7weg", "die", "Men\u00b7schen", ",", "Ei\u00b7ner", "nach", "dem", "An\u00b7dern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PIS", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Blick auf meinen Sarg, vor\u00fcber schon.", "tokens": ["Ein", "Blick", "auf", "mei\u00b7nen", "Sarg", ",", "vor\u00b7\u00fc\u00b7ber", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}