{"textgrid.poem.48655": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "18. Die verletzte Charitinne", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wol dem, der Gnad' um Recht kan finden", "tokens": ["Wol", "dem", ",", "der", "Gnad'", "um", "Recht", "kan", "fin\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "$,", "ART", "NN", "APPR", "NN", "VMFIN", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "bei der, die \u00fcber ihn ruft Weh'!", "tokens": ["bei", "der", ",", "die", "\u00fc\u00b7ber", "ihn", "ruft", "Weh'", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "APPR", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er giebt sein Leid den leichten Winden", "tokens": ["Er", "giebt", "sein", "Leid", "den", "leich\u00b7ten", "Win\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und l\u00e4\u00dft es tragen \u00fcber See.", "tokens": ["und", "l\u00e4\u00dft", "es", "tra\u00b7gen", "\u00fc\u00b7ber", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O du, verletzte Charitinne,", "tokens": ["O", "du", ",", "ver\u00b7letz\u00b7te", "Cha\u00b7ri\u00b7tin\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "bist noch auf deinem harten Sinne.", "tokens": ["bist", "noch", "auf", "dei\u00b7nem", "har\u00b7ten", "Sin\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er spielet f\u00f6rder aufs Gewisse,", "tokens": ["Er", "spie\u00b7let", "f\u00f6r\u00b7der", "aufs", "Ge\u00b7wis\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "h\u00f6rt nicht, was dem und jenem tr\u00e4umt,", "tokens": ["h\u00f6rt", "nicht", ",", "was", "dem", "und", "je\u00b7nem", "tr\u00e4umt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PRELS", "ART", "KON", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "giebt seiner Liebsten K\u00fcss' um K\u00fcsse", "tokens": ["giebt", "sei\u00b7ner", "Liebs\u00b7ten", "K\u00fcss'", "um", "K\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und holet nach, was er vers\u00e4umt.", "tokens": ["und", "ho\u00b7let", "nach", ",", "was", "er", "ver\u00b7s\u00e4umt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O du, verletzte Charitinne,", "tokens": ["O", "du", ",", "ver\u00b7letz\u00b7te", "Cha\u00b7ri\u00b7tin\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "bist noch auf deinem harten Sinne.", "tokens": ["bist", "noch", "auf", "dei\u00b7nem", "har\u00b7ten", "Sin\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie hastu mich so lassen fallen,", "tokens": ["Wie", "has\u00b7tu", "mich", "so", "las\u00b7sen", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verh\u00e4ngn\u00fc\u00df oder was du bist?", "tokens": ["Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "o\u00b7der", "was", "du", "bist", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das sch\u00f6nste M\u00e4gdlein unter allen", "tokens": ["Das", "sch\u00f6ns\u00b7te", "M\u00e4gd\u00b7lein", "un\u00b7ter", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "hast du betr\u00fcbt durch deine List.", "tokens": ["hast", "du", "be\u00b7tr\u00fcbt", "durch", "dei\u00b7ne", "List", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O du, verletzte Charitinne,", "tokens": ["O", "du", ",", "ver\u00b7letz\u00b7te", "Cha\u00b7ri\u00b7tin\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "bist noch auf deinem harten Sinne.", "tokens": ["bist", "noch", "auf", "dei\u00b7nem", "har\u00b7ten", "Sin\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich schw\u00f6re bei den Flitz' und Pfeilen,", "tokens": ["Ich", "schw\u00f6\u00b7re", "bei", "den", "Flitz'", "und", "Pfei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "darmit der kleine Gott uns zwingt,", "tokens": ["dar\u00b7mit", "der", "klei\u00b7ne", "Gott", "uns", "zwingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "da\u00df ich mich lassen \u00fcbereilen", "tokens": ["da\u00df", "ich", "mich", "las\u00b7sen", "\u00fc\u00b7be\u00b7rei\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "di\u00df, was mir nun den Tod fast bringt.", "tokens": ["di\u00df", ",", "was", "mir", "nun", "den", "Tod", "fast", "bringt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "ADV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O du, verletzte Charitinne,", "tokens": ["O", "du", ",", "ver\u00b7letz\u00b7te", "Cha\u00b7ri\u00b7tin\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "bist noch auf deinem harten Sinne.", "tokens": ["bist", "noch", "auf", "dei\u00b7nem", "har\u00b7ten", "Sin\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Hab' ich seit der Zeit recht geschlafen,", "tokens": ["Hab'", "ich", "seit", "der", "Zeit", "recht", "ge\u00b7schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "hab' ich gepflogen ein'ger Lust,", "tokens": ["hab'", "ich", "ge\u00b7pflo\u00b7gen", "ein'\u00b7ger", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "so m\u00fcsse mich der Knabe strafen,", "tokens": ["so", "m\u00fcs\u00b7se", "mich", "der", "Kna\u00b7be", "stra\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "dem du so stets zuwider tust!", "tokens": ["dem", "du", "so", "stets", "zu\u00b7wi\u00b7der", "tust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und du, verletzte Charitinne,", "tokens": ["Und", "du", ",", "ver\u00b7letz\u00b7te", "Cha\u00b7ri\u00b7tin\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "bist noch auf deinem harten Sinne.", "tokens": ["bist", "noch", "auf", "dei\u00b7nem", "har\u00b7ten", "Sin\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ist dieses auch erh\u00f6ret worden,", "tokens": ["Ist", "die\u00b7ses", "auch", "er\u00b7h\u00f6\u00b7ret", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADV", "VVPP", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "zugleiche sch\u00f6n und grausam sein?", "tokens": ["zu\u00b7glei\u00b7che", "sch\u00f6n", "und", "grau\u00b7sam", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kupido f\u00fchrt den frommen Orden,", "tokens": ["Ku\u00b7pi\u00b7do", "f\u00fchrt", "den", "from\u00b7men", "Or\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "bei ihm rei\u00dft ganz kein Zank nicht ein.", "tokens": ["bei", "ihm", "rei\u00dft", "ganz", "kein", "Zank", "nicht", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ADV", "PIAT", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Und du, verletzte Charitinne,", "tokens": ["Und", "du", ",", "ver\u00b7letz\u00b7te", "Cha\u00b7ri\u00b7tin\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "bist noch auf deinem harten Sinne,", "tokens": ["bist", "noch", "auf", "dei\u00b7nem", "har\u00b7ten", "Sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ie h\u00f6her Einer ist vom Stande,", "tokens": ["Ie", "h\u00f6\u00b7her", "Ei\u00b7ner", "ist", "vom", "Stan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PIS", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "ie weniger bewegt er sich.", "tokens": ["ie", "we\u00b7ni\u00b7ger", "be\u00b7wegt", "er", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der P\u00f6vel braucht der Rach' und Schande,", "tokens": ["Der", "P\u00f6\u00b7vel", "braucht", "der", "Rach'", "und", "Schan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "verschonen, das steht k\u00f6niglich.", "tokens": ["ver\u00b7scho\u00b7nen", ",", "das", "steht", "k\u00f6\u00b7nig\u00b7lich", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PDS", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und du, verletzte Charitinne,", "tokens": ["Und", "du", ",", "ver\u00b7letz\u00b7te", "Cha\u00b7ri\u00b7tin\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "bist noch auf deinem harten Sinne.", "tokens": ["bist", "noch", "auf", "dei\