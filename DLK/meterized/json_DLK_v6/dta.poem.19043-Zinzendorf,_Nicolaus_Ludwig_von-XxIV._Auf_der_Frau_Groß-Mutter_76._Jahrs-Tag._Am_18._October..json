{"dta.poem.19043": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "XxIV.  Auf der Frau Gro\u00df-Mutter 76.  \n Jahrs-Tag.   Am 18. October.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Die ihr mit fremden Schmuck zu prangen f\u00e4hig seyd,", "tokens": ["Die", "ihr", "mit", "frem\u00b7den", "Schmuck", "zu", "pran\u00b7gen", "f\u00e4\u00b7hig", "seyd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "PTKZU", "VVFIN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht aber eignen Glantz der Tugend zu erreichen!", "tokens": ["Nicht", "a\u00b7ber", "eig\u00b7nen", "Glantz", "der", "Tu\u00b7gend", "zu", "er\u00b7rei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Geht, r\u00fchmet, was ihr k\u00f6nnt, der Ahnen Treflichkeit;", "tokens": ["Geht", ",", "r\u00fch\u00b7met", ",", "was", "ihr", "k\u00f6nnt", ",", "der", "Ah\u00b7nen", "Tref\u00b7lich\u00b7keit", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Pocht, Helden, destomehr auf eigne Sieges-Zeichen.", "tokens": ["Pocht", ",", "Hel\u00b7den", ",", "des\u00b7to\u00b7mehr", "auf", "eig\u00b7ne", "Sie\u00b7ges\u00b7Zei\u00b7chen", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Geht, grabet euren Prei\u00df in Marmel-Stein und Ertz,", "tokens": ["Geht", ",", "gra\u00b7bet", "eu\u00b7ren", "Prei\u00df", "in", "Mar\u00b7mel\u00b7Stein", "und", "Ertz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Setzt Pfal und Pfeilerwerck wo sonsten Str\u00f6me flossen;", "tokens": ["Setzt", "Pfal", "und", "Pfei\u00b7ler\u00b7werck", "wo", "sons\u00b7ten", "Str\u00f6\u00b7me", "flos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "PWAV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Erhebt den Obelisc von dannen Himmelwerts,", "tokens": ["Er\u00b7hebt", "den", "O\u00b7be\u00b7lisc", "von", "dan\u00b7nen", "Him\u00b7mel\u00b7werts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Th\u00fcrmt Ehren-Pforten auf, setzt prangende Colossen:", "tokens": ["Th\u00fcrmt", "Eh\u00b7ren\u00b7P\u00b7for\u00b7ten", "auf", ",", "setzt", "pran\u00b7gen\u00b7de", "Co\u00b7los\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKVZ", "$,", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "La\u00dft in Gem\u00e4hlden sehn, was eure Mord-Faust thut,", "tokens": ["La\u00dft", "in", "Ge\u00b7m\u00e4hl\u00b7den", "sehn", ",", "was", "eu\u00b7re", "Mord\u00b7Faust", "thut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "NN", "VVINF", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.10": {"text": "Der Reimer Phantasey von eurer H\u00e4rte dichten;", "tokens": ["Der", "Rei\u00b7mer", "Phan\u00b7ta\u00b7sey", "von", "eu\u00b7rer", "H\u00e4r\u00b7te", "dich\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPOSAT", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Beschreibt mit C\u00e4sare den eignen Helden-Muth,", "tokens": ["Be\u00b7schreibt", "mit", "C\u00e4\u00b7sa\u00b7re", "den", "eig\u00b7nen", "Hel\u00b7den\u00b7Muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "La\u00dft die Historien euch Ehren-Mahle richten.", "tokens": ["La\u00dft", "die", "His\u00b7to\u00b7ri\u00b7en", "euch", "Eh\u00b7ren\u00b7Mah\u00b7le", "rich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wie? oder eckelt euch vor Blut und Feuer-Strahl,", "tokens": ["Wie", "?", "o\u00b7der", "ec\u00b7kelt", "euch", "vor", "Blut", "und", "Feu\u00b7er\u00b7Strahl", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KON", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Betr\u00fcben euch vielleicht die drohenden Trompeten?", "tokens": ["Be\u00b7tr\u00fc\u00b7ben", "euch", "viel\u00b7leicht", "die", "dro\u00b7hen\u00b7den", "Trom\u00b7pe\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--++-+---+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Beliebet euch vielmehr ein lustig Freuden-Mahl,", "tokens": ["Be\u00b7lie\u00b7bet", "euch", "viel\u00b7mehr", "ein", "lus\u00b7tig", "Freu\u00b7den\u00b7Mahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "Und der ged\u00e4mpfte Klang der angenehmen Fl\u00f6ten?", "tokens": ["Und", "der", "ge\u00b7d\u00e4mpf\u00b7te", "Klang", "der", "an\u00b7ge\u00b7neh\u00b7men", "Fl\u00f6\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "So la\u00dft die k\u00fcnftge Welt von eurer Freundlichkeit,", "tokens": ["So", "la\u00dft", "die", "k\u00fcnft\u00b7ge", "Welt", "von", "eu\u00b7rer", "Freund\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Von eurer Wayde-Lust und Bau-Begierde sprechen;", "tokens": ["Von", "eu\u00b7rer", "Way\u00b7de\u00b7Lust", "und", "Bau\u00b7Be\u00b7gier\u00b7de", "spre\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und wenn der Erb-Printz einst mit Regimentern dr\u00e4ut,", "tokens": ["Und", "wenn", "der", "Er\u00b7bPrintz", "einst", "mit", "Re\u00b7gi\u00b7men\u00b7tern", "dr\u00e4ut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.20": {"text": "Der Diener weiches Hertz vor Reu und Sehnen brechen.", "tokens": ["Der", "Die\u00b7ner", "wei\u00b7ches", "Hertz", "vor", "Reu", "und", "Seh\u00b7nen", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "NE", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ist euer edler Geist den Musen zugeneigt,", "tokens": ["Ist", "eu\u00b7er", "ed\u00b7ler", "Geist", "den", "Mu\u00b7sen", "zu\u00b7gen\u00b7eigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und mag sich seine Zeit mit Schul-Gez\u00e4ncke k\u00fcrtzen;", "tokens": ["Und", "mag", "sich", "sei\u00b7ne", "Zeit", "mit", "Schul\u00b7Ge\u00b7z\u00e4n\u00b7cke", "k\u00fcrt\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Schreyt Aristoteles, Mars und Diana schweigt;", "tokens": ["Schreyt", "A\u00b7ris\u00b7to\u00b7te\u00b7les", ",", "Mars", "und", "Di\u00b7a\u00b7na", "schweigt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "KON", "NE", "VVFIN", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "So wird die Castalis auf euern Lehn-Stuhl st\u00fcrtzen.", "tokens": ["So", "wird", "die", "Cas\u00b7ta\u00b7lis", "auf", "eu\u00b7ern", "Lehn\u00b7Stuhl", "st\u00fcrt\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NE", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Den einen h\u00f6ret man auf Krieg und Kriegs-Geschrey,", "tokens": ["Den", "ei\u00b7nen", "h\u00f6\u00b7ret", "man", "auf", "Krieg", "und", "Kriegs\u00b7Ge\u00b7schrey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PIS", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Den andern auf die Lust und Eitelkeiten schm\u00e4hlen;", "tokens": ["Den", "an\u00b7dern", "auf", "die", "Lust", "und", "Ei\u00b7tel\u00b7kei\u00b7ten", "schm\u00e4h\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "ART", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Der dritte spricht von nichts, als von Pedanterey;", "tokens": ["Der", "drit\u00b7te", "spricht", "von", "nichts", ",", "als", "von", "Pe\u00b7dan\u00b7te\u00b7rey", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "PIS", "$,", "KOUS", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Ein jeder giebt der Zeit ein M\u00e4hrlein zu erzehlen.", "tokens": ["Ein", "je\u00b7der", "giebt", "der", "Zeit", "ein", "M\u00e4hr\u00b7lein", "zu", "er\u00b7zeh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ich lobe einen Geist der von dem Kinder-Spiel", "tokens": ["Ich", "lo\u00b7be", "ei\u00b7nen", "Geist", "der", "von", "dem", "Kin\u00b7der\u00b7Spiel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Der Lob-Gedichte noch in Zeiten \u00fcberzeuget,", "tokens": ["Der", "Lob\u00b7Ge\u00b7dich\u00b7te", "noch", "in", "Zei\u00b7ten", "\u00fc\u00b7berz\u00b7eu\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Auf eine Ewigkeit, das kurtze Lebens-Ziel,", "tokens": ["Auf", "ei\u00b7ne", "E\u00b7wig\u00b7keit", ",", "das", "kurt\u00b7ze", "Le\u00b7bens\u00b7Ziel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und seiner Thaten Zweck auf GOttes Wege neiget.", "tokens": ["Und", "sei\u00b7ner", "Tha\u00b7ten", "Zweck", "auf", "Got\u00b7tes", "We\u00b7ge", "nei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Den lob ich, dessen Geist aufs Himmlische gewandt,", "tokens": ["Den", "lob", "ich", ",", "des\u00b7sen", "Geist", "aufs", "Himm\u00b7li\u00b7sche", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$,", "PRELAT", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die hin und her zerstreut und eitle Menschen-Kinder", "tokens": ["Die", "hin", "und", "her", "zer\u00b7streut", "und", "eit\u00b7le", "Men\u00b7schen\u00b7Kin\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PTKVZ", "KON", "ADV", "VVPP", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Mit viel Erbarmen tr\u00e4gt, und seinen Ehren-Stand", "tokens": ["Mit", "viel", "Er\u00b7bar\u00b7men", "tr\u00e4gt", ",", "und", "sei\u00b7nen", "Eh\u00b7ren\u00b7Stand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Darinn alleine sucht: Er sey ein armer S\u00fcnder.", "tokens": ["Da\u00b7rinn", "al\u00b7lei\u00b7ne", "sucht", ":", "Er", "sey", "ein", "ar\u00b7mer", "S\u00fcn\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Frau, deren Helden-Muth in abgewichner Zeit,", "tokens": ["Frau", ",", "de\u00b7ren", "Hel\u00b7den\u00b7Muth", "in", "ab\u00b7ge\u00b7wich\u00b7ner", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.38": {"text": "Die hochgelahrte Schaar durch manchen Dichter ehrte,", "tokens": ["Die", "hoch\u00b7ge\u00b7lahr\u00b7te", "Schaar", "durch", "man\u00b7chen", "Dich\u00b7ter", "ehr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Nicht so? Der Ruhm behielt gar wenig Herrlichkeit,", "tokens": ["Nicht", "so", "?", "Der", "Ruhm", "be\u00b7hielt", "gar", "we\u00b7nig", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$.", "ART", "NN", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Seit dem dich unser HErr selbst etwas bessers lehrte.", "tokens": ["Seit", "dem", "dich", "un\u00b7ser", "Herr", "selbst", "et\u00b7was", "bes\u00b7sers", "lehr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPOSAT", "NN", "ADV", "PIAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Ich meyne, ", "tokens": ["Ich", "mey\u00b7ne", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.42": {"text": "Die Anfurth deines Geists aus dieser Welt-Gewirre,", "tokens": ["Die", "An\u00b7furth", "dei\u00b7nes", "Geists", "aus", "die\u00b7ser", "Welt\u00b7Ge\u00b7wir\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Sey von geraumer Zeit das s\u00fcsse Hirten-Wort:", "tokens": ["Sey", "von", "ge\u00b7ra