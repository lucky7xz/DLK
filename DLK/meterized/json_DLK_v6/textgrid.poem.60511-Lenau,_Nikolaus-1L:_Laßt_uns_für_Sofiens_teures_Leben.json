{"textgrid.poem.60511": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: La\u00dft uns f\u00fcr Sofiens teures Leben", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00dft uns f\u00fcr Sofiens teures Leben", "tokens": ["La\u00dft", "uns", "f\u00fcr", "So\u00b7fi\u00b7ens", "teu\u00b7res", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Herz und Glas in alle H\u00f6h erheben!", "tokens": ["Herz", "und", "Glas", "in", "al\u00b7le", "H\u00f6h", "er\u00b7he\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ihrem Leben segenfeste Dauer!", "tokens": ["Ih\u00b7rem", "Le\u00b7ben", "se\u00b7gen\u00b7fes\u00b7te", "Dau\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Jede Freude, jedes sch\u00f6ne Hoffen", "tokens": ["Je\u00b7de", "Freu\u00b7de", ",", "je\u00b7des", "sch\u00f6\u00b7ne", "Hof\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Soll ihr p\u00fcnktlich kommen und genauer,", "tokens": ["Soll", "ihr", "p\u00fcnkt\u00b7lich", "kom\u00b7men", "und", "ge\u00b7nau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VVINF", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Als sie selbst von Ischl eingetroffen!", "tokens": ["Als", "sie", "selbst", "von", "Ischl", "ein\u00b7ge\u00b7trof\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NE", "VVPP", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Aber will ein Unfall sie erfassen,", "tokens": ["A\u00b7ber", "will", "ein", "Un\u00b7fall", "sie", "er\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Soll er, wie sie selbst, nur Zeit sich lassen.", "tokens": ["Soll", "er", ",", "wie", "sie", "selbst", ",", "nur", "Zeit", "sich", "las\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "PWAV", "PPER", "ADV", "$,", "ADV", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Und er komme, folgend ihrem Gleise,", "tokens": ["Und", "er", "kom\u00b7me", ",", "fol\u00b7gend", "ih\u00b7rem", "Glei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Stets zu sp\u00e4t um eine Tagesreise!", "tokens": ["Stets", "zu", "sp\u00e4t", "um", "ei\u00b7ne", "Ta\u00b7ges\u00b7rei\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKA", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}