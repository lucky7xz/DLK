{"dta.poem.8298": {"metadata": {"author": {"name": "Milton, John", "birth": "N.A.", "death": "N.A."}, "title": "Das  \n  Verlohrne Paradies.  \n  Neunter Gesang.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1763", "urn": "urn:nbn:de:kobv:b4-200905199734", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht mehr Gespr\u00e4che, wie sonst, da mit dem Menschen vertraulich", "tokens": ["Nicht", "mehr", "Ge\u00b7spr\u00e4\u00b7che", ",", "wie", "sonst", ",", "da", "mit", "dem", "Men\u00b7schen", "ver\u00b7trau\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "PIAT", "NN", "$,", "PWAV", "ADV", "$,", "KOUS", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Gott noch, oder ein englischer Gast, wie ein Freund mit dem Freunde,", "tokens": ["Gott", "noch", ",", "o\u00b7der", "ein", "eng\u00b7li\u00b7scher", "Gast", ",", "wie", "ein", "Freund", "mit", "dem", "Freun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KON", "ART", "ADJA", "NN", "$,", "PWAV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Umgang pflog, und bey ihm sa\u00df, und mit ihm gef\u00e4llig", "tokens": ["Um\u00b7gang", "pflog", ",", "und", "bey", "ihm", "sa\u00df", ",", "und", "mit", "ihm", "ge\u00b7f\u00e4l\u00b7lig"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "KON", "APPR", "PPER", "VVFIN", "$,", "KON", "APPR", "PPER", "ADJD"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Eine l\u00e4ndliche Mahlzeit hielt, wobey ihm verg\u00f6nnt war,", "tokens": ["Ei\u00b7ne", "l\u00e4nd\u00b7li\u00b7che", "Mahl\u00b7zeit", "hielt", ",", "wo\u00b7bey", "ihm", "ver\u00b7g\u00f6nnt", "war", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.5": {"text": "Diese T\u00f6ne nunmehr ver\u00e4ndern. Von Seiten des Menschen", "tokens": ["Die\u00b7se", "T\u00f6\u00b7ne", "nun\u00b7mehr", "ver\u00b7\u00e4n\u00b7dern", ".", "Von", "Sei\u00b7ten", "des", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "ADV", "VVINF", "$.", "APPR", "NN", "ART", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Zeigt sich schn\u00f6der Verrath, und Treubruch, Emp\u00f6rung und Mistraun;", "tokens": ["Zeigt", "sich", "schn\u00f6\u00b7der", "Ver\u00b7rath", ",", "und", "Treu\u00b7bruch", ",", "Em\u00b7p\u00f6\u00b7rung", "und", "Mis\u00b7traun", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJA", "NN", "$,", "KON", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+--+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Und von Seiten des Himmels, der ihn verlassen, Entfernung,", "tokens": ["Und", "von", "Sei\u00b7ten", "des", "Him\u00b7mels", ",", "der", "ihn", "ver\u00b7las\u00b7sen", ",", "Ent\u00b7fer\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Zorn, und wohlverdienter Verweis, und das Urtheil des Todes,", "tokens": ["Zorn", ",", "und", "wohl\u00b7ver\u00b7dien\u00b7ter", "Ver\u00b7weis", ",", "und", "das", "Ur\u00b7theil", "des", "To\u00b7des", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ADJA", "NN", "$,", "KON", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.9": {"text": "Jhren Schatten, den Tod, und das Elend, des Todes Begleiter.", "tokens": ["Ih\u00b7ren", "Schat\u00b7ten", ",", "den", "Tod", ",", "und", "das", "E\u00b7lend", ",", "des", "To\u00b7des", "Be\u00b7glei\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "$,", "KON", "ART", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Ein zwar trauriges Werk, doch nicht minder, ja mehr noch heroisch ", "tokens": ["Ein", "zwar", "trau\u00b7ri\u00b7ges", "Werk", ",", "doch", "nicht", "min\u00b7der", ",", "ja", "mehr", "noch", "he\u00b7ro\u00b7isch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN", "$,", "ADV", "PTKNEG", "ADV", "$,", "ADV", "ADV", "ADV", "ADJD"], "meter": "+-+--+--+-+-+--+", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "Als der Zorn des harten ", "tokens": ["Als", "der", "Zorn", "des", "har\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Seinen fliehenden Feind um ", "tokens": ["Sei\u00b7nen", "flie\u00b7hen\u00b7den", "Feind", "um"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "APPR"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.13": {"text": "Jhm geraubte Verlobte, ", "tokens": ["Jhm", "ge\u00b7raub\u00b7te", "Ver\u00b7lob\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.14": {"text": "Vom ", "tokens": ["Vom"], "token_info": ["word"], "pos": ["APPRART"], "meter": "+", "measure": "single.up"}, "line.15": {"text": "Und der ", "tokens": ["Und", "der"], "token_info": ["word", "word"], "pos": ["KON", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "Wenn nur meinen Gesang durch wahren erhabenen Ausdruck", "tokens": ["Wenn", "nur", "mei\u00b7nen", "Ge\u00b7sang", "durch", "wah\u00b7ren", "er\u00b7ha\u00b7be\u00b7nen", "Aus\u00b7druck"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "APPR", "ADJA", "ADJA", "NN"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.17": {"text": "Mich von selbst zu besuchen des Nachts; und die mir im Schlummer", "tokens": ["Mich", "von", "selbst", "zu", "be\u00b7su\u00b7chen", "des", "Nachts", ";", "und", "die", "mir", "im", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ADV", "PTKZU", "VVINF", "ART", "ADV", "$.", "KON", "PRELS", "PPER", "APPRART", "NN"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Meine Gedanken begeistert, und, ohne da\u00df ich drauf sinne,", "tokens": ["Mei\u00b7ne", "Ge\u00b7dan\u00b7ken", "be\u00b7geis\u00b7tert", ",", "und", ",", "oh\u00b7ne", "da\u00df", "ich", "drauf", "sin\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,", "KON", "$,", "KOUI", "KOUS", "PPER", "PAV", "VVFIN", "$,"], "meter": "+--+--+--+--+-+-", "measure": "dactylic.tetra.plus"}, "line.19": {"text": "Selbst den flie\u00dfenden Vers in meiner Entz\u00fcckung mir vorsagt,", "tokens": ["Selbst", "den", "flie\u00b7\u00dfen\u00b7den", "Vers", "in", "mei\u00b7ner", "Ent\u00b7z\u00fc\u00b7ckung", "mir", "vor\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "+-+--+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.20": {"text": "Seitdem, da mir zuerst zu einem heroischen Liede", "tokens": ["Seit\u00b7dem", ",", "da", "mir", "zu\u00b7erst", "zu", "ei\u00b7nem", "he\u00b7ro\u00b7i\u00b7schen", "Lie\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "$,", "KOUS", "PPER", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Und sp\u00e4t anhub; indem von Natur mein Geist nicht geneigt ist,", "tokens": ["Und", "sp\u00e4t", "an\u00b7hub", ";", "in\u00b7dem", "von", "Na\u00b7tur", "mein", "Geist", "nicht", "ge\u00b7neigt", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$.", "KOUS", "APPR", "NN", "PPOSAT", "NN", "PTKNEG", "VVPP", "VAFIN", "$,"], "meter": "---+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Blutige Schlachten und Kriege zu singen, den einzigen Stoff nur,", "tokens": ["Blu\u00b7ti\u00b7ge", "Schlach\u00b7ten", "und", "Krie\u00b7ge", "zu", "sin\u00b7gen", ",", "den", "ein\u00b7zi\u00b7gen", "Stoff", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "PTKZU", "VVINF", "$,", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+--+--+--+---+-+-", "measure": "dactylic.tetra.plus"}, "line.23": {"text": "Den man bisher f\u00fcr heroisch erkl\u00e4rt; wenn etwan sich k\u00fcnstlich", "tokens": ["Den", "man", "bis\u00b7her", "f\u00fcr", "he\u00b7ro\u00b7isch", "er\u00b7kl\u00e4rt", ";", "wenn", "et\u00b7wan", "sich", "k\u00fcnst\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PIS", "ADV", "APPR", "ADJD", "VVPP", "$.", "KOUS", "ADV", "PRF", "ADJD"], "meter": "+-+-+-+--+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.24": {"text": "Mit verdr\u00fc\u00dflichen langen Gefechten die Ritter der Fabel", "tokens": ["Mit", "ver\u00b7dr\u00fc\u00df\u00b7li\u00b7chen", "lan\u00b7gen", "Ge\u00b7fech\u00b7ten", "die", "Rit\u00b7ter", "der", "Fa\u00b7bel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN", "ART", "NN", "ART", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.25": {"text": "Jene h\u00f6here St\u00e4rke der edlern Geduld, und die Thaten", "tokens": ["Je\u00b7ne", "h\u00f6\u00b7he\u00b7re", "St\u00e4r\u00b7ke", "der", "ed\u00b7lern", "Ge\u00b7duld", ",", "und", "die", "Tha\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "ART", "ADJA", "NN", "$,", "KON", "ART", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.26": {"text": "Tapfrer M\u00e4rtyrer nicht befang, und vorzog, Turniere", "tokens": ["Tapf\u00b7rer", "M\u00e4r\u00b7ty\u00b7rer", "nicht", "be\u00b7fang", ",", "und", "vor\u00b7zog", ",", "Tur\u00b7nie\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADJA", "NN", "PTKNEG", "VVFIN", "$,", "KON", "VVFIN", "$,", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.27": {"text": "Zu beschreiben, und Ritterspiel\u2019, und R\u00fcstungen, schimmernd", "tokens": ["Zu", "be\u00b7schrei\u00b7ben", ",", "und", "Rit\u00b7ter\u00b7spiel'", ",", "und", "R\u00fcs\u00b7tun\u00b7gen", ",", "schim\u00b7mernd"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PTKZU", "VVINF", "$,", "KON", "NN", "$,", "KON", "NN", "$,", "ADJD"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.28": {"text": "Von geschlagenem Gold; und blasonnierete Schilde,", "tokens": ["Von", "ge\u00b7schla\u00b7ge\u00b7nem", "Gold", ";", "und", "bla\u00b7son\u00b7nie\u00b7re\u00b7te", "Schil\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "KON", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.29": {"text": "Und von Golddrath gewirkte Schabracken und pr\u00e4chtige Ritter,", "tokens": ["Und", "von", "Gold\u00b7dra\u00b7th", "ge\u00b7wirk\u00b7te", "Scha\u00b7bra\u00b7cken", "und", "pr\u00e4ch\u00b7ti\u00b7ge", "Rit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "--+-+-+-++--+--+-", "measure": "iambic.septa.relaxed"}, "line.30": {"text": "Die mit Lanzen und Schwerdt in offener Rennbahn sich zeigten;", "tokens": ["Die", "mit", "Lan\u00b7zen", "und", "Schwerdt", "in", "of\u00b7fe\u00b7ner", "Renn\u00b7bahn", "sich", "zeig\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "KON", "NN", "APPR", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.31": {"text": "Dann ein pr\u00e4chtiges Mahl im Rittersaale gehalten,", "tokens": ["Dann", "ein", "pr\u00e4ch\u00b7ti\u00b7ges", "Mahl", "im", "Rit\u00b7ter\u00b7saa\u00b7le", "ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.32": {"text": "Wo sie Marschall und Truchse\u00df, und Seneschallen bedienten.", "tokens": ["Wo", "sie", "Mar\u00b7schall", "und", "Truch\u00b7se\u00df", ",", "und", "Se\u00b7ne\u00b7schal\u00b7len", "be\u00b7dien\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "KON", "NN", "$,", "KON", "NN", "VVFIN", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.33": {"text": "Weder dem Manne, noch auch dem Gedicht, den Namen heroisch", "tokens": ["We\u00b7der", "dem", "Man\u00b7ne", ",", "noch", "auch", "dem", "Ge\u00b7dicht", ",", "den", "Na\u00b7men", "he\u00b7ro\u00b7isch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "ADV", "ADV", "ART", "NN", "$,", "ART", "NN", "ADJD"], "meter": "+--+--+--+-+-+-+", "measure": "dactylic.tri.plus"}, "line.34": {"text": "Mitzutheilen vermocht. Hierinnen wenig ge\u00fcbet,", "tokens": ["Mit\u00b7zu\u00b7thei\u00b7len", "ver\u00b7mocht", ".", "Hie\u00b7rin\u00b7nen", "we\u00b7nig", "ge\u00b7\u00fc\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$.", "ADV", "PIS", "VVPP", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.35": {"text": "Ist mir ein h\u00f6herer Stoff zur\u00fcckegeblieben, der hinreicht,", "tokens": ["Ist", "mir", "ein", "h\u00f6\u00b7he\u00b7rer", "Stoff", "zu\u00b7r\u00fc\u00b7ck\u00b7e\u00b7ge\u00b7blie\u00b7ben", ",", "der", "hin\u00b7reicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+--+-+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.36": {"text": "Durch sich selber die\u00df Lied zum Heldenliede zu heben;", "tokens": ["Durch", "sich", "sel\u00b7ber", "die\u00df", "Lied", "zum", "Hel\u00b7den\u00b7lie\u00b7de", "zu", "he\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADV", "PDS", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+-+-+----", "measure": "trochaic.penta.relaxed"}, "line.37": {"text": "Oder die Last der Jahre die aufw\u00e4rtsgerichteten Schwingen", "tokens": ["O\u00b7der", "die", "Last", "der", "Jah\u00b7re", "die", "auf\u00b7w\u00e4rts\u00b7ge\u00b7rich\u00b7te\u00b7ten", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.38": {"text": "Niedergedr\u00fcckt; sie k\u00f6nnten es wohl, k\u00e4m\u2019 alles von mir her,", "tokens": ["Nie\u00b7der\u00b7ge\u00b7dr\u00fcckt", ";", "sie", "k\u00f6nn\u00b7ten", "es", "wohl", ",", "k\u00e4m'", "al\u00b7les", "von", "mir", "her", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "PPER", "VMFIN", "PPER", "ADV", "$,", "VVFIN", "PIS", "APPR", "PPER", "PTKVZ", "$,"], "meter": "---+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.39": {"text": "Und nicht von ihr, die des Nachts zu meinen Ohren es bringet.", "tokens": ["Und", "nicht", "von", "ihr", ",", "die", "des", "Nachts", "zu", "mei\u00b7nen", "Oh\u00b7ren", "es", "brin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "PPER", "$,", "PRELS", "ART", "ADV", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}}}}}