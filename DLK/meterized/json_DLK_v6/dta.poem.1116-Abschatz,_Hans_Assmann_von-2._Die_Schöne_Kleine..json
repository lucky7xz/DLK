{"dta.poem.1116": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "2. Die Sch\u00f6ne Kleine.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Du Mittel-Krei\u00df der Seel/ ein eng-umschr\u00e4ncktes Feld/ ", "tokens": ["Du", "Mit\u00b7tel\u00b7Krei\u00df", "der", "Seel", "/", "ein", "eng\u00b7um\u00b7schr\u00e4n\u00b7ck\u00b7tes", "Feld", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Mir aber ausgesezt zum Ruh-Punct meiner Sinnen/", "tokens": ["Mir", "a\u00b7ber", "aus\u00b7ge\u00b7sezt", "zum", "Ruh\u00b7Punct", "mei\u00b7ner", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "APPRART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die nur auff dich allein die Neigung richten k\u00fcnnen/", "tokens": ["Die", "nur", "auff", "dich", "al\u00b7lein", "die", "Nei\u00b7gung", "rich\u00b7ten", "k\u00fcn\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "ADV", "ART", "NN", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie sich iedweder Strich vom Rand ins Mittel h\u00e4lt.", "tokens": ["Wie", "sich", "ied\u00b7we\u00b7der", "Strich", "vom", "Rand", "ins", "Mit\u00b7tel", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PIAT", "NN", "APPRART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Klein ist der Angel-Stern/ die Richt-Schnur aller Welt;", "tokens": ["Klein", "ist", "der", "An\u00b7gel\u00b7S\u00b7tern", "/", "die", "Richt\u00b7Schnur", "al\u00b7ler", "Welt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$(", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Klein ist des Sch\u00fctzen Ziel/ dadurch er mu\u00df gewinnen;", "tokens": ["Klein", "ist", "des", "Sch\u00fct\u00b7zen", "Ziel", "/", "da\u00b7durch", "er", "mu\u00df", "ge\u00b7win\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "NN", "$(", "PAV", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Klein ist das Bienen-Volck; iedennoch wird man innen/", "tokens": ["Klein", "ist", "das", "Bie\u00b7nen\u00b7Volck", ";", "ie\u00b7den\u00b7noch", "wird", "man", "in\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$.", "ADV", "VAFIN", "PIS", "ADV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie s\u00fc\u00df ihr Honig und wie scharff ihr Stachel f\u00e4llt.", "tokens": ["Wie", "s\u00fc\u00df", "ihr", "Ho\u00b7nig", "und", "wie", "scharff", "ihr", "Sta\u00b7chel", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "KON", "PWAV", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "O Auszug alles gutts/ du bist ja billich klein/", "tokens": ["O", "Aus\u00b7zug", "al\u00b7les", "gutts", "/", "du", "bist", "ja", "bil\u00b7lich", "klein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PIAT", "ADJA", "$(", "PPER", "VAFIN", "ADV", "ADJD", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Weil auch in tieffer See und in der Berge Gr\u00fcnden", "tokens": ["Weil", "auch", "in", "tief\u00b7fer", "See", "und", "in", "der", "Ber\u00b7ge", "Gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ADJA", "NN", "KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Muschel-Kinder zart/ Demanten Zwerge seyn:", "tokens": ["Die", "Mu\u00b7schel\u00b7Kin\u00b7der", "zart", "/", "De\u00b7man\u00b7ten", "Zwer\u00b7ge", "seyn", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$(", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Begriff von aller Lust/ die auff der Welt zu finden/", "tokens": ["Be\u00b7griff", "von", "al\u00b7ler", "Lust", "/", "die", "auff", "der", "Welt", "zu", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$(", "ART", "APPR", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Den Himmels-Bau entwirfft der kleinen Kugel Ri\u00df;", "tokens": ["Den", "Him\u00b7mels\u00b7Bau", "ent\u00b7wirfft", "der", "klei\u00b7nen", "Ku\u00b7gel", "Ri\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Mir ist die kleine Scho\u00df ein irrdisch Paradi\u00df.", "tokens": ["Mir", "ist", "die", "klei\u00b7ne", "Scho\u00df", "ein", "irr\u00b7disch", "Pa\u00b7ra\u00b7di\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}