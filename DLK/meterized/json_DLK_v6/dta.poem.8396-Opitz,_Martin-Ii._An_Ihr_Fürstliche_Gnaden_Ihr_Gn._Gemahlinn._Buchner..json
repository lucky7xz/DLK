{"dta.poem.8396": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Ii.  \n  An Ihr F\u00fcrstliche Gnaden/  \n Ihr Gn. Gemahlinn.  \n Buchner.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1630", "urn": "urn:nbn:de:kobv:b4-200905197863", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Sch\u00e4fferey Von der Nimfen Hercinie. Breslau, 1630."}, "poem": {"stanza.1": {"line.1": {"text": "Solt ich das gro\u00dfe lob/ den K\u00f6niglichen schein/", "tokens": ["Solt", "ich", "das", "gro\u00b7\u00dfe", "lob", "/", "den", "K\u00f6\u00b7nig\u00b7li\u00b7chen", "schein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "$(", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die thaten vndt verdienst/ so von dem werthen stande/", "tokens": ["Die", "tha\u00b7ten", "vndt", "ver\u00b7dienst", "/", "so", "von", "dem", "wert\u00b7hen", "stan\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "$(", "ADV", "APPR", "ART", "ADJA", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der dich erzeuget hatt/ durch alle ferne lande", "tokens": ["Der", "dich", "er\u00b7zeu\u00b7get", "hatt", "/", "durch", "al\u00b7le", "fer\u00b7ne", "lan\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "VAFIN", "$(", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Am liechten tage sindt/ recht preisen k\u00f6nnen? nein;", "tokens": ["Am", "liech\u00b7ten", "ta\u00b7ge", "sindt", "/", "recht", "prei\u00b7sen", "k\u00f6n\u00b7nen", "?", "nein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$(", "ADJD", "VVINF", "VMINF", "$.", "PTKANT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mir sey die faust dann stahl/ die feder demantstein/", "tokens": ["Mir", "sey", "die", "faust", "dann", "stahl", "/", "die", "fe\u00b7der", "de\u00b7mant\u00b7stein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "VVFIN", "$(", "ART", "NN", "ART", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die tinte hergeholt von dem gelehrten strande", "tokens": ["Die", "tin\u00b7te", "her\u00b7ge\u00b7holt", "von", "dem", "ge\u00b7lehr\u00b7ten", "stran\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der beym Parna\u00df entspringt: mein schiff bleibt an dem rande/", "tokens": ["Der", "beym", "Par\u00b7na\u00df", "ent\u00b7springt", ":", "mein", "schiff", "bleibt", "an", "dem", "ran\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVFIN", "$.", "PPOSAT", "ADJD", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vndt le\u00dft sich k\u00fchnlich nicht in solche wellen ein.", "tokens": ["Vndt", "le\u00dft", "sich", "k\u00fchn\u00b7lich", "nicht", "in", "sol\u00b7che", "wel\u00b7len", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "PTKNEG", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wann einer ferner auch die sitten/ den verstandt/", "tokens": ["Wann", "ei\u00b7ner", "fer\u00b7ner", "auch", "die", "sit\u00b7ten", "/", "den", "ver\u00b7standt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADV", "ADV", "ART", "ADJA", "$(", "ART", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die tugendt so du hast/ der edlen gaben pfandt", "tokens": ["Die", "tu\u00b7gendt", "so", "du", "hast", "/", "der", "ed\u00b7len", "ga\u00b7ben", "pfandt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "PPER", "VAFIN", "$(", "ART", "ADJA", "VVFIN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die dir der himmel schenckt/ der gantzen welt wil zeigen/", "tokens": ["Die", "dir", "der", "him\u00b7mel", "schenckt", "/", "der", "gant\u00b7zen", "welt", "wil", "zei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$(", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Mu\u00df h\u00f6her gehn als ich/ wiewol Apollo mir", "tokens": ["Mu\u00df", "h\u00f6\u00b7her", "gehn", "als", "ich", "/", "wie\u00b7wol", "A\u00b7pol\u00b7lo", "mir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "ADJD", "VVINF", "KOKOM", "PPER", "$(", "KOUS", "NE", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mitt milden handen reicht die leyer meine ziehr/", "tokens": ["Mitt", "mil\u00b7den", "han\u00b7den", "reicht", "die", "le\u00b7yer", "mei\u00b7ne", "ziehr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+--++-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Mu\u00df/ heldinn/ \u00fcberau\u00df wol singen oder schweigen.", "tokens": ["Mu\u00df", "/", "hel\u00b7dinn", "/", "\u00fc\u00b7be\u00b7rau\u00df", "wol", "sin\u00b7gen", "o\u00b7der", "schwei\u00b7gen", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$(", "NN", "$(", "ADV", "ADV", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}