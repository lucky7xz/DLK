{"textgrid.poem.56677": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Die Sterne", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn sich die Nacht zaghaft mit euch besteckt,", "tokens": ["Wenn", "sich", "die", "Nacht", "zag\u00b7haft", "mit", "euch", "be\u00b7steckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "wie eine dunkle T\u00e4nzerin den seide-", "tokens": ["wie", "ei\u00b7ne", "dunk\u00b7le", "T\u00e4n\u00b7ze\u00b7rin", "den", "sei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "weichen Leib mit sp\u00e4rlichem Geschmeide,", "tokens": ["wei\u00b7chen", "Leib", "mit", "sp\u00e4r\u00b7li\u00b7chem", "Ge\u00b7schmei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "wenn ihr gleich brennendem Staub den Himmel deckt", "tokens": ["wenn", "ihr", "gleich", "bren\u00b7nen\u00b7dem", "Staub", "den", "Him\u00b7mel", "deckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "und leuchtend in das Nichts hin\u00fcberleckt,", "tokens": ["und", "leuch\u00b7tend", "in", "das", "Nichts", "hin\u00b7\u00fc\u00b7ber\u00b7leckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "fliegt wohl von dieser d\u00fcrren L\u00e4mmerheide", "tokens": ["fliegt", "wohl", "von", "die\u00b7ser", "d\u00fcr\u00b7ren", "L\u00e4m\u00b7mer\u00b7hei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und abgegrasten Tr\u00fcbsalsrinderweide", "tokens": ["und", "ab\u00b7ge\u00b7gras\u00b7ten", "Tr\u00fcb\u00b7sals\u00b7rin\u00b7der\u00b7wei\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "die Seele lechzend zu euch hoch und reckt", "tokens": ["die", "See\u00b7le", "lech\u00b7zend", "zu", "euch", "hoch", "und", "reckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "APPR", "PPER", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "der Sehnsucht Fackel hoch in euch empor,", "tokens": ["der", "Sehn\u00b7sucht", "Fa\u00b7ckel", "hoch", "in", "euch", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "bis sie vom Weine der Unendlichkeiten", "tokens": ["bis", "sie", "vom", "Wei\u00b7ne", "der", "Un\u00b7end\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "trunken taumelt und ein wirrer Flor", "tokens": ["trun\u00b7ken", "tau\u00b7melt", "und", "ein", "wir\u00b7rer", "Flor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "KON", "ART", "ADJA", "NN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "sich um die Sinne legt \u2013: aus euren Weiten,", "tokens": ["sich", "um", "die", "Sin\u00b7ne", "legt", "\u2013", ":", "aus", "eu\u00b7ren", "Wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN", "$(", "$.", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die ewig grenzenlos ich hochbeschwor,", "tokens": ["die", "e\u00b7wig", "gren\u00b7zen\u00b7los", "ich", "hoch\u00b7be\u00b7schwor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "fall ich zur\u00fcck in Staub und Sterblichkeiten.", "tokens": ["fall", "ich", "zu\u00b7r\u00fcck", "in", "Staub", "und", "Sterb\u00b7lich\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKVZ", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}