{"textgrid.poem.63351": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Die Ballade des Vergessens", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In den L\u00fcften schreien die Geier schon,", "tokens": ["In", "den", "L\u00fcf\u00b7ten", "schrei\u00b7en", "die", "Gei\u00b7er", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "L\u00fcstern nach neuem Aase.", "tokens": ["L\u00fcs\u00b7tern", "nach", "neu\u00b7em", "Aa\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Es hebt so mancher die Leier schon", "tokens": ["Es", "hebt", "so", "man\u00b7cher", "die", "Lei\u00b7er", "schon"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "ART", "NN", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Beim freibiergef\u00fcllten Glase,", "tokens": ["Beim", "frei\u00b7bier\u00b7ge\u00b7f\u00fcll\u00b7ten", "Gla\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Zu schlagen siegreich den alten b\u00f6sen Feind,", "tokens": ["Zu", "schla\u00b7gen", "sieg\u00b7reich", "den", "al\u00b7ten", "b\u00f6\u00b7sen", "Feind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PRF", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "T\u00e4t er den Humpen pressen...", "tokens": ["T\u00e4t", "er", "den", "Hum\u00b7pen", "pres\u00b7sen", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Habt ihr die Tr\u00e4nen, die ihr geweint,", "tokens": ["Habt", "ihr", "die", "Tr\u00e4\u00b7nen", ",", "die", "ihr", "ge\u00b7weint", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Vergessen, vergessen, vergessen?", "tokens": ["Ver\u00b7ges\u00b7sen", ",", "ver\u00b7ges\u00b7sen", ",", "ver\u00b7ges\u00b7sen", "?"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$,", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Habt ihr vergessen, was man euch tat,", "tokens": ["Habt", "ihr", "ver\u00b7ges\u00b7sen", ",", "was", "man", "euch", "tat", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "PRELS", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Des Mordes Dengeln und M\u00e4hen?", "tokens": ["Des", "Mor\u00b7des", "Den\u00b7geln", "und", "M\u00e4\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es l\u00e4\u00dft sich bei Gott der Geschichte Rad,", "tokens": ["Es", "l\u00e4\u00dft", "sich", "bei", "Gott", "der", "Ge\u00b7schich\u00b7te", "Rad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "ART", "NN", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Beim Teufel nicht r\u00fcckw\u00e4rts drehen.", "tokens": ["Beim", "Teu\u00b7fel", "nicht", "r\u00fcck\u00b7w\u00e4rts", "dre\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Der Feldherr, der Krieg und Nerven verlor,", "tokens": ["Der", "Feld\u00b7herr", ",", "der", "Krieg", "und", "Ner\u00b7ven", "ver\u00b7lor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Er tr\u00e4gt noch immer die Tressen.", "tokens": ["Er", "tr\u00e4gt", "noch", "im\u00b7mer", "die", "Tres\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Seine Niederlage erstrahlt in Glor", "tokens": ["Sei\u00b7ne", "Nie\u00b7der\u00b7la\u00b7ge", "er\u00b7strahlt", "in", "Glor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Und Glanz: Ihr habt sie vergessen.", "tokens": ["Und", "Glanz", ":", "Ihr", "habt", "sie", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$.", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Verga\u00dft ihr die gute alte Zeit,", "tokens": ["Ver\u00b7ga\u00dft", "ihr", "die", "gu\u00b7te", "al\u00b7te", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die schlechteste je im Lande?", "tokens": ["Die", "schlech\u00b7tes\u00b7te", "je", "im", "Lan\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Euer Herrscher hie\u00df Narr, seine Tochter Leid,", "tokens": ["Eu\u00b7er", "Herr\u00b7scher", "hie\u00df", "Narr", ",", "sei\u00b7ne", "Toch\u00b7ter", "Leid", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NE", "$,", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Die Hofherren Feigheit und Schande.", "tokens": ["Die", "Hof\u00b7her\u00b7ren", "Feig\u00b7heit", "und", "Schan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Er f\u00fchrte euch in den Untergang", "tokens": ["Er", "f\u00fchr\u00b7te", "euch", "in", "den", "Un\u00b7ter\u00b7gang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Mit heitern Mienen, mit kessen.", "tokens": ["Mit", "hei\u00b7tern", "Mie\u00b7nen", ",", "mit", "kes\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "VVINF", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.7": {"text": "L\u00e4ngst habt ihr's bei Wein, Weib und Gesang", "tokens": ["L\u00e4ngst", "habt", "ih\u00b7r's", "bei", "Wein", ",", "Weib", "und", "Ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Vergessen, vergessen, vergessen.", "tokens": ["Ver\u00b7ges\u00b7sen", ",", "ver\u00b7ges\u00b7sen", ",", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$,", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Wir haben Gott und Vaterland", "tokens": ["Wir", "ha\u00b7ben", "Gott", "und", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit geifernden M\u00e4ulern gesch\u00e4ndet,", "tokens": ["Mit", "gei\u00b7fern\u00b7den", "M\u00e4u\u00b7lern", "ge\u00b7sch\u00e4n\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wir haben mit unsrer dreckigen Hand", "tokens": ["Wir", "ha\u00b7ben", "mit", "uns\u00b7rer", "dre\u00b7cki\u00b7gen", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-++-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Hemd und Meinung gewendet.", "tokens": ["Hemd", "und", "Mei\u00b7nung", "ge\u00b7wen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Es galt kein Wort mehr ehrlich und klar,", "tokens": ["Es", "galt", "kein", "Wort", "mehr", "ehr\u00b7lich", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Nur L\u00fcgen unermessen...", "tokens": ["Nur", "L\u00fc\u00b7gen", "un\u00b7er\u00b7mes\u00b7sen", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wir hatten die Wahrheit so ganz und gar", "tokens": ["Wir", "hat\u00b7ten", "die", "Wahr\u00b7heit", "so", "ganz", "und", "gar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ADV", "KON", "ADV"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Vergessen, vergessen, vergessen.", "tokens": ["Ver\u00b7ges\u00b7sen", ",", "ver\u00b7ges\u00b7sen", ",", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$,", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Millionen krepierten in diesem Krieg,", "tokens": ["Mil\u00b7lion\u00b7en", "kre\u00b7pier\u00b7ten", "in", "die\u00b7sem", "Krieg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Den nur ein paar Dutzend gewannen.", "tokens": ["Den", "nur", "ein", "paar", "Dut\u00b7zend", "ge\u00b7wan\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Sie schlichen nach ihrem teuflischen Sieg", "tokens": ["Sie", "schli\u00b7chen", "nach", "ih\u00b7rem", "teuf\u00b7li\u00b7schen", "Sieg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit vollen S\u00e4cken von dannen.", "tokens": ["Mit", "vol\u00b7len", "S\u00e4\u00b7cken", "von", "dan\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADV", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Im Hauptquartier bei Wein und Sekt", "tokens": ["Im", "Haupt\u00b7quar\u00b7tier", "bei", "Wein", "und", "Sekt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "T\u00e4t mancher sein Liebchen pressen.", "tokens": ["T\u00e4t", "man\u00b7cher", "sein", "Lieb\u00b7chen", "pres\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "An der Front lag der Kerl, verlaust und verdreckt", "tokens": ["An", "der", "Front", "lag", "der", "Kerl", ",", "ver\u00b7laust", "und", "ver\u00b7dreckt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,", "VVFIN", "KON", "VVFIN"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Und vergessen, vergessen, vergessen.", "tokens": ["Und", "ver\u00b7ges\u00b7sen", ",", "ver\u00b7ges\u00b7sen", ",", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVPP", "$,", "VVPP", "$,", "VVPP", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.6": {"line.1": {"text": "Es bl\u00fchte noch nach dem Kriege der Mord,", "tokens": ["Es", "bl\u00fch\u00b7te", "noch", "nach", "dem", "Krie\u00b7ge", "der", "Mord", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es war eine Lust, zu knallen.", "tokens": ["Es", "war", "ei\u00b7ne", "Lust", ",", "zu", "knal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es zeigte in diesem traurigen Sport", "tokens": ["Es", "zeig\u00b7te", "in", "die\u00b7sem", "trau\u00b7ri\u00b7gen", "Sport"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sich Deutschland \u00fcber Allen.", "tokens": ["Sich", "Deutschland", "\u00fc\u00b7ber", "Al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "NE", "APPR", "NE", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ein jeder Schurke hielt Gericht,", "tokens": ["Ein", "je\u00b7der", "Schur\u00b7ke", "hielt", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Erde mit Blut zu n\u00e4ssen.", "tokens": ["Die", "Er\u00b7de", "mit", "Blut", "zu", "n\u00e4s\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Deutschland, du sollst die Ermordeten nicht", "tokens": ["Deutschland", ",", "du", "sollst", "die", "Er\u00b7mor\u00b7de\u00b7ten", "nicht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos"