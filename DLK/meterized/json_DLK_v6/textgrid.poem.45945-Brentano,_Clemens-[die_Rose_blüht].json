{"textgrid.poem.45945": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[die Rose bl\u00fcht]", "genre": "verse", "period": "N.A.", "pub_year": 1811, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Rose bl\u00fcht; schlo\u00df gleich ein rauher Wind,", "tokens": ["Die", "Ro\u00b7se", "bl\u00fcht", ";", "schlo\u00df", "gleich", "ein", "rau\u00b7her", "Wind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als sie der goldnen Imme sich erschlossen,", "tokens": ["Als", "sie", "der", "gold\u00b7nen", "Im\u00b7me", "sich", "er\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Liebe arglos offnen Kelch geschwind,", "tokens": ["Der", "Lie\u00b7be", "arg\u00b7los", "off\u00b7nen", "Kelch", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hat doch der Ha\u00df nicht Gift hineingegossen!", "tokens": ["Hat", "doch", "der", "Ha\u00df", "nicht", "Gift", "hin\u00b7ein\u00b7ge\u00b7gos\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PTKNEG", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sie schlo\u00df gleich einem bangen, zarten Kind", "tokens": ["Sie", "schlo\u00df", "gleich", "ei\u00b7nem", "ban\u00b7gen", ",", "zar\u00b7ten", "Kind"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Augen, bis die Zornflut abgeflossen;", "tokens": ["Die", "Au\u00b7gen", ",", "bis", "die", "Zorn\u00b7flut", "ab\u00b7ge\u00b7flos\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Vielleicht schlo\u00df sie in br\u00fcnstigem Verlangen", "tokens": ["Viel\u00b7leicht", "schlo\u00df", "sie", "in", "br\u00fcns\u00b7ti\u00b7gem", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Sich nur so schnell, die Biene einzufangen?", "tokens": ["Sich", "nur", "so", "schnell", ",", "die", "Bie\u00b7ne", "ein\u00b7zu\u00b7fan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "ADJD", "$,", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Rose bl\u00fcht, die Biene ist entflohn,", "tokens": ["Die", "Ro\u00b7se", "bl\u00fcht", ",", "die", "Bie\u00b7ne", "ist", "ent\u00b7flohn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aufs neue mu\u00df sie mit den Fr\u00fchlingsglocken", "tokens": ["Aufs", "neu\u00b7e", "mu\u00df", "sie", "mit", "den", "Fr\u00fch\u00b7lings\u00b7glo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Zornes Stachel f\u00fchrnden, goldnen Sohn", "tokens": ["Des", "Zor\u00b7nes", "Sta\u00b7chel", "f\u00fchrn\u00b7den", ",", "gold\u00b7nen", "Sohn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In ihres Duftes keuschen Busen locken;", "tokens": ["In", "ih\u00b7res", "Duf\u00b7tes", "keu\u00b7schen", "Bu\u00b7sen", "lo\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ihr s\u00fc\u00df'ster Tau, kehrt er, wird ihm zum Lohn;", "tokens": ["Ihr", "s\u00fc\u00df'\u00b7ster", "Tau", ",", "kehrt", "er", ",", "wird", "ihm", "zum", "Lohn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN", "PPER", "$,", "VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "O kehr, mein Bienlein, sei nicht so erschrocken!", "tokens": ["O", "kehr", ",", "mein", "Bien\u00b7lein", ",", "sei", "nicht", "so", "er\u00b7schro\u00b7cken", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PPOSAT", "NN", "$,", "VAFIN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Zum Garten sieht mein Fenster, dorten wohn' ich.", "tokens": ["Zum", "Gar\u00b7ten", "sieht", "mein", "Fens\u00b7ter", ",", "dor\u00b7ten", "wohn'", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Komm, liebe Imme, sammle Wachs und Honig!", "tokens": ["Komm", ",", "lie\u00b7be", "Im\u00b7me", ",", "samm\u00b7le", "Wachs", "und", "Ho\u00b7nig", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Rose bl\u00fcht; wenn alle V\u00f6glein schlafen,", "tokens": ["Die", "Ro\u00b7se", "bl\u00fcht", ";", "wenn", "al\u00b7le", "V\u00f6\u00b7glein", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "KOUS", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn nieder hintern Wald die Sonne flieht,", "tokens": ["Wenn", "nie\u00b7der", "hin\u00b7tern", "Wald", "die", "Son\u00b7ne", "flieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKVZ", "VVFIN", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn treu der Mond mit seinen Wolkenschafen", "tokens": ["Wenn", "treu", "der", "Mond", "mit", "sei\u00b7nen", "Wol\u00b7ken\u00b7scha\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An deiner Rose Stand vor\u00fcberzieht,", "tokens": ["An", "dei\u00b7ner", "Ro\u00b7se", "Stand", "vor\u00b7\u00fc\u00b7berz\u00b7ieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zur Stunde, als Imeldens T\u00f6ne trafen", "tokens": ["Zur", "Stun\u00b7de", ",", "als", "I\u00b7mel\u00b7dens", "T\u00f6\u00b7ne", "tra\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "KOUS", "NN", "NN", "VVINF"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Ein liebes Herz durch ein unschuldig Lied,", "tokens": ["Ein", "lie\u00b7bes", "Herz", "durch", "ein", "un\u00b7schul\u00b7dig", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da will am Fenster nieder zu dem Garten", "tokens": ["Da", "will", "am", "Fens\u00b7ter", "nie\u00b7der", "zu", "dem", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPRART", "NN", "PTKVZ", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Rose auf die fromme Biene warten.", "tokens": ["Die", "Ro\u00b7se", "auf", "die", "from\u00b7me", "Bie\u00b7ne", "war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Rose bl\u00fcht; o fliehe, Licht der Sonnen,", "tokens": ["Die", "Ro\u00b7se", "bl\u00fcht", ";", "o", "flie\u00b7he", ",", "Licht", "der", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "FM", "VVFIN", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O f\u00fchre, Mond, die Sternenherde bald", "tokens": ["O", "f\u00fch\u00b7re", ",", "Mond", ",", "die", "Ster\u00b7nen\u00b7her\u00b7de", "bald"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "NN", "$,", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zum stillen, vollen, goldnen Mondesbronnen,", "tokens": ["Zum", "stil\u00b7len", ",", "vol\u00b7len", ",", "gold\u00b7nen", "Mon\u00b7des\u00b7bron\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Streu aus den sichren Schatten, dunkler Wald,", "tokens": ["Streu", "aus", "den", "sich\u00b7ren", "Schat\u00b7ten", ",", "dunk\u00b7ler", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und bleiche, Mond, was Liebe hat gesponnen!", "tokens": ["Und", "blei\u00b7che", ",", "Mond", ",", "was", "Lie\u00b7be", "hat", "ge\u00b7spon\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "NN", "$,", "PWS", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Doch mit Musik, die anderswo erschallt,", "tokens": ["Doch", "mit", "Mu\u00b7sik", ",", "die", "an\u00b7ders\u00b7wo", "er\u00b7schallt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.7": {"text": "Mag Amor all die Schmetterlinge irren,", "tokens": ["Mag", "A\u00b7mor", "all", "die", "Schmet\u00b7ter\u00b7lin\u00b7ge", "ir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "PIAT", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die lauschend gern die Rose dir umschwirren.", "tokens": ["Die", "lau\u00b7schend", "gern", "die", "Ro\u00b7se", "dir", "um\u00b7schwir\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Die Rose bl\u00fcht, der Zorn ist voll Verderben!", "tokens": ["Die", "Ro\u00b7se", "bl\u00fcht", ",", "der", "Zorn", "ist", "voll", "Ver\u00b7der\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VAFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wer, Zorn, ger\u00e4t in deine finstre Haft,", "tokens": ["Wer", ",", "Zorn", ",", "ge\u00b7r\u00e4t", "in", "dei\u00b7ne", "finst\u00b7re", "Haft", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "NN", "$,", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der mordet, martert, t\u00f6tet ohne Sterben", "tokens": ["Der", "mor\u00b7det", ",", "mar\u00b7tert", ",", "t\u00f6\u00b7tet", "oh\u00b7ne", "Ster\u00b7ben"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und hat der bittren H\u00f6lle Eigenschaft!", "tokens": ["Und", "hat", "der", "bit\u00b7tren", "H\u00f6l\u00b7le", "Ei\u00b7gen\u00b7schaft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "O Liebe, wer die Einsicht d\u00fcrft' erwerben", "tokens": ["O", "Lie\u00b7be", ",", "wer", "die", "Ein\u00b7sicht", "d\u00fcrft'", "er\u00b7wer\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PWS", "ART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Von deiner Gottestiefe Wunderkraft!", "tokens": ["Von", "dei\u00b7ner", "Got\u00b7tes\u00b7tie\u00b7fe", "Wun\u00b7der\u00b7kraft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "O Liebe, wer, ein Tr\u00f6pflein, sich verl\u00f6re", "tokens": ["O", "Lie\u00b7be", ",", "wer", ",", "ein", "Tr\u00f6pf\u00b7lein", ",", "sich", "ver\u00b7l\u00f6\u00b7re"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$,", "PWS", "$,", "ART", "NN", "$,", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "In deines Segens weltumspielndem Meere!", "tokens": ["In", "dei\u00b7nes", "Se\u00b7gens", "welt\u00b7um\u00b7spieln\u00b7dem", "Mee\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PWAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}