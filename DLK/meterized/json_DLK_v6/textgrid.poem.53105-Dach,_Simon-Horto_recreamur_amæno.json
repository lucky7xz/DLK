{"textgrid.poem.53105": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Horto recreamur am\u00e6no", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An diesem Orth alhie", "tokens": ["An", "die\u00b7sem", "Orth", "al\u00b7hie"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wil ich mich aller M\u00fch'", "tokens": ["Wil", "ich", "mich", "al\u00b7ler", "M\u00fch'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PRF", "PIAT", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Vnd Trawrigkeit entschlagen,", "tokens": ["Vnd", "Traw\u00b7rig\u00b7keit", "ent\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd was hieher erspahrt,", "tokens": ["Vnd", "was", "hie\u00b7her", "er\u00b7spahrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PAV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nach Liedern bester Arth", "tokens": ["Nach", "Lie\u00b7dern", "bes\u00b7ter", "Ar\u00b7th"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Instendig fragen.", "tokens": ["Ins\u00b7ten\u00b7dig", "fra\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Herr Bruder, Orpheus Kind,", "tokens": ["Herr", "Bru\u00b7der", ",", "Or\u00b7pheus", "Kind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "NE", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Hebt an, mit mir beginnt", "tokens": ["Hebt", "an", ",", "mit", "mir", "be\u00b7ginnt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Lied so vns ergetze!", "tokens": ["Ein", "Lied", "so", "vns", "er\u00b7get\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In was f\u00fcr Noht vnd Pein", "tokens": ["In", "was", "f\u00fcr", "Noht", "vnd", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der falschen Liebes-Schein", "tokens": ["Der", "fal\u00b7schen", "Lie\u00b7bes\u00b7Schein"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Vns Menschen setze,", "tokens": ["Vns", "Men\u00b7schen", "set\u00b7ze", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Wie Sylvius der Hirt", "tokens": ["Wie", "Syl\u00b7vius", "der", "Hirt"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NE", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "So sehr geplaget wird,", "tokens": ["So", "sehr", "ge\u00b7pla\u00b7get", "wird", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn Phyllis jhn verachtet,", "tokens": ["Wenn", "Phyl\u00b7lis", "jhn", "ver\u00b7ach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd nach wild-frembder Gunst", "tokens": ["Vnd", "nach", "wild\u00b7fremb\u00b7der", "Gunst"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Au\u00df leicht-gesinnter Brunst", "tokens": ["Au\u00df", "leicht\u00b7ge\u00b7sinn\u00b7ter", "Brunst"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Begierig trachtet.", "tokens": ["Be\u00b7gie\u00b7rig", "trach\u00b7tet", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Ich bin mein Bawer-Lied", "tokens": ["Ich", "bin", "mein", "Ba\u00b7wer\u00b7Lied"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nach ewrem bald bem\u00fcht", "tokens": ["Nach", "ew\u00b7rem", "bald", "be\u00b7m\u00fcht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Au\u00df kurtzweil anzuheben;", "tokens": ["Au\u00df", "kurt\u00b7zweil", "an\u00b7zu\u00b7he\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn die\u00df zu End gebracht,", "tokens": ["Wenn", "die\u00df", "zu", "End", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So sing' ich: Gute Nacht", "tokens": ["So", "sing'", "ich", ":", "Gu\u00b7te", "Nacht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Du falsches Leben!", "tokens": ["Du", "fal\u00b7sches", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Die\u00df wil der B\u00e4ume Zier", "tokens": ["Die\u00df", "wil", "der", "B\u00e4u\u00b7me", "Zier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Vnd dieses gute Bier,", "tokens": ["Vnd", "die\u00b7ses", "gu\u00b7te", "Bier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die\u00df wil der Garte wissen;", "tokens": ["Die\u00df", "wil", "der", "Gar\u00b7te", "wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die\u00df W\u00fcnscht die kleine Bach,", "tokens": ["Die\u00df", "W\u00fcnscht", "die", "klei\u00b7ne", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Indem sie nach vnd nach", "tokens": ["In\u00b7dem", "sie", "nach", "vnd", "nach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "KON", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Geht vor sich fliessen.", "tokens": ["Geht", "vor", "sich", "flies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Die Zeit vnd wir vergehn;", "tokens": ["Die", "Zeit", "vnd", "wir", "ver\u00b7gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Was wir hie sehen stehn", "tokens": ["Was", "wir", "hie", "se\u00b7hen", "stehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "VVINF", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In diesem sch\u00f6nen Garten,", "tokens": ["In", "die\u00b7sem", "sch\u00f6\u00b7nen", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verwelckt in kurtzer Zeit,", "tokens": ["Ver\u00b7welckt", "in", "kurt\u00b7zer", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Weil schon de\u00df Herbstes Neid", "tokens": ["Weil", "schon", "de\u00df", "Herbs\u00b7tes", "Neid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Scheint drauff zu warten.", "tokens": ["Scheint", "drauff", "zu", "war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}