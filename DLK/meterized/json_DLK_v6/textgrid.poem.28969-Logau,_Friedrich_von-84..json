{"textgrid.poem.28969": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "84.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer die Zeit verklagen wil, da\u00df so zeitlich sie verraucht,", "tokens": ["Wer", "die", "Zeit", "ver\u00b7kla\u00b7gen", "wil", ",", "da\u00df", "so", "zeit\u00b7lich", "sie", "ver\u00b7raucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVINF", "VMFIN", "$,", "KOUS", "ADV", "ADJD", "PPER", "VVPP", "$,"], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der verklage sich nur selbst, da\u00df er sie nicht zeitlich braucht.", "tokens": ["Der", "ver\u00b7kla\u00b7ge", "sich", "nur", "selbst", ",", "da\u00df", "er", "sie", "nicht", "zeit\u00b7lich", "braucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "ADV", "$,", "KOUS", "PPER", "PPER", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "--+---+--+-+-+", "measure": "iambic.penta.relaxed"}}}}}