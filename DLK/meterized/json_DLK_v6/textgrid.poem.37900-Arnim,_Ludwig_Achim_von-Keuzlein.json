{"textgrid.poem.37900": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Keuzlein", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich armes Keuzlein kleine,", "tokens": ["Ich", "ar\u00b7mes", "Keuz\u00b7lein", "klei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo soll ich fliegen aus,", "tokens": ["Wo", "soll", "ich", "flie\u00b7gen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Bey Nacht so gar alleine,", "tokens": ["Bey", "Nacht", "so", "gar", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bringt mir so manchen Graus:", "tokens": ["Bringt", "mir", "so", "man\u00b7chen", "Graus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das macht der Eulen Ungestalt,", "tokens": ["Das", "macht", "der", "Eu\u00b7len", "Un\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr Trauern mannigfalt.", "tokens": ["Ihr", "Trau\u00b7ern", "man\u00b7nig\u00b7falt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich wills Gefieder schwingen", "tokens": ["Ich", "wills", "Ge\u00b7fie\u00b7der", "schwin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gen Holz in gr\u00fcnen Wald,", "tokens": ["Gen", "Holz", "in", "gr\u00fc\u00b7nen", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die V\u00f6glein h\u00f6ren singen", "tokens": ["Die", "V\u00f6\u00b7glein", "h\u00f6\u00b7ren", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In mancherley Gestalt.", "tokens": ["In", "man\u00b7cher\u00b7ley", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vor allen lieb ich Nachtigal,", "tokens": ["Vor", "al\u00b7len", "lieb", "ich", "Nach\u00b7ti\u00b7gal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJD", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vor allen liebt mich Nachtigal.", "tokens": ["Vor", "al\u00b7len", "liebt", "mich", "Nach\u00b7ti\u00b7gal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Kinder unten glauben,", "tokens": ["Die", "Kin\u00b7der", "un\u00b7ten", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich deute B\u00f6ses an,", "tokens": ["Ich", "deu\u00b7te", "B\u00f6\u00b7ses", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie wollen mich vertreiben,", "tokens": ["Sie", "wol\u00b7len", "mich", "ver\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df ich nicht schreien kann:", "tokens": ["Da\u00df", "ich", "nicht", "schrei\u00b7en", "kann", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn ich was deute, thut mir's leid,", "tokens": ["Wenn", "ich", "was", "deu\u00b7te", ",", "thut", "mir's", "leid", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVFIN", "$,", "VVFIN", "NE", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und was ich schrei, ist keine Freud.", "tokens": ["Und", "was", "ich", "schrei", ",", "ist", "kei\u00b7ne", "Freud", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADJD", "$,", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mein Ast ist mir entwichen,", "tokens": ["Mein", "Ast", "ist", "mir", "ent\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Darauf ich ruhen sollt,", "tokens": ["Da\u00b7rauf", "ich", "ru\u00b7hen", "sollt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein Bl\u00e4ttlein all verblichen,", "tokens": ["Sein", "Bl\u00e4tt\u00b7lein", "all", "ver\u00b7bli\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Frau Nachtigal geholt:", "tokens": ["Frau", "Nach\u00b7ti\u00b7gal", "ge\u00b7holt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das schafft der Eulen falsche T\u00fcck,", "tokens": ["Das", "schafft", "der", "Eu\u00b7len", "fal\u00b7sche", "T\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die st\u00f6ret all mein Gl\u00fcck.", "tokens": ["Die", "st\u00f6\u00b7ret", "all", "mein", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}