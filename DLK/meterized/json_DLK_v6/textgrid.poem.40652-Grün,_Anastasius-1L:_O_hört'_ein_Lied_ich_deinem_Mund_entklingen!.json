{"textgrid.poem.40652": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: O h\u00f6rt' ein Lied ich deinem Mund entklingen!", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O h\u00f6rt' ein Lied ich deinem Mund entklingen!", "tokens": ["O", "h\u00f6rt'", "ein", "Lied", "ich", "dei\u00b7nem", "Mund", "ent\u00b7klin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Genesung ist's, bl\u00fchst du in S\u00e4ngen wieder;", "tokens": ["Ge\u00b7ne\u00b7sung", "ist's", ",", "bl\u00fchst", "du", "in", "S\u00e4n\u00b7gen", "wie\u00b7der", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "VVFIN", "PPER", "APPR", "NN", "ADV", "$."], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Des Dichterbaumes Bl\u00fcthen sind die Lieder,", "tokens": ["Des", "Dich\u00b7ter\u00b7bau\u00b7mes", "Bl\u00fc\u00b7then", "sind", "die", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kein kranker Baum wird solche Bl\u00fcthen bringen.", "tokens": ["Kein", "kran\u00b7ker", "Baum", "wird", "sol\u00b7che", "Bl\u00fc\u00b7then", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sei's auch ein d\u00fcstres Lied, wenn nur dein Singen!", "tokens": ["Sei's", "auch", "ein", "d\u00fcst\u00b7res", "Lied", ",", "wenn", "nur", "dein", "Sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "ADJA", "NN", "$,", "KOUS", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die dunkle Tanne bl\u00fcht nicht hell wie Flieder,", "tokens": ["Die", "dunk\u00b7le", "Tan\u00b7ne", "bl\u00fcht", "nicht", "hell", "wie", "Flie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKNEG", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Selbst deine Lerchen tragen schwarz Gefieder,", "tokens": ["Selbst", "dei\u00b7ne", "Ler\u00b7chen", "tra\u00b7gen", "schwarz", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur Morgenroth vergoldet ihre Schwingen.", "tokens": ["Nur", "Mor\u00b7gen\u00b7roth", "ver\u00b7gol\u00b7det", "ih\u00b7re", "Schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es ist dein Lied der r\u00e4thselvolle Falter,", "tokens": ["Es", "ist", "dein", "Lied", "der", "r\u00e4th\u00b7sel\u00b7vol\u00b7le", "Fal\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der einen Todtensch\u00e4del tr\u00e4gt zum Schilde;", "tokens": ["Der", "ei\u00b7nen", "Tod\u00b7ten\u00b7sch\u00e4\u00b7del", "tr\u00e4gt", "zum", "Schil\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch nur durch sch\u00f6ne Fr\u00fchlingsn\u00e4chte wallt er!", "tokens": ["Doch", "nur", "durch", "sch\u00f6\u00b7ne", "Fr\u00fch\u00b7lings\u00b7n\u00e4ch\u00b7te", "wallt", "er", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Passiflore gleicht's, ein Kreuz umschwankend,", "tokens": ["Der", "Pas\u00b7sif\u00b7lo\u00b7re", "gleicht's", ",", "ein", "Kreuz", "um\u00b7schwan\u00b7kend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein g\u00f6ttlich Leiden formt ihr Bl\u00fchn zum Bilde;", "tokens": ["Ein", "g\u00f6tt\u00b7lich", "Lei\u00b7den", "formt", "ihr", "Bl\u00fchn", "zum", "Bil\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch nur in Fr\u00fchlingssonnen bl\u00fcht sie rankend.", "tokens": ["Doch", "nur", "in", "Fr\u00fch\u00b7lings\u00b7son\u00b7nen", "bl\u00fcht", "sie", "ran\u00b7kend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}