{"textgrid.poem.64324": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "45. Der Freigeist", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn auf der Verzweiflung Wogen ich bin,", "tokens": ["Wenn", "auf", "der", "Ver\u00b7zwei\u00b7flung", "Wo\u00b7gen", "ich", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "PPER", "VAFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Treibt rund mich umher mein wilder Sinn;", "tokens": ["Treibt", "rund", "mich", "um\u00b7her", "mein", "wil\u00b7der", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "PTKVZ", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er treibet mich kreuz, er treibet mich quer,", "tokens": ["Er", "trei\u00b7bet", "mich", "kreuz", ",", "er", "trei\u00b7bet", "mich", "quer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Durch Klippen und Sandb\u00e4nke hin und her.", "tokens": ["Durch", "Klip\u00b7pen", "und", "Sand\u00b7b\u00e4n\u00b7ke", "hin", "und", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Und trieben nur vorw\u00e4rts die St\u00fcrme mich weiter,", "tokens": ["Und", "trie\u00b7ben", "nur", "vor\u00b7w\u00e4rts", "die", "St\u00fcr\u00b7me", "mich", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "So w\u00fcrde mein Nachen mit Ehre zur Scheiter!", "tokens": ["So", "w\u00fcr\u00b7de", "mein", "Na\u00b7chen", "mit", "Eh\u00b7re", "zur", "Schei\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Zur Windsbraut ruf' ich: Sei mein Geno\u00df!", "tokens": ["Zur", "Winds\u00b7braut", "ruf'", "ich", ":", "Sei", "mein", "Ge\u00b7no\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$.", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.4": {"text": "Zum Strudel: Nimm du mich in deinen Scho\u00df!", "tokens": ["Zum", "Stru\u00b7del", ":", "Nimm", "du", "mich", "in", "dei\u00b7nen", "Scho\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "NE", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch Strudel und Windsbraut h\u00f6ren mich nicht;", "tokens": ["Doch", "Stru\u00b7del", "und", "Winds\u00b7braut", "h\u00f6\u00b7ren", "mich", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kein Wetterstrahl sendet mir leuchtendes Licht;", "tokens": ["Kein", "Wet\u00b7ter\u00b7strahl", "sen\u00b7det", "mir", "leuch\u00b7ten\u00b7des", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Rund um mich schwimmt alles in Mitternacht,", "tokens": ["Rund", "um", "mich", "schwimmt", "al\u00b7les", "in", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VVFIN", "PIS", "APPR", "NN", "$,"], "meter": "+--++--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Die mich unth\u00e4tig und rasend macht!", "tokens": ["Die", "mich", "un\u00b7th\u00e4\u00b7tig", "und", "ra\u00b7send", "macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "+-++--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Es dr\u00e4ngen sich Welten in meiner Brust,", "tokens": ["Es", "dr\u00e4n\u00b7gen", "sich", "Wel\u00b7ten", "in", "mei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Entflammtes Verlangen, verderbende Lust,", "tokens": ["Ent\u00b7flamm\u00b7tes", "Ver\u00b7lan\u00b7gen", ",", "ver\u00b7der\u00b7ben\u00b7de", "Lust", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die Elemente zu kneten zusammen,", "tokens": ["Die", "E\u00b7le\u00b7men\u00b7te", "zu", "kne\u00b7ten", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVFIN", "PTKVZ", "$,"], "meter": "--+---+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Meer und Erde zu peitschen mit Flammen!", "tokens": ["Meer", "und", "Er\u00b7de", "zu", "peit\u00b7schen", "mit", "Flam\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKZU", "VVINF", "APPR", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "O w\u00e4r' ich, entfernt von Erd' und See,", "tokens": ["O", "w\u00e4r'", "ich", ",", "ent\u00b7fernt", "von", "Erd'", "und", "See", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "$,", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Hoch \u00fcber Arkturs und Orions H\u00f6h,", "tokens": ["Hoch", "\u00fc\u00b7ber", "Ark\u00b7turs", "und", "O\u00b7rions", "H\u00f6h", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NE", "KON", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und s\u00e4he den Strom der Vernichtungen flie\u00dfen,", "tokens": ["Und", "s\u00e4\u00b7he", "den", "Strom", "der", "Ver\u00b7nich\u00b7tun\u00b7gen", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Gleich B\u00e4chen die Himmel hinein sich ergie\u00dfen!", "tokens": ["Gleich", "B\u00e4\u00b7chen", "die", "Him\u00b7mel", "hin\u00b7ein", "sich", "er\u00b7gie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ART", "NN", "APZR", "PRF", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "Und s\u00e4h' und h\u00f6rte all\u00fcberall", "tokens": ["Und", "s\u00e4h'", "und", "h\u00f6r\u00b7te", "al\u00b7l\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PIAT"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Geschleuderte Tr\u00fcmmer und donnernden Fall,", "tokens": ["Ge\u00b7schleu\u00b7der\u00b7te", "Tr\u00fcm\u00b7mer", "und", "don\u00b7nern\u00b7den", "Fall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und in den himmelverschlingenden Wellen", "tokens": ["Und", "in", "den", "him\u00b7mel\u00b7ver\u00b7schlin\u00b7gen\u00b7den", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Scheitern die Sonnen, die Erden zerschellen!", "tokens": ["Schei\u00b7tern", "die", "Son\u00b7nen", ",", "die", "Er\u00b7den", "zer\u00b7schel\u00b7len", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.7": {"line.1": {"text": "Und blieb' hohnlachend noch \u00fcbrig allein,", "tokens": ["Und", "blieb'", "hohn\u00b7la\u00b7chend", "noch", "\u00fcb\u00b7rig", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "ADJD", "ADV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und st\u00fcrzte mich dann in die Wogen hinein!", "tokens": ["Und", "st\u00fcrz\u00b7te", "mich", "dann", "in", "die", "Wo\u00b7gen", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Es deckte mich Mitternacht, Tr\u00fcmmer und Graus,", "tokens": ["Es", "deck\u00b7te", "mich", "Mit\u00b7ter\u00b7nacht", ",", "Tr\u00fcm\u00b7mer", "und", "Graus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und feierlich spielt' ich mein Possenspiel aus!", "tokens": ["Und", "fei\u00b7er\u00b7lich", "spielt'", "ich", "mein", "Pos\u00b7sen\u00b7spiel", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}