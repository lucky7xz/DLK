{"dta.poem.295": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An die strenge  Lesbia.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.71", "fr:0.28"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Galante Lesbia du t\u00f6dtest mir mein Leben", "tokens": ["Ga\u00b7lan\u00b7te", "Les\u00b7bia", "du", "t\u00f6d\u00b7test", "mir", "mein", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "PPER", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wenn deine harte Brust sich noch nicht will ergeben;", "tokens": ["Wenn", "dei\u00b7ne", "har\u00b7te", "Brust", "sich", "noch", "nicht", "will", "er\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PRF", "ADV", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mich qu\u00e4hlet deine Pracht/ dein Hart-seyn foltert mich/", "tokens": ["Mich", "qu\u00e4h\u00b7let", "dei\u00b7ne", "Pracht", "/", "dein", "Har\u00b7tseyn", "fol\u00b7tert", "mich", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "VVFIN", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich bin lebendig todt/ und seuffze \u00e4ngstiglich.", "tokens": ["Ich", "bin", "le\u00b7ben\u00b7dig", "todt", "/", "und", "seuff\u00b7ze", "\u00e4ngs\u00b7tig\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADJD", "$(", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Allein umsonst! umsonst sind meine Jammer-Zeichen/", "tokens": ["Al\u00b7lein", "um\u00b7sonst", "!", "um\u00b7sonst", "sind", "mei\u00b7ne", "Jam\u00b7mer\u00b7Zei\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "ADV", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein Hertz/ das steinern ist/ ist gar nicht zu erweichen/", "tokens": ["Dein", "Hertz", "/", "das", "stei\u00b7nern", "ist", "/", "ist", "gar", "nicht", "zu", "er\u00b7wei\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ART", "ADJA", "VAFIN", "$(", "VAFIN", "ADV", "PTKNEG", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du bleibest allezeit bey meinen Klagen hart/", "tokens": ["Du", "blei\u00b7best", "al\u00b7le\u00b7zeit", "bey", "mei\u00b7nen", "Kla\u00b7gen", "hart", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ob schon das Seuffzen sich mit meinen Thr\u00e4nen paart.", "tokens": ["Ob", "schon", "das", "Seuff\u00b7zen", "sich", "mit", "mei\u00b7nen", "Thr\u00e4\u00b7nen", "paart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein Diamant zerspringt im schlechten Bockes-Blute", "tokens": ["Ein", "Di\u00b7a\u00b7mant", "zer\u00b7springt", "im", "schlech\u00b7ten", "Bo\u00b7ckes\u00b7Blu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mir aber kommt mein Blut/ und Weinen nicht zu gute/", "tokens": ["Mir", "a\u00b7ber", "kommt", "mein", "Blut", "/", "und", "Wei\u00b7nen", "nicht", "zu", "gu\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPOSAT", "NN", "$(", "KON", "NN", "PTKNEG", "APPR", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein harter Stein wird weich/ das Eisen z\u00e4h gemacht/", "tokens": ["Ein", "har\u00b7ter", "Stein", "wird", "weich", "/", "das", "Ei\u00b7sen", "z\u00e4h", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$(", "ART", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nur du/ mein Kind/ wirst nie auf andern Sinn gebracht.", "tokens": ["Nur", "du", "/", "mein", "Kind", "/", "wirst", "nie", "auf", "an\u00b7dern", "Sinn", "ge\u00b7bracht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$(", "PPOSAT", "NN", "$(", "VAFIN", "ADV", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sonst pflegt man nach der Nacht/ und ihren Finsternissen/", "tokens": ["Sonst", "pflegt", "man", "nach", "der", "Nacht", "/", "und", "ih\u00b7ren", "Fins\u00b7ter\u00b7nis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "$(", "KON", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den frohen Morgen-Stern im hellen Licht zu k\u00fcssen/", "tokens": ["Den", "fro\u00b7hen", "Mor\u00b7gen\u00b7S\u00b7tern", "im", "hel\u00b7len", "Licht", "zu", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nach langen Fasten bricht das Oster-Fest herein/", "tokens": ["Nach", "lan\u00b7gen", "Fas\u00b7ten", "bricht", "das", "Os\u00b7ter\u00b7Fest", "her\u00b7ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$("], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Du aber wilt mir Nacht/ und ewigs Fasten seyn.", "tokens": ["Du", "a\u00b7ber", "wilt", "mir", "Nacht", "/", "und", "e\u00b7wigs", "Fas\u00b7ten", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PPER", "NN", "$(", "KON", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ich soll das Jubel-Jahr auf deiner Brust nicht halten/", "tokens": ["Ich", "soll", "das", "Ju\u00b7bel\u00b7Jahr", "auf", "dei\u00b7ner", "Brust", "nicht", "hal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein heisses Seuffzen mu\u00df in k\u00fchler Lufft erkalten.", "tokens": ["Mein", "heis\u00b7ses", "Seuff\u00b7zen", "mu\u00df", "in", "k\u00fch\u00b7ler", "Lufft", "er\u00b7kal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da stets die Marter-Woch\u2019 in meiner Seelen ist/", "tokens": ["Da", "stets", "die", "Mar\u00b7ter\u00b7Wo\u00b7ch'", "in", "mei\u00b7ner", "See\u00b7len", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "So klag\u2019 ich/ da\u00df mein Mund noch keinen Abla\u00df k\u00fc\u00dft.", "tokens": ["So", "klag'", "ich", "/", "da\u00df", "mein", "Mund", "noch", "kei\u00b7nen", "Ab\u00b7la\u00df", "k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "PPOSAT", "NN", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ich schiffe um den Port/ wo Freud\u2019 und Anmuth wohnet/", "tokens": ["Ich", "schif\u00b7fe", "um", "den", "Port", "/", "wo", "Freud'", "und", "An\u00b7muth", "woh\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NE", "$(", "PWAV", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Allein vor jene Lust/ werd\u2019 ich mit Angst belohnet/", "tokens": ["Al\u00b7lein", "vor", "je\u00b7ne", "Lust", "/", "werd'", "ich", "mit", "Angst", "be\u00b7loh\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$(", "VAFIN", "PPER", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Indem das Vor-Geb\u00fcrg/ so nach der Hoffnung hei\u00dft/", "tokens": ["In\u00b7dem", "das", "Vor\u00b7Ge\u00b7b\u00fcrg", "/", "so", "nach", "der", "Hoff\u00b7nung", "hei\u00dft", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$(", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sich/ wenn ich landen will/ von meinem Schifflein rei\u00dft.", "tokens": ["Sich", "/", "wenn", "ich", "lan\u00b7den", "will", "/", "von", "mei\u00b7nem", "Schif\u00b7flein", "rei\u00dft", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "$(", "KOUS", "PPER", "VVINF", "VMFIN", "$(", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "So mu\u00df ich gleiche Quaal wie ", "tokens": ["So", "mu\u00df", "ich", "glei\u00b7che", "Qua\u00b7al", "wie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJA", "NN", "KOKOM"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich sehe meine Lust/ und darf es doch nicht wagen/", "tokens": ["Ich", "se\u00b7he", "mei\u00b7ne", "Lust", "/", "und", "darf", "es", "doch", "nicht", "wa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "KON", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df ich sie kosten will/ weil du sonst wirst erhitzt/", "tokens": ["Da\u00df", "ich", "sie", "kos\u00b7ten", "will", "/", "weil", "du", "sonst", "wirst", "er\u00b7hitzt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$(", "KOUS", "PPER", "ADV", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und Donner st\u00f6ssest aus/ dabey mein Himmel blitzt.", "tokens": ["Und", "Don\u00b7ner", "st\u00f6s\u00b7sest", "aus", "/", "da\u00b7bey", "mein", "Him\u00b7mel", "blitzt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PTKVZ", "$(", "PAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Jxion ist mir gleich/ ich dulde seine Schmertzen/", "tokens": ["Jxion", "ist", "mir", "gleich", "/", "ich", "dul\u00b7de", "sei\u00b7ne", "Schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "$(", "PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn ich dich k\u00fcssen will/ so mu\u00df ich Wolcken hertzen/", "tokens": ["Wenn", "ich", "dich", "k\u00fcs\u00b7sen", "will", "/", "so", "mu\u00df", "ich", "Wol\u00b7cken", "hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$(", "ADV", "VMFIN", "PPER", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Umarmen einen Dunst/ und einen falschen Schein/", "tokens": ["Um\u00b7ar\u00b7men", "ei\u00b7nen", "Dunst", "/", "und", "ei\u00b7nen", "fal\u00b7schen", "Schein", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$(", "KON", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn du wilt auch so gar im K\u00fcssen grausam seyn.", "tokens": ["Denn", "du", "wilt", "auch", "so", "gar", "im", "K\u00fcs\u00b7sen", "grau\u00b7sam", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "ADV", "ADV", "APPRART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "So qu\u00e4hlest du mein Hertz mit ", "tokens": ["So", "qu\u00e4h\u00b7lest", "du", "mein", "Hertz", "mit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und doch kan stets dein Mund von grosser G\u00fcte sagen/", "tokens": ["Und", "doch", "kan", "stets", "dein", "Mund", "von", "gros\u00b7ser", "G\u00fc\u00b7te", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "ADV", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von deines Hertzens-Brand/ von deiner Treu und Huld", "tokens": ["Von", "dei\u00b7nes", "Hert\u00b7zens\u00b7Brand", "/", "von", "dei\u00b7ner", "Treu", "und", "Huld"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$(", "APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da doch mein Leben stirbt vor grosser Ungedult.", "tokens": ["Da", "doch", "mein", "Le\u00b7ben", "stirbt", "vor", "gros\u00b7ser", "Un\u00b7ge\u00b7dult", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Ach harte G\u00fct\u2019 ach! ach! feindseeliges Erbarmen/", "tokens": ["Ach", "har\u00b7te", "G\u00fct'", "ach", "!", "ach", "!", "feind\u00b7see\u00b7li\u00b7ges", "Er\u00b7bar\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "ITJ", "$.", "XY", "$.", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie qu\u00e4hlet doch dein Stoltz mich halb-erstorbnen Armen!", "tokens": ["Wie", "qu\u00e4h\u00b7let", "doch", "dein", "Stoltz", "mich", "halb\u00b7er\u00b7storb\u00b7nen", "Ar\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "PPOSAT", "NN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bald lebe ich in Lust wenn mich ein Blick vergn\u00fcgt/", "tokens": ["Bald", "le\u00b7be", "ich", "in", "Lust", "wenn", "mich", "ein", "Blick", "ver\u00b7gn\u00fcgt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KOUS", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bald aber stirbt der Geist/ wenn mich der Schein betriegt.", "tokens": ["Bald", "a\u00b7ber", "stirbt", "der", "Geist", "/", "wenn", "mich", "der", "Schein", "be\u00b7triegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "wo