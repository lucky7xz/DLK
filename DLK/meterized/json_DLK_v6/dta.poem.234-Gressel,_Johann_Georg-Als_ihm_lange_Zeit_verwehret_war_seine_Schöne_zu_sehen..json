{"dta.poem.234": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als ihm lange Zeit verwehret war seine  \n Sch\u00f6ne zu sehen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6nes Kind/ ach mein Verlangen!", "tokens": ["Sch\u00f6\u00b7nes", "Kind", "/", "ach", "mein", "Ver\u00b7lan\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "XY", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Soll ich dich denn nicht mehr sehn?", "tokens": ["Soll", "ich", "dich", "denn", "nicht", "mehr", "sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist mein Gl\u00fccks-Stern untergangen", "tokens": ["Ist", "mein", "Gl\u00fccks\u00b7S\u00b7tern", "un\u00b7ter\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und soll es nicht mehr geschehn?", "tokens": ["Und", "soll", "es", "nicht", "mehr", "ge\u00b7schehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Da\u00df ich aus den holden Augen", "tokens": ["Da\u00df", "ich", "aus", "den", "hol\u00b7den", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und den Liljen deiner Brust/", "tokens": ["Und", "den", "Lil\u00b7jen", "dei\u00b7ner", "Brust", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Darf als eine Biene saugen/", "tokens": ["Darf", "als", "ei\u00b7ne", "Bie\u00b7ne", "sau\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "KOKOM", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Anmuhts volle Himmels-Lust.", "tokens": ["An\u00b7muhts", "vol\u00b7le", "Him\u00b7mels\u00b7Lust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Zwar mein Engel deine G\u00fcte/", "tokens": ["Zwar", "mein", "En\u00b7gel", "dei\u00b7ne", "G\u00fc\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Strahlt noch immer auf mich zu/", "tokens": ["Strahlt", "noch", "im\u00b7mer", "auf", "mich", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "PPER", "PTKZU", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mein trauriges Gem\u00fchte/", "tokens": ["Und", "mein", "trau\u00b7ri\u00b7ges", "Ge\u00b7m\u00fch\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00fc\u00dfte die erw\u00fcnschte Ruh/", "tokens": ["K\u00fc\u00df\u00b7te", "die", "er\u00b7w\u00fcnschte", "Ruh", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Wenn die Mi\u00dfgunst ihr bewachen/", "tokens": ["Wenn", "die", "Mi\u00df\u00b7gunst", "ihr", "be\u00b7wa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und ihr H\u00fcten unterlie\u00df/", "tokens": ["Und", "ihr", "H\u00fc\u00b7ten", "un\u00b7ter\u00b7lie\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die/ als wie vor dem die Drachen/", "tokens": ["Die", "/", "als", "wie", "vor", "dem", "die", "Dra\u00b7chen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "KOUS", "KOKOM", "APPR", "ART", "ART", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Dich bewahrt/ mein g\u00fcldnes Vl\u00fc\u00df.", "tokens": ["Dich", "be\u00b7wahrt", "/", "mein", "g\u00fcld\u00b7nes", "Vl\u00fc\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$(", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Geht ihr Himmel/ da\u00df mein Warten/", "tokens": ["Geht", "ihr", "Him\u00b7mel", "/", "da\u00df", "mein", "War\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$(", "KOUS", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den erw\u00fcnschten Zweck erreicht/", "tokens": ["Den", "er\u00b7w\u00fcnschten", "Zweck", "er\u00b7reicht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$("], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Oeffnet den verschlo\u00dfnen Garten", "tokens": ["Oeff\u00b7net", "den", "ver\u00b7schlo\u00df\u00b7nen", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schaffet da\u00df die Schlange weicht/", "tokens": ["Schaf\u00b7fet", "da\u00df", "die", "Schlan\u00b7ge", "weicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die mir von dem Lebens-Baume/", "tokens": ["Die", "mir", "von", "dem", "Le\u00b7bens\u00b7Bau\u00b7me", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "$("], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.6": {"text": "Nicht die s\u00fcsse Frucht gewehrt/", "tokens": ["Nicht", "die", "s\u00fcs\u00b7se", "Frucht", "ge\u00b7wehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und nur stets im leerem Traume", "tokens": ["Und", "nur", "stets", "im", "lee\u00b7rem", "Trau\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sodoms Fr\u00fcchte mir verehrt.", "tokens": ["So\u00b7doms", "Fr\u00fcch\u00b7te", "mir", "ver\u00b7ehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PPER", "VVPP", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Doch wie \u00f6ffters nach dem Weinen/", "tokens": ["Doch", "wie", "\u00f6ff\u00b7ters", "nach", "dem", "Wei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mancher feyrt ein Freuden-Fest/", "tokens": ["Man\u00b7cher", "feyrt", "ein", "Freu\u00b7den\u00b7Fest", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Sonne pflegt zu scheinen/", "tokens": ["Und", "die", "Son\u00b7ne", "pflegt", "zu", "schei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn es nach mit St\u00fcrmen l\u00e4st/", "tokens": ["Wenn", "es", "nach", "mit", "St\u00fcr\u00b7men", "l\u00e4st", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "APPR", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So werd ich/ was mein Verlangen/", "tokens": ["So", "werd", "ich", "/", "was", "mein", "Ver\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$(", "PWS", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und was mich erfreuen kan/", "tokens": ["Und", "was", "mich", "er\u00b7freu\u00b7en", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Endlich wiederum umfangen/", "tokens": ["End\u00b7lich", "wie\u00b7de\u00b7rum", "um\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gleich wie ich vor dem gethan.", "tokens": ["Gleich", "wie", "ich", "vor", "dem", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "APPR", "ART", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}