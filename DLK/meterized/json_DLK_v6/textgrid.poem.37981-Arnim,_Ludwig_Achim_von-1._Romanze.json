{"textgrid.poem.37981": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1. Romanze", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor\u00fcber zieht manch edler Aar,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "zieht", "manch", "ed\u00b7ler", "Aar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herr Peter ein theurer Ritter war,", "tokens": ["Herr", "Pe\u00b7ter", "ein", "theu\u00b7rer", "Rit\u00b7ter", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er war so keusch, er war so rein,", "tokens": ["Er", "war", "so", "keusch", ",", "er", "war", "so", "rein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie seines Antlitz edler Schein,", "tokens": ["Wie", "sei\u00b7nes", "Ant\u00b7litz", "ed\u00b7ler", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er war bereit zu jeder Zeit,", "tokens": ["Er", "war", "be\u00b7reit", "zu", "je\u00b7der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu Schimpf, zu Ernst, zu Lust, zu Streit.", "tokens": ["Zu", "Schimpf", ",", "zu", "Ernst", ",", "zu", "Lust", ",", "zu", "Streit", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NE", "$,", "APPR", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In junger Kraft, in fremdem Land,", "tokens": ["In", "jun\u00b7ger", "Kraft", ",", "in", "frem\u00b7dem", "Land", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Mannheit machte ihn bekannt,", "tokens": ["Sein", "Mann\u00b7heit", "mach\u00b7te", "ihn", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als er nach Hause kehrt zur\u00fcck,", "tokens": ["Als", "er", "nach", "Hau\u00b7se", "kehrt", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bedenkt in sich sein hohes Gl\u00fcck,", "tokens": ["Be\u00b7denkt", "in", "sich", "sein", "ho\u00b7hes", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Langsam zur Burg hinauf thut reiten,", "tokens": ["Lang\u00b7sam", "zur", "Burg", "hin\u00b7auf", "thut", "rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ADV", "VVFIN", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Was sieht sein Knecht zu einer Seiten?", "tokens": ["Was", "sieht", "sein", "Knecht", "zu", "ei\u00b7ner", "Sei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er sieht ein sch\u00f6nes Weib da sitzen,", "tokens": ["Er", "sieht", "ein", "sch\u00f6\u00b7nes", "Weib", "da", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Gold und Silber herrlich blitzen,", "tokens": ["Von", "Gold", "und", "Sil\u00b7ber", "herr\u00b7lich", "blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von Perlen und von Edelstein,", "tokens": ["Von", "Per\u00b7len", "und", "von", "E\u00b7del\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie eine Sonne reich und rein,", "tokens": ["Wie", "ei\u00b7ne", "Son\u00b7ne", "reich", "und", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Knecht winkt seinen Herrn zu sich:", "tokens": ["Der", "Knecht", "winkt", "sei\u00b7nen", "Herrn", "zu", "sich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbgern diente dieser Fraue ich!\u00ab", "tokens": ["\u00bb", "gern", "dien\u00b7te", "die\u00b7ser", "Frau\u00b7e", "ich", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "PDAT", "NN", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Ritter gr\u00fc\u00dft in gro\u00dfer Zucht,", "tokens": ["Der", "Rit\u00b7ter", "gr\u00fc\u00dft", "in", "gro\u00b7\u00dfer", "Zucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er dr\u00fcckt an sich die edle Frucht:", "tokens": ["Er", "dr\u00fcckt", "an", "sich", "die", "ed\u00b7le", "Frucht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbihr seyd es Ritter, edler Herr,", "tokens": ["\u00bb", "ihr", "seyd", "es", "Rit\u00b7ter", ",", "ed\u00b7ler", "Herr", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPER", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Wunder das mich treibet her,", "tokens": ["Das", "Wun\u00b7der", "das", "mich", "trei\u00b7bet", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In allen Landen, wo ihr wart,", "tokens": ["In", "al\u00b7len", "Lan\u00b7den", ",", "wo", "ihr", "wart", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hab ich euch gl\u00fccklich stets bewahrt.\u00ab", "tokens": ["Hab", "ich", "euch", "gl\u00fcck\u00b7lich", "stets", "be\u00b7wahrt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "PPER", "ADJD", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbkein sch\u00f6ner Weib hab ich erblickt,", "tokens": ["\u00bb", "kein", "sch\u00f6\u00b7ner", "Weib", "hab", "ich", "er\u00b7blickt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich lieb euch wie es aus mir blickt.", "tokens": ["Ich", "lieb", "euch", "wie", "es", "aus", "mir", "blickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PWAV", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich sah euch oft im tiefsten Traum,", "tokens": ["Ich", "sah", "euch", "oft", "im", "tiefs\u00b7ten", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jezt glaub ich meinen Sinnen kaum,", "tokens": ["Jezt", "glaub", "ich", "mei\u00b7nen", "Sin\u00b7nen", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wollt Gott, ihr w\u00e4rt mein ehlich Weib,", "tokens": ["Wollt", "Gott", ",", "ihr", "w\u00e4rt", "mein", "eh\u00b7lich", "Weib", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "$,", "PPER", "VAFIN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Ehren dient ich eurem Leib.\u00ab", "tokens": ["In", "Eh\u00b7ren", "dient", "ich", "eu\u00b7rem", "Leib", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00bbnun so wohl hin, sprach da die Zart:", "tokens": ["\u00bb", "nun", "so", "wohl", "hin", ",", "sprach", "da", "die", "Zart", ":"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "ADV", "PTKVZ", "$,", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+----+-+", "measure": "dactylic.init"}, "line.2": {"text": "Auf diese Red hab ich gewart,", "tokens": ["Auf", "die\u00b7se", "Red", "hab", "ich", "ge\u00b7wart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich zog dich auf mit Liebeskraft,", "tokens": ["Ich", "zog", "dich", "auf", "mit", "Lie\u00b7bes\u00b7kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die alles wirkt, die alles schafft,", "tokens": ["Die", "al\u00b7les", "wirkt", ",", "die", "al\u00b7les", "schafft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich bin die Deine, ewig dein,", "tokens": ["Ich", "bin", "die", "Dei\u00b7ne", ",", "e\u00b7wig", "dein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "PPOSAT", "$,", "ADJD", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch must du auch der Meine seyn.", "tokens": ["Doch", "must", "du", "auch", "der", "Mei\u00b7ne", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ART", "PPOSAT", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Nie darfst du nehmen ein ander Weib,", "tokens": ["Nie", "darfst", "du", "neh\u00b7men", "ein", "an\u00b7der", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dir eigen ist mein sch\u00f6ner Leib,", "tokens": ["Dir", "ei\u00b7gen", "ist", "mein", "sch\u00f6\u00b7ner", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In jeder Nacht, wo du begehrst,", "tokens": ["In", "je\u00b7der", "Nacht", ",", "wo", "du", "be\u00b7gehrst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Macht und Reichthum dir beschert,", "tokens": ["Und", "Macht", "und", "Reicht\u00b7hum", "dir", "be\u00b7schert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein ewig endeloses Leben,", "tokens": ["Ein", "e\u00b7wig", "en\u00b7de\u00b7lo\u00b7ses", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Will ich durch meine Kraft dir geben.", "tokens": ["Will", "ich", "durch", "mei\u00b7ne", "Kraft", "dir", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Unangefocht wirst du nicht bleiben,", "tokens": ["Un\u00b7an\u00b7ge\u00b7focht", "wirst", "du", "nicht", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Man wird dich treiben, dich zu weiben,", "tokens": ["Man", "wird", "dich", "trei\u00b7ben", ",", "dich", "zu", "wei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "VVINF", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo dus dann thust, red ich ohn Zagen,", "tokens": ["Wo", "dus", "dann", "thust", ",", "red", "ich", "ohn", "Za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "VVFIN", "$,", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So bist du todt in dreyen Tagen;", "tokens": ["So", "bist", "du", "todt", "in", "drey\u00b7en", "Ta\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sieh weg von mir und denke nach,", "tokens": ["Sieh", "weg", "von", "mir", "und", "den\u00b7ke", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPER", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was dir dein eignes Herze sagt.\u00ab", "tokens": ["Was", "dir", "dein", "eig\u00b7nes", "Her\u00b7ze", "sagt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "\u00bbnun herzigs Weib ist dem also,", "tokens": ["\u00bb", "nun", "her\u00b7zigs", "Weib", "ist", "dem", "al\u00b7so", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJA", "NN", "VAFIN", "ART", "ADV", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "So werdet meiner Treue froh,", "tokens": ["So", "wer\u00b7det", "mei\u00b7ner", "Treu\u00b7e", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was soll ich f\u00fcr ein Zeichen haben,", "tokens": ["Was", "soll", "ich", "f\u00fcr", "ein", "Zei\u00b7chen", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ihr von mir wollt nimmer lassen?\u00ab", "tokens": ["Da\u00df", "ihr", "von", "mir", "wollt", "nim\u00b7mer", "las\u00b7sen", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VMFIN", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bbso trag von mir den goldnen Ring,", "tokens": ["\u00bb", "so", "trag", "von", "mir", "den", "gold\u00b7nen", "Ring", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vor Ungl\u00fcck sch\u00fctzet dich der Ring.\u00ab", "tokens": ["Vor", "Un\u00b7gl\u00fcck", "sch\u00fct\u00b7zet", "dich", "der", "Ring", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Mit spielendem Ku\u00df er Abschied nahm,", "tokens": ["Mit", "spie\u00b7len\u00b7dem", "Ku\u00df", "er", "Ab\u00b7schied", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zur Messe er nach Nu\u00dfbach kam,", "tokens": ["Zur", "Mes\u00b7se", "er", "nach", "Nu\u00df\u00b7bach", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da ging er mit den Kreuzen auch,", "tokens": ["Da", "ging", "er", "mit", "den", "Kreu\u00b7zen", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nahte sich dem Weiherauch,", "tokens": ["Und", "nah\u00b7te", "sich", "dem", "Wei\u00b7her\u00b7auch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sein Leib und Seel er Gott befahl,", "tokens": ["Sein", "Leib", "und", "Seel", "er", "Gott", "be\u00b7fahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er sollt ihn sch\u00fctzen \u00fcberall.", "tokens": ["Er", "sollt", "ihn", "sch\u00fct\u00b7zen", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}