{"dta.poem.8438": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An Voss .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Durch die Wolken hinauf, bis du den Stralenkranz,", "tokens": ["Durch", "die", "Wol\u00b7ken", "hin\u00b7auf", ",", "bis", "du", "den", "Stra\u00b7len\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,", "KOUS", "PPER", "ART", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Der nur weiseren Dichtern", "tokens": ["Der", "nur", "wei\u00b7se\u00b7ren", "Dich\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Funkelt, dir um die Schl\u00e4fe schlingst.", "tokens": ["Fun\u00b7kelt", ",", "dir", "um", "die", "Schl\u00e4\u00b7fe", "schlingst", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Heisser liebe durch dich Enkel und Enkelin", "tokens": ["Heis\u00b7ser", "lie\u00b7be", "durch", "dich", "En\u00b7kel", "und", "En\u00b7ke\u00b7lin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PPER", "NN", "KON", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Gott und seine Natur, herzliche Brudertreu,", "tokens": ["Gott", "und", "sei\u00b7ne", "Na\u00b7tur", ",", "herz\u00b7li\u00b7che", "Bru\u00b7der\u00b7treu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "PPOSAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Einfalt, Freiheit und Unschuld,", "tokens": ["Ein\u00b7falt", ",", "Frei\u00b7heit", "und", "Un\u00b7schuld", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Deutsche Tugend und Redlichkeit.", "tokens": ["Deut\u00b7sche", "Tu\u00b7gend", "und", "Red\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Stilles Trittes, o Voss, wandelt indess dein Freund", "tokens": ["Stil\u00b7les", "Trit\u00b7tes", ",", "o", "Voss", ",", "wan\u00b7delt", "in\u00b7dess", "dein", "Freund"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "FM", "FM", "$,", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Durch Gefilde der Ruh, lauschet der Nachtigall", "tokens": ["Durch", "Ge\u00b7fil\u00b7de", "der", "Ruh", ",", "lau\u00b7schet", "der", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Und der Stimme des leisen", "tokens": ["Und", "der", "Stim\u00b7me", "des", "lei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Mondbeschimmerten Wiesenborns;", "tokens": ["Mond\u00b7be\u00b7schim\u00b7mer\u00b7ten", "Wie\u00b7sen\u00b7borns", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Singt den duftenden Hain, welchen das Morgenroth", "tokens": ["Singt", "den", "duf\u00b7ten\u00b7den", "Hain", ",", "wel\u00b7chen", "das", "Mor\u00b7gen\u00b7roth"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "PWAT", "ART", "NN"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ueberflimmert mit Gold', oder den Fr\u00fchlingsstrauss,", "tokens": ["Ue\u00b7ber\u00b7flim\u00b7mert", "mit", "Gold'", ",", "o\u00b7der", "den", "Fr\u00fch\u00b7lings\u00b7strauss", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$,", "KON", "ART", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Der am Busen des M\u00e4dchens,", "tokens": ["Der", "am", "Bu\u00b7sen", "des", "M\u00e4d\u00b7chens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Mildger\u00f6thet vom Abend, bebt.", "tokens": ["Mild\u00b7ge\u00b7r\u00f6\u00b7thet", "vom", "A\u00b7bend", ",", "bebt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Mir auch weinet, auch mir, Wonne! das M\u00e4dchen Dank,", "tokens": ["Mir", "auch", "wei\u00b7net", ",", "auch", "mir", ",", "Won\u00b7ne", "!", "das", "M\u00e4d\u00b7chen", "Dank", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "ADV", "PPER", "$,", "NN", "$.", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "K\u00fcsst mein z\u00e4rtliches Lied, dr\u00fcckt es an ihre Brust,", "tokens": ["K\u00fcsst", "mein", "z\u00e4rt\u00b7li\u00b7ches", "Lied", ",", "dr\u00fcckt", "es", "an", "ih\u00b7re", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Seufzt: Du redlicher J\u00fcngling,", "tokens": ["Seufzt", ":", "Du", "red\u00b7li\u00b7cher", "J\u00fcng\u00b7ling", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Warum barg dich die Gruft so fr\u00fch!", "tokens": ["Wa\u00b7rum", "barg", "dich", "die", "Gruft", "so", "fr\u00fch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}