{"textgrid.poem.50094": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "50. Auff den vom Geitz bekehrten Hydaspes", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und lebte von dem ", "tokens": ["Und", "leb\u00b7te", "von", "dem"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Doch bracht' ein ", "tokens": ["Doch", "bracht'", "ein"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.3": {"text": "Den ", "tokens": ["Den"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Was nach dem Tod' er einst gedachte dem zu geben,", "tokens": ["Was", "nach", "dem", "Tod'", "er", "einst", "ge\u00b7dach\u00b7te", "dem", "zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "PPER", "ADV", "VVFIN", "ART", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das legt' er selber an, und bracht' es in die Welt;", "tokens": ["Das", "legt'", "er", "sel\u00b7ber", "an", ",", "und", "bracht'", "es", "in", "die", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Er sagte, dass man m\u00fcss', ", "tokens": ["Er", "sag\u00b7te", ",", "dass", "man", "m\u00fcss'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PIS", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}