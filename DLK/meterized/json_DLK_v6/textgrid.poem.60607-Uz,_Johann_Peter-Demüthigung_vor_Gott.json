{"textgrid.poem.60607": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Dem\u00fcthigung vor Gott", "genre": "verse", "period": "N.A.", "pub_year": 1758, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Darf sich der arme Mensch erheben,", "tokens": ["Darf", "sich", "der", "ar\u00b7me", "Mensch", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vor dir, Allm\u00e4chtiger, der Staub?", "tokens": ["Vor", "dir", ",", "All\u00b7m\u00e4ch\u00b7ti\u00b7ger", ",", "der", "Staub", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ADJA", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bor dir, der alles ihm gegeben,", "tokens": ["Bor", "dir", ",", "der", "al\u00b7les", "ihm", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "PRELS", "PIS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Worauf er pocht, als einen Raub?", "tokens": ["Wo\u00b7rauf", "er", "pocht", ",", "als", "ei\u00b7nen", "Raub", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und kannst du ihm nicht wieder nehmen,", "tokens": ["Und", "kannst", "du", "ihm", "nicht", "wie\u00b7der", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was du ihm gabst, und er vergi\u00dft,", "tokens": ["Was", "du", "ihm", "gabst", ",", "und", "er", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Damit der stolze Staub sich sch\u00e4men", "tokens": ["Da\u00b7mit", "der", "stol\u00b7ze", "Staub", "sich", "sch\u00e4\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "ADJA", "NN", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und f\u00fchlen m\u00fcsse, was er ist?", "tokens": ["Und", "f\u00fch\u00b7len", "m\u00fcs\u00b7se", ",", "was", "er", "ist", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du ziehst dem aufgeblasnen Reichen", "tokens": ["Du", "ziehst", "dem", "auf\u00b7ge\u00b7blas\u00b7nen", "Rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Purpur seiner Hoheit aus,", "tokens": ["Den", "Pur\u00b7pur", "sei\u00b7ner", "Ho\u00b7heit", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und setzest ihn zu einem Zeichen,", "tokens": ["Und", "set\u00b7zest", "ihn", "zu", "ei\u00b7nem", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihn und sein \u00fcberm\u00fcthig Haus.", "tokens": ["Ihn", "und", "sein", "\u00fc\u00b7ber\u00b7m\u00fct\u00b7hig", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du hauchest rosenvolle Wangen", "tokens": ["Du", "hau\u00b7chest", "ro\u00b7sen\u00b7vol\u00b7le", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Am fr\u00fchen Morgen z\u00fcrnend an,", "tokens": ["Am", "fr\u00fc\u00b7hen", "Mor\u00b7gen", "z\u00fcr\u00b7nend", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und noch vor Abend ist vergangen,", "tokens": ["Und", "noch", "vor", "A\u00b7bend", "ist", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was tausend mit Entz\u00fccken sahn.", "tokens": ["Was", "tau\u00b7send", "mit", "Ent\u00b7z\u00fc\u00b7cken", "sahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "CARD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Das Leben steht in deinen H\u00e4nden:", "tokens": ["Das", "Le\u00b7ben", "steht", "in", "dei\u00b7nen", "H\u00e4n\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nach deinem Winke w\u00fcrgt der Tod;", "tokens": ["Nach", "dei\u00b7nem", "Win\u00b7ke", "w\u00fcrgt", "der", "Tod", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und w\u00fcrgt auch zwischen Marmorw\u00e4nden", "tokens": ["Und", "w\u00fcrgt", "auch", "zwi\u00b7schen", "Mar\u00b7mor\u00b7w\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und beym Altar, und eh er droht.", "tokens": ["Und", "beym", "Al\u00b7tar", ",", "und", "eh", "er", "droht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "$,", "KON", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Auch mich ruft einst die finstre H\u00f6hle!", "tokens": ["Auch", "mich", "ruft", "einst", "die", "finst\u00b7re", "H\u00f6h\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Doch bis die irdne H\u00fctte bricht,", "tokens": ["Doch", "bis", "die", "ird\u00b7ne", "H\u00fct\u00b7te", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Entzeuch, ich flehe, meiner Seele", "tokens": ["Ent\u00b7zeuch", ",", "ich", "fle\u00b7he", ",", "mei\u00b7ner", "See\u00b7le"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Funken deiner Gottheit nicht!", "tokens": ["Den", "Fun\u00b7ken", "dei\u00b7ner", "Got\u00b7theit", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Nimm, wenn ich eine Welt gew\u00f6nne,", "tokens": ["Nimm", ",", "wenn", "ich", "ei\u00b7ne", "Welt", "ge\u00b7w\u00f6n\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nimm nicht mein himmlisch Vorrecht hin,", "tokens": ["Nimm", "nicht", "mein", "himm\u00b7lisch", "Vor\u00b7recht", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "PPOSAT", "ADJD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich dich menschlich preisen k\u00f6nne,", "tokens": ["Da\u00df", "ich", "dich", "menschlich", "prei\u00b7sen", "k\u00f6n\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So lang ich unter Menschen bin!", "tokens": ["So", "lang", "ich", "un\u00b7ter", "Men\u00b7schen", "bin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Erhalte mir, was du gegeben!", "tokens": ["Er\u00b7hal\u00b7te", "mir", ",", "was", "du", "ge\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Denn diese Seele mit Verstand,", "tokens": ["Denn", "die\u00b7se", "See\u00b7le", "mit", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und was ich habe, Leib und Leben,", "tokens": ["Und", "was", "ich", "ha\u00b7be", ",", "Leib", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VAFIN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hab ich aus deiner Vaterhand.", "tokens": ["Hab", "ich", "aus", "dei\u00b7ner", "Va\u00b7ter\u00b7hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "So sing ich jeden neuen Morgen,", "tokens": ["So", "sing", "ich", "je\u00b7den", "neu\u00b7en", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So sing ich, wann die schwarze Nacht", "tokens": ["So", "sing", "ich", ",", "wann", "die", "schwar\u00b7ze", "Nacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Schauplatz der Natur verborgen,", "tokens": ["Den", "Schau\u00b7platz", "der", "Na\u00b7tur", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und nur das Heer des Himmels wacht.", "tokens": ["Und", "nur", "das", "Heer", "des", "Him\u00b7mels", "wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}