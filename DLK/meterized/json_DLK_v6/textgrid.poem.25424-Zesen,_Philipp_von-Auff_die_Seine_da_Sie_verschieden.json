{"textgrid.poem.25424": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Auff die Seine/ da Sie verschieden", "genre": "verse", "period": "N.A.", "pub_year": 1654, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du warst vorhin verwundt durch einen Pfeil der Liebe/", "tokens": ["Du", "warst", "vor\u00b7hin", "ver\u00b7wundt", "durch", "ei\u00b7nen", "Pfeil", "der", "Lie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "APPR", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und nun hat gar gef\u00e4llt der Pfeil des Todes dich/", "tokens": ["und", "nun", "hat", "gar", "ge\u00b7f\u00e4llt", "der", "Pfeil", "des", "To\u00b7des", "dich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADV", "VVPP", "ART", "NN", "ART", "NN", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O Adelheit mein Schatz; Die Sterne stehen tr\u00fcbe:", "tokens": ["O", "A\u00b7del\u00b7heit", "mein", "Schatz", ";", "Die", "Ster\u00b7ne", "ste\u00b7hen", "tr\u00fc\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PPOSAT", "NN", "$.", "ART", "NN", "VVFIN", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "weil du verletzet bist/ so bin Ich nicht mehr Ich.", "tokens": ["weil", "du", "ver\u00b7let\u00b7zet", "bist", "/", "so", "bin", "Ich", "nicht", "mehr", "Ich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "VAFIN", "$(", "ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich steh' im Zweifel noch/ wei\u00df nicht/ ob Liebe sey", "tokens": ["Ich", "steh'", "im", "Zwei\u00b7fel", "noch", "/", "wei\u00df", "nicht", "/", "ob", "Lie\u00b7be", "sey"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADV", "$(", "VVFIN", "PTKNEG", "$(", "KOUS", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Noch st\u00e4rcker als der Todt/ weil er das reisst entzwey", "tokens": ["Noch", "st\u00e4r\u00b7cker", "als", "der", "Todt", "/", "weil", "er", "das", "reisst", "ent\u00b7zwey"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "$(", "KOUS", "PPER", "PDS", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was treue Liebe bindet/", "tokens": ["Was", "treu\u00b7e", "Lie\u00b7be", "bin\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df Sie den Todt empfindet.", "tokens": ["Da\u00df", "Sie", "den", "Todt", "emp\u00b7fin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}