{"textgrid.poem.57236": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sch\u00f6n ist, Mutter Natur, deiner Erfindung Pracht", "genre": "verse", "period": "N.A.", "pub_year": 1750, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6n ist, Mutter Natur, deiner Erfindung Pracht", "tokens": ["Sch\u00f6n", "ist", ",", "Mut\u00b7ter", "Na\u00b7tur", ",", "dei\u00b7ner", "Er\u00b7fin\u00b7dung", "Pracht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VAFIN", "$,", "NN", "NN", "$,", "PPOSAT", "NN", "NN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Auf die Fluren verstreut, sch\u00f6ner ein froh Gesicht,", "tokens": ["Auf", "die", "Flu\u00b7ren", "ver\u00b7streut", ",", "sch\u00f6\u00b7ner", "ein", "froh", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "ADJA", "ART", "ADJD", "NN", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Das den grossen Gedanken", "tokens": ["Das", "den", "gros\u00b7sen", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Deiner Sch\u00f6pfung noch Einmal denkt.", "tokens": ["Dei\u00b7ner", "Sch\u00f6p\u00b7fung", "noch", "Ein\u00b7mal", "denkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Von des schimmernden Sees Traubengestaden her,", "tokens": ["Von", "des", "schim\u00b7mern\u00b7den", "Sees", "Trau\u00b7ben\u00b7ge\u00b7sta\u00b7den", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "PTKVZ", "$,"], "meter": "--+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Oder, flohest du schon wieder zum Himmel auf,", "tokens": ["O\u00b7der", ",", "flo\u00b7hest", "du", "schon", "wie\u00b7der", "zum", "Him\u00b7mel", "auf", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Kom in r\u00f6thendem Strale", "tokens": ["Kom", "in", "r\u00f6\u00b7then\u00b7dem", "Stra\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Auf dem Fl\u00fcgel der Abendluft,", "tokens": ["Auf", "dem", "Fl\u00fc\u00b7gel", "der", "A\u00b7ben\u00b7dluft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Kom, und lehre mein Lied jugendlich heiter seyn,", "tokens": ["Kom", ",", "und", "leh\u00b7re", "mein", "Lied", "ju\u00b7gend\u00b7lich", "hei\u00b7ter", "seyn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "ADJD", "ADJD", "VAINF", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "S\u00fcsse Freude, wie du! gleich dem beseelteren", "tokens": ["S\u00fcs\u00b7se", "Freu\u00b7de", ",", "wie", "du", "!", "gleich", "dem", "be\u00b7seel\u00b7te\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "$,", "PWAV", "PPER", "$.", "ADV", "ART", "ADJA"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Schnellen Jauchzen des J\u00fcnglings,", "tokens": ["Schnel\u00b7len", "Jauch\u00b7zen", "des", "J\u00fcng\u00b7lings", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Sanft, der f\u00fchlenden Fanny gleich.", "tokens": ["Sanft", ",", "der", "f\u00fch\u00b7len\u00b7den", "Fan\u00b7ny", "gleich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Schon lag hinter uns weit Uto, an dessen Fuss", "tokens": ["Schon", "lag", "hin\u00b7ter", "uns", "weit", "U\u00b7to", ",", "an", "des\u00b7sen", "Fuss"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ADJD", "NE", "$,", "APPR", "PRELAT", "NN"], "meter": "+-+-+-+-++-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Z\u00fcrch in ruhigem Thal freye Bewohner n\u00e4hrt;", "tokens": ["Z\u00fcrch", "in", "ru\u00b7hi\u00b7gem", "Thal", "frey\u00b7e", "Be\u00b7woh\u00b7ner", "n\u00e4hrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Schon war manches Gebirge", "tokens": ["Schon", "war", "man\u00b7ches", "Ge\u00b7bir\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Voll von Reben vorbeygeflohn.", "tokens": ["Voll", "von", "Re\u00b7ben", "vor\u00b7bey\u00b7ge\u00b7flohn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVIZU", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Jetzt entw\u00f6lkte sich fern silberner Alpen H\u00f6h,", "tokens": ["Jetzt", "ent\u00b7w\u00f6lk\u00b7te", "sich", "fern", "sil\u00b7ber\u00b7ner", "Al\u00b7pen", "H\u00f6h", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADJD", "ADJA", "NN", "NN", "$,"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und der J\u00fcnglinge Herz schlug schon empfindender,", "tokens": ["Und", "der", "J\u00fcng\u00b7lin\u00b7ge", "Herz", "schlug", "schon", "emp\u00b7fin\u00b7den\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+++-+-+-+-+", "measure": "unknown.measure.septa"}, "line.3": {"text": "Schon verrieth es beredter", "tokens": ["Schon", "ver\u00b7rieth", "es", "be\u00b7red\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sich der sch\u00f6nen Begleiterin.", "tokens": ["Sich", "der", "sch\u00f6\u00b7nen", "Be\u00b7glei\u00b7te\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "\u00bbhallers Doris,\u00ab die sang, selber des Liedes werth,", "tokens": ["\u00bb", "hal\u00b7lers", "Do\u00b7ris", ",", "\u00ab", "die", "sang", ",", "sel\u00b7ber", "des", "Lie\u00b7des", "werth", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NE", "$,", "$(", "ART", "ADJD", "$,", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Hirzels Daphne, den Kleist innig wie Gleimen liebt;", "tokens": ["Hir\u00b7zels", "Daph\u00b7ne", ",", "den", "Kleist", "in\u00b7nig", "wie", "Glei\u00b7men", "liebt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "NN", "ADJD", "KOKOM", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Und wir J\u00fcnglinge sangen,", "tokens": ["Und", "wir", "J\u00fcng\u00b7lin\u00b7ge", "san\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "VVFIN", "$,"], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Und empfanden, wie Hagedorn,", "tokens": ["Und", "emp\u00b7fan\u00b7den", ",", "wie", "Ha\u00b7ge\u00b7dorn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "NE", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Jetzo nahm uns die Au in die beschattenden", "tokens": ["Jet\u00b7zo", "nahm", "uns", "die", "Au", "in", "die", "be\u00b7schat\u00b7ten\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "K\u00fchlen Arme des Walds, welcher die Insel kr\u00f6nt;", "tokens": ["K\u00fch\u00b7len", "Ar\u00b7me", "des", "Walds", ",", "wel\u00b7cher", "die", "In\u00b7sel", "kr\u00f6nt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Da, da kamest du, Freude!", "tokens": ["Da", ",", "da", "ka\u00b7mest", "du", ",", "Freu\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Volles Masses auf uns herab!", "tokens": ["Vol\u00b7les", "Mas\u00b7ses", "auf", "uns", "her\u00b7ab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPER", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.8": {"line.1": {"text": "G\u00f6ttin Freude, du selbst! dich, wir empfanden dich!", "tokens": ["G\u00f6t\u00b7tin", "Freu\u00b7de", ",", "du", "selbst", "!", "dich", ",", "wir", "emp\u00b7fan\u00b7den", "dich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "ADV", "$.", "PPER", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ja, du warest es selbst, Schwester der Menschlichkeit,", "tokens": ["Ja", ",", "du", "wa\u00b7rest", "es", "selbst", ",", "Schwes\u00b7ter", "der", "Menschlich\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "PPER", "ADV", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+--++", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Deiner Unschuld Gespielin,", "tokens": ["Dei\u00b7ner", "Un\u00b7schuld", "Ge\u00b7spie\u00b7lin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Die sich \u00fcber uns ganz ergoss!", "tokens": ["Die", "sich", "\u00fc\u00b7ber", "uns", "ganz", "er\u00b7goss", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPER", "ADV", "ADJD", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.9": {"line.1": {"text": "S\u00fcss ist, fr\u00f6hlicher Lenz, deiner Begeistrung Hauch,", "tokens": ["S\u00fcss", "ist", ",", "fr\u00f6h\u00b7li\u00b7cher", "Lenz", ",", "dei\u00b7ner", "Be\u00b7geis\u00b7trung", "Hauch", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "$,", "ADJA", "NN", "$,", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wenn die Flur dich gebiert, wenn sich dein Odem sanft", "tokens": ["Wenn", "die", "Flur", "dich", "ge\u00b7biert", ",", "wenn", "sich", "dein", "O\u00b7dem", "sanft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "VVPP", "$,", "KOUS", "PRF", "PPOSAT", "NN", "ADJD"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "In der J\u00fcnglinge Herzen,", "tokens": ["In", "der", "J\u00fcng\u00b7lin\u00b7ge", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Und die Herzen der M\u00e4dchen giesst.", "tokens": ["Und", "die", "Her\u00b7zen", "der", "M\u00e4d\u00b7chen", "giesst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.10": {"line.1": {"text": "Ach du machst das Gef\u00fchl siegend, es steigt durch dich", "tokens": ["Ach", "du", "machst", "das", "Ge\u00b7f\u00fchl", "sie\u00b7gend", ",", "es", "steigt", "durch", "dich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "PPER", "VVFIN", "ART", "NN", "VVPP", "$,", "PPER", "VVFIN", "APPR", "PPER"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Jede bl\u00fchende Brust sch\u00f6ner, und bebender,", "tokens": ["Je\u00b7de", "bl\u00fc\u00b7hen\u00b7de", "Brust", "sch\u00f6\u00b7ner", ",", "und", "be\u00b7ben\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ADJA", "$,", "KON", "ADJD", "$,"], "meter": "+-+--++-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Lauter redet der Liebe", "tokens": ["Lau\u00b7ter", "re\u00b7det", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Nun entzauberter Mund durch dich!", "tokens": ["Nun", "ent\u00b7zau\u00b7ber\u00b7ter", "Mund", "durch", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.11": {"line.1": {"text": "Lieblich winket der Wein, wenn er Empfindungen,", "tokens": ["Lieb\u00b7lich", "win\u00b7ket", "der", "Wein", ",", "wenn", "er", "Emp\u00b7fin\u00b7dun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "NN", "$,"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Bessre sanftere Lust, wenn er Gedauken weckt", "tokens": ["Bess\u00b7re", "sanf\u00b7te\u00b7re", "Lust", ",", "wenn", "er", "Ge\u00b7dau\u00b7ken", "weckt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "$,", "KOUS", "PPER", "NN", "VVFIN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Im sokratischen Becher", "tokens": ["Im", "sok\u00b7ra\u00b7ti\u00b7schen", "Be\u00b7cher"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von der thauenden Ros' umkr\u00e4nzt;", "tokens": ["Von", "der", "thau\u00b7en\u00b7den", "Ros'", "um\u00b7kr\u00e4nzt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.12": {"line.1": {"text": "Wenn er dringt bis ins Herz, und zu Entschliessungen,", "tokens": ["Wenn", "er", "dringt", "bis", "ins", "Herz", ",", "und", "zu", "Ent\u00b7schlies\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$,", "KON", "APPR", "NN", "$,"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Die der S\u00e4ufer verkennt, jeden Gedanken weckt,", "tokens": ["Die", "der", "S\u00e4u\u00b7fer", "ver\u00b7kennt", ",", "je\u00b7den", "Ge\u00b7dan\u00b7ken", "weckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,", "PIAT", "NN", "VVFIN", "$,"], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wenn er lehret verachten,", "tokens": ["Wenn", "er", "leh\u00b7ret", "ver\u00b7ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Was nicht w\u00fcrdig des Weisen ist.", "tokens": ["Was", "nicht", "w\u00fcr\u00b7dig", "des", "Wei\u00b7sen", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ADJD", "ART", "NN", "VAFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.13": {"line.1": {"text": "Reizvoll klinget des Ruhms lockender Silberton", "tokens": ["Reiz\u00b7voll", "klin\u00b7get", "des", "Ruhms", "lo\u00b7cken\u00b7der", "Sil\u00b7ber\u00b7ton"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "In das schlagende Herz, und die Unsterblichkeit", "tokens": ["In", "das", "schla\u00b7gen\u00b7de", "Herz", ",", "und", "die", "U\u00b7nsterb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KON", "ART", "NN"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ist ein grosser Gedanke,", "tokens": ["Ist", "ein", "gros\u00b7ser", "Ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Ist des Schweisses der Edlen werth!", "tokens": ["Ist", "des", "Schweis\u00b7ses", "der", "Ed\u00b7len", "werth", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.14": {"line.1": {"text": "Durch der Lieder Gewalt, bey der Urenkelin", "tokens": ["Durch", "der", "Lie\u00b7der", "Ge\u00b7walt", ",", "bey", "der", "Ur\u00b7en\u00b7ke\u00b7lin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Sohn und Tochter noch seyn; mit der Entz\u00fcckung Ton", "tokens": ["Sohn", "und", "Toch\u00b7ter", "noch", "seyn", ";", "mit", "der", "Ent\u00b7z\u00fc\u00b7ckung", "Ton"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "ADV", "VAINF", "$.", "APPR", "ART", "NN", "NN"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Oft beym Namen, genennet,", "tokens": ["Oft", "beym", "Na\u00b7men", ",", "ge\u00b7nen\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "VVPP", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.4": {"text": "Oft gerufen vom Grabe her,", "tokens": ["Oft", "ge\u00b7ru\u00b7fen", "vom", "Gra\u00b7be", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.15": {"line.1": {"text": "Dann ihr sanfteres Herz bilden, und, Liebe, dich,", "tokens": ["Dann", "ihr", "sanf\u00b7te\u00b7res", "Herz", "bil\u00b7den", ",", "und", ",", "Lie\u00b7be", ",", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVINF", "$,", "KON", "$,", "NN", "$,", "PPER", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Fromme Tugend, dich auch giessen ins sanfte Herz,", "tokens": ["From\u00b7me", "Tu\u00b7gend", ",", "dich", "auch", "gies\u00b7sen", "ins", "sanf\u00b7te", "Herz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "ADV", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Ist, beym Himmel! nicht wenig!", "tokens": ["Ist", ",", "beym", "Him\u00b7mel", "!", "nicht", "we\u00b7nig", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "APPRART", "NN", "$.", "PTKNEG", "ADV", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Ist des Schweisses der Edlen werth!", "tokens": ["Ist", "des", "Schweis\u00b7ses", "der", "Ed\u00b7len", "werth", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.16": {"line.1": {"text": "Aber s\u00fcsser ist noch, sch\u00f6ner und reizender,", "tokens": ["A\u00b7ber", "s\u00fcs\u00b7ser", "ist", "noch", ",", "sch\u00f6\u00b7ner", "und", "rei\u00b7zen\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ADV", "$,", "ADJA", "KON", "ADJD", "$,"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "In dem Arme des Freunds wissen ein Freund zu seyn!", "tokens": ["In", "dem", "Ar\u00b7me", "des", "Freunds", "wis\u00b7sen", "ein", "Freund", "zu", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "So das Leben geniessen,", "tokens": ["So", "das", "Le\u00b7ben", "ge\u00b7nies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Nicht unw\u00fcrdig der Ewigkeit!", "tokens": ["Nicht", "un\u00b7w\u00fcr\u00b7dig", "der", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.17": {"line.1": {"text": "Treuer Z\u00e4rtlichkeit voll, in den Umschattungen,", "tokens": ["Treu\u00b7er", "Z\u00e4rt\u00b7lich\u00b7keit", "voll", ",", "in", "den", "Um\u00b7schat\u00b7tun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+++-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "In den L\u00fcften des Walds, und mit gesenktem Blick", "tokens": ["In", "den", "L\u00fcf\u00b7ten", "des", "Walds", ",", "und", "mit", "ge\u00b7senk\u00b7tem", "Blick"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "KON", "APPR", "ADJA", "NN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Auf die silberne Welle,", "tokens": ["Auf", "die", "sil\u00b7ber\u00b7ne", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "That ich schweigend den frommen Wunsch:", "tokens": ["That", "ich", "schwei\u00b7gend", "den", "from\u00b7men", "Wunsch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.18": {"line.1": {"text": "W\u00e4ret ihr auch bey uns, die ihr mich ferne liebt,", "tokens": ["W\u00e4\u00b7ret", "ihr", "auch", "bey", "uns", ",", "die", "ihr", "mich", "fer\u00b7ne", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PPER", "$,", "PRELS", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "In des Vaterlands Schooss einsam von mir verstreut,", "tokens": ["In", "des", "Va\u00b7ter\u00b7lands", "Scho\u00b7oss", "ein\u00b7sam", "von", "mir", "ver\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "--+--+-+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Die in seligen Stunden", "tokens": ["Die", "in", "se\u00b7li\u00b7gen", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Meine suchende Seele fand;", "tokens": ["Mei\u00b7ne", "su\u00b7chen\u00b7de", "See\u00b7le", "fand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.19": {"line.1": {"text": "O so bauten wir hier H\u00fctten der Freundschaft uns!", "tokens": ["O", "so", "bau\u00b7ten", "wir", "hier", "H\u00fct\u00b7ten", "der", "Freund\u00b7schaft", "uns", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PPER", "ADV", "NN", "ART", "NN", "PPER", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ewig wohnten wir hier, ewig! Der Schattenwald", "tokens": ["E\u00b7wig", "wohn\u00b7ten", "wir", "hier", ",", "e\u00b7wig", "!", "Der", "Schat\u00b7ten\u00b7wald"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ADJD", "$.", "ART", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Wandelt' uns sich in Tempe,", "tokens": ["Wan\u00b7delt'", "uns", "sich", "in", "Tem\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "NE", "$,"], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Jenes Thal in Elysium!", "tokens": ["Je\u00b7nes", "Thal", "in", "E\u00b7ly\u00b7si\u00b7um", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "NE", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}