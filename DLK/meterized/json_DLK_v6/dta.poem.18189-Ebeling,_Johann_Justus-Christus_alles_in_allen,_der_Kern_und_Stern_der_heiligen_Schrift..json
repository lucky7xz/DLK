{"dta.poem.18189": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Christus alles in allen, der Kern  \n und Stern der heiligen Schrift.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198805", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Menschen Wiz schweift aus den", "tokens": ["Der", "Men\u00b7schen", "Wiz", "schweift", "aus", "den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schranken,", "tokens": ["Schran\u00b7ken", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Und daher kommt es da\u00df sie wan-", "tokens": ["Und", "da\u00b7her", "kommt", "es", "da\u00df", "sie", "wan"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VVFIN", "PPER", "KOUS", "PPER", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ken", "tokens": ["ken"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "In Sachen ihrer Seeligkeit:", "tokens": ["In", "Sa\u00b7chen", "ih\u00b7rer", "See\u00b7lig\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Woher entsteht so vieler Streit,", "tokens": ["Wo\u00b7her", "ent\u00b7steht", "so", "vie\u00b7ler", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In Dingen die wir gl\u00e4uben sollen?", "tokens": ["In", "Din\u00b7gen", "die", "wir", "gl\u00e4u\u00b7ben", "sol\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Daher, weil wirs begreiffen wol-", "tokens": ["Da\u00b7her", ",", "weil", "wirs", "be\u00b7greif\u00b7fen", "wol"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "$,", "KOUS", "PIS", "VVINF", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "len.", "tokens": ["len", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-", "measure": "single.down"}, "line.10": {"text": "Wir haben ", "tokens": ["Wir", "ha\u00b7ben"], "token_info": ["word", "word"], "pos": ["PPER", "VAFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Worin man nichts, als wahr antrift.", "tokens": ["Wo\u00b7rin", "man", "nichts", ",", "als", "wahr", "an\u00b7trift", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PIS", "$,", "KOUS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Sie ist ein Licht, das kl\u00e4rlich zeiget,", "tokens": ["Sie", "ist", "ein", "Licht", ",", "das", "kl\u00e4r\u00b7lich", "zei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Das, was uns die Vernunft verschweiget:", "tokens": ["Das", ",", "was", "uns", "die", "Ver\u00b7nunft", "ver\u00b7schwei\u00b7get", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Wer dieses sich zum Grunde sezt,", "tokens": ["Wer", "die\u00b7ses", "sich", "zum", "Grun\u00b7de", "sezt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Der bleibt im Glauben unverlezt.", "tokens": ["Der", "bleibt", "im", "Glau\u00b7ben", "un\u00b7ver\u00b7lezt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Wie viele sind, die dies Buch kennen,", "tokens": ["Wie", "vie\u00b7le", "sind", ",", "die", "dies", "Buch", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "$,", "PRELS", "PDS", "NN", "VVINF", "$,"], "meter": "-+-++-+--", "measure": "unknown.measure.tetra"}, "line.17": {"text": "Es ", "tokens": ["Es"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.18": {"text": "Die doch im Glauben ungewis;", "tokens": ["Die", "doch", "im", "Glau\u00b7ben", "un\u00b7ge\u00b7wis", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Wo her kommt diese Finsternis?", "tokens": ["Wo", "her", "kommt", "die\u00b7se", "Fins\u00b7ter\u00b7nis", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APZR", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Bei denen, die vom Glauben prahlen?", "tokens": ["Bei", "de\u00b7nen", ",", "die", "vom", "Glau\u00b7ben", "prah\u00b7len", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "$,", "PRELS", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Daher sie lieben nur die Schalen.", "tokens": ["Da\u00b7her", "sie", "lie\u00b7ben", "nur", "die", "Scha\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Wer nicht den Kern der Bibel sucht,", "tokens": ["Wer", "nicht", "den", "Kern", "der", "Bi\u00b7bel", "sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Der findet darin wenig Frucht:", "tokens": ["Der", "fin\u00b7det", "da\u00b7rin", "we\u00b7nig", "Frucht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PAV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Wer aber darnach em\u00dfig schmachtet,", "tokens": ["Wer", "a\u00b7ber", "dar\u00b7nach", "em\u00b7\u00dfig", "schmach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PAV", "ADJD", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.25": {"text": "Der Bibel wahren Zwek betrachtet,", "tokens": ["Der", "Bi\u00b7bel", "wah\u00b7ren", "Zwek", "be\u00b7trach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Der sucht in einen jeden Buch", "tokens": ["Der", "sucht", "in", "ei\u00b7nen", "je\u00b7den", "Buch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ART", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Den Heiland, das ist ihm genug:", "tokens": ["Den", "Hei\u00b7land", ",", "das", "ist", "ihm", "ge\u00b7nug", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VAFIN", "PPER", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.28": {"text": "Der ist der Kern, wer dieses gl\u00e4ubet,", "tokens": ["Der", "ist", "der", "Kern", ",", "wer", "die\u00b7ses", "gl\u00e4u\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PWS", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Und bei dem klaren Worte bleibet,", "tokens": ["Und", "bei", "dem", "kla\u00b7ren", "Wor\u00b7te", "blei\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Der sagt: D", "tokens": ["Der", "sagt", ":", "D"], "token_info": ["word", "word", "punct", "word"], "pos": ["PDS", "VVFIN", "$.", "XY"], "meter": "+-+", "measure": "trochaic.di"}, "line.31": {"text": "Mein Leitstern zu dem Heiland sein,", "tokens": ["Mein", "Leits\u00b7tern", "zu", "dem", "Hei\u00b7land", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Den will ich suchen, kennen, ehren,", "tokens": ["Den", "will", "ich", "su\u00b7chen", ",", "ken\u00b7nen", ",", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "VVINF", "$,", "VVFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "In seinen Wort sind Warheitslehren,", "tokens": ["In", "sei\u00b7nen", "Wort", "sind", "War\u00b7heits\u00b7leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Die nehm ich zum Wegweiser an,", "tokens": ["Die", "nehm", "ich", "zum", "Weg\u00b7wei\u00b7ser", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Den folg ich auf der Lebensbahn:", "tokens": ["Den", "folg", "ich", "auf", "der", "Le\u00b7bens\u00b7bahn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "So kann mich nicht der Wiz verderben,", "tokens": ["So", "kann", "mich", "nicht", "der", "Wiz", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Drauf leb ich, darauf will ich sterben,", "tokens": ["Drauf", "leb", "ich", ",", "da\u00b7rauf", "will", "ich", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "PAV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.38": {"text": "Kommt denn, das Ende meiner Zeit,", "tokens": ["Kommt", "denn", ",", "das", "En\u00b7de", "mei\u00b7ner", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "So folgt die frohe Ewigkeit.", "tokens": ["So", "folgt", "die", "fro\u00b7he", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}