{"textgrid.poem.38588": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An den Abendstern", "genre": "verse", "period": "N.A.", "pub_year": 1785, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ruhig blinkt aus wolkenloser Ferne", "tokens": ["Wie", "ru\u00b7hig", "blinkt", "aus", "wol\u00b7ken\u00b7lo\u00b7ser", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein sch\u00f6nes Licht, du freundlichster der Sterne,", "tokens": ["Dein", "sch\u00f6\u00b7nes", "Licht", ",", "du", "freund\u00b7lichs\u00b7ter", "der", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPER", "ADJA", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie lieblich wallt im See dein zitternd Bild!", "tokens": ["Wie", "lieb\u00b7lich", "wallt", "im", "See", "dein", "zit\u00b7ternd", "Bild", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "APPRART", "NN", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie oft hast du, wenn ich, vom West umf\u00e4chelt,", "tokens": ["Wie", "oft", "hast", "du", ",", "wenn", "ich", ",", "vom", "West", "um\u00b7f\u00e4\u00b7chelt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "$,", "KOUS", "PPER", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Im Gr\u00fcnen lag, mir Seelenruh' gel\u00e4chelt,", "tokens": ["Im", "Gr\u00fc\u00b7nen", "lag", ",", "mir", "See\u00b7len\u00b7ruh'", "ge\u00b7l\u00e4\u00b7chelt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie oft mit hoher Ahndung mich erf\u00fcllt!", "tokens": ["Wie", "oft", "mit", "ho\u00b7her", "Ahn\u00b7dung", "mich", "er\u00b7f\u00fcllt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ist's Mitleid, was dein sanftes Auge tr\u00fcbte?", "tokens": ["Ist's", "Mit\u00b7leid", ",", "was", "dein", "sanf\u00b7tes", "Au\u00b7ge", "tr\u00fcb\u00b7te", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Allen fern die meine Seele liebte", "tokens": ["Von", "Al\u00b7len", "fern", "die", "mei\u00b7ne", "See\u00b7le", "lieb\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJD", "ART", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wall' ich des Lebens dunkle Bahn hinab!", "tokens": ["Wall'", "ich", "des", "Le\u00b7bens", "dunk\u00b7le", "Bahn", "hin\u00b7ab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wann wird der Schwermuth tr\u00fcbe D\u00e4mmrung tagen?", "tokens": ["Wann", "wird", "der", "Schwer\u00b7muth", "tr\u00fc\u00b7be", "D\u00e4mm\u00b7rung", "ta\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ach! wann verhallt die lezte meiner Klagen?", "tokens": ["Ach", "!", "wann", "ver\u00b7hallt", "die", "lez\u00b7te", "mei\u00b7ner", "Kla\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VVFIN", "ART", "ADJA", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wann blickst du auf mein unbethr\u00e4ntes Grab?", "tokens": ["Wann", "blickst", "du", "auf", "mein", "un\u00b7be\u00b7thr\u00e4n\u00b7tes", "Grab", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}