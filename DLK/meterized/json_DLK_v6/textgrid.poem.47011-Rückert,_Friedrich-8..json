{"textgrid.poem.47011": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du magst doch sonst gern was besonders haben,", "tokens": ["Du", "magst", "doch", "sonst", "gern", "was", "be\u00b7son\u00b7ders", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ADV", "PWS", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Magst gerne, wenn die andern in dem Pfuhle", "tokens": ["Magst", "ger\u00b7ne", ",", "wenn", "die", "an\u00b7dern", "in", "dem", "Pfuh\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "$,", "KOUS", "ART", "ADJA", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Wirtschaft w\u00fchlen, sitzen auf dem Stuhle,", "tokens": ["Der", "Wirt\u00b7schaft", "w\u00fch\u00b7len", ",", "sit\u00b7zen", "auf", "dem", "Stuh\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und etwa stricken, wenn die andern graben.", "tokens": ["Und", "et\u00b7wa", "stri\u00b7cken", ",", "wenn", "die", "an\u00b7dern", "gra\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "$,", "KOUS", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sprich, kann's denn nicht dein eitles Herzchen laben,", "tokens": ["Sprich", ",", "kann's", "denn", "nicht", "dein", "eit\u00b7les", "Herz\u00b7chen", "la\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "VMFIN", "ADV", "PTKNEG", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df dir auch werde ein besondrer Buhle,", "tokens": ["Da\u00df", "dir", "auch", "wer\u00b7de", "ein", "be\u00b7sond\u00b7rer", "Buh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem Zufall und Geschick von ihrer Spule", "tokens": ["Dem", "Zu\u00b7fall", "und", "Ge\u00b7schick", "von", "ih\u00b7rer", "Spu\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "'nen feinern Rock als hier den andern gaben?", "tokens": ["'nen", "fei\u00b7nern", "Rock", "als", "hier", "den", "an\u00b7dern", "ga\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KOKOM", "ADV", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und ist der Rock dir so verha\u00dft, der feine,", "tokens": ["Und", "ist", "der", "Rock", "dir", "so", "ver\u00b7ha\u00dft", ",", "der", "fei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PPER", "ADV", "ADJD", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So will ich unterm Rock das Herz dir weisen;", "tokens": ["So", "will", "ich", "un\u00b7term", "Rock", "das", "Herz", "dir", "wei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPRART", "NN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nimm hin und gib daf\u00fcr ein Sch\u00e4ferwammes!", "tokens": ["Nimm", "hin", "und", "gib", "da\u00b7f\u00fcr", "ein", "Sch\u00e4\u00b7fer\u00b7wam\u00b7mes", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "KON", "VVIMP", "PAV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein t\u00fccht'ger Sch\u00e4fer m\u00fc\u00dft' ich sein, ich meine,", "tokens": ["Ein", "t\u00fccht'\u00b7ger", "Sch\u00e4\u00b7fer", "m\u00fc\u00dft'", "ich", "sein", ",", "ich", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "VAINF", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und mit dem Blick wollt' ich den Wolf zerrei\u00dfen,", "tokens": ["Und", "mit", "dem", "Blick", "wollt'", "ich", "den", "Wolf", "zer\u00b7rei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VMFIN", "PPER", "ART", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der dein begehrte, meines einzigen Lammes.", "tokens": ["Der", "dein", "be\u00b7gehr\u00b7te", ",", "mei\u00b7nes", "ein\u00b7zi\u00b7gen", "Lam\u00b7mes", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}