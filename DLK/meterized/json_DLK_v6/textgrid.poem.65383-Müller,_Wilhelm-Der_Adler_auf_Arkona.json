{"textgrid.poem.65383": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Adler auf Arkona", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf Arkona's Berge", "tokens": ["Auf", "Ar\u00b7ko\u00b7na's", "Ber\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ist ein Adlerhorst,", "tokens": ["Ist", "ein", "Ad\u00b7ler\u00b7horst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wo vom Schlag der Woge", "tokens": ["Wo", "vom", "Schlag", "der", "Wo\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Seine Spitze borst.", "tokens": ["Sei\u00b7ne", "Spit\u00b7ze", "borst", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Spitze deutschen Landes,", "tokens": ["Spit\u00b7ze", "deut\u00b7schen", "Lan\u00b7des", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Willst sein Bild du sein?", "tokens": ["Willst", "sein", "Bild", "du", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PPER", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Riss' und Spalten splittern", "tokens": ["Riss'", "und", "Spal\u00b7ten", "split\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KON", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Deinen festen Stein.", "tokens": ["Dei\u00b7nen", "fes\u00b7ten", "Stein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Adler, setz' dich oben", "tokens": ["Ad\u00b7ler", ",", "setz'", "dich", "o\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Auf den Felsenthron,", "tokens": ["Auf", "den", "Fel\u00b7sen\u00b7thron", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Deutschen Landes H\u00fcter,", "tokens": ["Deut\u00b7schen", "Lan\u00b7des", "H\u00fc\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Freier Wolkensohn!", "tokens": ["Frei\u00b7er", "Wol\u00b7ken\u00b7sohn", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Schau' hinaus nach Morgen,", "tokens": ["Schau'", "hin\u00b7aus", "nach", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APZR", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Schau' nach Mitternacht,", "tokens": ["Schau'", "nach", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Schaue gegen Abend", "tokens": ["Schau\u00b7e", "ge\u00b7gen", "A\u00b7bend"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Von der hohen Wacht!", "tokens": ["Von", "der", "ho\u00b7hen", "Wacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Lie\u00df der deutsche Kaiser", "tokens": ["Lie\u00df", "der", "deut\u00b7sche", "Kai\u00b7ser"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Fliegen dich zugleich,", "tokens": ["Flie\u00b7gen", "dich", "zu\u00b7gleich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Als er brach in St\u00fccke,", "tokens": ["Als", "er", "brach", "in", "St\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ach, das deutsche Reich?", "tokens": ["Ach", ",", "das", "deut\u00b7sche", "Reich", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "H\u00fcte, deutscher Adler,", "tokens": ["H\u00fc\u00b7te", ",", "deut\u00b7scher", "Ad\u00b7ler", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Deutsches Volk und Land,", "tokens": ["Deut\u00b7sches", "Volk", "und", "Land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Deutsche Sitt' und Zunge,", "tokens": ["Deut\u00b7sche", "Sitt'", "und", "Zun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Deutsche Stirn und Hand!", "tokens": ["Deut\u00b7sche", "Stirn", "und", "Hand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}