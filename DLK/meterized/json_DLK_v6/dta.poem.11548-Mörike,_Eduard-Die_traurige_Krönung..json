{"dta.poem.11548": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Die traurige Kr\u00f6nung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Es war ein K\u00f6nig Milesint,", "tokens": ["Es", "war", "ein", "K\u00f6\u00b7nig", "Mi\u00b7le\u00b7sint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von dem will ich euch sagen;", "tokens": ["Von", "dem", "will", "ich", "euch", "sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Der meuchelte sein Bruders-Kind,", "tokens": ["Der", "meu\u00b7chel\u00b7te", "sein", "Bru\u00b7der\u00b7sKind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wollte selbst die Krone tragen.", "tokens": ["Woll\u00b7te", "selbst", "die", "Kro\u00b7ne", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die Kr\u00f6nung ward mit Prangen", "tokens": ["Die", "Kr\u00f6\u00b7nung", "ward", "mit", "Pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Auf Liffey-Schlo\u00df begangen.", "tokens": ["Auf", "Lif\u00b7fey\u00b7Schlo\u00df", "be\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "O Irland! Irland! warest du so blind?", "tokens": ["O", "Ir\u00b7land", "!", "Ir\u00b7land", "!", "wa\u00b7rest", "du", "so", "blind", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "NE", "$.", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der K\u00f6nig sizt um Mitternacht", "tokens": ["Der", "K\u00f6\u00b7nig", "sizt", "um", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im \u00f6den Marmorsaale,", "tokens": ["Im", "\u00f6\u00b7den", "Mar\u00b7mor\u00b7saa\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er freut sich seiner neuen Pracht", "tokens": ["Er", "freut", "sich", "sei\u00b7ner", "neu\u00b7en", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Beim einsamen Pokale;", "tokens": ["Beim", "ein\u00b7sa\u00b7men", "Po\u00b7ka\u00b7le", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Er spricht zu seinem Sohne:", "tokens": ["Er", "spricht", "zu", "sei\u00b7nem", "Soh\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "\u201enoch einmal bring' die Krone!", "tokens": ["\u201e", "noch", "ein\u00b7mal", "bring'", "die", "Kro\u00b7ne", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Doch schau, wer hat die Pforten aufgemacht?\u201c", "tokens": ["Doch", "schau", ",", "wer", "hat", "die", "Pfor\u00b7ten", "auf\u00b7ge\u00b7macht", "?", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "$,", "PWS", "VAFIN", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da kommt ein seltsam Todtenspiel,", "tokens": ["Da", "kommt", "ein", "selt\u00b7sam", "Tod\u00b7ten\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Zug mit leisen Tritten,", "tokens": ["Ein", "Zug", "mit", "lei\u00b7sen", "Trit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vermummte G\u00e4ste gro\u00df und viel,", "tokens": ["Ver\u00b7mumm\u00b7te", "G\u00e4s\u00b7te", "gro\u00df", "und", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Eine Krone schwankt in Mitten;", "tokens": ["Ei\u00b7ne", "Kro\u00b7ne", "schwankt", "in", "Mit\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Es dr\u00e4ngt sich durch die Pforte", "tokens": ["Es", "dr\u00e4ngt", "sich", "durch", "die", "Pfor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit Fl\u00fcstern ohne Worte;", "tokens": ["Mit", "Fl\u00fcs\u00b7tern", "oh\u00b7ne", "Wor\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dem K\u00f6nige, dem wird so geisterschw\u00fcl.", "tokens": ["Dem", "K\u00f6\u00b7ni\u00b7ge", ",", "dem", "wird", "so", "geis\u00b7ter\u00b7schw\u00fcl", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und aus der schwarzen Menge blickt", "tokens": ["Und", "aus", "der", "schwar\u00b7zen", "Men\u00b7ge", "blickt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Kind mit frischer Wunde,", "tokens": ["Ein", "Kind", "mit", "fri\u00b7scher", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es l\u00e4chelt sterbensweh und nickt,", "tokens": ["Es", "l\u00e4\u00b7chelt", "ster\u00b7bens\u00b7weh", "und", "nickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es macht im Saal die Runde,", "tokens": ["Es", "macht", "im", "Saal", "die", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Es trippelt zu dem Throne,", "tokens": ["Es", "trip\u00b7pelt", "zu", "dem", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es reichet eine Krone", "tokens": ["Es", "rei\u00b7chet", "ei\u00b7ne", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dem K\u00f6nige, de\u00df Herze tief erschrickt.", "tokens": ["Dem", "K\u00f6\u00b7ni\u00b7ge", ",", "de\u00df", "Her\u00b7ze", "tief", "er\u00b7schrickt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Darauf der Zug von dannen strich,", "tokens": ["Da\u00b7rauf", "der", "Zug", "von", "dan\u00b7nen", "strich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "APPR", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Morgenluft berauschet;", "tokens": ["Von", "Mor\u00b7gen\u00b7luft", "be\u00b7rau\u00b7schet", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Kerzen flackern wunderlich,", "tokens": ["Die", "Ker\u00b7zen", "fla\u00b7ckern", "wun\u00b7der\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Mond am Fenster lauschet;", "tokens": ["Der", "Mond", "am", "Fens\u00b7ter", "lau\u00b7schet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Sohn mit Angst und Schweigen", "tokens": ["Der", "Sohn", "mit", "Angst", "und", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zum Vater th\u00e4t sich neigen, \u2014", "tokens": ["Zum", "Va\u00b7ter", "th\u00e4t", "sich", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PRF", "VVFIN", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Er neiget \u00fcber eine Leiche sich.", "tokens": ["Er", "nei\u00b7get", "\u00fc\u00b7ber", "ei\u00b7ne", "Lei\u00b7che", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PRF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}