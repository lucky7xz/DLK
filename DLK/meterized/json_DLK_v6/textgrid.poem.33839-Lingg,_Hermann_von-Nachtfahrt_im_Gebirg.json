{"textgrid.poem.33839": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Nachtfahrt im Gebirg", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dunkle Felsw\u00e4nde die Bergh\u00f6h'n entlang,", "tokens": ["Dunk\u00b7le", "Fels\u00b7w\u00e4n\u00b7de", "die", "Berg\u00b7h\u00f6h'n", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+---+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Taleinw\u00e4rts fuhren wir, es zogen", "tokens": ["Ta\u00b7lein\u00b7w\u00e4rts", "fuh\u00b7ren", "wir", ",", "es", "zo\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Nebel mit uns in hellen Wogen,", "tokens": ["Die", "Ne\u00b7bel", "mit", "uns", "in", "hel\u00b7len", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein wildes Heer, das sich auf und nieder schwang,", "tokens": ["Ein", "wil\u00b7des", "Heer", ",", "das", "sich", "auf", "und", "nie\u00b7der", "schwang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PRF", "PTKVZ", "KON", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ein Meer, das mit den L\u00fcften rang.", "tokens": ["Ein", "Meer", ",", "das", "mit", "den", "L\u00fcf\u00b7ten", "rang", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch reingezackte Gipfel hoben", "tokens": ["Doch", "rein\u00b7ge\u00b7zack\u00b7te", "Gip\u00b7fel", "ho\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Licht des Mondes sich hervor,", "tokens": ["Im", "Licht", "des", "Mon\u00b7des", "sich", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vom herrlichsten Blau der Nacht umwoben,", "tokens": ["Vom", "herr\u00b7lichs\u00b7ten", "Blau", "der", "Nacht", "um\u00b7wo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und dar\u00fcber flog im Schleierflor", "tokens": ["Und", "da\u00b7r\u00fc\u00b7ber", "flog", "im", "Schlei\u00b7er\u00b7flor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VVFIN", "APPRART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Sein silbern Antlitz. Es tauchten", "tokens": ["Sein", "sil\u00b7bern", "Ant\u00b7litz", ".", "Es", "tauch\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Zuweilen auch Wolken auf, gl\u00fchrot,", "tokens": ["Zu\u00b7wei\u00b7len", "auch", "Wol\u00b7ken", "auf", ",", "gl\u00fch\u00b7rot", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "NN", "PTKVZ", "$,", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Als ob brennende St\u00e4dte rauchten", "tokens": ["Als", "ob", "bren\u00b7nen\u00b7de", "St\u00e4d\u00b7te", "rauch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ADJA", "NN", "VVFIN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Hinter den Bergen, als w\u00e4r entloht", "tokens": ["Hin\u00b7ter", "den", "Ber\u00b7gen", ",", "als", "w\u00e4r", "ent\u00b7loht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KOKOM", "VAFIN", "VVPP"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Ein Lavastrom und w\u00e4lzte sich her; doch eilte", "tokens": ["Ein", "La\u00b7vast\u00b7rom", "und", "w\u00e4lz\u00b7te", "sich", "her", ";", "doch", "eil\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "KON", "VVFIN", "PRF", "PTKVZ", "$.", "ADV", "VVFIN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Dar\u00fcber hin im Flug", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "hin", "im", "Flug"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ADV", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Das leuchtende Gestirn und teilte", "tokens": ["Das", "leuch\u00b7ten\u00b7de", "Ge\u00b7stirn", "und", "teil\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "In der Wolken raschem Vor\u00fcberzug", "tokens": ["In", "der", "Wol\u00b7ken", "ra\u00b7schem", "Vor\u00b7\u00fc\u00b7ber\u00b7zug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Den n\u00e4chtlichen Irrpfad, wo tief im Dunkeln", "tokens": ["Den", "n\u00e4cht\u00b7li\u00b7chen", "Irr\u00b7pfad", ",", "wo", "tief", "im", "Dun\u00b7keln"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "ADJD", "APPRART", "NN"], "meter": "-+--++-+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Umwaldeter Schluchten Licht an Licht", "tokens": ["Um\u00b7wal\u00b7de\u00b7ter", "Schluch\u00b7ten", "Licht", "an", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "NN", "APPR", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Aus fernen H\u00e4usern begann zu funkeln,", "tokens": ["Aus", "fer\u00b7nen", "H\u00e4u\u00b7sern", "be\u00b7gann", "zu", "fun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Bald einzeln und bald wieder dicht,", "tokens": ["Bald", "ein\u00b7zeln", "und", "bald", "wie\u00b7der", "dicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Wie Sterne des Himmels, \u2013 und die darin hausten,", "tokens": ["Wie", "Ster\u00b7ne", "des", "Him\u00b7mels", ",", "\u2013", "und", "die", "da\u00b7rin", "haus\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "$,", "$(", "KON", "ART", "PAV", "VVFIN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Die h\u00f6rten, vielleicht schon halb im Schlummer,", "tokens": ["Die", "h\u00f6r\u00b7ten", ",", "viel\u00b7leicht", "schon", "halb", "im", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ADV", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Wie wir vor\u00fcberbrausten,", "tokens": ["Wie", "wir", "vor\u00b7\u00fc\u00b7berb\u00b7raus\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Wenn sie nicht wach hielt nagender Kummer.", "tokens": ["Wenn", "sie", "nicht", "wach", "hielt", "na\u00b7gen\u00b7der", "Kum\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Denn auch in diese H\u00fctten ein,", "tokens": ["Denn", "auch", "in", "die\u00b7se", "H\u00fct\u00b7ten", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "In die weltverborgensten T\u00e4ler", "tokens": ["In", "die", "welt\u00b7ver\u00b7bor\u00b7gens\u00b7ten", "T\u00e4\u00b7ler"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.23": {"text": "Schleicht ja die Sorge sich, dringt die Pein,", "tokens": ["Schleicht", "ja", "die", "Sor\u00b7ge", "sich", ",", "dringt", "die", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PRF", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.24": {"text": "Der Menschen nie m\u00fcde Qu\u00e4ler.", "tokens": ["Der", "Men\u00b7schen", "nie", "m\u00fc\u00b7de", "Qu\u00e4\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.25": {"text": "Aber was w\u00e4re, frug ich, das Dasein hienieden,", "tokens": ["A\u00b7ber", "was", "w\u00e4\u00b7re", ",", "frug", "ich", ",", "das", "Da\u00b7sein", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "$,", "VVFIN", "PPER", "$,", "PRELS", "ART", "ADJA", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.26": {"text": "W\u00e4re dem Herzen nicht Kampf beschieden,", "tokens": ["W\u00e4\u00b7re", "dem", "Her\u00b7zen", "nicht", "Kampf", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKNEG", "NN", "VVPP", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.27": {"text": "Der Kampf mit Schmerz und Qual?", "tokens": ["Der", "Kampf", "mit", "Schmerz", "und", "Qual", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.28": {"text": "Dieser blutrote H\u00f6llenstrahl", "tokens": ["Die\u00b7ser", "blut\u00b7ro\u00b7te", "H\u00f6l\u00b7len\u00b7strahl"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.29": {"text": "Erleuchtet die Tiefen der Menschenbrust,", "tokens": ["Er\u00b7leuch\u00b7tet", "die", "Tie\u00b7fen", "der", "Men\u00b7schen\u00b7brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.30": {"text": "Und Seelengr\u00f6\u00dfe w\u00e4re nicht", "tokens": ["Und", "See\u00b7len\u00b7gr\u00f6\u00b7\u00dfe", "w\u00e4\u00b7re", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Und nicht des Sieges stolze Lust,", "tokens": ["Und", "nicht", "des", "Sie\u00b7ges", "stol\u00b7ze", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "W\u00e4r nicht der Schmerz, der weiht, wenn er zerbricht.", "tokens": ["W\u00e4r", "nicht", "der", "Schmerz", ",", "der", "weiht", ",", "wenn", "er", "zer\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "$,", "PRELS", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ach, schon erschauert mir tief", "tokens": ["Ach", ",", "schon", "er\u00b7schau\u00b7ert", "mir", "tief"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Das eigne Herz, und ich f\u00fchle mich zagen.", "tokens": ["Das", "eig\u00b7ne", "Herz", ",", "und", "ich", "f\u00fch\u00b7le", "mich", "za\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "PPER", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie? wenn zum Kampfe das Ungl\u00fcck mich rief',", "tokens": ["Wie", "?", "wenn", "zum", "Kamp\u00b7fe", "das", "Un\u00b7gl\u00fcck", "mich", "rie\u00b7f'", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KOUS", "APPRART", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "W\u00fcrd' ich's ertragen?", "tokens": ["W\u00fcrd'", "ich's", "er\u00b7tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "M\u00fc\u00dft' ich aller Errungenschaft,", "tokens": ["M\u00fc\u00dft'", "ich", "al\u00b7ler", "Er\u00b7run\u00b7gen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Jedem edleren Tun entsagen,", "tokens": ["Je\u00b7dem", "ed\u00b7le\u00b7ren", "Tun", "ent\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Und s\u00e4he mich weggerafft", "tokens": ["Und", "s\u00e4\u00b7he", "mich", "weg\u00b7ge\u00b7rafft"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Vor allem Erhabnen auf Erden,", "tokens": ["Vor", "al\u00b7lem", "Er\u00b7hab\u00b7nen", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.9": {"text": "Zur Fron des Tags mich gezwungen werden!", "tokens": ["Zur", "Fron", "des", "Tags", "mich", "ge\u00b7zwun\u00b7gen", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Und m\u00fc\u00dft' ich wieder wie vor Jahren", "tokens": ["Und", "m\u00fc\u00dft'", "ich", "wie\u00b7der", "wie", "vor", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "KOKOM", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Das Furchtbare bestehn", "tokens": ["Das", "Furcht\u00b7ba\u00b7re", "be\u00b7stehn"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Und das bitterste Leid erfahren,", "tokens": ["Und", "das", "bit\u00b7ters\u00b7te", "Leid", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.13": {"text": "In Geliebter brechendes Auge sehn?", "tokens": ["In", "Ge\u00b7lieb\u00b7ter", "bre\u00b7chen\u00b7des", "Au\u00b7ge", "sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "In Zagnis f\u00fchl' ich vergehn", "tokens": ["In", "Zag\u00b7nis", "f\u00fchl'", "ich", "ver\u00b7gehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "PPER", "VVINF"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.15": {"text": "Den trotzigen Mut, der noch eben", "tokens": ["Den", "trot\u00b7zi\u00b7gen", "Mut", ",", "der", "noch", "e\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ADV"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.16": {"text": "Mit dem Verderben gespielt,", "tokens": ["Mit", "dem", "Ver\u00b7der\u00b7ben", "ge\u00b7spielt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART",