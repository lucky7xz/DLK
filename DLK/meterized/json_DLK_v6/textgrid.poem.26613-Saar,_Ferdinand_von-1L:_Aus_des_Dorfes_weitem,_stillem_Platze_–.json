{"textgrid.poem.26613": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aus des Dorfes weitem, stillem Platze \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus des Dorfes weitem, stillem Platze \u2013", "tokens": ["Aus", "des", "Dor\u00b7fes", "wei\u00b7tem", ",", "stil\u00b7lem", "Plat\u00b7ze", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sonntag ist es und die Arbeit ruht \u2013", "tokens": ["Sonn\u00b7tag", "ist", "es", "und", "die", "Ar\u00b7beit", "ruht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "KON", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Gehen plaudernd, Arm in Arm geschlungen,", "tokens": ["Ge\u00b7hen", "plau\u00b7dernd", ",", "Arm", "in", "Arm", "ge\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "M\u00e4gde auf und ab in vollem Staat.", "tokens": ["M\u00e4g\u00b7de", "auf", "und", "ab", "in", "vol\u00b7lem", "Staat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Weithin rauschen die gesteiften R\u00f6cke,", "tokens": ["Weit\u00b7hin", "rau\u00b7schen", "die", "ge\u00b7steif\u00b7ten", "R\u00f6\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "H\u00f6rbar knarrt der ungewohnte Schuh;", "tokens": ["H\u00f6r\u00b7bar", "knarrt", "der", "un\u00b7ge\u00b7wohn\u00b7te", "Schuh", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Fr\u00f6hlich wiegen sich die prallen Leiber", "tokens": ["Fr\u00f6h\u00b7lich", "wie\u00b7gen", "sich", "die", "pral\u00b7len", "Lei\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Schon im Vorgef\u00fchl der Tanzmusik.", "tokens": ["Schon", "im", "Vor\u00b7ge\u00b7f\u00fchl", "der", "Tanz\u00b7mu\u00b7sik", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Sch\u00f6nheit ist es nicht, doch Kraft und Jugend,", "tokens": ["Sch\u00f6n\u00b7heit", "ist", "es", "nicht", ",", "doch", "Kraft", "und", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PTKNEG", "$,", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Was sie harmlos tragen hier zur Schau,", "tokens": ["Was", "sie", "harm\u00b7los", "tra\u00b7gen", "hier", "zur", "Schau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und in hellster Lebensfreude strahlen", "tokens": ["Und", "in", "hells\u00b7ter", "Le\u00b7bens\u00b7freu\u00b7de", "strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ihre derben Wangen, roth und rund.", "tokens": ["Ih\u00b7re", "der\u00b7ben", "Wan\u00b7gen", ",", "roth", "und", "rund", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "H\u00e4lt sich abseits in des Hauses Flur;", "tokens": ["H\u00e4lt", "sich", "ab\u00b7seits", "in", "des", "Hau\u00b7ses", "Flur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "D\u00fcnn ist ihres Haares braune Flechte,", "tokens": ["D\u00fcnn", "ist", "ih\u00b7res", "Haa\u00b7res", "brau\u00b7ne", "Flech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "F\u00e4ltchen spielen schon um Aug' und Mund.", "tokens": ["F\u00e4lt\u00b7chen", "spie\u00b7len", "schon", "um", "Aug'", "und", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Leicht geschm\u00fcckt mit einem bunten Tuche,", "tokens": ["Leicht", "ge\u00b7schm\u00fcckt", "mit", "ei\u00b7nem", "bun\u00b7ten", "Tu\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Einte sie sich gern der j\u00fcng'ren Schaar;", "tokens": ["Ein\u00b7te", "sie", "sich", "gern", "der", "j\u00fcng'\u00b7ren", "Schaar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Doch sie wird von Schaam zur\u00fcckgehalten,", "tokens": ["Doch", "sie", "wird", "von", "Schaam", "zu\u00b7r\u00fcck\u00b7ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Denn sie f\u00fchlt es, ihre Zeit ist um.", "tokens": ["Denn", "sie", "f\u00fchlt", "es", ",", "ih\u00b7re", "Zeit", "ist", "um", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "VAFIN", "KOUI", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Dennoch scheint sie eines Winks zu harren,", "tokens": ["Den\u00b7noch", "scheint", "sie", "ei\u00b7nes", "Winks", "zu", "har\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Eines Wortes, das herbei sie ruft \u2013", "tokens": ["Ei\u00b7nes", "Wor\u00b7tes", ",", "das", "her\u00b7bei", "sie", "ruft", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Von den Andern aber merkt es keine,", "tokens": ["Von", "den", "An\u00b7dern", "a\u00b7ber", "merkt", "es", "kei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADV", "VVFIN", "PPER", "PIAT", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wie ihr Blick in scheuer Sehnsucht brennt.", "tokens": ["Wie", "ihr", "Blick", "in", "scheu\u00b7er", "Sehn\u00b7sucht", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Horch! Da t\u00f6nen Geigen, Clarinetten \u2013", "tokens": ["Horch", "!", "Da", "t\u00f6\u00b7nen", "Gei\u00b7gen", ",", "Cla\u00b7ri\u00b7net\u00b7ten", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$.", "ADV", "ADJA", "NN", "$,", "NE", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Rasch hin\u00fcber nach des Wirthes Saal!", "tokens": ["Rasch", "hin\u00b7\u00fc\u00b7ber", "nach", "des", "Wirt\u00b7hes", "Saal", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dort, erwartungsvoll, die Pfeif' im Munde,", "tokens": ["Dort", ",", "er\u00b7war\u00b7tungs\u00b7voll", ",", "die", "Pfeif'", "im", "Mun\u00b7de", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Steh'n die Bursche schon zur Wahl bereit ...", "tokens": ["Steh'n", "die", "Bur\u00b7sche", "schon", "zur", "Wahl", "be\u00b7reit", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "APPRART", "NN", "ADJD", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Auf die \u00f6de Schwelle tritt sie jetzt,", "tokens": ["Auf", "die", "\u00f6\u00b7de", "Schwel\u00b7le", "tritt", "sie", "jetzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Lauscht gedankenvoll den hellen Kl\u00e4ngen", "tokens": ["Lauscht", "ge\u00b7dan\u00b7ken\u00b7voll", "den", "hel\u00b7len", "Kl\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und des Tanzes wuchtig frohem L\u00e4rm.", "tokens": ["Und", "des", "Tan\u00b7zes", "wuch\u00b7tig", "fro\u00b7hem", "L\u00e4rm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Endlich geht sie, langsam ab sich wendend;", "tokens": ["End\u00b7lich", "geht", "sie", ",", "lang\u00b7sam", "ab", "sich", "wen\u00b7dend", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "APPR", "PRF", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Niedersitzt sie an des Herdes Rand,", "tokens": ["Nie\u00b7der\u00b7sitzt", "sie", "an", "des", "Her\u00b7des", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und hinab zum kalten Stein des Estrichs", "tokens": ["Und", "hin\u00b7ab", "zum", "kal\u00b7ten", "Stein", "des", "Est\u00b7richs"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Rollen ihre Thr\u00e4nen hei\u00df und stumm.", "tokens": ["Rol\u00b7len", "ih\u00b7re", "Thr\u00e4\u00b7nen", "hei\u00df", "und", "stumm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}