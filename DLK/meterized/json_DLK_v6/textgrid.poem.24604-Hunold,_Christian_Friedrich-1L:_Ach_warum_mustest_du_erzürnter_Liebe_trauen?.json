{"textgrid.poem.24604": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach warum mustest du erz\u00fcrnter Liebe trauen?", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach warum mustest du erz\u00fcrnter Liebe trauen?", "tokens": ["Ach", "wa\u00b7rum", "mus\u00b7test", "du", "er\u00b7z\u00fcrn\u00b7ter", "Lie\u00b7be", "trau\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWAV", "VMFIN", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du h\u00e4ttest auf den Grund des Hertzens sollen schauen.", "tokens": ["Du", "h\u00e4t\u00b7test", "auf", "den", "Grund", "des", "Hert\u00b7zens", "sol\u00b7len", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Offt hegt ein rasend Hertz die gr\u00f6ste Z\u00e4rtlichkeit/", "tokens": ["Offt", "hegt", "ein", "ra\u00b7send", "Hertz", "die", "gr\u00f6s\u00b7te", "Z\u00e4rt\u00b7lich\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Strafft L\u00fcgen einen Mund/ der nichts als Morden dr\u00e4ut.", "tokens": ["Strafft", "L\u00fc\u00b7gen", "ei\u00b7nen", "Mund", "/", "der", "nichts", "als", "Mor\u00b7den", "dr\u00e4ut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "NN", "$(", "ART", "PIS", "KOKOM", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und wenn ich es gewollt/ hast du es m\u00fcssen wagen/", "tokens": ["Und", "wenn", "ich", "es", "ge\u00b7wollt", "/", "hast", "du", "es", "m\u00fcs\u00b7sen", "wa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VMPP", "$(", "VAFIN", "PPER", "PPER", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und mirs noch hundertmahl nicht erstlich m\u00fcssen sagen?", "tokens": ["Und", "mirs", "noch", "hun\u00b7dert\u00b7mahl", "nicht", "erst\u00b7lich", "m\u00fcs\u00b7sen", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADV", "ADV", "PTKNEG", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du soltest vor dem Streich mich noch zu Rathe ziehn/", "tokens": ["Du", "sol\u00b7test", "vor", "dem", "Streich", "mich", "noch", "zu", "Ra\u00b7the", "ziehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "PPER", "ADV", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und da du es gethan/ so soltest du mich fliehn.", "tokens": ["Und", "da", "du", "es", "ge\u00b7than", "/", "so", "sol\u00b7test", "du", "mich", "fliehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVPP", "$(", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}