{"dta.poem.21361": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XIV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Schl\u00e4ft er oder denkt er nach?", "tokens": ["Schl\u00e4ft", "er", "o\u00b7der", "denkt", "er", "nach", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Man kann\u2019s nicht genau ermitteln;", "tokens": ["Man", "kann's", "nicht", "ge\u00b7nau", "er\u00b7mit\u00b7teln", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch wenn die rechte Stunde kommt,", "tokens": ["Doch", "wenn", "die", "rech\u00b7te", "Stun\u00b7de", "kommt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird er gewaltig sich r\u00fctteln.", "tokens": ["Wird", "er", "ge\u00b7wal\u00b7tig", "sich", "r\u00fct\u00b7teln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "PRF", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Die gute Fahne ergreift er dann", "tokens": ["Die", "gu\u00b7te", "Fah\u00b7ne", "er\u00b7greift", "er", "dann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und ruft: Zu Pferd\u2019! zu Pferde!", "tokens": ["Und", "ruft", ":", "Zu", "Pferd'", "!", "zu", "Pfer\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "APPR", "NN", "$.", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sein reisiges Volk erwacht und springt", "tokens": ["Sein", "rei\u00b7si\u00b7ges", "Volk", "er\u00b7wacht", "und", "springt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "KON", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Lautrasselnd empor von der Erde.", "tokens": ["Laut\u00b7ras\u00b7selnd", "em\u00b7por", "von", "der", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "APPR", "ART", "NN", "$."], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Ein jeder schwingt sich auf sein Ro\u00df,", "tokens": ["Ein", "je\u00b7der", "schwingt", "sich", "auf", "sein", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das wiehert und stampft mit den Hufen!", "tokens": ["Das", "wie\u00b7hert", "und", "stampft", "mit", "den", "Hu\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie reiten hinaus in die klirrende Welt,", "tokens": ["Sie", "rei\u00b7ten", "hin\u00b7aus", "in", "die", "klir\u00b7ren\u00b7de", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und die Trompeten rufen.", "tokens": ["Und", "die", "Trom\u00b7pe\u00b7ten", "ru\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$."], "meter": "--++-+-", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Sie reiten gut, sie schlagen gut,", "tokens": ["Sie", "rei\u00b7ten", "gut", ",", "sie", "schla\u00b7gen", "gut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie haben ausgeschlafen.", "tokens": ["Sie", "ha\u00b7ben", "aus\u00b7ge\u00b7schla\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Kaiser h\u00e4lt ein strenges Gericht,", "tokens": ["Der", "Kai\u00b7ser", "h\u00e4lt", "ein", "stren\u00b7ges", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Er will die M\u00f6rder bestrafen \u2013", "tokens": ["Er", "will", "die", "M\u00f6r\u00b7der", "be\u00b7stra\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVPP", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}