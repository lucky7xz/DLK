{"textgrid.poem.49539": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du sagst, ich w\u00fcrde dich recht sonderbar verbinden,", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du sagst, ich w\u00fcrde dich recht sonderbar verbinden,", "tokens": ["Du", "sagst", ",", "ich", "w\u00fcr\u00b7de", "dich", "recht", "son\u00b7der\u00b7bar", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "K\u00f6nnt ich im Januar ein R\u00f6sgen f\u00fcr dich finden.", "tokens": ["K\u00f6nnt", "ich", "im", "Ja\u00b7nu\u00b7ar", "ein", "R\u00f6s\u00b7gen", "f\u00fcr", "dich", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "ART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wohlan, geliebtes Kind,", "tokens": ["Wo\u00b7hlan", ",", "ge\u00b7lieb\u00b7tes", "Kind", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Hat deiner Wangen Paar die Rosen so vonn\u00f6then,", "tokens": ["Hat", "dei\u00b7ner", "Wan\u00b7gen", "Paar", "die", "Ro\u00b7sen", "so", "von\u00b7n\u00f6\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sprich nur: ich liebe dich! so sollen sie err\u00f6then,", "tokens": ["Sprich", "nur", ":", "ich", "lie\u00b7be", "dich", "!", "so", "sol\u00b7len", "sie", "er\u00b7r\u00f6\u00b7then", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$.", "PPER", "VVFIN", "PPER", "$.", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und hundert Rosen stehn, wo itzo Lilgen sind.", "tokens": ["Und", "hun\u00b7dert", "Ro\u00b7sen", "stehn", ",", "wo", "it\u00b7zo", "Lil\u00b7gen", "sind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "VVINF", "$,", "PWAV", "ADV", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}