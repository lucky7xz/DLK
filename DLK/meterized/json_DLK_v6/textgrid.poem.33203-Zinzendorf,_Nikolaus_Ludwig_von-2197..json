{"textgrid.poem.33203": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "2197.", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.71", "en:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was seyd ihr so gutes muths, ihr Mahanaim! o ihr formidable Zeboos Haschamaim:", "tokens": ["Was", "seyd", "ihr", "so", "gu\u00b7tes", "muths", ",", "ihr", "Ma\u00b7ha\u00b7naim", "!", "o", "ihr", "for\u00b7mi\u00b7dab\u00b7le", "Ze\u00b7boos", "Ha\u00b7scha\u00b7maim", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "ADJA", "NN", "$,", "PPOSAT", "NN", "$.", "FM", "PPOSAT", "ADJA", "NN", "NE", "$."], "meter": "-+--+-+-+-+--+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Gott gr\u00fc\u00df dich, du selige s\u00fcnder-gemeinschaft! wir freuen uns deiner gesundheit und freundschaft.", "tokens": ["Gott", "gr\u00fc\u00df", "dich", ",", "du", "se\u00b7li\u00b7ge", "s\u00fcn\u00b7der\u00b7ge\u00b7mein\u00b7schaft", "!", "wir", "freu\u00b7en", "uns", "dei\u00b7ner", "ge\u00b7sund\u00b7heit", "und", "freund\u00b7schaft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "PPER", "ADJA", "NN", "$.", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "ADJD", "$."], "meter": "+---+--+--+--+--+--+--+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich d\u00e4chte, wir w\u00e4ren wohl ganz unbetr\u00e4chtlich, und euch, ihr hellflammenden geister! ver\u00e4chtlich;", "tokens": ["Ich", "d\u00e4ch\u00b7te", ",", "wir", "w\u00e4\u00b7ren", "wohl", "ganz", "un\u00b7be\u00b7tr\u00e4cht\u00b7lich", ",", "und", "euch", ",", "ihr", "hell\u00b7flam\u00b7men\u00b7den", "geis\u00b7ter", "!", "ver\u00b7\u00e4cht\u00b7lich", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "ADV", "ADJD", "$,", "KON", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$.", "ADJD", "$."], "meter": "-+--+-+-+-+--+--+--+--+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.4": {"line.1": {"text": "Beh\u00fcts Gott! wir f\u00fchlen in eurer verbindung die seligste liebes- und friedens-empfindung.", "tokens": ["Be\u00b7h\u00fcts", "Gott", "!", "wir", "f\u00fch\u00b7len", "in", "eu\u00b7rer", "ver\u00b7bin\u00b7dung", "die", "se\u00b7ligs\u00b7te", "lie\u00b7bes", "und", "frie\u00b7dens\u00b7emp\u00b7fin\u00b7dung", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "TRUNC", "KON", "NN", "$."], "meter": "-+--+--+--+-+-+-+--+--+-", "measure": "amphibrach.tetra.plus"}}, "stanza.5": {"line.1": {"text": "Wie k\u00f6nt ihr euch denn doch bis zu uns entfernen?", "tokens": ["Wie", "k\u00f6nt", "ihr", "euch", "denn", "doch", "bis", "zu", "uns", "ent\u00b7fer\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPER", "ADV", "ADV", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Wir pflegen euch aber doch oft zu betr\u00fcben?", "tokens": ["Wir", "pfle\u00b7gen", "euch", "a\u00b7ber", "doch", "oft", "zu", "be\u00b7tr\u00fc\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}}}}