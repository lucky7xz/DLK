{"textgrid.poem.55110": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "6", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Entw\u00f6hnen sollt ich mich vom Glanz der Blicke,", "tokens": ["Ent\u00b7w\u00f6h\u00b7nen", "sollt", "ich", "mich", "vom", "Glanz", "der", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein Leben sollten sie nicht mehr versch\u00f6nen.", "tokens": ["Mein", "Le\u00b7ben", "soll\u00b7ten", "sie", "nicht", "mehr", "ver\u00b7sch\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was man Geschick nennt, l\u00e4\u00dft sich nicht vers\u00f6hnen,", "tokens": ["Was", "man", "Ge\u00b7schick", "nennt", ",", "l\u00e4\u00dft", "sich", "nicht", "ver\u00b7s\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "NN", "VVFIN", "$,", "VVFIN", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich wei\u00df es wohl und trat best\u00fcrzt zur\u00fccke.", "tokens": ["Ich", "wei\u00df", "es", "wohl", "und", "trat", "be\u00b7st\u00fcrzt", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nun wu\u00dft ich auch von keinem weitern Gl\u00fccke;", "tokens": ["Nun", "wu\u00dft", "ich", "auch", "von", "kei\u00b7nem", "wei\u00b7tern", "Gl\u00fc\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gleich fing ich an, von diesen und von jenen", "tokens": ["Gleich", "fing", "ich", "an", ",", "von", "die\u00b7sen", "und", "von", "je\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "APPR", "PDAT", "KON", "APPR", "PDAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Notwend'gen Dingen sonst mich zu entw\u00f6hnen:", "tokens": ["Not\u00b7wen\u00b7d'\u00b7gen", "Din\u00b7gen", "sonst", "mich", "zu", "ent\u00b7w\u00f6h\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Notwendig schien mir nichts als ihre Blicke.", "tokens": ["Not\u00b7wen\u00b7dig", "schien", "mir", "nichts", "als", "ih\u00b7re", "Bli\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PIS", "KOKOM", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Des Weines Glut, den Vielgenu\u00df der Speisen,", "tokens": ["Des", "Wei\u00b7nes", "Glut", ",", "den", "Viel\u00b7ge\u00b7nu\u00df", "der", "Spei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bequemlichkeit und Schlaf und sonst'ge Gaben,", "tokens": ["Be\u00b7quem\u00b7lich\u00b7keit", "und", "Schlaf", "und", "sonst'\u00b7ge", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gesellschaft wies ich weg, da\u00df wenig bliebe.", "tokens": ["Ge\u00b7sell\u00b7schaft", "wies", "ich", "weg", ",", "da\u00df", "we\u00b7nig", "blie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So kann ich ruhig durch die Welt nun reisen:", "tokens": ["So", "kann", "ich", "ru\u00b7hig", "durch", "die", "Welt", "nun", "rei\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "APPR", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was ich bedarf, ist \u00fcberall zu haben,", "tokens": ["Was", "ich", "be\u00b7darf", ",", "ist", "\u00fc\u00b7be\u00b7rall", "zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VAFIN", "ADV", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Unentbehrlichs bring ich mit \u2013 die Liebe.", "tokens": ["Und", "Un\u00b7ent\u00b7behr\u00b7lichs", "bring", "ich", "mit", "\u2013", "die", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "APPR", "$(", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}