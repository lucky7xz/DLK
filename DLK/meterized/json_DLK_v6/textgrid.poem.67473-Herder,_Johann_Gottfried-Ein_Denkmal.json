{"textgrid.poem.67473": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Ein Denkmal", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Strenge gegen sich selbst, nachgebend gegen die Freunde,", "tokens": ["Stren\u00b7ge", "ge\u00b7gen", "sich", "selbst", ",", "nach\u00b7ge\u00b7bend", "ge\u00b7gen", "die", "Freun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "ADV", "$,", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "War er dem Redlichen hold, aber dem Laster ein Feind,", "tokens": ["War", "er", "dem", "Red\u00b7li\u00b7chen", "hold", ",", "a\u00b7ber", "dem", "Las\u00b7ter", "ein", "Feind", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADJD", "$,", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "That und f\u00f6rderte Gutes und half als Vater den Armen.", "tokens": ["That", "und", "f\u00f6r\u00b7der\u00b7te", "Gu\u00b7tes", "und", "half", "als", "Va\u00b7ter", "den", "Ar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "KON", "VVFIN", "KOKOM", "NN", "ART", "NN", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Seinem Andenken sei dankbar dies Denkmal geweiht!", "tokens": ["Sei\u00b7nem", "An\u00b7den\u00b7ken", "sei", "dank\u00b7bar", "dies", "Denk\u00b7mal", "ge\u00b7weiht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "PDS", "NN", "VVPP", "$."], "meter": "+----+-+-+--+", "measure": "iambic.penta.chol"}}}}}