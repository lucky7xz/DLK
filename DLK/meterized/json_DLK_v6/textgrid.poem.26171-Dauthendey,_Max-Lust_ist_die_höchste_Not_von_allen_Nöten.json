{"textgrid.poem.26171": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Lust ist die h\u00f6chste Not von allen N\u00f6ten", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und der Akazienduft macht zwei, umarmt im Tanze, schw\u00fcl err\u00f6ten,", "tokens": ["Und", "der", "A\u00b7ka\u00b7zi\u00b7en\u00b7duft", "macht", "zwei", ",", "um\u00b7armt", "im", "Tan\u00b7ze", ",", "schw\u00fcl", "er\u00b7r\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "CARD", "$,", "VVFIN", "APPRART", "NN", "$,", "ADJD", "VVINF", "$,"], "meter": "--+---+-+-+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Kommt schwindelnd und bet\u00e4ubend dicht herbei", "tokens": ["Kommt", "schwin\u00b7delnd", "und", "be\u00b7t\u00e4u\u00b7bend", "dicht", "her\u00b7bei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und spricht: Lust ist die h\u00f6chste Not von allen N\u00f6ten.", "tokens": ["Und", "spricht", ":", "Lust", "ist", "die", "h\u00f6chs\u00b7te", "Not", "von", "al\u00b7len", "N\u00f6\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NN", "VAFIN", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Akazienbl\u00fcten an die Brust sich einstmals meine Liebste w\u00e4hlte,", "tokens": ["A\u00b7ka\u00b7zi\u00b7en\u00b7bl\u00fc\u00b7ten", "an", "die", "Brust", "sich", "einst\u00b7mals", "mei\u00b7ne", "Liebs\u00b7te", "w\u00e4hl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PRF", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Als sie bei keinem Abendtanz im Mai noch fehlte,", "tokens": ["Als", "sie", "bei", "kei\u00b7nem", "A\u00b7bend\u00b7tanz", "im", "Mai", "noch", "fehl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "NN", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und j\u00e4he Unruh', wie der Duft der Dolde, sie best\u00e4ndig qu\u00e4lte.", "tokens": ["Und", "j\u00e4\u00b7he", "Un\u00b7ruh'", ",", "wie", "der", "Duft", "der", "Dol\u00b7de", ",", "sie", "be\u00b7st\u00e4n\u00b7dig", "qu\u00e4l\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "PWAV", "ART", "NN", "ART", "NN", "$,", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.4": {"text": "Heut nennt sie die Akazienbl\u00fct' beim Wiedersehen: T\u00e4nzerin!", "tokens": ["Heut", "nennt", "sie", "die", "A\u00b7ka\u00b7zi\u00b7en\u00b7bl\u00fct'", "beim", "Wie\u00b7der\u00b7se\u00b7hen", ":", "T\u00e4n\u00b7ze\u00b7rin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$.", "NN", "$."], "meter": "-+--+-+-+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.5": {"text": "Und deutet mit der Hand versonnen winkend zu ihr hin,", "tokens": ["Und", "deu\u00b7tet", "mit", "der", "Hand", "ver\u00b7son\u00b7nen", "win\u00b7kend", "zu", "ihr", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "VVFIN", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Als l\u00e4ge in dem einen Wort aller Gen\u00fcsse Sinn.", "tokens": ["Als", "l\u00e4\u00b7ge", "in", "dem", "ei\u00b7nen", "Wort", "al\u00b7ler", "Ge\u00b7n\u00fcs\u00b7se", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "APPR", "ART", "ART", "NN", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}