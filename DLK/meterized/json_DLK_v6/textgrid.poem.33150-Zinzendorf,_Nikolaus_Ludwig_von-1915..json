{"textgrid.poem.33150": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "1915.", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er hat wol an uns gedacht, da\u00df er euch erhalten, bis du sahst in seiner sach freye gnade walten.", "tokens": ["Er", "hat", "wol", "an", "uns", "ge\u00b7dacht", ",", "da\u00df", "er", "euch", "er\u00b7hal\u00b7ten", ",", "bis", "du", "sahst", "in", "sei\u00b7ner", "sach", "frey\u00b7e", "gna\u00b7de", "wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPER", "VVPP", "$,", "KOUS", "PPER", "PPER", "VVPP", "$,", "KOUS", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJD", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+--+-+-+-+-+-+--+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Wunder ohne ziel und zahl, gnade ohne schranken, so da\u00df man der gnaden-wahl nicht genug kan danken.", "tokens": ["Wun\u00b7der", "oh\u00b7ne", "ziel", "und", "zahl", ",", "gna\u00b7de", "oh\u00b7ne", "schran\u00b7ken", ",", "so", "da\u00df", "man", "der", "gna\u00b7den\u00b7wahl", "nicht", "ge\u00b7nug", "kan", "dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,", "VVFIN", "APPR", "VVINF", "$,", "ADV", "KOUS", "PIS", "ART", "NN", "PTKNEG", "ADV", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+--+-+--+--+-+--+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "Nahe war es an der zeit, da man in den kl\u00fcften einsam seine seligkeit h\u00e4tte m\u00f6gen stiften;", "tokens": ["Na\u00b7he", "war", "es", "an", "der", "zeit", ",", "da", "man", "in", "den", "kl\u00fcf\u00b7ten", "ein\u00b7sam", "sei\u00b7ne", "se\u00b7lig\u00b7keit", "h\u00e4t\u00b7te", "m\u00f6\u00b7gen", "stif\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "ART", "NN", "$,", "KOUS", "PIS", "APPR", "ART", "ADJA", "ADJD", "PPOSAT", "NN", "VAFIN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-+--+-+-+-+--+-+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.4": {"line.1": {"text": "Und auf einmal bricht der Herr aus in eine weite und f\u00fchrt seine wanderer in die l\u00e4ng und breite.", "tokens": ["Und", "auf", "ein\u00b7mal", "bricht", "der", "Herr", "aus", "in", "ei\u00b7ne", "wei\u00b7te", "und", "f\u00fchrt", "sei\u00b7ne", "wan\u00b7de\u00b7rer", "in", "die", "l\u00e4ng", "und", "brei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "VVFIN", "ART", "NN", "APPR", "APPR", "ART", "ADJA", "KON", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "ADJD", "KON", "ADJA", "$."], "meter": "-+--+-+--+-+--+--+--+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.5": {"line.1": {"text": "Er besetzt sich dieses feld; fast auf iedem flekke hat er eine wacht bestellt, eine gnaden-ekke.", "tokens": ["Er", "be\u00b7setzt", "sich", "die\u00b7ses", "feld", ";", "fast", "auf", "ie\u00b7dem", "flek\u00b7ke", "hat", "er", "ei\u00b7ne", "wacht", "be\u00b7stellt", ",", "ei\u00b7ne", "gna\u00b7den\u00b7ek\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PDAT", "NN", "$.", "ADV", "APPR", "PIAT", "NN", "VAFIN", "PPER", "ART", "ADJD", "VVPP", "$,", "ART", "NN", "$."], "meter": "+-+-+-+--+-+-+-+-+-+--+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.6": {"line.1": {"text": "La\u00df in deiner eignen seel und den andern herzen diese blutge seiten-h\u00f6hl brennen gleich den kerzen.", "tokens": ["La\u00df", "in", "dei\u00b7ner", "eig\u00b7nen", "seel", "und", "den", "an\u00b7dern", "her\u00b7zen", "die\u00b7se", "blut\u00b7ge", "sei\u00b7ten\u00b7h\u00f6hl", "bren\u00b7nen", "gleich", "den", "ker\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PPOSAT", "ADJA", "NN", "KON", "ART", "ADJA", "VVFIN", "PDAT", "ADJA", "NN", "VVINF", "ADV", "ART", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-+-+--+-+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.7": {"line.1": {"text": "Deine gegend m\u00fcsse nun ihre blutge strahlen, und wie sie dem herzen thun, allerwegen mahlen.", "tokens": ["Dei\u00b7ne", "ge\u00b7gend", "m\u00fcs\u00b7se", "nun", "ih\u00b7re", "blut\u00b7ge", "strah\u00b7len", ",", "und", "wie", "sie", "dem", "her\u00b7zen", "thun", ",", "al\u00b7ler\u00b7we\u00b7gen", "mah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "VVPP", "VMFIN", "ADV", "PPOSAT", "NN", "VVFIN", "$,", "KON", "PWAV", "PPER", "ART", "VVINF", "VVINF", "$,", "ADV", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-+-+--+-+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.8": {"line.1": {"text": "Von dem mit dem l\u00f6se-geld baar bezahlten heile", "tokens": ["Von", "dem", "mit", "dem", "l\u00f6\u00b7se\u00b7geld", "baar", "be\u00b7zahl\u00b7ten", "hei\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "APPR", "ART", "NN", "ADJD", "ADJA", "ADJA"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Jesus hat zur seligkeit dich selbst auserwehlet, und zum zeichen dieser zeit seliglich gezehlet.", "tokens": ["Je\u00b7sus", "hat", "zur", "se\u00b7lig\u00b7keit", "dich", "selbst", "au\u00b7ser\u00b7weh\u00b7let", ",", "und", "zum", "zei\u00b7chen", "die\u00b7ser", "zeit", "se\u00b7lig\u00b7lich", "ge\u00b7zeh\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPRART", "NN", "PPER", "ADV", "VVFIN", "$,", "KON", "APPRART", "NN", "PDAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+--+--+-+-+-+-+-+-+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.10": {"line.1": {"text": "Zu dem wunderbaren volk, das so blutig bl\u00fchet, und sich mit der zeugen-wolk durch die welten ziehet.", "tokens": ["Zu", "dem", "wun\u00b7der\u00b7ba\u00b7ren", "volk", ",", "das", "so", "blu\u00b7tig", "bl\u00fc\u00b7het", ",", "und", "sich", "mit", "der", "zeu\u00b7gen\u00b7wolk", "durch", "die", "wel\u00b7ten", "zie\u00b7het", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$,", "KON", "PRF", "APPR", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "--+-+-+--+-+-+-+-+-+--+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.11": {"line.1": {"text": "Satan hat wol grossen zorn, aber wenig kr\u00e4fte, Jesu creuz ist ihm ein dorn und das blut-gesch\u00e4ffte;", "tokens": ["Sa\u00b7tan", "hat", "wol", "gros\u00b7sen", "zorn", ",", "a\u00b7ber", "we\u00b7nig", "kr\u00e4f\u00b7te", ",", "Je\u00b7su", "creuz", "ist", "ihm", "ein", "dorn", "und", "das", "blut\u00b7ge\u00b7sch\u00e4ff\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ADJA", "NN", "$,", "ADV", "PIS", "VVFIN", "$,", "NE", "NN", "VAFIN", "PPER", "ART", "NN", "KON", "ART", "ADJA", "$."], "meter": "+-+-+-+--+-+-+--+-+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.12": {"line.1": {"text": "Kirche, lebe im gef\u00fchl aller blutgen maden, bis du endlich k\u00f6mst ans ziel der zuk\u00fcnftgen gnaden;", "tokens": ["Kir\u00b7che", ",", "le\u00b7be", "im", "ge\u00b7f\u00fchl", "al\u00b7ler", "blut\u00b7gen", "ma\u00b7den", ",", "bis", "du", "end\u00b7lich", "k\u00f6mst", "ans", "ziel", "der", "zu\u00b7k\u00fcnft\u00b7gen", "gna\u00b7den", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPRART", "NN", "PIAT", "ADJA", "NN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-+-+-+-+-+--+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.13": {"line.1": {"text": "Bis du dahin mit der zeit k\u00f6mst nach streiter-treue, wo das abendmahl bereit, das versprochne neue.", "tokens": ["Bis", "du", "da\u00b7hin", "mit", "der", "zeit", "k\u00f6mst", "nach", "strei\u00b7ter\u00b7treue", ",", "wo", "das", "a\u00b7bend\u00b7mahl", "be\u00b7reit", ",", "das", "ver\u00b7sproch\u00b7ne", "neu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "APPR", "ART", "NN", "VVFIN", "APPR", "ADJA", "$,", "PWAV", "ART", "ADV", "ADJD", "$,", "PDS", "VVFIN", "ADJA", "$."], "meter": "+-----+--+-+--+-+-+--+-+-", "measure": "trochaic.octa.plus.relaxed"}}}}}