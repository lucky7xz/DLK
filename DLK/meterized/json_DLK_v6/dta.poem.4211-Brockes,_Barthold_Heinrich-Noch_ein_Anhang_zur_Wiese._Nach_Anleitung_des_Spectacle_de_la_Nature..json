{"dta.poem.4211": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Noch ein Anhang zur Wiese.  \n Nach  \n Anleitung des  Spectacle de la Nature.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Allein der allergr\u00f6\u00dfte Nutzen, den wir noch von den", "tokens": ["Al\u00b7lein", "der", "al\u00b7ler\u00b7gr\u00f6\u00df\u00b7te", "Nut\u00b7zen", ",", "den", "wir", "noch", "von", "den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "APPR", "ART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wiesen ziehn,", "tokens": ["Wie\u00b7sen", "ziehn", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Ist, da\u00df dieselben ohne Kosten, und, sonder da\u00df wir uns", "tokens": ["Ist", ",", "da\u00df", "die\u00b7sel\u00b7ben", "oh\u00b7ne", "Kos\u00b7ten", ",", "und", ",", "son\u00b7der", "da\u00df", "wir", "uns"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "$,", "KOUS", "PDAT", "APPR", "NN", "$,", "KON", "$,", "KON", "KOUS", "PPER", "PRF"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "bem\u00fchn,", "tokens": ["be\u00b7m\u00fchn", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Die Thiere, derer wir im Leben fast nicht verm\u00f6gen zu", "tokens": ["Die", "Thie\u00b7re", ",", "de\u00b7rer", "wir", "im", "Le\u00b7ben", "fast", "nicht", "ver\u00b7m\u00f6\u00b7gen", "zu"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "ADV", "PTKNEG", "VVINF", "PTKZU"], "meter": "-+---+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "entbehren,", "tokens": ["ent\u00b7beh\u00b7ren", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Durch ihr von selbst sich zeugend Futter im vollen Ueber-", "tokens": ["Durch", "ihr", "von", "selbst", "sich", "zeu\u00b7gend", "Fut\u00b7ter", "im", "vol\u00b7len", "Ue\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPR", "ADV", "PRF", "ADJD", "NN", "APPRART", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "flu\u00df ern\u00e4hren.", "tokens": ["flu\u00df", "er\u00b7n\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Der Ochse, dessen Fleisch uns speiset, sowohl als jener,", "tokens": ["Der", "O\u00b7chse", ",", "des\u00b7sen", "Fleisch", "uns", "spei\u00b7set", ",", "so\u00b7wohl", "als", "je\u00b7ner", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELAT", "NN", "PPER", "VVFIN", "$,", "KON", "KOUS", "PDS", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "der uns pfl\u00fcgen", "tokens": ["der", "uns", "pfl\u00fc\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPER", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Und unsern Acker bauen hilft, l\u00e4\u00dft sich an Gras und Kraut", "tokens": ["Und", "un\u00b7sern", "A\u00b7cker", "bau\u00b7en", "hilft", ",", "l\u00e4\u00dft", "sich", "an", "Gras", "und", "Kraut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "VVFIN", "$,", "VVFIN", "PRF", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-++--+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "gen\u00fcgen.", "tokens": ["ge\u00b7n\u00fc\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Das Pferd, das auf verschiedne Weise, so da\u00df man sie", "tokens": ["Das", "Pferd", ",", "das", "auf", "ver\u00b7schied\u00b7ne", "Wei\u00b7se", ",", "so", "da\u00df", "man", "sie"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "$,", "ADV", "KOUS", "PIS", "PPER"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "kaum z\u00e4hlt, uns dient,", "tokens": ["kaum", "z\u00e4hlt", ",", "uns", "dient", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Verlanget zur Belohnung nichts, als was auf unsern", "tokens": ["Ver\u00b7lan\u00b7get", "zur", "Be\u00b7loh\u00b7nung", "nichts", ",", "als", "was", "auf", "un\u00b7sern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "PIS", "$,", "KOUS", "PIS", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wiesen gr\u00fcnt.", "tokens": ["Wie\u00b7sen", "gr\u00fcnt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Es wirft sich gleichsam in dieselben mit munterm Anstand", "tokens": ["Es", "wirft", "sich", "gleich\u00b7sam", "in", "die\u00b7sel\u00b7ben", "mit", "mun\u00b7term", "An\u00b7stand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "APPR", "PDAT", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "selbst hinein,", "tokens": ["selbst", "hin\u00b7ein", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Nach seiner Arbeit, und verlangt sonst nicht von uns", "tokens": ["Nach", "sei\u00b7ner", "Ar\u00b7beit", ",", "und", "ver\u00b7langt", "sonst", "nicht", "von", "uns"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "VVFIN", "ADV", "PTKNEG", "APPR", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "verpflegt zu seyn.", "tokens": ["ver\u00b7pflegt", "zu", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Die Kuh, von welcher wir zum Leben so manchen Bey-", "tokens": ["Die", "Kuh", ",", "von", "wel\u00b7cher", "wir", "zum", "Le\u00b7ben", "so", "man\u00b7chen", "Bey"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "APPRART", "NN", "ADV", "PIAT", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "trag all' empfangen,", "tokens": ["trag", "all'", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Wird anders nichts daf\u00fcr von uns, als unsrer Wiesen", "tokens": ["Wird", "an\u00b7ders", "nichts", "da\u00b7f\u00fcr", "von", "uns", ",", "als", "uns\u00b7rer", "Wie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PIS", "PAV", "APPR", "PPER", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Frucht, verlangen.", "tokens": ["Frucht", ",", "ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Die Wollen-reiche Zucht der Schafe, die uns so speisen", "tokens": ["Die", "Wol\u00b7len\u00b7rei\u00b7che", "Zucht", "der", "Scha\u00b7fe", ",", "die", "uns", "so", "spei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "als auch kleiden,", "tokens": ["als", "auch", "klei\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Sind f\u00fcr die Gaben wohl zufrieden mit dem Genu\u00df von", "tokens": ["Sind", "f\u00fcr", "die", "Ga\u00b7ben", "wohl", "zu\u00b7frie\u00b7den", "mit", "dem", "Ge\u00b7nu\u00df", "von"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADV", "ADJD", "APPR", "ART", "NN", "APPR"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "unsern Weiden.", "tokens": ["un\u00b7sern", "Wei\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Wer ist, der dieses Wunder fa\u00dft, wie nemlich Gras die", "tokens": ["Wer", "ist", ",", "der", "die\u00b7ses", "Wun\u00b7der", "fa\u00dft", ",", "wie", "nem\u00b7lich", "Gras", "die"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "PDAT", "NN", "VVFIN", "$,", "PWAV", "ADV", "NN", "ART"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Thiere n\u00e4hre,", "tokens": ["Thie\u00b7re", "n\u00e4h\u00b7re", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Wie sich sein bitt\u2019rer strenger Saft in Fleisch und s\u00fcsse", "tokens": ["Wie", "sich", "sein", "bitt'\u00b7rer", "stren\u00b7ger", "Saft", "in", "Fleisch", "und", "s\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "PPOSAT", "ADJA", "ADJA", "NN", "APPR", "NN", "KON", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Milch verkehre,", "tokens": ["Milch", "ver\u00b7keh\u00b7re", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Und wie es einem muntern Pferde so viele St\u00e4rk\u2019 und", "tokens": ["Und", "wie", "es", "ei\u00b7nem", "mun\u00b7tern", "Pfer\u00b7de", "so", "vie\u00b7le", "St\u00e4rk'", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ART", "ADJA", "NN", "ADV", "PIAT", "NN", "KON"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Kraft gew\u00e4hre?", "tokens": ["Kraft", "ge\u00b7w\u00e4h\u00b7re", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Versuch es jemand und zerquetsche das Gras, filtrire,", "tokens": ["Ver\u00b7such", "es", "je\u00b7mand", "und", "zer\u00b7quet\u00b7sche", "das", "Gras", ",", "filt\u00b7ri\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "PIS", "KON", "VVFIN", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "pre\u00df', zerdr\u00fccke,", "tokens": ["pre\u00df'", ",", "zer\u00b7dr\u00fc\u00b7cke", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["PTKVZ", "$,", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Ja koch\u2019 und distillir\u2019 es gar, ob sichs nur einst zur Suppe", "tokens": ["Ja", "koch'", "und", "di\u00b7stil\u00b7lir'", "es", "gar", ",", "ob", "sichs", "nur", "einst", "zur", "Sup\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PIS", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.10": {"text": "schicke,", "tokens": ["schi\u00b7cke", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Die e\u00dfbar ist, da in den Eitern der K\u00fch\u2019 es lieblich zuge-", "tokens": ["Die", "e\u00df\u00b7bar", "ist", ",", "da", "in", "den", "Ei\u00b7tern", "der", "K\u00fch'", "es", "lieb\u00b7lich", "zu\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VAFIN", "$,", "KOUS", "APPR", "ART", "NN", "ART", "NN", "PPER", "ADJD", "TRUNC"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "schickt", "tokens": ["schickt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Und wunderbar bereitet wird, da\u00df es uns n\u00e4hret und", "tokens": ["Und", "wun\u00b7der\u00b7bar", "be\u00b7rei\u00b7tet", "wird", ",", "da\u00df", "es", "uns", "n\u00e4h\u00b7ret", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVPP", "VAFIN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "KON"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.14": {"text": "erquickt.", "tokens": ["er\u00b7quickt", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.5": {"line.1": {"text": "Mit diesem Wunder geht es nun so, wie es mit den", "tokens": ["Mit", "die\u00b7sem", "Wun\u00b7der", "geht", "es", "nun", "so", ",", "wie", "es", "mit", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER", "ADV", "ADV", "$,", "PWAV", "PPER", "APPR", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "meisten Werken", "tokens": ["meis\u00b7ten", "Wer\u00b7ken"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Des liebreich-weisen Sch\u00f6pfers geht,", "tokens": ["Des", "lieb\u00b7reich\u00b7wei\u00b7sen", "Sch\u00f6p\u00b7fers", "geht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df wir darauf so wenig merken,", "tokens": ["Da\u00df", "wir", "da\u00b7rauf", "so", "we\u00b7nig", "mer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Weil es, ohn unsre Sorge, M\u00fch und Flei\u00df, ganz von", "tokens": ["Weil", "es", ",", "ohn", "uns\u00b7re", "Sor\u00b7ge", ",", "M\u00fch", "und", "Flei\u00df", ",", "ganz", "von"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "$,", "KOUI", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "$,", "ADV", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "sich selbst entsteht,", "tokens": ["sich", "selbst", "ent\u00b7steht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Recht unter unsern F\u00fcssen w\u00e4chst, da doch daran so viel", "tokens": ["Recht", "un\u00b7ter", "un\u00b7sern", "F\u00fcs\u00b7sen", "w\u00e4chst", ",", "da", "doch", "da\u00b7ran", "so", "viel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,", "KOUS", "ADV", "PAV", "ADV", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "gelegen,", "tokens": ["ge\u00b7le\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Da es ein unvermi\u00dflichs Gut und ein so wunderbarer", "tokens": ["Da", "es", "ein", "un\u00b7ver\u00b7mi\u00df\u00b7lichs", "Gut", "und", "ein", "so", "wun\u00b7der\u00b7ba\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "KON", "ART", "ADV", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.10": {"text": "Segen,", "tokens": ["Se\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.6": {"line.1": {"text": "Da\u00df, wenn uns etwan strenge D\u00fcrre das Gras von un-", "tokens": ["Da\u00df", ",", "wenn", "uns", "et\u00b7wan", "stren\u00b7ge", "D\u00fcr\u00b7re", "das", "Gras", "von", "un"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "KOUS", "PPER", "ADV", "ADJA", "NN", "ART", "NN", "APPR", "TRUNC"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.2": {"text": "sern Wiesen fri\u00dft,", "tokens": ["sern", "Wie\u00b7sen", "fri\u00dft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der Acker-Bau sogleich zerst\u00f6rt und alles in Verwir-", "tokens": ["Der", "A\u00b7cke\u00b7rBau", "sog\u00b7leich", "zer\u00b7st\u00f6rt", "und", "al\u00b7les", "in", "Ver\u00b7wir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVPP", "KON", "PIS", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "rung ist.", "tokens": ["rung", "ist", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Da die nohtwend\u2019gen Pferde fehlen, die Thiere, die uns", "tokens": ["Da", "die", "noht\u00b7wen\u00b7d'\u00b7gen", "Pfer\u00b7de", "feh\u00b7len", ",", "die", "Thie\u00b7re", ",", "die", "uns"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVINF", "$,", "ART", "NN", "$,", "PRELS", "PPER"], "meter": "--+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Nahrung g\u00f6nnen,", "tokens": ["Nah\u00b7rung", "g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Und so viel Gutes thun, sind weg, weil sie sich nicht mehr", "tokens": ["Und", "so", "viel", "Gu\u00b7tes", "thun", ",", "sind", "weg", ",", "weil", "sie", "sich", "nicht", "mehr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PIAT", "NN", "VVINF", "$,", "VAFIN", "ADV", "$,", "KOUS", "PPER", "PRF", "PTKNEG", "ADV"], "meter": "---+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "n\u00e4hren k\u00f6nnen.", "tokens": ["n\u00e4h\u00b7ren", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Ein d\u00fcrrer Wind versengt das Gras, worauf wir mit", "tokens": ["Ein", "d\u00fcr\u00b7rer", "Wind", "ver\u00b7sengt", "das", "Gras", ",", "wo\u00b7rauf", "wir", "mit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,", "PWAV", "PPER", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "den F\u00fcssen gehn,", "tokens": ["den", "F\u00fcs\u00b7sen", "gehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Und gleich wird man die ganze Menschheit verwirrt und", "tokens": ["Und", "gleich", "wird", "man", "die", "gan\u00b7ze", "Menschheit", "ver\u00b7wirrt", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PIS", "ART", "ADJA", "NN", "ADJD", "KON"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.12": {"text": "ungl\u00fcckselig sehn.", "tokens": ["un\u00b7gl\u00fcck\u00b7se\u00b7lig", "sehn", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "----+", "measure": "unknown.measure.single"}}}}}