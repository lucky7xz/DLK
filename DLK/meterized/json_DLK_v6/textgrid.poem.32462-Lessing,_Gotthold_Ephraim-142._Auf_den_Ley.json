{"textgrid.poem.32462": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "142. Auf den Ley", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der gute Mann, den Ley bei Seite dort gezogen!", "tokens": ["Der", "gu\u00b7te", "Mann", ",", "den", "Ley", "bei", "Sei\u00b7te", "dort", "ge\u00b7zo\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was Ley ihm sagt, das ist erlogen.", "tokens": ["Was", "Ley", "ihm", "sagt", ",", "das", "ist", "er\u00b7lo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "PPER", "VVFIN", "$,", "PDS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie wei\u00df ich das? \u2013 Ich h\u00f6r' ihn freilich nicht:", "tokens": ["Wie", "wei\u00df", "ich", "das", "?", "\u2013", "Ich", "h\u00f6r'", "ihn", "frei\u00b7lich", "nicht", ":"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PDS", "$.", "$(", "PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Allein ich seh doch, da\u00df er spricht.", "tokens": ["Al\u00b7lein", "ich", "seh", "doch", ",", "da\u00df", "er", "spricht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}