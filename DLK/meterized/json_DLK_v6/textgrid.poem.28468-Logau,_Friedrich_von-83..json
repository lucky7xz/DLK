{"textgrid.poem.28468": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "83.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Diener tragen in gemein ihrer Herren Lieverey;", "tokens": ["Die\u00b7ner", "tra\u00b7gen", "in", "ge\u00b7mein", "ih\u00b7rer", "Her\u00b7ren", "Lie\u00b7ve\u00b7rey", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJD", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Solls dann seyn, da\u00df Franckreich Herr, Deutschland aber Diener sey?", "tokens": ["Solls", "dann", "seyn", ",", "da\u00df", "Fran\u00b7ck\u00b7reich", "Herr", ",", "Deutschland", "a\u00b7ber", "Die\u00b7ner", "sey", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VAINF", "$,", "KOUS", "NE", "NN", "$,", "NE", "KON", "NN", "VAFIN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Freyes Deutschland sch\u00e4m dich doch dieser schn\u00f6den Knechterey!", "tokens": ["Frey\u00b7es", "Deutschland", "sch\u00e4m", "dich", "doch", "die\u00b7ser", "schn\u00f6\u00b7den", "Knech\u00b7te\u00b7rey", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PPER", "ADV", "PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}}}}