{"textgrid.poem.53993": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Diskretion", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df Josefine eine schiefe Nase hat;", "tokens": ["Da\u00df", "Jo\u00b7se\u00b7fi\u00b7ne", "ei\u00b7ne", "schie\u00b7fe", "Na\u00b7se", "hat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "da\u00df Karlchen eine schwache Blase hat;", "tokens": ["da\u00df", "Karl\u00b7chen", "ei\u00b7ne", "schwa\u00b7che", "Bla\u00b7se", "hat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "da\u00df Doktor O., was sicher stimmt,", "tokens": ["da\u00df", "Dok\u00b7tor", "O.", ",", "was", "si\u00b7cher", "stimmt", ","], "token_info": ["word", "word", "abbreviation", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NE", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "aus einem dunkeln Fonds sich Gelder nimmt;", "tokens": ["aus", "ei\u00b7nem", "dun\u00b7keln", "Fonds", "sich", "Gel\u00b7der", "nimmt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PRF", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "da\u00df Zempels Briefchen nur zum Spa\u00df ein Spa\u00df ist,", "tokens": ["da\u00df", "Zem\u00b7pels", "Brief\u00b7chen", "nur", "zum", "Spa\u00df", "ein", "Spa\u00df", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NE", "ADV", "APPRART", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "und da\u00df er selbst ein falsches Aas ist", "tokens": ["und", "da\u00df", "er", "selbst", "ein", "fal\u00b7sches", "Aas", "ist"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "in allen sieben Lebenslagen \u2013:", "tokens": ["in", "al\u00b7len", "sie\u00b7ben", "Le\u00b7bens\u00b7la\u00b7gen", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIAT", "CARD", "NN", "$(", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "das kann man einem Menschen doch nicht sagen!", "tokens": ["das", "kann", "man", "ei\u00b7nem", "Men\u00b7schen", "doch", "nicht", "sa\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIS", "ART", "NN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Na, ich wei\u00df nicht \u2013", "tokens": ["Na", ",", "ich", "wei\u00df", "nicht", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PTKNEG", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Da\u00df Willy mit der Schwester Rudolfs muddelt;", "tokens": ["Da\u00df", "Wil\u00b7ly", "mit", "der", "Schwes\u00b7ter", "Ru\u00b7dolfs", "mud\u00b7delt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "ART", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "da\u00df Walter mehr als n\u00f6tig sich beschmuddelt;", "tokens": ["da\u00df", "Wal\u00b7ter", "mehr", "als", "n\u00f6\u00b7tig", "sich", "be\u00b7schmud\u00b7delt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PIAT", "KOKOM", "ADJD", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "da\u00df Eugen eine \u00fcbersch\u00e4tzte Charge;", "tokens": ["da\u00df", "Eu\u00b7gen", "ei\u00b7ne", "\u00fc\u00b7bersc\u00b7h\u00e4tz\u00b7te", "Char\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "da\u00df das Theater . . . dieser Reim wird large . . .", "tokens": ["da\u00df", "das", "The\u00b7a\u00b7ter", ".", ".", ".", "die\u00b7ser", "Reim", "wird", "lar\u00b7ge", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KOUS", "ART", "NN", "$.", "$.", "$.", "PDAT", "NN", "VAFIN", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "da\u00df Kloschs Talent, mit allem, was er macht,", "tokens": ["da\u00df", "Kloschs", "Ta\u00b7lent", ",", "mit", "al\u00b7lem", ",", "was", "er", "macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NN", "$,", "APPR", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "nicht weiter reicht als bis Berlin W 8;", "tokens": ["nicht", "wei\u00b7ter", "reicht", "als", "bis", "Ber\u00b7lin", "W", "8", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "number", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "KOKOM", "APPR", "NE", "XY", "CARD", "$."], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "da\u00df die Frau Doktor eine Bl\u00e4hung hat im Magen \u2013:", "tokens": ["da\u00df", "die", "Frau", "Dok\u00b7tor", "ei\u00b7ne", "Bl\u00e4\u00b7hung", "hat", "im", "Ma\u00b7gen", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "ART", "NN", "VAFIN", "APPRART", "NN", "$(", "$."], "meter": "+-++-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.8": {"text": "das kann man einem Menschen doch nicht sagen!", "tokens": ["das", "kann", "man", "ei\u00b7nem", "Men\u00b7schen", "doch", "nicht", "sa\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIS", "ART", "NN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Na, ich wei\u00df nicht \u2013", "tokens": ["Na", ",", "ich", "wei\u00df", "nicht", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PTKNEG", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Man mu\u00df nicht. Doch man kann.", "tokens": ["Man", "mu\u00df", "nicht", ".", "Doch", "man", "kann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PTKNEG", "$.", "KON", "PIS", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Basis unsres Lebens", "tokens": ["Die", "Ba\u00b7sis", "uns\u00b7res", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NE", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "ist: Schweigen und Verschweigen \u2013 manchmal ganz vergebens.", "tokens": ["ist", ":", "Schwei\u00b7gen", "und", "Ver\u00b7schwei\u00b7gen", "\u2013", "manch\u00b7mal", "ganz", "ver\u00b7ge\u00b7bens", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$.", "NN", "KON", "NN", "$(", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn manchmal l\u00e4uft die Wahrheit ihre Bahn \u2013", "tokens": ["Denn", "manch\u00b7mal", "l\u00e4uft", "die", "Wahr\u00b7heit", "ih\u00b7re", "Bahn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "dann werden alle wild. Dann geht es: Zahn um Zahn!", "tokens": ["dann", "wer\u00b7den", "al\u00b7le", "wild", ".", "Dann", "geht", "es", ":", "Zahn", "um", "Zahn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADJD", "$.", "ADV", "VVFIN", "PPER", "$.", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und sind sie zu dir selber offen,", "tokens": ["Und", "sind", "sie", "zu", "dir", "sel\u00b7ber", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPR", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "dann nimmst du \u00fcbel und stehst tief betroffen.", "tokens": ["dann", "nimmst", "du", "\u00fc\u00b7bel", "und", "stehst", "tief", "be\u00b7trof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Wahrheit ist ein Ding: hart und beschwerlich,", "tokens": ["Die", "Wahr\u00b7heit", "ist", "ein", "Ding", ":", "hart", "und", "be\u00b7schwer\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$.", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "sowie in h\u00f6chstem Ma\u00dfe feuergef\u00e4hrlich.", "tokens": ["so\u00b7wie", "in", "h\u00f6chs\u00b7tem", "Ma\u00b7\u00dfe", "feu\u00b7er\u00b7ge\u00b7f\u00e4hr\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Brenn mit ihr nieder, was da morsch ist \u2013", "tokens": ["Brenn", "mit", "ihr", "nie\u00b7der", ",", "was", "da", "morsch", "ist", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "PTKVZ", "$,", "PRELS", "ADV", "ADJD", "VAFIN", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.11": {"text": "und wenns dein eigner Bruder Schorsch ist!", "tokens": ["und", "wenns", "dein", "eig\u00b7ner", "Bru\u00b7der", "Schorsch", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "ADJA", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Beliebt wird man so nicht! Nach einem Menschenalter", "tokens": ["Be\u00b7liebt", "wird", "man", "so", "nicht", "!", "Nach", "ei\u00b7nem", "Men\u00b7schen\u00b7al\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PIS", "ADV", "PTKNEG", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "l\u00e4\u00dft man vom Doktor O. und Klosch und Walter", "tokens": ["l\u00e4\u00dft", "man", "vom", "Dok\u00b7tor", "O.", "und", "Klosch", "und", "Wal\u00b7ter"], "token_info": ["word", "word", "word", "word", "abbreviation", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "APPRART", "NN", "NE", "KON", "NN", "KON", "NE"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "und l\u00e4\u00dft gew\u00e4hren, wie das Leben will . . .", "tokens": ["und", "l\u00e4\u00dft", "ge\u00b7w\u00e4h\u00b7ren", ",", "wie", "das", "Le\u00b7ben", "will", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "VVFIN", "VVINF", "$,", "PWAV", "ART", "NN", "VMFIN", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Und brennt sich selber aus. Und wird ganz still.", "tokens": ["Und", "brennt", "sich", "sel\u00b7ber", "aus", ".", "Und", "wird", "ganz", "still", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "PTKVZ", "$.", "KON", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Na, ich wei\u00df nicht \u2013.", "tokens": ["Na", ",", "ich", "wei\u00df", "nicht", "\u2013", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PTKNEG", "$(", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}