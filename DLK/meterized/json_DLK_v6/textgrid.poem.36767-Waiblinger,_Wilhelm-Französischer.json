{"textgrid.poem.36767": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Franz\u00f6sischer", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcrchterlich saust der Orkan; es sch\u00e4umt das Meer aus dem Grund auf,", "tokens": ["F\u00fcrch\u00b7ter\u00b7lich", "saust", "der", "Or\u00b7kan", ";", "es", "sch\u00e4umt", "das", "Meer", "aus", "dem", "Grund", "auf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Wolken bersten, es scheint heut' die Natur zu vergehn.", "tokens": ["Wol\u00b7ken", "bers\u00b7ten", ",", "es", "scheint", "heut'", "die", "Na\u00b7tur", "zu", "ver\u00b7gehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Regen sch\u00fcttet, es zittert die Erd', es w\u00fcthet der Donner,", "tokens": ["Re\u00b7gen", "sch\u00fct\u00b7tet", ",", "es", "zit\u00b7tert", "die", "Erd'", ",", "es", "w\u00fct\u00b7het", "der", "Don\u00b7ner", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Eichen splittern, der Wind wurzelt in Wahnsinn sie aus.", "tokens": ["Ei\u00b7chen", "split\u00b7tern", ",", "der", "Wind", "wur\u00b7zelt", "in", "Wahn\u00b7sinn", "sie", "aus", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "ART", "NN", "VVFIN", "APPR", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Grauenerweckende Nacht! der verheerende Blitz nur beleuchtet,", "tokens": ["Grau\u00b7e\u00b7ner\u00b7we\u00b7cken\u00b7de", "Nacht", "!", "der", "ver\u00b7hee\u00b7ren\u00b7de", "Blitz", "nur", "be\u00b7leuch\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ART", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "St\u00fcrme wehen das Haar schrecklich dem Maler empor,", "tokens": ["St\u00fcr\u00b7me", "we\u00b7hen", "das", "Haar", "schreck\u00b7lich", "dem", "Ma\u00b7ler", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "St\u00fcrme fassen ihm schon den Regenschirm, und der Blitz zuckt", "tokens": ["St\u00fcr\u00b7me", "fas\u00b7sen", "ihm", "schon", "den", "Re\u00b7gen\u00b7schirm", ",", "und", "der", "Blitz", "zuckt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$,", "KON", "ART", "NN", "VVFIN"], "meter": "+-+--+-+-+--++", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Flammend auf ihn, doch er bleibt, malt und studirt die Natur.", "tokens": ["Flam\u00b7mend", "auf", "ihn", ",", "doch", "er", "bleibt", ",", "malt", "und", "stu\u00b7dirt", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "$,", "KON", "PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}}}}}