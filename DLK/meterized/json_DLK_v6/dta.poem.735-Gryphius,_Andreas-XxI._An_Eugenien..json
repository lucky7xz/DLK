{"dta.poem.735": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxI.  An  Eugenien.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6n ist ein sch\u00f6ner Leib den aller Lippen preisen! ", "tokens": ["Sch\u00f6n", "ist", "ein", "sch\u00f6\u00b7ner", "Leib", "den", "al\u00b7ler", "Lip\u00b7pen", "prei\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der von nicht schlechtem Stam v\u00f1 edlen ", "tokens": ["Der", "von", "nicht", "schlech\u00b7tem", "Stam", "v\u00f1", "ed\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PTKNEG", "ADJA", "NN", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch sch\u00f6ner wenn den Leib ein edle Seele zihrt ", "tokens": ["Doch", "sch\u00f6\u00b7ner", "wenn", "den", "Leib", "ein", "ed\u00b7le", "See\u00b7le", "zihrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "KOUS", "ART", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die einig sich nur l\u00e4st die Tugend vnterweisen.", "tokens": ["Die", "ei\u00b7nig", "sich", "nur", "l\u00e4st", "die", "Tu\u00b7gend", "vn\u00b7ter\u00b7wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "ADV", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Vielmehr wenn Wei\u00dfheit noch/ nach der wir oftmals reisen", "tokens": ["Viel\u00b7mehr", "wenn", "Wei\u00df\u00b7heit", "noch", "/", "nach", "der", "wir", "oft\u00b7mals", "rei\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "NN", "ADV", "$(", "APPR", "PRELS", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie in der Wiegen lehrt/ mehr wenn sie Zucht anf\u00fchrt ", "tokens": ["Sie", "in", "der", "Wie\u00b7gen", "lehrt", "/", "mehr", "wenn", "sie", "Zucht", "an\u00b7f\u00fchrt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$(", "ADV", "KOUS", "PPER", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd Heilig seyn ergetzt/ vnd Demut stets regirt.", "tokens": ["Vnd", "Hei\u00b7lig", "seyn", "er\u00b7getzt", "/", "vnd", "De\u00b7mut", "stets", "re\u00b7girt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVPP", "$(", "KON", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mehr wenn jhr keuscher Geist nicht zagt f\u00fcr ", "tokens": ["Mehr", "wenn", "jhr", "keu\u00b7scher", "Geist", "nicht", "zagt", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPOSAT", "ADJA", "NN", "PTKNEG", "VVFIN", "APPR"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Di\u00df sch\u00e4tz ich r\u00fchmens wehrt/ di\u00df ist was diese Welt ", "tokens": ["Di\u00df", "sch\u00e4tz", "ich", "r\u00fch\u00b7mens", "wehrt", "/", "di\u00df", "ist", "was", "die\u00b7se", "Welt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "VVFIN", "$(", "PDS", "VAFIN", "PIS", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die aller Sch\u00f6nheit ", "tokens": ["Die", "al\u00b7ler", "Sch\u00f6n\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["ART", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Vnd da\u00df man billich mag der ", "tokens": ["Vnd", "da\u00df", "man", "bil\u00b7lich", "mag", "der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PIS", "ADJD", "VMFIN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wer dieses schawen wil wird finden was er sucht", "tokens": ["Wer", "die\u00b7ses", "scha\u00b7wen", "wil", "wird", "fin\u00b7den", "was", "er", "sucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PDAT", "VVINF", "VMFIN", "VAFIN", "VVINF", "PWS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Vnd kaum zu finden ist/ wenn er O Blum der Zucht/", "tokens": ["Vnd", "kaum", "zu", "fin\u00b7den", "ist", "/", "wenn", "er", "O", "Blum", "der", "Zucht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKZU", "VVINF", "VAFIN", "$(", "KOUS", "PPER", "NE", "NE", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O sch\u00f6nste/ wenn er euch/ ", "tokens": ["O", "sch\u00f6ns\u00b7te", "/", "wenn", "er", "euch", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$(", "KOUS", "PPER", "PPER", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}