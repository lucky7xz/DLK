{"textgrid.poem.29318": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "33.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier liegt ein Grab im Grab, und in de\u00df Grabes Grabe,", "tokens": ["Hier", "liegt", "ein", "Grab", "im", "Grab", ",", "und", "in", "de\u00df", "Gra\u00b7bes", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,", "KON", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was Welt noch nie gesehn, ihm auch nicht Namen gabe;", "tokens": ["Was", "Welt", "noch", "nie", "ge\u00b7sehn", ",", "ihm", "auch", "nicht", "Na\u00b7men", "ga\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ADV", "ADV", "VVPP", "$,", "PPER", "ADV", "PTKNEG", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Grab begrub zuvor, eh Grab begraben ward;", "tokens": ["Das", "Grab", "be\u00b7grub", "zu\u00b7vor", ",", "eh", "Grab", "be\u00b7gra\u00b7ben", "ward", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "KOUS", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zwey Gr\u00e4ber sind nur Eins, und eine Leich ein Paar.", "tokens": ["Zwey", "Gr\u00e4\u00b7ber", "sind", "nur", "Eins", ",", "und", "ei\u00b7ne", "Leich", "ein", "Paar", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "ADV", "NN", "$,", "KON", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}