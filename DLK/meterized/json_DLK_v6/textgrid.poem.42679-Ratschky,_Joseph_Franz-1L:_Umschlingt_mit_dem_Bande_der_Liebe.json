{"textgrid.poem.42679": {"metadata": {"author": {"name": "Ratschky, Joseph Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Umschlingt mit dem Bande der Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Umschlingt mit dem Bande der Liebe", "tokens": ["Um\u00b7schlingt", "mit", "dem", "Ban\u00b7de", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Den Vater der Eintracht! Vom Triebe", "tokens": ["Den", "Va\u00b7ter", "der", "Ein\u00b7tracht", "!", "Vom", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$.", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des Dankes sey jeder beseelt!", "tokens": ["Des", "Dan\u00b7kes", "sey", "je\u00b7der", "be\u00b7seelt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Er ist's, der die Glieder der Kette,", "tokens": ["Er", "ist's", ",", "der", "die", "Glie\u00b7der", "der", "Ket\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Die wir in des Heiligthums St\u00e4tte", "tokens": ["Die", "wir", "in", "des", "Hei\u00b7lig\u00b7thums", "St\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Hier schliessen, so weislich gew\u00e4hlt.", "tokens": ["Hier", "schlies\u00b7sen", ",", "so", "weis\u00b7lich", "ge\u00b7w\u00e4hlt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "ADJD", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Die edelsten Erze nur schied er", "tokens": ["Die", "e\u00b7dels\u00b7ten", "Er\u00b7ze", "nur", "schied", "er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "PPER"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Zum Stoff f\u00fcr die Kette: drum, Br\u00fcder,", "tokens": ["Zum", "Stoff", "f\u00fcr", "die", "Ket\u00b7te", ":", "drum", ",", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$.", "PAV", "$,", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Riss keiner der Ringe bisher,", "tokens": ["Riss", "kei\u00b7ner", "der", "Rin\u00b7ge", "bis\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIS", "ART", "NN", "ADV", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und freudig bezeugen wir alle:", "tokens": ["Und", "freu\u00b7dig", "be\u00b7zeu\u00b7gen", "wir", "al\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PIS", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Nie schied noch die edlen Metalle", "tokens": ["Nie", "schied", "noch", "die", "ed\u00b7len", "Me\u00b7tal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Ein K\u00fcnstler so sicher, als er.", "tokens": ["Ein", "K\u00fcnst\u00b7ler", "so", "si\u00b7cher", ",", "als", "er", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,", "KOUS", "PPER", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}