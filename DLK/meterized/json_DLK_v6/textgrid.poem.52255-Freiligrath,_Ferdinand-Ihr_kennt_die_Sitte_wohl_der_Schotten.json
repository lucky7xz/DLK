{"textgrid.poem.52255": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Ihr kennt die Sitte wohl der Schotten", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr kennt die Sitte wohl der Schotten: \u2013", "tokens": ["Ihr", "kennt", "die", "Sit\u00b7te", "wohl", "der", "Schot\u00b7ten", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Galt es ein rasch Zusammenrotten,", "tokens": ["Galt", "es", "ein", "rasch", "Zu\u00b7sam\u00b7men\u00b7rot\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Aufgl\u00fchte dann der Feuerbrand.", "tokens": ["Auf\u00b7gl\u00fch\u00b7te", "dann", "der", "Feu\u00b7er\u00b7brand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gel\u00f6scht in Blut an beiden Enden,", "tokens": ["Ge\u00b7l\u00f6scht", "in", "Blut", "an", "bei\u00b7den", "En\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Krieg heischend, lie\u00df er sich entsenden", "tokens": ["Krieg", "hei\u00b7schend", ",", "lie\u00df", "er", "sich", "ent\u00b7sen\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "$,", "VVFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von Haus zu Haus, von Hand zu Hand. \u2013", "tokens": ["Von", "Haus", "zu", "Haus", ",", "von", "Hand", "zu", "Hand", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,", "APPR", "NN", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und als der Sandwirt wollte schlagen;", "tokens": ["Und", "als", "der", "Sand\u00b7wirt", "woll\u00b7te", "schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als er bereit nun stand, zu wagen", "tokens": ["Als", "er", "be\u00b7reit", "nun", "stand", ",", "zu", "wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "ADV", "VVFIN", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den Adlerflug, den Gemsensprung:", "tokens": ["Den", "Ad\u00b7ler\u00b7flug", ",", "den", "Gem\u00b7sen\u00b7sprung", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da trat sein Hausweib hin zu Passer,", "tokens": ["Da", "trat", "sein", "Haus\u00b7weib", "hin", "zu", "Pas\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "und warf in das emp\u00f6rte Wasser", "tokens": ["und", "warf", "in", "das", "em\u00b7p\u00f6r\u00b7te", "Was\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Sp\u00e4ne der Verk\u00fcndigung.", "tokens": ["Die", "Sp\u00e4\u00b7ne", "der", "Ver\u00b7k\u00fcn\u00b7di\u00b7gung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Rasch in die Tale mit den Wellen", "tokens": ["Rasch", "in", "die", "Ta\u00b7le", "mit", "den", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bis vor des Talvolks rauhe Schwellen", "tokens": ["Bis", "vor", "des", "Tal\u00b7volks", "rau\u00b7he", "Schwel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bachabw\u00e4rts rollte Span auf Span.", "tokens": ["Bach\u00b7ab\u00b7w\u00e4rts", "roll\u00b7te", "Span", "auf", "Span", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df alles fertig auf den Firnen,", "tokens": ["Da\u00df", "al\u00b7les", "fer\u00b7tig", "auf", "den", "Fir\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und da\u00df zum Losbruch reif ihr Z\u00fcrnen \u2013", "tokens": ["Und", "da\u00df", "zum", "Los\u00b7bruch", "reif", "ihr", "Z\u00fcr\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPRART", "NN", "ADJD", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Blut, Mehl und Sp\u00e4ne sagten's an!", "tokens": ["Blut", ",", "Mehl", "und", "Sp\u00e4\u00b7ne", "sag\u00b7ten's", "an", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So meine Lieder m\u00f6cht' ich s\u00e4en! \u2013", "tokens": ["So", "mei\u00b7ne", "Lie\u00b7der", "m\u00f6cht'", "ich", "s\u00e4\u00b7en", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie die Ladurner m\u00e4cht' ich stehen", "tokens": ["Wie", "die", "La\u00b7dur\u00b7ner", "m\u00e4cht'", "ich", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "An dem bewegten Strom der Zeit!", "tokens": ["An", "dem", "be\u00b7weg\u00b7ten", "Strom", "der", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wahrzeichen, frisch und rauh wie jene,", "tokens": ["Wahr\u00b7zei\u00b7chen", ",", "frisch", "und", "rauh", "wie", "je\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "KOKOM", "PDS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "M\u00f6cht' ich sie werfen, blut'ge Sp\u00e4ne,", "tokens": ["M\u00f6cht'", "ich", "sie", "wer\u00b7fen", ",", "blut'\u00b7ge", "Sp\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Aus in der Tageswogen Streit!", "tokens": ["Aus", "in", "der", "Ta\u00b7ges\u00b7wo\u00b7gen", "Streit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und, gleich Hochschotlands Feuerbr\u00e4nden,", "tokens": ["Und", ",", "gleich", "Hoch\u00b7schot\u00b7lands", "Feu\u00b7er\u00b7br\u00e4n\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Hei\u00df durch mein Volk m\u00e4cht' ich sie senden", "tokens": ["Hei\u00df", "durch", "mein", "Volk", "m\u00e4cht'", "ich", "sie", "sen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In jede Mark, an jeden Herd:", "tokens": ["In", "je\u00b7de", "Mark", ",", "an", "je\u00b7den", "Herd", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df alles zu den Waffen f\u00fchre,", "tokens": ["Da\u00df", "al\u00b7les", "zu", "den", "Waf\u00b7fen", "f\u00fch\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und rasselnd riefe: \u00bbSch\u00fcre, sch\u00fcre!", "tokens": ["Und", "ras\u00b7selnd", "rie\u00b7fe", ":", "\u00bb", "Sch\u00fc\u00b7re", ",", "sch\u00fc\u00b7re", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$.", "$(", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wo ist der Kampf? Wir stehn bewehrt!\u00ab", "tokens": ["Wo", "ist", "der", "Kampf", "?", "Wir", "stehn", "be\u00b7wehrt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$.", "PPER", "VVFIN", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Noch harr' ich, in mich selbst versunken!", "tokens": ["Noch", "harr'", "ich", ",", "in", "mich", "selbst", "ver\u00b7sun\u00b7ken", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur dann und wann blitzt auf ein Funken", "tokens": ["Nur", "dann", "und", "wann", "blitzt", "auf", "ein", "Fun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "KON", "PWAV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Glut, die meine Br\u00e4nde brennt!", "tokens": ["Der", "Glut", ",", "die", "mei\u00b7ne", "Br\u00e4n\u00b7de", "brennt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur dann und wann mit frischem Munde", "tokens": ["Nur", "dann", "und", "wann", "mit", "fri\u00b7schem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "KON", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Geb' einen Blutspan ich der Stunde", "tokens": ["Geb'", "ei\u00b7nen", "Bluts\u00b7pan", "ich", "der", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von denen, so die Passer kennt!", "tokens": ["Von", "de\u00b7nen", ",", "so", "die", "Pas\u00b7ser", "kennt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "$,", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Was h\u00fclfen mehr? Schleicht doch in D\u00e4mmen", "tokens": ["Was", "h\u00fcl\u00b7fen", "mehr", "?", "Schleicht", "doch", "in", "D\u00e4m\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "$.", "NN", "ADV", "APPR", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ihr Wasser heut! \u2013 Doch \u00fcberschwemmen", "tokens": ["Ihr", "Was\u00b7ser", "heut", "!", "\u2013", "Doch", "\u00fc\u00b7bersc\u00b7hwem\u00b7men"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "$.", "$(", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wird einst das Land sie, k\u00fchn zu schaun!", "tokens": ["Wird", "einst", "das", "Land", "sie", ",", "k\u00fchn", "zu", "schaun", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PPER", "$,", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann tret' ich vor mit Blut und Mehle \u2013", "tokens": ["Dann", "tret'", "ich", "vor", "mit", "Blut", "und", "Meh\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Frei weht die Eiche meiner Seele:", "tokens": ["Frei", "weht", "die", "Ei\u00b7che", "mei\u00b7ner", "See\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Ich glaub', ich werde Sp\u00e4ne haun!", "tokens": ["Ich", "glaub'", ",", "ich", "wer\u00b7de", "Sp\u00e4\u00b7ne", "haun", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}