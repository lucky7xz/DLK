{"dta.poem.19321": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "54.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist Zusammenhang der Rede, den du lobst,", "tokens": ["Was", "ist", "Zu\u00b7sam\u00b7men\u00b7hang", "der", "Re\u00b7de", ",", "den", "du", "lobst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dessen Zauberkraft du tief an dir erprobst?", "tokens": ["Und", "des\u00b7sen", "Zau\u00b7ber\u00b7kraft", "du", "tief", "an", "dir", "er\u00b7probst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "PPER", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zusammen scheinen dir zu hangen die Gedanken,", "tokens": ["Zu\u00b7sam\u00b7men", "schei\u00b7nen", "dir", "zu", "han\u00b7gen", "die", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKZU", "VVINF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und drum die Sachen auch, die stets so unst\u00e4t schwanken.", "tokens": ["Und", "drum", "die", "Sa\u00b7chen", "auch", ",", "die", "stets", "so", "un\u00b7st\u00e4t", "schwan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ART", "NN", "ADV", "$,", "PRELS", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Frage dr\u00e4ngt sich auf, und wird zur\u00fcckgedr\u00e4ngt,", "tokens": ["Die", "Fra\u00b7ge", "dr\u00e4ngt", "sich", "auf", ",", "und", "wird", "zu\u00b7r\u00fcck\u00b7ge\u00b7dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKVZ", "$,", "KON", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob nur der Denker so zusammen sie geh\u00e4ngt?", "tokens": ["Ob", "nur", "der", "Den\u00b7ker", "so", "zu\u00b7sam\u00b7men", "sie", "ge\u00b7h\u00e4ngt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Zusammenhang der Ding' ist wirklich ihm erschienen,", "tokens": ["Zu\u00b7sam\u00b7men\u00b7hang", "der", "Ding'", "ist", "wirk\u00b7lich", "ihm", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VAFIN", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und seine Rede zeigt dir diesen Schein an ihnen.", "tokens": ["Und", "sei\u00b7ne", "Re\u00b7de", "zeigt", "dir", "die\u00b7sen", "Schein", "an", "ih\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "PDAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Am ganzen Netze willst du keine Masche missen;", "tokens": ["Am", "gan\u00b7zen", "Net\u00b7ze", "willst", "du", "kei\u00b7ne", "Ma\u00b7sche", "mis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VMFIN", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn eine fehlt, so ist der sch\u00f6ne Schein zerrissen.", "tokens": ["Denn", "ei\u00b7ne", "fehlt", ",", "so", "ist", "der", "sch\u00f6\u00b7ne", "Schein", "zer\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "$,", "ADV", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}