{"textgrid.poem.38106": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Tr\u00fcmmeken Tanz", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.57", "nl:0.42"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Hinrich und siene Br\u00f6der alle dree, voll grone,", "tokens": ["Herr", "Hin\u00b7rich", "und", "sie\u00b7ne", "Br\u00f6\u00b7der", "al\u00b7le", "dree", ",", "voll", "gro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "KON", "ADJA", "NN", "PIAT", "CARD", "$,", "ADJD", "ADJA", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Se buuden een Schepken tor See, um de adlige Rosenblome,", "tokens": ["Se", "buu\u00b7den", "e\u00b7en", "Schep\u00b7ken", "tor", "See", ",", "um", "de", "ad\u00b7li\u00b7ge", "Ro\u00b7sen\u00b7blo\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "VVFIN", "NN", "$,", "KOUI", "NE", "ADJA", "NN", "$,"], "meter": "+--+-+--+--+--+-+-", "measure": "iambic.septa.invert"}, "line.3": {"text": "Do dat Schepken rede was, voll grone,", "tokens": ["Do", "dat", "Schep\u00b7ken", "re\u00b7de", "was", ",", "voll", "gro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PIS", "$,", "ADJD", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Se setten sick darin, se f\u00f6hrde alle daher, um de adliche Rosenblome", "tokens": ["Se", "set\u00b7ten", "sick", "da\u00b7rin", ",", "se", "f\u00f6hr\u00b7de", "al\u00b7le", "da\u00b7her", ",", "um", "de", "ad\u00b7li\u00b7che", "Ro\u00b7sen\u00b7blo\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PRF", "PAV", "$,", "FM", "FM", "PIS", "PAV", "$,", "KOUI", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+--+--+--+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.5": {"text": "Do se Westwerts averkemen, voll grone,", "tokens": ["Do", "se", "West\u00b7werts", "av\u00b7er\u00b7ke\u00b7men", ",", "voll", "gro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVINF", "$,", "ADJD", "ADJA", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Do stond dar een Goldschmits S\u00f6hne vor de D\u00f6hr, mit de adlige Rosenblome,", "tokens": ["Do", "stond", "dar", "e\u00b7en", "Gold\u00b7schmits", "S\u00f6h\u00b7ne", "vor", "de", "D\u00f6hr", ",", "mit", "de", "ad\u00b7li\u00b7ge", "Ro\u00b7sen\u00b7blo\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "ART", "NN", "NN", "APPR", "NE", "NE", "$,", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+--+--+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.7": {"text": "Weset mir willkommen, ji Herren alle dree gar h\u00fcbsch und schone", "tokens": ["We\u00b7set", "mir", "will\u00b7kom\u00b7men", ",", "ji", "Her\u00b7ren", "al\u00b7le", "dree", "gar", "h\u00fcbsch", "und", "scho\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "ADJA", "NN", "PIAT", "CARD", "ADV", "ADJD", "KON", "VVFIN"], "meter": "+-+-+--+-+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.8": {"text": "Will ji Mede, efte will ji nun Wien, sprack de adlige Rosenblome,", "tokens": ["Will", "ji", "Me\u00b7de", ",", "ef\u00b7te", "will", "ji", "nun", "Wi\u00b7en", ",", "sprack", "de", "ad\u00b7li\u00b7ge", "Ro\u00b7sen\u00b7blo\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "VVFIN", "NE", "ADJA", "NN", "$,"], "meter": "--+-+--+-+-+-+--+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.9": {"text": "Wy willen neen Mede, wy willen neen Wien, voll grone", "tokens": ["Wy", "wil\u00b7len", "ne\u00b7en", "Me\u00b7de", ",", "wy", "wil\u00b7len", "ne\u00b7en", "Wi\u00b7en", ",", "voll", "gro\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "VVFIN", "NN", "$,", "KOUS", "NN", "VVFIN", "NE", "$,", "ADJD", "ADJA"], "meter": "-+---+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Wy willen en Goldschmits Tochter han, de van de adlige Rosenblome.", "tokens": ["Wy", "wil\u00b7len", "en", "Gold\u00b7schmits", "Toch\u00b7ter", "han", ",", "de", "van", "de", "ad\u00b7li\u00b7ge", "Ro\u00b7sen\u00b7blo\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "NE", "NE", "NN", "VAFIN", "$,", "NE", "NE", "NE", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-+--+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.11": {"text": "Des Goldschmits Tochter krieg ji nig, gar h\u00fcbsch und schone,", "tokens": ["Des", "Gold\u00b7schmits", "Toch\u00b7ter", "krieg", "ji", "nig", ",", "gar", "h\u00fcbsch", "und", "scho\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "NN", "NE", "NE", "$,", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Se is L\u00fctke Leicke al togesegt, de adlige Rosenblome.", "tokens": ["Se", "is", "L\u00fct\u00b7ke", "Lei\u00b7cke", "al", "to\u00b7ge\u00b7segt", ",", "de", "ad\u00b7li\u00b7ge", "Ro\u00b7sen\u00b7blo\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM", "FM", "NE", "NN", "ADV", "VVFIN", "$,", "NE", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-+--+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.13": {"text": "L\u00fctke Leicke de kriegt se nig, voll grone,", "tokens": ["L\u00fct\u00b7ke", "Lei\u00b7cke", "de", "kriegt", "se", "nig", ",", "voll", "gro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,", "ADJD", "ADJA", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "Dar will wy dree unse Halse um wagen, um de adlige Rosenblome.", "tokens": ["Dar", "will", "wy", "dree", "un\u00b7se", "Hal\u00b7se", "um", "wa\u00b7gen", ",", "um", "de", "ad\u00b7li\u00b7ge", "Ro\u00b7sen\u00b7blo\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "KOUI", "NE", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+-+--+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.15": {"text": "L\u00fctke Leicke tog ut sien blankes Schwerd, voll grone,", "tokens": ["L\u00fct\u00b7ke", "Lei\u00b7cke", "tog", "ut", "sien", "blan\u00b7kes", "Schwerd", ",", "voll", "gro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "FM", "FM", "ADJA", "NN", "$,", "ADJD", "ADJA", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.16": {"text": "He houde Herr Hinrich sien l\u00fctgen Finger af, um de adlige Rosenblome.", "tokens": ["He", "hou\u00b7de", "Herr", "Hin\u00b7rich", "sien", "l\u00fct\u00b7gen", "Fin\u00b7ger", "af", ",", "um", "de", "ad\u00b7li\u00b7ge", "Ro\u00b7sen\u00b7blo\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ADV", "CARD", "ADJA", "NN", "NE", "$,", "KOUI", "NE", "ADJA", "NN", "$."], "meter": "-+-+---+-+-+--+--+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.17": {"text": "Herr Hinrich tog ut sien blankes Schwerd, gar h\u00fcbsch und schone,", "tokens": ["Herr", "Hin\u00b7rich", "tog", "ut", "sien", "blan\u00b7kes", "Schwerd", ",", "gar", "h\u00fcbsch", "und", "scho\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "FM", "FM", "FM", "ADJA", "NN", "$,", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "He houde L\u00fctke Leicke sien H\u00f6vende wedder af, um de adlige Rosenblome.", "tokens": ["He", "hou\u00b7de", "L\u00fct\u00b7ke", "Lei\u00b7cke", "sien", "H\u00f6\u00b7ven\u00b7de", "wed\u00b7der", "af", ",", "um", "de", "ad\u00b7li\u00b7ge", "Ro\u00b7sen\u00b7blo\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NN", "CARD", "NN", "KON", "NE", "$,", "KOUI", "NE", "ADJA", "NN", "$."], "meter": "---+-+--+--+-+--+--+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.19": {"text": "Myn Hert is hundert tusend Freuden voll, um de adlige Rosenblome,", "tokens": ["Myn", "Hert", "is", "hun\u00b7dert", "tu\u00b7send", "Freu\u00b7den", "voll", ",", "um", "de", "ad\u00b7li\u00b7ge", "Ro\u00b7sen\u00b7blo\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "FM", "CARD", "CARD", "NN", "ADJD", "$,", "KOUI", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+--+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.20": {"text": "L\u00fctke Leicke siene Kinder wenden all so sehr voll grone,", "tokens": ["L\u00fct\u00b7ke", "Lei\u00b7cke", "sie\u00b7ne", "Kin\u00b7der", "wen\u00b7den", "all", "so", "sehr", "voll", "gro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJA", "NN", "VVFIN", "PIAT", "ADV", "ADV", "ADJD", "ADJA", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.21": {"text": "Morgen schallen wy unsern Vader begraven, um de adlige Rosenblome.", "tokens": ["Mor\u00b7gen", "schal\u00b7len", "wy", "un\u00b7sern", "Va\u00b7der", "be\u00b7gra\u00b7ven", ",", "um", "de", "ad\u00b7li\u00b7ge", "Ro\u00b7sen\u00b7blo\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NE", "PPOSAT", "NN", "VVINF", "$,", "KOUI", "NE", "ADJA", "NN", "$."], "meter": "+-+-+--+--+---+--+-+-", "measure": "trochaic.octa.plus.relaxed"}}}}}