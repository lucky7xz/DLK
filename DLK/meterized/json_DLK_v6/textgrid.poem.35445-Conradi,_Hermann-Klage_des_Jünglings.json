{"textgrid.poem.35445": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Klage des J\u00fcnglings", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo seid ihr hingegangen,", "tokens": ["Wo", "seid", "ihr", "hin\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Meine frommen, unschuldigen Kinderaugen?", "tokens": ["Mei\u00b7ne", "from\u00b7men", ",", "un\u00b7schul\u00b7di\u00b7gen", "Kin\u00b7der\u00b7au\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Wo seid ihr hingegangen,", "tokens": ["Wo", "seid", "ihr", "hin\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die ihr in prangenden Reizen", "tokens": ["Die", "ihr", "in", "pran\u00b7gen\u00b7den", "Rei\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die Welt mir verk\u00fcndigt", "tokens": ["Die", "Welt", "mir", "ver\u00b7k\u00fcn\u00b7digt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "In meines Lebens erster Morgenfr\u00fche?", "tokens": ["In", "mei\u00b7nes", "Le\u00b7bens", "ers\u00b7ter", "Mor\u00b7gen\u00b7fr\u00fc\u00b7he", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wo seid ihr hingegangen,", "tokens": ["Wo", "seid", "ihr", "hin\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die ihr z\u00e4rtlich bestauntet", "tokens": ["Die", "ihr", "z\u00e4rt\u00b7lich", "be\u00b7staun\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "VVPP"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Jedwede Kreatur,", "tokens": ["Jed\u00b7we\u00b7de", "Kre\u00b7a\u00b7tur", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Flut und Kristall,", "tokens": ["Flut", "und", "Kris\u00b7tall", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Und voll Inbrunst", "tokens": ["Und", "voll", "In\u00b7brunst"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Wunder um Wunder schautet?", "tokens": ["Wun\u00b7der", "um", "Wun\u00b7der", "schau\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Wo seid ihr hingegangen,", "tokens": ["Wo", "seid", "ihr", "hin\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Meine frommen, unschuldigen Kinderaugen?", "tokens": ["Mei\u00b7ne", "from\u00b7men", ",", "un\u00b7schul\u00b7di\u00b7gen", "Kin\u00b7der\u00b7au\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Sehet! Ich sehne mich euch nach,", "tokens": ["Se\u00b7het", "!", "Ich", "seh\u00b7ne", "mich", "euch", "nach", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PPER", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Ein J\u00fcngling, ein Mann,", "tokens": ["Ein", "J\u00fcng\u00b7ling", ",", "ein", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Dem die Welt sich nun malt", "tokens": ["Dem", "die", "Welt", "sich", "nun", "malt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "NN", "PRF", "ADV", "VVFIN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.6": {"text": "In nackten, n\u00fcchternen Farben!", "tokens": ["In", "nack\u00b7ten", ",", "n\u00fcch\u00b7ter\u00b7nen", "Far\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Sehet! Ich sehne mich euch nach,", "tokens": ["Se\u00b7het", "!", "Ich", "seh\u00b7ne", "mich", "euch", "nach", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PPER", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ich weine euch nach \u2013", "tokens": ["Ich", "wei\u00b7ne", "euch", "nach", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Dem keuschen Blick", "tokens": ["Dem", "keu\u00b7schen", "Blick"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Meiner ersten Jugend \u2013", "tokens": ["Mei\u00b7ner", "ers\u00b7ten", "Ju\u00b7gend", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Als zum ersten Male", "tokens": ["Als", "zum", "ers\u00b7ten", "Ma\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Ich um mich blickte", "tokens": ["Ich", "um", "mich", "blick\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "PPER", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Und der Bilder F\u00fclle", "tokens": ["Und", "der", "Bil\u00b7der", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Mich trunken machte \u2013", "tokens": ["Mich", "trun\u00b7ken", "mach\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Uns\u00e4gliche Sehnsucht", "tokens": ["Un\u00b7s\u00e4g\u00b7li\u00b7che", "Sehn\u00b7sucht"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "In mir weckte \u2013", "tokens": ["In", "mir", "weck\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Doch stilles Gen\u00fcgen", "tokens": ["Doch", "stil\u00b7les", "Ge\u00b7n\u00fc\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Zugleich mich besa\u00df!", "tokens": ["Zu\u00b7gleich", "mich", "be\u00b7sa\u00df", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Sehet! Ich sehne mich euch nach,", "tokens": ["Se\u00b7het", "!", "Ich", "seh\u00b7ne", "mich", "euch", "nach", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PPER", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Verlorene Augen der Unschuld,", "tokens": ["Ver\u00b7lo\u00b7re\u00b7ne", "Au\u00b7gen", "der", "Un\u00b7schuld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Nun ich ein anderer ward", "tokens": ["Nun", "ich", "ein", "an\u00b7de\u00b7rer", "ward"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "ART", "ADJA", "VAFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und anders die Welt", "tokens": ["Und", "an\u00b7ders", "die", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Sich mir verk\u00fcndigt.", "tokens": ["Sich", "mir", "ver\u00b7k\u00fcn\u00b7digt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "PPER", "VVPP", "$."], "meter": "++-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Es fiel", "tokens": ["Es", "fiel"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "In der hingleitenden Zeiten Spiel", "tokens": ["In", "der", "hing\u00b7lei\u00b7ten\u00b7den", "Zei\u00b7ten", "Spiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Binde und H\u00fclle \u2013", "tokens": ["Bin\u00b7de", "und", "H\u00fcl\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und \u00fcber mich str\u00f6mte sich aus", "tokens": ["Und", "\u00fc\u00b7ber", "mich", "str\u00f6m\u00b7te", "sich", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PRF", "APPR"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Die F\u00fclle", "tokens": ["Die", "F\u00fcl\u00b7le"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Der Wirklichkeiten, der m\u00e4rchenlosen \u2013", "tokens": ["Der", "Wirk\u00b7lich\u00b7kei\u00b7ten", ",", "der", "m\u00e4r\u00b7chen\u00b7lo\u00b7sen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Es verdorrten", "tokens": ["Es", "ver\u00b7dorr\u00b7ten"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Meiner frommen Neugier \u2013", "tokens": ["Mei\u00b7ner", "from\u00b7men", "Neu\u00b7gier", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Meiner keuschen Sehnsucht", "tokens": ["Mei\u00b7ner", "keu\u00b7schen", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "K\u00f6stliche Jugendrosen!", "tokens": ["K\u00f6st\u00b7li\u00b7che", "Ju\u00b7gen\u00b7dro\u00b7sen", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.7": {"line.1": {"text": "Satt bin ich \u2013", "tokens": ["Satt", "bin", "ich", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Und mein ungewirktes Auge", "tokens": ["Und", "mein", "un\u00b7ge\u00b7wirk\u00b7tes", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tr\u00e4ufelt in die zusammenschauernde Seele", "tokens": ["Tr\u00e4u\u00b7felt", "in", "die", "zu\u00b7sam\u00b7men\u00b7schau\u00b7ern\u00b7de", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Nur Tropfen des Ekels ...", "tokens": ["Nur", "Trop\u00b7fen", "des", "E\u00b7kels", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ART", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Weltgierig ward ich", "tokens": ["Welt\u00b7gie\u00b7rig", "ward", "ich"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Und allgierig", "tokens": ["Und", "all\u00b7gie\u00b7rig"], "token_info": ["word", "word"], "pos": ["KON", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Und uners\u00e4ttlich \u2013", "tokens": ["Und", "un\u00b7er\u00b7s\u00e4tt\u00b7lich", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJD", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Und sp\u00e4t und fr\u00fche", "tokens": ["Und", "sp\u00e4t", "und", "fr\u00fc\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Durchtaumelte diese Brust", "tokens": ["Durch\u00b7tau\u00b7mel\u00b7te", "die\u00b7se", "Brust"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PDAT", "NN"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.10": {"text": "Unheimlicher Sehnsuchtsflammen", "tokens": ["Un\u00b7heim\u00b7li\u00b7cher", "Sehn\u00b7suchts\u00b7flam\u00b7men"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Schlangengez\u00fcngel.", "tokens": ["Schlan\u00b7gen\u00b7ge\u00b7z\u00fcn\u00b7gel", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "Nimmer mir tat ich genug \u2013", "tokens": ["Nim\u00b7mer", "mir", "tat", "ich", "ge\u00b7nug", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "ADV", "$("], "meter": "+-+++-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Und auf mir lastete", "tokens": ["Und", "auf", "mir", "las\u00b7te\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Segen zugleich und h\u00e4rtester Fluch ...", "tokens": ["Se\u00b7gen", "zu\u00b7gleich", "und", "h\u00e4r\u00b7tes\u00b7ter", "Fluch", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "ADJA", "NN", "$("], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.9": {"line.1": {"text": "Und ich wuchs und ich lebte,", "tokens": ["Und", "ich", "wuchs", "und", "ich", "leb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "PPER", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Bis in der zweiten", "tokens": ["Bis", "in", "der", "zwei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Oder der dritten Morgenfr\u00fche meines Lebens", "tokens": ["O\u00b7der", "der", "drit\u00b7ten", "Mor\u00b7gen\u00b7fr\u00fc\u00b7he", "mei\u00b7nes", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich alt schon ward", "tokens": ["Ich", "alt", "schon", "ward"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADJD", "ADV", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und m\u00fcde schon vor der Zeit ...", "tokens": ["Und", "m\u00fc\u00b7de", "schon", "vor", "der", "Zeit", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.10": {"line.1": {"text": "Von mir hinweggegangen", "tokens": ["Von", "mir", "hin\u00b7weg\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sind Drang und Sehnsucht", "tokens": ["Sind", "Drang", "und", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "NN", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und die Wollust des Wanderns", "tokens": ["Und", "die", "Wol\u00b7lust", "des", "Wan\u00b7derns"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapae