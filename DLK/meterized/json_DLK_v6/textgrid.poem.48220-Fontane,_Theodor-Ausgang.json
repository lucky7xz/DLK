{"textgrid.poem.48220": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Ausgang", "genre": "verse", "period": "N.A.", "pub_year": 1886, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Immer enger, leise, leise,", "tokens": ["Im\u00b7mer", "en\u00b7ger", ",", "lei\u00b7se", ",", "lei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ziehen sich die Lebenskreise,", "tokens": ["Zie\u00b7hen", "sich", "die", "Le\u00b7bens\u00b7krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwindet hin, was prahlt und prunkt,", "tokens": ["Schwin\u00b7det", "hin", ",", "was", "prahlt", "und", "prunkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PWS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwindet Hoffen, Hassen, Lieben,", "tokens": ["Schwin\u00b7det", "Hof\u00b7fen", ",", "Has\u00b7sen", ",", "Lie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "NN", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ist nichts in Sicht geblieben", "tokens": ["Und", "ist", "nichts", "in", "Sicht", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PIS", "APPR", "NN", "VVPP"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Als der letzte dunkle Punkt.", "tokens": ["Als", "der", "letz\u00b7te", "dunk\u00b7le", "Punkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}