{"textgrid.poem.34661": {"metadata": {"author": {"name": "Neukirch, Benjamin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach Flavia! du qvelle meiner schmertzen/", "genre": "verse", "period": "N.A.", "pub_year": 1697, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach Flavia! du qvelle meiner schmertzen/", "tokens": ["Ach", "Fla\u00b7via", "!", "du", "qvel\u00b7le", "mei\u00b7ner", "schmert\u00b7zen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "PPER", "VVFIN", "PPOSAT", "VVINF", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Was hat dir doch dein treuer knecht gethan?", "tokens": ["Was", "hat", "dir", "doch", "dein", "treu\u00b7er", "knecht", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df nicht dein ohr die seuffzer meines hertzen", "tokens": ["Da\u00df", "nicht", "dein", "ohr", "die", "seuff\u00b7zer", "mei\u00b7nes", "hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "PPOSAT", "NN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mehr/ wie vorhin/ gedultig leiden kan?", "tokens": ["Mehr", "/", "wie", "vor\u00b7hin", "/", "ge\u00b7dul\u00b7tig", "lei\u00b7den", "kan", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "$(", "KOKOM", "ADV", "$(", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und da\u00df mein mund nach hundert tausend k\u00fcssen/", "tokens": ["Und", "da\u00df", "mein", "mund", "nach", "hun\u00b7dert", "tau\u00b7send", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "APPR", "CARD", "CARD", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Doch endlich nun in thr\u00e4nen mu\u00df zerfliessen?", "tokens": ["Doch", "end\u00b7lich", "nun", "in", "thr\u00e4\u00b7nen", "mu\u00df", "zer\u00b7flies\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein sclave/ der die jammer-vollen ketten", "tokens": ["Ein", "scla\u00b7ve", "/", "der", "die", "jam\u00b7mer\u00b7vol\u00b7len", "ket\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "$(", "ART", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur mit gedult und tieffer demut tr\u00e4gt/", "tokens": ["Nur", "mit", "ge\u00b7dult", "und", "tief\u00b7fer", "de\u00b7mut", "tr\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Kan endlich doch sich durch die flucht erretten/", "tokens": ["Kan", "end\u00b7lich", "doch", "sich", "durch", "die", "flucht", "er\u00b7ret\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "PRF", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn ihm die zeit die fessel abgelegt:", "tokens": ["Wenn", "ihm", "die", "zeit", "die", "fes\u00b7sel", "ab\u00b7ge\u00b7legt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ach aber! ach! wo soll ich rettung finden?", "tokens": ["Ach", "a\u00b7ber", "!", "ach", "!", "wo", "soll", "ich", "ret\u00b7tung", "fin\u00b7den", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$.", "XY", "$.", "PWAV", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Weil mich dein arm kan allenthalben binden.", "tokens": ["Weil", "mich", "dein", "arm", "kan", "al\u00b7len\u00b7thal\u00b7ben", "bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJD", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich mu\u00df nur stehn und deine straffe suchen/", "tokens": ["Ich", "mu\u00df", "nur", "stehn", "und", "dei\u00b7ne", "straf\u00b7fe", "su\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "KON", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die/ sch\u00f6nste/ mir dein strenges auge spricht:", "tokens": ["Die", "/", "sch\u00f6ns\u00b7te", "/", "mir", "dein", "stren\u00b7ges", "au\u00b7ge", "spricht", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "VVFIN", "$(", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sonst m\u00fcst ich offt dem himmel selber fluchen/", "tokens": ["Sonst", "m\u00fcst", "ich", "offt", "dem", "him\u00b7mel", "sel\u00b7ber", "flu\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn er den glantz durch tr\u00fcbe wolcken bricht.", "tokens": ["Wenn", "er", "den", "glantz", "durch", "tr\u00fc\u00b7be", "wol\u00b7cken", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dein hertz ist ja der himmel meiner freuden;", "tokens": ["Dein", "hertz", "ist", "ja", "der", "him\u00b7mel", "mei\u00b7ner", "freu\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Drum will ich auch itzt seine donner leiden.", "tokens": ["Drum", "will", "ich", "auch", "itzt", "sei\u00b7ne", "don\u00b7ner", "lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich bin bi\u00dfher zu gl\u00fccklich fast gewesen/", "tokens": ["Ich", "bin", "bi\u00df\u00b7her", "zu", "gl\u00fcck\u00b7lich", "fast", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKA", "ADJD", "ADV", "VAPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich habe dich mehr als zu viel gek\u00fcst:", "tokens": ["Ich", "ha\u00b7be", "dich", "mehr", "als", "zu", "viel", "ge\u00b7k\u00fcst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIAT", "KOKOM", "APPR", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn ich den schnee von deiner brust gelesen/", "tokens": ["Wenn", "ich", "den", "schnee", "von", "dei\u00b7ner", "brust", "ge\u00b7le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und unser mund wie thau zerflossen ist:", "tokens": ["Und", "un\u00b7ser", "mund", "wie", "thau", "zer\u00b7flos\u00b7sen", "ist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "KOKOM", "NE", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dieweil ich nun verbotne frucht genossen/", "tokens": ["Die\u00b7weil", "ich", "nun", "ver\u00b7bot\u00b7ne", "frucht", "ge\u00b7nos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So wird mir auch mein paradie\u00df verschlossen.", "tokens": ["So", "wird", "mir", "auch", "mein", "pa\u00b7ra\u00b7die\u00df", "ver\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wohlan denn! wenn ich nicht soll l\u00e4nger leben/", "tokens": ["Wo\u00b7hlan", "denn", "!", "wenn", "ich", "nicht", "soll", "l\u00e4n\u00b7ger", "le\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "KOUS", "PPER", "PTKNEG", "VMFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O Flavia! so sterb ich mit gedult.", "tokens": ["O", "Fla\u00b7via", "!", "so", "sterb", "ich", "mit", "ge\u00b7dult", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "ADV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Hastu mich gleich mit thr\u00e4nen itzt vergeben/", "tokens": ["Has\u00b7tu", "mich", "gleich", "mit", "thr\u00e4\u00b7nen", "itzt", "ver\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So geb ich dir doch/ sch\u00f6nste/ keine schuld.", "tokens": ["So", "geb", "ich", "dir", "doch", "/", "sch\u00f6ns\u00b7te", "/", "kei\u00b7ne", "schuld", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "$(", "VVFIN", "$(", "PIAT", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Denn wer zu fr\u00fch will mit dem feuer spielen/", "tokens": ["Denn", "wer", "zu", "fr\u00fch", "will", "mit", "dem", "feu\u00b7er", "spie\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PTKA", "ADJD", "VMFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mu\u00df endlich sich mit solchem wasser k\u00fchlen.", "tokens": ["Mu\u00df", "end\u00b7lich", "sich", "mit", "sol\u00b7chem", "was\u00b7ser", "k\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PRF", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}