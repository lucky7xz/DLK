{"textgrid.poem.48819": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "44. An ihr Herze, in ihrer Krankheit", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach wehe dir und mir, o Brunnen meiner Z\u00e4ren!", "tokens": ["Ach", "we\u00b7he", "dir", "und", "mir", ",", "o", "Brun\u00b7nen", "mei\u00b7ner", "Z\u00e4\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "KON", "PPER", "$,", "FM", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die Hitze, die dich kreischt, die treibet mir den Schwei\u00df", "tokens": ["die", "Hit\u00b7ze", ",", "die", "dich", "kreischt", ",", "die", "trei\u00b7bet", "mir", "den", "Schwei\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "PRELS", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "des kalten Todes aus. Mir wird f\u00fcr K\u00e4lte hei\u00df", "tokens": ["des", "kal\u00b7ten", "To\u00b7des", "aus", ".", "Mir", "wird", "f\u00fcr", "K\u00e4l\u00b7te", "hei\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$.", "PPER", "VAFIN", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "von deiner nahen Brunst, dem feurigen Beschweren.", "tokens": ["von", "dei\u00b7ner", "na\u00b7hen", "Brunst", ",", "dem", "feu\u00b7ri\u00b7gen", "Be\u00b7schwe\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was kanst doch du von mir, von dir ich Rat begehren?", "tokens": ["Was", "kanst", "doch", "du", "von", "mir", ",", "von", "dir", "ich", "Rat", "be\u00b7geh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "PPER", "APPR", "PPER", "$,", "APPR", "PPER", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein Feuer ist mein Frost; ich werd' ein kaltes Eis,", "tokens": ["Dein", "Feu\u00b7er", "ist", "mein", "Frost", ";", "ich", "werd'", "ein", "kal\u00b7tes", "Eis", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "das zu entz\u00fcnden nur und nicht zu l\u00f6schen wei\u00df.", "tokens": ["das", "zu", "ent\u00b7z\u00fcn\u00b7den", "nur", "und", "nicht", "zu", "l\u00f6\u00b7schen", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKZU", "VVINF", "ADV", "KON", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ach wehe dir und mir, da\u00df wir uns so gef\u00e4hren!", "tokens": ["Ach", "we\u00b7he", "dir", "und", "mir", ",", "da\u00df", "wir", "uns", "so", "ge\u00b7f\u00e4h\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "KON", "PPER", "$,", "KOUS", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Macht nun die Hitze Frost und l\u00f6scht das Eis nicht mehr?", "tokens": ["Macht", "nun", "die", "Hit\u00b7ze", "Frost", "und", "l\u00f6scht", "das", "Eis", "nicht", "mehr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "NN", "KON", "VVFIN", "ART", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ach, widrige Natur, du scherzest unsre Schmerzen!", "tokens": ["Ach", ",", "wid\u00b7ri\u00b7ge", "Na\u00b7tur", ",", "du", "scher\u00b7zest", "uns\u00b7re", "Schmer\u00b7zen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O gar nicht gleicher Tod zwei gleichgesinnter Herzen!", "tokens": ["O", "gar", "nicht", "glei\u00b7cher", "Tod", "zwei", "gleich\u00b7ge\u00b7sinn\u00b7ter", "Her\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKNEG", "ADJA", "NN", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch wird uns scheiden Nichts und z\u00fcrnt sie noch so sehr.", "tokens": ["Doch", "wird", "uns", "schei\u00b7den", "Nichts", "und", "z\u00fcrnt", "sie", "noch", "so", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJA", "NN", "KON", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Tod, der macht uns gleich, wir sterben doch zusammen.", "tokens": ["Der", "Tod", ",", "der", "macht", "uns", "gleich", ",", "wir", "ster\u00b7ben", "doch", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein Feuer nehrt mein Eis, mein Eis nehrt deine Flammen.", "tokens": ["Dein", "Feu\u00b7er", "nehrt", "mein", "Eis", ",", "mein", "Eis", "nehrt", "dei\u00b7ne", "Flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}