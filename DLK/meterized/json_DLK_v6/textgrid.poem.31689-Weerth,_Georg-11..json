{"textgrid.poem.31689": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sind wohl \u00fcber der Stadt", "tokens": ["Es", "sind", "wohl", "\u00fc\u00b7ber", "der", "Stadt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Die Abendglocken erklungen.", "tokens": ["Die", "A\u00b7bend\u00b7glo\u00b7cken", "er\u00b7klun\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Des K\u00fcsters T\u00f6chterlein hat", "tokens": ["Des", "K\u00fcs\u00b7ters", "T\u00f6ch\u00b7ter\u00b7lein", "hat"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Sie mit wei\u00dfer Hand geschwungen.", "tokens": ["Sie", "mit", "wei\u00b7\u00dfer", "Hand", "ge\u00b7schwun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Als der erste Ton erklang,", "tokens": ["Als", "der", "ers\u00b7te", "Ton", "er\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da sah ich hinter dem Walde,", "tokens": ["Da", "sah", "ich", "hin\u00b7ter", "dem", "Wal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie die leuchtende Sonne sank \u2013", "tokens": ["Wie", "die", "leuch\u00b7ten\u00b7de", "Son\u00b7ne", "sank", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und Nacht lag \u00fcber der Halde.", "tokens": ["Und", "Nacht", "lag", "\u00fc\u00b7ber", "der", "Hal\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Der zweite t\u00f6nte darauf;", "tokens": ["Der", "zwei\u00b7te", "t\u00f6n\u00b7te", "da\u00b7rauf", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PAV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Da zogen viel muntere Sterne", "tokens": ["Da", "zo\u00b7gen", "viel", "mun\u00b7te\u00b7re", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Den dunklen Himmel herauf", "tokens": ["Den", "dunk\u00b7len", "Him\u00b7mel", "her\u00b7auf"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und blitzten in duftiger Ferne.", "tokens": ["Und", "blitz\u00b7ten", "in", "duf\u00b7ti\u00b7ger", "Fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Den letzten h\u00f6rte ich nicht \u2013", "tokens": ["Den", "letz\u00b7ten", "h\u00f6r\u00b7te", "ich", "nicht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "PTKNEG", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Ihn holten die Abendwinde.", "tokens": ["Ihn", "hol\u00b7ten", "die", "Ab\u00b7end\u00b7win\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich k\u00fc\u00dfte ein rosig Gesicht,", "tokens": ["Ich", "k\u00fc\u00df\u00b7te", "ein", "ro\u00b7sig", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Und \u00fcber uns rauschte die Linde.", "tokens": ["Und", "\u00fc\u00b7ber", "uns", "rauschte", "die", "Lin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "NE", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}