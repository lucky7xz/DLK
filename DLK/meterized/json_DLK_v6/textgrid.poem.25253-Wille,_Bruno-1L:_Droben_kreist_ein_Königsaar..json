{"textgrid.poem.25253": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Droben kreist ein K\u00f6nigsaar.", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Droben kreist ein K\u00f6nigsaar.", "tokens": ["Dro\u00b7ben", "kreist", "ein", "K\u00f6\u00b7nig\u00b7saar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf zu ihm ins Blau der L\u00fcfte,", "tokens": ["Auf", "zu", "ihm", "ins", "Blau", "der", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00dcber Tann und H\u00f6hlengr\u00fcfte!", "tokens": ["\u00dc\u00b7ber", "Tann", "und", "H\u00f6h\u00b7len\u00b7gr\u00fcf\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Himmlische Ferne", "tokens": ["Himm\u00b7li\u00b7sche", "Fer\u00b7ne"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Lockt und l\u00e4chelt mir wolkenlos klar.", "tokens": ["Lockt", "und", "l\u00e4\u00b7chelt", "mir", "wol\u00b7ken\u00b7los", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Bist du droben, Heimatland?", "tokens": ["Bist", "du", "dro\u00b7ben", ",", "Hei\u00b7mat\u00b7land", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sturm und Woge rauscht hienieden,", "tokens": ["Sturm", "und", "Wo\u00b7ge", "rauscht", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein Pilgram seufzt um Frieden,", "tokens": ["Und", "ein", "Pilg\u00b7ram", "seufzt", "um", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil er die Heimat", "tokens": ["Weil", "er", "die", "Hei\u00b7mat"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Immer nur ahnt und nirgends fand.", "tokens": ["Im\u00b7mer", "nur", "ahnt", "und", "nir\u00b7gends", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "KON", "ADV", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Nur im Traume wird sie mein.", "tokens": ["Nur", "im", "Trau\u00b7me", "wird", "sie", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VAFIN", "PPER", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bette, Fels, dies m\u00fcde Haupt,", "tokens": ["Bet\u00b7te", ",", "Fels", ",", "dies", "m\u00fc\u00b7de", "Haupt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "PDS", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das entt\u00e4uscht noch immer glaubt.", "tokens": ["Das", "ent\u00b7t\u00e4uscht", "noch", "im\u00b7mer", "glaubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kehre nun, Seele,", "tokens": ["Keh\u00b7re", "nun", ",", "See\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Zu den Gefilden tief innen ein!", "tokens": ["Zu", "den", "Ge\u00b7fil\u00b7den", "tief", "in\u00b7nen", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "ADV", "PTKVZ", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Werde Hauch und Melodie!", "tokens": ["Wer\u00b7de", "Hauch", "und", "Me\u00b7lo\u00b7die", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie des Mondes Duft auf Auen", "tokens": ["Wie", "des", "Mon\u00b7des", "Duft", "auf", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df dein Schmachten niedertauen!", "tokens": ["La\u00df", "dein", "Schmach\u00b7ten", "nie\u00b7der\u00b7tau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Br\u00e4utliche Blumen", "tokens": ["Br\u00e4ut\u00b7li\u00b7che", "Blu\u00b7men"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Wecken im Kusse des Schauens Magie.", "tokens": ["We\u00b7cken", "im", "Kus\u00b7se", "des", "Schau\u00b7ens", "Ma\u00b7gie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.5": {"line.1": {"text": "Schau in alle Kreatur!", "tokens": ["Schau", "in", "al\u00b7le", "Kre\u00b7a\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lausche! Und mit frommen T\u00f6nen", "tokens": ["Lau\u00b7sche", "!", "Und", "mit", "from\u00b7men", "T\u00f6\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "R\u00fchre dich das Allvers\u00f6hnen!", "tokens": ["R\u00fch\u00b7re", "dich", "das", "All\u00b7ver\u00b7s\u00f6h\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Suchender J\u00fcnger,", "tokens": ["Su\u00b7chen\u00b7der", "J\u00fcn\u00b7ger", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Folge des Lichtes heiliger Spur/", "tokens": ["Fol\u00b7ge", "des", "Lich\u00b7tes", "hei\u00b7li\u00b7ger", "Spur", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$("], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Bis das Heiligtum enth\u00fcllt,", "tokens": ["Bis", "das", "Hei\u00b7lig\u00b7tum", "ent\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo aus Z\u00e4hrenflut sich Wonnen", "tokens": ["Wo", "aus", "Z\u00e4h\u00b7ren\u00b7flut", "sich", "Won\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "PRF", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00e4utern und aus S\u00fcndern Sonnen.", "tokens": ["L\u00e4u\u00b7tern", "und", "aus", "S\u00fcn\u00b7dern", "Son\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "APPR", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00f6lbungen blauen;", "tokens": ["W\u00f6l\u00b7bun\u00b7gen", "blau\u00b7en", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Liebende Sehnsucht wird endlos erf\u00fcllt.", "tokens": ["Lie\u00b7ben\u00b7de", "Sehn\u00b7sucht", "wird", "end\u00b7los", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.7": {"line.1": {"text": "Droben kreist ein K\u00f6nigsaar", "tokens": ["Dro\u00b7ben", "kreist", "ein", "K\u00f6\u00b7nig\u00b7saar"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcber Tann und H\u00f6hlengr\u00fcfte ...", "tokens": ["\u00dc\u00b7ber", "Tann", "und", "H\u00f6h\u00b7len\u00b7gr\u00fcf\u00b7te", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es l\u00e4cheln mild die L\u00fcfte:", "tokens": ["Und", "es", "l\u00e4\u00b7cheln", "mild", "die", "L\u00fcf\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbtr\u00e4umender Pilgram,", "tokens": ["\u00bb", "tr\u00e4u\u00b7men\u00b7der", "Pilg\u00b7ram", ","], "token_info": ["punct", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Dein ist die Heimat! Du tr\u00e4umest wahr!\u00ab", "tokens": ["Dein", "ist", "die", "Hei\u00b7mat", "!", "Du", "tr\u00e4u\u00b7mest", "wahr", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "NN", "$.", "PPER", "VVFIN", "PTKVZ", "$.", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}}}}