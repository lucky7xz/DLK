{"dta.poem.18004": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "247.  Die Beschauligkeit ist Seeligkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fckseelig ist wer steht auf der beschauer Bahn/", "tokens": ["Gl\u00fck\u00b7see\u00b7lig", "ist", "wer", "steht", "auf", "der", "be\u00b7schau\u00b7er", "Bahn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PWS", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Er s\u00e4het schon allhier das Seelge Leben an.", "tokens": ["Er", "s\u00e4\u00b7het", "schon", "all\u00b7hier", "das", "Seel\u00b7ge", "Le\u00b7ben", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}