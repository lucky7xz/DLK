{"textgrid.poem.40823": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seht dort den M\u00f6nch, kapuzumh\u00fcllt die Augen!", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seht dort den M\u00f6nch, kapuzumh\u00fcllt die Augen!", "tokens": ["Seht", "dort", "den", "M\u00f6nch", ",", "ka\u00b7pu\u00b7zum\u00b7h\u00fcllt", "die", "Au\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch diesen scheint ihr Wohnhaus nicht zu taugen,", "tokens": ["Doch", "die\u00b7sen", "scheint", "ihr", "Wohn\u00b7haus", "nicht", "zu", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPOSAT", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zween Adlern gleich, aus dunkeln, \u00f6den Kl\u00fcften", "tokens": ["Zween", "Ad\u00b7lern", "gleich", ",", "aus", "dun\u00b7keln", ",", "\u00f6\u00b7den", "Kl\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "ADV", "$,", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum Flug sich schwingend nach den sonn'gen L\u00fcften.", "tokens": ["Zum", "Flug", "sich", "schwin\u00b7gend", "nach", "den", "sonn'\u00b7gen", "L\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "\u00bbauf meinem Haupt, von der Kapuz' umdunkelt,", "tokens": ["\u00bb", "auf", "mei\u00b7nem", "Haupt", ",", "von", "der", "Ka\u00b7puz'", "um\u00b7dun\u00b7kelt", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hat einst ein Helm mit gr\u00fcnem Zweig' gefunkelt;", "tokens": ["Hat", "einst", "ein", "Helm", "mit", "gr\u00fc\u00b7nem", "Zweig'", "ge\u00b7fun\u00b7kelt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die\u00df Herz, in eine Kutte jetzt verkrochen,", "tokens": ["Die\u00df", "Herz", ",", "in", "ei\u00b7ne", "Kut\u00b7te", "jetzt", "ver\u00b7kro\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "$,", "APPR", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An einen Panzer trieb's kampflustig Pochen!", "tokens": ["An", "ei\u00b7nen", "Pan\u00b7zer", "trie\u00b7b's", "kampf\u00b7lus\u00b7tig", "Po\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "ADJD", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Wie rauschten, Leipzig, einst auf deinen Bahnen", "tokens": ["Wie", "rauschten", ",", "Leip\u00b7zig", ",", "einst", "auf", "dei\u00b7nen", "Bah\u00b7nen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "$,", "NE", "$,", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ums trunkne Haupt uns der Begeist'rung Fahnen", "tokens": ["Ums", "trunk\u00b7ne", "Haupt", "uns", "der", "Be\u00b7geist'\u00b7rung", "Fah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ADJA", "NN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df, wer da fiel, mit Jauchzen, wohl wie trunken", "tokens": ["Da\u00df", ",", "wer", "da", "fiel", ",", "mit", "Jauch\u00b7zen", ",", "wohl", "wie", "trun\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "$,", "PWS", "ADV", "VVFIN", "$,", "APPR", "NN", "$,", "ADV", "KOKOM", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Unter des Lebens gr\u00fcnen Tisch gesunken!", "tokens": ["Un\u00b7ter", "des", "Le\u00b7bens", "gr\u00fc\u00b7nen", "Tisch", "ge\u00b7sun\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Der Himmel gl\u00fcht', als schien' er selbst zu bluten,", "tokens": ["Der", "Him\u00b7mel", "gl\u00fcht'", ",", "als", "schien'", "er", "selbst", "zu", "blu\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Sonne lag auf rothen Dampfesfluthen,", "tokens": ["Die", "Son\u00b7ne", "lag", "auf", "ro\u00b7then", "Damp\u00b7fes\u00b7flut\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als wenn ob uns der Purpur Deutschlands schwebte,", "tokens": ["Als", "wenn", "ob", "uns", "der", "Pur\u00b7pur", "Deutschlands", "schweb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "KOUS", "PPER", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und sie auf ihm als Kaiserkrone bebte!", "tokens": ["Und", "sie", "auf", "ihm", "als", "Kai\u00b7ser\u00b7kro\u00b7ne", "beb\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPER", "KOUS", "NN", "VVFIN", "$."], "meter": "-+---+-+-+-", "measure": "dactylic.init"}}, "stanza.5": {"line.1": {"text": "Uns Alle deckte mild sein Riesenschatten,", "tokens": ["Uns", "Al\u00b7le", "deck\u00b7te", "mild", "sein", "Rie\u00b7sen\u00b7schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Darunter focht sich's gut und ohn' Ermatten!", "tokens": ["Da\u00b7run\u00b7ter", "focht", "sich's", "gut", "und", "ohn'", "Er\u00b7mat\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "ADJD", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch saht ihr's, wie in Fetzen er zerflogen", "tokens": ["Doch", "saht", "ih\u00b7r's", ",", "wie", "in", "Fet\u00b7zen", "er", "zer\u00b7flo\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWAV", "APPR", "NN", "PPER", "VVINF"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und Nebel blieb, der gaukelnd uns belogen!", "tokens": ["Und", "Ne\u00b7bel", "blieb", ",", "der", "gau\u00b7kelnd", "uns", "be\u00b7lo\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,", "PRELS", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Die Banner, drauf in Gold: Freiheit! geschienen,", "tokens": ["Die", "Ban\u00b7ner", ",", "drauf", "in", "Gold", ":", "Frei\u00b7heit", "!", "ge\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "APPR", "NN", "$.", "NN", "$.", "VVPP", "$,"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sie sind zerrissen, und das Wort mit ihnen!", "tokens": ["Sie", "sind", "zer\u00b7ris\u00b7sen", ",", "und", "das", "Wort", "mit", "ih\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "KON", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mir graute nimmer vor des Kampfes W\u00fcthen,", "tokens": ["Mir", "grau\u00b7te", "nim\u00b7mer", "vor", "des", "Kamp\u00b7fes", "W\u00fct\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch bebt' ich vor des Siegeskranzes Bl\u00fcthen!", "tokens": ["Doch", "bebt'", "ich", "vor", "des", "Sie\u00b7ges\u00b7kran\u00b7zes", "Bl\u00fc\u00b7then", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Mein Lorberreis, ich gab es preis den L\u00fcften,", "tokens": ["Mein", "Lor\u00b7berr\u00b7eis", ",", "ich", "gab", "es", "preis", "den", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und die Begeist'rung trug ich stumm zu Gr\u00fcften,", "tokens": ["Und", "die", "Be\u00b7geist'\u00b7rung", "trug", "ich", "stumm", "zu", "Gr\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df sie, wie Todte in der schwarzen Erde,", "tokens": ["Da\u00df", "sie", ",", "wie", "Tod\u00b7te", "in", "der", "schwar\u00b7zen", "Er\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWAV", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In dieser Kutte still bestattet werde.", "tokens": ["In", "die\u00b7ser", "Kut\u00b7te", "still", "be\u00b7stat\u00b7tet", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Ihr, die ihr schlaft auf Leipzigs Fluren, Br\u00fcder,", "tokens": ["Ihr", ",", "die", "ihr", "schlaft", "auf", "Leip\u00b7zigs", "Flu\u00b7ren", ",", "Br\u00fc\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADJD", "APPR", "NN", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Einst tritt zu euch der Waffenbruder wieder;", "tokens": ["Einst", "tritt", "zu", "euch", "der", "Waf\u00b7fen\u00b7bru\u00b7der", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das wird ein lustbarlich Erkennen geben,", "tokens": ["Das", "wird", "ein", "lust\u00b7bar\u00b7lich", "Er\u00b7ken\u00b7nen", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJD", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Seht ihr im Maskenkleid heran ihn schweben!", "tokens": ["Seht", "ihr", "im", "Mas\u00b7ken\u00b7kleid", "he\u00b7ran", "ihn", "schwe\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Statt mich in freies, gr\u00fcnes Feld zu neigen,", "tokens": ["Statt", "mich", "in", "frei\u00b7es", ",", "gr\u00fc\u00b7nes", "Feld", "zu", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ADJA", "$,", "ADJA", "NN", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df meinem Herzen Blumen frisch entsteigen,", "tokens": ["Da\u00df", "mei\u00b7nem", "Her\u00b7zen", "Blu\u00b7men", "frisch", "ent\u00b7stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mu\u00df dann in dumpfe, dunkle Gruft ich schweben,", "tokens": ["Mu\u00df", "dann", "in", "dump\u00b7fe", ",", "dunk\u00b7le", "Gruft", "ich", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "ADJA", "$,", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Unfruchtbar, ach, im Tode, wie im Leben!", "tokens": ["Un\u00b7frucht\u00b7bar", ",", "ach", ",", "im", "To\u00b7de", ",", "wie", "im", "Le\u00b7ben", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ITJ", "$,", "APPRART", "NN", "$,", "PWAV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Statt farb'gem Kleid und blankem Wehrgeschmeide,", "tokens": ["Statt", "fa\u00b7rb'\u00b7gem", "Kleid", "und", "blan\u00b7kem", "Wehr\u00b7ge\u00b7schmei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Dran sich die arme, kahle Erde weide,", "tokens": ["Dran", "sich", "die", "ar\u00b7me", ",", "kah\u00b7le", "Er\u00b7de", "wei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "PRF", "ART", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wird meinen Leib die Kutte scheu umschleichen", "tokens": ["Wird", "mei\u00b7nen", "Leib", "die", "Kut\u00b7te", "scheu", "um\u00b7schlei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "ART", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und meine Lend' ein Strick, das Sklavenzeichen!", "tokens": ["Und", "mei\u00b7ne", "Lend'", "ein", "Strick", ",", "das", "Skla\u00b7ven\u00b7zei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Statt da\u00df bekr\u00e4nzt die Fahn' aufs Grab sich senke,", "tokens": ["Statt", "da\u00df", "be\u00b7kr\u00e4nzt", "die", "Fahn'", "aufs", "Grab", "sich", "sen\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "VVFIN", "ART", "NN", "APPRART", "NN", "PRF", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Als ob sie mein in stillem Dank gedenke,", "tokens": ["Als", "ob", "sie", "mein", "in", "stil\u00b7lem", "Dank", "ge\u00b7den\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PPOSAT", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wird die Kapuz' aufs Auge mir gerissen,", "tokens": ["Wird", "die", "Ka\u00b7puz'", "aufs", "Au\u00b7ge", "mir", "ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcrwahr, als ob sie mein sich sch\u00e4men m\u00fcssen!", "tokens": ["F\u00fcr\u00b7wahr", ",", "als", "ob", "sie", "mein", "sich", "sch\u00e4\u00b7men", "m\u00fcs\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOKOM", "KOUS", "PPER", "PPOSAT", "PRF", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Statt da\u00df im Trauermarsch die Trommeln hallen,", "tokens": ["Statt", "da\u00df", "im",