{"textgrid.poem.58236": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "274. Wo die Bundeslade kommt, mu\u00df Dagon fallen", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vergi\u00df der S\u00fcnde nur und innig an Gott klebe,", "tokens": ["Ver\u00b7gi\u00df", "der", "S\u00fcn\u00b7de", "nur", "und", "in\u00b7nig", "an", "Gott", "kle\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ADV", "KON", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00df ihm dein Herze blo\u00df, da\u00df er als Herr drin lebe!", "tokens": ["La\u00df", "ihm", "dein", "Her\u00b7ze", "blo\u00df", ",", "da\u00df", "er", "als", "Herr", "drin", "le\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "VVFIN", "ADV", "$,", "KOUS", "PPER", "KOUS", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-++-+-+", "measure": "unknown.measure.septa"}, "line.3": {"text": "Wenn seine Gegenwart erscheint in deinem Grund,", "tokens": ["Wenn", "sei\u00b7ne", "Ge\u00b7gen\u00b7wart", "er\u00b7scheint", "in", "dei\u00b7nem", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Finsternis und S\u00fcnd' verschwinden mu\u00df zur Stund.", "tokens": ["Die", "Fins\u00b7ter\u00b7nis", "und", "S\u00fcnd'", "ver\u00b7schwin\u00b7den", "mu\u00df", "zur", "Stund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos"