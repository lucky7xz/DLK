{"textgrid.poem.64234": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und sollten sie, die stets so taten,", "genre": "verse", "period": "N.A.", "pub_year": 1864, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und sollten sie, die stets so taten,", "tokens": ["Und", "soll\u00b7ten", "sie", ",", "die", "stets", "so", "ta\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "$,", "PRELS", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch jetzt des Sieges Frucht verraten: \u2013", "tokens": ["Auch", "jetzt", "des", "Sie\u00b7ges", "Frucht", "ver\u00b7ra\u00b7ten", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Kampf war dennoch k\u00e4mpfenswert.", "tokens": ["Der", "Kampf", "war", "den\u00b7noch", "k\u00e4mp\u00b7fens\u00b7wert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn wieder einmal hat nach Jahren", "tokens": ["Denn", "wie\u00b7der", "ein\u00b7mal", "hat", "nach", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mit Furcht und Schreck die Welt erfahren,", "tokens": ["Mit", "Furcht", "und", "Schreck", "die", "Welt", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie scharf es schl\u00e4gt, das deutsche Schwert!", "tokens": ["Wie", "scharf", "es", "schl\u00e4gt", ",", "das", "deut\u00b7sche", "Schwert", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das war ein Sieg, nicht lau und leidig,", "tokens": ["Das", "war", "ein", "Sieg", ",", "nicht", "lau", "und", "lei\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PTKNEG", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nein, rasch und ganz und voll und schneidig,", "tokens": ["Nein", ",", "rasch", "und", "ganz", "und", "voll", "und", "schnei\u00b7dig", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "KON", "ADV", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dran selbst der Neid nicht m\u00e4keln mag.", "tokens": ["Dran", "selbst", "der", "Neid", "nicht", "m\u00e4\u00b7keln", "mag."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PAV", "ADV", "ART", "NN", "PTKNEG", "VVINF", "VMFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das kam daher wie Wetterst\u00fcrme", "tokens": ["Das", "kam", "da\u00b7her", "wie", "Wet\u00b7ter\u00b7st\u00fcr\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PAV", "KOKOM", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und brach durch Schanzen, Tor und T\u00fcrme", "tokens": ["Und", "brach", "durch", "Schan\u00b7zen", ",", "Tor", "und", "T\u00fcr\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,", "NE", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie Gottes Blitz und Donnerschlag.", "tokens": ["Wie", "Got\u00b7tes", "Blitz", "und", "Don\u00b7ner\u00b7schlag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und als, verscheucht vom letzten Riffe,", "tokens": ["Und", "als", ",", "ver\u00b7scheucht", "vom", "letz\u00b7ten", "Rif\u00b7fe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$,", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der D\u00e4ne floh auf seine Schiffe,", "tokens": ["Der", "D\u00e4\u00b7ne", "floh", "auf", "sei\u00b7ne", "Schif\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Folgt' ihm die Rache bis ins Meer", "tokens": ["Folgt'", "ihm", "die", "Ra\u00b7che", "bis", "ins", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "APPRART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und weiter trugen schwanke Barken", "tokens": ["Und", "wei\u00b7ter", "tru\u00b7gen", "schwan\u00b7ke", "Bar\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Des deutschen Sieges stolze Marken", "tokens": ["Des", "deut\u00b7schen", "Sie\u00b7ges", "stol\u00b7ze", "Mar\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als weiland Kaiser Ottos Speer.", "tokens": ["Als", "wei\u00b7land", "Kai\u00b7ser", "Ot\u00b7tos", "Speer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Vernehmt's, ihr deutschen Namens Hasser!", "tokens": ["Ver\u00b7nehmt's", ",", "ihr", "deut\u00b7schen", "Na\u00b7mens", "Has\u00b7ser", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Lande Sieger und zu Wasser", "tokens": ["Zu", "Lan\u00b7de", "Sie\u00b7ger", "und", "zu", "Was\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Frohlockt die deutsche Heldenschaft:", "tokens": ["Froh\u00b7lockt", "die", "deut\u00b7sche", "Hel\u00b7den\u00b7schaft", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und doch ist tief dies Volk zerspalten: \u2013", "tokens": ["Und", "doch", "ist", "tief", "dies", "Volk", "zer\u00b7spal\u00b7ten", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADJD", "PDS", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nun sagt, wer mag dawider halten,", "tokens": ["Nun", "sagt", ",", "wer", "mag", "da\u00b7wi\u00b7der", "hal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWS", "VMFIN", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Versammelt einst es seine Kraft?", "tokens": ["Ver\u00b7sam\u00b7melt", "einst", "es", "sei\u00b7ne", "Kraft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}