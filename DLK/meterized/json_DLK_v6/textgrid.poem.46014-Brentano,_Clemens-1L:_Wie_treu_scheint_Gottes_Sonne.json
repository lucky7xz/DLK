 "tokens": ["La\u00df", "mir", "die", "Han\u00b7na", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df sie mich freudig gr\u00fc\u00dfe,", "tokens": ["Da\u00df", "sie", "mich", "freu\u00b7dig", "gr\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In deiner Sel'gen Reihn.", "tokens": ["In", "dei\u00b7ner", "Sel'\u00b7gen", "Reihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.30": {"line.1": {"text": "Mein Hoffen, Lieben, Glauben", "tokens": ["Mein", "Hof\u00b7fen", ",", "Lie\u00b7ben", ",", "Glau\u00b7ben"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bring' dir die Kirche dar,", "tokens": ["Bring'", "dir", "die", "Kir\u00b7che", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie deine Mutter Tauben", "tokens": ["Wie", "dei\u00b7ne", "Mut\u00b7ter", "Tau\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Geopfert am Altar.", "tokens": ["Ge\u00b7o\u00b7pfert", "am", "Al\u00b7tar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.31": {"line.1": {"text": "O Herr, zu solcher Wonne", "tokens": ["O", "Herr", ",", "zu", "sol\u00b7cher", "Won\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gabst du mir selbst Geleit,", "tokens": ["Gabst", "du", "mir", "selbst", "Ge\u00b7leit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und die geschaffne Sonne", "tokens": ["Und", "die", "ge\u00b7schaff\u00b7ne", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Scheint dennoch mir zu Leid.", "tokens": ["Scheint", "den\u00b7noch", "mir", "zu", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.32": {"line.1": {"text": "Was ist's, da\u00df ich verzage,", "tokens": ["Was", "ist's", ",", "da\u00df", "ich", "ver\u00b7za\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Welch Leid ist mir geschehn,", "tokens": ["Welch", "Leid", "ist", "mir", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die armen fl\u00fccht'gen Tage", "tokens": ["Die", "ar\u00b7men", "fl\u00fccht'\u00b7gen", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von dir geschm\u00fcckt zu sehn.", "tokens": ["Von", "dir", "ge\u00b7schm\u00fcckt", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.33": {"line.1": {"text": "Mit Sonnenglanz begr\u00fc\u00dfte", "tokens": ["Mit", "Son\u00b7nen\u00b7glanz", "be\u00b7gr\u00fc\u00df\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Zeit das Erdenland,", "tokens": ["Die", "Zeit", "das", "Er\u00b7den\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die fastend in der W\u00fcste", "tokens": ["Die", "fas\u00b7tend", "in", "der", "W\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dir im Gebet verschwand.", "tokens": ["Dir", "im", "Ge\u00b7bet", "ver\u00b7schwand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.34": {"line.1": {"text": "Das Leben dich versuchet,", "tokens": ["Das", "Le\u00b7ben", "dich", "ver\u00b7su\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mach Brot aus diesem Stein,", "tokens": ["Mach", "Brot", "aus", "die\u00b7sem", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "H\u00f6r' ich, die du verfluchet,", "tokens": ["H\u00f6r'", "ich", ",", "die", "du", "ver\u00b7flu\u00b7chet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die bunte Schlange schrein.", "tokens": ["Die", "bun\u00b7te", "Schlan\u00b7ge", "schrein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.35": {"line.1": {"text": "Und auf des Tempels Zinnen", "tokens": ["Und", "auf", "des", "Tem\u00b7pels", "Zin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und zu dem Reich der Welt", "tokens": ["Und", "zu", "dem", "Reich", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wird auf dem Berg den Sinnen", "tokens": ["Wird", "auf", "dem", "Berg", "den", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Seele ausgestellt.", "tokens": ["Die", "See\u00b7le", "aus\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.36": {"line.1": {"text": "Herr, la\u00df dein Wort mich h\u00f6ren;", "tokens": ["Herr", ",", "la\u00df", "dein", "Wort", "mich", "h\u00f6\u00b7ren", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sprich, Satan weich von ihr,", "tokens": ["Sprich", ",", "Sa\u00b7tan", "weich", "von", "ihr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ruf mit den Engelch\u00f6ren", "tokens": ["Ruf", "mit", "den", "En\u00b7gelc\u00b7h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Dein Kind zu dienen dir.", "tokens": ["Dein", "Kind", "zu", "die\u00b7nen", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.37": {"line.1": {"text": "Es sei die junge Sonne", "tokens": ["Es", "sei", "die", "jun\u00b7ge", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und diese milde Zeit", "tokens": ["Und", "die\u00b7se", "mil\u00b7de", "Zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dir eine Festeswonne", "tokens": ["Dir", "ei\u00b7ne", "Fes\u00b7tes\u00b7won\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In meiner Brust bereit.", "tokens": ["In", "mei\u00b7ner", "Brust", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.38": {"line.1": {"text": "La\u00df nicht tirannisieren", "tokens": ["La\u00df", "nicht", "ti\u00b7ran\u00b7ni\u00b7sie\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In mir das eigne Blut,", "tokens": ["In", "mir", "das", "eig\u00b7ne", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Herr la\u00df mich triumphieren", "tokens": ["Herr", "la\u00df", "mich", "tri\u00b7um\u00b7phie\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVIMP", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In deiner Wunden Flut.", "tokens": ["In", "dei\u00b7ner", "Wun\u00b7den", "Flut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.39": {"line.1": {"text": "Heran, heran ihr Bl\u00fcten", "tokens": ["He\u00b7ran", ",", "he\u00b7ran", "ihr", "Bl\u00fc\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKVZ", "$,", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nun \u00f6ffnet euren Scho\u00df", "tokens": ["Nun", "\u00f6ff\u00b7net", "eu\u00b7ren", "Scho\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Neu bricht nun ohn' Erm\u00fcden", "tokens": ["Neu", "bricht", "nun", "ohn'", "Er\u00b7m\u00fc\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Strom der Gnade los.", "tokens": ["Der", "Strom", "der", "Gna\u00b7de", "los", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.40": {"line.1": {"text": "Mit reinen Kelchen trinken", "tokens": ["Mit", "rei\u00b7nen", "Kel\u00b7chen", "trin\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sollt bald ihr Jesu Blut", "tokens": ["Sollt", "bald", "ihr", "Je\u00b7su", "Blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PPER", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn er sein Haupt l\u00e4\u00dft sinken", "tokens": ["Wenn", "er", "sein", "Haupt", "l\u00e4\u00dft", "sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dann, dann ist alles gut.", "tokens": ["Dann", ",", "dann", "ist", "al\u00b7les", "gut", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VAFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.41": {"line.1": {"text": "Wenn er erst ausgesprochen,", "tokens": ["Wenn", "er", "erst", "aus\u00b7ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Vater, das Vollbracht,", "tokens": ["Das", "Va\u00b7ter", ",", "das", "Voll\u00b7bracht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann ist der Tod gebrochen,", "tokens": ["Dann", "ist", "der", "Tod", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und nur auf Erden Nacht.", "tokens": ["Und", "nur", "auf", "Er\u00b7den", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.42": {"line.1": {"text": "Er wird