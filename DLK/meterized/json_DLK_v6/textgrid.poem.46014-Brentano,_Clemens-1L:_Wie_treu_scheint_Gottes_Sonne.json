{"textgrid.poem.46014": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie treu scheint Gottes Sonne", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie treu scheint Gottes Sonne", "tokens": ["Wie", "treu", "scheint", "Got\u00b7tes", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Heut in die Welt herein,", "tokens": ["Heut", "in", "die", "Welt", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Zeit erwacht mit Wonne", "tokens": ["Die", "Zeit", "er\u00b7wacht", "mit", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im neuen Gnadenschein.", "tokens": ["Im", "neu\u00b7en", "Gna\u00b7den\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es z\u00fcnden alle Kerzen", "tokens": ["Es", "z\u00fcn\u00b7den", "al\u00b7le", "Ker\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich schon zum Feste an,", "tokens": ["Sich", "schon", "zum", "Fes\u00b7te", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und alle frommen Herzen", "tokens": ["Und", "al\u00b7le", "from\u00b7men", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sind festlich angetan.", "tokens": ["Sind", "fest\u00b7lich", "an\u00b7ge\u00b7tan", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Welch Kleid soll ich denn nehmen,", "tokens": ["Welch", "Kleid", "soll", "ich", "denn", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hab' ich kein Hochzeitskleid?", "tokens": ["Hab'", "ich", "kein", "Hoch\u00b7zeits\u00b7kleid", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Soll ich allein mich sch\u00e4men", "tokens": ["Soll", "ich", "al\u00b7lein", "mich", "sch\u00e4\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In dieser heil'gen Zeit?", "tokens": ["In", "die\u00b7ser", "heil'\u00b7gen", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ist denn mein Kranz zerrissen,", "tokens": ["Ist", "denn", "mein", "Kranz", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist mein Gewand befleckt,", "tokens": ["Ist", "mein", "Ge\u00b7wand", "be\u00b7fleckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hilf, Herr, der mein Gewissen,", "tokens": ["Hilf", ",", "Herr", ",", "der", "mein", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "PRELS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der meine Bl\u00f6\u00dfe deckt!", "tokens": ["Der", "mei\u00b7ne", "Bl\u00f6\u00b7\u00dfe", "deckt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "La\u00df einsam mich hier trauern,", "tokens": ["La\u00df", "ein\u00b7sam", "mich", "hier", "trau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJD", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da du zur W\u00fcste giengst,", "tokens": ["Da", "du", "zur", "W\u00fcs\u00b7te", "giengst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf deine R\u00fcckkehr lauern,", "tokens": ["Auf", "dei\u00b7ne", "R\u00fcck\u00b7kehr", "lau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bis du ein Kleid mir bringst.", "tokens": ["Bis", "du", "ein", "Kleid", "mir", "bringst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich streu' mein Haupt mit Asche", "tokens": ["Ich", "streu'", "mein", "Haupt", "mit", "A\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "H\u00fcll' mich in Bu\u00dfe ein,", "tokens": ["H\u00fcll'", "mich", "in", "Bu\u00b7\u00dfe", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Mit bittren Tr\u00e4nen wasche,", "tokens": ["Mit", "bit\u00b7tren", "Tr\u00e4\u00b7nen", "wa\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich doch mein Kleid nicht rein.", "tokens": ["Ich", "doch", "mein", "Kleid", "nicht", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPOSAT", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und denk' auf alle Fragen,", "tokens": ["Und", "denk'", "auf", "al\u00b7le", "Fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Warum ich so betr\u00fcbt,", "tokens": ["Wa\u00b7rum", "ich", "so", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In diesen vierzig Tagen", "tokens": ["In", "die\u00b7sen", "vier\u00b7zig", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hat Jesus sich ge\u00fcbt.", "tokens": ["Hat", "Je\u00b7sus", "sich", "ge\u00b7\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "PRF", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Mein Heil gieng in die W\u00fcste", "tokens": ["Mein", "Heil", "gieng", "in", "die", "W\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und ward vom Feind versucht,", "tokens": ["Und", "ward", "vom", "Feind", "ver\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ach, so lang ich b\u00fc\u00dfte,", "tokens": ["Und", "ach", ",", "so", "lang", "ich", "b\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "ADV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bracht' ich doch keine Frucht.", "tokens": ["Bracht'", "ich", "doch", "kei\u00b7ne", "Frucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "So oft ich mich auch wagte", "tokens": ["So", "oft", "ich", "mich", "auch", "wag\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "PRF", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In seiner Gnade Schein,", "tokens": ["In", "sei\u00b7ner", "Gna\u00b7de", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "War mir's, als ob ich sagte,", "tokens": ["War", "mir's", ",", "als", "ob", "ich", "sag\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mach Brot aus diesem Stein.", "tokens": ["Mach", "Brot", "aus", "die\u00b7sem", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Und doch hat er vergossen", "tokens": ["Und", "doch", "hat", "er", "ver\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVINF"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Um mich sein teures Blut,", "tokens": ["Um", "mich", "sein", "teu\u00b7res", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf mich ist es geflossen,", "tokens": ["Auf", "mich", "ist", "es", "ge\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Und doch bin ich nicht gut.", "tokens": ["Und", "doch", "bin", "ich", "nicht", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Bin immer nicht ergeben,", "tokens": ["Bin", "im\u00b7mer", "nicht", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Treib' ewig hin und her,", "tokens": ["Treib'", "e\u00b7wig", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mach' das erl\u00f6ste Leben", "tokens": ["Mach'", "das", "er\u00b7l\u00f6s\u00b7te", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der armen Seele schwer.", "tokens": ["Der", "ar\u00b7men", "See\u00b7le", "schwer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Mein eignes Blut unb\u00e4ndig", "tokens": ["Mein", "eig\u00b7nes", "Blut", "un\u00b7b\u00e4n\u00b7dig"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Will stets der Herrscher sein,", "tokens": ["Will", "stets", "der", "Herr\u00b7scher", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Gott, mach mich lebendig,", "tokens": ["O", "Gott", ",", "mach", "mich", "le\u00b7ben\u00b7dig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Sei du mein Herr allein.", "tokens": ["Sei", "du", "mein", "Herr", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "La\u00df dieses Eis zerbrechen", "tokens": ["La\u00df", "die\u00b7ses", "Eis", "zer\u00b7bre\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PDAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vor deinem Sonnenschein,", "tokens": ["Vor", "dei\u00b7nem", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und zieh auf Gnadenb\u00e4chen", "tokens": ["Und", "zieh", "auf", "Gna\u00b7den\u00b7b\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Fr\u00fchling zu mir ein.", "tokens": ["Im", "Fr\u00fch\u00b7ling", "zu", "mir", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "In deiner Lieb' geborgen,", "tokens": ["In", "dei\u00b7ner", "Lieb'", "ge\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mag Lilie, die nicht spinnt,", "tokens": ["Mag", "Li\u00b7lie", ",", "die", "nicht", "spinnt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "$,", "PRELS", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mag auch kein V\u00f6glein sorgen,", "tokens": ["Mag", "auch", "kein", "V\u00f6\u00b7glein", "sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Viel wen'ger noch ein Kind.", "tokens": ["Viel", "wen'\u00b7ger", "noch", "ein", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Wie oft im jungen Herzen", "tokens": ["Wie", "oft", "im", "jun\u00b7gen", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nannt' ich mich selig so,", "tokens": ["Nannt'", "ich", "mich", "se\u00b7lig", "so", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und ward in Freud und Schmerzen", "tokens": ["Und", "ward", "in", "Freud", "und", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Recht meiner Kindschaft froh.", "tokens": ["Recht", "mei\u00b7ner", "Kind\u00b7schaft", "froh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Nur du allein kannst wissen", "tokens": ["Nur", "du", "al\u00b7lein", "kannst", "wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie mich dein Gru\u00df erquickt,", "tokens": ["Wie", "mich", "dein", "Gru\u00df", "er\u00b7quickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und was mein Herz mu\u00dft' missen,", "tokens": ["Und", "was", "mein", "Herz", "mu\u00dft'", "mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn es von dir geblickt.", "tokens": ["Wenn", "es", "von", "dir", "ge\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Kein andrer Blick kann taugen,", "tokens": ["Kein", "an\u00b7drer", "Blick", "kann", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Jesu mach mich blind,", "tokens": ["Mein", "Je\u00b7su", "mach", "mich", "blind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fchr' unter deinen Augen", "tokens": ["F\u00fchr'", "un\u00b7ter", "dei\u00b7nen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf reiner Bahn dein Kind.", "tokens": ["Auf", "rei\u00b7ner", "Bahn", "dein", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Dein Tau steht auf den Auen", "tokens": ["Dein", "Tau", "steht", "auf", "den", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und macht die Gr\u00e4ser frisch,", "tokens": ["Und", "macht", "die", "Gr\u00e4\u00b7ser", "frisch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "