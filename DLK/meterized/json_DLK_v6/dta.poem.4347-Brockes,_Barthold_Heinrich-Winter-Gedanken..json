{"dta.poem.4347": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Winter-Gedanken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Seht, wie auch zur Winter-Zeit,", "tokens": ["Seht", ",", "wie", "auch", "zur", "Win\u00b7ter\u00b7Zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Selbst wenns frieret, reift und schneit,", "tokens": ["Selbst", "wenns", "frie\u00b7ret", ",", "reift", "und", "schneit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf der hell-bestrahlten Welt", "tokens": ["Auf", "der", "hell\u00b7be\u00b7strahl\u00b7ten", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Manche Lust und Lieblichkeit", "tokens": ["Man\u00b7che", "Lust", "und", "Lieb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unsern Blicken vorgestellt!", "tokens": ["Un\u00b7sern", "Bli\u00b7cken", "vor\u00b7ge\u00b7stellt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ja es kann der Schmuck der Erden,", "tokens": ["Ja", "es", "kann", "der", "Schmuck", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VMFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn man mit Vernunft ihn sp\u00fchrt,", "tokens": ["Wenn", "man", "mit", "Ver\u00b7nunft", "ihn", "sp\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und er Blick und Herze r\u00fchrt,", "tokens": ["Und", "er", "Blick", "und", "Her\u00b7ze", "r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "KON", "VVFIN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Unsrer Andacht Vorwurf werden.", "tokens": ["Uns\u00b7rer", "An\u00b7dacht", "Vor\u00b7wurf", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Auch vom Schnee die kleinsten Blitze,", "tokens": ["Auch", "vom", "Schnee", "die", "kleins\u00b7ten", "Blit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Wenn auf sie die Sonne strahlt,", "tokens": ["Wenn", "auf", "sie", "die", "Son\u00b7ne", "strahlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Und mit Glanz und Gluht sie mahlt,", "tokens": ["Und", "mit", "Glanz", "und", "Gluht", "sie", "mahlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "PPER", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.13": {"text": "Sind uns zur Erbauung n\u00fctze.", "tokens": ["Sind", "uns", "zur", "Er\u00b7bau\u00b7ung", "n\u00fct\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Denn da von der Sonnen Schein", "tokens": ["Denn", "da", "von", "der", "Son\u00b7nen", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Sie nur kleine Spiegel seyn;", "tokens": ["Sie", "nur", "klei\u00b7ne", "Spie\u00b7gel", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "VAINF", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.16": {"text": "Deucht mich, da\u00df ihr reines Licht", "tokens": ["Deucht", "mich", ",", "da\u00df", "ihr", "rei\u00b7nes", "Licht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Durch sein Gl\u00e4nzen gleichsam spricht:", "tokens": ["Durch", "sein", "Gl\u00e4n\u00b7zen", "gleich\u00b7sam", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da ich dir die Sonne zeige;", "tokens": ["Da", "ich", "dir", "die", "Son\u00b7ne", "zei\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "So ergetz dich. Aber steige,", "tokens": ["So", "er\u00b7getz", "dich", ".", "A\u00b7ber", "stei\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit dadurch ger\u00fchrtem Sinn,", "tokens": ["Mit", "da\u00b7durch", "ge\u00b7r\u00fchr\u00b7tem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PAV", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Von mir zu dem Urbild hin,", "tokens": ["Von", "mir", "zu", "dem", "Ur\u00b7bild", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und la\u00df seinen Strahl dich r\u00fchren.", "tokens": ["Und", "la\u00df", "sei\u00b7nen", "Strahl", "dich", "r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Dann wird dich desselben Zier,", "tokens": ["Dann", "wird", "dich", "des\u00b7sel\u00b7ben", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PDAT", "NN", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.7": {"text": "So wie dich zu ihm von mir,", "tokens": ["So", "wie", "dich", "zu", "ihm", "von", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "APPR", "PPER", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Auch von ihm, zum Sch\u00f6pfer f\u00fchren,", "tokens": ["Auch", "von", "ihm", ",", "zum", "Sch\u00f6p\u00b7fer", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$,", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und dir ihren Ursprung weisen,", "tokens": ["Und", "dir", "ih\u00b7ren", "Ur\u00b7sprung", "wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um Jhn Ehrfurchts-voll zu preisen.", "tokens": ["Um", "Jhn", "Ehr\u00b7furchts\u00b7voll", "zu", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da ja aller Sonnen Schein", "tokens": ["Da", "ja", "al\u00b7ler", "Son\u00b7nen", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PIAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "(wie der Schnee, durch eine, flammet)", "tokens": ["(", "wie", "der", "Schnee", ",", "durch", "ei\u00b7ne", ",", "flam\u00b7met", ")"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "KOKOM", "ART", "NN", "$,", "APPR", "ART", "$,", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur von einer Sonn\u2019 allein,", "tokens": ["Nur", "von", "ei\u00b7ner", "Sonn'", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Als der Sonnen Sonne, stammet.", "tokens": ["Als", "der", "Son\u00b7nen", "Son\u00b7ne", ",", "stam\u00b7met", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}