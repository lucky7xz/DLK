{"dta.poem.4030": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Ermunterung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Echo des Himmels! heiliges Herz! warum", "tokens": ["E\u00b7cho", "des", "Him\u00b7mels", "!", "hei\u00b7li\u00b7ges", "Herz", "!", "wa\u00b7rum"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "ART", "NN", "$.", "ADJA", "NN", "$.", "PWAV"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Warum verstummst du unter den Lebenden,", "tokens": ["Wa\u00b7rum", "ver\u00b7stummst", "du", "un\u00b7ter", "den", "Le\u00b7ben\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Schl\u00e4fst, freies! von den G\u00f6tterlosen", "tokens": ["Schl\u00e4fst", ",", "frei\u00b7es", "!", "von", "den", "G\u00f6t\u00b7ter\u00b7lo\u00b7sen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ewig hinab in die Nacht verwiesen?", "tokens": ["E\u00b7wig", "hin\u00b7ab", "in", "die", "Nacht", "ver\u00b7wie\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Wacht denn, wie vormals, nimmer des Aethers", "tokens": ["Wacht", "denn", ",", "wie", "vor\u00b7mals", ",", "nim\u00b7mer", "des", "A\u00b7e\u00b7thers"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "$,", "PWAV", "ADV", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Licht?", "tokens": ["Licht", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Und bl\u00fcht die alte Mutter, die Erde nicht?", "tokens": ["Und", "bl\u00fcht", "die", "al\u00b7te", "Mut\u00b7ter", ",", "die", "Er\u00b7de", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und \u00fcbt der Geist nicht da und dort, nicht", "tokens": ["Und", "\u00fcbt", "der", "Geist", "nicht", "da", "und", "dort", ",", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKNEG", "ADV", "KON", "ADV", "$,", "PTKNEG"], "meter": "-+-+--++-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "L\u00e4chelnd die Liebe das Recht noch immer?", "tokens": ["L\u00e4\u00b7chelnd", "die", "Lie\u00b7be", "das", "Recht", "noch", "im\u00b7mer", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ART", "NN", "ADV", "ADV", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Nur du nicht mehr! doch mahnen die Himmlischen,", "tokens": ["Nur", "du", "nicht", "mehr", "!", "doch", "mah\u00b7nen", "die", "Himm\u00b7li\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PTKNEG", "ADV", "$.", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und stillebildend weht, wie ein kahl Gefild,", "tokens": ["Und", "stil\u00b7le\u00b7bil\u00b7dend", "weht", ",", "wie", "ein", "kahl", "Ge\u00b7fild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$,", "PWAV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Athem der Natur dich an, der", "tokens": ["Der", "A\u00b7them", "der", "Na\u00b7tur", "dich", "an", ",", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ART", "NN", "PPER", "PTKVZ", "$,", "PRELS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Alleserheiternde, seelenvolle.", "tokens": ["Al\u00b7le\u00b7ser\u00b7hei\u00b7tern\u00b7de", ",", "see\u00b7len\u00b7vol\u00b7le", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJA", "$,", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "O Hoffnung! bald, bald singen die Haine nicht", "tokens": ["O", "Hoff\u00b7nung", "!", "bald", ",", "bald", "sin\u00b7gen", "die", "Hai\u00b7ne", "nicht"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "ADV", "$,", "ADV", "VVFIN", "ART", "NN", "PTKNEG"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Lebens Lob allein, denn es ist die Zeit,", "tokens": ["Des", "Le\u00b7bens", "Lob", "al\u00b7lein", ",", "denn", "es", "ist", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "$,", "KON", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da\u00df aus der Menschen Munde sie, die", "tokens": ["Da\u00df", "aus", "der", "Men\u00b7schen", "Mun\u00b7de", "sie", ",", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "PPER", "$,", "PRELS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sch\u00f6nere Seele sich neu verk\u00fcndet,", "tokens": ["Sch\u00f6\u00b7ne\u00b7re", "See\u00b7le", "sich", "neu", "ver\u00b7k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "ADJD", "VVPP", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Dann liebender im Bunde mit Sterblichen", "tokens": ["Dann", "lie\u00b7ben\u00b7der", "im", "Bun\u00b7de", "mit", "Sterb\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPRART", "NN", "APPR", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das Element sich bildet, und dann erst reich,", "tokens": ["Das", "E\u00b7le\u00b7ment", "sich", "bil\u00b7det", ",", "und", "dann", "erst", "reich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "$,", "KON", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Bei frommer Kinder Dank, der Erde", "tokens": ["Bei", "from\u00b7mer", "Kin\u00b7der", "Dank", ",", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Brust, die unendliche, sich entfaltet,", "tokens": ["Brust", ",", "die", "un\u00b7end\u00b7li\u00b7che", ",", "sich", "ent\u00b7fal\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "$,", "PRF", "VVFIN", "$,"], "meter": "+--+-++-+-", "measure": "iambic.penta.invert"}}, "stanza.6": {"line.1": {"text": "Und unsre Tage wieder, wie Blumen, sind,", "tokens": ["Und", "uns\u00b7re", "Ta\u00b7ge", "wie\u00b7der", ",", "wie", "Blu\u00b7men", ",", "sind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "$,", "PWAV", "NN", "$,", "VAFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wo sie, des Himmels Sonne sich ausgetheilt", "tokens": ["Wo", "sie", ",", "des", "Him\u00b7mels", "Son\u00b7ne", "sich", "aus\u00b7ge\u00b7theilt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "$,", "ART", "NN", "NN", "PRF", "VVPP"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Im stillen Wechsel sieht und wieder", "tokens": ["Im", "stil\u00b7len", "Wech\u00b7sel", "sieht", "und", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "KON", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Froh in den frohen das Licht sich findet,", "tokens": ["Froh", "in", "den", "fro\u00b7hen", "das", "Licht", "sich", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Und er, der sprachlos waltet und unbekannt", "tokens": ["Und", "er", ",", "der", "sprach\u00b7los", "wal\u00b7tet", "und", "un\u00b7be\u00b7kannt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "PRELS", "ADJD", "VVFIN", "KON", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zuk\u00fcnftiges bereitet, der Gott, der Geist", "tokens": ["Zu\u00b7k\u00fcnf\u00b7ti\u00b7ges", "be\u00b7rei\u00b7tet", ",", "der", "Gott", ",", "der", "Geist"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "$,", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Im Menschenwort, am sch\u00f6nen Tage", "tokens": ["Im", "Men\u00b7schen\u00b7wort", ",", "am", "sch\u00f6\u00b7nen", "Ta\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kommenden Jahren, wie einst, sich ausspricht.", "tokens": ["Kom\u00b7men\u00b7den", "Jah\u00b7ren", ",", "wie", "einst", ",", "sich", "aus\u00b7spricht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "ADV", "$,", "PRF", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}