{"dta.poem.21457": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Seh\u2019 ich dein Aug\u2019 in Starrheit s\u00fc\u00df verloren \u2026", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Seh\u2019 ich dein Aug\u2019 in Starrheit s\u00fc\u00df verloren,", "tokens": ["Seh'", "ich", "dein", "Aug'", "in", "Star\u00b7rheit", "s\u00fc\u00df", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem sonst so sonniges Leben hold entspr\u00fcht,", "tokens": ["Dem", "sonst", "so", "son\u00b7ni\u00b7ges", "Le\u00b7ben", "hold", "ent\u00b7spr\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "D\u00fcnkt mir, Anna: du seist nicht erdgeboren,", "tokens": ["D\u00fcnkt", "mir", ",", "An\u00b7na", ":", "du", "seist", "nicht", "erd\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NE", "$.", "PPER", "VAFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df fremder Welten Zauber dich umbl\u00fcht.", "tokens": ["Da\u00df", "frem\u00b7der", "Wel\u00b7ten", "Zau\u00b7ber", "dich", "um\u00b7bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Seh\u2019 ich dann Thr\u00e4nen deine Wangen feuchten,", "tokens": ["Seh'", "ich", "dann", "Thr\u00e4\u00b7nen", "dei\u00b7ne", "Wan\u00b7gen", "feuch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Schimmern in zarter Wimper Perlen gleich,", "tokens": ["Schim\u00b7mern", "in", "zar\u00b7ter", "Wim\u00b7per", "Per\u00b7len", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "NN", "ADV", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Erschlie\u00dft mir deiner Z\u00fcge Wetterleuchten,", "tokens": ["Er\u00b7schlie\u00dft", "mir", "dei\u00b7ner", "Z\u00fc\u00b7ge", "Wet\u00b7ter\u00b7leuch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wie thaufrisch deine Seele und wie reich!", "tokens": ["Wie", "thauf\u00b7risch", "dei\u00b7ne", "See\u00b7le", "und", "wie", "reich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "KON", "PWAV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ich wei\u00df: du m\u00f6chtest gern ein Herz begl\u00fccken,", "tokens": ["Ich", "wei\u00df", ":", "du", "m\u00f6ch\u00b7test", "gern", "ein", "Herz", "be\u00b7gl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "So sch\u00f6n, so lieb, wie nimmer es geschieht.", "tokens": ["So", "sch\u00f6n", ",", "so", "lieb", ",", "wie", "nim\u00b7mer", "es", "ge\u00b7schieht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "PWAV", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Ich wei\u00df: du m\u00f6chtest es der Welt entr\u00fccken,", "tokens": ["Ich", "wei\u00df", ":", "du", "m\u00f6ch\u00b7test", "es", "der", "Welt", "ent\u00b7r\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Da\u00df also keusch und rein es auch ergl\u00fcht:", "tokens": ["Da\u00df", "al\u00b7so", "keusch", "und", "rein", "es", "auch", "er\u00b7gl\u00fcht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "KON", "ADJD", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Wie all\u2019 die Tr\u00e4ume, die dich lind umweben,", "tokens": ["Wie", "all'", "die", "Tr\u00e4u\u00b7me", ",", "die", "dich", "lind", "um\u00b7we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "$,", "PRELS", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Wie all\u2019 die Lust, die jauchzend aus dir bricht,", "tokens": ["Wie", "all'", "die", "Lust", ",", "die", "jauch\u00b7zend", "aus", "dir", "bricht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "$,", "PRELS", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Wie all\u2019 die Strahlen, die dich leis\u2019 umschweben,", "tokens": ["Wie", "all'", "die", "Strah\u00b7len", ",", "die", "dich", "leis'", "um\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "$,", "PRELS", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Du sch\u00f6ne Himmelsseele hehr und licht.", "tokens": ["Du", "sch\u00f6\u00b7ne", "Him\u00b7mels\u00b7see\u00b7le", "hehr", "und", "licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}