{"textgrid.poem.45972": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "An Frau Milderhauptmann, bei Gelegenheit der zweiten Auff\u00fchrung des Fidelio in Berlin", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hast du das Leben als Theaterproben", "tokens": ["Hast", "du", "das", "Le\u00b7ben", "als", "The\u00b7a\u00b7ter\u00b7pro\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "KOUS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vielleicht erkannt; dann freilich scheint dir matt", "tokens": ["Viel\u00b7leicht", "er\u00b7kannt", ";", "dann", "frei\u00b7lich", "scheint", "dir", "matt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "$.", "ADV", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der bretterne Triumph, zu gut zum Loben", "tokens": ["Der", "bret\u00b7ter\u00b7ne", "Tri\u00b7umph", ",", "zu", "gut", "zum", "Lo\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PTKA", "ADJD", "APPRART", "NN"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Bist du dann wohl des Erdenbeifalls satt;", "tokens": ["Bist", "du", "dann", "wohl", "des", "Er\u00b7den\u00b7bei\u00b7falls", "satt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nicht hilft dir's, wird der Vorhang dort gehoben,", "tokens": ["Nicht", "hilft", "dir's", ",", "wird", "der", "Vor\u00b7hang", "dort", "ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PIS", "$,", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df man dich hier herausgerufen hat;", "tokens": ["Da\u00df", "man", "dich", "hier", "her\u00b7aus\u00b7ge\u00b7ru\u00b7fen", "hat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Hier gilt heraus, dort gilt herein gerufen,", "tokens": ["Hier", "gilt", "he\u00b7raus", ",", "dort", "gilt", "her\u00b7ein", "ge\u00b7ru\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "$,", "ADV", "VVFIN", "PTKVZ", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Diesseit'ge Gipfel sind jenseit'ge Stufen.", "tokens": ["Dies\u00b7seit'\u00b7ge", "Gip\u00b7fel", "sind", "jen\u00b7seit'\u00b7ge", "Stu\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "PDAT", "NN", "$."], "meter": "-+-+-+++-+-", "measure": "unknown.measure.hexa"}}, "stanza.2": {"line.1": {"text": "Drum z\u00fcrne nicht, mein Lob geht dich nichts an,", "tokens": ["Drum", "z\u00fcr\u00b7ne", "nicht", ",", "mein", "Lob", "geht", "dich", "nichts", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PTKNEG", "$,", "PPOSAT", "NN", "VVFIN", "PPER", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es mag dir wohl gefallen oder schlecht,", "tokens": ["Es", "mag", "dir", "wohl", "ge\u00b7fal\u00b7len", "o\u00b7der", "schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nicht was du selbst, was Gott an dir getan,", "tokens": ["Nicht", "was", "du", "selbst", ",", "was", "Gott", "an", "dir", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PWS", "PPER", "ADV", "$,", "PRELS", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das r\u00fchrte mich so tief, das war mir recht.", "tokens": ["Das", "r\u00fchr\u00b7te", "mich", "so", "tief", ",", "das", "war", "mir", "recht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADJD", "$,", "PDS", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Himmel wei\u00df, ich bin nicht untertan", "tokens": ["Der", "Him\u00b7mel", "wei\u00df", ",", "ich", "bin", "nicht", "un\u00b7ter\u00b7tan"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VAFIN", "PTKNEG", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dem trillernden agierenden Geschlecht,", "tokens": ["Dem", "tril\u00b7lern\u00b7den", "a\u00b7gie\u00b7ren\u00b7den", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich mu\u00df die meisten lauter Schuld beschuld'gen", "tokens": ["Ich", "mu\u00df", "die", "meis\u00b7ten", "lau\u00b7ter", "Schuld", "be\u00b7schuld'\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "VVFIN", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Um deines Genius Zucht und Huld zu huld'gen.", "tokens": ["Um", "dei\u00b7nes", "Ge\u00b7nius", "Zucht", "und", "Huld", "zu", "huld'\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--++-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Bisarrheit w\u00e4r' Pizarro der Tyrann,", "tokens": ["Bi\u00b7sar\u00b7rheit", "w\u00e4r'", "Pi\u00b7zar\u00b7ro", "der", "Ty\u00b7rann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "ART", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Der uns Beethoven, Herrn der tiefern Kunst,", "tokens": ["Der", "uns", "Beet\u00b7ho\u00b7ven", ",", "Herrn", "der", "tie\u00b7fern", "Kunst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NE", "$,", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Gefangen hielte gleich dem Florestan?", "tokens": ["Ge\u00b7fan\u00b7gen", "hiel\u00b7te", "gleich", "dem", "Flo\u00b7res\u00b7tan", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nein Schlendrian und Neid um Brettergunst", "tokens": ["Nein", "Schlen\u00b7dri\u00b7an", "und", "Neid", "um", "Bret\u00b7ter\u00b7gunst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "NE", "KON", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Verdr\u00e4ngten ihn; doch alten Leierbann", "tokens": ["Ver\u00b7dr\u00e4ng\u00b7ten", "ihn", ";", "doch", "al\u00b7ten", "Lei\u00b7er\u00b7bann"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Brach nicht vergebens seine heil'ge Brunst;", "tokens": ["Brach", "nicht", "ver\u00b7ge\u00b7bens", "sei\u00b7ne", "heil'\u00b7ge", "Brunst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dir Fesseln und der Menge Taubheit springen,", "tokens": ["Dir", "Fes\u00b7seln", "und", "der", "Men\u00b7ge", "Taub\u00b7heit", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sie h\u00f6rt ihn ", "tokens": ["Sie", "h\u00f6rt", "ihn"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Wem so sein Lied aus ", "tokens": ["Wem", "so", "sein", "Lied", "aus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "PPOSAT", "NN", "APPR"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "W\u00e4r's nur ein Trostquell, der aus Kerkerswand", "tokens": ["W\u00e4r's", "nur", "ein", "Trost\u00b7quell", ",", "der", "aus", "Ker\u00b7kers\u00b7wand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Zeit durch ", "tokens": ["Der", "Zeit", "durch"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Dess' Durst kredenzet ", "tokens": ["Dess'", "Durst", "kre\u00b7den\u00b7zet"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Den Becher, da\u00df er selig T\u00f6ne trinkt,", "tokens": ["Den", "Be\u00b7cher", ",", "da\u00df", "er", "se\u00b7lig", "T\u00f6\u00b7ne", "trinkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich sage selig, weil er an dem Rand", "tokens": ["Ich", "sa\u00b7ge", "se\u00b7lig", ",", "weil", "er", "an", "dem", "Rand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Des Klangpokals mit Lust ber\u00fchrt die Stelle,", "tokens": ["Des", "Klang\u00b7po\u00b7kals", "mit", "Lust", "be\u00b7r\u00fchrt", "die", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wo ", "tokens": ["Wo"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}}}}}