{"dta.poem.473": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als sie im Garten spatziren ging.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr R\u00f6mer/ die ihr habt der Floren Bild verehret/", "tokens": ["Ihr", "R\u00f6\u00b7mer", "/", "die", "ihr", "habt", "der", "Flo\u00b7ren", "Bild", "ver\u00b7eh\u00b7ret", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PRELS", "PPER", "VAFIN", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die sich durch Huren-Lohn den G\u00f6ttern gleich gemacht/", "tokens": ["Die", "sich", "durch", "Hu\u00b7ren\u00b7Lohn", "den", "G\u00f6t\u00b7tern", "gleich", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "NN", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Seht wie die Anmuth hier aus vollem Halse lacht/", "tokens": ["Seht", "wie", "die", "An\u00b7muth", "hier", "aus", "vol\u00b7lem", "Hal\u00b7se", "lacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn meine ", "tokens": ["Wenn", "mei\u00b7ne"], "token_info": ["word", "word"], "pos": ["KOUS", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "In ihrem Garten geht/ und sich im Laub verh\u00fcllt.", "tokens": ["In", "ih\u00b7rem", "Gar\u00b7ten", "geht", "/", "und", "sich", "im", "Laub", "ver\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$(", "KON", "PRF", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "W\u00e4rt ihr zu dieser Zeit/ die ", "tokens": ["W\u00e4rt", "ihr", "zu", "die\u00b7ser", "Zeit", "/", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "PPER", "APPR", "PDAT", "NN", "$(", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und so ichs sagen darf was meine Meynung sey/", "tokens": ["Und", "so", "ichs", "sa\u00b7gen", "darf", "was", "mei\u00b7ne", "Mey\u00b7nung", "sey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "VVINF", "VMFIN", "PIS", "PPOSAT", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Durch ", "tokens": ["Durch"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}}}}}