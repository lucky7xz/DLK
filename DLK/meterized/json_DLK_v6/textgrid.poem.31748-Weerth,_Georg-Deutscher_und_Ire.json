{"textgrid.poem.31748": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Deutscher und Ire", "genre": "verse", "period": "N.A.", "pub_year": 1846, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In England war die Nacht kalt;", "tokens": ["In", "En\u00b7gland", "war", "die", "Nacht", "kalt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zwei junge Gesellen, wohlgestalt,", "tokens": ["Zwei", "jun\u00b7ge", "Ge\u00b7sel\u00b7len", ",", "wohl\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$,", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ein Deutscher und Ire, sich trafen", "tokens": ["Ein", "Deut\u00b7scher", "und", "I\u00b7re", ",", "sich", "tra\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "$,", "PRF", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und sanken auf eine Streu, zu schlafen.", "tokens": ["Und", "san\u00b7ken", "auf", "ei\u00b7ne", "Streu", ",", "zu", "schla\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Der eine schaute den andern an,", "tokens": ["Der", "ei\u00b7ne", "schau\u00b7te", "den", "an\u00b7dern", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ART", "ADJA", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und jeder dachte: \u00bbMein Schlafkumpan,", "tokens": ["Und", "je\u00b7der", "dach\u00b7te", ":", "\u00bb", "Mein", "Schlaf\u00b7kum\u00b7pan", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$.", "$(", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der ist nicht zu Haus an diesem Strande,", "tokens": ["Der", "ist", "nicht", "zu", "Haus", "an", "die\u00b7sem", "Stran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "APPR", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Der ist geboren in anderem Lande.\u00ab", "tokens": ["Der", "ist", "ge\u00b7bo\u00b7ren", "in", "an\u00b7de\u00b7rem", "Lan\u00b7de", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Und murmelten drauf zur selben Zeit:", "tokens": ["Und", "mur\u00b7mel\u00b7ten", "drauf", "zur", "sel\u00b7ben", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbund ach, das ist ein Jammer und Leid;", "tokens": ["\u00bb", "und", "ach", ",", "das", "ist", "ein", "Jam\u00b7mer", "und", "Leid", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "XY", "$,", "PDS", "VAFIN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Es scheint, ihm bl\u00fchten noch wenig Rosen \u2013", "tokens": ["Es", "scheint", ",", "ihm", "bl\u00fch\u00b7ten", "noch", "we\u00b7nig", "Ro\u00b7sen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "PIAT", "NN", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Schau seinen Rock und die schlechten Hosen.\u00ab", "tokens": ["Schau", "sei\u00b7nen", "Rock", "und", "die", "schlech\u00b7ten", "Ho\u00b7sen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPOSAT", "NN", "KON", "ART", "ADJA", "NN", "$.", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Und riefen endlich wohl lachend zugleich:", "tokens": ["Und", "rie\u00b7fen", "end\u00b7lich", "wohl", "la\u00b7chend", "zu\u00b7gleich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ADJD", "ADV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbund du kommst auch nimmer auf gr\u00fcnen Zweig!\u00ab", "tokens": ["\u00bb", "und", "du", "kommst", "auch", "nim\u00b7mer", "auf", "gr\u00fc\u00b7nen", "Zweig", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "PPER", "VVFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "$.", "$("], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und da gr\u00fc\u00dften sie sich, da\u00df hell es geklungen", "tokens": ["Und", "da", "gr\u00fc\u00df\u00b7ten", "sie", "sich", ",", "da\u00df", "hell", "es", "ge\u00b7klun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "$,", "KOUS", "ADJD", "PPER", "VVPP"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "In deutscher wohl und in irischer Zungen.", "tokens": ["In", "deut\u00b7scher", "wohl", "und", "in", "i\u00b7risc\u00b7her", "Zun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "KON", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und ob auch keiner den andern verstand \u2013", "tokens": ["Und", "ob", "auch", "kei\u00b7ner", "den", "an\u00b7dern", "ver\u00b7stand", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PIS", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Treuherzig reichten sie sich die Hand", "tokens": ["Treu\u00b7her\u00b7zig", "reich\u00b7ten", "sie", "sich", "die", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "PRF", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und wurden Genossen in Freud und Leide \u2013", "tokens": ["Und", "wur\u00b7den", "Ge\u00b7nos\u00b7sen", "in", "Freud", "und", "Lei\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Denn arme Teufel waren sie beide.", "tokens": ["Denn", "ar\u00b7me", "Teu\u00b7fel", "wa\u00b7ren", "sie", "bei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "PPER", "PIS", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}