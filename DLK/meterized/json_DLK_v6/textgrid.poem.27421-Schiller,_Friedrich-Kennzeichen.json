{"textgrid.poem.27421": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Kennzeichen", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie unterscheidet sich ", "tokens": ["Wie", "un\u00b7ter\u00b7schei\u00b7det", "sich"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "VVFIN", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Fehlen die Grazien stets, diese verlassen sie nie.", "tokens": ["Feh\u00b7len", "die", "Gra\u00b7zi\u00b7en", "stets", ",", "die\u00b7se", "ver\u00b7las\u00b7sen", "sie", "nie", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "$,", "PDS", "VVFIN", "PPER", "ADV", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Ist das Knie nur geschmeidig, so darf die Zunge schon l\u00e4stern.", "tokens": ["Ist", "das", "Knie", "nur", "ge\u00b7schmei\u00b7dig", ",", "so", "darf", "die", "Zun\u00b7ge", "schon", "l\u00e4s\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ADJD", "$,", "ADV", "VMFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Was darf der nicht begehn, der sich zu kriechen nicht sch\u00e4mt!", "tokens": ["Was", "darf", "der", "nicht", "be\u00b7gehn", ",", "der", "sich", "zu", "krie\u00b7chen", "nicht", "sch\u00e4mt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "PTKNEG", "VVINF", "$,", "PRELS", "PRF", "PTKZU", "VVINF", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.3": {"line.1": {"text": "Was du mit Bei\u00dfen verdorben, das bringst du mit Schmeicheln ins gleiche;", "tokens": ["Was", "du", "mit", "Bei\u00b7\u00dfen", "ver\u00b7dor\u00b7ben", ",", "das", "bringst", "du", "mit", "Schmei\u00b7cheln", "ins", "glei\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "VVPP", "$,", "PDS", "VVFIN", "PPER", "APPR", "NN", "APPRART", "ADJA", "$."], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Recht so, auf h\u00fcndische Art zahlst du die h\u00fcndische Schuld.", "tokens": ["Recht", "so", ",", "auf", "h\u00fcn\u00b7di\u00b7sche", "Art", "zahlst", "du", "die", "h\u00fcn\u00b7di\u00b7sche", "Schuld", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "APPR", "ADJA", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}}}}}