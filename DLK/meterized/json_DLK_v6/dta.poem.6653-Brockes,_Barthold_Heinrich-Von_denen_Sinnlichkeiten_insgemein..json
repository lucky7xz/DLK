{"dta.poem.6653": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von denen Sinnlichkeiten  \n insgemein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df wir zugleich den Geist, sein Wesen, seine Art", "tokens": ["Da\u00df", "wir", "zu\u00b7gleich", "den", "Geist", ",", "sein", "We\u00b7sen", ",", "sei\u00b7ne", "Art"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von dem, was e\u00f6rperlich, und offt die Sinnen lencket,", "tokens": ["Von", "dem", ",", "was", "e\u00b7\u00f6r\u00b7per\u00b7lich", ",", "und", "offt", "die", "Sin\u00b7nen", "len\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "ADJD", "$,", "KON", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+-+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Bem\u00fchet seyn zu unterscheiden.", "tokens": ["Be\u00b7m\u00fc\u00b7het", "seyn", "zu", "un\u00b7ter\u00b7schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VAINF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es setz\u2019t der Mensch, und wol recht als mit Freuden,", "tokens": ["Es", "setz't", "der", "Mensch", ",", "und", "wol", "recht", "als", "mit", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "ADV", "ADJD", "KOKOM", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sich aus sich selbst heraus, und zwar die meiste Zeit,", "tokens": ["Sich", "aus", "sich", "selbst", "he\u00b7raus", ",", "und", "zwar", "die", "meis\u00b7te", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PRF", "ADV", "PTKVZ", "$,", "KON", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ja, durch sich selbst geteuscht, setzt er in andre Dinge", "tokens": ["Ja", ",", "durch", "sich", "selbst", "ge\u00b7teuscht", ",", "setzt", "er", "in", "and\u00b7re", "Din\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "APPR", "PRF", "ADV", "VVPP", "$,", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das, was blo\u00df durch Gelegenheit", "tokens": ["Das", ",", "was", "blo\u00df", "durch", "Ge\u00b7le\u00b7gen\u00b7heit"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "$,", "PRELS", "ADV", "APPR", "NN"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.8": {"text": "Der Vorw\u00fcrff\u2019 er in sich von Sinnlichen empfinge.", "tokens": ["Der", "Vor\u00b7w\u00fcrff'", "er", "in", "sich", "von", "Sinn\u00b7li\u00b7chen", "emp\u00b7fin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wir sperren uns und wollen nicht verstehen,", "tokens": ["Wir", "sper\u00b7ren", "uns", "und", "wol\u00b7len", "nicht", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Da\u00df C\u00f6rper, von so unterschiednen Gaben,", "tokens": ["Da\u00df", "C\u00f6r\u00b7per", ",", "von", "so", "un\u00b7ter\u00b7schied\u00b7nen", "Ga\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Nur blo\u00df von uns die Eigenschafften haben,", "tokens": ["Nur", "blo\u00df", "von", "uns", "die", "Ei\u00b7gen\u00b7schaff\u00b7ten", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Da\u00df nichts bey ihnen sey, als blo\u00df nur ein Bewegen,", "tokens": ["Da\u00df", "nichts", "bey", "ih\u00b7nen", "sey", ",", "als", "blo\u00df", "nur", "ein", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPER", "VAFIN", "$,", "KOUS", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wodurch sie denn den Nerven das erregen,", "tokens": ["Wo\u00b7durch", "sie", "denn", "den", "Ner\u00b7ven", "das", "er\u00b7re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "ART", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Was C\u00f6rper insgemein", "tokens": ["Was", "C\u00f6r\u00b7per", "ins\u00b7ge\u00b7mein"], "token_info": ["word", "word", "word"], "pos": ["PWS", "NE", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Von C\u00f6rpern zu empfangen pflegen:", "tokens": ["Von", "C\u00f6r\u00b7pern", "zu", "emp\u00b7fan\u00b7gen", "pfle\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Wie denn auch ebenfalls kein\u2019 andre Eigenschafften", "tokens": ["Wie", "denn", "auch", "e\u00b7ben\u00b7falls", "kein'", "and\u00b7re", "Ei\u00b7gen\u00b7schaff\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "An denen Nerven selber hafften;", "tokens": ["An", "de\u00b7nen", "Ner\u00b7ven", "sel\u00b7ber", "haff\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Als da\u00df sie minder bald, bald mehr beweget seyn,", "tokens": ["Als", "da\u00df", "sie", "min\u00b7der", "bald", ",", "bald", "mehr", "be\u00b7we\u00b7get", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "ADV", "$,", "ADV", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und jede von dem Druck bald mehr bald minder leidet:", "tokens": ["Und", "je\u00b7de", "von", "dem", "Druck", "bald", "mehr", "bald", "min\u00b7der", "lei\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "APPR", "ART", "NN", "ADV", "ADV", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wann aber sich ein Sinn von audern unterscheidet;", "tokens": ["Wann", "a\u00b7ber", "sich", "ein", "Sinn", "von", "au\u00b7dern", "un\u00b7ter\u00b7schei\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PRF", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "So sey man \u00fcberf\u00fchrt, da\u00df blo\u00df der Geist allein", "tokens": ["So", "sey", "man", "\u00fc\u00b7berf\u00b7\u00fchrt", ",", "da\u00df", "blo\u00df", "der", "Geist", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "VVPP", "$,", "KOUS", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Geschickt ist zu befinden", "tokens": ["Ge\u00b7schickt", "ist", "zu", "be\u00b7fin\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Den Unterscheid, den wir durch sie empfinden.", "tokens": ["Den", "Un\u00b7ter\u00b7scheid", ",", "den", "wir", "durch", "sie", "emp\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Wenn man beflissen ist, da\u00df man erkenne,", "tokens": ["Wenn", "man", "be\u00b7flis\u00b7sen", "ist", ",", "da\u00df", "man", "er\u00b7ken\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVPP", "VAFIN", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Auf welche Weise uns das Feuer w\u00e4rm\u2019 und brenne;", "tokens": ["Auf", "wel\u00b7che", "Wei\u00b7se", "uns", "das", "Feu\u00b7er", "w\u00e4rm'", "und", "bren\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PPER", "ART", "NN", "VAFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "So dencke man nur frey,", "tokens": ["So", "den\u00b7cke", "man", "nur", "frey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Da\u00df es recht l\u00e4cherlich und kindisch sey,", "tokens": ["Da\u00df", "es", "recht", "l\u00e4\u00b7cher\u00b7lich", "und", "kin\u00b7disch", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Wofern man in das Feur die W\u00e4rme wollte setzen,", "tokens": ["Wo\u00b7fern", "man", "in", "das", "Feur", "die", "W\u00e4r\u00b7me", "woll\u00b7te", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die uns, nachdem die Hand, bald fern, bald nah dabey,", "tokens": ["Die", "uns", ",", "nach\u00b7dem", "die", "Hand", ",", "bald", "fern", ",", "bald", "nah", "da\u00b7bey", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "KOUS", "ART", "NN", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Jm Wechsel, kan ergetzen und verletzen.", "tokens": ["Jm", "Wech\u00b7sel", ",", "kan", "er\u00b7get\u00b7zen", "und", "ver\u00b7let\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VMFIN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}