{"dta.poem.1180": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Liebe und Gegen-Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Worzu dient so s\u00fcsses Blicken/ ", "tokens": ["Wor\u00b7zu", "dient", "so", "s\u00fcs\u00b7ses", "Bli\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn du bist in nichts verliebt?", "tokens": ["Wenn", "du", "bist", "in", "nichts", "ver\u00b7liebt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "APPR", "PIS", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ists/ da\u00df unser Seufftzer-schicken", "tokens": ["Ists", "/", "da\u00df", "un\u00b7ser", "Seufft\u00b7zer\u00b7schi\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$(", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Cloris dir Vergn\u00fcgen giebt?", "tokens": ["Clo\u00b7ris", "dir", "Ver\u00b7gn\u00fc\u00b7gen", "giebt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Zwar offt heist das Hertze geben", "tokens": ["Zwar", "offt", "heist", "das", "Hert\u00b7ze", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "PDS", "VVFIN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sich begeben seiner Ruh/", "tokens": ["Sich", "be\u00b7ge\u00b7ben", "sei\u00b7ner", "Ruh", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch wer immer frey will leben/", "tokens": ["Doch", "wer", "im\u00b7mer", "frey", "will", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ADJD", "VMFIN", "VVINF", "$("], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Bringt sein Leben \u00fcbel zu.", "tokens": ["Bringt", "sein", "Le\u00b7ben", "\u00fc\u00b7bel", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sch\u00f6nheit mit Verstand verm\u00e4hlet", "tokens": ["Sch\u00f6n\u00b7heit", "mit", "Ver\u00b7stand", "ver\u00b7m\u00e4h\u00b7let"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trifft offt schlechte Gleichheit an:", "tokens": ["Trifft", "offt", "schlech\u00b7te", "Gleich\u00b7heit", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Manch getreues Hertz erwehlet", "tokens": ["Manch", "ge\u00b7treu\u00b7es", "Hertz", "er\u00b7weh\u00b7let"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was nicht Farbe halten kan:", "tokens": ["Was", "nicht", "Far\u00b7be", "hal\u00b7ten", "kan", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Fremde Qual heist Achtung geben", "tokens": ["Frem\u00b7de", "Qual", "heist", "Ach\u00b7tung", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VAFIN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was f\u00fcr eine Wahl man thu;", "tokens": ["Was", "f\u00fcr", "ei\u00b7ne", "Wahl", "man", "thu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch/ wer unverliebt will leben", "tokens": ["Doch", "/", "wer", "un\u00b7ver\u00b7liebt", "will", "le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$(", "PWS", "ADJD", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bringt sein Leben \u00fcbel zu.", "tokens": ["Bringt", "sein", "Le\u00b7ben", "\u00fc\u00b7bel", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Liebe/ Cloris/ lieb in Zeiten/", "tokens": ["Lie\u00b7be", "/", "Clo\u00b7ris", "/", "lieb", "in", "Zei\u00b7ten", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NE", "$(", "ADJD", "APPR", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebe was dich wieder liebt/", "tokens": ["Lie\u00b7be", "was", "dich", "wie\u00b7der", "liebt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "PPER", "ADV", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was dir/ ohne Widerstreiten/", "tokens": ["Was", "dir", "/", "oh\u00b7ne", "Wi\u00b7der\u00b7strei\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "$(", "APPR", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sein getreues Hertze giebt.", "tokens": ["Sein", "ge\u00b7treu\u00b7es", "Hert\u00b7ze", "giebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Lieb\u2019 und Gegen-Liebe geben", "tokens": ["Lieb'", "und", "Ge\u00b7gen\u00b7Lie\u00b7be", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "S\u00fcsse Lust und stille Ruh/", "tokens": ["S\u00fcs\u00b7se", "Lust", "und", "stil\u00b7le", "Ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer von Liebe frey will leben", "tokens": ["Wer", "von", "Lie\u00b7be", "frey", "will", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "NN", "ADJD", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bringt sein Leben \u00fcbel zu.", "tokens": ["Bringt", "sein", "Le\u00b7ben", "\u00fc\u00b7bel", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}