{"dta.poem.6343": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Hitze und von der K\u00e4lte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wofern sie aber sich nicht recht entz\u00fcndet, r\u00fchret,", "tokens": ["Wo\u00b7fern", "sie", "a\u00b7ber", "sich", "nicht", "recht", "ent\u00b7z\u00fcn\u00b7det", ",", "r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PRF", "PTKNEG", "ADJD", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sich nur langsam aufwerts f\u00fchret;", "tokens": ["Und", "sich", "nur", "lang\u00b7sam", "auf\u00b7werts", "f\u00fch\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ADV", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So d\u00fcnstet sie sich aus in einer matten Gluht", "tokens": ["So", "d\u00fcns\u00b7tet", "sie", "sich", "aus", "in", "ei\u00b7ner", "mat\u00b7ten", "Gluht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vom Rauch, als eine Circkel-Fluth,", "tokens": ["Vom", "Rauch", ",", "als", "ei\u00b7ne", "Cir\u00b7ckel\u00b7Fluth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die ihr Bewegen bald in dunckler Lufft verliehret.", "tokens": ["Die", "ihr", "Be\u00b7we\u00b7gen", "bald", "in", "dunck\u00b7ler", "Lufft", "ver\u00b7lieh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}