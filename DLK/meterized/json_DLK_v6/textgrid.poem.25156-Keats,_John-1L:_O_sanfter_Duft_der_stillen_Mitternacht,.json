{"textgrid.poem.25156": {"metadata": {"author": {"name": "Keats, John", "birth": "N.A.", "death": "N.A."}, "title": "1L: O sanfter Duft der stillen Mitternacht,", "genre": "verse", "period": "N.A.", "pub_year": 1819, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O sanfter Duft der stillen Mitternacht,", "tokens": ["O", "sanf\u00b7ter", "Duft", "der", "stil\u00b7len", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der zart und sorgsam unsre Augen schlie\u00dft", "tokens": ["Der", "zart", "und", "sorg\u00b7sam", "uns\u00b7re", "Au\u00b7gen", "schlie\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "KON", "ADJD", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und schattend vor dem Lichte sie bewacht,", "tokens": ["Und", "schat\u00b7tend", "vor", "dem", "Lich\u00b7te", "sie", "be\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In Seelen g\u00f6ttliches Vergessen gie\u00dft.", "tokens": ["In", "See\u00b7len", "g\u00f6tt\u00b7li\u00b7ches", "Ver\u00b7ges\u00b7sen", "gie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O sanfter Schlaf! Schlie\u00df mir die willigen Lider,", "tokens": ["O", "sanf\u00b7ter", "Schlaf", "!", "Schlie\u00df", "mir", "die", "wil\u00b7li\u00b7gen", "Li\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-++--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Eh dieses Hymnus' letztes Wort verklingt,", "tokens": ["Eh", "die\u00b7ses", "Hym\u00b7nus'", "letz\u00b7tes", "Wort", "ver\u00b7klingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nein, h\u00f6r das Amen erst, eh schl\u00e4fernd nieder", "tokens": ["Nein", ",", "h\u00f6r", "das", "A\u00b7men", "erst", ",", "eh", "schl\u00e4\u00b7fernd", "nie\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "VVFIN", "ART", "NN", "ADV", "$,", "KOUS", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein Mohn die s\u00fc\u00dfen Gnadengaben bringt.", "tokens": ["Dein", "Mohn", "die", "s\u00fc\u00b7\u00dfen", "Gna\u00b7den\u00b7ga\u00b7ben", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dann h\u00fcte mich, sonst gie\u00dft der Tag sein Licht,", "tokens": ["Dann", "h\u00fc\u00b7te", "mich", ",", "sonst", "gie\u00dft", "der", "Tag", "sein", "Licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vielfachen Jammer br\u00fctend, auf mein Kissen,", "tokens": ["Viel\u00b7fa\u00b7chen", "Jam\u00b7mer", "br\u00fc\u00b7tend", ",", "auf", "mein", "Kis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Beh\u00fcte mich, denn ach, es schlummert nicht", "tokens": ["Be\u00b7h\u00fc\u00b7te", "mich", ",", "denn", "ach", ",", "es", "schlum\u00b7mert", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KON", "XY", "$,", "PPER", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Das wie ein Maulwurf w\u00fchlende Gewissen;", "tokens": ["Das", "wie", "ein", "Maul\u00b7wurf", "w\u00fch\u00b7len\u00b7de", "Ge\u00b7wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "KOKOM", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dreh flink den Schl\u00fcssel in ge\u00f6lten Riegeln,", "tokens": ["Dreh", "flink", "den", "Schl\u00fcs\u00b7sel", "in", "ge\u00b7\u00f6l\u00b7ten", "Rie\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die meiner Seele Springbrunn sanft versiegeln.", "tokens": ["Die", "mei\u00b7ner", "See\u00b7le", "Spring\u00b7brunn", "sanft", "ver\u00b7sie\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}