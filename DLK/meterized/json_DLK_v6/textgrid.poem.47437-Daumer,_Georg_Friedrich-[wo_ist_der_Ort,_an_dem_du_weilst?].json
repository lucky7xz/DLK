{"textgrid.poem.47437": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wo ist der Ort, an dem du weilst?]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo ist der Ort, an dem du weilst?", "tokens": ["Wo", "ist", "der", "Ort", ",", "an", "dem", "du", "weilst", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An diesem Orte will ich sterben.", "tokens": ["An", "die\u00b7sem", "Or\u00b7te", "will", "ich", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kein andrer Port auf wildem Meer;", "tokens": ["Kein", "an\u00b7drer", "Port", "auf", "wil\u00b7dem", "Meer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In diesem Porte will ich sterben.", "tokens": ["In", "die\u00b7sem", "Por\u00b7te", "will", "ich", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es l\u00e4\u00dft Karun von seinem Gold,", "tokens": ["Es", "l\u00e4\u00dft", "Ka\u00b7run", "von", "sei\u00b7nem", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Reiche nicht von seinem Horte;", "tokens": ["Der", "Rei\u00b7che", "nicht", "von", "sei\u00b7nem", "Hor\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo w\u00e4re mir ein Hort, wie du?", "tokens": ["Wo", "w\u00e4\u00b7re", "mir", "ein", "Hort", ",", "wie", "du", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ART", "NN", "$,", "PWAV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bei diesem Horte will ich sterben.", "tokens": ["Bei", "die\u00b7sem", "Hor\u00b7te", "will", "ich", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wenn du dich vor mir verbirgst,", "tokens": ["Und", "wenn", "du", "dich", "vor", "mir", "ver\u00b7birgst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wenn du deine Pforte schlie\u00dfest,", "tokens": ["Und", "wenn", "du", "dei\u00b7ne", "Pfor\u00b7te", "schlie\u00b7\u00dfest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "An dieser Pforte lager' ich,", "tokens": ["An", "die\u00b7ser", "Pfor\u00b7te", "la\u00b7ger'", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An dieser Pforte will ich sterben.", "tokens": ["An", "die\u00b7ser", "Pfor\u00b7te", "will", "ich", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Das letzte Wort aus meinem Mund,", "tokens": ["Das", "letz\u00b7te", "Wort", "aus", "mei\u00b7nem", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was wird es sein? Dein s\u00fc\u00dfer Name.", "tokens": ["Was", "wird", "es", "sein", "?", "Dein", "s\u00fc\u00b7\u00dfer", "Na\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "VAINF", "$.", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie fiele mir ein andres ein?", "tokens": ["Wie", "fie\u00b7le", "mir", "ein", "and\u00b7res", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit diesem Worte will ich sterben.", "tokens": ["Mit", "die\u00b7sem", "Wor\u00b7te", "will", "ich", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}