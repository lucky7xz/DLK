{"textgrid.poem.64924": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer m\u00f6chte wohl die Flur, die reich an Segen schwillt,", "tokens": ["Wer", "m\u00f6ch\u00b7te", "wohl", "die", "Flur", ",", "die", "reich", "an", "Se\u00b7gen", "schwillt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ART", "NN", "$,", "PRELS", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Vorsatz wandeln um zu blut'gem Schlachtgefild?", "tokens": ["Mit", "Vor\u00b7satz", "wan\u00b7deln", "um", "zu", "blut'\u00b7gem", "Schlacht\u00b7ge\u00b7fild", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "KOUI", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wer in den Rosenkelch die ekle Raupe setzen? \u2013", "tokens": ["Wer", "in", "den", "Ro\u00b7sen\u00b7kelch", "die", "ek\u00b7le", "Rau\u00b7pe", "set\u00b7zen", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die duft'gen Bl\u00e4tter wer mit gift'gem Thau benetzen?", "tokens": ["Die", "duft'\u00b7gen", "Bl\u00e4t\u00b7ter", "wer", "mit", "gift'\u00b7gem", "Thau", "be\u00b7net\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PWS", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und th\u00e4t es Einer doch, wie klagte man ihn an! \u2013", "tokens": ["Und", "th\u00e4t", "es", "Ei\u00b7ner", "doch", ",", "wie", "klag\u00b7te", "man", "ihn", "an", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "ADV", "$,", "PWAV", "VVFIN", "PIS", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie steht es dann um dich, der Schlimmres noch gethan?", "tokens": ["Wie", "steht", "es", "dann", "um", "dich", ",", "der", "Schlimm\u00b7res", "noch", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$,", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der seine Brust erschlo\u00df f\u00fcr alle Hassesplagen,", "tokens": ["Der", "sei\u00b7ne", "Brust", "er\u00b7schlo\u00df", "f\u00fcr", "al\u00b7le", "Has\u00b7se\u00b7spla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der nur den Himmelsflor der Liebe sollte tragen!", "tokens": ["Der", "nur", "den", "Him\u00b7mels\u00b7flor", "der", "Lie\u00b7be", "soll\u00b7te", "tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der ungleich edlerm Kelch, benetzt von heil'gem Born,", "tokens": ["Der", "un\u00b7gleich", "ed\u00b7lerm", "Kelch", ",", "be\u00b7netzt", "von", "heil'\u00b7gem", "Born", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sein eigen Herz entweiht durch Mi\u00dfgunst und durch Zorn!", "tokens": ["Sein", "ei\u00b7gen", "Herz", "ent\u00b7weiht", "durch", "Mi\u00df\u00b7gunst", "und", "durch", "Zorn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}