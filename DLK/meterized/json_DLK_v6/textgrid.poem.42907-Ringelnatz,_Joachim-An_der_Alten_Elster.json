{"textgrid.poem.42907": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "An der Alten Elster", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn die Pappeln an dem Uferhange", "tokens": ["Wenn", "die", "Pap\u00b7peln", "an", "dem", "U\u00b7fer\u00b7han\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schrecklich sich im Sturme bogen,", "tokens": ["Schreck\u00b7lich", "sich", "im", "Stur\u00b7me", "bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hu, wie war mir kleinem Kinde bange! \u2013", "tokens": ["Hu", ",", "wie", "war", "mir", "klei\u00b7nem", "Kin\u00b7de", "ban\u00b7ge", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "PWAV", "VAFIN", "PPER", "ADJA", "NN", "ADV", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Drohend gelb ist unten Flu\u00df gezogen.", "tokens": ["Dro\u00b7hend", "gelb", "ist", "un\u00b7ten", "Flu\u00df", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ADV", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Jenseits, an der Pferdeschwemme,", "tokens": ["Jen\u00b7seits", ",", "an", "der", "Pfer\u00b7de\u00b7schwem\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zog einmal ein Mann mit einer Stange", "tokens": ["Zog", "ein\u00b7mal", "ein", "Mann", "mit", "ei\u00b7ner", "Stan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Eine Leiche an das Land.", "tokens": ["Ei\u00b7ne", "Lei\u00b7che", "an", "das", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Butterbemme", "tokens": ["Mei\u00b7ne", "But\u00b7ter\u00b7bem\u00b7me"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Bi\u00df ein Hund mir aus der Hand. \u2013", "tokens": ["Bi\u00df", "ein", "Hund", "mir", "aus", "der", "Hand", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "O wie war mir bange,", "tokens": ["O", "wie", "war", "mir", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Als der gro\u00dfe Hund pl\u00f6tzlich neben mir stand!", "tokens": ["Als", "der", "gro\u00b7\u00dfe", "Hund", "pl\u00f6tz\u00b7lich", "ne\u00b7ben", "mir", "stand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "L\u00e4ngs des steilen Abhangs waren", "tokens": ["L\u00e4ngs", "des", "stei\u00b7len", "Ab\u00b7hangs", "wa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "B\u00fcsche, H\u00f6hlen, \u00dcbergangsgefahren. \u2013", "tokens": ["B\u00fc\u00b7sche", ",", "H\u00f6h\u00b7len", ",", "\u00dc\u00b7ber\u00b7gangs\u00b7ge\u00b7fah\u00b7ren", ".", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Dumme abenteuerliche Spiele lie\u00dfen", "tokens": ["Dum\u00b7me", "a\u00b7bent\u00b7eu\u00b7er\u00b7li\u00b7che", "Spie\u00b7le", "lie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Mich nach niemand anvertrauten Tr\u00e4umen", "tokens": ["Mich", "nach", "nie\u00b7mand", "an\u00b7ver\u00b7trau\u00b7ten", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "PIS", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Allzuoft und allzulange", "tokens": ["All\u00b7zu\u00b7oft", "und", "all\u00b7zu\u00b7lan\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["ADV", "KON", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schulzeit, Gunst und F\u00f6rderndes vers\u00e4umen. \u2013", "tokens": ["Schul\u00b7zeit", ",", "Gunst", "und", "F\u00f6r\u00b7dern\u00b7des", "ver\u00b7s\u00e4u\u00b7men", ".", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Hulewind beugte die Pappelriesen.", "tokens": ["Hu\u00b7le\u00b7wind", "beug\u00b7te", "die", "Pap\u00b7pel\u00b7rie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$."], "meter": "+-++--+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "O wie war mir bange!", "tokens": ["O", "wie", "war", "mir", "ban\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Pappeln, Hang und Flu\u00df, wo dieses Kind", "tokens": ["Pap\u00b7peln", ",", "Hang", "und", "Flu\u00df", ",", "wo", "die\u00b7ses", "Kind"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,", "PWAV", "PDAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "So viel heimlichstes Erleben hatte,", "tokens": ["So", "viel", "heim\u00b7lichs\u00b7tes", "Er\u00b7le\u00b7ben", "hat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "VAFIN", "$,"], "meter": "-++-+-+-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Sind nicht mehr. Mir spiegelt dort der glatte", "tokens": ["Sind", "nicht", "mehr", ".", "Mir", "spie\u00b7gelt", "dort", "der", "glat\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "ADV", "$.", "PPER", "VVFIN", "ADV", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Asphalt Wolken, wie sie heute sind.", "tokens": ["As\u00b7phalt", "Wol\u00b7ken", ",", "wie", "sie", "heu\u00b7te", "sind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PWAV", "PPER", "ADV", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}