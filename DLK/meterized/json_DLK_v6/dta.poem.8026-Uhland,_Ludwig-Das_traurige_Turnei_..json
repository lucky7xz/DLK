{"dta.poem.8026": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Das traurige Turnei .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Es ritten sieben Ritter frei,", "tokens": ["Es", "rit\u00b7ten", "sie\u00b7ben", "Rit\u00b7ter", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Schilden und mit Speeren,", "tokens": ["Mit", "Schil\u00b7den", "und", "mit", "Spee\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie wollten halten gut Turnei,", "tokens": ["Sie", "woll\u00b7ten", "hal\u00b7ten", "gut", "Tur\u00b7nei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des K\u00f6nigs Kind zu Ehren.", "tokens": ["Des", "K\u00f6\u00b7nigs", "Kind", "zu", "Eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und als sie sahen Thurm und Wall,", "tokens": ["Und", "als", "sie", "sa\u00b7hen", "Thurm", "und", "Wall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Gl\u00f6cklein h\u00f6rten sie dr\u00fcben;", "tokens": ["Ein", "Gl\u00f6c\u00b7klein", "h\u00f6r\u00b7ten", "sie", "dr\u00fc\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und als sie traten in K\u00f6nigs Hall\u2019,", "tokens": ["Und", "als", "sie", "tra\u00b7ten", "in", "K\u00f6\u00b7nigs", "Hall'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "NN", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da sahen sie Kerzen sieben.", "tokens": ["Da", "sa\u00b7hen", "sie", "Ker\u00b7zen", "sie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Da sahen sie liegen, todesbla\u00df,", "tokens": ["Da", "sa\u00b7hen", "sie", "lie\u00b7gen", ",", "to\u00b7des\u00b7bla\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "$,", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die holde Adelheide,", "tokens": ["Die", "hol\u00b7de", "A\u00b7del\u00b7hei\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der K\u00f6nig zu ihrem Haupte sa\u00df", "tokens": ["Der", "K\u00f6\u00b7nig", "zu", "ih\u00b7rem", "Haup\u00b7te", "sa\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In gro\u00dfen Herzeleide.", "tokens": ["In", "gro\u00b7\u00dfen", "Her\u00b7ze\u00b7lei\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da sprach der stolze Degenwerth:", "tokens": ["Da", "sprach", "der", "stol\u00b7ze", "De\u00b7gen\u00b7werth", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201edas mu\u00df ich immer klagen,", "tokens": ["\u201e", "das", "mu\u00df", "ich", "im\u00b7mer", "kla\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ich umsonst geg\u00fcrt\u2019t mein Pferd,", "tokens": ["Da\u00df", "ich", "um\u00b7sonst", "ge\u00b7g\u00fcrt't", "mein", "Pferd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Schild und Speer getragen.\u201c", "tokens": ["Mein", "Schild", "und", "Speer", "ge\u00b7tra\u00b7gen", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Drauf sprach der jung\u2019 Herr Adelbert:", "tokens": ["Drauf", "sprach", "der", "jung'", "Herr", "A\u00b7del\u00b7bert", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201ewir sollen das nicht klagen!", "tokens": ["\u201e", "wir", "sol\u00b7len", "das", "nicht", "kla\u00b7gen", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PDS", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des K\u00f6nigs Tochter ist immer werth,", "tokens": ["Des", "K\u00f6\u00b7nigs", "Toch\u00b7ter", "ist", "im\u00b7mer", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da\u00df wir drum stechen und schlagen.\u201c", "tokens": ["Da\u00df", "wir", "drum", "ste\u00b7chen", "und", "schla\u00b7gen", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PAV", "VVINF", "KON", "VVINF", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Herr Walther sprach, ein Ritter k\u00fchn:", "tokens": ["Herr", "Walt\u00b7her", "sprach", ",", "ein", "Rit\u00b7ter", "k\u00fchn", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "$,", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201enach Hause wollen wir reiten,", "tokens": ["\u201e", "nach", "Hau\u00b7se", "wol\u00b7len", "wir", "rei\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es kann uns wenig Heil erbl\u00fchn,", "tokens": ["Es", "kann", "uns", "we\u00b7nig", "Heil", "er\u00b7bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um eine Todte zu streiten.\u201c", "tokens": ["Um", "ei\u00b7ne", "Tod\u00b7te", "zu", "strei\u00b7ten", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUI", "ART", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Sprach Adelbert: \u201ewohl ist sie todt,", "tokens": ["Sprach", "A\u00b7del\u00b7bert", ":", "\u201e", "wohl", "ist", "sie", "todt", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "$(", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch lebet keine so holde,", "tokens": ["Doch", "le\u00b7bet", "kei\u00b7ne", "so", "hol\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "ADV", "ADJA", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie tr\u00e4gt einen Kranz von Rosen roth", "tokens": ["Sie", "tr\u00e4gt", "ei\u00b7nen", "Kranz", "von", "Ro\u00b7sen", "roth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und einen Ring von Golde.\u201c", "tokens": ["Und", "ei\u00b7nen", "Ring", "von", "Gol\u00b7de", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NE", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Sie ritten auf den Sand hinaus,", "tokens": ["Sie", "rit\u00b7ten", "auf", "den", "Sand", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APZR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die freien Ritter sieben.", "tokens": ["Die", "frei\u00b7en", "Rit\u00b7ter", "sie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie stritten also harten Strau\u00df,", "tokens": ["Sie", "strit\u00b7ten", "al\u00b7so", "har\u00b7ten", "Strau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis sechse todt geblieben.", "tokens": ["Bis", "sech\u00b7se", "todt", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Der siebente war Herr Adelbert,", "tokens": ["Der", "sie\u00b7ben\u00b7te", "war", "Herr", "A\u00b7del\u00b7bert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "NN", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Der Sieger \u00fcber alle.", "tokens": ["Der", "Sie\u00b7ger", "\u00fc\u00b7ber", "al\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er stieg so bleich von seinem Pferd,", "tokens": ["Er", "stieg", "so", "bleich", "von", "sei\u00b7nem", "Pferd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und trat in K\u00f6nigs Halle.", "tokens": ["Und", "trat", "in", "K\u00f6\u00b7nigs", "Hal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Er nahm den Kranz von Rosen roth,", "tokens": ["Er", "nahm", "den", "Kranz", "von", "Ro\u00b7sen", "roth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dazu den Ring von Golde,", "tokens": ["Da\u00b7zu", "den", "Ring", "von", "Gol\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "APPR", "NE", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Er fiel zur Erde, bleich und todt,", "tokens": ["Er", "fiel", "zur", "Er\u00b7de", ",", "bleich", "und", "todt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So bleich wie seine Holde.", "tokens": ["So", "bleich", "wie", "sei\u00b7ne", "Hol\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Der K\u00f6nig trug ein schwarz Gewand,", "tokens": ["Der", "K\u00f6\u00b7nig", "trug", "ein", "schwarz", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er lie\u00df die Glocke l\u00e4uten,", "tokens": ["Er", "lie\u00df", "die", "Glo\u00b7cke", "l\u00e4u\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sechs freie Ritter von dem Sand", "tokens": ["Sechs", "frei\u00b7e", "Rit\u00b7ter", "von", "dem", "Sand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["CARD", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Th\u00e4t er zu Grab begleiten.", "tokens": ["Th\u00e4t", "er", "zu", "Grab", "be\u00b7glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Der siebente war Herr Adelbert,", "tokens": ["Der", "sie\u00b7ben\u00b7te", "war", "Herr", "A\u00b7del\u00b7bert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "NN", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Mit seiner Adelheide.", "tokens": ["Mit", "sei\u00b7ner", "A\u00b7del\u00b7hei\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die liegen zusammen in k\u00fchler Erd\u2019,", "tokens": ["Die", "lie\u00b7gen", "zu\u00b7sam\u00b7men", "in", "k\u00fch\u00b7ler", "Erd'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ein Stein bedecket Beide.", "tokens": ["Ein", "Stein", "be\u00b7de\u00b7cket", "Bei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}