{"textgrid.poem.25846": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[winde qu\u00e4len die B\u00e4ume]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Winde qu\u00e4len die B\u00e4ume", "tokens": ["Win\u00b7de", "qu\u00e4\u00b7len", "die", "B\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Die Bl\u00e4tter frieren und gilben.", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "frie\u00b7ren", "und", "gil\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Menschen, noch braun die Sommerwangen,", "tokens": ["Men\u00b7schen", ",", "noch", "braun", "die", "Som\u00b7mer\u00b7wan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Aber die Lippen sangen die letzten Silben.", "tokens": ["A\u00b7ber", "die", "Lip\u00b7pen", "san\u00b7gen", "die", "letz\u00b7ten", "Sil\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Bald ist das Lied zergangen.", "tokens": ["Bald", "ist", "das", "Lied", "zer\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}