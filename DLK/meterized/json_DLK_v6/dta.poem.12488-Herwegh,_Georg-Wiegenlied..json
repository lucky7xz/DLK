{"dta.poem.12488": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Wiegenlied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Deutschland \u2014 auf weichem Pf\u00fchle", "tokens": ["Deutschland", "auf", "wei\u00b7chem", "Pf\u00fch\u00b7le"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$(", "APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Mach' dir den Kopf nicht schwer!", "tokens": ["Mach'", "dir", "den", "Kopf", "nicht", "schwer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Im irdischen Gew\u00fchle", "tokens": ["Im", "ir\u00b7di\u00b7schen", "Ge\u00b7w\u00fch\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schlafe, was willst du mehr?", "tokens": ["Schla\u00b7fe", ",", "was", "willst", "du", "mehr", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VMFIN", "PPER", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "La\u00df' jede Freiheit dir rauben,", "tokens": ["La\u00df'", "je\u00b7de", "Frei\u00b7heit", "dir", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIAT", "NN", "PPER", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Setze dich nicht zur Wehr,", "tokens": ["Set\u00b7ze", "dich", "nicht", "zur", "Wehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Du beh\u00e4ltst ja den christlichen Glauben:", "tokens": ["Du", "be\u00b7h\u00e4ltst", "ja", "den", "christ\u00b7li\u00b7chen", "Glau\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Schlafe, was willst du mehr?", "tokens": ["Schla\u00b7fe", ",", "was", "willst", "du", "mehr", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VMFIN", "PPER", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Und ob man dir Alles verb\u00f6te,", "tokens": ["Und", "ob", "man", "dir", "Al\u00b7les", "ver\u00b7b\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Doch gr\u00e4me dich nicht zu sehr,", "tokens": ["Doch", "gr\u00e4\u00b7me", "dich", "nicht", "zu", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "PTKA", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Du hast ja Schiller und G\u00f6the:", "tokens": ["Du", "hast", "ja", "Schil\u00b7ler", "und", "G\u00f6\u00b7the", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NE", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Schlafe, was willst du mehr?", "tokens": ["Schla\u00b7fe", ",", "was", "willst", "du", "mehr", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VMFIN", "PPER", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Dein K\u00f6nig besch\u00fctzt die Kameele", "tokens": ["Dein", "K\u00f6\u00b7nig", "be\u00b7sch\u00fctzt", "die", "Ka\u00b7mee\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und macht sie pension\u00e4r,", "tokens": ["Und", "macht", "sie", "pen\u00b7sio\u00b7n\u00e4r", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dreihundert Thaler die Seele:", "tokens": ["Drei\u00b7hun\u00b7dert", "Tha\u00b7ler", "die", "See\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Schlafe, was willst du mehr?", "tokens": ["Schla\u00b7fe", ",", "was", "willst", "du", "mehr", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VMFIN", "PPER", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Es fechten dreihundert ", "tokens": ["Es", "fech\u00b7ten", "drei\u00b7hun\u00b7dert"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "CARD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Im Schatten, ein Sparterheer;", "tokens": ["Im", "Schat\u00b7ten", ",", "ein", "Spar\u00b7ter\u00b7heer", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und t\u00e4glich erf\u00e4hrst du das Wetter:", "tokens": ["Und", "t\u00e4g\u00b7lich", "er\u00b7f\u00e4hrst", "du", "das", "Wet\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Schlafe, was willst du mehr?", "tokens": ["Schla\u00b7fe", ",", "was", "willst", "du", "mehr", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VMFIN", "PPER", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Kein Kind l\u00e4uft ohne H\u00f6schen", "tokens": ["Kein", "Kind", "l\u00e4uft", "oh\u00b7ne", "H\u00f6\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am Rhein, dem freien, umher:", "tokens": ["Am", "Rhein", ",", "dem", "frei\u00b7en", ",", "um\u00b7her", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NE", "$,", "ART", "ADJA", "$,", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Mein Deutschland, mein Dornr\u00f6schen", "tokens": ["Mein", "Deutschland", ",", "mein", "Dorn\u00b7r\u00f6\u00b7schen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Schlafe, was willst du mehr? \u2014", "tokens": ["Schla\u00b7fe", ",", "was", "willst", "du", "mehr", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "PWS", "VMFIN", "PPER", "ADV", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}