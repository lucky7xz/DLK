{"textgrid.poem.34791": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er steht so starr wie ein Baumstamm,", "tokens": ["Er", "steht", "so", "starr", "wie", "ein", "Baum\u00b7stamm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In Hitz' und Frost und Wind,", "tokens": ["In", "Hitz'", "und", "Frost", "und", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Boden wurzelt die Fu\u00dfzeh',", "tokens": ["Im", "Bo\u00b7den", "wur\u00b7zelt", "die", "Fu\u00df\u00b7zeh'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Arme erhoben sind.", "tokens": ["Die", "Ar\u00b7me", "er\u00b7ho\u00b7ben", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "So qu\u00e4lt sich Bagiratha lange,", "tokens": ["So", "qu\u00e4lt", "sich", "Ba\u00b7gi\u00b7ra\u00b7tha", "lan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "NE", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Brahma will enden sein Weh,", "tokens": ["Und", "Brah\u00b7ma", "will", "en\u00b7den", "sein", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VMFIN", "VVINF", "PPOSAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Er l\u00e4\u00dft den Ganges flie\u00dfen", "tokens": ["Er", "l\u00e4\u00dft", "den", "Gan\u00b7ges", "flie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Herab von der Himmelsh\u00f6h'.", "tokens": ["Her\u00b7ab", "von", "der", "Him\u00b7mels\u00b7h\u00f6h'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich aber, Geliebte, vergebens", "tokens": ["Ich", "a\u00b7ber", ",", "Ge\u00b7lieb\u00b7te", ",", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "ADV", "$,", "NN", "$,", "ADV"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Martre und qu\u00e4l ich mich ab,", "tokens": ["Mar\u00b7tre", "und", "qu\u00e4l", "ich", "mich", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Aus deinen Himmelsaugen", "tokens": ["Aus", "dei\u00b7nen", "Him\u00b7mel\u00b7sau\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Flie\u00dft mir kein Tropfen herab.", "tokens": ["Flie\u00dft", "mir", "kein", "Trop\u00b7fen", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}