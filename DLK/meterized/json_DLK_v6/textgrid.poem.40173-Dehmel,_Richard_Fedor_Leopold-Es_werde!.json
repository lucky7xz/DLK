{"textgrid.poem.40173": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Es werde!", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O meine bleiche Braut! du blasse Wolke", "tokens": ["O", "mei\u00b7ne", "blei\u00b7che", "Braut", "!", "du", "blas\u00b7se", "Wol\u00b7ke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$.", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "im Arm des Sturms! du zuckend Haupt,", "tokens": ["im", "Arm", "des", "Sturms", "!", "du", "zu\u00b7ckend", "Haupt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$.", "PPER", "VVPP", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "an meine Brust geneigt aus deinen Schleiern!", "tokens": ["an", "mei\u00b7ne", "Brust", "ge\u00b7neigt", "aus", "dei\u00b7nen", "Schlei\u00b7ern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "erbleichst, erbebst du ", "tokens": ["er\u00b7bleichst", ",", "er\u00b7bebst", "du"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "PPER"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "O nun ", "tokens": ["O", "nun"], "token_info": ["word", "word"], "pos": ["NE", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "du tau gef\u00fcllter Kelch der schwanken Lilie,", "tokens": ["du", "tau", "ge\u00b7f\u00fcll\u00b7ter", "Kelch", "der", "schwan\u00b7ken", "Li\u00b7lie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.7": {"text": "und hei\u00dfer k\u00fcss' ich dich \u2013 wir sind allein!", "tokens": ["und", "hei\u00b7\u00dfer", "k\u00fcss'", "ich", "dich", "\u2013", "wir", "sind", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "VVFIN", "PPER", "PRF", "$(", "PPER", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Allein \u2013! in rosige D\u00e4mmrung taucht", "tokens": ["Al\u00b7lein", "\u2013", "!", "in", "ro\u00b7si\u00b7ge", "D\u00e4mm\u00b7rung", "taucht"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$(", "$.", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "die Ampel sacht das schattige Gemach", "tokens": ["die", "Am\u00b7pel", "sacht", "das", "schat\u00b7ti\u00b7ge", "Ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "und Dich, du wei\u00dfe Taube, \u2013 fl\u00fcchte nicht:", "tokens": ["und", "Dich", ",", "du", "wei\u00b7\u00dfe", "Tau\u00b7be", ",", "\u2013", "fl\u00fcch\u00b7te", "nicht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "ADJA", "NN", "$,", "$(", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "dem keuschen Himmel selbst verwehr' ich nun", "tokens": ["dem", "keu\u00b7schen", "Him\u00b7mel", "selbst", "ver\u00b7wehr'", "ich", "nun"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "dich anzulauschen! sieh: der Vorhang rollt,", "tokens": ["dich", "an\u00b7zu\u00b7lau\u00b7schen", "!", "sieh", ":", "der", "Vor\u00b7hang", "rollt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVIZU", "$.", "VVIMP", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "und jeden Spalt verh\u00fcll' ich faltenschwer,", "tokens": ["und", "je\u00b7den", "Spalt", "ver\u00b7h\u00fcll'", "ich", "fal\u00b7ten\u00b7schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "da\u00df nicht die Nacht die schw\u00e4rzlich blauende", "tokens": ["da\u00df", "nicht", "die", "Nacht", "die", "schw\u00e4rz\u00b7lich", "blau\u00b7en\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "ART", "ADJD", "ADJA"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.15": {"text": "err\u00f6te, mu\u00df sie deine Sch\u00f6nheit schaun,", "tokens": ["er\u00b7r\u00f6\u00b7te", ",", "mu\u00df", "sie", "dei\u00b7ne", "Sch\u00f6n\u00b7heit", "schaun", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "da\u00df nicht der Sterne reines Silberlicht", "tokens": ["da\u00df", "nicht", "der", "Ster\u00b7ne", "rei\u00b7nes", "Sil\u00b7ber\u00b7licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "sich neidisch tr\u00fcbe, sehn sie Deine Reinheit! \u2013", "tokens": ["sich", "nei\u00b7disch", "tr\u00fc\u00b7be", ",", "sehn", "sie", "Dei\u00b7ne", "Rein\u00b7heit", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PRF", "ADJD", "ADJA", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Thu ", "tokens": ["Thu"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.19": {"text": "du bist allein! die jungen Rosen nur", "tokens": ["du", "bist", "al\u00b7lein", "!", "die", "jun\u00b7gen", "Ro\u00b7sen", "nur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "$.", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "um dein verschwiegen Pf\u00fchl, die schlummernden,", "tokens": ["um", "dein", "ver\u00b7schwie\u00b7gen", "Pf\u00fchl", ",", "die", "schlum\u00b7mern\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.21": {"text": "duftselige Tr\u00e4ume spinnen", "tokens": ["duf\u00b7tse\u00b7li\u00b7ge", "Tr\u00e4u\u00b7me", "spin\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.22": {"text": "von purpurner Entfaltung scheuer Knospen,", "tokens": ["von", "pur\u00b7pur\u00b7ner", "Ent\u00b7fal\u00b7tung", "scheu\u00b7er", "Knos\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "die Rosen nur \u2013 \u2013 und ich!", "tokens": ["die", "Ro\u00b7sen", "nur", "\u2013", "\u2013", "und", "ich", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$(", "$(", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "Und wie in Tr\u00e4umen taumelnd, d\u00fcfteleicht,", "tokens": ["Und", "wie", "in", "Tr\u00e4u\u00b7men", "tau\u00b7melnd", ",", "d\u00fcf\u00b7te\u00b7leicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "NN", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "versunknen Blicks auf weichen L\u00fcften schweb' ich", "tokens": ["ver\u00b7sun\u00b7knen", "Blicks", "auf", "wei\u00b7chen", "L\u00fcf\u00b7ten", "schweb'", "ich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "und winke, winke dir:", "tokens": ["und", "win\u00b7ke", ",", "win\u00b7ke", "dir", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "es st\u00fcrzen und schwinden", "tokens": ["es", "st\u00fcr\u00b7zen", "und", "schwin\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVINF", "KON", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.28": {"text": "tiefer und tiefer die irdischen Dunstgewande,", "tokens": ["tie\u00b7fer", "und", "tie\u00b7fer", "die", "ir\u00b7di\u00b7schen", "Dunst\u00b7ge\u00b7wan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.29": {"text": "auf seidnen Wogen treibst du her zu mir,", "tokens": ["auf", "seid\u00b7nen", "Wo\u00b7gen", "treibst", "du", "her", "zu", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "und Schoo\u00df an Schoo\u00df gesp\u00fclt von str\u00f6menden Schauern", "tokens": ["und", "Schoo\u00df", "an", "Schoo\u00df", "ge\u00b7sp\u00fclt", "von", "str\u00f6\u00b7men\u00b7den", "Schau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "NN", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "von goldnen Dunkelheiten sanft umsch\u00e4umt,", "tokens": ["von", "gold\u00b7nen", "Dun\u00b7kel\u00b7hei\u00b7ten", "sanft", "um\u00b7sch\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "wiegen wir uns auf tastenden Schwingen", "tokens": ["wie\u00b7gen", "wir", "uns", "auf", "tas\u00b7ten\u00b7den", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.33": {"text": "bang umwunden hin\u00fcber", "tokens": ["bang", "um\u00b7wun\u00b7den", "hin\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "VVFIN", "ADV"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.34": {"text": "in die G\u00e4rten der Ewigkeit: \u2013", "tokens": ["in", "die", "G\u00e4r\u00b7ten", "der", "E\u00b7wig\u00b7keit", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.35": {"text": "Keime der Sehnsucht sprie\u00dfen da,", "tokens": ["Kei\u00b7me", "der", "Sehn\u00b7sucht", "sprie\u00b7\u00dfen", "da", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.36": {"text": "bl\u00fchende Ranken holder Erf\u00fcllung flechten", "tokens": ["bl\u00fc\u00b7hen\u00b7de", "Ran\u00b7ken", "hol\u00b7der", "Er\u00b7f\u00fcl\u00b7lung", "flech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJA", "NN", "VVINF"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.37": {"text": "da ", "tokens": ["da"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.38": {"text": "Puls in Puls zu Flammengarben fluten", "tokens": ["Puls", "in", "Puls", "zu", "Flam\u00b7men\u00b7gar\u00b7ben", "flu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.39": {"text": "glutverm\u00e4hlt die lohenden W\u00fcnsche,", "tokens": ["glut\u00b7ver\u00b7m\u00e4hlt", "die", "lo\u00b7hen\u00b7den", "W\u00fcn\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.40": {"text": "und im weherl\u00f6sten, schwellenden Busen ", "tokens": ["und", "im", "we\u00b7her\u00b7l\u00f6s\u00b7ten", ",", "schwel\u00b7len\u00b7den", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPRART", "VVFIN", "$,", "ADJA", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.41": {"text": "die starre Furcht vor Tod und Unendlichkeit: \u2013", "tokens": ["die", "star\u00b7re", "Furcht", "vor", "Tod", "und", "Un\u00b7end\u00b7lich\u00b7keit", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.42": {"text": "grauen", "tokens": ["grau\u00b7en"], "token_info": ["word"], "pos": ["ADJA"], "meter": "+-", "measure": "trochaic.single"}, "line.43": {"text": "ganz erloschnen Dranges alle Sinne,", "tokens": ["ganz", "er\u00b7lo\u00b7schnen", "Dran\u00b7ges", "al\u00b7le", "Sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.44": {"text": "still verblutet in seligen Thr\u00e4nen der Wille,", "tokens": ["still", "ver\u00b7blu\u00b7tet", "in", "se\u00b7li\u00b7gen", "Thr\u00e4\u00b7nen", "der", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.45": {"text": "d\u00fcrstend umsaust ihn der Odem der Allmacht,", "tokens": ["d\u00fcrs\u00b7tend", "um\u00b7saust", "ihn", "der", "O\u00b7dem", "der", "All\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+---+", "measure": "dactylic.tri.plus"}, "line.46": {"text": "und den welt umfangenden Fittig ", "tokens": ["und", "den", "welt", "um\u00b7fan\u00b7gen\u00b7den", "Fit\u00b7tig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.47": {"text": "auszuruhn vom Fluge am Herzen Gottes,", "tokens": ["aus\u00b7zu\u00b7ruhn", "vom", "Flu\u00b7ge", "am", "Her\u00b7zen", "Got\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "APPRART", "NN", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.48": {"text": "und in matter Hand", "tokens": ["und", "in", "mat\u00b7ter", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.49": {"text": "beut sie die funkelnden Tropfen", "tokens": ["beut", "sie", "die", "fun\u00b7keln\u00b7den", "Trop\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.50": {"text": "Seinem befruchtenden Hauche dar: ich f\u00fchle \u2013", "tokens": ["Sei\u00b7nem", "be\u00b7fruch\u00b7ten\u00b7den", "Hau\u00b7che", "dar", ":", "ich", "f\u00fch\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$.", "PPER", "VVFIN", "$("], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.51": {"text": "f\u00fchlst du? Geliebte \u2013 rinnen die Quellen des Lebens \u2013", "tokens": ["f\u00fchlst", "du", "?", "Ge\u00b7lieb\u00b7te", "\u2013", "rin\u00b7nen", "die", "Quel\u00b7len", "des", "Le\u00b7bens", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "NN", "$(", "VVFIN", "ART", "NN", "ART", "NN", "$("], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.52": {"text": "Mund an Mund Ihm \u2013 trinke \u2013 trink' ich \u2013", "tokens": ["Mund", "an", "Mund", "Ihm", "\u2013", "trin\u00b7ke", "\u2013", "trink'", "ich", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PPER", "$(", "VVFIN", "$(", "VVFIN", "PPER", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.53": {"text": "trunken", "tokens": ["trun\u00b7ken"], "token_info": ["word"], "pos": ["ADJD"], "meter": "+-", "measure": "trochaic.single"}, "line.54": {"text": "stamml' ich nach das Sch\u00f6pferwort.", "tokens": ["stamml'", "ich", "nach", "das", "Sch\u00f6p\u00b7fer\u00b7wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}