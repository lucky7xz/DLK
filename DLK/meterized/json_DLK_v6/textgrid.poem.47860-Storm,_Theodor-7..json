{"textgrid.poem.47860": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weiter geht's und immer weiter!", "tokens": ["Wei\u00b7ter", "geht's", "und", "im\u00b7mer", "wei\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "ADV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieh, da kommt auf m\u00fcdem Fu\u00df", "tokens": ["Sieh", ",", "da", "kommt", "auf", "m\u00fc\u00b7dem", "Fu\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Noch ein Wandrer mir entgegen.", "tokens": ["Noch", "ein", "Wand\u00b7rer", "mir", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbbring dem St\u00e4dtchen meinen Gru\u00df!", "tokens": ["\u00bb", "bring", "dem", "St\u00e4dt\u00b7chen", "mei\u00b7nen", "Gru\u00df", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und am Tore, wenn des Z\u00f6llners", "tokens": ["Und", "am", "To\u00b7re", ",", "wenn", "des", "Z\u00f6ll\u00b7ners"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blonde Tochter schaut herf\u00fcr,", "tokens": ["Blon\u00b7de", "Toch\u00b7ter", "schaut", "her\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bring ihr diese wilde Rose,", "tokens": ["Bring", "ihr", "die\u00b7se", "wil\u00b7de", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gr\u00fc\u00df sie einmal noch von mir!\u00ab \u2013", "tokens": ["Gr\u00fc\u00df", "sie", "ein\u00b7mal", "noch", "von", "mir", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "APPR", "PPER", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Weiter geht's und immer weiter!", "tokens": ["Wei\u00b7ter", "geht's", "und", "im\u00b7mer", "wei\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "ADV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da schweigen alle V\u00f6gel bald", "tokens": ["Da", "schwei\u00b7gen", "al\u00b7le", "V\u00f6\u00b7gel", "bald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vor mir stehn im Duft die W\u00e4lder,", "tokens": ["Vor", "mir", "stehn", "im", "Duft", "die", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "R\u00fcckw\u00e4rts brennt der Abendschein.", "tokens": ["R\u00fcck\u00b7w\u00e4rts", "brennt", "der", "A\u00b7bend\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Einsam werden Weg' und Stege,", "tokens": ["Ein\u00b7sam", "wer\u00b7den", "Weg'", "und", "Ste\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ganz alleine wandr' ich bald;", "tokens": ["Ganz", "al\u00b7lei\u00b7ne", "wandr'", "ich", "bald", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einen Falken seh ich kreisen \u2013", "tokens": ["Ei\u00b7nen", "Fal\u00b7ken", "seh", "ich", "krei\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcber mir schon rauscht der Wald.", "tokens": ["\u00dc\u00b7ber", "mir", "schon", "rauscht", "der", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}