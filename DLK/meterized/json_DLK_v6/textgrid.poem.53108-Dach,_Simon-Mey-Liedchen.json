{"textgrid.poem.53108": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Mey-Liedchen", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm, Dorinde, lass vns eilen,", "tokens": ["Komm", ",", "Do\u00b7rin\u00b7de", ",", "lass", "vns", "ei\u00b7len", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NE", "$,", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nimm der Zeiten G\u00fct in acht,", "tokens": ["Nimm", "der", "Zei\u00b7ten", "G\u00fct", "in", "acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "NN", "APPR", "CARD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Angesehen, das verweilen", "tokens": ["An\u00b7ge\u00b7se\u00b7hen", ",", "das", "ver\u00b7wei\u00b7len"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PDS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Selten grossen Nutz gebracht,", "tokens": ["Sel\u00b7ten", "gros\u00b7sen", "Nutz", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber wei\u00dflich fortgesetzt,", "tokens": ["A\u00b7ber", "wei\u00df\u00b7lich", "fort\u00b7ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hat so manches Paar ergetzt.", "tokens": ["Hat", "so", "man\u00b7ches", "Paar", "er\u00b7getzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wir sind in den Fr\u00fclings Jahren,", "tokens": ["Wir", "sind", "in", "den", "Fr\u00fc\u00b7lings", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Lass vns die Gelegenheit", "tokens": ["Lass", "vns", "die", "Ge\u00b7le\u00b7gen\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Forn ergreiffen bey den Haaren,", "tokens": ["Forn", "er\u00b7greif\u00b7fen", "bey", "den", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sehn auff diese Meyen-Zeit,", "tokens": ["Sehn", "auff", "die\u00b7se", "Meyen\u00b7Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Da sich Himmel, See vnd Land", "tokens": ["Da", "sich", "Him\u00b7mel", ",", "See", "vnd", "Land"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PRF", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Kn\u00fcpffen in ein Heyraht-Band.", "tokens": ["Kn\u00fcpf\u00b7fen", "in", "ein", "Hey\u00b7raht\u00b7Band", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn sich die Natur verj\u00fcnget,", "tokens": ["Wenn", "sich", "die", "Na\u00b7tur", "ver\u00b7j\u00fcn\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liegt in Liebe kranck vnd wund,", "tokens": ["Liegt", "in", "Lie\u00b7be", "kranck", "vnd", "wund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles sich zu nehmen zwinget,", "tokens": ["Al\u00b7les", "sich", "zu", "neh\u00b7men", "zwin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PRF", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Thut sie frey dem Menschen kundt:", "tokens": ["Thut", "sie", "frey", "dem", "Men\u00b7schen", "kundt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df sich Er, die kleine Welt,", "tokens": ["Da\u00df", "sich", "Er", ",", "die", "klei\u00b7ne", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Billich nach der grossen Helt.", "tokens": ["Bil\u00b7lich", "nach", "der", "gros\u00b7sen", "Helt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Still zu seyn von Feld vnd P\u00fcschen,", "tokens": ["Still", "zu", "seyn", "von", "Feld", "vnd", "P\u00fc\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VAINF", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von dem leichten Heer der Lufft,", "tokens": ["Von", "dem", "leich\u00b7ten", "Heer", "der", "Lufft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da sich jedes will vermischen,", "tokens": ["Da", "sich", "je\u00b7des", "will", "ver\u00b7mi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PIAT", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jedes seines Gleichen rufft,", "tokens": ["Je\u00b7des", "sei\u00b7nes", "Glei\u00b7chen", "rufft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "H\u00f6rt man in den W\u00e4ldern nicht,", "tokens": ["H\u00f6rt", "man", "in", "den", "W\u00e4l\u00b7dern", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie sich Baum vnd Baum bespricht?", "tokens": ["Wie", "sich", "Baum", "vnd", "Baum", "be\u00b7spricht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NE", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "An den Bircken, an den Linden,", "tokens": ["An", "den", "Bir\u00b7cken", ",", "an", "den", "Lin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vnd den Eichen nimbt man wahr,", "tokens": ["Vnd", "den", "Ei\u00b7chen", "nimbt", "man", "wahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PIS", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie sich Aest' in Aeste binden,", "tokens": ["Wie", "sich", "A\u00b7est'", "in", "A\u00b7es\u00b7te", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "---+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Alles machet offenbahr", "tokens": ["Al\u00b7les", "ma\u00b7chet", "of\u00b7fen\u00b7bahr"], "token_info": ["word", "word", "word"], "pos": ["PIS", "VVFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Durch das Rauschen, so es \u00fcbt,", "tokens": ["Durch", "das", "Rau\u00b7schen", ",", "so", "es", "\u00fcbt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df es sey, wie wir, verliebt.", "tokens": ["Da\u00df", "es", "sey", ",", "wie", "wir", ",", "ver\u00b7liebt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "$,", "PWAV", "PPER", "$,", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Lust betr\u00fcbt, die man verscheubet.", "tokens": ["Lust", "be\u00b7tr\u00fcbt", ",", "die", "man", "ver\u00b7scheu\u00b7bet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "PRELS", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieser Eyfer, dieser Brand,", "tokens": ["Die\u00b7ser", "Ey\u00b7fer", ",", "die\u00b7ser", "Brand", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Diese Jugend, so vns treibet,", "tokens": ["Die\u00b7se", "Ju\u00b7gend", ",", "so", "vns", "trei\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat nicht ewig den Bestand,", "tokens": ["Hat", "nicht", "e\u00b7wig", "den", "Be\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zeigt sich Wind- vnd Vogel-leicht,", "tokens": ["Zeigt", "sich", "Win\u00b7d", "vnd", "Vo\u00b7gel\u00b7leicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "TRUNC", "KON", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Ist gefl\u00fcgelt, k\u00f6mpt vnd weicht.", "tokens": ["Ist", "ge\u00b7fl\u00fc\u00b7gelt", ",", "k\u00f6mpt", "vnd", "weicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}