{"textgrid.poem.27989": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Volvinus ist gelehrt und gibt materi her;", "tokens": ["Vol\u00b7vi\u00b7nus", "ist", "ge\u00b7lehrt", "und", "gibt", "ma\u00b7te\u00b7ri", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "VVPP", "KON", "VVFIN", "NE", "PTKVZ", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Sein Weib, die concipirt, so w\u00e4chset ohngefehr", "tokens": ["Sein", "Weib", ",", "die", "con\u00b7ci\u00b7pirt", ",", "so", "w\u00e4ch\u00b7set", "ohn\u00b7ge\u00b7fehr"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "VVPP", "$,", "ADV", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein richtiger Context, der, wann er ist f\u00fcr voll,", "tokens": ["Ein", "rich\u00b7ti\u00b7ger", "Con\u00b7text", ",", "der", ",", "wann", "er", "ist", "f\u00fcr", "voll", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "$,", "PWAV", "PPER", "VAFIN", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kan sagen alsdann selbst, wie man ihn nennen soll.", "tokens": ["Kan", "sa\u00b7gen", "als\u00b7dann", "selbst", ",", "wie", "man", "ihn", "nen\u00b7nen", "soll", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "ADV", "ADV", "$,", "PWAV", "PIS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}}}}