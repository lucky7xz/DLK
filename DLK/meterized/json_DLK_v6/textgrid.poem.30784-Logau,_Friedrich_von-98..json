{"textgrid.poem.30784": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "98.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Paul ist fleissig mich zu fragen,", "tokens": ["Paul", "ist", "fleis\u00b7sig", "mich", "zu", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJD", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich verdr\u00fcssig was zu sagen;", "tokens": ["Ich", "ver\u00b7dr\u00fcs\u00b7sig", "was", "zu", "sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PIS", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dann mit allem meinem sagen,", "tokens": ["Dann", "mit", "al\u00b7lem", "mei\u00b7nem", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIS", "PPOSAT", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stillt sich nimmer doch sein fragen.", "tokens": ["Stillt", "sich", "nim\u00b7mer", "doch", "sein", "fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ADV", "PPOSAT", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}