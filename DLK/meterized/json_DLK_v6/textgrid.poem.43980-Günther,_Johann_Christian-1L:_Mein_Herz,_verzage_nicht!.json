{"textgrid.poem.43980": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Herz, verzage nicht!", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Herz, verzage nicht!", "tokens": ["Mein", "Herz", ",", "ver\u00b7za\u00b7ge", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Liebe macht's mit allen so;", "tokens": ["Die", "Lie\u00b7be", "macht's", "mit", "al\u00b7len", "so", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Herz voll treuer Pflicht,", "tokens": ["Ein", "Herz", "voll", "treu\u00b7er", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wird ohne Gram nicht froh.", "tokens": ["Wird", "oh\u00b7ne", "Gram", "nicht", "froh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es f\u00e4llt zwar ziemlich schwer,", "tokens": ["Es", "f\u00e4llt", "zwar", "ziem\u00b7lich", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Eh uns das Kummermeer", "tokens": ["Eh", "uns", "das", "Kum\u00b7mer\u00b7meer"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Zum sichern Friedenshafen bringt;", "tokens": ["Zum", "si\u00b7chern", "Frie\u00b7dens\u00b7ha\u00b7fen", "bringt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Man zittert, seufzt und sinckt", "tokens": ["Man", "zit\u00b7tert", ",", "seufzt", "und", "sinckt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "An Muth und Sinn", "tokens": ["An", "Muth", "und", "Sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "In St\u00fcrmen hin,", "tokens": ["In", "St\u00fcr\u00b7men", "hin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Der Ancker rei\u00dft die Hand,", "tokens": ["Der", "An\u00b7cker", "rei\u00dft", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Doch wer sich zwingt und hoft, der kommt gleichwohl ans Land.", "tokens": ["Doch", "wer", "sich", "zwingt", "und", "hoft", ",", "der", "kommt", "gleich\u00b7wohl", "ans", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PRF", "VVFIN", "KON", "VVFIN", "$,", "PRELS", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was leid ich nicht um dich,", "tokens": ["Was", "leid", "ich", "nicht", "um", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PPER", "PTKNEG", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Du mir ins Herz gepr\u00e4gtes Bild!", "tokens": ["Du", "mir", "ins", "Herz", "ge\u00b7pr\u00e4g\u00b7tes", "Bild", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Sehnsucht jaget mich", "tokens": ["Die", "Sehn\u00b7sucht", "ja\u00b7get", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So wie ein sch\u00fcchtern Wild;", "tokens": ["So", "wie", "ein", "sch\u00fcch\u00b7tern", "Wild", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein Schlaf ist nur ein Qualm,", "tokens": ["Mein", "Schlaf", "ist", "nur", "ein", "Qualm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mein Lied ein Klagepsalm;", "tokens": ["Mein", "Lied", "ein", "Kla\u00b7ge\u00b7psalm", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Angst der bangen Einsamkeit", "tokens": ["Die", "Angst", "der", "ban\u00b7gen", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Begr\u00e4bt mich vor der Zeit,", "tokens": ["Be\u00b7gr\u00e4bt", "mich", "vor", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Weil ich den Ku\u00df", "tokens": ["Weil", "ich", "den", "Ku\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Entbehren mu\u00df,", "tokens": ["Ent\u00b7beh\u00b7ren", "mu\u00df", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VMFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Der so viel Lust verspricht.", "tokens": ["Der", "so", "viel", "Lust", "ver\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Doch hof ich alles auszustehn; verlas nur du mich nicht!", "tokens": ["Doch", "hof", "ich", "al\u00b7les", "aus\u00b7zu\u00b7stehn", ";", "ver\u00b7las", "nur", "du", "mich", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "VVINF", "$.", "VVFIN", "ADV", "PPER", "PRF", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "Verlas nur du mich nicht,", "tokens": ["Ver\u00b7las", "nur", "du", "mich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "PRF", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Du Engel, de\u00dfen treuer Geist", "tokens": ["Du", "En\u00b7gel", ",", "de\u00b7\u00dfen", "treu\u00b7er", "Geist"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "NE", "$,", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und holdes Angesicht", "tokens": ["Und", "hol\u00b7des", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mir noch den Troststern weist;", "tokens": ["Mir", "noch", "den", "Trost\u00b7stern", "weist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Himmel wird einmahl", "tokens": ["Der", "Him\u00b7mel", "wird", "ein\u00b7mahl"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Uns nach so vieler Qual", "tokens": ["Uns", "nach", "so", "vie\u00b7ler", "Qual"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ADV", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Hofnung Siegeskranz verleihn", "tokens": ["Der", "Hof\u00b7nung", "Sie\u00b7ges\u00b7kranz", "ver\u00b7leihn"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und mich durch dich erfreun.", "tokens": ["Und", "mich", "durch", "dich", "er\u00b7freun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Drum liebe still,", "tokens": ["Drum", "lie\u00b7be", "still", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Wie ich auch will,", "tokens": ["Wie", "ich", "auch", "will", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VMFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Und sieh geduldig zu;", "tokens": ["Und", "sieh", "ge\u00b7dul\u00b7dig", "zu", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Die Stra\u00dfe, so uns jezo trennt, f\u00fchrt unvermerckt zur Ruh.", "tokens": ["Die", "Stra\u00b7\u00dfe", ",", "so", "uns", "je\u00b7zo", "trennt", ",", "f\u00fchrt", "un\u00b7ver\u00b7merckt", "zur", "Ruh", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "ADV", "VVFIN", "$,", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.4": {"line.1": {"text": "Ich liebe meinen Schmerz,", "tokens": ["Ich", "lie\u00b7be", "mei\u00b7nen", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Weil du, mein Engel, Ursach bist;", "tokens": ["Weil", "du", ",", "mein", "En\u00b7gel", ",", "Ur\u00b7sach", "bist", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PPOSAT", "NN", "$,", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du hast mein ganzes Herz,", "tokens": ["Du", "hast", "mein", "gan\u00b7zes", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dies raubt dir keine List.", "tokens": ["Dies", "raubt", "dir", "kei\u00b7ne", "List", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was hilft's uns, da\u00df man weint?", "tokens": ["Was", "hilft's", "uns", ",", "da\u00df", "man", "weint", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Was jezt unm\u00f6glich scheint,", "tokens": ["Was", "jezt", "un\u00b7m\u00f6g\u00b7lich", "scheint", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das ist gewis ein \u00dcbergang;", "tokens": ["Das", "ist", "ge\u00b7wis", "ein", "\u00dc\u00b7ber\u00b7gang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Grillenfang macht kranck.", "tokens": ["Der", "Gril\u00b7len\u00b7fang", "macht", "kranck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Es r\u00fchret mich", "tokens": ["Es", "r\u00fch\u00b7ret", "mich"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Schon innerlich", "tokens": ["Schon", "in\u00b7ner\u00b7lich"], "token_info": ["word", "word"], "pos": ["ADV", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Ein Trieb der Z\u00e4rtligkeit,", "tokens": ["Ein", "Trieb", "der", "Z\u00e4rt\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Die mir dein k\u00fcnftiger Besiz so wie dein Nahme deut.", "tokens": ["Die", "mir", "dein", "k\u00fcnf\u00b7ti\u00b7ger", "Be\u00b7siz", "so", "wie", "dein", "Nah\u00b7me", "deut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "ADJA", "NN", "ADV", "KOKOM", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}