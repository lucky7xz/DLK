{"textgrid.poem.40085": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fr\u00fchling im Lande! \u2013 G\u00e4rend quoll sein Blut,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fchling im Lande! \u2013 G\u00e4rend quoll sein Blut,", "tokens": ["Fr\u00fch\u00b7ling", "im", "Lan\u00b7de", "!", "\u2013", "G\u00e4\u00b7rend", "quoll", "sein", "Blut", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$.", "$(", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "des K\u00f6rpers enge Last den Wesen rings", "tokens": ["des", "K\u00f6r\u00b7pers", "en\u00b7ge", "Last", "den", "We\u00b7sen", "rings"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "in schwellenden Wonnewogen schier zersprengend,", "tokens": ["in", "schwel\u00b7len\u00b7den", "Won\u00b7ne\u00b7wo\u00b7gen", "schier", "zer\u00b7spren\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "durch alle Th\u00e4ler, \u00fcber alle H\u00f6h'n;", "tokens": ["durch", "al\u00b7le", "Th\u00e4\u00b7ler", ",", "\u00fc\u00b7ber", "al\u00b7le", "H\u00f6h'n", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "die strotzenden Knospen schienen nur zu ", "tokens": ["die", "strot\u00b7zen\u00b7den", "Knos\u00b7pen", "schie\u00b7nen", "nur", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPR"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "auf den Erl\u00f6sungshauch, der endlich sie", "tokens": ["auf", "den", "Er\u00b7l\u00f6\u00b7sungs\u00b7hauch", ",", "der", "end\u00b7lich", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "PPER"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.7": {"text": "aus ihrer starren H\u00fclle sollte rei\u00dfen,", "tokens": ["aus", "ih\u00b7rer", "star\u00b7ren", "H\u00fcl\u00b7le", "soll\u00b7te", "rei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "sich satt zu trinken an der jungen Luft", "tokens": ["sich", "satt", "zu", "trin\u00b7ken", "an", "der", "jun\u00b7gen", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADJD", "PTKZU", "VVINF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "und lebensselig all ihr keusches Gr\u00fcn", "tokens": ["und", "le\u00b7bens\u00b7se\u00b7lig", "all", "ihr", "keu\u00b7sches", "Gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PIAT", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "dem flammenden Ku\u00df des Lichtes zu verm\u00e4hlen.", "tokens": ["dem", "flam\u00b7men\u00b7den", "Ku\u00df", "des", "Lich\u00b7tes", "zu", "ver\u00b7m\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Hinein ich lauschte in dies trunkne Werden,", "tokens": ["Hin\u00b7ein", "ich", "lauschte", "in", "dies", "trunk\u00b7ne", "Wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "APPR", "PDS", "ADJA", "NN", "$,"], "meter": "++-+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "ein einzig lauschend Aug' und Ohr und Herz,", "tokens": ["ein", "ein\u00b7zig", "lau\u00b7schend", "Aug'", "und", "Ohr", "und", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "erlauschte Alles \u2013 Nichts \u2013 o Alles all:", "tokens": ["er\u00b7lauschte", "Al\u00b7les", "\u2013", "Nichts", "\u2013", "o", "Al\u00b7les", "all", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$(", "PIS", "$(", "FM", "PIS", "PIAT", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "des Baums, der Gr\u00e4ser Durst, den schmetternden Vogel,", "tokens": ["des", "Baums", ",", "der", "Gr\u00e4\u00b7ser", "Durst", ",", "den", "schmet\u00b7tern\u00b7den", "Vo\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "den Gie\u00dfbach, der zur Ebne zischend sprang,", "tokens": ["den", "Gie\u00df\u00b7bach", ",", "der", "zur", "Eb\u00b7ne", "zi\u00b7schend", "sprang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "und in der tiefen Ferne, unentwirrbar", "tokens": ["und", "in", "der", "tie\u00b7fen", "Fer\u00b7ne", ",", "un\u00b7ent\u00b7wirr\u00b7bar"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "dem zitternden Duft, der Menschenstadt Gewimmel.", "tokens": ["dem", "zit\u00b7tern\u00b7den", "Duft", ",", "der", "Men\u00b7schen\u00b7stadt", "Ge\u00b7wim\u00b7mel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Und Menschen, Bach und Halm und Baum und Vogel,", "tokens": ["Und", "Men\u00b7schen", ",", "Bach", "und", "Halm", "und", "Baum", "und", "Vo\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NE", "KON", "NN", "KON", "NE", "KON", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "von Einer Brunst umschlungen f\u00fchlt' ich Alles,", "tokens": ["von", "Ei\u00b7ner", "Brunst", "um\u00b7schlun\u00b7gen", "f\u00fchlt'", "ich", "Al\u00b7les", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "VVFIN", "PPER", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "in Eine Inbrunst Alles untertauchen,", "tokens": ["in", "Ei\u00b7ne", "In\u00b7brunst", "Al\u00b7les", "un\u00b7ter\u00b7tau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "in Eines branden mit dem str\u00f6menden Glanz \u2013", "tokens": ["in", "Ei\u00b7nes", "bran\u00b7den", "mit", "dem", "str\u00f6\u00b7men\u00b7den", "Glanz", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.12": {"text": "und eine Sehnsucht mir die Brust best\u00fcrmen,", "tokens": ["und", "ei\u00b7ne", "Sehn\u00b7sucht", "mir", "die", "Brust", "be\u00b7st\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "mich hinzugeben in das All der Welt", "tokens": ["mich", "hin\u00b7zu\u00b7ge\u00b7ben", "in", "das", "All", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "und ", "tokens": ["und"], "token_info": ["word"], "pos": ["KON"], "meter": "-", "measure": "single.down"}}, "stanza.3": {"line.1": {"text": "Doch klammernd hielt mich die Erinnrung fest;", "tokens": ["Doch", "klam\u00b7mernd", "hielt", "mich", "die", "E\u00b7rinn\u00b7rung", "fest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "und taumelnd, in zerknirschten Wonnen ahnt' ich,", "tokens": ["und", "tau\u00b7melnd", ",", "in", "zer\u00b7knirschten", "Won\u00b7nen", "ahnt'", "ich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "$,", "APPR", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "da\u00df Menschenkindern nur ein Menschenherz,", "tokens": ["da\u00df", "Men\u00b7schen\u00b7kin\u00b7dern", "nur", "ein", "Men\u00b7schen\u00b7herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "selbstloser Liebe voll, die Fluten birgt,", "tokens": ["selbst\u00b7lo\u00b7ser", "Lie\u00b7be", "voll", ",", "die", "Flu\u00b7ten", "birgt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "erl\u00f6st zu werden von des Leibes Schranken", "tokens": ["er\u00b7l\u00f6st", "zu", "wer\u00b7den", "von", "des", "Lei\u00b7bes", "Schran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "PTKZU", "VAINF", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "und selbstvergessen in das All zu sinken:", "tokens": ["und", "selbst\u00b7ver\u00b7ges\u00b7sen", "in", "das", "All", "zu", "sin\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "ein Andres Herz, selbstloser Liebe voll.", "tokens": ["ein", "And\u00b7res", "Herz", ",", "selbst\u00b7lo\u00b7ser", "Lie\u00b7be", "voll", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Nach Liebe, Liebe schrie es laut in mir,", "tokens": ["Nach", "Lie\u00b7be", ",", "Lie\u00b7be", "schrie", "es", "laut", "in", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "VVFIN", "PPER", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "nach einem Herzen, das f\u00fcr Mich nur schl\u00fcge,", "tokens": ["nach", "ei\u00b7nem", "Her\u00b7zen", ",", "das", "f\u00fcr", "Mich", "nur", "schl\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "f\u00fcr mich, f\u00fcr mich, der \u2013 selber lieblos immer!", "tokens": ["f\u00fcr", "mich", ",", "f\u00fcr", "mich", ",", "der", "\u2013", "sel\u00b7ber", "lieb\u00b7los", "im\u00b7mer", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "$,", "PRELS", "$(", "ADV", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da brach's empor, da sah ich ", "tokens": ["Da", "brach's", "em\u00b7por", ",", "da", "sah", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKVZ", "$,", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "und sah's und schlug die H\u00e4nde vors Gesicht", "tokens": ["und", "sah's", "und", "schlug", "die", "H\u00e4n\u00b7de", "vors", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "KON", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und warf zur Erde mich und weinte.", "tokens": ["und", "warf", "zur", "Er\u00b7de", "mich", "und", "wein\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPER", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}