{"textgrid.poem.63349": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Oberammergau in Amerika", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was unsern Christus Lang betrifft,", "tokens": ["Was", "un\u00b7sern", "Chris\u00b7tus", "Lang", "be\u00b7tr\u00b7ifft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "So hatte er sich eingeschifft,", "tokens": ["So", "hat\u00b7te", "er", "sich", "ein\u00b7ge\u00b7schifft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um in atlantischen Bezirken", "tokens": ["Um", "in", "at\u00b7lan\u00b7ti\u00b7schen", "Be\u00b7zir\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr's heilige Christentum zu wirken.", "tokens": ["F\u00fcr's", "hei\u00b7li\u00b7ge", "Chris\u00b7ten\u00b7tum", "zu", "wir\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "In Boston war er hinterm Zaun", "tokens": ["In", "Bos\u00b7ton", "war", "er", "hin\u00b7term", "Zaun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VAFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie'n Gnu f\u00fcr'n Dollar anzuschau'n,", "tokens": ["Wie'n", "Gnu", "f\u00fcr'n", "Dol\u00b7lar", "an\u00b7zu\u00b7schau'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit ihm im feschen Dirndlkleid", "tokens": ["Mit", "ihm", "im", "fe\u00b7schen", "Dirndl\u00b7kleid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Maria Magdala. All right.", "tokens": ["Ma\u00b7ria", "Mag\u00b7da\u00b7la", ".", "All", "right", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Es wu\u00dften Mister, Mi\u00df und Missis", "tokens": ["Es", "wu\u00df\u00b7ten", "Mis\u00b7ter", ",", "Mi\u00df", "und", "Mis\u00b7sis"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bisher von Christus nichts gewisses,", "tokens": ["Bis\u00b7her", "von", "Chris\u00b7tus", "nichts", "ge\u00b7wis\u00b7ses", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "PIS", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bis salbungsvoll und blondbehaart", "tokens": ["Bis", "sal\u00b7bungs\u00b7voll", "und", "blond\u00b7be\u00b7haart"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er sich leibhaftig offenbart.", "tokens": ["Er", "sich", "leib\u00b7haf\u00b7tig", "of\u00b7fen\u00b7bart", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er kommt aus Bayerns Urwaldwildnis,", "tokens": ["Er", "kommt", "aus", "Bay\u00b7erns", "Ur\u00b7wald\u00b7wild\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verkauft f\u00fcr zwanzig Cents sein Bildnis", "tokens": ["Ver\u00b7kauft", "f\u00fcr", "zwan\u00b7zig", "Cents", "sein", "Bild\u00b7nis"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "CARD", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Palme, Kreuz und \u00d6lbaumreis.", "tokens": ["Mit", "Pal\u00b7me", ",", "Kreuz", "und", "\u00d6l\u00b7baum\u00b7reis", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "(in Holz geschnitzt ein h\u00f6herer Preis.)", "tokens": ["(", "in", "Holz", "ge\u00b7schnitzt", "ein", "h\u00f6\u00b7he\u00b7rer", "Preis", ".", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "NN", "VVPP", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Ach, manche Mi\u00df entbrannte schon", "tokens": ["Ach", ",", "man\u00b7che", "Mi\u00df", "ent\u00b7brann\u00b7te", "schon"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PIAT", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr ihn in gro\u00dfer \u2013 yes \u2013 Passion.", "tokens": ["F\u00fcr", "ihn", "in", "gro\u00b7\u00dfer", "\u2013", "yes", "\u2013", "Pas\u00b7si\u00b7on", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ADJA", "$(", "FM", "$(", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Barnum erbla\u00dft vor Neid und kl\u00e4fft:", "tokens": ["Bar\u00b7num", "er\u00b7bla\u00dft", "vor", "Neid", "und", "kl\u00e4fft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wei\u00df Gott, sein Sohn versteht's Gesch\u00e4ft...", "tokens": ["Wei\u00df", "Gott", ",", "sein", "Sohn", "ver\u00b7steht's", "Ge\u00b7sch\u00e4ft", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "PPOSAT", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}