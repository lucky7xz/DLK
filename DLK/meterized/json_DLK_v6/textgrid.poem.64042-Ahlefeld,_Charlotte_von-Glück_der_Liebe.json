{"textgrid.poem.64042": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcck der Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einem Schmetterlinge gleicht die Liebe;", "tokens": ["Ei\u00b7nem", "Schmet\u00b7ter\u00b7lin\u00b7ge", "gleicht", "die", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie er flatternd \u00fcber Blumen schwebt,", "tokens": ["Wie", "er", "flat\u00b7ternd", "\u00fc\u00b7ber", "Blu\u00b7men", "schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "So entflieht sie oft auf leichten Schwingen,", "tokens": ["So", "ent\u00b7flieht", "sie", "oft", "auf", "leich\u00b7ten", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und nur selten kehrt sie uns zur\u00fcck.", "tokens": ["Und", "nur", "sel\u00b7ten", "kehrt", "sie", "uns", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Um gewaltsam ihre Flucht zu hemmen,", "tokens": ["Um", "ge\u00b7walt\u00b7sam", "ih\u00b7re", "Flucht", "zu", "hem\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Strebt das kranke Herz mit leisem Weh;", "tokens": ["Strebt", "das", "kran\u00b7ke", "Herz", "mit", "lei\u00b7sem", "Weh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "M\u00f6cht' ihr gern die raschen Fl\u00fcgel binden,", "tokens": ["M\u00f6cht'", "ihr", "gern", "die", "ra\u00b7schen", "Fl\u00fc\u00b7gel", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Gern sie bannen in der Treue Kreis.", "tokens": ["Gern", "sie", "ban\u00b7nen", "in", "der", "Treu\u00b7e", "Kreis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Aber wie des Schmetterlinges Farben", "tokens": ["A\u00b7ber", "wie", "des", "Schmet\u00b7ter\u00b7lin\u00b7ges", "Far\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Selbst in zarten H\u00e4nden untergehn,", "tokens": ["Selbst", "in", "zar\u00b7ten", "H\u00e4n\u00b7den", "un\u00b7ter\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "So vernichten Fesseln auch die Reize,", "tokens": ["So", "ver\u00b7nich\u00b7ten", "Fes\u00b7seln", "auch", "die", "Rei\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die der Liebe freie Regung schm\u00fccken.", "tokens": ["Die", "der", "Lie\u00b7be", "frei\u00b7e", "Re\u00b7gung", "schm\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Darum \u00f6ffne ihrem kurzen Gl\u00fccke", "tokens": ["Da\u00b7rum", "\u00f6ff\u00b7ne", "ih\u00b7rem", "kur\u00b7zen", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Willig und geniessend Geist und Herz;", "tokens": ["Wil\u00b7lig", "und", "ge\u00b7nies\u00b7send", "Geist", "und", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVPP", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Aber will es wankelm\u00fcthig weichen", "tokens": ["A\u00b7ber", "will", "es", "wan\u00b7kel\u00b7m\u00fct\u00b7hig", "wei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Trauere dann \u2013 doch halt es nicht zur\u00fcck!", "tokens": ["Trau\u00b7e\u00b7re", "dann", "\u2013", "doch", "halt", "es", "nicht", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$(", "ADV", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}