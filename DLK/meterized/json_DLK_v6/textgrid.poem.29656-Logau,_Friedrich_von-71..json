{"textgrid.poem.29656": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "71.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwar das Tuch zu F\u00fcrsten-Kleidern wird genummen von dem guten;", "tokens": ["Zwar", "das", "Tuch", "zu", "F\u00fcrs\u00b7ten\u00b7Klei\u00b7dern", "wird", "ge\u00b7num\u00b7men", "von", "dem", "gu\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "NN", "VAFIN", "VVPP", "APPR", "ART", "ADJA", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Dennoch d\u00fcrffen sie offt neues; dann bei Hofe hats viel Moten.", "tokens": ["Den\u00b7noch", "d\u00fcrf\u00b7fen", "sie", "offt", "neu\u00b7es", ";", "dann", "bei", "Ho\u00b7fe", "hats", "viel", "Mo\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADJA", "$.", "ADV", "APPR", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}