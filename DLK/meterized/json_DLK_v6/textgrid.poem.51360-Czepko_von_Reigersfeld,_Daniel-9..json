{"textgrid.poem.51360": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich fliehe, die mich suchet,", "tokens": ["Ich", "flie\u00b7he", ",", "die", "mich", "su\u00b7chet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die mich fleucht, die such ich,", "tokens": ["Die", "mich", "fleucht", ",", "die", "such", "ich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "PRELS", "VVFIN", "PPER", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ich lobe, was mir fluchet,", "tokens": ["Ich", "lo\u00b7be", ",", "was", "mir", "flu\u00b7chet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der ich fluch, ehret mich.", "tokens": ["Der", "ich", "fluch", ",", "eh\u00b7ret", "mich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nicht alles, was bequem,", "tokens": ["Nicht", "al\u00b7les", ",", "was", "be\u00b7quem", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "PIS", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ist lieb und angenehm.", "tokens": ["Ist", "lieb", "und", "an\u00b7ge\u00b7nehm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Was uns die Augen giebt,", "tokens": ["Was", "uns", "die", "Au\u00b7gen", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Das hasst man offt im Hertzen,", "tokens": ["Das", "hasst", "man", "offt", "im", "Hert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und dis, was uns betr\u00fcbt,", "tokens": ["Und", "dis", ",", "was", "uns", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Verehrn wir voller Schmertzen.", "tokens": ["Ver\u00b7ehrn", "wir", "vol\u00b7ler", "Schmert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Ich wil, die mich nicht wil,", "tokens": ["Ich", "wil", ",", "die", "mich", "nicht", "wil", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PRELS", "PPER", "PTKNEG", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Die wil, wil ich betr\u00fcben:", "tokens": ["Die", "wil", ",", "wil", "ich", "be\u00b7tr\u00fc\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "$,", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Die mich liebt, plag ich viel,", "tokens": ["Die", "mich", "liebt", ",", "plag", "ich", "viel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Die mich plagt, wil ich lieben.", "tokens": ["Die", "mich", "plagt", ",", "wil", "ich", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "So lebt die Lieb allzeit", "tokens": ["So", "lebt", "die", "Lieb", "all\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "In Wiederw\u00e4rtigkeit.", "tokens": ["In", "Wie\u00b7der\u00b7w\u00e4r\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}