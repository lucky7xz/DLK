{"textgrid.poem.24823": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch h\u00e4ngen um des toten Tages Bahre", "tokens": ["Noch", "h\u00e4n\u00b7gen", "um", "des", "to\u00b7ten", "Ta\u00b7ges", "Bah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Wolkenfalten wie ein Trauerflor,", "tokens": ["Die", "Wol\u00b7ken\u00b7fal\u00b7ten", "wie", "ein", "Trau\u00b7er\u00b7flor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch m\u00e4hlich schwebt die Nacht, die heilig-klare,", "tokens": ["Doch", "m\u00e4h\u00b7lich", "schwebt", "die", "Nacht", ",", "die", "hei\u00b7lig\u00b7kla\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Tag der Tr\u00e4umenden, empor.", "tokens": ["Der", "Tag", "der", "Tr\u00e4u\u00b7men\u00b7den", ",", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf Meer und Erde senkt sie stille Feier", "tokens": ["Auf", "Meer", "und", "Er\u00b7de", "senkt", "sie", "stil\u00b7le", "Fei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und d\u00e4mpft den letzten Ton, der sich noch regt;", "tokens": ["Und", "d\u00e4mpft", "den", "letz\u00b7ten", "Ton", ",", "der", "sich", "noch", "regt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es wehn und wallen ihre Sternenschleier,", "tokens": ["Es", "wehn", "und", "wal\u00b7len", "ih\u00b7re", "Ster\u00b7nen\u00b7schlei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Himmelsl\u00fcften sanft bewegt.", "tokens": ["Von", "Him\u00b7mel\u00b7sl\u00fcf\u00b7ten", "sanft", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nun klopft ein jedes Herz mit leiserm Schlage,", "tokens": ["Nun", "klopft", "ein", "je\u00b7des", "Herz", "mit", "lei\u00b7serm", "Schla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "PIAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Jammer wird jetzt regungslos und mild,", "tokens": ["Der", "Jam\u00b7mer", "wird", "jetzt", "re\u00b7gungs\u00b7los", "und", "mild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und still zur Andacht wandelt sich die Klage,", "tokens": ["Und", "still", "zur", "An\u00b7dacht", "wan\u00b7delt", "sich", "die", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch eh' sie aus der Seele quillt.", "tokens": ["Noch", "eh'", "sie", "aus", "der", "See\u00b7le", "quillt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du aber, Ruheloser, dem sich bange", "tokens": ["Du", "a\u00b7ber", ",", "Ru\u00b7he\u00b7lo\u00b7ser", ",", "dem", "sich", "ban\u00b7ge"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "NN", "$,", "PRELS", "PRF", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In Hoffnung und in Gram das Herz verzehrt,", "tokens": ["In", "Hoff\u00b7nung", "und", "in", "Gram", "das", "Herz", "ver\u00b7zehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NE", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der ewig sucht mit ungestilltem Drange,", "tokens": ["Der", "e\u00b7wig", "sucht", "mit", "un\u00b7ge\u00b7still\u00b7tem", "Dran\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was ihm die Erde nie gew\u00e4hrt:", "tokens": ["Was", "ihm", "die", "Er\u00b7de", "nie", "ge\u00b7w\u00e4hrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Flieh du die Sommernacht, die sternbes\u00e4te,", "tokens": ["Flieh", "du", "die", "Som\u00b7mer\u00b7nacht", ",", "die", "stern\u00b7be\u00b7s\u00e4\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Flieh, bis das tiefste Dunkel dich begr\u00e4bt,", "tokens": ["Flieh", ",", "bis", "das", "tiefs\u00b7te", "Dun\u00b7kel", "dich", "be\u00b7gr\u00e4bt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Damit kein Mi\u00dflaut sei in dem Gebete,", "tokens": ["Da\u00b7mit", "kein", "Mi\u00df\u00b7laut", "sei", "in", "dem", "Ge\u00b7be\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "NN", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das auf des Weltalls Lippen schwebt.", "tokens": ["Das", "auf", "des", "Welt\u00b7alls", "Lip\u00b7pen", "schwebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}