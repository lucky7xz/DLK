{"textgrid.poem.45298": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Warnung", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du von Fortschritt reden, mein armer Christ,", "tokens": ["Willst", "du", "von", "Fort\u00b7schritt", "re\u00b7den", ",", "mein", "ar\u00b7mer", "Christ", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "VVINF", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+---+-+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Mu\u00dft sicher du sein zu jeder Frist,", "tokens": ["Mu\u00dft", "si\u00b7cher", "du", "sein", "zu", "je\u00b7der", "Frist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "PPER", "VAINF", "APPR", "PIAT", "NN", "$,"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Da\u00df du auf dem rechten Wege bist,", "tokens": ["Da\u00df", "du", "auf", "dem", "rech\u00b7ten", "We\u00b7ge", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Sonst f\u00fchrt dein Plagen hart und viel", "tokens": ["Sonst", "f\u00fchrt", "dein", "Pla\u00b7gen", "hart", "und", "viel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADJD", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dich immer weiter ab vom Ziel,", "tokens": ["Dich", "im\u00b7mer", "wei\u00b7ter", "ab", "vom", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "PTKVZ", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und all dein Fortschritt will nichts bedeuten,", "tokens": ["Und", "all", "dein", "Fort\u00b7schritt", "will", "nichts", "be\u00b7deu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Als seitw\u00e4rts oder r\u00fcckw\u00e4rts schreiten.", "tokens": ["Als", "seit\u00b7w\u00e4rts", "o\u00b7der", "r\u00fcck\u00b7w\u00e4rts", "schrei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}