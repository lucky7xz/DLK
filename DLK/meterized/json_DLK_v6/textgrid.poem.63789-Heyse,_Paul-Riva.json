{"textgrid.poem.63789": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Riva", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich stieg von Riva jenen Pfad hinan,", "tokens": ["Ich", "stieg", "von", "Ri\u00b7va", "je\u00b7nen", "Pfad", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den breitgebahnten, nach dem Ledrotale,", "tokens": ["Den", "breit\u00b7ge\u00b7bahn\u00b7ten", ",", "nach", "dem", "Led\u00b7ro\u00b7ta\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch den in Katarakten der Ponale", "tokens": ["Durch", "den", "in", "Ka\u00b7ta\u00b7rak\u00b7ten", "der", "Po\u00b7na\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "APPR", "NN", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Sich st\u00fcrzt; und eh' ich noch die Schlucht gewann,", "tokens": ["Sich", "st\u00fcrzt", ";", "und", "eh'", "ich", "noch", "die", "Schlucht", "ge\u00b7wann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "$.", "KON", "KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Fand ich ein Haus am Weg. Ein St\u00fcck daran", "tokens": ["Fand", "ich", "ein", "Haus", "am", "Weg", ".", "Ein", "St\u00fcck", "da\u00b7ran"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$.", "ART", "NN", "PAV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "War frisch gemauert \u00fcber dem Portale,", "tokens": ["War", "frisch", "ge\u00b7mau\u00b7ert", "\u00fc\u00b7ber", "dem", "Por\u00b7ta\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Daneben trug die alte Wand, die kahle,", "tokens": ["Da\u00b7ne\u00b7ben", "trug", "die", "al\u00b7te", "Wand", ",", "die", "kah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Kohleninschrift, die der Zorn ersann:", "tokens": ["Die", "Koh\u00b7le\u00b7nin\u00b7schrift", ",", "die", "der", "Zorn", "er\u00b7sann", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "\u00bbdu halt nun Ruh, vermaledeiter Wicht,", "tokens": ["\u00bb", "du", "halt", "nun", "Ruh", ",", "ver\u00b7ma\u00b7le\u00b7dei\u00b7ter", "Wicht", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der du vom Hochgebirg zu dieser Mauer", "tokens": ["Der", "du", "vom", "Hoch\u00b7ge\u00b7birg", "zu", "die\u00b7ser", "Mau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPRART", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kamst, wider Recht den T\u00fcrsturz einzubrechen!\u00ab", "tokens": ["Kamst", ",", "wi\u00b7der", "Recht", "den", "T\u00fcr\u00b7sturz", "ein\u00b7zu\u00b7bre\u00b7chen", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$,", "APPR", "NN", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O Vater Shakespeare, dein Kothurn ist nicht", "tokens": ["O", "Va\u00b7ter", "Sha\u00b7ke\u00b7spe\u00b7a\u00b7re", ",", "dein", "Ko\u00b7thurn", "ist", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "NE", "$,", "PPOSAT", "NN", "VAFIN", "PTKNEG"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Zu hoch f\u00fcr sie! Wo lernte dieser Bauer", "tokens": ["Zu", "hoch", "f\u00fcr", "sie", "!", "Wo", "lern\u00b7te", "die\u00b7ser", "Bau\u00b7er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "APPR", "PPER", "$.", "PWAV", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie deine K\u00f6nige und Helden sprechen?", "tokens": ["Wie", "dei\u00b7ne", "K\u00f6\u00b7ni\u00b7ge", "und", "Hel\u00b7den", "spre\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}