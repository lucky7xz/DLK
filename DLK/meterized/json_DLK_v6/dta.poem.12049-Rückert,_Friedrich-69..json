{"dta.poem.12049": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "69.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer strebte nach dem Ziel, wenn er so fern es s\u00e4he,", "tokens": ["Wer", "streb\u00b7te", "nach", "dem", "Ziel", ",", "wenn", "er", "so", "fern", "es", "s\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "ADV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie's wirklich ist? der Wunsch sieht alles in der N\u00e4he.", "tokens": ["Wie's", "wirk\u00b7lich", "ist", "?", "der", "Wunsch", "sieht", "al\u00b7les", "in", "der", "N\u00e4\u00b7he", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VAFIN", "$.", "ART", "NN", "VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und wenn du n\u00e4her r\u00fcckst, und merkst den Augentrug,", "tokens": ["Und", "wenn", "du", "n\u00e4\u00b7her", "r\u00fcckst", ",", "und", "merkst", "den", "Au\u00b7gen\u00b7trug", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVFIN", "$,", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Treibt weiter dich der Trieb, der einmal ist im Zug.", "tokens": ["Treibt", "wei\u00b7ter", "dich", "der", "Trieb", ",", "der", "ein\u00b7mal", "ist", "im", "Zug", "."], "token_info": ["word", "word", "word", "word", "w