{"dta.poem.73": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "Rothschilds Gr\u00e4ber.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1771", "urn": "urn:nbn:de:kobv:b4-200905199486", "language": ["de:0.99"], "booktitle": "[Klopstock, Friedrich Gottlieb]: Oden. Hamburg, 1771."}, "poem": {"stanza.1": {"line.1": {"text": "Ach, hier haben sie Dich bey deinen V\u00e4tern begraben,", "tokens": ["Ach", ",", "hier", "ha\u00b7ben", "sie", "Dich", "bey", "dei\u00b7nen", "V\u00e4\u00b7tern", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VAFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Den wir liebten, um den lange die Thr\u00e4ne noch flie\u00dft;", "tokens": ["Den", "wir", "lieb\u00b7ten", ",", "um", "den", "lan\u00b7ge", "die", "Thr\u00e4\u00b7ne", "noch", "flie\u00dft", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "KOUI", "ART", "ADV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Jene treuere, die aus nie vergessendem Herzen", "tokens": ["Je\u00b7ne", "treu\u00b7e\u00b7re", ",", "die", "aus", "nie", "ver\u00b7ges\u00b7sen\u00b7dem", "Her\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDAT", "ADJA", "$,", "PRELS", "APPR", "ADV", "ADJA", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Kommt, und des Einsamen Blick sp\u00e4t mit Er-", "tokens": ["Kommt", ",", "und", "des", "Ein\u00b7sa\u00b7men", "Blick", "sp\u00e4t", "mit", "Er"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KON", "ART", "ADJA", "NN", "ADJD", "APPR", "TRUNC"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Sollt um seinen entschlafenen K\u00f6nig nicht Thr\u00e4nen", "tokens": ["Sollt", "um", "sei\u00b7nen", "ent\u00b7schla\u00b7fe\u00b7nen", "K\u00f6\u00b7nig", "nicht", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "PPOSAT", "ADJA", "NN", "PTKNEG", "NN"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Lange vergiessen ein Volk, dessen Wittwe nicht", "tokens": ["Lan\u00b7ge", "ver\u00b7gies\u00b7sen", "ein", "Volk", ",", "des\u00b7sen", "Witt\u00b7we", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELAT", "NN", "PTKNEG"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.7": {"text": "Ach, um einen K\u00f6nig, von dem der Waise, des Dankes", "tokens": ["Ach", ",", "um", "ei\u00b7nen", "K\u00f6\u00b7nig", ",", "von", "dem", "der", "Wai\u00b7se", ",", "des", "Dan\u00b7kes"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$,", "KOUI", "ART", "NN", "$,", "APPR", "PRELS", "ART", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Z\u00e4hren im Aug\u2019, oft kam, lange nicht klagen sein", "tokens": ["Z\u00e4h\u00b7ren", "im", "Aug'", ",", "oft", "kam", ",", "lan\u00b7ge", "nicht", "kla\u00b7gen", "sein"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "$,", "ADV", "VVFIN", "$,", "ADV", "PTKNEG", "VVFIN", "PPOSAT"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.9": {"text": "Aber noch wend\u2019 ich mich weg, kann noch zu der Halle", "tokens": ["A\u00b7ber", "noch", "wend'", "ich", "mich", "weg", ",", "kann", "noch", "zu", "der", "Hal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KOUS", "PPER", "PRF", "PTKVZ", "$,", "VMFIN", "ADV", "APPR", "ART", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.10": {"text": "Wo des Todten Gebein neben der Todten itzt ruht,", "tokens": ["Wo", "des", "Tod\u00b7ten", "Ge\u00b7bein", "ne\u00b7ben", "der", "Tod\u00b7ten", "itzt", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Neben Luisa, die uns des Kummers einzigen Trost gab,", "tokens": ["Ne\u00b7ben", "Lu\u00b7i\u00b7sa", ",", "die", "uns", "des", "Kum\u00b7mers", "ein\u00b7zi\u00b7gen", "Trost", "gab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PRELS", "PPER", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+--+--+", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "Die wir liebten, der auch sp\u00e4tere Traurigkeit rann!", "tokens": ["Die", "wir", "lieb\u00b7ten", ",", "der", "auch", "sp\u00e4\u00b7te\u00b7re", "Trau\u00b7rig\u00b7keit", "rann", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "PRELS", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "O ihr \u00e4lteren Todten, ihr Staub! einst K\u00f6nige, fr\u00fch rief", "tokens": ["O", "ihr", "\u00e4l\u00b7te\u00b7ren", "Tod\u00b7ten", ",", "ihr", "Staub", "!", "einst", "K\u00f6\u00b7ni\u00b7ge", ",", "fr\u00fch", "rief"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NN", "$.", "ADV", "NN", "$,", "ADJD", "VVFIN"], "meter": "--+--+--+-+-+-+", "measure": "anapaest.tri.plus"}, "line.14": {"text": "Er den Enkel zu euch, der die Welten beherrscht!", "tokens": ["Er", "den", "En\u00b7kel", "zu", "euch", ",", "der", "die", "Wel\u00b7ten", "be\u00b7herrscht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPR", "PPER", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.15": {"text": "Ernst, in Sterbegedanken, umwandl\u2019 ich die Gr\u00e4ber,", "tokens": ["Ernst", ",", "in", "Ster\u00b7be\u00b7ge\u00b7dan\u00b7ken", ",", "um\u00b7wandl'", "ich", "die", "Gr\u00e4\u00b7ber", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "Ihren Marmor, und seh Schrift wie Flammen", "tokens": ["Ih\u00b7ren", "Mar\u00b7mor", ",", "und", "seh", "Schrift", "wie", "Flam\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KON", "VVFIN", "NN", "KOKOM", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Andre, wie die, so die Aussengestalt der Thaten nur bildet,", "tokens": ["And\u00b7re", ",", "wie", "die", ",", "so", "die", "Aus\u00b7sen\u00b7ge\u00b7stalt", "der", "Tha\u00b7ten", "nur", "bil\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PWAV", "ART", "$,", "ADV", "ART", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.18": {"text": "Unbekannt mit dem Zweck, welchen die Seele verbarg.", "tokens": ["Un\u00b7be\u00b7kannt", "mit", "dem", "Zweck", ",", "wel\u00b7chen", "die", "See\u00b7le", "ver\u00b7barg", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,", "PWAT", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.19": {"text": "Furchtbar schimmert die himmlische Schrift: Dort sind", "tokens": ["Furcht\u00b7bar", "schim\u00b7mert", "die", "himm\u00b7li\u00b7sche", "Schrift", ":", "Dort", "sind"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$.", "ADV", "VAFIN"], "meter": "+-+--+--++-", "measure": "trochaic.penta.relaxed"}, "line.20": {"text": "Wo die Krone des Lohns, keine verg\u00e4ngliche, strahlt!", "tokens": ["Wo", "die", "Kro\u00b7ne", "des", "Lohns", ",", "kei\u00b7ne", "ver\u00b7g\u00e4ng\u00b7li\u00b7che", ",", "strahlt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "$,", "PIAT", "ADJA", "$,", "VVFIN", "$."], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Ernster, in tieferer Todesbetrachtung meid\u2019 ich die Halle", "tokens": ["Erns\u00b7ter", ",", "in", "tie\u00b7fe\u00b7rer", "To\u00b7des\u00b7be\u00b7trach\u00b7tung", "meid'", "ich", "die", "Hal\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.22": {"text": "Stets noch, in welche dem Thron Friederichs", "tokens": ["Stets", "noch", ",", "in", "wel\u00b7che", "dem", "Thron", "Frie\u00b7de\u00b7richs"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "APPR", "PRELS", "ART", "NN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Denn mir blutet mein Herz um Ihn! O Nacht des", "tokens": ["Denn", "mir", "blu\u00b7tet", "mein", "Herz", "um", "Ihn", "!", "O", "Nacht", "des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$.", "NE", "NN", "ART"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.24": {"text": "Als die Aussaat Gott s\u00e4te, wie traurig warst du!", "tokens": ["Als", "die", "Aus\u00b7saat", "Gott", "s\u00e4\u00b7te", ",", "wie", "trau\u00b7rig", "warst", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVFIN", "$,", "PWAV", "ADJD", "VAFIN", "PPER", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.25": {"text": "Aber warum wank\u2019 ich, und s\u00e4ume noch stets, zu dem", "tokens": ["A\u00b7ber", "wa\u00b7rum", "wank'", "ich", ",", "und", "s\u00e4u\u00b7me", "noch", "stets", ",", "zu", "dem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "$,", "KON", "VVFIN", "ADV", "ADV", "$,", "APPR", "ART"], "meter": "+---+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.26": {"text": "Hinzugehen, wo Er einst mit den Todten erwacht?", "tokens": ["Hin\u00b7zu\u00b7ge\u00b7hen", ",", "wo", "Er", "einst", "mit", "den", "Tod\u00b7ten", "er\u00b7wacht", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIZU", "$,", "PWAV", "PPER", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.27": {"text": "Ist es nicht Gott, der Ihn in seine Gefilde ges\u00e4t hat?", "tokens": ["Ist", "es", "nicht", "Gott", ",", "der", "Ihn", "in", "sei\u00b7ne", "Ge\u00b7fil\u00b7de", "ge\u00b7s\u00e4t", "hat", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "NN", "$,", "PRELS", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.28": {"text": "Ach, zu des ewigen Tags dankenden Freuden ges\u00e4t?", "tokens": ["Ach", ",", "zu", "des", "e\u00b7wi\u00b7gen", "Tags", "dan\u00b7ken\u00b7den", "Freu\u00b7den", "ge\u00b7s\u00e4t", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "ART", "ADJA", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.29": {"text": "Und, o sollte noch weich de\u00df Herz seyn, welcher so", "tokens": ["Und", ",", "o", "soll\u00b7te", "noch", "weich", "de\u00df", "Herz", "seyn", ",", "wel\u00b7cher", "so"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "FM", "VMFIN", "ADV", "ADJD", "ART", "NN", "VAINF", "$,", "PRELS", "ADV"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.30": {"text": "Die er liebte, verlor, Viele, die gl\u00fccklicher sind?", "tokens": ["Die", "er", "lieb\u00b7te", ",", "ver\u00b7lor", ",", "Vie\u00b7le", ",", "die", "gl\u00fcck\u00b7li\u00b7cher", "sind", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "VVFIN", "$,", "PIS", "$,", "PRELS", "ADJD", "VAFIN", "$."], "meter": "+-+--++--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.31": {"text": "Dessen Gedanken um ihn schon viel Unsterbliche sam-", "tokens": ["Des\u00b7sen", "Ge\u00b7dan\u00b7ken", "um", "ihn", "schon", "viel", "U\u00b7nsterb\u00b7li\u00b7che", "sam"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "APPR", "PPER", "ADV", "PIAT", "ADJA", "TRUNC"], "meter": "+--+--++-+--+-", "measure": "dactylic.di.plus"}, "line.32": {"text": "Wenn er den engeren Kreis dieser Verg\u00e4nglich-", "tokens": ["Wenn", "er", "den", "en\u00b7ge\u00b7ren", "Kreis", "die\u00b7ser", "Ver\u00b7g\u00e4ng\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PDAT", "TRUNC"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.33": {"text": "Und die H\u00fctten an Gr\u00e4bern betrachtet, worinn die", "tokens": ["Und", "die", "H\u00fct\u00b7ten", "an", "Gr\u00e4\u00b7bern", "be\u00b7trach\u00b7tet", ",", "wo\u00b7rinn", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NN", "VVPP", "$,", "PWAV", "ART"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.34": {"text": "Tr\u00e4umen, bis endlich der Tod sie zu dem Leben", "tokens": ["Tr\u00e4u\u00b7men", ",", "bis", "end\u00b7lich", "der", "Tod", "sie", "zu", "dem", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "ADV", "ART", "NN", "PPER", "APPR", "ART", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.35": {"text": "Diese St\u00e4rke bewafne mein Herz! Doch beb\u2019 ich im", "tokens": ["Die\u00b7se", "St\u00e4r\u00b7ke", "be\u00b7waf\u00b7ne", "mein", "Herz", "!", "Doch", "beb'", "ich", "im"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "VVFIN", "PPOSAT", "NN", "$.", "KON", "VVFIN", "PPER", "APPRART"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.36": {"text": "Ach! des Todten Gebein! unsers K\u00f6nigs Ge-", "tokens": ["Ach", "!", "des", "Tod\u00b7ten", "Ge\u00b7bein", "!", "un\u00b7sers", "K\u00f6\u00b7nigs", "Ge"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "ART", "NN", "NN", "$.", "PPOSAT", "NN", "TRUNC"], "meter": "+-+--+--+--", "measure": "trochaic.tetra.relaxed"}, "line.37": {"text": "Streuet Blumen umher! Der Fr\u00fchling ist wiederge-", "tokens": ["Streu\u00b7et", "Blu\u00b7men", "um\u00b7her", "!", "Der", "Fr\u00fch\u00b7ling", "ist", "wie\u00b7der\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "PTKVZ", "$.", "ART", "NN", "VAFIN", "TRUNC"], "meter": "+-+--+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.38": {"text": "Wiedergekommen \u2026 ohn\u2019 Ihn! \u2026 Bl\u00fcthe be-", "tokens": ["Wie\u00b7der\u00b7ge\u00b7kom\u00b7men", "\u2026", "ohn'", "Ihn", "!", "\u2026", "Bl\u00fc\u00b7the", "be"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word"], "pos": ["NN", "$(", "APPR", "PPER", "$.", "$(", "NN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Daniens sch\u00f6ne Sitte, die selbst dem ruhenden Land-", "tokens": ["Da\u00b7ni\u00b7ens", "sch\u00f6\u00b7ne", "Sit\u00b7te", ",", "die", "selbst", "dem", "ru\u00b7hen\u00b7den", "Lan\u00b7d"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "$,", "PRELS", "ADV", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.40": {"text": "Freudighoffend das Grab j\u00e4hrlich mit Blumen", "tokens": ["Freu\u00b7dig\u00b7hof\u00b7fend", "das", "Grab", "j\u00e4hr\u00b7lich", "mit", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADJD", "APPR", "NN"], "meter": "+-+--++--+-", "measure": "trochaic.penta.relaxed"}, "line.41": {"text": "Sey du festlicher jezt, und streu um des K\u00f6ni