{"textgrid.poem.38537": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "J\u00fcnglingswonne", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So lang' im deutschen Eichenthale,", "tokens": ["So", "lang'", "im", "deut\u00b7schen", "Ei\u00b7chen\u00b7tha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Natur! dein hehrer Schauer webt,", "tokens": ["Na\u00b7tur", "!", "dein", "heh\u00b7rer", "Schau\u00b7er", "webt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und, bei des Mondes Geisterstrale,", "tokens": ["Und", ",", "bei", "des", "Mon\u00b7des", "Geis\u00b7ter\u00b7stra\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Adler Wodans mich umschwebt;", "tokens": ["Der", "Ad\u00b7ler", "Wo\u00b7dans", "mich", "um\u00b7schwebt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So lang' in der Erw\u00e4hlten Blicken", "tokens": ["So", "lang'", "in", "der", "Er\u00b7w\u00e4hl\u00b7ten", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir tausend Himmel offen stehn,", "tokens": ["Mir", "tau\u00b7send", "Him\u00b7mel", "of\u00b7fen", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "CARD", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und, mit verg\u00f6tterndem Entz\u00fccken,", "tokens": ["Und", ",", "mit", "ver\u00b7g\u00f6t\u00b7tern\u00b7dem", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Wir Arm in Arm durchs Leben gehn;", "tokens": ["Wir", "Arm", "in", "Arm", "durchs", "Le\u00b7ben", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So lang', in wackrer Br\u00fcder Kreise,", "tokens": ["So", "lang'", ",", "in", "wack\u00b7rer", "Br\u00fc\u00b7der", "Krei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Bundeskelch zur Weihe klingt,", "tokens": ["Der", "Bun\u00b7des\u00b7kelch", "zur", "Wei\u00b7he", "klingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und jeder, nach der Ahnherrn Weise,", "tokens": ["Und", "je\u00b7der", ",", "nach", "der", "Ahn\u00b7herrn", "Wei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In Tells und Hermanns Jubel singt:", "tokens": ["In", "Tells", "und", "Her\u00b7manns", "Ju\u00b7bel", "singt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Will ich den Gram den Winden geben,", "tokens": ["Will", "ich", "den", "Gram", "den", "Win\u00b7den", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Selbst Augenblicken Kr\u00e4nze weihn,", "tokens": ["Selbst", "Au\u00b7gen\u00b7bli\u00b7cken", "Kr\u00e4n\u00b7ze", "weihn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und noch, wo Todesengel schweben,", "tokens": ["Und", "noch", ",", "wo", "To\u00b7de\u00b7sen\u00b7gel", "schwe\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Pfad mit Rosen mir bestreun!", "tokens": ["Den", "Pfad", "mit", "Ro\u00b7sen", "mir", "be\u00b7streun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}