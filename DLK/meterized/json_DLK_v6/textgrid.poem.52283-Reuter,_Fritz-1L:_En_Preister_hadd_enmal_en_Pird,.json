{"textgrid.poem.52283": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "1L: En Preister hadd enmal en Pird,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "En Preister hadd enmal en Pird,", "tokens": ["En", "Preis\u00b7ter", "hadd", "en\u00b7mal", "en", "Pird", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "En sch\u00f6nen, brunen Bl\u00e4\u00dften wir't!", "tokens": ["En", "sch\u00f6\u00b7nen", ",", "bru\u00b7nen", "Bl\u00e4\u00df\u00b7ten", "wir't", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch stunn hei nich mihr up sin Pal", "tokens": ["Doch", "stunn", "hei", "nich", "mihr", "up", "sin", "Pal"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "FM", "FM", "FM", "FM", "FM", "FM", "NN"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Un hadd en beten Spat un wat von Schal.", "tokens": ["Un", "hadd", "en", "be\u00b7ten", "Spat", "un", "wat", "von", "Schal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "FM", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dat hadd em just nu noch so vel nich dahn.", "tokens": ["Dat", "hadd", "em", "just", "nu", "noch", "so", "vel", "nich", "dahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "ADV", "ADV", "ADV", "ADJD", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Doch wat de Hauptsak was bi't olle Dirt,", "tokens": ["Doch", "wat", "de", "Haupt\u00b7sak", "was", "bi't", "ol\u00b7le", "Dirt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Hei was tau vel tau Faut all gahn.", "tokens": ["Hei", "was", "tau", "vel", "tau", "Faut", "all", "gahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "PIAT", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "\u00bbv\u00f6r'n Schinner is hei noch tau gaud\u00ab,", "tokens": ["\u00bb", "v\u00f6r'n", "Schin\u00b7ner", "is", "hei", "noch", "tau", "gaud", "\u00ab", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJA", "NN", "FM", "FM", "FM", "FM", "FM", "$(", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Seggt tau den Preister Jehann Haut,", "tokens": ["Seggt", "tau", "den", "Preis\u00b7ter", "Je\u00b7hann", "Haut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "\u00bbwi will'n em irst noch d\u00fcchtig ruter b\u00e4kern", "tokens": ["\u00bb", "wi", "will'n", "em", "irst", "noch", "d\u00fcch\u00b7tig", "ru\u00b7ter", "b\u00e4\u00b7kern"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "ADV", "ADV", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Un up en Mark em denn verh\u00e4kern.\u00ab", "tokens": ["Un", "up", "en", "Mark", "em", "denn", "ver\u00b7h\u00e4\u00b7kern", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "KON", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Un as nu Hans is fett un glatt,", "tokens": ["Un", "as", "nu", "Hans", "is", "fett", "un", "glatt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.13": {"text": "Dunn f\u00fchrt de Preister nah de Stadt,", "tokens": ["Dunn", "f\u00fchrt", "de", "Preis\u00b7ter", "nah", "de", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "NE", "ADJD", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Un durt nich lang', dunn b\u00fctt en Handelsmann", "tokens": ["Un", "durt", "nich", "lang'", ",", "dunn", "b\u00fctt", "en", "Han\u00b7dels\u00b7mann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKNEG", "ADV", "$,", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.15": {"text": "Vir Luggedur f\u00f6r Preister-Han\u00dfen an", "tokens": ["Vir", "Lug\u00b7ge\u00b7dur", "f\u00f6r", "Preis\u00b7ter\u00b7Han\u00b7\u00dfen", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Un einen Daler Halftergeld.", "tokens": ["Un", "ei\u00b7nen", "Da\u00b7ler", "Half\u00b7ter\u00b7geld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Na, endlich k\u00fcmmt de Handel t'recht.", "tokens": ["Na", ",", "end\u00b7lich", "k\u00fcmmt", "de", "Han\u00b7del", "t'\u00b7recht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "NE", "NE", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.18": {"text": "De Preister geiht nu r\u00fcmm un s\u00f6cht un s\u00f6cht", "tokens": ["De", "Preis\u00b7ter", "geiht", "nu", "r\u00fcmm", "un", "s\u00f6cht", "un", "s\u00f6cht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "ADV", "FM", "FM", "FM", "FM", "VVFIN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "En anner Pird, wat beter em gef\u00f6llt.", "tokens": ["En", "an\u00b7ner", "Pird", ",", "wat", "be\u00b7ter", "em", "ge\u00b7f\u00f6llt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PRELS", "ADJD", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Hei s\u00f6cht un s\u00f6cht! De ein is em tau l\u00fctt,", "tokens": ["Hei", "s\u00f6cht", "un", "s\u00f6cht", "!", "De", "ein", "is", "em", "tau", "l\u00fctt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "VVFIN", "$.", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.21": {"text": "Un den'n sin Farw is em nich mit,", "tokens": ["Un", "den'n", "sin", "Farw", "is", "em", "nich", "mit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "De hett dat Spat, un de hett Gallen,", "tokens": ["De", "hett", "dat", "Spat", ",", "un", "de", "hett", "Gal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "FM", "FM", "VAFIN", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.23": {"text": "De anner kann em ok nich recht gefallen,", "tokens": ["De", "an\u00b7ner", "kann", "em", "ok", "nich", "recht", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VMFIN", "ADJA", "NN", "PTKNEG", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "De Brun, de ward en Kr\u00fcbbensetter sin,", "tokens": ["De", "Brun", ",", "de", "ward", "en", "Kr\u00fcb\u00b7ben\u00b7set\u00b7ter", "sin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NE", "VAFIN", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "De Vo\u00df is d\u00e4mlich ganz gewi\u00df,", "tokens": ["De", "Vo\u00df", "is", "d\u00e4m\u00b7lich", "ganz", "ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "FM", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "De Swart is in de Flanken em tau d\u00fcnn,", "tokens": ["De", "Swart", "is", "in", "de", "Flan\u00b7ken", "em", "tau", "d\u00fcnn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+--++", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Un de un de, de hewwen keinen Bli\u00df,", "tokens": ["Un", "de", "un", "de", ",", "de", "hew\u00b7wen", "kei\u00b7nen", "Bli\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,", "NE", "NE", "PIAT", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.28": {"text": "Un einen Bl\u00e4\u00dften s\u00fcll dat doch nu sin;", "tokens": ["Un", "ei\u00b7nen", "Bl\u00e4\u00df\u00b7ten", "s\u00fcll", "dat", "doch", "nu", "sin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "NE", "VVFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Denn wenn hei 'n beten statsch wull f\u00fchren,", "tokens": ["Denn", "wenn", "hei", "'n", "be\u00b7ten", "statsch", "wull", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NE", "NE", "VVFIN", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.30": {"text": "Denn m\u00fc\u00dft hei mit den annern doch cal\u00fcren.", "tokens": ["Denn", "m\u00fc\u00dft", "hei", "mit", "den", "an\u00b7nern", "doch", "ca\u00b7l\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NE", "APPR", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.31": {"text": "Na, mit de Wil dunn w\u00fcrd dat d\u00fcstre Nacht,", "tokens": ["Na", ",", "mit", "de", "Wil", "dunn", "w\u00fcrd", "dat", "d\u00fcst\u00b7re", "Nacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "NE", "NE", "NE", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Taum Handel was't binah tau sp\u00e4d,", "tokens": ["Taum", "Han\u00b7del", "was't", "bi\u00b7nah", "tau", "sp\u00e4d", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Un as de Preister dr\u00e4wer nah hett dacht,", "tokens": ["Un", "as", "de", "Preis\u00b7ter", "dr\u00e4\u00b7wer", "nah", "hett", "dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NE", "ADJD", "ADJD", "VAFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Wo hei 'ne M\u00e4hr kreg, kamm en Jud un s\u00e4d:", "tokens": ["Wo", "hei", "'ne", "M\u00e4hr", "kreg", ",", "kamm", "en", "Jud", "un", "s\u00e4d", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "VVFIN", "$,", "VVFIN", "FM", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "\u00bbwi\u00df un wohrhaftig, straf mi Gott, Herr Paster!", "tokens": ["\u00bb", "wi\u00df", "un", "wohr\u00b7haf\u00b7tig", ",", "straf", "mi", "Gott", ",", "Herr", "Pas\u00b7ter", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "FM", "FM", "ADJD", "$,", "VVFIN", "NE", "NN", "$,", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Ick heww en Wallach, dat's en Pird;", "tokens": ["Ick", "heww", "en", "Wal\u00b7lach", ",", "dat's", "en", "Pird", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "De's unner Br\u00e4uder dusend wirt,", "tokens": ["De's", "un\u00b7ner", "Br\u00e4u\u00b7der", "du\u00b7send", "wirt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Den kepen Sei, un pa\u00dft er", "tokens": ["Den", "ke\u00b7pen", "Sei", ",", "un", "pa\u00dft", "er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$,", "FM", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.39": {"text": "Sei nich, denn will ick in de Ird hir sinken,", "tokens": ["Sei", "nich", ",", "denn", "will", "ick", "in", "de", "Ird", "hir", "sin\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "$,", "KON", "VMFIN", "PPER", "APPR", "NE", "NE", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Denn will'ck verswarzen ganz un gor,", "tokens": ["Denn", "will'ck", "vers\u00b7war\u00b7zen", "ganz", "un", "gor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "FM", "FM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Denn fret'ck em up mit Hut un Hor", "tokens": ["Denn", "fret'ck", "em", "up", "mit", "Hut", "un", "Hor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Un will en ganzen Sod dortau utdrinken.", "tokens": ["Un", "will", "en", "gan\u00b7zen", "Sod", "dor\u00b7tau", "ut\u00b7drin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "FM", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Dat is en Wallach, segg ick, ut en grot Gest\u00fct!", "tokens": ["Dat", "is", "en", "Wal\u00b7lach", ",", "segg", "ick", ",", "ut", "en", "grot", "Ge\u00b7st\u00fct", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "NN", "$,", "VVFIN", "PPER", "$,", "FM", "FM", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Hei is von vornehmes Gebl\u00fct", "tokens": ["Hei", "is", "von", "vor\u00b7neh\u00b7mes", "Ge\u00b7bl\u00fct"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM", "FM", "APPR", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.45": {"text": "Un hett enmal den Grafen Hahnen h\u00fcrt,", "tokens": ["Un", "hett", "en\u00b7mal", "den", "Gra\u00b7fen", "Hah\u00b7nen", "h\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "Un hett vir extrawei\u00dfe Hinterf\u00fc\u00df'", "tokens": ["Un", "hett", "vir", "ext\u00b7ra\u00b7wei\u00b7\u00dfe", "Hin\u00b7ter\u00b7f\u00fc\u00df'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.47": {"text": "Un an den Kopp en schenen Bli\u00df.", "tokens": ["Un", "an", "den", "Kopp", "en", "sche\u00b7nen", "Bli\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Ick segg Se, ne! dat is en Pird,", "tokens": ["Ick", "segg", "Se", ",", "ne", "!", "dat", "is", "en", "Pird", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE"