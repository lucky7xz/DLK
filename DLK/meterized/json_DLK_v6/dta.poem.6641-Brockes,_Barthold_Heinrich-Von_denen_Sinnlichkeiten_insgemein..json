{"dta.poem.6641": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von denen Sinnlichkeiten  \n insgemein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von einem schimpflichen und groben Vorurtheil", "tokens": ["Von", "ei\u00b7nem", "schimpf\u00b7li\u00b7chen", "und", "gro\u00b7ben", "Vor\u00b7urt\u00b7heil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sind wir best\u00e4ndig fortgerissen.", "tokens": ["Sind", "wir", "be\u00b7st\u00e4n\u00b7dig", "fort\u00b7ge\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von Dingen deren Druck wir stets empfinden m\u00fcssen;", "tokens": ["Von", "Din\u00b7gen", "de\u00b7ren", "Druck", "wir", "stets", "emp\u00b7fin\u00b7den", "m\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRELAT", "NN", "PPER", "ADV", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wird die Materie nur blo\u00df von uns erblickt", "tokens": ["Wird", "die", "Ma\u00b7te\u00b7rie", "nur", "blo\u00df", "von", "uns", "er\u00b7blickt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ADV", "ADV", "APPR", "PPER", "VVPP"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "Die uns auf allen Seiten dr\u00fcckt.", "tokens": ["Die", "uns", "auf", "al\u00b7len", "Sei\u00b7ten", "dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}