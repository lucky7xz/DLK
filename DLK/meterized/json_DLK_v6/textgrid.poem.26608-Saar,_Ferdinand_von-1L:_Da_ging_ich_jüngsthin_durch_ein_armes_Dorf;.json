{"textgrid.poem.26608": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da ging ich j\u00fcngsthin durch ein armes Dorf;", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da ging ich j\u00fcngsthin durch ein armes Dorf;", "tokens": ["Da", "ging", "ich", "j\u00fcng\u00b7sthin", "durch", "ein", "ar\u00b7mes", "Dorf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verfall'ne H\u00fctten, breitgetret'ne Gossen,", "tokens": ["Ver\u00b7fall'\u00b7ne", "H\u00fct\u00b7ten", ",", "breit\u00b7ge\u00b7tret'\u00b7ne", "Gos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch welche, sp\u00fclend an der Erde Schorf,", "tokens": ["Durch", "wel\u00b7che", ",", "sp\u00fc\u00b7lend", "an", "der", "Er\u00b7de", "Schorf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "$,", "ADJD", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im Sonnenbrande seichte Wasser flossen.", "tokens": ["Im", "Son\u00b7nen\u00b7bran\u00b7de", "seich\u00b7te", "Was\u00b7ser", "flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und in dem einen sah ich hochgesch\u00fcrzt,", "tokens": ["Und", "in", "dem", "ei\u00b7nen", "sah", "ich", "hoch\u00b7ge\u00b7sch\u00fcrzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "PIS", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den festgebund'nen S\u00e4ugling auf dem R\u00fccken,", "tokens": ["Den", "fest\u00b7ge\u00b7bun\u00b7d'\u00b7nen", "S\u00e4ug\u00b7ling", "auf", "dem", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ein junges Weib stehn, malerisch verk\u00fcrzt", "tokens": ["Ein", "jun\u00b7ges", "Weib", "stehn", ",", "ma\u00b7le\u00b7risch", "ver\u00b7k\u00fcrzt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,", "ADJD", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den kr\u00e4ft'gen Leib bei tiefem Niederb\u00fccken.", "tokens": ["Den", "kr\u00e4ft'\u00b7gen", "Leib", "bei", "tie\u00b7fem", "Nie\u00b7der\u00b7b\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein Pf\u00e4nnlein wusch sie in der tr\u00fcben Fluth,", "tokens": ["Ein", "Pf\u00e4nn\u00b7lein", "wusch", "sie", "in", "der", "tr\u00fc\u00b7ben", "Fluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Pf\u00e4nnlein, d'rin sie erst gekocht das Essen,", "tokens": ["Ein", "Pf\u00e4nn\u00b7lein", ",", "d'\u00b7rin", "sie", "erst", "ge\u00b7kocht", "das", "Es\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "ADV", "VVPP", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Und sang ein Lied dabei mit frischem Muth \u2013", "tokens": ["Und", "sang", "ein", "Lied", "da\u00b7bei", "mit", "fri\u00b7schem", "Muth", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PAV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Kindchen aber schlummerte indessen.", "tokens": ["Das", "Kind\u00b7chen", "a\u00b7ber", "schlum\u00b7mer\u00b7te", "in\u00b7des\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Bei meinem Nahen hob sie rasch das Haupt", "tokens": ["Bei", "mei\u00b7nem", "Na\u00b7hen", "hob", "sie", "rasch", "das", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und sah mich an mit Augen, hellen, braunen,", "tokens": ["Und", "sah", "mich", "an", "mit", "Au\u00b7gen", ",", "hel\u00b7len", ",", "brau\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "APPR", "NN", "$,", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als wollt' sie fragen: \u00bbHerr, wenn ihr erlaubt,", "tokens": ["Als", "wollt'", "sie", "fra\u00b7gen", ":", "\u00bb", "Herr", ",", "wenn", "ihr", "er\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "VVINF", "$.", "$(", "NN", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was habt ihr denn so gro\u00df mich anzustaunen?\u00ab", "tokens": ["Was", "habt", "ihr", "denn", "so", "gro\u00df", "mich", "an\u00b7zu\u00b7stau\u00b7nen", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "PPER", "VVIZU", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "\u00bbich bin ein Weib \u2013 darum auch Magd und Amme,", "tokens": ["\u00bb", "ich", "bin", "ein", "Weib", "\u2013", "da\u00b7rum", "auch", "Magd", "und", "Am\u00b7me", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "$(", "PAV", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie St\u00e4dterfrau'n kann ich mich nicht erh\u00f6h'n;", "tokens": ["Wie", "St\u00e4d\u00b7ter\u00b7frau'n", "kann", "ich", "mich", "nicht", "er\u00b7h\u00f6h'n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein Mann ist arm, wir sind von nied'rem Stamme,", "tokens": ["Mein", "Mann", "ist", "arm", ",", "wir", "sind", "von", "nie\u00b7d'\u00b7rem", "Stam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPER", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Doch bin ich froh \u2013 und, wie ihr seht, auch sch\u00f6n!\u00ab", "tokens": ["Doch", "bin", "ich", "froh", "\u2013", "und", ",", "wie", "ihr", "seht", ",", "auch", "sch\u00f6n", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "$(", "KON", "$,", "PWAV", "PPER", "VVFIN", "$,", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Sie schlug in's Wasser, da\u00df es sie umsprang;", "tokens": ["Sie", "schlug", "in's", "Was\u00b7ser", ",", "da\u00df", "es", "sie", "um\u00b7sprang", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Behend dann l\u00f6send ihres Haares Str\u00e4hne,", "tokens": ["Be\u00b7hend", "dann", "l\u00f6\u00b7send", "ih\u00b7res", "Haa\u00b7res", "Str\u00e4h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADJD", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wusch sie ihr Antlitz, lachte, da\u00df es klang,", "tokens": ["Wusch", "sie", "ihr", "Ant\u00b7litz", ",", "lach\u00b7te", ",", "da\u00df", "es", "klang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$,", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und wies dabei mir ihre wei\u00dfen Z\u00e4hne.", "tokens": ["Und", "wies", "da\u00b7bei", "mir", "ih\u00b7re", "wei\u00b7\u00dfen", "Z\u00e4h\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ich aber ging, zu j\u00e4hem Schmerz erregt,", "tokens": ["Ich", "a\u00b7ber", "ging", ",", "zu", "j\u00e4\u00b7hem", "Schmerz", "er\u00b7regt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So wie ber\u00fchrt an einer off'nen Wunde,", "tokens": ["So", "wie", "be\u00b7r\u00fchrt", "an", "ei\u00b7ner", "off'\u00b7nen", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die heute noch gar Mancher schweigend tr\u00e4gt", "tokens": ["Die", "heu\u00b7te", "noch", "gar", "Man\u00b7cher", "schwei\u00b7gend", "tr\u00e4gt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "ADV", "PIS", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In seines Herzens tiefgeheimstem Grunde.", "tokens": ["In", "sei\u00b7nes", "Her\u00b7zens", "tief\u00b7ge\u00b7heims\u00b7tem", "Grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}