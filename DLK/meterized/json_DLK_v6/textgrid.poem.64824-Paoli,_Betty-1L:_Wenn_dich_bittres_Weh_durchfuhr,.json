{"textgrid.poem.64824": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn dich bittres Weh durchfuhr,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn dich bittres Weh durchfuhr,", "tokens": ["Wenn", "dich", "bitt\u00b7res", "Weh", "durch\u00b7fuhr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Trachte dann, eh' dich's bezwungen,", "tokens": ["Trach\u00b7te", "dann", ",", "eh'", "dich's", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "PIS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu verfolgen seine Spur", "tokens": ["Zu", "ver\u00b7fol\u00b7gen", "sei\u00b7ne", "Spur"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis zum Quell, dem es entsprungen.", "tokens": ["Bis", "zum", "Quell", ",", "dem", "es", "ent\u00b7sprun\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Findest du dann, da\u00df der Gram,", "tokens": ["Fin\u00b7dest", "du", "dann", ",", "da\u00df", "der", "Gram", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KOUS", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00f6rend deiner N\u00e4chte Schlummer,", "tokens": ["St\u00f6\u00b7rend", "dei\u00b7ner", "N\u00e4ch\u00b7te", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Von dem Schicksal zu Dir kam,", "tokens": ["Von", "dem", "Schick\u00b7sal", "zu", "Dir", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "So bezwinge deinen Kummer.", "tokens": ["So", "be\u00b7zwin\u00b7ge", "dei\u00b7nen", "Kum\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Denkend, da\u00df des Schicksals Witz", "tokens": ["Den\u00b7kend", ",", "da\u00df", "des", "Schick\u00b7sals", "Witz"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "KOUS", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Neu will sein an jedem Morgen", "tokens": ["Neu", "will", "sein", "an", "je\u00b7dem", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VMFIN", "PPOSAT", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und da\u00df drum ein gleicher Blitz", "tokens": ["Und", "da\u00df", "drum", "ein", "glei\u00b7cher", "Blitz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00fcnftig nicht mehr zu besorgen.", "tokens": ["K\u00fcnf\u00b7tig", "nicht", "mehr", "zu", "be\u00b7sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKNEG", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wohl verschieden ist der Fall,", "tokens": ["Wohl", "ver\u00b7schie\u00b7den", "ist", "der", "Fall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch nicht gr\u00f6\u00dfer sei die Beugni\u00df,", "tokens": ["Doch", "nicht", "gr\u00f6\u00b7\u00dfer", "sei", "die", "Beug\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nennt dich Ursach Deiner Qual", "tokens": ["Nennt", "dich", "Ur\u00b7sach", "Dei\u00b7ner", "Qual"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deines Geist's wahrhaft'ges Zeugni\u00df.", "tokens": ["Dei\u00b7nes", "Geist's", "wahr\u00b7haft'\u00b7ges", "Zeug\u00b7ni\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Suche dann ohn' Ruh' und Rast", "tokens": ["Su\u00b7che", "dann", "ohn'", "Ruh'", "und", "Rast"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Deinen Fehler zu entdecken;", "tokens": ["Dei\u00b7nen", "Feh\u00b7ler", "zu", "ent\u00b7de\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn du ihn gefunden hast,", "tokens": ["Wenn", "du", "ihn", "ge\u00b7fun\u00b7den", "hast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wirf hinaus den dunkeln Flecken!", "tokens": ["Wirf", "hin\u00b7aus", "den", "dun\u00b7keln", "Fle\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APZR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "K\u00e4mpfe, bis, was dich beth\u00f6rt,", "tokens": ["K\u00e4mp\u00b7fe", ",", "bis", ",", "was", "dich", "be\u00b7th\u00f6rt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Du besiegt und \u00fcberwunden.", "tokens": ["Du", "be\u00b7siegt", "und", "\u00fc\u00b7berw\u00b7un\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "KON", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist sein b\u00f6ser Keim zerst\u00f6rt,", "tokens": ["Ist", "sein", "b\u00f6\u00b7ser", "Keim", "zer\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist das Ungl\u00fcck bald verschwunden.", "tokens": ["Ist", "das", "Un\u00b7gl\u00fcck", "bald", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "So kannst du in jeder Art,", "tokens": ["So", "kannst", "du", "in", "je\u00b7der", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Hoffend glauben, da\u00df das Leiden,", "tokens": ["Hof\u00b7fend", "glau\u00b7ben", ",", "da\u00df", "das", "Lei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "KOUS", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tr\u00fcbend Deine Gegenwart,", "tokens": ["Tr\u00fc\u00b7bend", "Dei\u00b7ne", "Ge\u00b7gen\u00b7wart", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine Zukunft werde meiden.", "tokens": ["Dei\u00b7ne", "Zu\u00b7kunft", "wer\u00b7de", "mei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}