{"dta.poem.12704": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eines Studentens.  \n  D.  V. A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein allzufr\u00fcher tod verk\u00fcrtzt sein junges leben,", "tokens": ["Ein", "all\u00b7zu\u00b7fr\u00fc\u00b7her", "tod", "ver\u00b7k\u00fcrtzt", "sein", "jun\u00b7ges", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und raubt uns unsre lust an seiner fr\u00f6mmigkeit,", "tokens": ["Und", "raubt", "uns", "uns\u00b7re", "lust", "an", "sei\u00b7ner", "fr\u00f6m\u00b7mig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "An sanfftmuth, kunst und flei\u00df. GOtt aber trifft die zeit", "tokens": ["An", "sanfft\u00b7muth", ",", "kunst", "und", "flei\u00df", ".", "Gott", "a\u00b7ber", "trifft", "die", "zeit"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "$,", "ADV", "KON", "NN", "$.", "NN", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und nimmt auf ewig hin, was er so kurtz gegeben.", "tokens": ["Und", "nimmt", "auf", "e\u00b7wig", "hin", ",", "was", "er", "so", "kurtz", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR"