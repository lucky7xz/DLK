{"textgrid.poem.24783": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "Ein Traum von gro\u00dfer Magie", "genre": "verse", "period": "N.A.", "pub_year": 1901, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Viel k\u00f6niglicher als ein Perlenband", "tokens": ["Viel", "k\u00f6\u00b7nig\u00b7li\u00b7cher", "als", "ein", "Per\u00b7len\u00b7band"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und k\u00fchn wie junges Meer im Morgenduft,", "tokens": ["Und", "k\u00fchn", "wie", "jun\u00b7ges", "Meer", "im", "Mor\u00b7gen\u00b7duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So war ein gro\u00dfer Traum \u2013 wie ich ihn fand.", "tokens": ["So", "war", "ein", "gro\u00b7\u00dfer", "Traum", "\u2013", "wie", "ich", "ihn", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$(", "PWAV", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Durch offene Glast\u00fcren ging die Luft.", "tokens": ["Durch", "of\u00b7fe\u00b7ne", "Glas\u00b7t\u00fc\u00b7ren", "ging", "die", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich schlief im Pavillon zu ebner Erde,", "tokens": ["Ich", "schlief", "im", "Pa\u00b7vil\u00b7lon", "zu", "eb\u00b7ner", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und durch vier offne T\u00fcren ging die Luft \u2013", "tokens": ["Und", "durch", "vier", "off\u00b7ne", "T\u00fc\u00b7ren", "ging", "die", "Luft", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "CARD", "ADJA", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-++--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Und fr\u00fcher liefen schon geschirrte Pferde", "tokens": ["Und", "fr\u00fc\u00b7her", "lie\u00b7fen", "schon", "ge\u00b7schirr\u00b7te", "Pfer\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hindurch und Hunde eine ganze Schar", "tokens": ["Hin\u00b7durch", "und", "Hun\u00b7de", "ei\u00b7ne", "gan\u00b7ze", "Schar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "KON", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "An meinem Bett vorbei. Doch die Geb\u00e4rde", "tokens": ["An", "mei\u00b7nem", "Bett", "vor\u00b7bei", ".", "Doch", "die", "Ge\u00b7b\u00e4r\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$.", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Des Magiers \u2013 des Ersten, Gro\u00dfen \u2013 war", "tokens": ["Des", "Ma\u00b7giers", "\u2013", "des", "Ers\u00b7ten", ",", "Gro\u00b7\u00dfen", "\u2013", "war"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "$(", "ART", "NN", "$,", "NN", "$(", "VAFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Auf einmal zwischen mir und einer Wand:", "tokens": ["Auf", "ein\u00b7mal", "zwi\u00b7schen", "mir", "und", "ei\u00b7ner", "Wand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "PPER", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sein stolzes Nicken, k\u00f6nigliches Haar.", "tokens": ["Sein", "stol\u00b7zes", "Ni\u00b7cken", ",", "k\u00f6\u00b7nig\u00b7li\u00b7ches", "Haar", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und hinter ihm nicht Mauer: es entstand", "tokens": ["Und", "hin\u00b7ter", "ihm", "nicht", "Mau\u00b7er", ":", "es", "ent\u00b7stand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "PPER", "PTKNEG", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein weiter Prunk von Abgrund, dunklem Meer", "tokens": ["Ein", "wei\u00b7ter", "Prunk", "von", "Ab\u00b7grund", ",", "dunk\u00b7lem", "Meer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und gr\u00fcnen Matten hinter seiner Hand.", "tokens": ["Und", "gr\u00fc\u00b7nen", "Mat\u00b7ten", "hin\u00b7ter", "sei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Er b\u00fcckte sich und zog das Tiefe her.", "tokens": ["Er", "b\u00fcck\u00b7te", "sich", "und", "zog", "das", "Tie\u00b7fe", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er b\u00fcckte sich, und seine Finger gingen", "tokens": ["Er", "b\u00fcck\u00b7te", "sich", ",", "und", "sei\u00b7ne", "Fin\u00b7ger", "gin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "$,", "KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Boden so, als ob es Wasser w\u00e4r.", "tokens": ["Im", "Bo\u00b7den", "so", ",", "als", "ob", "es", "Was\u00b7ser", "w\u00e4r", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,", "KOKOM", "KOUS", "PPER", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Vom d\u00fcnnen Quellenwasser aber fingen", "tokens": ["Vom", "d\u00fcn\u00b7nen", "Quel\u00b7len\u00b7was\u00b7ser", "a\u00b7ber", "fin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich riesige Opale in den H\u00e4nden", "tokens": ["Sich", "rie\u00b7si\u00b7ge", "O\u00b7pa\u00b7le", "in", "den", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und fielen t\u00f6nend wieder ab in Ringen.", "tokens": ["Und", "fie\u00b7len", "t\u00f6\u00b7nend", "wie\u00b7der", "ab", "in", "Rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "PTKVZ", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Dann warf er sich mit leichtem Schwang der Lenden \u2013", "tokens": ["Dann", "warf", "er", "sich", "mit", "leich\u00b7tem", "Schwang", "der", "Len\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie nur aus Stolz \u2013 der n\u00e4chsten Klippe zu;", "tokens": ["Wie", "nur", "aus", "Stolz", "\u2013", "der", "n\u00e4chs\u00b7ten", "Klip\u00b7pe", "zu", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "NN", "$(", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "An ihm sah ich die Macht der Schwere enden.", "tokens": ["An", "ihm", "sah", "ich", "die", "Macht", "der", "Schwe\u00b7re", "en\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.9": {"line.1": {"text": "In seinen Augen aber war die Ruh", "tokens": ["In", "sei\u00b7nen", "Au\u00b7gen", "a\u00b7ber", "war", "die", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von schlafend- doch lebendgen Edelsteinen.", "tokens": ["Von", "schla\u00b7fen\u00b7d", "doch", "le\u00b7bend\u00b7gen", "E\u00b7del\u00b7stei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Er setzte sich und sprach ein solches Du", "tokens": ["Er", "setz\u00b7te", "sich", "und", "sprach", "ein", "sol\u00b7ches", "Du"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "KON", "VVFIN", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Zu Tagen, die uns ganz vergangen scheinen,", "tokens": ["Zu", "Ta\u00b7gen", ",", "die", "uns", "ganz", "ver\u00b7gan\u00b7gen", "schei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df sie herkamen trauervoll und gro\u00df:", "tokens": ["Da\u00df", "sie", "her\u00b7ka\u00b7men", "trau\u00b7er\u00b7voll", "und", "gro\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das freute ihn zu lachen und zu weinen.", "tokens": ["Das", "freu\u00b7te", "ihn", "zu", "la\u00b7chen", "und", "zu", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Er f\u00fchlte traumhaft aller Menschen Los,", "tokens": ["Er", "f\u00fchl\u00b7te", "traum\u00b7haft", "al\u00b7ler", "Men\u00b7schen", "Los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PIAT", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So wie er seine eignen Glieder f\u00fchlte.", "tokens": ["So", "wie", "er", "sei\u00b7ne", "eig\u00b7nen", "Glie\u00b7der", "f\u00fchl\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihm war nichts nah und fern, nichts klein und gro\u00df.", "tokens": ["Ihm", "war", "nichts", "nah", "und", "fern", ",", "nichts", "klein", "und", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "ADJD", "KON", "ADJD", "$,", "PIS", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Und wie tief unten sich die Erde k\u00fchlte,", "tokens": ["Und", "wie", "tief", "un\u00b7ten", "sich", "die", "Er\u00b7de", "k\u00fchl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "ADV", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Dunkel aus den Tiefen aufw\u00e4rts drang,", "tokens": ["Das", "Dun\u00b7kel", "aus", "den", "Tie\u00b7fen", "auf\u00b7w\u00e4rts", "drang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Nacht das Laue aus den Wipfeln w\u00fchlte,", "tokens": ["Die", "Nacht", "das", "Lau\u00b7e", "aus", "den", "Wip\u00b7feln", "w\u00fchl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Geno\u00df er allen Lebens gro\u00dfen Gang", "tokens": ["Ge\u00b7no\u00df", "er", "al\u00b7len", "Le\u00b7bens", "gro\u00b7\u00dfen", "Gang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So sehr \u2013 da\u00df er in gro\u00dfer Trunkenheit", "tokens": ["So", "sehr", "\u2013", "da\u00df", "er", "in", "gro\u00b7\u00dfer", "Trun\u00b7ken\u00b7heit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$(", "KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So wie ein L\u00f6we \u00fcber Klippen sprang.", "tokens": ["So", "wie", "ein", "L\u00f6\u00b7we", "\u00fc\u00b7ber", "Klip\u00b7pen", "sprang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": ". . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . .", "tokens": [".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", "."], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$."]}}, "stanza.15": {"line.1": {"text": "Cherub und hoher Herr ist unser Geist \u2013", "tokens": ["Che\u00b7rub", "und", "ho\u00b7her", "Herr", "ist", "un\u00b7ser", "Geist", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wohnt nicht in uns, und in die obern Sterne", "tokens": ["Wohnt", "nicht", "in", "uns", ",", "und", "in", "die", "o\u00b7bern", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "APPR", "PPER", "$,", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Setzt er den Stuhl und l\u00e4\u00dft uns viel verwaist:", "tokens": ["Setzt", "er", "den", "Stuhl", "und", "l\u00e4\u00dft", "uns", "viel", "ver\u00b7waist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "KON", "VVFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.16": {"line.1": {"text": "Doch Er ist Feuer uns im tiefsten Kerne", "tokens": ["Doch", "Er", "ist", "Feu\u00b7er", "uns", "im", "tiefs\u00b7ten", "Ker\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "NN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u2013 So ahnte mir, da ich den Traum da fand \u2013", "tokens": ["\u2013", "So", "ahn\u00b7te", "mir", ",", "da", "ich", "den", "Traum", "da", "fand", "\u2013"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und redet mit den Feuern jener Ferne", "tokens": ["Und", "re\u00b7det", "mit", "den", "Feu\u00b7ern", "je\u00b7ner", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.17": {"line.1": {"text": "Und lebt in mir wie ich in meiner Hand.", "tokens": ["Und", "lebt", "in", "mir", "wie", "ich", "in", "mei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "KOKOM", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}