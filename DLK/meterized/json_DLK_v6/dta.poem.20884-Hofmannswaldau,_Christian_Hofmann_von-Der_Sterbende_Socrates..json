{"dta.poem.20884": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Wann uns Gem\u00fcth und Hertz in Rach und Eifer glimmen/", "tokens": ["Wann", "uns", "Ge\u00b7m\u00fcth", "und", "Hertz", "in", "Rach", "und", "Ei\u00b7fer", "glim\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "KON", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann des Verliebten Geist auf einen Korb gedenckt/", "tokens": ["Wann", "des", "Ver\u00b7lieb\u00b7ten", "Geist", "auf", "ei\u00b7nen", "Korb", "ge\u00b7denckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wann der Schiffer soll auf einen Balcken schwimmen/", "tokens": ["Und", "wann", "der", "Schif\u00b7fer", "soll", "auf", "ei\u00b7nen", "Bal\u00b7cken", "schwim\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wird die Seele nicht auf gleicher Bahn gelenckt.", "tokens": ["So", "wird", "die", "See\u00b7le", "nicht", "auf", "glei\u00b7cher", "Bahn", "ge\u00b7lenckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKNEG", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es ist ein Uberflu\u00df/ wann man die Ege treibet/", "tokens": ["Es", "ist", "ein", "U\u00b7ber\u00b7flu\u00df", "/", "wann", "man", "die", "E\u00b7ge", "trei\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "PWAV", "PIS", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df man bered und wol zu Pferd und Ochsen spricht/", "tokens": ["Da\u00df", "man", "be\u00b7red", "und", "wol", "zu", "Pferd", "und", "Och\u00b7sen", "spricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "KON", "ADV", "APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und da\u00df ein Kriegesmann/ wann er die Schlacht beschreibet/", "tokens": ["Und", "da\u00df", "ein", "Krie\u00b7ges\u00b7mann", "/", "wann", "er", "die", "Schlacht", "be\u00b7schrei\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "$(", "PWAV", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nach Redners Ma\u00df und Art ihm Wort und Sylbe richt.", "tokens": ["Nach", "Red\u00b7ners", "Ma\u00df", "und", "Art", "ihm", "Wort", "und", "Syl\u00b7be", "richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "KON", "NN", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Auf Cytherons Begrieff/ wo Bachus Weiber lachen/", "tokens": ["Auf", "Cy\u00b7the\u00b7rons", "Be\u00b7grieff", "/", "wo", "Ba\u00b7chus", "Wei\u00b7ber", "la\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$(", "PWAV", "NE", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann ihr ber\u00fchmter Gott die nasse Taffel deckt/", "tokens": ["Wann", "ihr", "be\u00b7r\u00fchm\u00b7ter", "Gott", "die", "nas\u00b7se", "Taf\u00b7fel", "deckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da h\u00f6rt kein Ohre nicht viel kluge W\u00f6rter machen/", "tokens": ["Da", "h\u00f6rt", "kein", "Oh\u00b7re", "nicht", "viel", "klu\u00b7ge", "W\u00f6r\u00b7ter", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "PTKNEG", "ADV", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Indem ein voller Galm uns das Geh\u00f6re schreckt.", "tokens": ["In\u00b7dem", "ein", "vol\u00b7ler", "Galm", "uns", "das", "Ge\u00b7h\u00f6\u00b7re", "schreckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der aber/ dessen Geist nicht aus der Wage lencket/", "tokens": ["Der", "a\u00b7ber", "/", "des\u00b7sen", "Geist", "nicht", "aus", "der", "Wa\u00b7ge", "len\u00b7cket", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$(", "PRELAT", "NN", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem keine Regung hat die s\u00fcsse Ruh verst\u00f6rt/", "tokens": ["Dem", "kei\u00b7ne", "Re\u00b7gung", "hat", "die", "s\u00fcs\u00b7se", "Ruh", "ver\u00b7st\u00f6rt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der trachte/ wil er nicht/ da\u00df man was falsches dencket/", "tokens": ["Der", "trach\u00b7te", "/", "wil", "er", "nicht", "/", "da\u00df", "man", "was", "fal\u00b7sches", "den\u00b7cket", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$(", "VMFIN", "PPER", "PTKNEG", "$(", "KOUS", "PIS", "PWS", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Damit kein einig Mensch was ungereimtes h\u00f6rt.", "tokens": ["Da\u00b7mit", "kein", "ei\u00b7nig", "Mensch", "was", "un\u00b7ge\u00b7reim\u00b7tes", "h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "ADJD", "NN", "PWS", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}