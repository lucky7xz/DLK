{"textgrid.poem.26325": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Wann liegt alle Not fern in Gedanken?", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, im H\u00fcgelland am alten Main,", "tokens": ["Ach", ",", "im", "H\u00fc\u00b7gel\u00b7land", "am", "al\u00b7ten", "Main", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPRART", "NN", "APPRART", "ADJA", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "In dem Rebenland in frohen Franken", "tokens": ["In", "dem", "Re\u00b7ben\u00b7land", "in", "fro\u00b7hen", "Fran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "M\u00f6chte ich mit beiden F\u00fc\u00dfen sein,", "tokens": ["M\u00f6ch\u00b7te", "ich", "mit", "bei\u00b7den", "F\u00fc\u00b7\u00dfen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PIAT", "NN", "VAINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Nicht nur mit den sehnenden Gedanken.", "tokens": ["Nicht", "nur", "mit", "den", "seh\u00b7nen\u00b7den", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Manches gute Lied singt man am \u00bbStein\u00ab,", "tokens": ["Man\u00b7ches", "gu\u00b7te", "Lied", "singt", "man", "am", "\u00bb", "Stein", "\u00ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PIS", "APPRART", "$(", "NN", "$(", "$,"], "meter": "+-+-++--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Manchen guten Tropfen wir dort tranken,", "tokens": ["Man\u00b7chen", "gu\u00b7ten", "Trop\u00b7fen", "wir", "dort", "tran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Warum mu\u00df das Gute fern jetzt sein?", "tokens": ["Wa\u00b7rum", "mu\u00df", "das", "Gu\u00b7te", "fern", "jetzt", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "ADJD", "ADV", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ach, die Liedertage, sie versanken.", "tokens": ["Ach", ",", "die", "Lie\u00b7der\u00b7ta\u00b7ge", ",", "sie", "ver\u00b7san\u00b7ken", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "$,", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "In den Guttenberger Wald hinein", "tokens": ["In", "den", "Gut\u00b7ten\u00b7ber\u00b7ger", "Wald", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Liegt mein Dach im ewigjungen Franken,", "tokens": ["Liegt", "mein", "Dach", "im", "e\u00b7wig\u00b7jun\u00b7gen", "Fran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Tr\u00e4te gern zur gr\u00fcnen Pforte ein,", "tokens": ["Tr\u00e4\u00b7te", "gern", "zur", "gr\u00fc\u00b7nen", "Pfor\u00b7te", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Greifend nach zwei H\u00e4nden, lieben, schlanken.", "tokens": ["Grei\u00b7fend", "nach", "zwei", "H\u00e4n\u00b7den", ",", "lie\u00b7ben", ",", "schlan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "APPR", "CARD", "NN", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Ach, sie geht im Garten dort allein,", "tokens": ["Ach", ",", "sie", "geht", "im", "Gar\u00b7ten", "dort", "al\u00b7lein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "APPRART", "NN", "ADV", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Drinnen sich Erinnerungen ranken.", "tokens": ["Drin\u00b7nen", "sich", "E\u00b7rin\u00b7ne\u00b7run\u00b7gen", "ran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wann steht wieder zwischen uns der Wein?", "tokens": ["Wann", "steht", "wie\u00b7der", "zwi\u00b7schen", "uns", "der", "Wein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wann liegt alle Not fern in Gedanken?", "tokens": ["Wann", "liegt", "al\u00b7le", "Not", "fern", "in", "Ge\u00b7dan\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIAT", "NN", "ADJD", "APPR", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}}}}