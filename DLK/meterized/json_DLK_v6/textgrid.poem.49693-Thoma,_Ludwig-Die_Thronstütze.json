{"textgrid.poem.49693": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Thronst\u00fctze", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Immer nur so durchjeschloffen,", "tokens": ["Im\u00b7mer", "nur", "so", "durch\u00b7jesc\u00b7hlof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nischt jelernt und viel jesoffen,", "tokens": ["Nischt", "je\u00b7lernt", "und", "viel", "je\u00b7sof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "KON", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Roch ich sehr nach Biere.", "tokens": ["Roch", "ich", "sehr", "nach", "Bie\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Endlich bin ich durchjeschwommen,", "tokens": ["End\u00b7lich", "bin", "ich", "durch\u00b7je\u00b7schwom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bin im Staatsdienst anjekommen", "tokens": ["Bin", "im", "Staats\u00b7dienst", "an\u00b7je\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit 'ner sauren Niere,", "tokens": ["Mit", "'ner", "sau\u00b7ren", "Nie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Hopsasa, tralala!", "tokens": ["Hop\u00b7sa\u00b7sa", ",", "tra\u00b7la\u00b7la", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "XY", "$."], "meter": "++-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Mit 'ner sauren Niere.", "tokens": ["Mit", "'ner", "sau\u00b7ren", "Nie\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Doch da peu \u00e0 peu die Kr\u00f6ten,", "tokens": ["Doch", "da", "peu", "\u00e0", "peu", "die", "Kr\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "FM", "FM", "FM", "ART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Die ich hatte, jingen fl\u00f6ten,", "tokens": ["Die", "ich", "hat\u00b7te", ",", "jin\u00b7gen", "fl\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "VAFIN", "$,", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil ich's trieb zu tolle,", "tokens": ["Weil", "ich's", "trieb", "zu", "tol\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "APPR", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Hab' ich mich nicht lang besonnen,", "tokens": ["Hab'", "ich", "mich", "nicht", "lang", "be\u00b7son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hab' mich feste injesponnen,", "tokens": ["Hab'", "mich", "fes\u00b7te", "in\u00b7jes\u00b7pon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJA", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nahm mir eene Olle,", "tokens": ["Nahm", "mir", "e\u00b7e\u00b7ne", "Ol\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Hopsasa, tralala!", "tokens": ["Hop\u00b7sa\u00b7sa", ",", "tra\u00b7la\u00b7la", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "XY", "$."], "meter": "++-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Nahm mir eene Olle.", "tokens": ["Nahm", "mir", "e\u00b7e\u00b7ne", "Ol\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "So 'ne olle, fette, dicke,", "tokens": ["So", "'ne", "ol\u00b7le", ",", "fet\u00b7te", ",", "di\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "$,", "VVFIN", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So 'ne rechte plumpe Zicke", "tokens": ["So", "'ne", "rech\u00b7te", "plum\u00b7pe", "Zi\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Aus dem B\u00fcrgerstande.", "tokens": ["Aus", "dem", "B\u00fcr\u00b7ger\u00b7stan\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "'s is nicht sch\u00f6n, mit ihr zu leben,", "tokens": ["'s", "is", "nicht", "sch\u00f6n", ",", "mit", "ihr", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "FM", "PTKNEG", "ADJD", "$,", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Darum hab' ich mich jejeben", "tokens": ["Da\u00b7rum", "hab'", "ich", "mich", "je\u00b7je\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Janz dem Vaterlande,", "tokens": ["Janz", "dem", "Va\u00b7ter\u00b7lan\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Hopsasa, tralala!", "tokens": ["Hop\u00b7sa\u00b7sa", ",", "tra\u00b7la\u00b7la", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "XY", "$."], "meter": "++-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Janz dem Vaterlande.", "tokens": ["Janz", "dem", "Va\u00b7ter\u00b7lan\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "F\u00fchr' 'ne heftige Bataille", "tokens": ["F\u00fchr'", "'ne", "hef\u00b7ti\u00b7ge", "Ba\u00b7tail\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit der dummen Pre\u00dfcanaille,", "tokens": ["Mit", "der", "dum\u00b7men", "Pre\u00df\u00b7ca\u00b7nail\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Leg' sie auf die Latte.", "tokens": ["Leg'", "sie", "auf", "die", "Lat\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Will ich mir mal am\u00fcsieren,", "tokens": ["Will", "ich", "mir", "mal", "a\u00b7m\u00fc\u00b7sie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df den Jeist ich maltr\u00e4tieren,", "tokens": ["La\u00df", "den", "Jeist", "ich", "malt\u00b7r\u00e4\u00b7tie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Den ich selbst nich hatte,", "tokens": ["Den", "ich", "selbst", "nich", "hat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "PTKNEG", "VAFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Hopsasa, tralala!", "tokens": ["Hop\u00b7sa\u00b7sa", ",", "tra\u00b7la\u00b7la", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "XY", "$."], "meter": "++-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Den ich selbst nich hatte.", "tokens": ["Den", "ich", "selbst", "nich", "hat\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "PTKNEG", "VAFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Scharf nach unten, mild nach oben,", "tokens": ["Scharf", "nach", "un\u00b7ten", ",", "mild", "nach", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADV", "$,", "ADJD", "APPR", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00d6ffentlich den Herrgott loben,", "tokens": ["\u00d6f\u00b7fent\u00b7lich", "den", "Herr\u00b7gott", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Heimlich is man kalte.", "tokens": ["Heim\u00b7lich", "is", "man", "kal\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "FM", "PIS", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Bald 'nen Tritt, un bald 'nen Orden,", "tokens": ["Bald", "'nen", "Tritt", ",", "un", "bald", "'nen", "Or\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "FM", "ADV", "ADJA", "NN", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.5": {"text": "Mancher is schon so jeworden", "tokens": ["Man\u00b7cher", "is", "schon", "so", "je\u00b7wor\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "FM", "ADV", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Oberstaatsanwalte,", "tokens": ["O\u00b7bers\u00b7taats\u00b7an\u00b7wal\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Hopsasa, tralala!", "tokens": ["Hop\u00b7sa\u00b7sa", ",", "tra\u00b7la\u00b7la", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "XY", "$."], "meter": "++-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Oberstaatsanwalte.", "tokens": ["O\u00b7bers\u00b7taats\u00b7an\u00b7wal\u00b7te", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}