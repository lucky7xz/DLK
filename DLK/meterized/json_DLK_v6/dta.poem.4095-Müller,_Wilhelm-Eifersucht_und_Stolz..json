{"dta.poem.4095": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Eifersucht und Stolz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Eilst du voll Zorn dem frechen Bruder J\u00e4ger nach?", "tokens": ["Eilst", "du", "voll", "Zorn", "dem", "fre\u00b7chen", "Bru\u00b7der", "J\u00e4\u00b7ger", "nach", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "NN", "ART", "ADJA", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kehr' um, kehr' um, und schilt erst deine M\u00fcllerin,", "tokens": ["Kehr'", "um", ",", "kehr'", "um", ",", "und", "schilt", "erst", "dei\u00b7ne", "M\u00fcl\u00b7le\u00b7rin", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "F\u00fcr ihren leichten, losen, kleinen Flattersinn.", "tokens": ["F\u00fcr", "ih\u00b7ren", "leich\u00b7ten", ",", "lo\u00b7sen", ",", "klei\u00b7nen", "Flat\u00b7ter\u00b7sinn", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "VVPP", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sahst du sie gestern Abend nicht am Thore stehn,", "tokens": ["Sahst", "du", "sie", "ge\u00b7stern", "A\u00b7bend", "nicht", "am", "Tho\u00b7re", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "NN", "PTKNEG", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mit langem Halse nach der gro\u00dfen Stra\u00dfe sehn?", "tokens": ["Mit", "lan\u00b7gem", "Hal\u00b7se", "nach", "der", "gro\u00b7\u00dfen", "Stra\u00b7\u00dfe", "sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn von dem Fang der J\u00e4ger lustig zieht nach Haus,", "tokens": ["Wenn", "von", "dem", "Fang", "der", "J\u00e4\u00b7ger", "lus\u00b7tig", "zieht", "nach", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "ADJD", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da steckt kein sittsam Kind den Kopf zum Fenster 'naus.", "tokens": ["Da", "steckt", "kein", "sitt\u00b7sam", "Kind", "den", "Kopf", "zum", "Fens\u00b7ter", "'naus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "ADJD", "NN", "ART", "NN", "APPRART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Geh', B\u00e4chlein, hin und sag' ihr das, doch sag' ihr nicht,", "tokens": ["Geh'", ",", "B\u00e4ch\u00b7lein", ",", "hin", "und", "sag'", "ihr", "das", ",", "doch", "sag'", "ihr", "nicht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "ADV", "KON", "VVFIN", "PPER", "PDS", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "H\u00f6rst du, kein Wort, von meinem traurigen Gesicht;", "tokens": ["H\u00f6rst", "du", ",", "kein", "Wort", ",", "von", "mei\u00b7nem", "trau\u00b7ri\u00b7gen", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PIAT", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sag' ihr: Er schnitzt bei mir sich eine Pfeif' aus Rohr,", "tokens": ["Sag'", "ihr", ":", "Er", "schnitzt", "bei", "mir", "sich", "ei\u00b7ne", "Pfeif'", "aus", "Rohr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "$.", "PPER", "VVFIN", "APPR", "PPER", "PRF", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und bl\u00e4st den Kindern sch\u00f6ne T\u00e4nz' und Lieder vor.", "tokens": ["Und", "bl\u00e4st", "den", "Kin\u00b7dern", "sch\u00f6\u00b7ne", "T\u00e4nz'", "und", "Lie\u00b7der", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}