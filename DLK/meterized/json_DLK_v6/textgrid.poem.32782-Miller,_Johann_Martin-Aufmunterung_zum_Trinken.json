{"textgrid.poem.32782": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Aufmunterung zum Trinken", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du der Klagen", "tokens": ["Willst", "du", "der", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Dich ganz entschlagen;", "tokens": ["Dich", "ganz", "ent\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Trink Wein, mein Sohn!", "tokens": ["Trink", "Wein", ",", "mein", "Sohn", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Folgst du der Lehre,", "tokens": ["Folgst", "du", "der", "Leh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Dann eilt, ich schw\u00f6re,", "tokens": ["Dann", "eilt", ",", "ich", "schw\u00f6\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Der Gram davon.", "tokens": ["Der", "Gram", "da\u00b7von", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Beim vollen Becher", "tokens": ["Beim", "vol\u00b7len", "Be\u00b7cher"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Verlacht der Zecher", "tokens": ["Ver\u00b7lacht", "der", "Ze\u00b7cher"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Die ganze Welt.", "tokens": ["Die", "gan\u00b7ze", "Welt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Dies wirst du glauben,", "tokens": ["Dies", "wirst", "du", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Wann Saft der Trauben", "tokens": ["Wann", "Saft", "der", "Trau\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Dein Aug' erhellt.", "tokens": ["Dein", "Aug'", "er\u00b7hellt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Wer wird den Sch\u00f6nen", "tokens": ["Wer", "wird", "den", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Auch immer fr\u00f6nen", "tokens": ["Auch", "im\u00b7mer", "fr\u00f6\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Um einen Ku\u00df?", "tokens": ["Um", "ei\u00b7nen", "Ku\u00df", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Erst mu\u00df man leiden,", "tokens": ["Erst", "mu\u00df", "man", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Dann folgen Freuden,", "tokens": ["Dann", "fol\u00b7gen", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Dann \u00dcberdru\u00df.", "tokens": ["Dann", "\u00dc\u00b7berd\u00b7ru\u00df", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "NN", "$."], "meter": "-+--", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Wir aber trinken,", "tokens": ["Wir", "a\u00b7ber", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Und Freuden winken", "tokens": ["Und", "Freu\u00b7den", "win\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Uns jeden Tag.", "tokens": ["Uns", "je\u00b7den", "Tag", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Der Unmut weichet,", "tokens": ["Der", "Un\u00b7mut", "wei\u00b7chet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Und Ekel schleichet", "tokens": ["Und", "E\u00b7kel", "schlei\u00b7chet"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Uns nimmer nach.", "tokens": ["Uns", "nim\u00b7mer", "nach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}