{"textgrid.poem.62740": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Madrigal, auf einen lasterhaften Philosophen", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du kanst vortreflich sch\u00f6n die Feder f\u00fchren;", "tokens": ["Du", "kanst", "vor\u00b7tre\u00b7flich", "sch\u00f6n", "die", "Fe\u00b7der", "f\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Allein du wilst dich selbst nicht so regieren.", "tokens": ["Al\u00b7lein", "du", "wilst", "dich", "selbst", "nicht", "so", "re\u00b7gie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So bist du wie ein blinder Mann,", "tokens": ["So", "bist", "du", "wie", "ein", "blin\u00b7der", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der eine helle Fackel tr\u00e4gt,", "tokens": ["Der", "ei\u00b7ne", "hel\u00b7le", "Fa\u00b7ckel", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wodurch er andern leicht; doch selbst nicht sehen kan.", "tokens": ["Wo\u00b7durch", "er", "an\u00b7dern", "leicht", ";", "doch", "selbst", "nicht", "se\u00b7hen", "kan", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIS", "ADJD", "$.", "ADV", "ADV", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}