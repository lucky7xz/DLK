{"dta.poem.16075": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "129.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Himmel ist so voll von Sternen nah und fern,", "tokens": ["Der", "Him\u00b7mel", "ist", "so", "voll", "von", "Ster\u00b7nen", "nah", "und", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "APPR", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von allen welcher wol ist meines Gl\u00fcckes Stern?", "tokens": ["Von", "al\u00b7len", "wel\u00b7cher", "wol", "ist", "mei\u00b7nes", "Gl\u00fc\u00b7ckes", "Stern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PRELS", "ADV", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich w\u00fcnschte, da\u00df einmal ich meinen Gl\u00fcckstern s\u00e4he,", "tokens": ["Ich", "w\u00fcnschte", ",", "da\u00df", "ein\u00b7mal", "ich", "mei\u00b7nen", "Gl\u00fccks\u00b7tern", "s\u00e4\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und da\u00df kein Ungl\u00fcckstern auch st\u00fcnd' in seiner N\u00e4he.", "tokens": ["Und", "da\u00df", "kein", "Un\u00b7gl\u00fccks\u00b7tern", "auch", "st\u00fcnd'", "in", "sei\u00b7ner", "N\u00e4\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIAT", "NN", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Nun, ist es mir versagt, den guten zu entdecken,", "tokens": ["Nun", ",", "ist", "es", "mir", "ver\u00b7sagt", ",", "den", "gu\u00b7ten", "zu", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VAFIN", "PPER", "PPER", "VVPP", "$,", "ART", "ADJA", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ist mirs auch erspart, vorm b\u00f6sen zu erschrecken.", "tokens": ["So", "ist", "mirs", "auch", "er\u00b7spart", ",", "vorm", "b\u00f6\u00b7sen", "zu", "er\u00b7schre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "ADV", "VVPP", "$,", "APPRART", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}