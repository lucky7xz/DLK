{"textgrid.poem.34194": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Meine neuen Gedichte", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Blumen will ich nicht mehr! So rief ich und h\u00e4tte die Keime", "tokens": ["Blu\u00b7men", "will", "ich", "nicht", "mehr", "!", "So", "rief", "ich", "und", "h\u00e4t\u00b7te", "die", "Kei\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "PTKNEG", "ADV", "$.", "ADV", "VVFIN", "PPER", "KON", "VAFIN", "ART", "NN"], "meter": "+----+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Mit dem erquetschenden Stein gerne f\u00fcr immer erstickt.", "tokens": ["Mit", "dem", "er\u00b7quet\u00b7schen\u00b7den", "Stein", "ger\u00b7ne", "f\u00fcr", "im\u00b7mer", "er\u00b7stickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "APPR", "ADV", "VVPP", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Aber sie spannen die Wurzeln gelassen weiter und schlingen", "tokens": ["A\u00b7ber", "sie", "span\u00b7nen", "die", "Wur\u00b7zeln", "ge\u00b7las\u00b7sen", "wei\u00b7ter", "und", "schlin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "VVPP", "PTKVZ", "KON", "VVFIN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Um ihn selbst nun als Kranz farbig und frisch sich herum.", "tokens": ["Um", "ihn", "selbst", "nun", "als", "Kranz", "far\u00b7big", "und", "frisch", "sich", "he\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ADV", "ADV", "KOUS", "NN", "ADJD", "KON", "ADJD", "PRF", "PTKVZ", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}}}}}