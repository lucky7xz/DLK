{"textgrid.poem.34877": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Frommen schenkt's der Herr im Traum,", "tokens": ["Den", "From\u00b7men", "schenkt's", "der", "Herr", "im", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wei\u00dft nicht, wie dir geschah!", "tokens": ["Wei\u00dft", "nicht", ",", "wie", "dir", "ge\u00b7schah", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du kriegst ein Kind und merkst es kaum,", "tokens": ["Du", "kriegst", "ein", "Kind", "und", "merkst", "es", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jungfrau Germania.", "tokens": ["Jung\u00b7frau", "Ger\u00b7ma\u00b7nia", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Es windet sich ein B\u00fcbelein", "tokens": ["Es", "win\u00b7det", "sich", "ein", "B\u00fc\u00b7be\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von deiner Nabelschnur,", "tokens": ["Von", "dei\u00b7ner", "Na\u00b7bel\u00b7schnur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es wird ein h\u00fcbscher Sch\u00fctze sein,", "tokens": ["Es", "wird", "ein", "h\u00fcb\u00b7scher", "Sch\u00fct\u00b7ze", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als wie der Gott Amour.", "tokens": ["Als", "wie", "der", "Gott", "A\u00b7mour", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Trifft einst in h\u00f6chster Luft den Aar,", "tokens": ["Trifft", "einst", "in", "h\u00f6chs\u00b7ter", "Luft", "den", "Aar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und fl\u00f6g er noch so stolz,", "tokens": ["Und", "fl\u00f6g", "er", "noch", "so", "stolz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den doppelk\u00f6pfigen sogar", "tokens": ["Den", "dop\u00b7pel\u00b7k\u00f6p\u00b7fi\u00b7gen", "so\u00b7gar"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erreicht sein guter Bolz.", "tokens": ["Er\u00b7reicht", "sein", "gu\u00b7ter", "Bolz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch nicht wie jener blinde Heid',", "tokens": ["Doch", "nicht", "wie", "je\u00b7ner", "blin\u00b7de", "Heid'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "KOKOM", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht wie der Liebesgott,", "tokens": ["Nicht", "wie", "der", "Lie\u00b7bes\u00b7gott", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Soll er sich ohne Hos' und Kleid", "tokens": ["Soll", "er", "sich", "oh\u00b7ne", "Hos'", "und", "Kleid"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zeigen als Sansculott'.", "tokens": ["Zei\u00b7gen", "als", "Sans\u00b7cu\u00b7lott'", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Bei uns zu Land die Witterung,", "tokens": ["Bei", "uns", "zu", "Land", "die", "Wit\u00b7te\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Moral und Polizei", "tokens": ["Mo\u00b7ral", "und", "Po\u00b7li\u00b7zei"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gebieten streng, da\u00df alt und jung", "tokens": ["Ge\u00b7bie\u00b7ten", "streng", ",", "da\u00df", "alt", "und", "jung"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "KOUS", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Leiblich bekleidet sei.", "tokens": ["Leib\u00b7lich", "be\u00b7klei\u00b7det", "sei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VAFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}