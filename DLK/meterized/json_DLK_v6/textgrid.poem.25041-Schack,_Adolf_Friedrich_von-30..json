{"textgrid.poem.25041": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stumm liegt die tr\u00e4umende Natur;", "tokens": ["Stumm", "liegt", "die", "tr\u00e4u\u00b7men\u00b7de", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Wozu die gro\u00dfe Stille brechen?", "tokens": ["Wo\u00b7zu", "die", "gro\u00b7\u00dfe", "Stil\u00b7le", "bre\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Herz la\u00df mit dem Herzen nur,", "tokens": ["Das", "Herz", "la\u00df", "mit", "dem", "Her\u00b7zen", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Auge mit dem Auge sprechen!", "tokens": ["Das", "Au\u00b7ge", "mit", "dem", "Au\u00b7ge", "spre\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Spricht Bl\u00fcte so mit Bl\u00fcte nicht", "tokens": ["Spricht", "Bl\u00fc\u00b7te", "so", "mit", "Bl\u00fc\u00b7te", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "ADV", "APPR", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An des Jasminstrauchs duft'gen Zweigen?", "tokens": ["An", "des", "Jas\u00b7mins\u00b7trauchs", "duft'\u00b7gen", "Zwei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "So Stern zu Stern mit goldnem Licht", "tokens": ["So", "Stern", "zu", "Stern", "mit", "gold\u00b7nem", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "APPR", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht in der Sommern\u00e4chte Schweigen?", "tokens": ["Nicht", "in", "der", "Som\u00b7mer\u00b7n\u00e4ch\u00b7te", "Schwei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das ist die Sprache, weltenalt,", "tokens": ["Das", "ist", "die", "Spra\u00b7che", ",", "wel\u00b7ten\u00b7alt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die lang die Liebe schon gesprochen,", "tokens": ["Die", "lang", "die", "Lie\u00b7be", "schon", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Eh sie den ersten Laut gelallt;", "tokens": ["Eh", "sie", "den", "ers\u00b7ten", "Laut", "ge\u00b7lallt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "In Worten spricht sie nur gebrochen.", "tokens": ["In", "Wor\u00b7ten", "spricht", "sie", "nur", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}