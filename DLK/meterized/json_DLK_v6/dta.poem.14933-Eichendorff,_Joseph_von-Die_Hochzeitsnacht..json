{"dta.poem.14933": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Hochzeitsnacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Sie hat mir Treu' versprochen,", "tokens": ["Sie", "hat", "mir", "Treu'", "ver\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bis ich gekommen sei,", "tokens": ["Bis", "ich", "ge\u00b7kom\u00b7men", "sei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie hat die Treu gebrochen,", "tokens": ["Sie", "hat", "die", "Treu", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und alles ist vorbei.\u201c", "tokens": ["Und", "al\u00b7les", "ist", "vor\u00b7bei", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}