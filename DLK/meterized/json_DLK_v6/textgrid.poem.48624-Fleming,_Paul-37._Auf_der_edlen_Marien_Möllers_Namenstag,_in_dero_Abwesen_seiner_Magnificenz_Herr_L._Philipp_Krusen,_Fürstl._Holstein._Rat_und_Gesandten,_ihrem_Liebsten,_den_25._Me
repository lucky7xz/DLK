{"textgrid.poem.48624": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "37. Auf der edlen Marien M\u00f6llers Namenstag, in dero Abwesen seiner Magnificenz Herr L. Philipp Krusen, F\u00fcrstl. Holstein. Rat und Gesandten, ihrem Liebsten, den 25. Merz 1636. zu Spasabulka in Reu\u00dfen unterwegs \u00fcbergeben", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Di\u00df ist ", "tokens": ["Di\u00df", "ist"], "token_info": ["word", "word"], "pos": ["PDS", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "di\u00df sind ihre s\u00fc\u00dfe Stunden.", "tokens": ["di\u00df", "sind", "ih\u00b7re", "s\u00fc\u00b7\u00dfe", "Stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "S\u00e4umt, ihr Hirten, s\u00e4umt euch nicht!", "tokens": ["S\u00e4umt", ",", "ihr", "Hir\u00b7ten", ",", "s\u00e4umt", "euch", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie, sie mu\u00df sein angebunden.", "tokens": ["Sie", ",", "sie", "mu\u00df", "sein", "an\u00b7ge\u00b7bun\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "VMFIN", "PPOSAT", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch wo ist das sch\u00f6ne Kind,", "tokens": ["Doch", "wo", "ist", "das", "sch\u00f6\u00b7ne", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "um das wir bem\u00fchet sind?", "tokens": ["um", "das", "wir", "be\u00b7m\u00fc\u00b7het", "sind", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dreimal siebenmal ist gleich", "tokens": ["Drei\u00b7mal", "sie\u00b7ben\u00b7mal", "ist", "gleich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "heut' Apollo wieder kommen,", "tokens": ["heut'", "A\u00b7pol\u00b7lo", "wie\u00b7der", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "seit da\u00df wir von Trauren bleich", "tokens": ["seit", "da\u00df", "wir", "von", "Trau\u00b7ren", "bleich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "APPR", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "haben Abschied dort genommen.", "tokens": ["ha\u00b7ben", "Ab\u00b7schied", "dort", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In so einer kleinen Zeit", "tokens": ["In", "so", "ei\u00b7ner", "klei\u00b7nen", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "sind wir nun von ihr so weit.", "tokens": ["sind", "wir", "nun", "von", "ihr", "so", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch, ihr Hirten, unbetr\u00fcbt!", "tokens": ["Doch", ",", "ihr", "Hir\u00b7ten", ",", "un\u00b7be\u00b7tr\u00fcbt", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "PPOSAT", "NN", "$,", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00dft uns ihren ", "tokens": ["La\u00dft", "uns", "ih\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "ihren ", "tokens": ["ih\u00b7ren"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Giebt uns ", "tokens": ["Giebt", "uns"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "so soll ", "tokens": ["so", "soll"], "token_info": ["word", "word"], "pos": ["ADV", "VMFIN"], "meter": "-+", "measure": "iambic.single"}}}}}