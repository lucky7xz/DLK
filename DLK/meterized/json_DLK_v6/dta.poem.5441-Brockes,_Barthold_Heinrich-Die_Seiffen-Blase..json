{"dta.poem.5441": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Seiffen-Blase.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als von meinen S\u00f6hnen einer neulich Seiffen-Blasen\nmachte,", "tokens": ["Als", "von", "mei\u00b7nen", "S\u00f6h\u00b7nen", "ei\u00b7ner", "neu\u00b7lich", "Seif\u00b7fen\u00b7Bla\u00b7sen", "mach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "ART", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Und ich \u00fcber den Betrieb seiner Einf\u00e4ll\u2019 anfangs lachte;", "tokens": ["Und", "ich", "\u00fc\u00b7ber", "den", "Be\u00b7trieb", "sei\u00b7ner", "Ein\u00b7f\u00e4ll'", "an\u00b7fangs", "lach\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ART", "NN", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Ward ich endlich, da er eine, die vor andern gro\u00df und", "tokens": ["Ward", "ich", "end\u00b7lich", ",", "da", "er", "ei\u00b7ne", ",", "die", "vor", "an\u00b7dern", "gro\u00df", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ART", "$,", "PRELS", "APPR", "PIS", "ADJD", "KON"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Und von wandelbaren Farben unbeschreiblich herrlich war,", "tokens": ["Und", "von", "wan\u00b7del\u00b7ba\u00b7ren", "Far\u00b7ben", "un\u00b7be\u00b7schreib\u00b7lich", "herr\u00b7lich", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ADJD", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Durch den wundersch\u00f6nen Glantz, der recht unvergleichlich", "tokens": ["Durch", "den", "wun\u00b7der\u00b7sch\u00f6\u00b7nen", "Glantz", ",", "der", "recht", "un\u00b7ver\u00b7gleich\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ADJD"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Fast gezwungen, mit Bedacht, ihre Sch\u00f6nheit anzusehn.", "tokens": ["Fast", "ge\u00b7zwun\u00b7gen", ",", "mit", "Be\u00b7dacht", ",", "ih\u00b7re", "Sch\u00f6n\u00b7heit", "an\u00b7zu\u00b7sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "APPR", "NN", "$,", "PPOSAT", "NN", "VVIZU", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Ich erstaunte, wie ich hier ein so bunt-gef\u00e4rbtes Licht,", "tokens": ["Ich", "er\u00b7staun\u00b7te", ",", "wie", "ich", "hier", "ein", "so", "bunt\u00b7ge\u00b7f\u00e4rb\u00b7tes", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "ADV", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "In fast \u00fcber-ird\u2019schem Schimmer, ein fast brennend Roht,", "tokens": ["In", "fast", "\u00fc\u00b7ber\u00b7ird'\u00b7schem", "Schim\u00b7mer", ",", "ein", "fast", "bren\u00b7nend", "Roht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,", "ART", "ADV", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.9": {"text": "Das den reinesten Smaragd, so wie jenes den Rubin,", "tokens": ["Das", "den", "rei\u00b7nes\u00b7ten", "Sma\u00b7ragd", ",", "so", "wie", "je\u00b7nes", "den", "Ru\u00b7bin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "$,", "ADV", "KOKOM", "PDS", "ART", "NE", "$,"], "meter": "--+--++--+--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "W\u00fcrcklich \u00fcbertraf, erblickte. Aber ein Sapphirner", "tokens": ["W\u00fcrck\u00b7lich", "\u00fc\u00b7bert\u00b7raf", ",", "er\u00b7blick\u00b7te", ".", "A\u00b7ber", "ein", "Sap\u00b7phir\u00b7ner"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "$,", "VVFIN", "$.", "KON", "ART", "NN"], "meter": "+-+-+-+-+--++-", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "Und ein helles Purpur-Feuer, eine mehr als g\u00fcldne Glut", "tokens": ["Und", "ein", "hel\u00b7les", "Pur\u00b7pur\u00b7Feu\u00b7er", ",", "ei\u00b7ne", "mehr", "als", "g\u00fcld\u00b7ne", "Glut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "ART", "PIAT", "KOKOM", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Nahm, mit einem schnellen Wechsel, augenblicks die Stel-", "tokens": ["Nahm", ",", "mit", "ei\u00b7nem", "schnel\u00b7len", "Wech\u00b7sel", ",", "au\u00b7gen\u00b7blicks", "die", "Stel"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "ART", "ADJA", "NN", "$,", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.13": {"text": "Die erst gr\u00fcn und roth gewesen. In dem Glantz, der nim-", "tokens": ["Die", "erst", "gr\u00fcn", "und", "roth", "ge\u00b7we\u00b7sen", ".", "In", "dem", "Glantz", ",", "der", "nim"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADV", "ADJD", "KON", "ADJD", "VAPP", "$.", "APPR", "ART", "NN", "$,", "ART", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.14": {"text": "Sah ich mit erstarrten Blicken, als im Diamantnen Spiegel,", "tokens": ["Sah", "ich", "mit", "er\u00b7starr\u00b7ten", "Bli\u00b7cken", ",", "als", "im", "Di\u00b7a\u00b7mant\u00b7nen", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,", "KOUS", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.15": {"text": "Himmel, Erde, H\u00e4user, Fenster, W\u00e4lder, Felder, Thal", "tokens": ["Him\u00b7mel", ",", "Er\u00b7de", ",", "H\u00e4u\u00b7ser", ",", "Fens\u00b7ter", ",", "W\u00e4l\u00b7der", ",", "Fel\u00b7der", ",", "Thal"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.16": {"text": "Sich in schnellen Farben bilden, als ein neues Wunder, an,", "tokens": ["Sich", "in", "schnel\u00b7len", "Far\u00b7ben", "bil\u00b7den", ",", "als", "ein", "neu\u00b7es", "Wun\u00b7der", ",", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "VVINF", "$,", "KOUS", "ART", "ADJA", "NN", "$,", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.17": {"text": "Welches alles \u00fcbertraf, was man seh\u2019n und dencken kann.", "tokens": ["Wel\u00b7ches", "al\u00b7les", "\u00fc\u00b7bert\u00b7raf", ",", "was", "man", "seh'n", "und", "den\u00b7cken", "kann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "$,", "PRELS", "PIS", "VVINF", "KON", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "Alles stand in buntem Schimmer, alles war gedoppelt", "tokens": ["Al\u00b7les", "stand", "in", "bun\u00b7tem", "Schim\u00b7mer", ",", "al\u00b7les", "war", "ge\u00b7dop\u00b7pelt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN", "$,", "PIS", "VAFIN", "VVPP"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.19": {"text": "Weil, was auf der obern Fl\u00e4che, sich auch auf der untern", "tokens": ["Weil", ",", "was", "auf", "der", "o\u00b7bern", "Fl\u00e4\u00b7che", ",", "sich", "auch", "auf", "der", "un\u00b7tern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "PRELS", "APPR", "ART", "ADJA", "NN", "$,", "PRF", "ADV", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.20": {"text": "Und, als wie die Luft im Wasser, alles doppelt sehen lie\u00df;", "tokens": ["Und", ",", "als", "wie", "die", "Luft", "im", "Was\u00b7ser", ",", "al\u00b7les", "dop\u00b7pelt", "se\u00b7hen", "lie\u00df", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "KOKOM", "ART", "NN", "APPRART", "NN", "$,", "PIS", "ADJD", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.21": {"text": "Formen, Farben, Glantz und Licht waren rund, auch hohl zu", "tokens": ["For\u00b7men", ",", "Far\u00b7ben", ",", "Glantz", "und", "Licht", "wa\u00b7ren", "rund", ",", "auch", "hohl", "zu"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "VAFIN", "ADJD", "$,", "ADV", "ADJD", "PTKZU"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Mich bed\u00fcnckt, indem ich scharf auf die Farben in der", "tokens": ["Mich", "be\u00b7d\u00fcnckt", ",", "in\u00b7dem", "ich", "scharf", "auf", "die", "Far\u00b7ben", "in", "der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "ART"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Mit gesch\u00e4rften Blicken, sehe;", "tokens": ["Mit", "ge\u00b7sch\u00e4rf\u00b7ten", "Bli\u00b7cken", ",", "se\u00b7he", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich eine nach der andern kommen, scheinen und vergehn,", "tokens": ["Da\u00df", "ich", "ei\u00b7ne", "nach", "der", "an\u00b7dern", "kom\u00b7men", ",", "schei\u00b7nen", "und", "ver\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "APPR", "ART", "ADJA", "VVINF", "$,", "VVFIN", "KON", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und, an ihren vor\u2019gen Stellen, andre schwinden und", "tokens": ["Und", ",", "an", "ih\u00b7ren", "vor'\u00b7gen", "Stel\u00b7len", ",", "and\u00b7re", "schwin\u00b7den", "und"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,", "PIS", "VVFIN", "KON"], "meter": "--+-+-+-+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Und auch die sich \u00e4ndern, finde. Weil fast nichts best\u00e4ndig", "tokens": ["Und", "auch", "die", "sich", "\u00e4n\u00b7dern", ",", "fin\u00b7de", ".", "Weil", "fast", "nichts", "be\u00b7st\u00e4n\u00b7dig"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PRELS", "PRF", "VVINF", "$,", "VVFIN", "$.", "KOUS", "ADV", "PIS", "ADJD"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "Was erst weis war, f\u00e4rbt sich gr\u00fcn; dieses roth; das", "tokens": ["Was", "erst", "weis", "war", ",", "f\u00e4rbt", "sich", "gr\u00fcn", ";", "die\u00b7ses", "roth", ";", "das"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PWS", "ADV", "PTKVZ", "VAFIN", "$,", "VVFIN", "PRF", "ADJD", "$.", "PDAT", "ADJD", "$.", "ART"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Denn erschien das weisse wieder, und die Aenderung war", "tokens": ["Denn", "er\u00b7schien", "das", "weis\u00b7se", "wie\u00b7der", ",", "und", "die", "A\u00b7en\u00b7de\u00b7rung", "war"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PDS", "VVFIN", "ADV", "$,", "KON", "ART", "NN", "VAFIN"], "meter": "+-+-+-+-+-+---+", "measure": "unknown.measure.septa"}}, "stanza.3": {"line.1": {"text": "Dieser Kugel Farben-Wechsel kam, wie ichs bedachte,", "tokens": ["Die\u00b7ser", "Ku\u00b7gel", "Fa\u00b7rben\u00b7Wech\u00b7sel", "kam", ",", "wie", "ichs", "be\u00b7dach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "VVFIN", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Recht, als ", "tokens": ["Recht", ",", "als"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "KOUS"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Da im Sommer, Herbst, und Winter und in dem be-", "tokens": ["Da", "im", "Som\u00b7mer", ",", "Herbst", ",", "und", "Win\u00b7ter", "und", "in", "dem", "be"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "$,", "NN", "$,", "KON", "NN", "KON", "APPR", "ART", "TRUNC"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Gelb und Roth und Wei\u00df und Gr\u00fcn, wechsels Weise,", "tokens": ["Gelb", "und", "Roth", "und", "Wei\u00df", "und", "Gr\u00fcn", ",", "wech\u00b7sels", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "KON", "NN", "KON", "NN", "KON", "NN", "$,", "KOUS", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Alle Jahres-Zeiten sind auf der Welt zu gleicher Zeit,", "tokens": ["Al\u00b7le", "Jah\u00b7res\u00b7Zei\u00b7ten", "sind", "auf", "der", "Welt", "zu", "glei\u00b7cher", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Und sie \u00e4ndern ihren Ort zwar in mindrer Schnelligkeit,", "tokens": ["Und", "sie", "\u00e4n\u00b7dern", "ih\u00b7ren", "Ort", "zwar", "in", "mind\u00b7rer", "Schnel\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Aber doch auch schnell genug. Ferner ward ich noch gewahr", "tokens": ["A\u00b7ber", "doch", "auch", "schnell", "ge\u00b7nug", ".", "Fer\u00b7ner", "ward", "ich", "noch", "ge\u00b7wahr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ADJD", "ADV", "$.", "ADV", "VAFIN", "PPER", "ADV", "ADJD"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Wie von allen Elementen gleichfals die gevierte Schaar", "tokens": ["Wie", "von", "al\u00b7len", "E\u00b7le\u00b7men\u00b7ten", "gleich\u00b7fals", "die", "ge\u00b7vier\u00b7te", "Schaar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PIAT", "NN", "ADV", "ART", "ADJA", "NN"], "meter": "--+--+--+-+-+-+", "measure": "anapaest.tri.plus"}, "line.9": {"text": "In dem kleinem Raum sich zeigte. In dem Gr\u00fcnen, in", "tokens": ["In", "dem", "klei\u00b7nem", "Raum", "sich", "zeig\u00b7te", ".", "In", "dem", "Gr\u00fc\u00b7nen", ",", "in"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "PRF", "VVFIN", "$.", "APPR", "ART", "NN", "$,", "APPR"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.10": {"text": "In dem Weissen, in dem Rohten, war die Erde, war die", "tokens": ["In", "dem", "Weis\u00b7sen", ",", "in", "dem", "Roh\u00b7ten", ",", "war", "die", "Er\u00b7de", ",", "war", "die"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,", "VAFIN", "ART", "NN", "$,", "VAFIN", "ART"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.11": {"text": "War die Luft und war die Glut,", "tokens": ["War", "die", "Luft", "und", "war", "die", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN