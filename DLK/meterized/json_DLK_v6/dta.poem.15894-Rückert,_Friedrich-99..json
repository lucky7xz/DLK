{"dta.poem.15894": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "99.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von keinem Helden, der noch lebet, sollst du singen,", "tokens": ["Von", "kei\u00b7nem", "Hel\u00b7den", ",", "der", "noch", "le\u00b7bet", ",", "sollst", "du", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er m\u00f6chte seinem Ruhm und dir noch Schande bringen.", "tokens": ["Er", "m\u00f6ch\u00b7te", "sei\u00b7nem", "Ruhm", "und", "dir", "noch", "Schan\u00b7de", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "KON", "PPER", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Erst wann man ihn begr\u00e4bt, wei\u00df man, wie er gelebt,", "tokens": ["Erst", "wann", "man", "ihn", "be\u00b7gr\u00e4bt", ",", "wei\u00df", "man", ",", "wie", "er", "ge\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "PIS", "PPER", "VVFIN", "$,", "VVFIN", "PIS", "$,", "PWAV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob w\u00fcrdig oder nicht, da\u00df ihn Gesang erhebt.", "tokens": ["Ob", "w\u00fcr\u00b7dig", "o\u00b7der", "nicht", ",", "da\u00df", "ihn", "Ge\u00b7sang", "er\u00b7hebt", "."], "token_info": ["word", "word", "word", "word", "pun