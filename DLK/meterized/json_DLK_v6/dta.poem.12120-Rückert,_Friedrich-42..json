{"dta.poem.12120": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "42.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Minnesingerharf', an der von allen Saiten", "tokens": ["Die", "Min\u00b7ne\u00b7sin\u00b7ger\u00b7ha\u00b7rf'", ",", "an", "der", "von", "al\u00b7len", "Sai\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "ART", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Nur eine ganz blieb als ein Nachhall sch\u00f6nrer Zeiten,", "tokens": ["Nur", "ei\u00b7ne", "ganz", "blieb", "als", "ein", "Nach\u00b7hall", "sch\u00f6n\u00b7rer", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADV", "VVFIN", "KOKOM", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Hab' ich auf Schlo\u00df Ambras gesehn, indem ich dachte,", "tokens": ["Hab'", "ich", "auf", "Schlo\u00df", "A\u00b7mbras", "ge\u00b7sehn", ",", "in\u00b7dem", "ich", "dach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "NE", "VVPP", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie soviel Herrlichkeit die Zeit zu Schanden machte.", "tokens": ["Wie", "so\u00b7viel", "Herr\u00b7lich\u00b7keit", "die", "Zeit", "zu", "Schan\u00b7den", "mach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das Schlo\u00df, wo Ferdinand wohnte mit Philippinen,", "tokens": ["Das", "Schlo\u00df", ",", "wo", "Fer\u00b7di\u00b7nand", "wohn\u00b7te", "mit", "Phil\u00b7ip\u00b7pi\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "NE", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mu\u00df zur Kaserne jetzt welschen Soldaten dienen.", "tokens": ["Mu\u00df", "zur", "Ka\u00b7ser\u00b7ne", "jetzt", "wel\u00b7schen", "Sol\u00b7da\u00b7ten", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "NN", "ADV", "PWAT", "NN", "VVINF", "$."], "meter": "+-+---+--+-+-", "measure": "trochaic.penta.relaxed"}}}}}