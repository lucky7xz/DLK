{"textgrid.poem.56678": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Der Tod", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn alles mi\u00dfger\u00e4t und ganz zersplittert", "tokens": ["Wenn", "al\u00b7les", "mi\u00df\u00b7ge\u00b7r\u00e4t", "und", "ganz", "zer\u00b7split\u00b7tert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADJD", "KON", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "sogar des Stolzes harte Ruhewiegen", "tokens": ["so\u00b7gar", "des", "Stol\u00b7zes", "har\u00b7te", "Ru\u00b7he\u00b7wie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "in armen Brocken mir zu F\u00fc\u00dfen liegen,", "tokens": ["in", "ar\u00b7men", "Bro\u00b7cken", "mir", "zu", "F\u00fc\u00b7\u00dfen", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "wenn mich der Ekel grau und gr\u00fcn umwittert,", "tokens": ["wenn", "mich", "der", "E\u00b7kel", "grau", "und", "gr\u00fcn", "um\u00b7wit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "mich die Verzweiflung mauernhoch umgittert,", "tokens": ["mich", "die", "Ver\u00b7zwei\u00b7flung", "mau\u00b7ern\u00b7hoch", "um\u00b7git\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "wei\u00df ich mich noch an einen Trost zu schmiegen,", "tokens": ["wei\u00df", "ich", "mich", "noch", "an", "ei\u00b7nen", "Trost", "zu", "schmie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "auf purpurrot belegten Marmorstiegen", "tokens": ["auf", "pur\u00b7pur\u00b7rot", "be\u00b7leg\u00b7ten", "Mar\u00b7mor\u00b7stie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "vom Dufte des Vergessens schon umzittert", "tokens": ["vom", "Duf\u00b7te", "des", "Ver\u00b7ges\u00b7sens", "schon", "um\u00b7zit\u00b7tert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "selbstherrlich in dein K\u00f6nigreich zu schreiten,", "tokens": ["selbst\u00b7herr\u00b7lich", "in", "dein", "K\u00f6\u00b7nig\u00b7reich", "zu", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "in dem der Sturm Begehren endlich schweigt,", "tokens": ["in", "dem", "der", "Sturm", "Be\u00b7geh\u00b7ren", "end\u00b7lich", "schweigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "in dem erstickt von tiefsten Sicherheiten", "tokens": ["in", "dem", "er\u00b7stickt", "von", "tiefs\u00b7ten", "Si\u00b7cher\u00b7hei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "der zungenlaute Zweifel von mir weicht", "tokens": ["der", "zun\u00b7gen\u00b7lau\u00b7te", "Zwei\u00b7fel", "von", "mir", "weicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und mir nicht mehr zu kurzen Trunkenheiten", "tokens": ["und", "mir", "nicht", "mehr", "zu", "kur\u00b7zen", "Trun\u00b7ken\u00b7hei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "PTKNEG", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "die Hoffnung ihren L\u00fcgenbecher reicht.", "tokens": ["die", "Hoff\u00b7nung", "ih\u00b7ren", "L\u00fc\u00b7gen\u00b7be\u00b7cher", "reicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}