{"textgrid.poem.57864": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Junggesellenlied", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fischen, Jagen und Vogelstellen", "tokens": ["Fi\u00b7schen", ",", "Ja\u00b7gen", "und", "Vo\u00b7gel\u00b7stel\u00b7len"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Das h\u00e4lt jung die Junggesellen;", "tokens": ["Das", "h\u00e4lt", "jung", "die", "Jung\u00b7ge\u00b7sel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Junggesellen die wollen wir sein", "tokens": ["Jung\u00b7ge\u00b7sel\u00b7len", "die", "wol\u00b7len", "wir", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "VMFIN", "PPER", "PPOSAT"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Bei Bier und k\u00fchligem Wein.", "tokens": ["Bei", "Bier", "und", "k\u00fch\u00b7li\u00b7gem", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Fisch und V\u00f6glein fangen wir", "tokens": ["Fisch", "und", "V\u00f6\u00b7glein", "fan\u00b7gen", "wir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem blauen und gr\u00fcnen Revier;", "tokens": ["In", "dem", "blau\u00b7en", "und", "gr\u00fc\u00b7nen", "Re\u00b7vier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Dazu manch Wildbret zart und fein,", "tokens": ["Da\u00b7zu", "manch", "Wild\u00b7bret", "zart", "und", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Junggesellen wollen wir sein.", "tokens": ["Jung\u00b7ge\u00b7sel\u00b7len", "wol\u00b7len", "wir", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VAINF", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Das Gl\u00e4slein geht reihum, reihum,", "tokens": ["Das", "Gl\u00e4s\u00b7lein", "geht", "rei\u00b7hum", ",", "rei\u00b7hum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer nicht singt, der bleibet stumm", "tokens": ["Wer", "nicht", "singt", ",", "der", "blei\u00b7bet", "stumm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PTKNEG", "VVFIN", "$,", "PRELS", "VVFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und schweiget fein, ja fein;", "tokens": ["Und", "schwei\u00b7get", "fein", ",", "ja", "fein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Junggesellen wollen wir sein.", "tokens": ["Jung\u00b7ge\u00b7sel\u00b7len", "wol\u00b7len", "wir", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VAINF", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Ist man erst ein Ehemann,", "tokens": ["Ist", "man", "erst", "ein", "E\u00b7he\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ART", "NN", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Gibt mans Fischen und Jagen dran,", "tokens": ["Gibt", "mans", "Fi\u00b7schen", "und", "Ja\u00b7gen", "dran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "L\u00e4\u00dfts V\u00f6gleinstellen sein;", "tokens": ["L\u00e4\u00dfts", "V\u00f6\u00b7glein\u00b7stel\u00b7len", "sein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Junggesellen wollen wir sein.", "tokens": ["Jung\u00b7ge\u00b7sel\u00b7len", "wol\u00b7len", "wir", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VAINF", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}