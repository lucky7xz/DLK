{"dta.poem.5903": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Elementen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Durch ein so k\u00fchn-als eiteles Bem\u00fchn,", "tokens": ["Durch", "ein", "so", "k\u00fchn\u00b7als", "ei\u00b7te\u00b7les", "Be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In Dingen der Natur,", "tokens": ["In", "Din\u00b7gen", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Lehre von dem ", "tokens": ["Die", "Leh\u00b7re", "von", "dem"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Aufs neu ans Licht zu ziehn.", "tokens": ["Aufs", "neu", "ans", "Licht", "zu", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da, in ", "tokens": ["Da", ",", "in"], "token_info": ["word", "punct", "word"], "pos": ["ADV", "$,", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Uns ", "tokens": ["Uns"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Die Nachricht vom Geb\u00e4ude dieser Welt:", "tokens": ["Die", "Nach\u00b7richt", "vom", "Ge\u00b7b\u00e4u\u00b7de", "die\u00b7ser", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Auch zu erweisen sucht, und uns vor Augen stellt,", "tokens": ["Auch", "zu", "er\u00b7wei\u00b7sen", "sucht", ",", "und", "uns", "vor", "Au\u00b7gen", "stellt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "VVFIN", "$,", "KON", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Durch manchen Schein-Grund seiner Lehre,", "tokens": ["Durch", "man\u00b7chen", "Schein\u00b7Grund", "sei\u00b7ner", "Leh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Als wenn der ", "tokens": ["Als", "wenn", "der"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "KOUS", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Bey diesem Ziel, vermeint er, m\u00fcsse man", "tokens": ["Bey", "die\u00b7sem", "Ziel", ",", "ver\u00b7meint", "er", ",", "m\u00fcs\u00b7se", "man"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "VVFIN", "PPER", "$,", "VMFIN", "PIS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Ohn weitres Gr\u00fcbeln bleiben,", "tokens": ["Ohn", "weit\u00b7res", "Gr\u00fc\u00b7beln", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Und nimmer dencken, da\u00df ein Grund", "tokens": ["Und", "nim\u00b7mer", "den\u00b7cken", ",", "da\u00df", "ein", "Grund"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVINF", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Von der Natur sich selbst zerreiben,", "tokens": ["Von", "der", "Na\u00b7tur", "sich", "selbst", "zer\u00b7rei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Vergehen, noch sich \u00e4ndern kan.", "tokens": ["Ver\u00b7ge\u00b7hen", ",", "noch", "sich", "\u00e4n\u00b7dern", "kan", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Das, woraus alle Ding\u2019 entstehen und bestehn,", "tokens": ["Das", ",", "wo\u00b7raus", "al\u00b7le", "Ding'", "ent\u00b7ste\u00b7hen", "und", "be\u00b7stehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWAV", "PIAT", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Kan, schreibt er, nimmermehr vergehn.", "tokens": ["Kan", ",", "schreibt", "er", ",", "nim\u00b7mer\u00b7mehr", "ver\u00b7gehn", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "$,", "VVFIN", "PPER", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Allein,", "tokens": ["Al\u00b7lein", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.19": {"text": "Kan man von C\u00f6rpern, welche sich", "tokens": ["Kan", "man", "von", "C\u00f6r\u00b7pern", ",", "wel\u00b7che", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PIS", "APPR", "NE", "$,", "PRELS", "PRF"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.20": {"text": "Nicht theilen, deren Daur so unver\u00e4nderlich,", "tokens": ["Nicht", "thei\u00b7len", ",", "de\u00b7ren", "Daur", "so", "un\u00b7ver\u00b7\u00e4n\u00b7der\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$,", "PRELAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Auch feste vergewissert seyn?", "tokens": ["Auch", "fes\u00b7te", "ver\u00b7ge\u00b7wis\u00b7sert", "seyn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Man kan es eben, wie das ", "tokens": ["Man", "kan", "es", "e\u00b7ben", ",", "wie", "das"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "VMFIN", "PPER", "ADV", "$,", "PWAV", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Die Regel f\u00e4llet weg. Wofern es wahr,", "tokens": ["Die", "Re\u00b7gel", "f\u00e4l\u00b7let", "weg", ".", "Wo\u00b7fern", "es", "wahr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "KOUS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Da\u00df alle C\u00f6rper Theile hegen;", "tokens": ["Da\u00df", "al\u00b7le", "C\u00f6r\u00b7per", "Thei\u00b7le", "he\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "So ist es klar:", "tokens": ["So", "ist", "es", "klar", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.26": {"text": "Die Sonnen-St\u00e4ubgen m\u00fcssen", "tokens": ["Die", "Son\u00b7nen\u00b7St\u00e4ub\u00b7gen", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VMFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Nothwendig auch ja Theile hegen,", "tokens": ["Noth\u00b7wen\u00b7dig", "auch", "ja", "Thei\u00b7le", "he\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Man mu\u00df dahero dann nothwendig schl\u00fcssen,", "tokens": ["Man", "mu\u00df", "da\u00b7he\u00b7ro", "dann", "noth\u00b7wen\u00b7dig", "schl\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PAV", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Da\u00df sie der Theilung nicht zu widerstehn verm\u00f6gen.", "tokens": ["Da\u00df", "sie", "der", "Thei\u00b7lung", "nicht", "zu", "wi\u00b7der\u00b7stehn", "ver\u00b7m\u00f6\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}