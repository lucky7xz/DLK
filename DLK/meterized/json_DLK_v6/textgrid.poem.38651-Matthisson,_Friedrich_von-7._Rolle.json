{"textgrid.poem.38651": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "7. Rolle", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Chandler erkohr mich zum Wohnsitz, der Britte, den Griechenlands Himmel", "tokens": ["Chand\u00b7ler", "er\u00b7kohr", "mich", "zum", "Wohn\u00b7sitz", ",", "der", "Brit\u00b7te", ",", "den", "Grie\u00b7chen\u00b7lands", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "APPRART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Von der Akropolis einst bis nach Miletus umgl\u00e4nzt.", "tokens": ["Von", "der", "Ak\u00b7ro\u00b7po\u00b7lis", "einst", "bis", "nach", "Mi\u00b7le\u00b7tus", "um\u00b7gl\u00e4nzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADV", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Gibbon besuchte mich oft, auch jubelt im Roguinschen Saale,", "tokens": ["Gib\u00b7bon", "be\u00b7such\u00b7te", "mich", "oft", ",", "auch", "ju\u00b7belt", "im", "Ro\u00b7gu\u00b7in\u00b7schen", "Saa\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$,", "ADV", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Eben ein deutscher Poet faunisch beim Blindekuhspiel.", "tokens": ["E\u00b7ben", "ein", "deut\u00b7scher", "Po\u00b7et", "fau\u00b7nisch", "beim", "Blin\u00b7de\u00b7kuh\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "wo