{"textgrid.poem.34358": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "83.", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dank Lavater Freude und Dank!", "tokens": ["Dank", "La\u00b7va\u00b7ter", "Freu\u00b7de", "und", "Dank", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "KON", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Meine Erwartungen \u00fcbertroffen,", "tokens": ["Mei\u00b7ne", "Er\u00b7war\u00b7tun\u00b7gen", "\u00fc\u00b7ber\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Welch eine Gottesaussicht offen!", "tokens": ["Welch", "ei\u00b7ne", "Got\u00b7tes\u00b7aus\u00b7sicht", "of\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O das Herz, das nicht versank", "tokens": ["O", "das", "Herz", ",", "das", "nicht", "ver\u00b7sank"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ART", "NN", "$,", "PRELS", "PTKNEG", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bey dem Hohnlachen, Dr\u00e4uen, Schm\u00e4hn,", "tokens": ["Bey", "dem", "Hohn\u00b7la\u00b7chen", ",", "Dr\u00e4u\u00b7en", ",", "Schm\u00e4hn", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Wie wirds nun getr\u00f6stet sich sehn!", "tokens": ["Wie", "wirds", "nun", "ge\u00b7tr\u00f6s\u00b7tet", "sich", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "VVPP", "PRF", "VVINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Ganze Geschlechter, V\u00f6lkeralter", "tokens": ["Gan\u00b7ze", "Ge\u00b7schlech\u00b7ter", ",", "V\u00f6l\u00b7ker\u00b7al\u00b7ter"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Mischen dich schon in ihre Psalter", "tokens": ["Mi\u00b7schen", "dich", "schon", "in", "ih\u00b7re", "Psal\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Oder knirschen dem Gericht,", "tokens": ["O\u00b7der", "knir\u00b7schen", "dem", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Dem r\u00e4chenden unwillkommenen Licht.", "tokens": ["Dem", "r\u00e4\u00b7chen\u00b7den", "un\u00b7will\u00b7kom\u00b7me\u00b7nen", "Licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Dank Lavater Freude und Dank!", "tokens": ["Dank", "La\u00b7va\u00b7ter", "Freu\u00b7de", "und", "Dank", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "KON", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "Thr\u00e4nen schw\u00e4rmen in meinen Gesang,", "tokens": ["Thr\u00e4\u00b7nen", "schw\u00e4r\u00b7men", "in", "mei\u00b7nen", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "Denn ich sehe vom Ost zum Belt", "tokens": ["Denn", "ich", "se\u00b7he", "vom", "Ost", "zum", "Belt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "APPRART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.14": {"text": "Schon die ", "tokens": ["Schon", "die"], "token_info": ["word", "word"], "pos": ["ADV", "ART"], "meter": "+-", "measure": "trochaic.single"}}}}}