{"textgrid.poem.56011": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Der Auferstandene", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er vermochte niemals bis zuletzt", "tokens": ["Er", "ver\u00b7moch\u00b7te", "nie\u00b7mals", "bis", "zu\u00b7letzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "ihr zu weigern oder abzuneinen,", "tokens": ["ihr", "zu", "wei\u00b7gern", "o\u00b7der", "ab\u00b7zu\u00b7nei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "KON", "VVIZU", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "da\u00df sie ihrer Liebe sich ber\u00fchme;", "tokens": ["da\u00df", "sie", "ih\u00b7rer", "Lie\u00b7be", "sich", "be\u00b7r\u00fch\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "und sie sank ans Kreuz in dem Kost\u00fcme", "tokens": ["und", "sie", "sank", "ans", "Kreuz", "in", "dem", "Kos\u00b7t\u00fc\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "--+-+--+--", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "eines Schmerzes, welches ganz besetzt", "tokens": ["ei\u00b7nes", "Schmer\u00b7zes", ",", "wel\u00b7ches", "ganz", "be\u00b7setzt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVPP"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "war mit ihrer Liebe gr\u00f6\u00dften Steinen.", "tokens": ["war", "mit", "ih\u00b7rer", "Lie\u00b7be", "gr\u00f6\u00df\u00b7ten", "Stei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Aber da sie dann, um ihn zu salben,", "tokens": ["A\u00b7ber", "da", "sie", "dann", ",", "um", "ihn", "zu", "sal\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "an das Grab kam, Tr\u00e4nen im Gesicht,", "tokens": ["an", "das", "Grab", "kam", ",", "Tr\u00e4\u00b7nen", "im", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "war er auferstanden ihrethalben,", "tokens": ["war", "er", "auf\u00b7er\u00b7stan\u00b7den", "ih\u00b7re\u00b7thal\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "da\u00df er seliger ihr sage: Nicht \u2013", "tokens": ["da\u00df", "er", "se\u00b7li\u00b7ger", "ihr", "sa\u00b7ge", ":", "Nicht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PPER", "VVFIN", "$.", "PTKNEG", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Sie begriff es erst in ihrer H\u00f6hle,", "tokens": ["Sie", "be\u00b7griff", "es", "erst", "in", "ih\u00b7rer", "H\u00f6h\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "wie er ihr, gest\u00e4rkt durch seinen Tod,", "tokens": ["wie", "er", "ihr", ",", "ge\u00b7st\u00e4rkt", "durch", "sei\u00b7nen", "Tod", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "$,", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "endlich das Erleichternde der \u00d6le", "tokens": ["end\u00b7lich", "das", "Er\u00b7leich\u00b7tern\u00b7de", "der", "\u00d6\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ART", "NN"], "meter": "+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "und des R\u00fchrens Vorgef\u00fchl verbot,", "tokens": ["und", "des", "R\u00fch\u00b7rens", "Vor\u00b7ge\u00b7f\u00fchl", "ver\u00b7bot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "um aus ihr die Liebende zu formen", "tokens": ["um", "aus", "ihr", "die", "Lie\u00b7ben\u00b7de", "zu", "for\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "APPR", "PPER", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "die sich nicht mehr zum Geliebten neigt,", "tokens": ["die", "sich", "nicht", "mehr", "zum", "Ge\u00b7lieb\u00b7ten", "neigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PRF", "PTKNEG", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "weil sie, hingerissen von enormen", "tokens": ["weil", "sie", ",", "hin\u00b7ge\u00b7ris\u00b7sen", "von", "en\u00b7or\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "VVPP", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "St\u00fcrmen, seine Stimme \u00fcbersteigt.", "tokens": ["St\u00fcr\u00b7men", ",", "sei\u00b7ne", "Stim\u00b7me", "\u00fc\u00b7bers\u00b7teigt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}