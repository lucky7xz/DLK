{"textgrid.poem.48977": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Heute scheid' ich, heute wandr' ich,", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heute scheid' ich, heute wandr' ich,", "tokens": ["Heu\u00b7te", "scheid'", "ich", ",", "heu\u00b7te", "wandr'", "ich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Keine Seele weint um mich.", "tokens": ["Kei\u00b7ne", "See\u00b7le", "weint", "um", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sind's nicht diese, sind's doch andre,", "tokens": ["Sin\u00b7d's", "nicht", "die\u00b7se", ",", "sin\u00b7d's", "doch", "and\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "PDS", "$,", "VAFIN", "ADV", "PIS", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Die da trauern, wenn ich wandre:", "tokens": ["Die", "da", "trau\u00b7ern", ",", "wenn", "ich", "wand\u00b7re", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Holder Schatz, ich denk' an dich.", "tokens": ["Hol\u00b7der", "Schatz", ",", "ich", "denk'", "an", "dich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf dem Bachstrom h\u00e4ngen Weiden,", "tokens": ["Auf", "dem", "Bach\u00b7strom", "h\u00e4n\u00b7gen", "Wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In den T\u00e4lern liegt der Schnee \u2013", "tokens": ["In", "den", "T\u00e4\u00b7lern", "liegt", "der", "Schnee", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Trautes Kind, da\u00df ich mu\u00df scheiden,", "tokens": ["Trau\u00b7tes", "Kind", ",", "da\u00df", "ich", "mu\u00df", "schei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KOUS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00df nun unsre Heimat meiden,", "tokens": ["Mu\u00df", "nun", "uns\u00b7re", "Hei\u00b7mat", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Tief im Herzen tut mir's weh.", "tokens": ["Tief", "im", "Her\u00b7zen", "tut", "mir's", "weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "NE", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hunderttausend Kugeln pfeifen", "tokens": ["Hun\u00b7dert\u00b7tau\u00b7send", "Ku\u00b7geln", "pfei\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["CARD", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcber meinem Haupte hin \u2013", "tokens": ["\u00dc\u00b7ber", "mei\u00b7nem", "Haup\u00b7te", "hin", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo ich fall', scharrt man mich nieder,", "tokens": ["Wo", "ich", "fall'", ",", "scharrt", "man", "mich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVFIN", "PIS", "PPER", "PTKVZ", "$,"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Ohne Klang und ohne Lieder,", "tokens": ["Oh\u00b7ne", "Klang", "und", "oh\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Niemand fraget, wer ich bin.", "tokens": ["Nie\u00b7mand", "fra\u00b7get", ",", "wer", "ich", "bin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Du allein wirst um mich weinen,", "tokens": ["Du", "al\u00b7lein", "wirst", "um", "mich", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "APPR", "PRF", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Siehst du meinen Totenschein.", "tokens": ["Siehst", "du", "mei\u00b7nen", "To\u00b7ten\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Trautes Kind, sollt' er erscheinen,", "tokens": ["Trau\u00b7tes", "Kind", ",", "sollt'", "er", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tu' im Stillen um mich weinen,", "tokens": ["Tu'", "im", "Stil\u00b7len", "um", "mich", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "APPR", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und gedenk' auch immer mein.", "tokens": ["Und", "ge\u00b7denk'", "auch", "im\u00b7mer", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Heb' zum Himmel unsern Kleinen,", "tokens": ["Heb'", "zum", "Him\u00b7mel", "un\u00b7sern", "Klei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schluchz': \u00bbNun tot der Vater dein!\u00ab", "tokens": ["Schluch\u00b7z'", ":", "\u00bb", "Nun", "tot", "der", "Va\u00b7ter", "dein", "!", "\u00ab"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "$(", "ADV", "ADJD", "ART", "NN", "PPOSAT", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lehr' in beten! Gib ihm Segen!", "tokens": ["Lehr'", "in", "be\u00b7ten", "!", "Gib", "ihm", "Se\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "VVINF", "$.", "VVIMP", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Reich' ihm seines Vaters Degen!", "tokens": ["Reich'", "ihm", "sei\u00b7nes", "Va\u00b7ters", "De\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mag die Welt sein Vater sein.", "tokens": ["Mag", "die", "Welt", "sein", "Va\u00b7ter", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "H\u00f6rst? Die Trommel ruft zum Scheiden:", "tokens": ["H\u00f6rst", "?", "Die", "Trom\u00b7mel", "ruft", "zum", "Schei\u00b7den", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dr\u00fcck' ich dir die wei\u00dfe Hand!", "tokens": ["Dr\u00fcck", "ich", "dir", "die", "wei\u00b7\u00dfe", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Still' die Tr\u00e4nen! La\u00df mich scheiden!", "tokens": ["Still'", "die", "Tr\u00e4\u00b7nen", "!", "La\u00df", "mich", "schei\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00df nun f\u00fcr die Ehre streiten,", "tokens": ["Mu\u00df", "nun", "f\u00fcr", "die", "Eh\u00b7re", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Streiten f\u00fcr das Vaterland.", "tokens": ["Strei\u00b7ten", "f\u00fcr", "das", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Sollt' ich unter freiem Himmel", "tokens": ["Sollt'", "ich", "un\u00b7ter", "frei\u00b7em", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlafen in der Feldschlacht ein \u2013", "tokens": ["Schla\u00b7fen", "in", "der", "Feld\u00b7schlacht", "ein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Soll aus meinem Grabe bl\u00fchen,", "tokens": ["Soll", "aus", "mei\u00b7nem", "Gra\u00b7be", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Soll auf meinem Grabe gl\u00fchen", "tokens": ["Soll", "auf", "mei\u00b7nem", "Gra\u00b7be", "gl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bl\u00fcmchen s\u00fc\u00df:", "tokens": ["Bl\u00fcm\u00b7chen", "s\u00fc\u00df", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Vergi\u00df nicht mein.", "tokens": ["Ver\u00b7gi\u00df", "nicht", "mein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "PPOSAT", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}