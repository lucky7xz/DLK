{"textgrid.poem.25012": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Lerchen, sch\u00fcttelt den Tau von der Brust!", "tokens": ["Ihr", "Ler\u00b7chen", ",", "sch\u00fct\u00b7telt", "den", "Tau", "von", "der", "Brust", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Fliegt auf aus Furche und gr\u00fcnender Saat,", "tokens": ["Fliegt", "auf", "aus", "Fur\u00b7che", "und", "gr\u00fc\u00b7nen\u00b7der", "Saat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Hoch \u00fcber der h\u00f6chsten Berge Grat", "tokens": ["Hoch", "\u00fc\u00b7ber", "der", "h\u00f6chs\u00b7ten", "Ber\u00b7ge", "Grat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Schwingt euch empor in jubelnder Lust", "tokens": ["Schwingt", "euch", "em\u00b7por", "in", "ju\u00b7beln\u00b7der", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Und jauchzt es in alle Lande hinein:", "tokens": ["Und", "jauchzt", "es", "in", "al\u00b7le", "Lan\u00b7de", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Flammt auf, ihr Alpen, golden und rot!", "tokens": ["Flammt", "auf", ",", "ihr", "Al\u00b7pen", ",", "gol\u00b7den", "und", "rot", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "ADJA", "KON", "ADJD", "$."], "meter": "---+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Von Zacke zu Zacke und Felsenrand", "tokens": ["Von", "Za\u00b7cke", "zu", "Za\u00b7cke", "und", "Fel\u00b7sen\u00b7rand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "La\u00dft schie\u00dfen die Strahlen, bis hoch der Brand", "tokens": ["La\u00dft", "schie\u00b7\u00dfen", "die", "Strah\u00b7len", ",", "bis", "hoch", "der", "Brand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "VVFIN", "ART", "NN", "$,", "KOUS", "ADJD", "ART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von Gletschern und Eisalt\u00e4ren loht,", "tokens": ["Von", "Glet\u00b7schern", "und", "Ei\u00b7sal\u00b7t\u00e4\u00b7ren", "loht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und leuchtet's in alle Lande hinein:", "tokens": ["Und", "leuch\u00b7tet's", "in", "al\u00b7le", "Lan\u00b7de", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}}}}