{"textgrid.poem.35118": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Ruhe", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ruh aus von deinem Tagewerke", "tokens": ["Ruh", "aus", "von", "dei\u00b7nem", "Ta\u00b7ge\u00b7wer\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Am Abend, wenn du m\u00fcde bist.", "tokens": ["Am", "A\u00b7bend", ",", "wenn", "du", "m\u00fc\u00b7de", "bist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du hast es n\u00f6thig, aber merke,", "tokens": ["Du", "hast", "es", "n\u00f6\u00b7thig", ",", "a\u00b7ber", "mer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df es zur Vorbereitung ist.", "tokens": ["Da\u00df", "es", "zur", "Vor\u00b7be\u00b7rei\u00b7tung", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erhole dich von deinen Sorgen,", "tokens": ["Er\u00b7ho\u00b7le", "dich", "von", "dei\u00b7nen", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch schlafe ohne sie nicht ein;", "tokens": ["Doch", "schla\u00b7fe", "oh\u00b7ne", "sie", "nicht", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vielleicht hast du am n\u00e4chsten Morgen", "tokens": ["Viel\u00b7leicht", "hast", "du", "am", "n\u00e4chs\u00b7ten", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Schon keine Zeit mehr, m\u00fcd zu sein.", "tokens": ["Schon", "kei\u00b7ne", "Zeit", "mehr", ",", "m\u00fcd", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADV", "$,", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ruh aus von deinem Lebenswerke", "tokens": ["Ruh", "aus", "von", "dei\u00b7nem", "Le\u00b7bens\u00b7wer\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Alter, wenn du m\u00fcde bist.", "tokens": ["Im", "Al\u00b7ter", ",", "wenn", "du", "m\u00fc\u00b7de", "bist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du darfst es thun, doch aber merke,", "tokens": ["Du", "darfst", "es", "thun", ",", "doch", "a\u00b7ber", "mer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$,", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df dies die letzte Ruhe ist.", "tokens": ["Da\u00df", "dies", "die", "letz\u00b7te", "Ru\u00b7he", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es wird die Arbeit dich begleiten", "tokens": ["Es", "wird", "die", "Ar\u00b7beit", "dich", "be\u00b7glei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In jenes andre Land hinein.", "tokens": ["In", "je\u00b7nes", "and\u00b7re", "Land", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dort ist es aus mit unsern Zeiten,", "tokens": ["Dort", "ist", "es", "aus", "mit", "un\u00b7sern", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Auch mit der Zeit zum M\u00fcde-sein.", "tokens": ["Auch", "mit", "der", "Zeit", "zum", "M\u00fc\u00b7de\u00b7sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}