{"textgrid.poem.34681": {"metadata": {"author": {"name": "K\u00f6rner, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "An L., als Dank f\u00fcr das Feldzeichen", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Ritter liebte stets die deutschen Weisen.", "tokens": ["Der", "Rit\u00b7ter", "lieb\u00b7te", "stets", "die", "deut\u00b7schen", "Wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zog er hinaus zum Streit f\u00fcrs Vaterland,", "tokens": ["Zog", "er", "hin\u00b7aus", "zum", "Streit", "f\u00fcrs", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Sklavenketten mutig zu zerrei\u00dfen,", "tokens": ["Die", "Skla\u00b7ven\u00b7ket\u00b7ten", "mu\u00b7tig", "zu", "zer\u00b7rei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durft' frohen Mutes z\u00fccken er sein Eisen,", "tokens": ["Durft'", "fro\u00b7hen", "Mu\u00b7tes", "z\u00fc\u00b7cken", "er", "sein", "Ei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "War er geschickt von sch\u00f6ner Frauen Hand.", "tokens": ["War", "er", "ge\u00b7schickt", "von", "sch\u00f6\u00b7ner", "Frau\u00b7en", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Sitte soll die alte Kraft beweisen,", "tokens": ["Die", "Sit\u00b7te", "soll", "die", "al\u00b7te", "Kraft", "be\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Noch ist der Sieg der Sch\u00f6nheit zugewandt:", "tokens": ["Noch", "ist", "der", "Sieg", "der", "Sch\u00f6n\u00b7heit", "zu\u00b7ge\u00b7wandt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Sie nur allein kann rohen Mut verkl\u00e4ren.", "tokens": ["Sie", "nur", "al\u00b7lein", "kann", "ro\u00b7hen", "Mut", "ver\u00b7kl\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VMFIN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Auch mich hat sie geschm\u00fcckt \u2013 ich trug ihr Band \u2013", "tokens": ["Auch", "mich", "hat", "sie", "ge\u00b7schm\u00fcckt", "\u2013", "ich", "trug", "ihr", "Band", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PPER", "VVPP", "$(", "PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "Als ihren Ritter will ich mich bew\u00e4hren.", "tokens": ["Als", "ih\u00b7ren", "Rit\u00b7ter", "will", "ich", "mich", "be\u00b7w\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}