{"textgrid.poem.56707": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Das Opfer", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Diesmal sollst du noch entrinnen,", "tokens": ["Dies\u00b7mal", "sollst", "du", "noch", "ent\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "heil aus meinen H\u00e4nden schl\u00fcpfen,", "tokens": ["heil", "aus", "mei\u00b7nen", "H\u00e4n\u00b7den", "schl\u00fcp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "morgen werd ich dich einspinnen,", "tokens": ["mor\u00b7gen", "werd", "ich", "dich", "ein\u00b7spin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "dich an meine Feder kn\u00fcpfen,", "tokens": ["dich", "an", "mei\u00b7ne", "Fe\u00b7der", "kn\u00fcp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "nackt auf meine Feder spie\u00dfen", "tokens": ["nackt", "auf", "mei\u00b7ne", "Fe\u00b7der", "spie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "werde ich dein kleines Herz,", "tokens": ["wer\u00b7de", "ich", "dein", "klei\u00b7nes", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Licht soll von ihm niederflie\u00dfen", "tokens": ["Licht", "soll", "von", "ihm", "nie\u00b7der\u00b7flie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "APPR", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "str\u00f6mend ewig tiefenw\u00e4rts,", "tokens": ["str\u00f6\u00b7mend", "e\u00b7wig", "tie\u00b7fen\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ADJA", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "sch\u00fcren wirst du ihre Glut,", "tokens": ["sch\u00fc\u00b7ren", "wirst", "du", "ih\u00b7re", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVINF", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "da\u00df du meine Fackel wirst,", "tokens": ["da\u00df", "du", "mei\u00b7ne", "Fa\u00b7ckel", "wirst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "bis in unsrer Feuerflut", "tokens": ["bis", "in", "uns\u00b7rer", "Feu\u00b7er\u00b7flut"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Feder sowie Herz zerbirst.", "tokens": ["Fe\u00b7der", "so\u00b7wie", "Herz", "zer\u00b7birst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}