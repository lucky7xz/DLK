{"textgrid.poem.63753": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Barf\u00fc\u00dfig, braun, das Haar zerzaust vom Wind,", "tokens": ["Bar\u00b7f\u00fc\u00b7\u00dfig", ",", "braun", ",", "das", "Haar", "zer\u00b7zaust", "vom", "Wind", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Trieb sie ihr Eselchen mit sonderbaren", "tokens": ["Trieb", "sie", "ihr", "E\u00b7sel\u00b7chen", "mit", "son\u00b7der\u00b7ba\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "APPR", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Zurufen an. Da wir gespr\u00e4chig waren,", "tokens": ["Zu\u00b7ru\u00b7fen", "an", ".", "Da", "wir", "ge\u00b7spr\u00e4\u00b7chig", "wa\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So l\u00f6st' auch ihr das Z\u00fcnglein sich geschwind.", "tokens": ["So", "l\u00f6st'", "auch", "ihr", "das", "Z\u00fcn\u00b7glein", "sich", "ge\u00b7schwind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "ART", "NN", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "\u00bbconcetta hei\u00df' ich. Hier auf Capri sind", "tokens": ["\u00bb", "con\u00b7cet\u00b7ta", "hei\u00df'", "ich", ".", "Hier", "auf", "Cap\u00b7ri", "sind"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "$.", "ADV", "APPR", "NE", "VAFIN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Die meisten M\u00e4dchen h\u00fcbsch. Vor wenig Jahren", "tokens": ["Die", "meis\u00b7ten", "M\u00e4d\u00b7chen", "h\u00fcbsch", ".", "Vor", "we\u00b7nig", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "ADJD", "$.", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kam ein Milordo \u00fcbers Meer gefahren,", "tokens": ["Kam", "ein", "Mi\u00b7lor\u00b7do", "\u00fc\u00b7bers", "Meer", "ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NE", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der nahm zur Frau sich ein Capreser Kind.", "tokens": ["Der", "nahm", "zur", "Frau", "sich", "ein", "Cap\u00b7re\u00b7ser", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "PRF", "ART", "NN", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Was half das Gl\u00fcck ihr? Weil's im Norden schneite,", "tokens": ["Was", "half", "das", "Gl\u00fcck", "ihr", "?", "Weil's", "im", "Nor\u00b7den", "schnei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PPER", "$.", "NE", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Starb sie vor Frost und Heimweh, ", "tokens": ["Starb", "sie", "vor", "Frost", "und", "Heim\u00b7weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der arme Herr! ", "tokens": ["Der", "ar\u00b7me", "Herr", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Man sagt, nun komm' er wieder, sich die zweite", "tokens": ["Man", "sagt", ",", "nun", "komm'", "er", "wie\u00b7der", ",", "sich", "die", "zwei\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "$,", "PRF", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu holen.\u00ab \u2013 H\u00e4ttest ", "tokens": ["Zu", "ho\u00b7len", ".", "\u00ab", "\u2013", "H\u00e4t\u00b7test"], "token_info": ["word", "word", "punct", "punct", "punct", "word"], "pos": ["PTKZU", "VVINF", "$.", "$(", "$(", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und sei, ganz ernsthaft: ", "tokens": ["Und", "sei", ",", "ganz", "ernst\u00b7haft", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}