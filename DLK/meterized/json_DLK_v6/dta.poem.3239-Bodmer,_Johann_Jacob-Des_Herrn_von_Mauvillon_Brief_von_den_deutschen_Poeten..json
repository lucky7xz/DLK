{"dta.poem.3239": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Des Herrn von Mauvillon Brief  \n von den deutschen Poeten.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1742", "urn": "urn:nbn:de:kobv:b4-200905198490", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Besingen de Croix und Royen im Latein,", "tokens": ["Be\u00b7sin\u00b7gen", "de", "Croix", "und", "Ro\u00b7yen", "im", "La\u00b7tein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NE", "KON", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sind sie schon, wie es scheint, zur Hochzeit nicht gebeten.", "tokens": ["Sind", "sie", "schon", ",", "wie", "es", "scheint", ",", "zur", "Hoch\u00b7zeit", "nicht", "ge\u00b7be\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,", "APPRART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In eben diesem Gedichte:", "tokens": ["In", "e\u00b7ben", "die\u00b7sem", "Ge\u00b7dich\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PDAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Vom Vater, Sohn u. Geist, u. von des Glaubens Grund", "tokens": ["Vom", "Va\u00b7ter", ",", "Sohn", "u.", "Geist", ",", "u.", "von", "des", "Glau\u00b7bens", "Grund"], "token_info": ["word", "word", "punct", "word", "abbreviation", "word", "punct", "abbreviation", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "NN", "APPR", "NN", "$,", "KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Hat Faithfull Th\u00e4t ein Buch in Versen f\u00fcrgetragen,", "tokens": ["Hat", "Faith\u00b7full", "Th\u00e4t", "ein", "Buch", "in", "Ver\u00b7sen", "f\u00fcr\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "NN", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die\u00df macht nun Wagner zwar in deutschen Reimen kund,", "tokens": ["Die\u00df", "macht", "nun", "Wag\u00b7ner", "zwar", "in", "deut\u00b7schen", "Rei\u00b7men", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "NE", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Allein sie klingen meist, wie ein noch neuer Wagen.", "tokens": ["Al\u00b7lein", "sie", "klin\u00b7gen", "meist", ",", "wie", "ein", "noch", "neu\u00b7er", "Wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "$,", "PWAV", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}