{"textgrid.poem.50993": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Gewitter im Mai", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Bl\u00fcten schwamm das Fr\u00fchlingsland,", "tokens": ["In", "Bl\u00fc\u00b7ten", "schwamm", "das", "Fr\u00fch\u00b7lings\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es wogte wei\u00df in schw\u00fcler Ruh;", "tokens": ["Es", "wog\u00b7te", "wei\u00df", "in", "schw\u00fc\u00b7ler", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der dunkle feuchte Himmel band", "tokens": ["Der", "dunk\u00b7le", "feuch\u00b7te", "Him\u00b7mel", "band"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir schwer die feuchten Augen zu.", "tokens": ["Mir", "schwer", "die", "feuch\u00b7ten", "Au\u00b7gen", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Voll Reu und Leid hatt ich den Mai", "tokens": ["Voll", "Reu", "und", "Leid", "hatt", "ich", "den", "Mai"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "KON", "NN", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gegr\u00fc\u00dft und seinen bunten Flor;", "tokens": ["Ge\u00b7gr\u00fc\u00dft", "und", "sei\u00b7nen", "bun\u00b7ten", "Flor", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun zog er mir im Schlaf vorbei,", "tokens": ["Nun", "zog", "er", "mir", "im", "Schlaf", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vertr\u00e4umt von dem vergr\u00e4mten Tor!", "tokens": ["Ver\u00b7tr\u00e4umt", "von", "dem", "ver\u00b7gr\u00e4m\u00b7ten", "Tor", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da war ein Donnerschlag geschehn,", "tokens": ["Da", "war", "ein", "Don\u00b7ner\u00b7schlag", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein einziger; den Berg entlang", "tokens": ["Ein", "ein\u00b7zi\u00b7ger", ";", "den", "Berg", "ent\u00b7lang"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$.", "ART", "NN", "APPO"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "H\u00f6rt ich Erwachender vergehn", "tokens": ["H\u00f6rt", "ich", "Er\u00b7wa\u00b7chen\u00b7der", "ver\u00b7gehn"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erschrocken seinen letzten Klang:", "tokens": ["Er\u00b7schro\u00b7cken", "sei\u00b7nen", "letz\u00b7ten", "Klang", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbsteh auf! steh auf! entraffe dich", "tokens": ["\u00bb", "steh", "auf", "!", "steh", "auf", "!", "en\u00b7traf\u00b7fe", "dich"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "VVFIN", "PTKVZ", "$.", "VVFIN", "PTKVZ", "$.", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der tr\u00e4gen, tatenlosen Reu!\u00ab", "tokens": ["Der", "tr\u00e4\u00b7gen", ",", "ta\u00b7ten\u00b7lo\u00b7sen", "Reu", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "VVINF", "$,", "VVFIN", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch Tal und Herz ein Schauer strich,", "tokens": ["Durch", "Tal", "und", "Herz", "ein", "Schau\u00b7er", "strich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Leben bl\u00fchte frisch und neu.", "tokens": ["Das", "Le\u00b7ben", "bl\u00fch\u00b7te", "frisch", "und", "neu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}