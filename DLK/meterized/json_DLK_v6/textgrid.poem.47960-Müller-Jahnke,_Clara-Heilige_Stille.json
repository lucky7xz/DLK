{"textgrid.poem.47960": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Heilige Stille", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im d\u00e4mmernden Tale,", "tokens": ["Im", "d\u00e4m\u00b7mern\u00b7den", "Ta\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "da wallen und wogen,", "tokens": ["da", "wal\u00b7len", "und", "wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "die weiten Gefilde", "tokens": ["die", "wei\u00b7ten", "Ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "allm\u00e4hlich verh\u00fcllend", "tokens": ["all\u00b7m\u00e4h\u00b7lich", "ver\u00b7h\u00fcl\u00b7lend"], "token_info": ["word", "word"], "pos": ["ADJD", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "mit bl\u00e4ulichem Duft,", "tokens": ["mit", "bl\u00e4u\u00b7li\u00b7chem", "Duft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "die Spitzen der Berge,", "tokens": ["die", "Spit\u00b7zen", "der", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "der fernen, verschleiernd,", "tokens": ["der", "fer\u00b7nen", ",", "ver\u00b7schlei\u00b7ernd", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "die Nebel des Abends,", "tokens": ["die", "Ne\u00b7bel", "des", "A\u00b7bends", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "die Schatten der Nacht.", "tokens": ["die", "Schat\u00b7ten", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Auf dunklem Gew\u00e4sser", "tokens": ["Auf", "dunk\u00b7lem", "Ge\u00b7w\u00e4s\u00b7ser"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "aufleuchten gleich Nixen", "tokens": ["auf\u00b7leuch\u00b7ten", "gleich", "Ni\u00b7xen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADV", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "die Lilien, die bleichen,", "tokens": ["die", "Li\u00b7li\u00b7en", ",", "die", "blei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "und beugen und neigen", "tokens": ["und", "beu\u00b7gen", "und", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVINF", "KON", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "zum Wasser hernieder", "tokens": ["zum", "Was\u00b7ser", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "PTKVZ"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "und heben dann wieder,", "tokens": ["und", "he\u00b7ben", "dann", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "mit blitzenden Tropfen,", "tokens": ["mit", "blit\u00b7zen\u00b7den", "Trop\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "mit Sternen bes\u00e4t,", "tokens": ["mit", "Ster\u00b7nen", "be\u00b7s\u00e4t", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "in dunkelnde L\u00fcfte", "tokens": ["in", "dun\u00b7keln\u00b7de", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "den duftenden Kelch.", "tokens": ["den", "duf\u00b7ten\u00b7den", "Kelch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Und fern aus dem Walde,", "tokens": ["Und", "fern", "aus", "dem", "Wal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "vom Rauschen des Nachtwinds", "tokens": ["vom", "Rau\u00b7schen", "des", "Nacht\u00b7winds"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "harmonisch getragen,", "tokens": ["har\u00b7mo\u00b7nisch", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "ert\u00f6net in s\u00fc\u00dfen", "tokens": ["er\u00b7t\u00f6\u00b7net", "in", "s\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "in r\u00fchrenden Weisen", "tokens": ["in", "r\u00fch\u00b7ren\u00b7den", "Wei\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "der Nachtigall Sang", "tokens": ["der", "Nach\u00b7ti\u00b7gall", "Sang"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "und haucht in den Frieden", "tokens": ["und", "haucht", "in", "den", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "der schlummernden Flur", "tokens": ["der", "schlum\u00b7mern\u00b7den", "Flur"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "hinschmelzende Sehnsucht.", "tokens": ["hin\u00b7schmel\u00b7zen\u00b7de", "Sehn\u00b7sucht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Kein Menschenwort st\u00f6rt", "tokens": ["Kein", "Men\u00b7schen\u00b7wort", "st\u00f6rt"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "die heilige Stille, \u2013 \u2013", "tokens": ["die", "hei\u00b7li\u00b7ge", "Stil\u00b7le", ",", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "$(", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "und ferne im Osten", "tokens": ["und", "fer\u00b7ne", "im", "Os\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "erhebt sich die Sichel", "tokens": ["er\u00b7hebt", "sich", "die", "Si\u00b7chel"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "des Mondes in silbernem", "tokens": ["des", "Mon\u00b7des", "in", "sil\u00b7ber\u00b7nem"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.6": {"text": "n\u00e4chtlichen Glanz.", "tokens": ["n\u00e4cht\u00b7li\u00b7chen", "Glanz", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Die Nachtigall schweigt . . .", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "schweigt", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$.", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "und tr\u00e4umerisch senken", "tokens": ["und", "tr\u00e4u\u00b7me\u00b7risch", "sen\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "die Lilien im Weiher,", "tokens": ["die", "Li\u00b7li\u00b7en", "im", "Wei\u00b7her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "ber\u00fchrt von dem Strahle", "tokens": ["be\u00b7r\u00fchrt", "von", "dem", "Strah\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "des Mondes, ihr Haupt.", "tokens": ["des", "Mon\u00b7des", ",", "ihr", "Haupt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}