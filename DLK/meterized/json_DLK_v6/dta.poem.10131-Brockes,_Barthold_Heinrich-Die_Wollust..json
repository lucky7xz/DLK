{"dta.poem.10131": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Wollust.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Alle Wollust in der Welt hat ihr s\u00fcssestes am Ende,", "tokens": ["Al\u00b7le", "Wol\u00b7lust", "in", "der", "Welt", "hat", "ihr", "s\u00fcs\u00b7ses\u00b7tes", "am", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "VAFIN", "PPOSAT", "ADJA", "APPRART", "NN", "$,"], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dieses ", "tokens": ["Die\u00b7ses"], "token_info": ["word"], "pos": ["PDAT"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Verdru\u00df.", "tokens": ["Ver\u00b7dru\u00df", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Da\u00df man sich demnach dar\u00fcber nicht so sehr verwundern", "tokens": ["Da\u00df", "man", "sich", "dem\u00b7nach", "da\u00b7r\u00fc\u00b7ber", "nicht", "so", "sehr", "ver\u00b7wun\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PRF", "PAV", "PAV", "PTKNEG", "ADV", "ADV", "VVINF"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.5": {"text": "mu\u00df,", "tokens": ["mu\u00df", ","], "token_info": ["word", "punct"], "pos": ["VMFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Wenn, auf Erden, Freud\u2019 und Lust, Anmuth, S\u00fc\u00dfigkeit,", "tokens": ["Wenn", ",", "auf", "Er\u00b7den", ",", "Freud'", "und", "Lust", ",", "An\u00b7muth", ",", "S\u00fc\u00b7\u00dfig\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "NN", "$,", "NN", "KON", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "--+-+-++-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Vergn\u00fcgen", "tokens": ["Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Von so kurtzer Dauer sind, und nur gar zu schnell vergehn:", "tokens": ["Von", "so", "kurt\u00b7zer", "Dau\u00b7er", "sind", ",", "und", "nur", "gar", "zu", "schnell", "ver\u00b7gehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "VAFIN", "$,", "KON", "ADV", "ADV", "PTKA", "ADJD", "VVINF", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Da sie fast zu gleicher Zeit untergehen und entstehn,", "tokens": ["Da", "sie", "fast", "zu", "glei\u00b7cher", "Zeit", "un\u00b7ter\u00b7ge\u00b7hen", "und", "ent\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ADJA", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Schon in der Gebuhrt ersticken, und, so bald sie da, ver-", "tokens": ["Schon", "in", "der", "Ge\u00b7buhrt", "er\u00b7sti\u00b7cken", ",", "und", ",", "so", "bald", "sie", "da", ",", "ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "$,", "KON", "$,", "ADV", "ADV", "PPER", "ADV", "$,", "TRUNC"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "fliegen.", "tokens": ["flie\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}