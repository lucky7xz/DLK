{"dta.poem.7821": {"metadata": {"author": {"name": "Birken, Sigmund von", "birth": "N.A.", "death": "N.A."}, "title": "Pegnesische  \n Fr\u00fclings-Freude.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1665", "urn": "urn:nbn:de:kobv:b4-200905198447", "language": ["de:0.85", "cy:0.14"], "booktitle": "Birken, Sigmund von: Pegnesische Gespr\u00e4chspiel-Gesellschaft von Nymfen und Hirten. N\u00fcrnberg, 1665."}, "poem": {"stanza.1": {"line.1": {"text": "Du wirst mir beyfallen/", "tokens": ["Du", "wirst", "mir", "bey\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "mein Fremdling!", "tokens": ["mein", "Fremd\u00b7ling", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "ach! eine unbillige W\u00fcrkung/", "tokens": ["ach", "!", "ei\u00b7ne", "un\u00b7bil\u00b7li\u00b7ge", "W\u00fcr\u00b7kung", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "der leidigen Sterblichkeit!", "tokens": ["der", "lei\u00b7di\u00b7gen", "Sterb\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Lise/ und urtheile!", "tokens": ["Li\u00b7se", "/", "und", "ur\u00b7thei\u00b7le", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$(", "KON", "VVFIN", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.6": {"text": "Alles/ was alle Wele bewundert/", "tokens": ["Al\u00b7les", "/", "was", "al\u00b7le", "We\u00b7le", "be\u00b7wun\u00b7dert", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$(", "PWS", "PIAT", "NN", "VVPP", "$("], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "bewohnet diesen \u00e4ngen und einigen", "tokens": ["be\u00b7woh\u00b7net", "die\u00b7sen", "\u00e4n\u00b7gen", "und", "ei\u00b7ni\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PDAT", "ADJA", "KON", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Grabschrein:", "tokens": ["Grab\u00b7schrein", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "oder vielmehr/", "tokens": ["o\u00b7der", "viel\u00b7mehr", "/"], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADV", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Alles dasselbe hatte beysammen/", "tokens": ["Al\u00b7les", "das\u00b7sel\u00b7be", "hat\u00b7te", "bey\u00b7sam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PDAT", "VAFIN", "VVPP", "$("], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.11": {"text": "die diesen Sarg bewohnet/", "tokens": ["die", "die\u00b7sen", "Sarg", "be\u00b7woh\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "die Hochf\u00fcrtrefflichste", "tokens": ["die", "Hoch\u00b7f\u00fcr\u00b7treff\u00b7lichs\u00b7te"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.13": {"text": "Emilia/ Gr\u00e4fin\u0303 vo\u0303 Holland-Brederode;", "tokens": ["E\u00b7mi\u00b7lia", "/", "Gr\u00e4\u00b7fi\u00f1", "v\u00f5", "Hol\u00b7lan\u00b7dB\u00b7re\u00b7de\u00b7ro\u00b7de", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "XY", "XY", "NN", "$."], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.14": {"text": "von der Fr. Mutter/", "tokens": ["von", "der", "Fr.", "Mut\u00b7ter", "/"], "token_info": ["word", "word", "abbreviation", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "aus dem F\u00fcrstlichen Haus Nassau:", "tokens": ["aus", "dem", "F\u00fcrst\u00b7li\u00b7chen", "Haus", "Nas\u00b7sau", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NE", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.16": {"text": "Dannenhero von Stande und Stam\u0304en/", "tokens": ["Dan\u00b7nen\u00b7he\u00b7ro", "von", "Stan\u00b7de", "und", "Stam\u0304en", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "NN", "KON", "NN", "$("], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.17": {"text": "nicht nur Wohl- sondern Hochgebohren.", "tokens": ["nicht", "nur", "Wohl", "son\u00b7dern", "Hoch\u00b7ge\u00b7boh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Von Gem\u00fcte/", "tokens": ["Von", "Ge\u00b7m\u00fc\u00b7te", "/"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.19": {"text": "welches/ aus des Mundes un\u0304 der Stirne", "tokens": ["wel\u00b7ches", "/", "aus", "des", "Mun\u00b7des", "un\u0304", "der", "Stir\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "$(", "APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Doppel Fenster/ hervorleuchtete/", "tokens": ["Dop\u00b7pel", "Fens\u00b7ter", "/", "her\u00b7vor\u00b7leuch\u00b7te\u00b7te", "/"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "NN", "$(", "VVFIN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.21": {"text": "ein Tempel aller Tugenden/", "tokens": ["ein", "Tem\u00b7pel", "al\u00b7ler", "Tu\u00b7gen\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.22": {"text": "und mit einem Wort:", "tokens": ["und", "mit", "ei\u00b7nem", "Wort", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.23": {"text": "Die Allervollkommenste.", "tokens": ["Die", "Al\u00b7ler\u00b7voll\u00b7kom\u00b7mens\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Von Gestalt des Leibes/", "tokens": ["Von", "Ge\u00b7stalt", "des", "Lei\u00b7bes", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.25": {"text": "dessen Zierde di\u00df Geschlecht als Eigen", "tokens": ["des\u00b7sen", "Zier\u00b7de", "di\u00df", "Ge\u00b7schlecht", "als", "Ei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PDS", "NN", "KOUS", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.26": {"text": "das Allersch\u00f6nste Frauenbild/", "tokens": ["das", "Al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Frau\u00b7en\u00b7bild", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "und der sch\u00f6nsten Tugend", "tokens": ["und", "der", "sch\u00f6ns\u00b7ten", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.28": {"text": "gleich-sch\u00f6ne Wohnung:", "tokens": ["gleich\u00b7sch\u00f6\u00b7ne", "Woh\u00b7nung", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.29": {"text": "Welche noch sch\u00f6ner wurde/", "tokens": ["Wel\u00b7che", "noch", "sch\u00f6\u00b7ner", "wur\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VAFIN", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.30": {"text": "durch der Sitten/", "tokens": ["durch", "der", "Sit\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.31": {"text": "mit angebohrner Geist-hoheit", "tokens": ["mit", "an\u00b7ge\u00b7bohr\u00b7ner", "Geist\u00b7ho\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.32": {"text": "Freundseeligkeit/", "tokens": ["Freund\u00b7see\u00b7lig\u00b7keit", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.33": {"text": "die bey den Anschauern", "tokens": ["die", "bey", "den", "An\u00b7schau\u00b7ern"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.34": {"text": "zugleich Liebe und Ehrfurcht w\u00fcrkte.", "tokens": ["zu\u00b7gleich", "Lie\u00b7be", "und", "Ehr\u00b7furcht", "w\u00fcrk\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.35": {"text": "Auch das Gl\u00fcck/", "tokens": ["Auch", "das", "Gl\u00fcck", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.36": {"text": "welches sonst mit der Tugend", "tokens": ["wel\u00b7ches", "sonst", "mit", "der", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.37": {"text": "selten Freundschaft machet/", "tokens": ["sel\u00b7ten", "Freund\u00b7schaft", "ma\u00b7chet", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.38": {"text": "konde nicht unterlassen/", "tokens": ["kon\u00b7de", "nicht", "un\u00b7ter\u00b7las\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "VVINF", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.39": {"text": "mit seine\u0304 Gabe\u0304 sie reichlich auszusteure", "tokens": ["mit", "sein\u0113", "Gab\u0113", "sie", "reich\u00b7lich", "aus\u00b7zu\u00b7steu\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "ADJD", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.40": {"text": "Sie ward zweymal/", "tokens": ["Sie", "ward", "zwey\u00b7mal", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.41": {"text": "und zwar Erstlich als Freulinn/", "tokens": ["und", "zwar", "Erst\u00b7lich", "als", "Freu\u00b7linn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "KOKOM", "NN", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.42": {"text": "an den Hoch- und Wohlgebornen Herrn", "tokens": ["an", "den", "Hoch", "und", "Wohl\u00b7ge\u00b7bor\u00b7nen", "Herrn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "TRUNC", "KON", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.43": {"text": "Herrn", "tokens": ["Herrn"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.44": {"text": "Letzlich/ als Wittib/", "tokens": ["Letz\u00b7lich", "/", "als", "Wit\u00b7tib", "/"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$(", "KOUS", "NN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.45": {"text": "an den auch Hoch- und Wohlgebornen", "tokens": ["an", "den", "auch", "Hoch", "und", "Wohl\u00b7ge\u00b7bor\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "TRUNC", "KON", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.46": {"text": "Herrn", "tokens": ["Herrn"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.47": {"text": "derer Jenen/ sie/ traurig vorangeschicket;", "tokens": ["de\u00b7rer", "Je\u00b7nen", "/", "sie", "/", "trau\u00b7rig", "vor\u00b7an\u00b7ge\u00b7schi\u00b7cket", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "NN", "$(", "PPER", "$(", "ADJD", "VVFIN", "$."], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.48": {"text": "Diesen aber h\u00f6chstbetr\u016fbt hinterlassen:", "tokens": ["Die\u00b7sen", "a\u00b7ber", "h\u00f6chst\u00b7be\u00b7tr\u016fbt", "hin\u00b7ter\u00b7las\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.49": {"text": "Welcher nun/", "tokens": ["Wel\u00b7cher", "nun", "/"], "token_info": ["word", "word", "punct"], "pos": ["PWAT", "ADV", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.50": {"text": "weil keine Klage", "tokens": ["weil", "kei\u00b7ne", "Kla\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.51": {"text": "so grossem Verlust genug gegenwage", "tokens": ["so", "gros\u00b7sem", "Ver\u