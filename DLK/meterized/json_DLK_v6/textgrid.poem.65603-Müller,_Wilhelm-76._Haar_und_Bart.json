{"textgrid.poem.65603": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "76. Haar und Bart", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Held Simson hatte seine Kraft in seines Hauptes Haaren,", "tokens": ["Held", "Sim\u00b7son", "hat\u00b7te", "sei\u00b7ne", "Kraft", "in", "sei\u00b7nes", "Haup\u00b7tes", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VAFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Den neuen Helden ist sie nun in ihren Bart gefahren.", "tokens": ["Den", "neu\u00b7en", "Hel\u00b7den", "ist", "sie", "nun", "in", "ih\u00b7ren", "Bart", "ge\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Und wer Rei\u00dfaus sie nehmen sieht vor Messern und Raseuren,", "tokens": ["Und", "wer", "Rei\u00df\u00b7aus", "sie", "neh\u00b7men", "sieht", "vor", "Mes\u00b7sern", "und", "Ra\u00b7seu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "NN", "PPER", "VVINF", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Wer wei\u00df, ob Simson tapfrer w\u00e4r' im Kampfe mit Friseuren?", "tokens": ["Wer", "wei\u00df", ",", "ob", "Sim\u00b7son", "tapf\u00b7rer", "w\u00e4r'", "im", "Kamp\u00b7fe", "mit", "Fri\u00b7seu\u00b7ren", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "NE", "ADJD", "VAFIN", "APPRART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}}}}