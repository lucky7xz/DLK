{"dta.poem.4450": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-200905197778", "language": ["de:0.99"], "booktitle": "Karsch, Anna Luise: Auserlesene Gedichte. Berlin, 1764."}, "poem": {"stanza.1": {"line.1": {"text": "Du machtest mir in sorgenlosen Tagen", "tokens": ["Du", "mach\u00b7test", "mir", "in", "sor\u00b7gen\u00b7lo\u00b7sen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Elis\u00e4er Sitz, das pr\u00e4chtige Berlin.", "tokens": ["Zum", "E\u00b7li\u00b7s\u00e4\u00b7er", "Sitz", ",", "das", "pr\u00e4ch\u00b7ti\u00b7ge", "Ber\u00b7lin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$,", "ART", "ADJA", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So ward einst \u00fcbers Meer ein Dichter fortgetragen,", "tokens": ["So", "ward", "einst", "\u00fc\u00b7bers", "Meer", "ein", "Dich\u00b7ter", "fort\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von einem freundlichen Delphin;", "tokens": ["Von", "ei\u00b7nem", "freund\u00b7li\u00b7chen", "Del\u00b7phin", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}