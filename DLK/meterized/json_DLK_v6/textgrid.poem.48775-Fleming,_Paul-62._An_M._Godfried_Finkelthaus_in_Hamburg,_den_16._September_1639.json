{"textgrid.poem.48775": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "62. An M. Godfried Finkelthaus in Hamburg, den 16. September 1639", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.85", "en:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Chrysillis ist dir hold, Astree liebet dich,", "tokens": ["Chry\u00b7sil\u00b7lis", "ist", "dir", "hold", ",", "A\u00b7stree", "lie\u00b7bet", "dich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADJD", "$,", "NE", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Armille hofft nach dir, Melite will dich haben;", "tokens": ["Ar\u00b7mil\u00b7le", "hofft", "nach", "dir", ",", "Me\u00b7li\u00b7te", "will", "dich", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPER", "$,", "NN", "VMFIN", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die braune Dorilis besendet dich mit Gaben,", "tokens": ["die", "brau\u00b7ne", "Do\u00b7ri\u00b7lis", "be\u00b7sen\u00b7det", "dich", "mit", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "die keusche Zynthie, die hat dich gern um sich.", "tokens": ["die", "keu\u00b7sche", "Zynt\u00b7hie", ",", "die", "hat", "dich", "gern", "um", "sich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "VAFIN", "PPER", "ADV", "APPR", "PRF", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Ach wie viel gl\u00fccklicher bist du doch, Freund, als ich.", "tokens": ["Ach", "wie", "viel", "gl\u00fcck\u00b7li\u00b7cher", "bist", "du", "doch", ",", "Freund", ",", "als", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADV", "ADJD", "VAFIN", "PPER", "ADV", "$,", "NN", "$,", "KOUS", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Rubelle die ist tot, Parthenie begraben,", "tokens": ["Ru\u00b7bel\u00b7le", "die", "ist", "tot", ",", "Par\u00b7the\u00b7nie", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ART", "VAFIN", "ADJD", "$,", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "die teure Basile will anderweit sich laben,", "tokens": ["die", "teu\u00b7re", "Ba\u00b7si\u00b7le", "will", "an\u00b7der\u00b7weit", "sich", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und mein Herz' Amnien, die seh' ich nicht um mich.", "tokens": ["und", "mein", "Her\u00b7z'", "Am\u00b7ni\u00b7en", ",", "die", "seh'", "ich", "nicht", "um", "mich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NE", "$,", "PRELS", "VVFIN", "PPER", "PTKNEG", "APPR", "PPER", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.3": {"line.1": {"text": "O strenge Widrigkeit so gleichgesinnter Freunde!", "tokens": ["O", "stren\u00b7ge", "Wid\u00b7rig\u00b7keit", "so", "gleich\u00b7ge\u00b7sinn\u00b7ter", "Freun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was mich dich preisen hei\u00dft, das macht sich mir zum Feinde,", "tokens": ["Was", "mich", "dich", "prei\u00b7sen", "hei\u00dft", ",", "das", "macht", "sich", "mir", "zum", "Fein\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVFIN", "VVFIN", "$,", "PDS", "VVFIN", "PRF", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "dein Lieben ist mein Ha\u00df, mein Hassen deine Lust.", "tokens": ["dein", "Lie\u00b7ben", "ist", "mein", "Ha\u00df", ",", "mein", "Has\u00b7sen", "dei\u00b7ne", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So tu denn, Freund, wie ich, hilf mein Verh\u00e4ngn\u00fc\u00df schelten,", "tokens": ["So", "tu", "denn", ",", "Freund", ",", "wie", "ich", ",", "hilf", "mein", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "schel\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "NN", "$,", "PWAV", "PPER", "$,", "VVIMP", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "wie ich dein Gl\u00fccke lob', und lasse was du tust.", "tokens": ["wie", "ich", "dein", "Gl\u00fc\u00b7cke", "lob'", ",", "und", "las\u00b7se", "was", "du", "tust", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$,", "KON", "VVFIN", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was aber, da\u00df du mich besuchest gar zu selten?", "tokens": ["Was", "a\u00b7ber", ",", "da\u00df", "du", "mich", "be\u00b7su\u00b7chest", "gar", "zu", "sel\u00b7ten", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "$,", "KOUS", "PPER", "PRF", "VVFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}