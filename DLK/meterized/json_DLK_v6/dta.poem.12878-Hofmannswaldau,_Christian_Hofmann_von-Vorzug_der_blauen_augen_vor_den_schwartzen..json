{"dta.poem.12878": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Vorzug der blauen augen vor  \n den schwartzen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lichtem blitz und sch\u00f6nen augen", "tokens": ["Lich\u00b7tem", "blitz", "und", "sch\u00f6\u00b7nen", "au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird keine seele widerstehn.", "tokens": ["Wird", "kei\u00b7ne", "see\u00b7le", "wi\u00b7der\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein strahl kan alle krafft aus unsern adern saugen,", "tokens": ["Ein", "strahl", "kan", "al\u00b7le", "krafft", "aus", "un\u00b7sern", "a\u00b7dern", "sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den eine sch\u00f6ne l\u00e4st aus ihren sonnen gehn.", "tokens": ["Den", "ei\u00b7ne", "sch\u00f6\u00b7ne", "l\u00e4st", "aus", "ih\u00b7ren", "son\u00b7nen", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da wei\u00df kein diamant zu halten,", "tokens": ["Da", "wei\u00df", "kein", "di\u00b7a\u00b7mant", "zu", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein recht verliebter blick kan harte falsen spalten.", "tokens": ["Ein", "recht", "ver\u00b7lieb\u00b7ter", "blick", "kan", "har\u00b7te", "fal\u00b7sen", "spal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VMFIN", "ADJA", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Durch zwey augen und die sonne", "tokens": ["Durch", "zwey", "au\u00b7gen", "und", "die", "son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Besteht die klein\u2019 und gro\u00dfe welt:", "tokens": ["Be\u00b7steht", "die", "klein'", "und", "gro\u00b7\u00dfe", "welt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf diese gr\u00fcndet sich der himmel unsrer wonne,", "tokens": ["Auf", "die\u00b7se", "gr\u00fcn\u00b7det", "sich", "der", "him\u00b7mel", "uns\u00b7rer", "won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "PRF", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn w\u00e4ren beyde nicht, so st\u00fcnd\u2019 es schlecht bestellt.", "tokens": ["Denn", "w\u00e4\u00b7ren", "bey\u00b7de", "nicht", ",", "so", "st\u00fcnd'", "es", "schlecht", "be\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die sonn\u2019 erleuchtet finstre h\u00f6len:", "tokens": ["Die", "sonn'", "er\u00b7leuch\u00b7tet", "finst\u00b7re", "h\u00f6\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der holden augen licht erqvickt betr\u00fcbte seelen.", "tokens": ["Der", "hol\u00b7den", "au\u00b7gen", "licht", "er\u00b7qvickt", "be\u00b7tr\u00fcb\u00b7te", "see\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVPP", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Augen sind der liebe wiege;", "tokens": ["Au\u00b7gen", "sind", "der", "lie\u00b7be", "wie\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-++", "measure": "unknown.measure.penta"}, "line.2": {"text": "In diesen muscheln w\u00e4chst das kind,", "tokens": ["In", "die\u00b7sen", "mu\u00b7scheln", "w\u00e4chst", "das", "kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vor dessen wunder-krafft ein adler eine fliege,", "tokens": ["Vor", "des\u00b7sen", "wun\u00b7der\u00b7krafft", "ein", "ad\u00b7ler", "ei\u00b7ne", "flie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "NN", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und starcke riesen nur geschw\u00e4chte zwerge sind.", "tokens": ["Und", "star\u00b7cke", "rie\u00b7sen", "nur", "ge\u00b7schw\u00e4ch\u00b7te", "zwer\u00b7ge", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "ADV", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "In solchen angenehmen wellen", "tokens": ["In", "sol\u00b7chen", "an\u00b7ge\u00b7neh\u00b7men", "wel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "L\u00e4st die vergn\u00fcgung ihr die lagerstatt bestellen.", "tokens": ["L\u00e4st", "die", "ver\u00b7gn\u00fc\u00b7gung", "ihr", "die", "la\u00b7ger\u00b7statt", "be\u00b7stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Gleichwol streuen iede sterne", "tokens": ["Gleich\u00b7wol", "streu\u00b7en", "ie\u00b7de", "ster\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht einen s\u00fc\u00dfen glantz von sich.", "tokens": ["Nicht", "ei\u00b7nen", "s\u00fc\u00b7\u00dfen", "glantz", "von", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "APPR", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo der Orion brennt, da creutzt kein schiffer gerne:", "tokens": ["Wo", "der", "O\u00b7rion", "brennt", ",", "da", "creutzt", "kein", "schif\u00b7fer", "ger\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PIAT", "ADJD", "ADV", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Kein schwartzes auge labt, ergetzt und reitzet mich.", "tokens": ["Kein", "schwart\u00b7zes", "au\u00b7ge", "labt", ",", "er\u00b7getzt", "und", "reit\u00b7zet", "mich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$,", "VVPP", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die finsterni\u00df ist mir zuwider;", "tokens": ["Die", "fins\u00b7ter\u00b7ni\u00df", "ist", "mir", "zu\u00b7wi\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vor nacht und schatten schlag\u2019 ich stets mein auge nieder.", "tokens": ["Vor", "nacht", "und", "schat\u00b7ten", "schlag'", "ich", "stets", "mein", "au\u00b7ge", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "VVFIN", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Blauen augen bleibt die crone,", "tokens": ["Blau\u00b7en", "au\u00b7gen", "bleibt", "die", "cro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil ihre farbe g\u00f6ttlich ist.", "tokens": ["Weil", "ih\u00b7re", "far\u00b7be", "g\u00f6tt\u00b7lich", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hier sitzt die anmuth selbst auf dem sapphirnen throne,", "tokens": ["Hier", "sitzt", "die", "an\u00b7muth", "selbst", "auf", "dem", "sap\u00b7phir\u00b7nen", "thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wenn sie der menschligkeit ein holdes urthel liest.", "tokens": ["Wenn", "sie", "der", "mensc\u00b7hlig\u00b7keit", "ein", "hol\u00b7des", "ur\u00b7thel", "liest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Aus schwartzen wolcken fahren blitze,", "tokens": ["Aus", "schwart\u00b7zen", "wol\u00b7cken", "fah\u00b7ren", "blit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Aus blauen aber spielt beliebte sonnen-hitze.", "tokens": ["Aus", "blau\u00b7en", "a\u00b7ber", "spielt", "be\u00b7lieb\u00b7te", "son\u00b7nen\u00b7hit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADV", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Schwartz stammt aus der finstern h\u00f6lle;", "tokens": ["Schwartz", "stammt", "aus", "der", "fins\u00b7tern", "h\u00f6l\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blau schreihet sich vom himmel her.", "tokens": ["Blau", "schrei\u00b7het", "sich", "vom", "him\u00b7mel", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPRART", "NN", "PTKVZ", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Welch aug\u2019 ergetzt sich nicht an einer blauen welle?", "tokens": ["Welch", "aug'", "er\u00b7getzt", "sich", "nicht", "an", "ei\u00b7ner", "blau\u00b7en", "wel\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von einer schwartzen grufft wird uns das hertze schwer.", "tokens": ["Von", "ei\u00b7ner", "schwart\u00b7zen", "grufft", "wird", "uns", "das", "hert\u00b7ze", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "In blauen augen ist mehr feuer.", "tokens": ["In", "blau\u00b7en", "au\u00b7gen", "ist", "mehr", "feu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In duncklen sternen sind beliebte strahlen theuer.", "tokens": ["In", "dunck\u00b7len", "ster\u00b7nen", "sind", "be\u00b7lieb\u00b7te", "strah\u00b7len", "theu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVINF", "VAFIN", "ADJA", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Sch\u00f6ne m\u00fctter, sch\u00f6ne kinder,", "tokens": ["Sch\u00f6\u00b7ne", "m\u00fct\u00b7ter", ",", "sch\u00f6\u00b7ne", "kin\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kein mohr erzieht ein weisses kind.", "tokens": ["Kein", "mohr", "er\u00b7zieht", "ein", "weis\u00b7ses", "kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein helden-vater zeugt nur lauter \u00fcberwinder,", "tokens": ["Ein", "hel\u00b7den\u00b7va\u00b7ter", "zeugt", "nur", "lau\u00b7ter", "\u00fc\u00b7berw\u00b7in\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADV", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gleichwie der perlen-than nur aus den wolcken rinnt.", "tokens": ["Gleich\u00b7wie", "der", "per\u00b7len\u00b7than", "nur", "aus", "den", "wol\u00b7cken", "rinnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NE", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Soll Venus erd und himmel gr\u00fc\u00dfen,", "tokens": ["Soll", "Ve\u00b7nus", "erd", "und", "him\u00b7mel", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mu\u00df sie die blaue see als ihre mutter k\u00fcssen.", "tokens": ["Mu\u00df", "sie", "die", "blau\u00b7e", "see", "als", "ih\u00b7re", "mut\u00b7ter", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "KOKOM", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Geht nur hin, ihr schwartzen lichter!", "tokens": ["Geht", "nur", "hin", ",", "ihr", "schwart\u00b7zen", "lich\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$,", "PPER", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die blauen gehen euch weit f\u00fcr.", "tokens": ["Die", "blau\u00b7en", "ge\u00b7hen", "euch", "weit", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "ADJD", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jhr himmlisch feuer cr\u00f6nt die trefflichsten gesichter:", "tokens": ["Ihr", "himm\u00b7lisch", "feu\u00b7er", "cr\u00f6nt", "die", "treff\u00b7lichs\u00b7ten", "ge\u00b7sich\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Sie sind der redlichsten und kl\u00fcgsten geister zier.", "tokens": ["Sie", "sind", "der", "red\u00b7lichs\u00b7ten", "und", "kl\u00fcgs\u00b7ten", "geis\u00b7ter", "zier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "KON", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und aus der Pallas blauen augen", "tokens": ["Und", "aus", "der", "Pal\u00b7las", "blau\u00b7en", "au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wei\u00df die gelehre welt die gr\