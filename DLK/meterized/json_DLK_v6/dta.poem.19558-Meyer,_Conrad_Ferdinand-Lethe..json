{"dta.poem.19558": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Lethe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst im Traume sah ich auf den Fluten", "tokens": ["J\u00fcngst", "im", "Trau\u00b7me", "sah", "ich", "auf", "den", "Flu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Einen Nachen ohne Ruder ziehn,", "tokens": ["Ei\u00b7nen", "Na\u00b7chen", "oh\u00b7ne", "Ru\u00b7der", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Strom und Himmel stand in matten Gluten", "tokens": ["Strom", "und", "Him\u00b7mel", "stand", "in", "mat\u00b7ten", "Glu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wie bei Tages Nahen oder Fliehn.", "tokens": ["Wie", "bei", "Ta\u00b7ges", "Na\u00b7hen", "o\u00b7der", "Fliehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Sa\u00dfen Knaben drin mit Lotoskr\u00e4nzen,", "tokens": ["Sa\u00b7\u00dfen", "Kna\u00b7ben", "drin", "mit", "Lo\u00b7tos\u00b7kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "M\u00e4dchen beugten \u00fcber Bord sich schlank,", "tokens": ["M\u00e4d\u00b7chen", "beug\u00b7ten", "\u00fc\u00b7ber", "Bord", "sich", "schlank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "PRF", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Kreisend durch die Reihe sah ich gl\u00e4nzen", "tokens": ["Krei\u00b7send", "durch", "die", "Rei\u00b7he", "sah", "ich", "gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Eine Schale, draus ein Jedes trank.", "tokens": ["Ei\u00b7ne", "Scha\u00b7le", ",", "draus", "ein", "Je\u00b7des", "trank", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "ART", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Jetzt erscholl ein Lied voll s\u00fc\u00dfer Wehmuth,", "tokens": ["Jetzt", "er\u00b7scholl", "ein", "Lied", "voll", "s\u00fc\u00b7\u00dfer", "Weh\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Das die Schaar der Kranzgenossen sang \u2014", "tokens": ["Das", "die", "Schaar", "der", "Kranz\u00b7ge\u00b7nos\u00b7sen", "sang"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ich erkannte deines Nackens Demuth,", "tokens": ["Ich", "er\u00b7kann\u00b7te", "dei\u00b7nes", "Na\u00b7ckens", "De\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Deine Stimme, die den Chor durchdrang.", "tokens": ["Dei\u00b7ne", "Stim\u00b7me", ",", "die", "den", "Chor", "durch\u00b7drang", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "In die Welle taucht' ich. Bis zum Marke", "tokens": ["In", "die", "Wel\u00b7le", "taucht'", "ich", ".", "Bis", "zum", "Mar\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "$.", "APPR", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schaudert' ich, wie seltsam k\u00fchl sie war.", "tokens": ["Schau\u00b7dert'", "ich", ",", "wie", "selt\u00b7sam", "k\u00fchl", "sie", "war", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ADJD", "ADJD", "PPER", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ich erreicht' die leise zieh'nde Barke,", "tokens": ["Ich", "er\u00b7reicht'", "die", "lei\u00b7se", "zieh'n\u00b7de", "Bar\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dr\u00e4ngte mich in die geweihte Schaar.", "tokens": ["Dr\u00e4ng\u00b7te", "mich", "in", "die", "ge\u00b7weih\u00b7te", "Schaar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Und die Reihe war an dir, zu trinken", "tokens": ["Und", "die", "Rei\u00b7he", "war", "an", "dir", ",", "zu", "trin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "APPR", "PPER", "$,", "PTKZU", "VVINF"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Und die volle Schale hobest du,", "tokens": ["Und", "die", "vol\u00b7le", "Scha\u00b7le", "ho\u00b7best", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sprachst zu mir mit trautem Augenwinken:", "tokens": ["Sprachst", "zu", "mir", "mit", "trau\u00b7tem", "Au\u00b7gen\u00b7win\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "\u201eherz, ich trinke dir Vergessen zu.\u201c", "tokens": ["\u201e", "herz", ",", "ich", "trin\u00b7ke", "dir", "Ver\u00b7ges\u00b7sen", "zu", ".", "\u201c"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NN", "$,", "PPER", "VVFIN", "PPER", "NN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Dir entri\u00df in trotz'gem Liebesdrange", "tokens": ["Dir", "ent\u00b7ri\u00df", "in", "trotz'\u00b7gem", "Lie\u00b7bes\u00b7dran\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ich die Schale, warf sie in die Flut,", "tokens": ["Ich", "die", "Scha\u00b7le", ",", "warf", "sie", "in", "die", "Flut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sie versank und siehe, deine Wange", "tokens": ["Sie", "ver\u00b7sank", "und", "sie\u00b7he", ",", "dei\u00b7ne", "Wan\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVIMP", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "F\u00e4rbte sich mit einem Schein von Blut.", "tokens": ["F\u00e4rb\u00b7te", "sich", "mit", "ei\u00b7nem", "Schein", "von", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Flehend k\u00fc\u00dft' ich dich in wildem Harme,", "tokens": ["Fle\u00b7hend", "k\u00fc\u00dft'", "ich", "dich", "in", "wil\u00b7dem", "Har\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die den bleichen Mund mir willig bot,", "tokens": ["Die", "den", "blei\u00b7chen", "Mund", "mir", "wil\u00b7lig", "bot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Da zerrannst du l\u00e4chelnd mir im Arme", "tokens": ["Da", "zer\u00b7rannst", "du", "l\u00e4\u00b7chelnd", "mir", "im", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und ich wu\u00dft' es wieder \u2014 du bist todt.", "tokens": ["Und", "ich", "wu\u00dft'", "es", "wie\u00b7der", "du", "bist", "todt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "$(", "PPER", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}