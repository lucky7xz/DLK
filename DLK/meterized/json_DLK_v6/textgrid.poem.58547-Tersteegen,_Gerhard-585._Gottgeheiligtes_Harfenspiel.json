{"textgrid.poem.58547": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "585. Gottgeheiligtes Harfenspiel", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dir, Gott, ich Herz und Lieder bringe;", "tokens": ["Dir", ",", "Gott", ",", "ich", "Herz", "und", "Lie\u00b7der", "brin\u00b7ge", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Damit ich nie vergeblich singe:", "tokens": ["Da\u00b7mit", "ich", "nie", "ver\u00b7geb\u00b7lich", "sin\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Andacht Weihrauch brenn' durch dein s\u00fc\u00df' Nahesein,", "tokens": ["Der", "An\u00b7dacht", "Weih\u00b7rauch", "brenn'", "durch", "dein", "s\u00fc\u00df'", "Na\u00b7he\u00b7sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mach selbst dich mir recht gro\u00df und mich von Herzen klein,", "tokens": ["Mach", "selbst", "dich", "mir", "recht", "gro\u00df", "und", "mich", "von", "Her\u00b7zen", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "PPER", "ADV", "ADJD", "KON", "PRF", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein Wille stimme sanft mit deinem Willen ein,", "tokens": ["Mein", "Wil\u00b7le", "stim\u00b7me", "sanft", "mit", "dei\u00b7nem", "Wil\u00b7len", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mein Innerstes nur dich einf\u00e4ltig such' und mein'!", "tokens": ["Mein", "In\u00b7ners\u00b7tes", "nur", "dich", "ein\u00b7f\u00e4l\u00b7tig", "such'", "und", "mein'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PPER", "ADJD", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}