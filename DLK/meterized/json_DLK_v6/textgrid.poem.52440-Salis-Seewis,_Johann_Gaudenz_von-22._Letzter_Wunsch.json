{"textgrid.poem.52440": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "22. Letzter Wunsch", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann, o Schicksal! wann wird endlich", "tokens": ["Wann", ",", "o", "Schick\u00b7sal", "!", "wann", "wird", "end\u00b7lich"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "$,", "FM", "NN", "$.", "PWAV", "VAFIN", "ADV"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Mir mein letzter Wunsch gew\u00e4hrt?", "tokens": ["Mir", "mein", "letz\u00b7ter", "Wunsch", "ge\u00b7w\u00e4hrt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur ein H\u00fcttchen, still und l\u00e4ndlich,", "tokens": ["Nur", "ein", "H\u00fctt\u00b7chen", ",", "still", "und", "l\u00e4nd\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur ein kleiner eigner Herd;", "tokens": ["Nur", "ein", "klei\u00b7ner", "eig\u00b7ner", "Herd", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ein Freund, bew\u00e4hrt und weise,", "tokens": ["Und", "ein", "Freund", ",", "be\u00b7w\u00e4hrt", "und", "wei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "VVPP", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Freiheit, Heiterkeit und Ruh'!", "tokens": ["Frei\u00b7heit", ",", "Hei\u00b7ter\u00b7keit", "und", "Ruh'", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach und sie! das seufz' ich leise,", "tokens": ["Ach", "und", "sie", "!", "das", "seuf\u00b7z'", "ich", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KON", "PPER", "$.", "PDS", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zur Gef\u00e4hrtin sie dazu.", "tokens": ["Zur", "Ge\u00b7f\u00e4hr\u00b7tin", "sie", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn ich noch ein G\u00e4rtchen h\u00e4tte,", "tokens": ["Wenn", "ich", "noch", "ein", "G\u00e4rt\u00b7chen", "h\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bauten wir's mit eigner Hand.", "tokens": ["Bau\u00b7ten", "wir's", "mit", "eig\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Statt geschorener Boskette", "tokens": ["Statt", "ge\u00b7scho\u00b7re\u00b7ner", "Bos\u00b7ket\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Und der Hagenbuchenwand", "tokens": ["Und", "der", "Ha\u00b7gen\u00b7bu\u00b7chen\u00b7wand"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "D\u00e4mmert' uns ein Dach von Latten,", "tokens": ["D\u00e4m\u00b7mert'", "uns", "ein", "Dach", "von", "Lat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dicht mit Rebengr\u00fcn bedeckt,", "tokens": ["Dicht", "mit", "Re\u00b7ben\u00b7gr\u00fcn", "be\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Tief in Silbertannenschatten", "tokens": ["Tief", "in", "Sil\u00b7ber\u00b7tan\u00b7nen\u00b7schat\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Vor des Neides Blick versteckt.", "tokens": ["Vor", "des", "Nei\u00b7des", "Blick", "ver\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Statt Kan\u00e4l' und Gartenteiche", "tokens": ["Statt", "Ka\u00b7n\u00e4l'", "und", "Gar\u00b7ten\u00b7tei\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur ein R\u00f6hrenbrunnentrog;", "tokens": ["Nur", "ein", "R\u00f6h\u00b7ren\u00b7brun\u00b7nen\u00b7trog", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Statt Alleen und Taxusstr\u00e4uche", "tokens": ["Statt", "Al\u00b7leen", "und", "Ta\u00b7xus\u00b7str\u00e4u\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fr\u00fcchte, die ich selbst erzog,", "tokens": ["Fr\u00fcch\u00b7te", ",", "die", "ich", "selbst", "er\u00b7zog", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Durch ein Gatter, nur von Pf\u00e4hlen,", "tokens": ["Durch", "ein", "Gat\u00b7ter", ",", "nur", "von", "Pf\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch den Vorhof, eng und klein,", "tokens": ["Durch", "den", "Vor\u00b7hof", ",", "eng", "und", "klein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Eilt' ich, statt nach Marmors\u00e4len,", "tokens": ["Eilt'", "ich", ",", "statt", "nach", "Mar\u00b7mor\u00b7s\u00e4\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUI", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In ihr trautes K\u00e4mmerlein.", "tokens": ["In", "ihr", "trau\u00b7tes", "K\u00e4m\u00b7mer\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Bei des heitern Morgens Frische", "tokens": ["Bei", "des", "hei\u00b7tern", "Mor\u00b7gens", "Fri\u00b7sche"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6rten wir im Buchenhain,", "tokens": ["H\u00f6r\u00b7ten", "wir", "im", "Bu\u00b7chen\u00b7hain", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dort am Wasser im Geb\u00fcsche,", "tokens": ["Dort", "am", "Was\u00b7ser", "im", "Ge\u00b7b\u00fc\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nachtigallenmelodein.", "tokens": ["Nach\u00b7ti\u00b7gal\u00b7len\u00b7me\u00b7lo\u00b7dein", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auch beg\u00e4nne sie Ges\u00e4nge,", "tokens": ["Auch", "be\u00b7g\u00e4n\u00b7ne", "sie", "Ge\u00b7s\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "W\u00e4re Philomel' entflohn,", "tokens": ["W\u00e4\u00b7re", "Phi\u00b7lo\u00b7mel'", "ent\u00b7flohn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und in meine Seele dr\u00e4nge", "tokens": ["Und", "in", "mei\u00b7ne", "See\u00b7le", "dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Tiefer noch ihr s\u00fc\u00dfer Ton.", "tokens": ["Tie\u00b7fer", "noch", "ihr", "s\u00fc\u00b7\u00dfer", "Ton", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Unterm Strauch voll Hagerosen,", "tokens": ["Un\u00b7term", "Strauch", "voll", "Ha\u00b7ge\u00b7ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf dem rotbebl\u00fcmten Klee,", "tokens": ["Auf", "dem", "rot\u00b7be\u00b7bl\u00fcm\u00b7ten", "Klee", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6nnten wir so traulich kosen,", "tokens": ["K\u00f6nn\u00b7ten", "wir", "so", "trau\u00b7lich", "ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie auf seidnem Kanapee.", "tokens": ["Wie", "auf", "seid\u00b7nem", "Ka\u00b7na\u00b7pee", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In dem Duft entbl\u00fchter Bohnen,", "tokens": ["In", "dem", "Duft", "ent\u00b7bl\u00fch\u00b7ter", "Boh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unter Pappeln, hoch und schlank,", "tokens": ["Un\u00b7ter", "Pap\u00b7peln", ",", "hoch", "und", "schlank", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Bauten wir, trotz goldnen Thronen,", "tokens": ["Bau\u00b7ten", "wir", ",", "trotz", "gold\u00b7nen", "Thro\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Eine kleine Bretterbank.", "tokens": ["Ei\u00b7ne", "klei\u00b7ne", "Bret\u00b7ter\u00b7bank", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Beeren, die ihr Finger dr\u00fcckte,", "tokens": ["Bee\u00b7ren", ",", "die", "ihr", "Fin\u00b7ger", "dr\u00fcck\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Honig, der der Wab' entflo\u00df,", "tokens": ["Ho\u00b7nig", ",", "der", "der", "Wab'", "ent\u00b7flo\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kr\u00e4uter, die vom Beet' sie pfl\u00fcckte,", "tokens": ["Kr\u00e4u\u00b7ter", ",", "die", "vom", "Beet'", "sie", "pfl\u00fcck\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPRART", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Milch, die sie in Schalen go\u00df:", "tokens": ["Milch", ",", "die", "sie", "in", "Scha\u00b7len", "go\u00df", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ha! bei solchem G\u00f6ttermahle", "tokens": ["Ha", "!", "bei", "sol\u00b7chem", "G\u00f6t\u00b7ter\u00b7mah\u00b7le"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "S\u00e4\u00dfen wir, wie froh, wie stolz!", "tokens": ["S\u00e4\u00b7\u00dfen", "wir", ",", "wie", "froh", ",", "wie", "stolz", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ADJD", "$,", "PWAV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "W\u00e4r' auch L\u00f6ffel, Kelch und Schale", "tokens": ["W\u00e4r'", "auch", "L\u00f6f\u00b7fel", ",", "Kelch", "und", "Scha\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nur aus wei\u00dfem Buchenholz.", "tokens": ["Nur", "aus", "wei\u00b7\u00dfem", "Bu\u00b7chen\u00b7holz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Mit den holden D\u00f6rferinnen,", "tokens": ["Mit", "den", "hol\u00b7den", "D\u00f6r\u00b7fe\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach der Weidenpfeife Schall,", "tokens": ["Nach", "der", "Wei\u00b7den\u00b7pfei\u00b7fe", "Schall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einen Maientanz beginnen,", "tokens": ["Ei\u00b7nen", "Mai\u00b7en\u00b7tanz", "be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "G\u00e4lt' uns mehr als Maskenball.", "tokens": ["G\u00e4lt'", "uns", "mehr", "als", "Mas\u00b7ken\u00b7ball", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KOUS", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lieber, als der Prunk der B\u00fchnen", "tokens": ["Lie\u00b7ber", ",", "als", "der", "Prunk", "der", "B\u00fch\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "KOUS", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dem verw\u00f6hnten St\u00e4dterschwarm,", "tokens": ["Dem", "ver\u00b7w\u00f6hn\u00b7ten", "St\u00e4d\u00b7ter\u00b7schwarm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "W\u00e4r' ein Pf\u00e4nderspiel im Gr\u00fcnen", "tokens": ["W\u00e4r'", "ein", "Pf\u00e4n\u00b7der\u00b7spiel", "im", "Gr\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mir an meines M\u00e4dchens Arm.", "tokens": ["Mir", "an", "mei\u00b7nes", "M\u00e4d\u00b7chens", "Arm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "In gestirnten Sommern\u00e4chten,", "tokens": ["In", "ge\u00b7stirn\u00b7ten", "Som\u00b7mer\u00b7n\u00e4ch\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn der Mond die Schatten hellt,", "tokens": ["Wenn", "der", "Mond", "die", "Schat\u00b7ten", "hellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wallte