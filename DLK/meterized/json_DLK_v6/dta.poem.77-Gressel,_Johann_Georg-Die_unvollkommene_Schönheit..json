{"dta.poem.77": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die unvollkommene Sch\u00f6nheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ob gleich die Lilien mit stoltzem Silber prangen/", "tokens": ["Ob", "gleich", "die", "Li\u00b7li\u00b7en", "mit", "stolt\u00b7zem", "Sil\u00b7ber", "pran\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sieht man doch manchen Fleck an ihrem ", "tokens": ["Sieht", "man", "doch", "man\u00b7chen", "Fleck", "an", "ih\u00b7rem"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADV", "PIAT", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der ihrer Wunder-Zier den gr\u00f6\u00dften Glantz beraubt/", "tokens": ["Der", "ih\u00b7rer", "Wun\u00b7der\u00b7Zier", "den", "gr\u00f6\u00df\u00b7ten", "Glantz", "be\u00b7raubt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ihrer Bl\u00e4tter Pracht mit dunckler Nacht behaubt.", "tokens": ["Und", "ih\u00b7rer", "Bl\u00e4t\u00b7ter", "Pracht", "mit", "dunck\u00b7ler", "Nacht", "be\u00b7haubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Krone/ und die Zier des angenehmen Lentzen/", "tokens": ["Die", "Kro\u00b7ne", "/", "und", "die", "Zier", "des", "an\u00b7ge\u00b7neh\u00b7men", "Lent\u00b7zen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KON", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Blumen K\u00f6niginn mit ihren Purpur-Kr\u00e4ntzen/", "tokens": ["Der", "Blu\u00b7men", "K\u00f6\u00b7ni\u00b7ginn", "mit", "ih\u00b7ren", "Pur\u00b7pur\u00b7Kr\u00e4nt\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Prangt zwar im stoltzem Schmuck/ doch weil sie Dornen hegt/", "tokens": ["Prangt", "zwar", "im", "stolt\u00b7zem", "Schmuck", "/", "doch", "weil", "sie", "Dor\u00b7nen", "hegt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "ADJA", "NN", "$(", "ADV", "KOUS", "PPER", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So kommt es/ da\u00df man offt vor Rosen Abscheu tr\u00e4gt.", "tokens": ["So", "kommt", "es", "/", "da\u00df", "man", "offt", "vor", "Ro\u00b7sen", "Ab\u00b7scheu", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "PIS", "ADV", "APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der bl\u00fchende ", "tokens": ["Der", "bl\u00fc\u00b7hen\u00b7de"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+--", "measure": "dactylic.init"}, "line.10": {"text": "Das k\u00f6stliche Gew\u00e4chs die pr\u00e4chtigen ", "tokens": ["Das", "k\u00f6st\u00b7li\u00b7che", "Ge\u00b7w\u00e4chs", "die", "pr\u00e4ch\u00b7ti\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Die prangen an dem Strauch/ und bl\u00fchen m\u00e4chtig sch\u00f6n/", "tokens": ["Die", "pran\u00b7gen", "an", "dem", "Strauch", "/", "und", "bl\u00fc\u00b7hen", "m\u00e4ch\u00b7tig", "sch\u00f6n", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$(", "KON", "VVFIN", "ADJD", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Doch ist bey dieser Pracht offtmahls ein Wurm zu sehn.", "tokens": ["Doch", "ist", "bey", "die\u00b7ser", "Pracht", "offt\u00b7mahls", "ein", "Wurm", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PDAT", "NN", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der Seiden reiche Sammt ist nicht ohn\u2019 alle Fehler/", "tokens": ["Der", "Sei\u00b7den", "rei\u00b7che", "Sammt", "ist", "nicht", "ohn'", "al\u00b7le", "Feh\u00b7ler", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "PTKNEG", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der Dafft und theurer Stoff zeigt ungerade Th\u00e4ler/", "tokens": ["Der", "Dafft", "und", "theu\u00b7rer", "Stoff", "zeigt", "un\u00b7ge\u00b7ra\u00b7de", "Th\u00e4\u00b7ler", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJD", "NN", "VVFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Der sch\u00f6nste Diamant ist nicht von Mackeln rein/", "tokens": ["Der", "sch\u00f6ns\u00b7te", "Di\u00b7a\u00b7mant", "ist", "nicht", "von", "Ma\u00b7ckeln", "rein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKNEG", "APPR", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Den Perlen fehlt etwas bey ihrem klahren Schein.", "tokens": ["Den", "Per\u00b7len", "fehlt", "et\u00b7was", "bey", "ih\u00b7rem", "klah\u00b7ren", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Das Auge dieser Welt zeigt auch verschiedne Flecken/", "tokens": ["Das", "Au\u00b7ge", "die\u00b7ser", "Welt", "zeigt", "auch", "ver\u00b7schied\u00b7ne", "Fle\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "VVFIN", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die ihm bald hie bald dort das reine Feur bedecken", "tokens": ["Die", "ihm", "bald", "hie", "bald", "dort", "das", "rei\u00b7ne", "Feur", "be\u00b7de\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADV", "ADV", "ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Des Monden silber Licht ist nicht von M\u00e4ngeln rein/", "tokens": ["Des", "Mon\u00b7den", "sil\u00b7ber", "Licht", "ist", "nicht", "von", "M\u00e4n\u00b7geln", "rein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VAFIN", "PTKNEG", "APPR", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die ihm bey seiner Pracht mit einges\u00e4met seyn.", "tokens": ["Die", "ihm", "bey", "sei\u00b7ner", "Pracht", "mit", "ein\u00b7ge\u00b7s\u00e4\u00b7met", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "APPR", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ja bey dem Kunst-Geb\u00e4u/ den hellen Himmels B\u00fchnen/", "tokens": ["Ja", "bey", "dem", "Kunst\u00b7Ge\u00b7b\u00e4u", "/", "den", "hel\u00b7len", "Him\u00b7mels", "B\u00fch\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "ART", "NE", "$(", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ist die geschm\u00fcckte Pracht mit Mangel auch erschienen/", "tokens": ["Ist", "die", "ge\u00b7schm\u00fcck\u00b7te", "Pracht", "mit", "Man\u00b7gel", "auch", "er\u00b7schie\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "APPR", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Indem das Sternen-Heer/ so lustig Wache h\u00e4lt/", "tokens": ["In\u00b7dem", "das", "Ster\u00b7nen\u00b7Heer", "/", "so", "lus\u00b7tig", "Wa\u00b7che", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$(", "ADV", "ADJD", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Sich nicht vollkommen sch\u00f6n in seinem Schmuck darstelt.", "tokens": ["Sich", "nicht", "voll\u00b7kom\u00b7men", "sch\u00f6n", "in", "sei\u00b7nem", "Schmuck", "dar\u00b7stelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKNEG", "ADJD", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Der weisse ", "tokens": ["Der", "weis\u00b7se"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.26": {"text": "Hegt Mackels mancher Art in seinem stlber Pflaster/", "tokens": ["Hegt", "Ma\u00b7ckels", "man\u00b7cher", "Art", "in", "sei\u00b7nem", "stl\u00b7ber", "Pflas\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PIAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Rubinen und Schmaragd/ den T\u00fcrcki\u00df und ", "tokens": ["Ru\u00b7bi\u00b7nen", "und", "Schma\u00b7ragd", "/", "den", "T\u00fcr\u00b7cki\u00df", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$(", "ART", "NN", "KON"], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "Die findet man befleckt bey ihrer ", "tokens": ["Die", "fin\u00b7det", "man", "be\u00b7fleckt", "bey", "ih\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PIS", "VVFIN", "APPR", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Des Meeres Wunder-Wald/ die \u00e4stigten Corallen/", "tokens": ["Des", "Mee\u00b7res", "Wun\u00b7der\u00b7Wald", "/", "die", "\u00e4s\u00b7tig\u00b7ten", "Co\u00b7ral\u00b7len", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Der Erden Demant-Gla\u00df die klahren Berg-Crystallen/", "tokens": ["Der", "Er\u00b7den", "De\u00b7mant\u00b7Gla\u00df", "die", "klah\u00b7ren", "Ber\u00b7gCry\u00b7stal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Und was von diesen mehr die gantze R\u00fcndung hegt/", "tokens": ["Und", "was", "von", "die\u00b7sen", "mehr", "die", "gant\u00b7ze", "R\u00fcn\u00b7dung", "hegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "PDAT", "ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ist nicht gantz sch\u00f6n da es auch M\u00e4ngels an sich tr\u00e4gt.", "tokens": ["Ist", "nicht", "gantz", "sch\u00f6n", "da", "es", "auch", "M\u00e4n\u00b7gels", "an", "sich", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "ADJD", "KOUS", "PPER", "ADV", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "So auch die kleine Welt/ das artige Geschlechte/", "tokens": ["So", "auch", "die", "klei\u00b7ne", "Welt", "/", "das", "ar\u00b7ti\u00b7ge", "Ge\u00b7schlech\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Das in die Banden legt/ und uns heist ihre Knechte/", "tokens": ["Das", "in", "die", "Ban\u00b7den", "legt", "/", "und", "uns", "heist", "ih\u00b7re", "Knech\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "$(", "KON", "PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Das sch\u00f6ne Jungfern Volck/ der unsch\u00e4tzbahre Schatz/", "tokens": ["Das", "sch\u00f6\u00b7ne", "Jung\u00b7fern", "Volck", "/", "der", "un\u00b7sch\u00e4tz\u00b7bah\u00b7re", "Schatz", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$(", "ART", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Der giebt bey seiner Pracht verschiednen M\u00e4ngeln Platz.", "tokens": ["Der", "giebt", "bey", "sei\u00b7ner", "Pracht", "ver\u00b7schied\u00b7nen", "M\u00e4n\u00b7geln", "Platz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Die Haare/ so als Gold/ als lichte Perlen prahlen/", "tokens": ["Die", "Haa\u00b7re", "/", "so", "als", "Gold", "/", "als", "lich\u00b7te", "Per\u00b7len", "prah\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "KOUS", "NN", "$(", "KOUS", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Die m\u00fcssen/ ob gleich spaht/ die alte Schuld bezahlen/", "tokens": ["Die", "m\u00fcs\u00b7sen", "/", "ob", "gleich", "spaht", "/", "die", "al\u00b7te", "Schuld", "be\u00b7zah\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "$(", "KOUS", "ADV", "VVFIN", "$(", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und sagen/ da\u00df sie nicht von allem Mangel frey/", "tokens": ["Und", "sa\u00b7gen", "/", "da\u00df", "sie", "nicht", "von", "al\u00b7lem", "Man\u00b7gel", "frey", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$(", "KOUS", "PPER", "PTKNEG", "APPR", "PIS", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Die andern fallen auch derselben Meynung bey.", "tokens": ["Die", "an\u00b7dern", "fal\u00b7len", "auch", "der\u00b7sel\u00b7ben", "Mey\u00b7nung", "bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADV", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Die glatte ", "tokens": ["Die", "glat\u00b7te"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.42": {"text": "Hengt/ wie ein Pfau/ besch\u00e4mmt die sonst gerichten Fl\u00fcgel/", "tokens": ["Hengt", "/", "wie", "ein", "Pfau", "/", "be\u00b7sch\u00e4mmt", "die", "sonst", "ge\u00b7rich\u00b7ten", "Fl\u00fc\u00b7gel", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "KOKOM", "ART", "NN", "$(", "VVFIN", "ART", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Wenn sie den Silber-Schein nicht gantz vollkommen sieht/", "tokens": ["Wenn", "sie", "den", "Sil\u00b7ber\u00b7Schein", "nicht", "gantz", "voll\u00b7kom\u00b7men", "sieht