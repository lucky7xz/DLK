{"textgrid.poem.45879": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[am Berge hoch in L\u00fcften]", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Berge hoch in L\u00fcften,", "tokens": ["Am", "Ber\u00b7ge", "hoch", "in", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da baute er sein Haus;", "tokens": ["Da", "bau\u00b7te", "er", "sein", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Am Tore liegt Gewitter,", "tokens": ["Am", "To\u00b7re", "liegt", "Ge\u00b7wit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nun kann er nicht hinaus.", "tokens": ["Nun", "kann", "er", "nicht", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Wolken, sie wollen nicht ziehen,", "tokens": ["Die", "Wol\u00b7ken", ",", "sie", "wol\u00b7len", "nicht", "zie\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Der Pfad ist steil und schwer,", "tokens": ["Der", "Pfad", "ist", "steil", "und", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "O Lieber, Herzlieber in L\u00fcften,", "tokens": ["O", "Lie\u00b7ber", ",", "Herz\u00b7lie\u00b7ber", "in", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "O wenn ich bei Dir w\u00e4r'!", "tokens": ["O", "wenn", "ich", "bei", "Dir", "w\u00e4r'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "APPR", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wohl bei Dir \u00fcber Wolken,", "tokens": ["Wohl", "bei", "Dir", "\u00fc\u00b7ber", "Wol\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl bei Dir \u00fcber Wind,", "tokens": ["Wohl", "bei", "Dir", "\u00fc\u00b7ber", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo fromme V\u00f6glein schweben", "tokens": ["Wo", "from\u00b7me", "V\u00f6\u00b7glein", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Himmelsluft so lind.", "tokens": ["In", "Him\u00b7mel\u00b7sluft", "so", "lind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Meine Fl\u00fcglein, die sind mir gebrochen", "tokens": ["Mei\u00b7ne", "Fl\u00fcg\u00b7lein", ",", "die", "sind", "mir", "ge\u00b7bro\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "VAFIN", "PPER", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und heilen auch nicht eh'", "tokens": ["Und", "hei\u00b7len", "auch", "nicht", "eh'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "KOUS"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bis ich zu der Herzliebsten", "tokens": ["Bis", "ich", "zu", "der", "Herz\u00b7liebs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Durch T\u00fcr und Tor eingeh'!", "tokens": ["Durch", "T\u00fcr", "und", "Tor", "ein\u00b7geh'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da\u00df ich so stolz in L\u00fcften", "tokens": ["Da\u00df", "ich", "so", "stolz", "in", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Haus gebauet hab',", "tokens": ["Mein", "Haus", "ge\u00b7bau\u00b7et", "hab'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das mu\u00df mich gar betr\u00fcben,", "tokens": ["Das", "mu\u00df", "mich", "gar", "be\u00b7tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich kann nicht mehr hinab;", "tokens": ["Ich", "kann", "nicht", "mehr", "hin\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Riegel sind alle verrostet,", "tokens": ["Die", "Rie\u00b7gel", "sind", "al\u00b7le", "ver\u00b7ros\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Die Tore sie gehen so schwer,", "tokens": ["Die", "To\u00b7re", "sie", "ge\u00b7hen", "so", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "O Liebchen, Herzliebchen im Tale,", "tokens": ["O", "Lieb\u00b7chen", ",", "Herz\u00b7lieb\u00b7chen", "im", "Ta\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "APPRART", "NN", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "O wenn ich bei Dir w\u00e4r'!", "tokens": ["O", "wenn", "ich", "bei", "Dir", "w\u00e4r'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "APPR", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wohl bei Dir in dem Garten,", "tokens": ["Wohl", "bei", "Dir", "in", "dem", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Wohl bei Dir in dem Wald,", "tokens": ["Wohl", "bei", "Dir", "in", "dem", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wo dichte B\u00e4ume stehen", "tokens": ["Wo", "dich\u00b7te", "B\u00e4u\u00b7me", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und Vogelsang erschallt.", "tokens": ["Und", "Vo\u00b7gel\u00b7sang", "er\u00b7schallt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich kann kein' Kranz mehr flechten", "tokens": ["Ich", "kann", "kein'", "Kranz", "mehr", "flech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und singen auch nicht eh'", "tokens": ["Und", "sin\u00b7gen", "auch", "nicht", "eh'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "KOUS"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bis ich zu Dir Herzliebste", "tokens": ["Bis", "ich", "zu", "Dir", "Herz\u00b7liebs\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Durch Flur und Wald eingeh'.", "tokens": ["Durch", "Flur", "und", "Wald", "ein\u00b7geh'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sie dringt wohl durch die Wolken,", "tokens": ["Sie", "dringt", "wohl", "durch", "die", "Wol\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Geht ein durch T\u00fcr und Tor,", "tokens": ["Geht", "ein", "durch", "T\u00fcr", "und", "Tor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Fl\u00fcglein schnell ihr heilen", "tokens": ["Die", "Fl\u00fcg\u00b7lein", "schnell", "ihr", "hei\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und heben sie empor,", "tokens": ["Und", "he\u00b7ben", "sie", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wohl \u00fcber die Wolken und h\u00f6her", "tokens": ["Wohl", "\u00fc\u00b7ber", "die", "Wol\u00b7ken", "und", "h\u00f6\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "KON", "ADJD"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Zu Gott wohl in die H\u00f6h',", "tokens": ["Zu", "Gott", "wohl", "in", "die", "H\u00f6h'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Tr\u00e4gt sie das treue Herze,", "tokens": ["Tr\u00e4gt", "sie", "das", "treu\u00b7e", "Her\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ade, Herzlieber, ade! \u2013", "tokens": ["A\u00b7de", ",", "Herz\u00b7lie\u00b7ber", ",", "a\u00b7de", "!", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADV", "$.", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.6": {"line.1": {"text": "Er dringt wohl durch die Wolke,", "tokens": ["Er", "dringt", "wohl", "durch", "die", "Wol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Geht ein durch Flur und Wald,", "tokens": ["Geht", "ein", "durch", "Flur", "und", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Kranz wird ihm geflochten,", "tokens": ["Ein", "Kranz", "wird", "ihm", "ge\u00b7floch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Lied ihm auch erschallt,", "tokens": ["Ein", "Lied", "ihm", "auch", "er\u00b7schallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wohl unter dem Baum und wohl tiefer", "tokens": ["Wohl", "un\u00b7ter", "dem", "Baum", "und", "wohl", "tie\u00b7fer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "KON", "ADV", "ADJD"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Wohl unter gr\u00fcnem Klee", "tokens": ["Wohl", "un\u00b7ter", "gr\u00fc\u00b7nem", "Klee"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ruht nun sein stolzes Herze,", "tokens": ["Ruht", "nun", "sein", "stol\u00b7zes", "Her\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ade, Herzliebste, ade! \u2013", "tokens": ["A\u00b7de", ",", "Herz\u00b7liebs\u00b7te", ",", "a\u00b7de", "!", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "ADV", "$.", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}