{"textgrid.poem.26017": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "\u00dcberall bl\u00fcht nun die Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcberall bl\u00fcht nun die Liebe,", "tokens": ["\u00dc\u00b7be\u00b7rall", "bl\u00fcht", "nun", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df uns in die G\u00e4rten gehn,", "tokens": ["La\u00df", "uns", "in", "die", "G\u00e4r\u00b7ten", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die kleinen frommen Primeln", "tokens": ["Wo", "die", "klei\u00b7nen", "from\u00b7men", "Pri\u00b7meln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Z\u00e4rtlich schon in Paaren stehn.", "tokens": ["Z\u00e4rt\u00b7lich", "schon", "in", "Paa\u00b7ren", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In den G\u00e4rten, wo die Schritte", "tokens": ["In", "den", "G\u00e4r\u00b7ten", ",", "wo", "die", "Schrit\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Worte nicht mehr eilen,", "tokens": ["Und", "die", "Wor\u00b7te", "nicht", "mehr", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die Tr\u00e4ume unter wei\u00dfen s\u00fc\u00dfen B\u00e4umen", "tokens": ["Wo", "die", "Tr\u00e4u\u00b7me", "un\u00b7ter", "wei\u00b7\u00dfen", "s\u00fc\u00b7\u00dfen", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Wie in lauter Wolken weilen.", "tokens": ["Wie", "in", "lau\u00b7ter", "Wol\u00b7ken", "wei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Viele kleine trunkne V\u00f6gel", "tokens": ["Vie\u00b7le", "klei\u00b7ne", "trunk\u00b7ne", "V\u00f6\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommen dir ans Herz geflogen,", "tokens": ["Kom\u00b7men", "dir", "ans", "Herz", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sind vom Land, wo Honig flie\u00dft,", "tokens": ["Sind", "vom", "Land", ",", "wo", "Ho\u00b7nig", "flie\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "$,", "PWAV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit der Sonne hergezogen.", "tokens": ["Mit", "der", "Son\u00b7ne", "her\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Lausche mit versunknem Auge,", "tokens": ["Lau\u00b7sche", "mit", "ver\u00b7sun\u00b7knem", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Lippen wollen schw\u00f6ren.", "tokens": ["Mei\u00b7ne", "Lip\u00b7pen", "wol\u00b7len", "schw\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gib Erh\u00f6rung meinen Lippen,", "tokens": ["Gib", "Er\u00b7h\u00f6\u00b7rung", "mei\u00b7nen", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meinem ewigjungen Sehnen", "tokens": ["Mei\u00b7nem", "e\u00b7wig\u00b7jun\u00b7gen", "Seh\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gib Bet\u00f6ren.", "tokens": ["Gib", "Be\u00b7t\u00f6\u00b7ren", "."], "token_info": ["word", "word", "punct"], "pos": ["VVIMP", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}