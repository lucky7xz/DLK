{"dta.poem.21030": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "XcIV.  \n Einer alten Magd.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Ein ungebrauchtes Schlo\u00df/ ein ungen\u00fctzter Herd/", "tokens": ["Ein", "un\u00b7ge\u00b7brauch\u00b7tes", "Schlo\u00df", "/", "ein", "un\u00b7ge\u00b7n\u00fctz\u00b7ter", "Herd", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein K\u00f6cher ohne Pfeil/ ein unbeschnitten Pferd/", "tokens": ["Ein", "K\u00f6\u00b7cher", "oh\u00b7ne", "Pfeil", "/", "ein", "un\u00b7be\u00b7schnit\u00b7ten", "Pferd", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die k\u00f6nten solte man sie recht und wohl begra-", "tokens": ["Die", "k\u00f6n\u00b7ten", "sol\u00b7te", "man", "sie", "recht", "und", "wohl", "be\u00b7gra"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "VMFIN", "PIS", "PPER", "ADJD", "KON", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ben/", "tokens": ["ben", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Bey dieser alten Magd ein f\u00fcglich Leichmahl ha-", "tokens": ["Bey", "die\u00b7ser", "al\u00b7ten", "Magd", "ein", "f\u00fcg\u00b7lich", "Lei