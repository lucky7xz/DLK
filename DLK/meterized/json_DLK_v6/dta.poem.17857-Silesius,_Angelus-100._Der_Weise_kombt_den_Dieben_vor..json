{"dta.poem.17857": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "100.  Der Weise kombt den Dieben vor.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Der Weise wartet nicht/ bi\u00df ihm was wird genommen:", "tokens": ["Der", "Wei\u00b7se", "war\u00b7tet", "nicht", "/", "bi\u00df", "ihm", "was", "wird", "ge\u00b7nom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$(", "KOUS", "PPER", "PIS", "VAFIN", "VVPP", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Er nihmt ihm alles selbst/ den Dieben vorzukommen.", "tokens": ["Er", "nihmt", "ihm", "al\u00b7les", "selbst", "/", "den", "Die\u00b7ben", "vor\u00b7zu\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "ADV", "$(", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}