{"dta.poem.23553": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Angebinde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn wir auch ohne Ahnen sterben", "tokens": ["Wenn", "wir", "auch", "oh\u00b7ne", "Ah\u00b7nen", "ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ohne Adelsgl\u00fcck und Ruhm:", "tokens": ["Und", "oh\u00b7ne", "A\u00b7dels\u00b7gl\u00fcck", "und", "Ruhm", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O gl\u00fccklich, wenn wir dort ererben", "tokens": ["O", "gl\u00fcck\u00b7lich", ",", "wenn", "wir", "dort", "er\u00b7er\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "$,", "KOUS", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Gotteslehn zum Eigenthum!", "tokens": ["Ein", "Got\u00b7tes\u00b7lehn", "zum", "Ei\u00b7gen\u00b7thum", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Auch ist's ein Trost f\u00fcr unser Leben,", "tokens": ["Auch", "ist's", "ein", "Trost", "f\u00fcr", "un\u00b7ser", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr unsre schw\u00e4chliche Natur:", "tokens": ["F\u00fcr", "uns\u00b7re", "schw\u00e4ch\u00b7li\u00b7che", "Na\u00b7tur", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erbs\u00fcnde hat uns Gott gegeben,", "tokens": ["Erb\u00b7s\u00fcn\u00b7de", "hat", "uns", "Gott", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erbadel gaben wir uns nur.", "tokens": ["Er\u00b7ba\u00b7del", "ga\u00b7ben", "wir", "uns", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}