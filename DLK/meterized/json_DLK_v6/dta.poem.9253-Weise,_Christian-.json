{"dta.poem.9253": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Nun bin ich meiner noth entnommen/", "tokens": ["Nun", "bin", "ich", "mei\u00b7ner", "noth", "ent\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die trauer-zeit ist wieder um:", "tokens": ["Die", "trau\u00b7e\u00b7rzeit", "ist", "wie\u00b7der", "um", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Willkommen/ tausendmahl willkommen/", "tokens": ["Will\u00b7kom\u00b7men", "/", "tau\u00b7send\u00b7mahl", "will\u00b7kom\u00b7men", "/"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$(", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du liebstes Clavichordium/", "tokens": ["Du", "liebs\u00b7tes", "Cla\u00b7vi\u00b7chor\u00b7di\u00b7um", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So wilst du nun best\u00e4ndig bleiben", "tokens": ["So", "wilst", "du", "nun", "be\u00b7st\u00e4n\u00b7dig", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und mir die m\u00fcde zeit vertreiben.", "tokens": ["Und", "mir", "die", "m\u00fc\u00b7de", "zeit", "ver\u00b7trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach la\u00df die seiten nicht zerspringen/", "tokens": ["Ach", "la\u00df", "die", "sei\u00b7ten", "nicht", "zer\u00b7sprin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ART", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und la\u00df den an genehmen klang", "tokens": ["Und", "la\u00df", "den", "an", "ge\u00b7neh\u00b7men", "klang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "ART", "APPR", "ADJA", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch einen leisen griff erzwingen/", "tokens": ["Durch", "ei\u00b7nen", "lei\u00b7sen", "griff", "er\u00b7zwin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So wil ich dir zu grossen danck/", "tokens": ["So", "wil", "ich", "dir", "zu", "gros\u00b7sen", "danck", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die besten freunde zu ergetzen", "tokens": ["Die", "bes\u00b7ten", "freun\u00b7de", "zu", "er\u00b7get\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auff dir manch sch\u00f6nes st\u00fccke setzen.", "tokens": ["Auff", "dir", "manch", "sch\u00f6\u00b7nes", "st\u00fc\u00b7cke", "set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du darffst dich itzo nicht befahren", "tokens": ["Du", "darffst", "dich", "it\u00b7zo", "nicht", "be\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als wolt ich bald von hinnen ziehn/", "tokens": ["Als", "wolt", "ich", "bald", "von", "hin\u00b7nen", "ziehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "ADV", "APPR", "ADV", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nebenst andern fuhrmans-wahren", "tokens": ["Und", "ne\u00b7benst", "an\u00b7dern", "fuhr\u00b7mans\u00b7wah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dich in den rauhen pack bem\u00fchn/", "tokens": ["Dich", "in", "den", "rau\u00b7hen", "pack", "be\u00b7m\u00fchn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wenn ich auch die weichsten bette", "tokens": ["Und", "wenn", "ich", "auch", "die", "weichs\u00b7ten", "bet\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Um dich herum geschlagen h\u00e4tte.", "tokens": ["Um", "dich", "he\u00b7rum", "ge\u00b7schla\u00b7gen", "h\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "APZR", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach nein/ ich wei\u00df nicht was ich wolte/", "tokens": ["Ach", "nein", "/", "ich", "wei\u00df", "nicht", "was", "ich", "wol\u00b7te", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKANT", "$(", "PPER", "VVFIN", "PTKNEG", "PWS", "PPER", "VMFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Eh ich dich nur ein eintzig mahl", "tokens": ["Eh", "ich", "dich", "nur", "ein", "eint\u00b7zig", "mahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ART", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So vor die hunde werffen solte:", "tokens": ["So", "vor", "die", "hun\u00b7de", "werf\u00b7fen", "sol\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die kutscher m\u00f6gen stein und stahl", "tokens": ["Die", "kut\u00b7scher", "m\u00f6\u00b7gen", "stein", "und", "stahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In ihrer groben hand verwalten/", "tokens": ["In", "ih\u00b7rer", "gro\u00b7ben", "hand", "ver\u00b7wal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die k\u00f6nnen besser wiederhalten.", "tokens": ["Die", "k\u00f6n\u00b7nen", "bes\u00b7ser", "wie\u00b7der\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du aber/ bist an allen enden", "tokens": ["Du", "a\u00b7ber", "/", "bist", "an", "al\u00b7len", "en\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "$(", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als eine jungfrau zart und fein/", "tokens": ["Als", "ei\u00b7ne", "jung\u00b7frau", "zart", "und", "fein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wilst von lauter weichen h\u00e4nde", "tokens": ["Und", "wilst", "von", "lau\u00b7ter", "wei\u00b7chen", "h\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gantz niedlich angegriffen seyn/", "tokens": ["Gantz", "nied\u00b7lich", "an\u00b7ge\u00b7grif\u00b7fen", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es kan ein kleiner griff geschehen/", "tokens": ["Es", "kan", "ein", "klei\u00b7ner", "griff", "ge\u00b7sche\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So gibt es bald ein gro\u00df versehen.", "tokens": ["So", "gibt", "es", "bald", "ein", "gro\u00df", "ver\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Darum was wilstu ferner schweigen/", "tokens": ["Da\u00b7rum", "was", "wils\u00b7tu", "fer\u00b7ner", "schwei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PWS", "VMFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Komm/ stimme deine lieder an/", "tokens": ["Komm", "/", "stim\u00b7me", "dei\u00b7ne", "lie\u00b7der", "an", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich will dir in der that bezeugen/", "tokens": ["Ich", "will", "dir", "in", "der", "that", "be\u00b7zeu\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "ART", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich die griffe richtig kan/", "tokens": ["Da\u00df", "ich", "die", "grif\u00b7fe", "rich\u00b7tig", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "VVFIN", "ADJD", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und in dergleichen lust-gewerbe", "tokens": ["Und", "in", "derg\u00b7lei\u00b7chen", "lust\u00b7ge\u00b7wer\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit greiffen leichtlich nichts verderbe.", "tokens": ["Mit", "greif\u00b7fen", "leicht\u00b7lich", "nichts", "ver\u00b7der\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ADJD", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}