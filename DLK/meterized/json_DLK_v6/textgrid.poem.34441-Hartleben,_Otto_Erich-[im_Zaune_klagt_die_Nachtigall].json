{"textgrid.poem.34441": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "[im Zaune klagt die Nachtigall]", "genre": "verse", "period": "N.A.", "pub_year": 1886, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Zaune klagt die Nachtigall,", "tokens": ["Im", "Zau\u00b7ne", "klagt", "die", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "im Winde bebt der Flieder.", "tokens": ["im", "Win\u00b7de", "bebt", "der", "Flie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie fliegt auf seinen schwanken Zweig,", "tokens": ["Sie", "fliegt", "auf", "sei\u00b7nen", "schwan\u00b7ken", "Zweig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "da beugt sich die Bl\u00fcthe hernieder.", "tokens": ["da", "beugt", "sich", "die", "Bl\u00fc\u00b7the", "her\u00b7nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Sie beugt sich nieder tiefbegl\u00fcckt,", "tokens": ["Sie", "beugt", "sich", "nie\u00b7der", "tief\u00b7be\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und Regentropfen rinnen", "tokens": ["und", "Re\u00b7gen\u00b7trop\u00b7fen", "rin\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "wie Thr\u00e4nen in das Grab hinab ...", "tokens": ["wie", "Thr\u00e4\u00b7nen", "in", "das", "Grab", "hin\u00b7ab", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da flattert der Vogel von hinnen.", "tokens": ["Da", "flat\u00b7tert", "der", "Vo\u00b7gel", "von", "hin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADV", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}