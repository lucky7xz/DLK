{"textgrid.poem.52218": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Dorfgeschichten", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Knabe schon von Berg- und H\u00fcttenm\u00e4nnern", "tokens": ["Als", "Kna\u00b7be", "schon", "von", "Ber\u00b7g", "und", "H\u00fct\u00b7ten\u00b7m\u00e4n\u00b7nern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADV", "APPR", "TRUNC", "KON", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Hab' ich entz\u00fcckt ein kleines Buch gelesen;", "tokens": ["Hab'", "ich", "ent\u00b7z\u00fcckt", "ein", "klei\u00b7nes", "Buch", "ge\u00b7le\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es f\u00fchrte mich zu frommen Kohlenbrennern,", "tokens": ["Es", "f\u00fchr\u00b7te", "mich", "zu", "from\u00b7men", "Koh\u00b7len\u00b7bren\u00b7nern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ist ein herzigs kleines Buch gewesen,", "tokens": ["Und", "ist", "ein", "her\u00b7zigs", "klei\u00b7nes", "Buch", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "ADJA", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein rechter Spiegel alter Bauerntugend; \u2013", "tokens": ["Ein", "rech\u00b7ter", "Spie\u00b7gel", "al\u00b7ter", "Bau\u00b7ern\u00b7tu\u00b7gend", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit Namen hie\u00df es: Heinrich Stillings Jugend.", "tokens": ["Mit", "Na\u00b7men", "hie\u00df", "es", ":", "Hein\u00b7rich", "Stil\u00b7lings", "Ju\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$.", "NE", "NE", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das war die erste deutsche Dorfgeschichte!", "tokens": ["Das", "war", "die", "ers\u00b7te", "deut\u00b7sche", "Dorf\u00b7ge\u00b7schich\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die hat mit Lied, mit M\u00e4rchen und mit Sage,", "tokens": ["Die", "hat", "mit", "Lied", ",", "mit", "M\u00e4r\u00b7chen", "und", "mit", "Sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NN", "$,", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die hat in Einfalt und in edler Schlichte", "tokens": ["Die", "hat", "in", "Ein\u00b7falt", "und", "in", "ed\u00b7ler", "Schlich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "NN", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Gold im Volke treu gesch\u00fcrft zutage;", "tokens": ["Das", "Gold", "im", "Vol\u00b7ke", "treu", "ge\u00b7sch\u00fcrft", "zu\u00b7ta\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ADJD", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die lie\u00df mich schaun durch ihrer Meiler Schwelen", "tokens": ["Die", "lie\u00df", "mich", "schaun", "durch", "ih\u00b7rer", "Mei\u00b7ler", "Schwe\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Im festen Umri\u00df starke, mut'ge Seelen.", "tokens": ["Im", "fes\u00b7ten", "Um\u00b7ri\u00df", "star\u00b7ke", ",", "mut'\u00b7ge", "See\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nach diesem auch hat Pestalozz geschrieben", "tokens": ["Nach", "die\u00b7sem", "auch", "hat", "Pes\u00b7ta\u00b7lozz", "ge\u00b7schrie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADV", "VAFIN", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von t\u00fccht'gen Herzen unter schlechtem Kittel:", "tokens": ["Von", "t\u00fccht'\u00b7gen", "Her\u00b7zen", "un\u00b7ter", "schlech\u00b7tem", "Kit\u00b7tel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie die Geringen dulden, hoffen, lieben \u2013", "tokens": ["Wie", "die", "Ge\u00b7rin\u00b7gen", "dul\u00b7den", ",", "hof\u00b7fen", ",", "lie\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Lienhard und Gertrud ist des Buches Titel.", "tokens": ["Li\u00b7en\u00b7hard", "und", "Ger\u00b7trud", "ist", "des", "Bu\u00b7ches", "Ti\u00b7tel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Oft las ich es \u2013 mit Augen, ach! die quollen! \u2013", "tokens": ["Oft", "las", "ich", "es", "\u2013", "mit", "Au\u00b7gen", ",", "ach", "!", "die", "quol\u00b7len", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$(", "APPR", "NN", "$,", "ITJ", "$.", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nun ist es auch wohl, jenem gleich, verschollen!", "tokens": ["Nun", "ist", "es", "auch", "wohl", ",", "je\u00b7nem", "gleich", ",", "ver\u00b7schol\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "$,", "PDAT", "ADV", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dann kam Brentano! Wie mit Blutestropfen", "tokens": ["Dann", "kam", "Bren\u00b7ta\u00b7no", "!", "Wie", "mit", "Blu\u00b7tes\u00b7trop\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "$.", "PWAV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schrieb er sein Annerl in gewalt'gen Z\u00fcgen!", "tokens": ["Schrieb", "er", "sein", "An\u00b7nerl", "in", "ge\u00b7walt'\u00b7gen", "Z\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der wu\u00dft' es wohl, wie niedre Herzen klopfen,", "tokens": ["Der", "wu\u00dft'", "es", "wohl", ",", "wie", "nied\u00b7re", "Her\u00b7zen", "klop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$,", "PWAV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wie so hei\u00df des Volkes Pulse fliegen!", "tokens": ["Und", "wie", "so", "hei\u00df", "des", "Vol\u00b7kes", "Pul\u00b7se", "flie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "ADJD", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der warf zuerst aus grauer B\u00fccherwolke", "tokens": ["Der", "warf", "zu\u00b7erst", "aus", "grau\u00b7er", "B\u00fc\u00b7cher\u00b7wol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Den pr\u00e4cht'gen Blitz: die ", "tokens": ["Den", "pr\u00e4cht'\u00b7gen", "Blitz", ":", "die"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ART"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Da sitzt die Lisbeth bei den Hofeseichen;", "tokens": ["Da", "sitzt", "die", "Lis\u00b7be\u00b7th", "bei", "den", "Ho\u00b7fe\u00b7sei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von seinen Knechten aber steht umgeben", "tokens": ["Von", "sei\u00b7nen", "Knech\u00b7ten", "a\u00b7ber", "steht", "um\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVFIN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Patriarch, der Hofschulz sondergleichen;", "tokens": ["Der", "Pat\u00b7ri\u00b7arch", ",", "der", "Hof\u00b7schulz", "son\u00b7derg\u00b7lei\u00b7chen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Fels von Mann, ein gold- und eisenhalt'ger!", "tokens": ["Ein", "Fels", "von", "Mann", ",", "ein", "gold", "und", "ei\u00b7sen\u00b7halt'\u00b7ger", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "ART", "TRUNC", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein j\u00fcngrer Ebert Stiling \u2013 nur gewalt'ger!", "tokens": ["Ein", "j\u00fcng\u00b7rer", "E\u00b7bert", "Sti\u00b7ling", "\u2013", "nur", "ge\u00b7walt'\u00b7ger", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "NE", "$(", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Als f\u00fcnfter nun gesellst du dich zu diesen,", "tokens": ["Als", "f\u00fcnf\u00b7ter", "nun", "ge\u00b7sellst", "du", "dich", "zu", "die\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "ADV", "VVFIN", "PPER", "PRF", "APPR", "PDAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die treu geschildert einfach kr\u00e4ft'ge Sitten;", "tokens": ["Die", "treu", "ge\u00b7schil\u00b7dert", "ein\u00b7fach", "kr\u00e4ft'\u00b7ge", "Sit\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus deines Schwarzwalds tannendunkeln Wiesen", "tokens": ["Aus", "dei\u00b7nes", "Schwarz\u00b7walds", "tan\u00b7nen\u00b7dun\u00b7keln", "Wie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit seinen Kindern kommst du froh geschritten", "tokens": ["Mit", "sei\u00b7nen", "Kin\u00b7dern", "kommst", "du", "froh", "ge\u00b7schrit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und setzest ein das Tuchwams und die Flechte", "tokens": ["Und", "set\u00b7zest", "ein", "das", "Tuch\u00b7wams", "und", "die", "Flech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In ihre alten dichterischen Rechte!", "tokens": ["In", "ih\u00b7re", "al\u00b7ten", "dich\u00b7te\u00b7ri\u00b7schen", "Rech\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Das ist ein Buch! Ich kann es dir nicht sagen,", "tokens": ["Das", "ist", "ein", "Buch", "!", "Ich", "kann", "es", "dir", "nicht", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$.", "PPER", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie mich's gepackt hat recht in tiefer Seele;", "tokens": ["Wie", "mich's", "ge\u00b7packt", "hat", "recht", "in", "tie\u00b7fer", "See\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVPP", "VAFIN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie mir das Herz bei diesem Blatt geschlagen,", "tokens": ["Wie", "mir", "das", "Herz", "bei", "die\u00b7sem", "Blatt", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wie mir jenes zugeschn\u00fcrt die Kehle;", "tokens": ["Und", "wie", "mir", "je\u00b7nes", "zu\u00b7ge\u00b7schn\u00fcrt", "die", "Keh\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PDS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wie ich bei dem die Lippen hab' gebissen", "tokens": ["Wie", "ich", "bei", "dem", "die", "Lip\u00b7pen", "hab'", "ge\u00b7bis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "PRELS", "ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und wieder dann hellauf hab' lachen m\u00fcssen!", "tokens": ["Und", "wie\u00b7der", "dann", "hel\u00b7lauf", "hab'", "la\u00b7chen", "m\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "VAFIN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Das alles aber ist dir nun gelungen,", "tokens": ["Das", "al\u00b7les", "a\u00b7ber", "ist", "dir", "nun", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weil du dein Werk am Leben lie\u00dfest reifen;", "tokens": ["Weil", "du", "dein", "Werk", "am", "Le\u00b7ben", "lie\u00b7\u00dfest", "rei\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was aus dem Leben frisch hervorgesprungen,", "tokens": ["Was", "aus", "dem", "Le\u00b7ben", "frisch", "her\u00b7vor\u00b7ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wird wie das Leben selber auch ergreifen,", "tokens": ["Wird", "wie", "das", "Le\u00b7ben", "sel\u00b7ber", "auch", "er\u00b7grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und rechts und links mit Wonnen und mit Schmerzen", "tokens": ["Und", "rechts", "und", "links", "mit", "Won\u00b7nen", "und", "mit", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KON", "ADV", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sturmschritts erobern warme Menschenherzen!", "tokens": ["Sturm\u00b7schritts", "er\u00b7o\u00b7bern", "war\u00b7me", "Men\u00b7schen\u00b7her\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "p