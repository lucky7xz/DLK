{"textgrid.poem.66829": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Ohr horcht auf: ist keiner heut", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Ohr horcht auf: ist keiner heut", "tokens": ["Mein", "Ohr", "horcht", "auf", ":", "ist", "kei\u00b7ner", "heut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$.", "VAFIN", "PIS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "vom Volk der Dichter, der noch wagt,", "tokens": ["vom", "Volk", "der", "Dich\u00b7ter", ",", "der", "noch", "wagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu singen, wie der Geist gebeut,", "tokens": ["Zu", "sin\u00b7gen", ",", "wie", "der", "Geist", "ge\u00b7beut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "zu sagen, was die Wahrheit sagt?", "tokens": ["zu", "sa\u00b7gen", ",", "was", "die", "Wahr\u00b7heit", "sagt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Ohr horcht auf: ich harre bang", "tokens": ["Mein", "Ohr", "horcht", "auf", ":", "ich", "har\u00b7re", "bang"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$.", "PPER", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und irre durch die Nacht umher,", "tokens": ["und", "ir\u00b7re", "durch", "die", "Nacht", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Verstummt ist deutscher Freiheit Sang,", "tokens": ["Ver\u00b7stummt", "ist", "deut\u00b7scher", "Frei\u00b7heit", "Sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "die stolze Weise t\u00f6nt nicht mehr.", "tokens": ["die", "stol\u00b7ze", "Wei\u00b7se", "t\u00f6nt", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie Froschgequak aus Sumpfgefild,", "tokens": ["Wie", "Froschge\u00b7quak", "aus", "Sumpf\u00b7ge\u00b7fild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "$,"], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "so schallt der satten Schmeichler Chor,", "tokens": ["so", "schallt", "der", "sat\u00b7ten", "Schmeich\u00b7ler", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gepl\u00e4rr um ein Gigantenbild,", "tokens": ["Ge\u00b7pl\u00e4rr", "um", "ein", "Gi\u00b7gan\u00b7ten\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "so plappert es im d\u00fcrren Rohr.", "tokens": ["so", "plap\u00b7pert", "es", "im", "d\u00fcr\u00b7ren", "Rohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie Botokuden, bumbumbum,", "tokens": ["Wie", "Bo\u00b7to\u00b7ku\u00b7den", ",", "bum\u00b7bum\u00b7bum", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PWAV", "NN", "$,", "XY", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "mit Trommeln gr\u00f6hlen dumpf und hohl,", "tokens": ["mit", "Trom\u00b7meln", "gr\u00f6h\u00b7len", "dumpf", "und", "hohl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So h\u00fcpft das Rudel rings herum", "tokens": ["So", "h\u00fcpft", "das", "Ru\u00b7del", "rings", "he\u00b7rum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "um sein verg\u00f6ttertes Idol.", "tokens": ["um", "sein", "ver\u00b7g\u00f6t\u00b7ter\u00b7tes", "I\u00b7dol", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Recht wie ein G\u00f6tz \u2013 das Volk sein Narr \u2013", "tokens": ["Recht", "wie", "ein", "G\u00f6tz", "\u2013", "das", "Volk", "sein", "Narr", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$(", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "reckt sich der riesige K\u00fcrassier", "tokens": ["reckt", "sich", "der", "rie\u00b7si\u00b7ge", "K\u00fc\u00b7ras\u00b7sier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Und glotzt ver\u00e4chtlich, maskenstarr", "tokens": ["Und", "glotzt", "ver\u00b7\u00e4cht\u00b7lich", ",", "mas\u00b7kens\u00b7tarr"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "ADJD", "$,", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "herunter auf das Sumpfgetier.", "tokens": ["her\u00b7un\u00b7ter", "auf", "das", "Sumpf\u00b7ge\u00b7tier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es paukt und klappert, bl\u00f6kt und br\u00fcllt", "tokens": ["Es", "paukt", "und", "klap\u00b7pert", ",", "bl\u00f6kt", "und", "br\u00fcllt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "der siegestolle Schwindelschwarm,", "tokens": ["der", "sie\u00b7ge\u00b7stol\u00b7le", "Schwin\u00b7del\u00b7schwarm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vom Geist des Gr\u00f6\u00dfenwahns erf\u00fcllt,", "tokens": ["Vom", "Geist", "des", "Gr\u00f6\u00b7\u00dfen\u00b7wahns", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "an Geistesgr\u00f6\u00dfe bettelarm.", "tokens": ["an", "Geis\u00b7tes\u00b7gr\u00f6\u00b7\u00dfe", "bet\u00b7tel\u00b7arm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Grell rasselt durch die Nacht der L\u00e4rm", "tokens": ["Grell", "ras\u00b7selt", "durch", "die", "Nacht", "der", "L\u00e4rm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "im Lande Lessings, Schillers, Kants,", "tokens": ["im", "Lan\u00b7de", "Les\u00b7sings", ",", "Schil\u00b7lers", ",", "Kants", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "NE", "$,", "NE", "$,", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir w\u00fchlt der Ekel ins Ged\u00e4rm,", "tokens": ["Mir", "w\u00fchlt", "der", "E\u00b7kel", "ins", "Ge\u00b7d\u00e4rm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "mich packt die Scham des Vaterlands.", "tokens": ["mich", "packt", "die", "Scham", "des", "Va\u00b7ter\u00b7lands", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Germania, heiliger Mutterleib,", "tokens": ["Ger\u00b7ma\u00b7nia", ",", "hei\u00b7li\u00b7ger", "Mut\u00b7ter\u00b7leib", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "mu\u00df dich verachten, wer dich liebt?", "tokens": ["mu\u00df", "dich", "ver\u00b7ach\u00b7ten", ",", "wer", "dich", "liebt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mein Antlitz kehr' ich von dem Weib,", "tokens": ["Mein", "Ant\u00b7litz", "kehr'", "ich", "von", "dem", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "das geilem Hochmut sich ergibt.", "tokens": ["das", "gei\u00b7lem", "Hoch\u00b7mut", "sich", "er\u00b7gibt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Mein Antlitz von der Mutter Schmach", "tokens": ["Mein", "Ant\u00b7litz", "von", "der", "Mut\u00b7ter", "Schmach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "heb' ich dem Bild der Ahnen zu,", "tokens": ["heb'", "ich", "dem", "Bild", "der", "Ah\u00b7nen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df Keuschheit so in Scherben brach,", "tokens": ["Da\u00df", "Keuschheit", "so", "in", "Scher\u00b7ben", "brach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "ersch\u00fcttert meines Geistes Ruh.", "tokens": ["er\u00b7sch\u00fct\u00b7tert", "mei\u00b7nes", "Geis\u00b7tes", "Ruh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie mich der Schande Sturm durchbraust!", "tokens": ["Wie", "mich", "der", "Schan\u00b7de", "Sturm", "durch\u00b7braust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "An Goethe, Fichte h\u00e4ngt mein Blick.", "tokens": ["An", "Goe\u00b7the", ",", "Fich\u00b7te", "h\u00e4ngt", "mein", "Blick", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u00bbo deutsche Menschen!\u00ab Weh, die Faust", "tokens": ["\u00bb", "o", "deut\u00b7sche", "Men\u00b7schen", "!", "\u00ab", "Weh", ",", "die", "Faust"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word"], "pos": ["$(", "FM", "ADJA", "NN", "$.", "$(", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "des B\u00fcttels saust mir ins Genick.", "tokens": ["des", "B\u00fct\u00b7tels", "saust", "mir", "ins", "Ge\u00b7nick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Entstellt zum Fetischkult die Kraft,", "tokens": ["Ent\u00b7stellt", "zum", "Fe\u00b7tischkult", "die", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "dein Stolz zu Windeis aufgebl\u00e4ht,", "tokens": ["dein", "Stolz", "zu", "Win\u00b7deis", "auf\u00b7ge\u00b7bl\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Entweiht zur hohlen Machenschaft", "tokens": ["Ent\u00b7weiht", "zur", "hoh\u00b7len", "Ma\u00b7chen\u00b7schaft"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "das Heiligtum der Majest\u00e4t.", "tokens": ["das", "Hei\u00b7lig\u00b7tum", "der", "Ma\u00b7jes\u00b7t\u00e4t", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Versteint der selbstbewu\u00dfte Sinn", "tokens": ["Ver\u00b7steint", "der", "selbst\u00b7be\u00b7wu\u00df\u00b7te", "Sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "zur d\u00fcnkelhaften Schneidigkeit,", "tokens": ["zur", "d\u00fcn\u00b7kel\u00b7haf\u00b7ten", "Schnei\u00b7dig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dein scharf Gepr\u00e4ge gabst du hin", "tokens": ["Dein", "scharf", "Ge\u00b7pr\u00e4\u00b7ge", "gabst", "du", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "f\u00fcr platteste Geschmeidigkeit.", "tokens": ["f\u00fcr", "plat\u00b7tes\u00b7te", "Ge\u00b7schmei\u00b7dig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Einst glaubt' ich hei\u00df, dein Ruhmesschild", "tokens": ["Einst", "glaubt'", "ich", "hei\u00df", ",", "dein", "Ruh\u00b7mes\u00b7schild"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "