{"textgrid.poem.55825": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Ritter", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Reitet der Ritter in schwarzem Stahl", "tokens": ["Rei\u00b7tet", "der", "Rit\u00b7ter", "in", "schwar\u00b7zem", "Stahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "hinaus in die rauschende Welt.", "tokens": ["hin\u00b7aus", "in", "die", "rau\u00b7schen\u00b7de", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Und drau\u00dfen ist Alles: der Tag und das Tal", "tokens": ["Und", "drau\u00b7\u00dfen", "ist", "Al\u00b7les", ":", "der", "Tag", "und", "das", "Tal"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PIS", "$.", "ART", "NN", "KON", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "und der Freund und der Feind und das Mahl im Saal", "tokens": ["und", "der", "Freund", "und", "der", "Feind", "und", "das", "Mahl", "im", "Saal"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "KON", "ART", "NN", "KON", "ART", "NN", "APPRART", "NN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "und der Mai und die Maid und der Wald und der Gral,", "tokens": ["und", "der", "Mai", "und", "die", "Maid", "und", "der", "Wald", "und", "der", "Gral", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "KON", "ART", "NN", "KON", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "und Gott ist selber vieltausendmal", "tokens": ["und", "Gott", "ist", "sel\u00b7ber", "viel\u00b7tau\u00b7send\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "ADV", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "an alle Stra\u00dfen gestellt.", "tokens": ["an", "al\u00b7le", "Stra\u00b7\u00dfen", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Doch in dem Panzer des Ritters drinnen,", "tokens": ["Doch", "in", "dem", "Pan\u00b7zer", "des", "Rit\u00b7ters", "drin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "hinter den finstersten Ringen,", "tokens": ["hin\u00b7ter", "den", "fins\u00b7ters\u00b7ten", "Rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "hockt der Tod und mu\u00df sinnen und sinnen:", "tokens": ["hockt", "der", "Tod", "und", "mu\u00df", "sin\u00b7nen", "und", "sin\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VMFIN", "VVINF", "KON", "VVINF", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wann wird die Klinge springen", "tokens": ["Wann", "wird", "die", "Klin\u00b7ge", "sprin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u00fcber die Eisenhecke,", "tokens": ["\u00fc\u00b7ber", "die", "Ei\u00b7sen\u00b7he\u00b7cke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "die fremde befreiende Klinge,", "tokens": ["die", "frem\u00b7de", "be\u00b7frei\u00b7en\u00b7de", "Klin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "die mich aus meinem Verstecke", "tokens": ["die", "mich", "aus", "mei\u00b7nem", "Ver\u00b7ste\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRELS", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "holt, drin ich so viele", "tokens": ["holt", ",", "drin", "ich", "so", "vie\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADV", "PPER", "ADV", "PIAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "geb\u00fcckte Tage verbringe, \u2013", "tokens": ["ge\u00b7b\u00fcck\u00b7te", "Ta\u00b7ge", "ver\u00b7brin\u00b7ge", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "da\u00df ich mich endlich strecke", "tokens": ["da\u00df", "ich", "mich", "end\u00b7lich", "stre\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "und spiele", "tokens": ["und", "spie\u00b7le"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "und singe.", "tokens": ["und", "sin\u00b7ge", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}