{"textgrid.poem.57302": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: An der Eiche Spr\u00f6ssling gelehnt, von hellen", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An der Eiche Spr\u00f6ssling gelehnt, von hellen", "tokens": ["An", "der", "Ei\u00b7che", "Spr\u00f6ss\u00b7ling", "ge\u00b7lehnt", ",", "von", "hel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,", "APPR", "ADJA"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "D\u00fcften umh\u00fcllt, stand die Telyn, und schnell", "tokens": ["D\u00fcf\u00b7ten", "um\u00b7h\u00fcllt", ",", "stand", "die", "Te\u00b7lyn", ",", "und", "schnell"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "$,", "VVFIN", "ART", "NN", "$,", "KON", "ADJD"], "meter": "+--+---+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Erscholl sie von selbst; doch ich liess", "tokens": ["Er\u00b7scholl", "sie", "von", "selbst", ";", "doch", "ich", "liess"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "PPER", "APPR", "ADV", "$.", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Unerweckt sie mir erschallen.", "tokens": ["Un\u00b7er\u00b7weckt", "sie", "mir", "er\u00b7schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da entstr\u00f6mt' ihr rascher Verdruss, da z\u00fcrnte", "tokens": ["Da", "ent\u00b7str\u00f6mt'", "ihr", "ra\u00b7scher", "Ver\u00b7druss", ",", "da", "z\u00fcrn\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$,", "ADV", "VVFIN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wirbelnd ihr Ton! Eilend ging ich, und nahm", "tokens": ["Wir\u00b7belnd", "ihr", "Ton", "!", "Ei\u00b7lend", "ging", "ich", ",", "und", "nahm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "PPOSAT", "NN", "$.", "NN", "VVFIN", "PPER", "$,", "KON", "VVFIN"], "meter": "-+-++-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Die drohende, dass sie dereinst", "tokens": ["Die", "dro\u00b7hen\u00b7de", ",", "dass", "sie", "de\u00b7reinst"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Vergelt nicht mir verstumte.", "tokens": ["Zum", "Ver\u00b7gelt", "nicht", "mir", "ver\u00b7stum\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aus des Rosses Auge, des Hufs Erhebung,", "tokens": ["Aus", "des", "Ros\u00b7ses", "Au\u00b7ge", ",", "des", "Hufs", "Er\u00b7he\u00b7bung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Stampfen des Hufs, Schnauben, Wiehern und Sprung", "tokens": ["Stamp\u00b7fen", "des", "Hufs", ",", "Schnau\u00b7ben", ",", "Wie\u00b7hern", "und", "Sprung"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+---+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Weissagten die Barden; auch mir", "tokens": ["Weis\u00b7sag\u00b7ten", "die", "Bar\u00b7den", ";", "auch", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$.", "ADV", "PPER"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Ist der Blick hell in die Zukunft.", "tokens": ["Ist", "der", "Blick", "hell", "in", "die", "Zu\u00b7kunft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Obs auf immer laste? Dein Joch, o Deutschland,", "tokens": ["Obs", "auf", "im\u00b7mer", "las\u00b7te", "?", "Dein", "Joch", ",", "o", "Deutschland", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ADV", "VVFIN", "$.", "PPOSAT", "NN", "$,", "FM", "NE", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sinket dereinst! Ein Jahrhundert nur noch;", "tokens": ["Sin\u00b7ket", "de\u00b7reinst", "!", "Ein", "Jahr\u00b7hun\u00b7dert", "nur", "noch", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$.", "ART", "NN", "ADV", "ADV", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "So ist es geschehen, so herscht", "tokens": ["So", "ist", "es", "ge\u00b7sche\u00b7hen", ",", "so", "herscht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,", "ADV", "VVFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Der Vernunft Recht vor dem Schwertrecht!", "tokens": ["Der", "Ver\u00b7nunft", "Recht", "vor", "dem", "Schwert\u00b7recht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Denn im Haine brauset' es her gehohnes", "tokens": ["Denn", "im", "Hai\u00b7ne", "brau\u00b7set'", "es", "her", "ge\u00b7hoh\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "ADV", "ADJA"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Halses, und sprang, Flug die M\u00e4hne, dahin", "tokens": ["Hal\u00b7ses", ",", "und", "sprang", ",", "Flug", "die", "M\u00e4h\u00b7ne", ",", "da\u00b7hin"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NE", "$,", "KON", "VVFIN", "$,", "NN", "ART", "NN", "$,", "PAV"], "meter": "+--++-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Das heilige Ross, und ein Spott", "tokens": ["Das", "hei\u00b7li\u00b7ge", "Ross", ",", "und", "ein", "Spott"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "War der Sturm ihm, und der Strom ihm!", "tokens": ["War", "der", "Sturm", "ihm", ",", "und", "der", "Strom", "ihm", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "$,", "KON", "ART", "NN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Auf der Wiese stand es, und stampft', und blickte", "tokens": ["Auf", "der", "Wie\u00b7se", "stand", "es", ",", "und", "stampft'", ",", "und", "blick\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "$,", "KON", "VVFIN", "$,", "KON", "VVFIN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Wiehernd umher; sorglos weidet' es, sah", "tokens": ["Wie\u00b7hernd", "um\u00b7her", ";", "sorg\u00b7los", "wei\u00b7det'", "es", ",", "sah"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "PTKVZ", "$.", "ADJD", "VVFIN", "PPER", "$,", "VVFIN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Voll Stolz nach dem Reiter nicht hin,", "tokens": ["Voll", "Stolz", "nach", "dem", "Rei\u00b7ter", "nicht", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "ART", "NN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Der im Blut lag an dem Gr\u00e4nzstein!", "tokens": ["Der", "im", "Blut", "lag", "an", "dem", "Gr\u00e4nz\u00b7stein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Nicht auf immer lastet es! Frey, o Deutschland,", "tokens": ["Nicht", "auf", "im\u00b7mer", "las\u00b7tet", "es", "!", "Frey", ",", "o", "Deutschland", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADV", "VVFIN", "PPER", "$.", "NN", "$,", "FM", "NE", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wirst du dereinst! Ein Jahrhundert nur noch;", "tokens": ["Wirst", "du", "de\u00b7reinst", "!", "Ein", "Jahr\u00b7hun\u00b7dert", "nur", "noch", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$.", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So ist es geschehen, so herscht", "tokens": ["So", "ist", "es", "ge\u00b7sche\u00b7hen", ",", "so", "herscht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,", "ADV", "VVFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Der Vernunft Recht vor dem Schwertrecht!", "tokens": ["Der", "Ver\u00b7nunft", "Recht", "vor", "dem", "Schwert\u00b7recht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}