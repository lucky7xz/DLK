{"dta.poem.11822": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "108.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Leier immer h\u00e4ngt gestimmt in meiner Klause,", "tokens": ["Die", "Lei\u00b7er", "im\u00b7mer", "h\u00e4ngt", "ge\u00b7stimmt", "in", "mei\u00b7ner", "Klau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wartet, welch ein Sturm durch ihre Saiten brause.", "tokens": ["Und", "war\u00b7tet", ",", "welch", "ein", "Sturm", "durch", "ih\u00b7re", "Sai\u00b7ten", "brau\u00b7se", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAT", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Bald ists des Himmels Sturm, der die Akkorde greift,", "tokens": ["Bald", "ists", "des", "Him\u00b7mels", "Sturm", ",", "der", "die", "Ak\u00b7kor\u00b7de", "greift", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und bald des Dichters Geist, der sie im Fluge streift.", "tokens": ["Und", "bald", "des", "Dich\u00b7ters", "Geist", ",", "der", "sie", "im", "Flu\u00b7ge", "streift", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn du, o Sturm der Nacht, aufspielest, h\u00f6r' ich zu;", "tokens": ["Wenn", "du", ",", "o", "Sturm", "der", "Nacht", ",", "auf\u00b7spie\u00b7lest", ",", "h\u00f6r'", "ich", "zu", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "FM", "NN", "ART", "NN", "$,", "VVFIN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und bist du m\u00fcd', und ich will spielen, h\u00f6re du!", "tokens": ["Und", "bist", "du", "m\u00fcd'", ",", "und", "ich", "will", "spie\u00b7len", ",", "h\u00f6\u00b7re", "du", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VMFIN", "$,", "KON", "PPER", "VMFIN", "VVINF", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Geheimnisse der Nacht hast du mir vorgesungen,", "tokens": ["Ge\u00b7heim\u00b7nis\u00b7se", "der", "Nacht", "hast", "du", "mir", "vor\u00b7ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VAFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun h\u00f6r' ein Lied aus Menschenbusensd\u00e4mmerungen.", "tokens": ["Nun", "h\u00f6r'", "ein", "Lied", "aus", "Men\u00b7schen\u00b7bu\u00b7sens\u00b7d\u00e4m\u00b7me\u00b7run\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}