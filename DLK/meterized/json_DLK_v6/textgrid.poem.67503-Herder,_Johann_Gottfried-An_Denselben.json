{"textgrid.poem.67503": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "An Denselben", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund, mein lechzender Mund kann keine Worte Dir sagen;", "tokens": ["Freund", ",", "mein", "lech\u00b7zen\u00b7der", "Mund", "kann", "kei\u00b7ne", "Wor\u00b7te", "Dir", "sa\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "ADJA", "NN", "VMFIN", "PIAT", "NN", "PPER", "VVINF", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Aber mein innigster Geist nahet umarmend sich Dir,", "tokens": ["A\u00b7ber", "mein", "in\u00b7nigs\u00b7ter", "Geist", "na\u00b7het", "um\u00b7ar\u00b7mend", "sich", "Dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "VVPP", "PRF", "PPER", "$,"], "meter": "+-+-+--+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Siehet neben Dir stehn den Genius Deines Lebens,", "tokens": ["Sie\u00b7het", "ne\u00b7ben", "Dir", "stehn", "den", "Ge\u00b7nius", "Dei\u00b7nes", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Des verflossenen Jahrs Genius segnen Dich heut.", "tokens": ["Des", "ver\u00b7flos\u00b7se\u00b7nen", "Jahrs", "Ge\u00b7nius", "seg\u00b7nen", "Dich", "heut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "VVFIN", "PPER", "ADV", "$."], "meter": "--+--+-++--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Kr\u00e4nz ihn, himmlischer J\u00fcngling, mit Deinem s\u00fc\u00dfesten Oelzweig;", "tokens": ["Kr\u00e4nz", "ihn", ",", "himm\u00b7li\u00b7scher", "J\u00fcng\u00b7ling", ",", "mit", "Dei\u00b7nem", "s\u00fc\u00b7\u00dfes\u00b7ten", "O\u00b7el\u00b7zweig", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADJA", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+--+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Aber f\u00fcr uns flich auch Rosen der Freundschaft hinein!", "tokens": ["A\u00b7ber", "f\u00fcr", "uns", "flich", "auch", "Ro\u00b7sen", "der", "Freund\u00b7schaft", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADJD", "ADV", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+---+-+--+--+", "measure": "trochaic.penta.relaxed"}}}}}