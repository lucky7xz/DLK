{"textgrid.poem.60796": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Halbdichter", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das nicht hei\u00dft ein Gedicht, wenn irgend ein guter Gedanke,", "tokens": ["Das", "nicht", "hei\u00dft", "ein", "Ge\u00b7dicht", ",", "wenn", "ir\u00b7gend", "ein", "gu\u00b7ter", "Ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "VVFIN", "ART", "NN", "$,", "KOUS", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Irgend ein Gl\u00fccklicher Vers zwischen erb\u00e4rmlichen steht:", "tokens": ["Ir\u00b7gend", "ein", "Gl\u00fcck\u00b7li\u00b7cher", "Vers", "zwi\u00b7schen", "er\u00b7b\u00e4rm\u00b7li\u00b7chen", "steht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF", "VVINF", "VVFIN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Jegliche Silbe verrate den Dichter, wofern er es ganz ist,", "tokens": ["Jeg\u00b7li\u00b7che", "Sil\u00b7be", "ver\u00b7ra\u00b7te", "den", "Dich\u00b7ter", ",", "wo\u00b7fern", "er", "es", "ganz", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "PPER", "ADV", "VAFIN", "$,"], "meter": "+--+--+--+--+-+-+", "measure": "dactylic.tetra.plus"}, "line.4": {"text": "Was er gedacht, scheint uns niedergeschrieben in Erz.", "tokens": ["Was", "er", "ge\u00b7dacht", ",", "scheint", "uns", "nie\u00b7der\u00b7ge\u00b7schrie\u00b7ben", "in", "Erz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}}}}}