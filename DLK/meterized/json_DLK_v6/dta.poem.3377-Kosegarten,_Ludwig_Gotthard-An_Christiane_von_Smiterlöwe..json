xt": "Eifer, den Bedr\u00e4ngten zu vertreten,", "tokens": ["Ei\u00b7fer", ",", "den", "Be\u00b7dr\u00e4ng\u00b7ten", "zu", "ver\u00b7tre\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "++--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Leiht auch matten Augen Glanz und Licht.", "tokens": ["Leiht", "auch", "mat\u00b7ten", "Au\u00b7gen", "Glanz", "und", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Leuchtender, als alle Morgenr\u00f6then,", "tokens": ["Leuch\u00b7ten\u00b7der", ",", "als", "al\u00b7le", "Mor\u00b7gen\u00b7r\u00f6\u00b7then", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Leuchtet, Menschlichkeit, dein Angesicht.", "tokens": ["Leuch\u00b7tet", ",", "Menschlich\u00b7keit", ",", "dein", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Solche Sch\u00f6nheit ist die Lust der Geister,", "tokens": ["Sol\u00b7che", "Sch\u00f6n\u00b7heit", "ist", "die", "Lust", "der", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Ist des Erdenrunds, ist selbst des Himmels Meister.", "tokens": ["Ist", "des", "Er\u00b7den\u00b7runds", ",", "ist", "selbst", "des", "Him\u00b7mels", "Meis\u00b7ter", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "VAFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.10": {"line.1": {"text": "Soll ich denn dich sch\u00f6n und edel preisen,", "tokens": ["Soll", "ich", "denn", "dich", "sch\u00f6n", "und", "e\u00b7del", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Holde Freundin, so sey tugendhaft!", "tokens": ["Hol\u00b7de", "Freun\u00b7din", ",", "so", "sey", "tu\u00b7gend\u00b7haft", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Willst du ernten Lob und Preis der Weisen,", "tokens": ["Willst", "du", "ern\u00b7ten", "Lob", "und", "Preis", "der", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJA", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "So besiege niedre Leidenschaft!", "tokens": ["So", "be\u00b7sie\u00b7ge", "nied\u00b7re", "Lei\u00b7den\u00b7schaft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Willst du graden Wegs zum Himmel reisen,", "tokens": ["Willst", "du", "gra\u00b7den", "Wegs", "zum", "Him\u00b7mel", "rei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJA", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ringe wohlzuthun mit re