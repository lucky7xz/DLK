{"dta.poem.3377": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "An  \n Christiane von Smiterl\u00f6we.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Komm herab von deiner Klageweide,", "tokens": ["Komm", "her\u00b7ab", "von", "dei\u00b7ner", "Kla\u00b7ge\u00b7wei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Meine goldne Harfe, du mein Ruhm,", "tokens": ["Mei\u00b7ne", "gold\u00b7ne", "Har\u00b7fe", ",", "du", "mein", "Ruhm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Meine Tr\u00f6sterin im Lebensleide,", "tokens": ["Mei\u00b7ne", "Tr\u00f6s\u00b7te\u00b7rin", "im", "Le\u00b7bens\u00b7lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Meine Lieblingin, mein Heiligthum,", "tokens": ["Mei\u00b7ne", "Lieb\u00b7lin\u00b7gin", ",", "mein", "Hei\u00b7lig\u00b7thum", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Meine Sehnsucht, meine s\u00fcsse Freude,", "tokens": ["Mei\u00b7ne", "Sehn\u00b7sucht", ",", "mei\u00b7ne", "s\u00fcs\u00b7se", "Freu\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Mein gew\u00fcnschtes einzigs Eigenthum,", "tokens": ["Mein", "ge\u00b7w\u00fcnschtes", "ein\u00b7zigs", "Ei\u00b7gen\u00b7thum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Komm herab, und klinge lind' und leise,", "tokens": ["Komm", "her\u00b7ab", ",", "und", "klin\u00b7ge", "lind'", "und", "lei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KON", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "S\u00fcss, wie Kussgelispel, hold, wie Liebesweise.", "tokens": ["S\u00fcss", ",", "wie", "Kuss\u00b7ge\u00b7lis\u00b7pel", ",", "hold", ",", "wie", "Lie\u00b7bes\u00b7wei\u00b7se", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "NN", "$,", "ADJD", "$,", "PWAV", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Dir, o Freundin, sey mein Lied gesungen,", "tokens": ["Dir", ",", "o", "Freun\u00b7din", ",", "sey", "mein", "Lied", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "FM", "NN", "$,", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die du liebst der Harfe Zauberklang!", "tokens": ["Die", "du", "liebst", "der", "Har\u00b7fe", "Zau\u00b7ber\u00b7klang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Gerne f\u00fchlt sich deine Brust durchdrungen", "tokens": ["Ger\u00b7ne", "f\u00fchlt", "sich", "dei\u00b7ne", "Brust", "durch\u00b7drun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Von der Dichter Weh- und Wonnedrang,", "tokens": ["Von", "der", "Dich\u00b7ter", "Weh", "und", "Won\u00b7ne\u00b7drang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "TRUNC", "KON", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Gerne deine Seel' emporgeschwungen", "tokens": ["Ger\u00b7ne", "dei\u00b7ne", "Seel'", "em\u00b7por\u00b7ge\u00b7schwun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Von des Psalters heilgem Weihgesang.", "tokens": ["Von", "des", "Psal\u00b7ters", "heil\u00b7gem", "Weih\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Solche Seelen sind dem Dichter theuer,", "tokens": ["Sol\u00b7che", "See\u00b7len", "sind", "dem", "Dich\u00b7ter", "theu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Solchen schwillt sein Herz, entbrennt sein sch\u00f6n-", "tokens": ["Sol\u00b7chen", "schwillt", "sein", "Herz", ",", "ent\u00b7brennt", "sein", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "TRUNC"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "stes Feuer.", "tokens": ["stes", "Feu\u00b7er", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Meines Blumengartens sch\u00f6nste Pflanze", "tokens": ["Mei\u00b7nes", "Blu\u00b7men\u00b7gar\u00b7tens", "sch\u00f6ns\u00b7te", "Pflan\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Br\u00e4cht' ich gerne dir zum Opfer dar;", "tokens": ["Br\u00e4cht'", "ich", "ger\u00b7ne", "dir", "zum", "Op\u00b7fer", "dar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Fl\u00f6chte gern' aus meinem Dichterkranze", "tokens": ["Fl\u00f6ch\u00b7te", "gern'", "aus", "mei\u00b7nem", "Dich\u00b7ter\u00b7kran\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Einen Lorbeer in dein blondes Haar;", "tokens": ["Ei\u00b7nen", "Lor\u00b7beer", "in", "dein", "blon\u00b7des", "Haar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "F\u00fchrte gerne dich zum Reihentanze", "tokens": ["F\u00fchr\u00b7te", "ger\u00b7ne", "dich", "zum", "Rei\u00b7hen\u00b7tan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "In der Grazien und Musen Schaar;", "tokens": ["In", "der", "Gra\u00b7zi\u00b7en", "und", "Mu\u00b7sen", "Schaar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Reichte gerne dir beym G\u00f6ttermahle", "tokens": ["Reich\u00b7te", "ger\u00b7ne", "dir", "beym", "G\u00f6t\u00b7ter\u00b7mah\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Der Unsterblichkeit kristallne Nektarschale.", "tokens": ["Der", "U\u00b7nsterb\u00b7lich\u00b7keit", "kris\u00b7tall\u00b7ne", "Nek\u00b7tarsc\u00b7ha\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Doch ein Geist, durchgl\u00fcht von Dichterfeuer,", "tokens": ["Doch", "ein", "Geist", ",", "durch\u00b7gl\u00fcht", "von", "Dich\u00b7ter\u00b7feu\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ist nicht edler, als ein reines Herz.", "tokens": ["Ist", "nicht", "ed\u00b7ler", ",", "als", "ein", "rei\u00b7nes", "Herz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADJA", "$,", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Edel, wie Gef\u00fchl f\u00fcr Harf' und Leyer", "tokens": ["E\u00b7del", ",", "wie", "Ge\u00b7f\u00fchl", "f\u00fcr", "Ha\u00b7rf'", "und", "Le\u00b7yer"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "NN", "APPR", "NE", "KON", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Ist Gef\u00fchl f\u00fcr Menschenwohl und Schmerz.", "tokens": ["Ist", "Ge\u00b7f\u00fchl", "f\u00fcr", "Men\u00b7schen\u00b7wohl", "und", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Theu'r dem Engel und dem Menschen theuer", "tokens": ["Theu'r", "dem", "En\u00b7gel", "und", "dem", "Men\u00b7schen", "theu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "KON", "ART", "NN", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ist ein zartes ungef\u00e4rbtes Herz,", "tokens": ["Ist", "ein", "zar\u00b7tes", "un\u00b7ge\u00b7f\u00e4rb\u00b7tes", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Dessen Einfalt noch kein Wahn verschraubte,", "tokens": ["Des\u00b7sen", "Ein\u00b7falt", "noch", "kein", "Wahn", "ver\u00b7schraub\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Dem noch Thorheit nicht die sch\u00f6ne Unschuld", "tokens": ["Dem", "noch", "Thor\u00b7heit", "nicht", "die", "sch\u00f6\u00b7ne", "Un\u00b7schuld"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "NN", "PTKNEG", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "raubte.", "tokens": ["raub\u00b7te", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.5": {"line.1": {"text": "Sch\u00f6ner ist, als Klopstocks sch\u00f6nste Ode,", "tokens": ["Sch\u00f6\u00b7ner", "ist", ",", "als", "Klops\u00b7tocks", "sch\u00f6ns\u00b7te", "O\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "KOUS", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Eine That der reinen Menschlichkeit;", "tokens": ["Ei\u00b7ne", "That", "der", "rei\u00b7nen", "Menschlich\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie besch\u00e4mt den Putz der sch\u00f6nsten Mode,", "tokens": ["Sie", "be\u00b7sch\u00e4mt", "den", "Putz", "der", "sch\u00f6ns\u00b7ten", "Mo\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Lohnt mit himmlischer Zufriedenheit,", "tokens": ["Lohnt", "mit", "himm\u00b7li\u00b7scher", "Zu\u00b7frie\u00b7den\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "L\u00e4chelt, wie ein Engel, Trost im Tode,", "tokens": ["L\u00e4\u00b7chelt", ",", "wie", "ein", "En\u00b7gel", ",", "Trost", "im", "To\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "$,", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und geleitet in die Ewigkeit.", "tokens": ["Und", "ge\u00b7lei\u00b7tet", "in", "die", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Solcher Thaten viel dir zu erstreben,", "tokens": ["Sol\u00b7cher", "Tha\u00b7ten", "viel", "dir", "zu", "er\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Freundin, sey dein Preis, dein Kranz, dein Heil", "tokens": ["Freun\u00b7din", ",", "sey", "dein", "Preis", ",", "dein", "Kranz", ",", "dein", "Heil"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "im Leben!", "tokens": ["im", "Le\u00b7ben", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.6": {"line.1": {"text": "Sey geadelt mit dem grossen Namen:", "tokens": ["Sey", "ge\u00b7a\u00b7delt", "mit", "dem", "gros\u00b7sen", "Na\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Menschenfreundin \u2014 durch ihn edel g'nug,", "tokens": ["Men\u00b7schen\u00b7freun\u00b7din", "durch", "ihn", "e\u00b7del", "g'\u00b7nug", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "APPR", "PPER", "ADJD", "ADV", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Tr\u00fcge gleich dein Schild nicht Helm und Fahnen,", "tokens": ["Tr\u00fc\u00b7ge", "gleich", "dein", "Schild", "nicht", "Helm", "und", "Fah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "PTKNEG", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die er schon seit sieben S\u00e4keln trug;", "tokens": ["Die", "er", "schon", "seit", "sie\u00b7ben", "S\u00e4\u00b7keln", "trug", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "CARD", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "W\u00e4rst du gleich nicht aus des Helden Samen,", "tokens": ["W\u00e4rst", "du", "gleich", "nicht", "aus", "des", "Hel\u00b7den", "Sa\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Der den L\u00f6wen in der W\u00fcste schlug.", "tokens": ["Der", "den", "L\u00f6\u00b7wen", "in", "der", "W\u00fcs\u00b7te", "schlug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Menschenadel beugt nur Knie und R\u00fccken,", "tokens": ["Men\u00b7sche\u00b7na\u00b7del", "beugt", "nur", "Knie", "und", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "W\u00e4hrend, Edle, dir die Seelen selbst sich b\u00fccken.", "tokens": ["W\u00e4h\u00b7rend", ",", "Ed\u00b7le", ",", "dir", "die", "See\u00b7len", "selbst", "sich", "b\u00fc\u00b7cken", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADJA", "$,", "PPER", "ART", "NN", "ADV", "PRF", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.7": {"line.1": {"text": "Schonend decken seines Bruders Bl\u00f6sse,", "tokens": ["Scho\u00b7nend", "de\u00b7cken", "sei\u00b7nes", "Bru\u00b7ders", "Bl\u00f6s\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sorgsam k\u00fchlen rascher Jugend Gluth;", "tokens": ["Sorg\u00b7sam", "k\u00fch\u00b7len", "ra\u00b7scher", "Ju\u00b7gend", "Gluth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Muthig dulden harte Schicksalsst\u00f6sse,", "tokens": ["Mut\u00b7hig", "dul\u00b7den", "har\u00b7te", "Schick\u00b7sals\u00b7st\u00f6s\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Gross verachten blinde Bubenwuth;", "tokens": ["Gross", "ver\u00b7ach\u00b7ten", "blin\u00b7de", "Bu\u00b7ben\u00b7wuth", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Giebet Seelenwerth und Geistesgr\u00f6sse,", "tokens": ["Gie\u00b7bet", "See\u00b7len\u00b7werth", "und", "Geis\u00b7tes\u00b7gr\u00f6s\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Zeugt von Edelsinn und Heldenblut.", "tokens": ["Zeugt", "von", "E\u00b7del\u00b7sinn", "und", "Hel\u00b7den\u00b7blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Solchem Adel huldigt auch der Weise,", "tokens": ["Sol\u00b7chem", "A\u00b7del", "hul\u00b7digt", "auch", "der", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Huldigen des Dichters auserw\u00e4hltste Preise.", "tokens": ["Hul\u00b7di\u00b7gen", "des", "Dich\u00b7ters", "au\u00b7ser\u00b7w\u00e4hlts\u00b7te", "Prei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.8": {"line.1": {"text": "Was ist Leibessch\u00f6nheit? was ihr Prangen?", "tokens": ["Was", "ist", "Lei\u00b7bes\u00b7sch\u00f6n\u00b7heit", "?", "was", "ihr", "Pran\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "$.", "PWS", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Was ist Lilienhals und ringelnd Haar?", "tokens": ["Was", "ist", "Li\u00b7li\u00b7en\u00b7hals", "und", "rin\u00b7gelnd", "Haar", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "KON", "ADJD", "NN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Was sind Purpurlippen, Rosenwangen,", "tokens": ["Was", "sind", "Pur\u00b7pur\u00b7lip\u00b7pen", ",", "Ro\u00b7sen\u00b7wan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Schwanenbrust und schimmernd Augenpaar?", "tokens": ["Schwa\u00b7nen\u00b7brust", "und", "schim\u00b7mernd", "Au\u00b7gen\u00b7paar", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Blumen sind sie, gestern aufgegangen,", "tokens": ["Blu\u00b7men", "sind", "sie", ",", "ge\u00