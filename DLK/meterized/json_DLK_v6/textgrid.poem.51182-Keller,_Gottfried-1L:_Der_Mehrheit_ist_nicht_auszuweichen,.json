{"textgrid.poem.51182": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Mehrheit ist nicht auszuweichen,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Mehrheit ist nicht auszuweichen,", "tokens": ["Der", "Mehr\u00b7heit", "ist", "nicht", "aus\u00b7zu\u00b7wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Helden- wie mit Schwabenstreichen", "tokens": ["Mit", "Hel\u00b7den", "wie", "mit", "Schwa\u00b7ben\u00b7strei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "TRUNC", "KOKOM", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Macht sie uns ihre Macht bekannt", "tokens": ["Macht", "sie", "uns", "ih\u00b7re", "Macht", "be\u00b7kannt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PPER", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf Weg und Steg im ganzen Land;", "tokens": ["Auf", "Weg", "und", "Steg", "im", "gan\u00b7zen", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So gebt dem Kind den rechten Namen,", "tokens": ["So", "gebt", "dem", "Kind", "den", "rech\u00b7ten", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "La\u00dft Ehr und Schuld ihm und sagt Amen!", "tokens": ["La\u00dft", "Ehr", "und", "Schuld", "ihm", "und", "sagt", "A\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "KON", "NN", "PPER", "KON", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und l\u00e4uft es dann auf schlechten Sohlen,", "tokens": ["Und", "l\u00e4uft", "es", "dann", "auf", "schlech\u00b7ten", "Soh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So wird es schon der Teufel holen!", "tokens": ["So", "wird", "es", "schon", "der", "Teu\u00b7fel", "ho\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}