{"textgrid.poem.59935": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "21.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich Sch\u00f6nheit, Lieb' und Wein besungen,", "tokens": ["Als", "ich", "Sch\u00f6n\u00b7heit", ",", "Lieb'", "und", "Wein", "be\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "$,", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ist mir tausendstimmig Lob erklungen.", "tokens": ["Ist", "mir", "tau\u00b7sends\u00b7tim\u00b7mig", "Lob", "er\u00b7klun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Als ich Sch\u00f6nheit, Lieb' und Wein genossen,", "tokens": ["Als", "ich", "Sch\u00f6n\u00b7heit", ",", "Lieb'", "und", "Wein", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "$,", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mir mein Erdendasein zu versch\u00f6nen:", "tokens": ["Mir", "mein", "Er\u00b7den\u00b7da\u00b7sein", "zu", "ver\u00b7sch\u00f6\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Hat es pl\u00f6tzlich alle Welt verdrossen,", "tokens": ["Hat", "es", "pl\u00f6tz\u00b7lich", "al\u00b7le", "Welt", "ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "H\u00f6rte ich mich schm\u00e4hen und verh\u00f6hnen.", "tokens": ["H\u00f6r\u00b7te", "ich", "mich", "schm\u00e4\u00b7hen", "und", "ver\u00b7h\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "VVINF", "KON", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "O Mirza-Schaffy! Du Sohn Abdullahs,", "tokens": ["O", "Mir\u00b7za\u00b7Schaffy", "!", "Du", "Sohn", "Ab\u00b7dul\u00b7lahs", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "PPER", "NN", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "\u00dcberla\u00df die Heuchelei den Mullahs!", "tokens": ["\u00dc\u00b7berl\u00b7a\u00df", "die", "Heu\u00b7che\u00b7lei", "den", "Mul\u00b7lahs", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Folg' im Lieben und im Trinken immer", "tokens": ["Fol\u00b7g'", "im", "Lie\u00b7ben", "und", "im", "Trin\u00b7ken", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "KON", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sch\u00f6ner Augen, voller Gl\u00e4ser Schimmer!", "tokens": ["Sch\u00f6\u00b7ner", "Au\u00b7gen", ",", "vol\u00b7ler", "Gl\u00e4\u00b7ser", "Schim\u00b7mer", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}