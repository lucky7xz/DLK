{"textgrid.poem.57982": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "20. W\u00e4r' ich so!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Klein und rein und abgeschieden,", "tokens": ["Klein", "und", "rein", "und", "ab\u00b7ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "KON", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sanft, einf\u00e4ltig, still, im Frieden,", "tokens": ["Sanft", ",", "ein\u00b7f\u00e4l\u00b7tig", ",", "still", ",", "im", "Frie\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Willenlos und innig froh;", "tokens": ["Wil\u00b7len\u00b7los", "und", "in\u00b7nig", "froh", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach w\u00e4r' mein Gem\u00fcte so!", "tokens": ["Ach", "w\u00e4r'", "mein", "Ge\u00b7m\u00fc\u00b7te", "so", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}