{"textgrid.poem.48110": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Zur Arbeit", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus Morgennebeln leuchtet", "tokens": ["Aus", "Mor\u00b7gen\u00b7ne\u00b7beln", "leuch\u00b7tet"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "der fr\u00fche rote Tag;", "tokens": ["der", "fr\u00fc\u00b7he", "ro\u00b7te", "Tag", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "da treibt mich auf vom Lager", "tokens": ["da", "treibt", "mich", "auf", "vom", "La\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "ein dumpfer Glockenschlag.", "tokens": ["ein", "dum\u00b7pfer", "Glo\u00b7cken\u00b7schlag", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der ruft aus s\u00fc\u00dfem Traume", "tokens": ["Der", "ruft", "aus", "s\u00fc\u00b7\u00dfem", "Trau\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "zum trauten Heim hinaus", "tokens": ["zum", "trau\u00b7ten", "Heim", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APZR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "mich in die Flammenschw\u00fcle", "tokens": ["mich", "in", "die", "Flam\u00b7men\u00b7schw\u00fc\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "ins dunstige Kesselhaus", "tokens": ["ins", "duns\u00b7ti\u00b7ge", "Kes\u00b7sel\u00b7haus"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "zur Arbeit.", "tokens": ["zur", "Ar\u00b7beit", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Ich schreite gleich dem Krieger", "tokens": ["Ich", "schrei\u00b7te", "gleich", "dem", "Krie\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "in eisenstarrer Wehr,", "tokens": ["in", "ei\u00b7sen\u00b7star\u00b7rer", "Wehr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "mit Hammer, Beil und Zange", "tokens": ["mit", "Ham\u00b7mer", ",", "Beil", "und", "Zan\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "zum Daseinskampf einher.", "tokens": ["zum", "Da\u00b7seins\u00b7kampf", "ein\u00b7her", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Und wo die B\u00e4lge sausen,", "tokens": ["Und", "wo", "die", "B\u00e4l\u00b7ge", "sau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "wo hell das Feuer spr\u00fcht,", "tokens": ["wo", "hell", "das", "Feu\u00b7er", "spr\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "singt mir die rote Flamme", "tokens": ["singt", "mir", "die", "ro\u00b7te", "Flam\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "ein hei\u00dfes Morgenlied", "tokens": ["ein", "hei\u00b7\u00dfes", "Mor\u00b7gen\u00b7lied"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "zur Arbeit.", "tokens": ["zur", "Ar\u00b7beit", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Des Schwei\u00dfes schwerer Tropfen,", "tokens": ["Des", "Schwei\u00b7\u00dfes", "schwe\u00b7rer", "Trop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "der von der Stirn mir l\u00e4uft,", "tokens": ["der", "von", "der", "Stirn", "mir", "l\u00e4uft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "ist Tau, der auf die Saaten", "tokens": ["ist", "Tau", ",", "der", "auf", "die", "Saa\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "der Zukunft niedertr\u00e4uft.", "tokens": ["der", "Zu\u00b7kunft", "nie\u00b7der\u00b7tr\u00e4uft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Kein Mordgewaffen schmiedet", "tokens": ["Kein", "Mord\u00b7ge\u00b7waf\u00b7fen", "schmie\u00b7det"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "die schwielenharte Hand;", "tokens": ["die", "schwie\u00b7len\u00b7har\u00b7te", "Hand", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "sie dehnt und schwei\u00dft und h\u00e4mmert", "tokens": ["sie", "dehnt", "und", "schwei\u00dft", "und", "h\u00e4m\u00b7mert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "ein ehern Friedensband", "tokens": ["ein", "e\u00b7hern", "Frie\u00b7dens\u00b7band"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "der Arbeit.", "tokens": ["der", "Ar\u00b7beit", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Schlaf ruhig du, mein Knabe,", "tokens": ["Schlaf", "ru\u00b7hig", "du", ",", "mein", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "in treuer Mutterhut;", "tokens": ["in", "treu\u00b7er", "Mut\u00b7ter\u00b7hut", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "auch dich ruft einst die Fr\u00fche,", "tokens": ["auch", "dich", "ruft", "einst", "die", "Fr\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.4": {"text": "auch dich ruft einst die Glut.", "tokens": ["auch", "dich", "ruft", "einst", "die", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Dann wirst in blaue Weiten", "tokens": ["Dann", "wirst", "in", "blau\u00b7e", "Wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "auf fernster Br\u00fcder Ruf", "tokens": ["auf", "ferns\u00b7ter", "Br\u00fc\u00b7der", "Ruf"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "du die Maschine leiten,", "tokens": ["du", "die", "Ma\u00b7schi\u00b7ne", "lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "die einst dein Vater schuf,", "tokens": ["die", "einst", "dein", "Va\u00b7ter", "schuf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "zur Arbeit.", "tokens": ["zur", "Ar\u00b7beit", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.5": {"line.1": {"text": "Zur Arbeit ruft ihr Sausen,", "tokens": ["Zur", "Ar\u00b7beit", "ruft", "ihr", "Sau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "zur Arbeit, nicht zur Fron!", "tokens": ["zur", "Ar\u00b7beit", ",", "nicht", "zur", "Fron", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann wird die Sonne scheinen", "tokens": ["Dann", "wird", "die", "Son\u00b7ne", "schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "hell auf dein Werk, mein Sohn.", "tokens": ["hell", "auf", "dein", "Werk", ",", "mein", "Sohn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In freier M\u00e4nner Kreise", "tokens": ["In", "frei\u00b7er", "M\u00e4n\u00b7ner", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "klingt dann in Nord und S\u00fcd", "tokens": ["klingt", "dann", "in", "Nord", "und", "S\u00fcd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "NE", "KON", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "jauchzend wie Siegesweise", "tokens": ["jauch\u00b7zend", "wie", "Sie\u00b7ges\u00b7wei\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "KOKOM", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "ein frohes Morgenlied.", "tokens": ["ein", "fro\u00b7hes", "Mor\u00b7gen\u00b7lied", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}