{"textgrid.poem.41989": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach, ich soll dich wiedersehen,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, ich soll dich wiedersehen,", "tokens": ["Ach", ",", "ich", "soll", "dich", "wie\u00b7der\u00b7se\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die so lange mir entschwand,", "tokens": ["Die", "so", "lan\u00b7ge", "mir", "ent\u00b7schwand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Soll an deiner zarten Hand", "tokens": ["Soll", "an", "dei\u00b7ner", "zar\u00b7ten", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch einmal durchs Leben gehen;", "tokens": ["Noch", "ein\u00b7mal", "durchs", "Le\u00b7ben", "ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ach, ich soll dich wiedersehen,", "tokens": ["Ach", ",", "ich", "soll", "dich", "wie\u00b7der\u00b7se\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und doch darf ich noch gestehen,", "tokens": ["Und", "doch", "darf", "ich", "noch", "ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df ich je den Schmerz gekannt?", "tokens": ["Da\u00df", "ich", "je", "den", "Schmerz", "ge\u00b7kannt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gl\u00fchend will ich dich umfangen,", "tokens": ["Gl\u00fc\u00b7hend", "will", "ich", "dich", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will mit leisem Liebesbangen", "tokens": ["Will", "mit", "lei\u00b7sem", "Lie\u00b7bes\u00b7ban\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fest an deiner weichen Brust", "tokens": ["Fest", "an", "dei\u00b7ner", "wei\u00b7chen", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und an deinen Lippen hangen,", "tokens": ["Und", "an", "dei\u00b7nen", "Lip\u00b7pen", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Will mit lechzendem Verlangen", "tokens": ["Will", "mit", "lech\u00b7zen\u00b7dem", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schl\u00fcrfen den Pokal der Lust.", "tokens": ["Schl\u00fcr\u00b7fen", "den", "Po\u00b7kal", "der", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Alle meine k\u00fcnft'gen Thr\u00e4nen", "tokens": ["Al\u00b7le", "mei\u00b7ne", "k\u00fcnft'\u00b7gen", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sollen Freudenthr\u00e4nen seyn;", "tokens": ["Sol\u00b7len", "Freu\u00b7dent\u00b7hr\u00e4\u00b7nen", "seyn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Sehn' ich mich, so sey mein Sehnen", "tokens": ["Sehn'", "ich", "mich", ",", "so", "sey", "mein", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "$,", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Der Gew\u00e4hrung D\u00e4mmerschein.", "tokens": ["Der", "Ge\u00b7w\u00e4h\u00b7rung", "D\u00e4m\u00b7mer\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Nur in dich will ich versinken,", "tokens": ["Nur", "in", "dich", "will", "ich", "ver\u00b7sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.12": {"text": "Will mir zarte Tr\u00e4ume trinken", "tokens": ["Will", "mir", "zar\u00b7te", "Tr\u00e4u\u00b7me", "trin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Aus der Liebe Wunderborn;", "tokens": ["Aus", "der", "Lie\u00b7be", "Wun\u00b7der\u00b7born", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Dich nur will ich ganz umschlingen,", "tokens": ["Dich", "nur", "will", "ich", "ganz", "um\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Will nach deinem L\u00e4cheln ringen", "tokens": ["Will", "nach", "dei\u00b7nem", "L\u00e4\u00b7cheln", "rin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Und erbleichen deinem Zorn.", "tokens": ["Und", "er\u00b7blei\u00b7chen", "dei\u00b7nem", "Zorn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Dich nur soll mein Wille fragen,", "tokens": ["Dich", "nur", "soll", "mein", "Wil\u00b7le", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Mit dir theilen Lust und Scherz,", "tokens": ["Mit", "dir", "thei\u00b7len", "Lust", "und", "Scherz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Mit dir weinen und verzagen;", "tokens": ["Mit", "dir", "wei\u00b7nen", "und", "ver\u00b7za\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Und es soll mein treues Herz", "tokens": ["Und", "es", "soll", "mein", "treu\u00b7es", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "L\u00e4nger nicht als deines schlagen! \u2013", "tokens": ["L\u00e4n\u00b7ger", "nicht", "als", "dei\u00b7nes", "schla\u00b7gen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PTKNEG", "KOUS", "PPOSAT", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Ach, kaum trug ich j\u00fcngst den Schmerz,", "tokens": ["Ach", ",", "kaum", "trug", "ich", "j\u00fcngst", "den", "Schmerz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Werd' ich jetzt die Wonne tragen?", "tokens": ["Werd'", "ich", "jetzt", "die", "Won\u00b7ne", "tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}