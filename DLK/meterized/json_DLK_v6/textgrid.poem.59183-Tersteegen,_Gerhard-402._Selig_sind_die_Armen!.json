{"textgrid.poem.59183": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "402. Selig sind die Armen!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Armut und das einsam Leben,", "tokens": ["Die", "Ar\u00b7mut", "und", "das", "ein\u00b7sam", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Schweigen und Verborgensein daneben,", "tokens": ["Das", "Schwei\u00b7gen", "und", "Ver\u00b7bor\u00b7gen\u00b7sein", "da\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PAV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So man in dir, o Kreuz, aus\u00fcbt,", "tokens": ["So", "man", "in", "dir", ",", "o", "Kreuz", ",", "aus\u00b7\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PIS", "APPR", "PPER", "$,", "FM", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die machen, da\u00df mein Herz dich herzlich liebt.", "tokens": ["Die", "ma\u00b7chen", ",", "da\u00df", "mein", "Herz", "dich", "herz\u00b7lich", "liebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "$,", "KOUS", "PPOSAT", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}