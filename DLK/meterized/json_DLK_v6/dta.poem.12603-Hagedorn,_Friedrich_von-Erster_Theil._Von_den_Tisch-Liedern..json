{"dta.poem.12603": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Erster Theil.  \n Von den Tisch-Liedern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905199128", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Heute, Br\u00fcder! heut", "tokens": ["Heu\u00b7te", ",", "Br\u00fc\u00b7der", "!", "heut"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["ADV", "$,", "NN", "$.", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Ist die rechte Zeit,", "tokens": ["Ist", "die", "rech\u00b7te", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df ihr trinkt, und trunken seyd.", "tokens": ["Da\u00df", "ihr", "trinkt", ",", "und", "trun\u00b7ken", "seyd", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KON", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lustig! eingeschenket!", "tokens": ["Lus\u00b7tig", "!", "ein\u00b7ge\u00b7schen\u00b7ket", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wer nicht will, der mu\u00df,", "tokens": ["Wer", "nicht", "will", ",", "der", "mu\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "VMFIN", "$,", "ART", "VMFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Weil des Schicksals Schlu\u00df", "tokens": ["Weil", "des", "Schick\u00b7sals", "Schlu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Unsern lieben Myrsilus", "tokens": ["Un\u00b7sern", "lie\u00b7ben", "Myr\u00b7si\u00b7lus"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "In die Gruft versenket.", "tokens": ["In", "die", "Gruft", "ver\u00b7sen\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}