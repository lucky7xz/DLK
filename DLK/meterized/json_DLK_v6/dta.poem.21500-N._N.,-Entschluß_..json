{"dta.poem.21500": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Entschlu\u00df .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Nach einem stillen Kloster will ich wandern,", "tokens": ["Nach", "ei\u00b7nem", "stil\u00b7len", "Klos\u00b7ter", "will", "ich", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Will flieh\u2019n den Menschenl\u00e4rm und Pulverdampf;", "tokens": ["Will", "flieh'n", "den", "Men\u00b7schen\u00b7l\u00e4rm", "und", "Pul\u00b7ver\u00b7dampf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verwundet ward mein Herz, mich schmerzt das Schrein,", "tokens": ["Ver\u00b7wun\u00b7det", "ward", "mein", "Herz", ",", "mich", "schmerzt", "das", "Schrein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich tauge nicht zu w\u00fcth\u2019gem, irrem Kampf.", "tokens": ["Ich", "tau\u00b7ge", "nicht", "zu", "w\u00fcth'\u00b7gem", ",", "ir\u00b7rem", "Kampf", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PTKZU", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich hab\u2019 nicht Schwert, nicht S\u00e4bel und Pistole,", "tokens": ["Ich", "hab'", "nicht", "Schwert", ",", "nicht", "S\u00e4\u00b7bel", "und", "Pis\u00b7to\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NN", "$,", "PTKNEG", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich ", "tokens": ["Ich"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Hier steh\u2019 ich stille, auf mir selber ruhend,", "tokens": ["Hier", "steh'", "ich", "stil\u00b7le", ",", "auf", "mir", "sel\u00b7ber", "ru\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "$,", "APPR", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Niemand hat zum Schutz sich mir gesellt.", "tokens": ["Und", "Nie\u00b7mand", "hat", "zum", "Schutz", "sich", "mir", "ge\u00b7sellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "APPRART", "NN", "PRF", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "In mir auch spr\u00fchte auf Prometheusfunke,", "tokens": ["In", "mir", "auch", "spr\u00fch\u00b7te", "auf", "Pro\u00b7me\u00b7theus\u00b7fun\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erringen wollt\u2019 ich Euch das Ideal \u2014", "tokens": ["Er\u00b7rin\u00b7gen", "wollt'", "ich", "Euch", "das", "I\u00b7deal"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und an den Felsen ward auch ich geschmiedet,", "tokens": ["Und", "an", "den", "Fel\u00b7sen", "ward", "auch", "ich", "ge\u00b7schmie\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Kraft zerbarst, zur\u00fcck blieb nur die Qual!", "tokens": ["Die", "Kraft", "zer\u00b7barst", ",", "zu\u00b7r\u00fcck", "blieb", "nur", "die", "Qual", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PTKVZ", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Seit ich ihn nutzlos wei\u00df, mir graut\u2019s vorm Kriege;", "tokens": ["Seit", "ich", "ihn", "nutz\u00b7los", "wei\u00df", ",", "mir", "graut's", "vorm", "Krie\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$,", "PPER", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "K\u00f6nnt' ich ihn streiten noch, ich wollte nicht:               ", "tokens": ["K\u00f6nnt'", "ich", "ihn", "strei\u00b7ten", "noch", ",", "ich", "woll\u00b7te", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVFIN", "ADV", "$,", "PPER", "VMFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Schafft er auch Recht \u2014 das Unrecht lauert, tilgt es;", "tokens": ["Schafft", "er", "auch", "Recht", "das", "Un\u00b7recht", "lau\u00b7ert", ",", "tilgt", "es", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "$(", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Der Pesthauch Wunsch l\u00f6scht aus des Friedens Licht.", "tokens": ["Der", "Pest\u00b7hauch", "Wunsch", "l\u00f6scht", "aus", "des", "Frie\u00b7dens", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich streite nicht, mu\u00df wandern ohne Waffen \u2014", "tokens": ["Ich", "strei\u00b7te", "nicht", ",", "mu\u00df", "wan\u00b7dern", "oh\u00b7ne", "Waf\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "VMFIN", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wohin? \u2014 Ich kenn\u2019 kein Ziel. Doch was ich such\u2019,", "tokens": ["Wo\u00b7hin", "?", "Ich", "kenn'", "kein", "Ziel", ".", "Doch", "was", "ich", "such'", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "$(", "PPER", "VVFIN", "PIAT", "NN", "$.", "KON", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Kloster ist\u2019s. Da harre ich und lese", "tokens": ["Ein", "Klos\u00b7ter", "ist'", "s.", "Da", "har\u00b7re", "ich", "und", "le\u00b7se"], "token_info": ["word", "word", "word", "abbreviation", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVIMP", "ADV", "VVFIN", "PPER", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Herzstill, was mir das Schicksal schreibt in\u2019s Buch.", "tokens": ["Herz\u00b7still", ",", "was", "mir", "das", "Schick\u00b7sal", "schreibt", "in's", "Buch", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "PPER", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}