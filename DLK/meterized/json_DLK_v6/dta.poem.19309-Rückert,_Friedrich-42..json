{"dta.poem.19309": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "42.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O Mensch, sieh hier das Nichts, aus welchem du entsprungen,", "tokens": ["O", "Mensch", ",", "sieh", "hier", "das", "Nichts", ",", "aus", "wel\u00b7chem", "du", "ent\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVIMP", "ADV", "ART", "PIS", "$,", "APPR", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dort die Unendlichkeit, in die du bist verschlungen.", "tokens": ["Dort", "die", "Un\u00b7end\u00b7lich\u00b7keit", ",", "in", "die", "du", "bist", "ver\u00b7schlun\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das Nichts hier, dort das All, und in der Mitte du,", "tokens": ["Das", "Nichts", "hier", ",", "dort", "das", "All", ",", "und", "in", "der", "Mit\u00b7te", "du", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "ADV", "$,", "ADV", "ART", "NN", "$,", "KON", "APPR", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du schwankst in jedem Nu von diesem jenem zu.", "tokens": ["Du", "schwankst", "in", "je\u00b7dem", "Nu", "von", "die\u00b7sem", "je\u00b7nem", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIS", "ADV", "APPR", "PDAT", "PDAT", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du streifest hier ans Nichts, und schweifest dort ins All,", "tokens": ["Du", "strei\u00b7fest", "hier", "ans", "Nichts", ",", "und", "schwei\u00b7fest", "dort", "ins", "All", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$,", "KON", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ergreifest keines doch im Steigen noch im Fall.", "tokens": ["Er\u00b7grei\u00b7fest", "kei\u00b7nes", "doch", "im", "Stei\u00b7gen", "noch", "im", "Fall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "APPRART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wielange f\u00fchrest du mit Wolkenbildern Streit?", "tokens": ["Wie\u00b7lan\u00b7ge", "f\u00fch\u00b7rest", "du", "mit", "Wol\u00b7ken\u00b7bil\u00b7dern", "Streit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Nichts ist nichts, und nichts ist die Unendlichkeit.", "tokens": ["Das", "Nichts", "ist", "nichts", ",", "und", "nichts", "ist", "die", "Un\u00b7end\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "PIS", "$,", "KON", "PIS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Gott ist wo Nichts dir scheint, Gott ist wo dir erscheinet", "tokens": ["Gott", "ist", "wo", "Nichts", "dir", "scheint", ",", "Gott", "ist", "wo", "dir", "er\u00b7schei\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PWAV", "PIS", "PPER", "VVFIN", "$,", "NN", "VAFIN", "PWAV", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Unendlichkeit, in ihm ist Nichts und Nichts verneinet.", "tokens": ["Un\u00b7end\u00b7lich\u00b7keit", ",", "in", "ihm", "ist", "Nichts", "und", "Nichts", "ver\u00b7nei\u00b7net", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPER", "VAFIN", "PIS", "KON", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Du bist vor jedem Nichts gerettet, Ihm vereint;", "tokens": ["Du", "bist", "vor", "je\u00b7dem", "Nichts", "ge\u00b7ret\u00b7tet", ",", "Ihm", "ver\u00b7eint", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "PIS", "VVPP", "$,", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nichts ist nur, was ohn' Ihn Etwas zu seyn vermeint.", "tokens": ["Nichts", "ist", "nur", ",", "was", "ohn'", "Ihn", "Et\u00b7was", "zu", "seyn", "ver\u00b7meint", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "$,", "PRELS", "APPR", "PPER", "ADV", "PTKZU", "VAINF", "VVFIN", "$."], "meter": "-+-++-+--+-+", "measure": "iambic.hexa.relaxed"}}}}}