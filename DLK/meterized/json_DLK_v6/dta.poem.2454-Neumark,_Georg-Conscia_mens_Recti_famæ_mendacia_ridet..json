{"dta.poem.2454": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Conscia mens Recti fam\u00e6 mendacia ridet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Wer hier in dieser Welt es nur also kan machen", "tokens": ["Wer", "hier", "in", "die\u00b7ser", "Welt", "es", "nur", "al\u00b7so", "kan", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "PDAT", "NN", "PPER", "ADV", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df sein Gewissen rein und unbeflekket bleibt/", "tokens": ["Da\u00df", "sein", "Ge\u00b7wis\u00b7sen", "rein", "und", "un\u00b7be\u00b7flek\u00b7ket", "bleibt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der kan/ was Der und Der f\u00fcr L\u00fcgenrede treibt/", "tokens": ["Der", "kan", "/", "was", "Der", "und", "Der", "f\u00fcr", "L\u00fc\u00b7gen\u00b7re\u00b7de", "treibt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "$(", "PWS", "ART", "KON", "ART", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als dessen unbewust mit frohem Munde lachen.", "tokens": ["Als", "des\u00b7sen", "un\u00b7be\u00b7wust", "mit", "fro\u00b7hem", "Mun\u00b7de", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ADJD", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}