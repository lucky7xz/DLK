"/", "mich", "schnau\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "$(", "APPR", "ADJA", "NN", "$(", "VVPP", "PPER", "$(", "PPER", "VVINF", "$."], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.5": {"text": "Wollst deiner Gnaden-Sonn' auch aufzugehn erlauben.", "tokens": ["Wollst", "dei\u00b7ner", "Gna\u00b7den\u00b7Sonn'", "auch", "auf\u00b7zu\u00b7gehn", "er\u00b7lau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Walz allen Irrungs-Stein von meinem Herz und Mund /", "tokens": ["Walz", "al\u00b7len", "Ir\u00b7rungs\u00b7Stein", "von", "mei\u00b7nem", "Herz", "und", "Mund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "APPR", "PPOSAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "dich zubekennen frey und kecklich / alle Stund.", "tokens": ["dich", "zu\u00b7be\u00b7ken\u00b7nen", "frey", "und", "keck\u00b7lich", "/", "al\u00b7le", "Stund", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "$(", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "La\u00df meinen Herzens-Trost mir kein Gesch\u00f6pffe rauben.", "tokens": ["La\u00df", "mei\u00b7nen", "Her\u00b7zens\u00b7Trost", "mir", "kein", "Ge\u00b7sch\u00f6pf\u00b7fe", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Bleib / weil es Abend ist / in dieser Welt bey mir.", "tokens": ["Bleib", "/", "weil", "es", "A\u00b7bend", "ist", "/", "in", "die\u00b7ser", "Welt", "bey", "mir", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PPER", "NN", "VAFIN", "$(", "APPR", "PDAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Gib Felsen-Honig auch den Trost-Safft deiner Wunden.", "tokens": ["Gib", "Fel\u00b7sen\u00b7Ho\u00b7nig", "auch", "den", "Trost\u00b7\u00b7S\u00b7afft", "dei\u00b7ner", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-++-+-+-", "measure": "unknown.measure.septa"}, "line.11": {"text": "Ich hab den Herzen-Brand aus deinem Wort entfunden:", "tokens": ["Ich", "hab", "den", "Her\u00b7zen\u00b7Brand", "aus", "dei\u00b7nem", "Wort", "ent\u00b7fun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "du regest nicht allein / du stillst auch die Begier.", "tokens": ["du", "re\u00b7gest", "nicht", "al\u00b7lein", "/", "du", "stillst", "auch", "die", "Be\u00b7gier", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "$(", "PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mein Herz / das aller Welt / nur dir / HERR / nicht verschlossen /", "tokens": ["Mein", "Herz", "/", "das", "al\u00b7ler", "Welt", "/", "nur", "dir", "/", "HeRR", "/", "nicht", "ver\u00b7schlos\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ART", "PIAT", "NN", "$(", "ADV", "PPER", "$(", "NN", "$(", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "hat dein' Erstehungs-Krafft und beyseyn recht genossen", "tokens": ["hat", "dein'", "Er\u00b7ste\u00b7hungs\u00b7Krafft", "und", "bey\u00b7seyn", "recht", "ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "KON", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}