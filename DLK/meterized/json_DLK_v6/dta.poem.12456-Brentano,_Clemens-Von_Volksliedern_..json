{"dta.poem.12456": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Von Volksliedern .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Kukuk mit seinem Schreyen,", "tokens": ["Der", "Ku\u00b7kuk", "mit", "sei\u00b7nem", "Schre\u00b7yen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Macht fr\u00f6hlich jedermann,", "tokens": ["Macht", "fr\u00f6h\u00b7lich", "je\u00b7der\u00b7mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PIS", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Abends fr\u00f6hlich reihen", "tokens": ["Des", "A\u00b7bends", "fr\u00f6h\u00b7lich", "rei\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Maidlein wohlgethan,", "tokens": ["Die", "Maid\u00b7lein", "wohl\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Spazieren zu den Brunnen,", "tokens": ["Spa\u00b7zie\u00b7ren", "zu", "den", "Brun\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bekr\u00e4nzen sie zur Zeit,", "tokens": ["Be\u00b7kr\u00e4n\u00b7zen", "sie", "zur", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "All Volk sucht Freud und Blumen,", "tokens": ["All", "Volk", "sucht", "Freud", "und", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit Reisen fern und weit.", "tokens": ["Mit", "Rei\u00b7sen", "fern", "und", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}