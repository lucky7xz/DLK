{"dta.poem.11001": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Sch\u00e4dlichkeit  \n der k\u00f6rperlichen Gottheits-Bilder.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie mancher hat, von Jugend auf, durchs Malers\nPinsel-Spiel verf\u00fchret,", "tokens": ["Wie", "man\u00b7cher", "hat", ",", "von", "Ju\u00b7gend", "auf", ",", "durchs", "Ma\u00b7lers", "Pin\u00b7sel\u00b7Spiel", "ver\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "$,", "APPR", "NN", "PTKVZ", "$,", "APPRART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "Anstatt der Gottheit, sein Gehirn, mit nichts, als Bildern,", "tokens": ["An\u00b7statt", "der", "Got\u00b7theit", ",", "sein", "Ge\u00b7hirn", ",", "mit", "nichts", ",", "als", "Bil\u00b7dern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PPOSAT", "NN", "$,", "APPR", "PIS", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was er von seines Gottes Wesen begreift und ehret, ist ein", "tokens": ["Was", "er", "von", "sei\u00b7nes", "Got\u00b7tes", "We\u00b7sen", "be\u00b7greift", "und", "eh\u00b7ret", ",", "ist", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "PPER", "APPR", "PPOSAT", "NN", "NN", "VVFIN", "KON", "VVFIN", "$,", "VAFIN", "ART"], "meter": "-+-+-+-+--+-+--+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "In welchen Bildern er doch Gott weit minder ehrt, als ihn", "tokens": ["In", "wel\u00b7chen", "Bil\u00b7dern", "er", "doch", "Gott", "weit", "min\u00b7der", "ehrt", ",", "als", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PWAT", "NN", "PPER", "ADV", "NN", "ADJD", "ADV", "VVFIN", "$,", "KOUS", "PPER"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Nehmt ihm, aus seiner Phantasey, den alten Mann, das", "tokens": ["Nehmt", "ihm", ",", "aus", "sei\u00b7ner", "Phan\u00b7ta\u00b7sey", ",", "den", "al\u00b7ten", "Mann", ",", "das"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,", "PRELS"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was bleibt ihm von der Gottheit \u00fcbrig? Wo ist der Christ?", "tokens": ["Was", "bleibt", "ihm", "von", "der", "Got\u00b7theit", "\u00fcb\u00b7rig", "?", "Wo", "ist", "der", "Christ", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$.", "PWAV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}}}}