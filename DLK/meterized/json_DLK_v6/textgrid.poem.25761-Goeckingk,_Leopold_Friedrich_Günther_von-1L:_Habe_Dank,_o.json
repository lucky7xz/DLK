{"textgrid.poem.25761": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Habe Dank, o ", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Habe Dank, o ", "tokens": ["Ha\u00b7be", "Dank", ",", "o"], "token_info": ["word", "word", "punct", "word"], "pos": ["NN", "NN", "$,", "FM"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Hast genossen als ein weiser Mann;", "tokens": ["Hast", "ge\u00b7nos\u00b7sen", "als", "ein", "wei\u00b7ser", "Mann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df dem j\u00fcngern Freund', der horchend neben", "tokens": ["Da\u00df", "dem", "j\u00fcn\u00b7gern", "Freund'", ",", "der", "hor\u00b7chend", "ne\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "APPR"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dir nun sitzt, du alles das kannst geben,", "tokens": ["Dir", "nun", "sitzt", ",", "du", "al\u00b7les", "das", "kannst", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "PPER", "PIS", "PDS", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Was ein froher Greis noch geben kann.", "tokens": ["Was", "ein", "fro\u00b7her", "Greis", "noch", "ge\u00b7ben", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Mit der Freud' allein hast du getrunken,", "tokens": ["Mit", "der", "Freud'", "al\u00b7lein", "hast", "du", "ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.7": {"text": "Mit den Scythen und Pr\u00e4laten nicht.", "tokens": ["Mit", "den", "Scyt\u00b7hen", "und", "Pr\u00e4\u00b7la\u00b7ten", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Diese sind schon l\u00e4ngst ins Grab gesunken,", "tokens": ["Die\u00b7se", "sind", "schon", "l\u00e4ngst", "ins", "Grab", "ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Doch aus deinem Auge spr\u00fchn noch Funken,", "tokens": ["Doch", "aus", "dei\u00b7nem", "Au\u00b7ge", "spr\u00fchn", "noch", "Fun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "ADV", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Wenn aus ihm der Nachruhm ", "tokens": ["Wenn", "aus", "ihm", "der", "Nach\u00b7ruhm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPER", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Amors Pfeil ward nach der ersten Wunde", "tokens": ["A\u00b7mors", "Pfeil", "ward", "nach", "der", "ers\u00b7ten", "Wun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Dir zum Griffel s\u00fc\u00dfer Lieder schon.", "tokens": ["Dir", "zum", "Grif\u00b7fel", "s\u00fc\u00b7\u00dfer", "Lie\u00b7der", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "Dich zu h\u00f6ren, sitzen in die Runde", "tokens": ["Dich", "zu", "h\u00f6\u00b7ren", ",", "sit\u00b7zen", "in", "die", "Run\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Noch um dich die Sch\u00f6nen. Solchem Munde", "tokens": ["Noch", "um", "dich", "die", "Sch\u00f6\u00b7nen", ".", "Sol\u00b7chem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "PRF", "ART", "NN", "$.", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Weigert keine Suada's sch\u00f6nsten Lohn.", "tokens": ["Wei\u00b7gert", "kei\u00b7ne", "Su\u00b7a\u00b7da's", "sch\u00f6ns\u00b7ten", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "Freude fernen Lieben hinzutragen", "tokens": ["Freu\u00b7de", "fer\u00b7nen", "Lie\u00b7ben", "hin\u00b7zu\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Fand Aurora dich auf rauhem Pfad'", "tokens": ["Fand", "Au\u00b7ro\u00b7ra", "dich", "auf", "rau\u00b7hem", "Pfad'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.18": {"text": "Munter oft in deinem Reisewagen,", "tokens": ["Mun\u00b7ter", "oft", "in", "dei\u00b7nem", "Rei\u00b7se\u00b7wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Aber niemals h\u00f6rte sie dich fragen:", "tokens": ["A\u00b7ber", "nie\u00b7mals", "h\u00f6r\u00b7te", "sie", "dich", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Wer gibt Karten, oder: wer legt Skat?", "tokens": ["Wer", "gibt", "Kar\u00b7ten", ",", "o\u00b7der", ":", "wer", "legt", "Skat", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NN", "$,", "KON", "$.", "PWS", "VVFIN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.21": {"text": "Allen Musen stand dein H\u00fcttchen offen,", "tokens": ["Al\u00b7len", "Mu\u00b7sen", "stand", "dein", "H\u00fctt\u00b7chen", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Alle, alle zogen sie hinein.", "tokens": ["Al\u00b7le", ",", "al\u00b7le", "zo\u00b7gen", "sie", "hin\u00b7ein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PIS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.23": {"text": "Schelm und Dummkopf blieben nur betroffen", "tokens": ["Schelm", "und", "Dumm\u00b7kopf", "blie\u00b7ben", "nur", "be\u00b7trof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.24": {"text": "Auf dem Schwell'; nur Weise durften hoffen,", "tokens": ["Auf", "dem", "Schwell'", ";", "nur", "Wei\u00b7se", "durf\u00b7ten", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "ADV", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.25": {"text": "Hier zu Hause wie bei sich zu seyn.", "tokens": ["Hier", "zu", "Hau\u00b7se", "wie", "bei", "sich", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KOKOM", "APPR", "PRF", "PTKZU", "VAINF", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.26": {"text": "Darum k\u00f6nnen wir dich noch umschweben,", "tokens": ["Da\u00b7rum", "k\u00f6n\u00b7nen", "wir", "dich", "noch", "um\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.27": {"text": "Deren Faden sp\u00e4ter Clotho spann,", "tokens": ["De\u00b7ren", "Fa\u00b7den", "sp\u00e4\u00b7ter", "Clo\u00b7tho", "spann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ADJD", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.28": {"text": "Und in deine Stunden Lieder weben.", "tokens": ["Und", "in", "dei\u00b7ne", "Stun\u00b7den", "Lie\u00b7der", "we\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.29": {"text": "Habe Dank, o ", "tokens": ["Ha\u00b7be", "Dank", ",", "o"], "token_info": ["word", "word", "punct", "word"], "pos": ["NN", "NN", "$,", "FM"], "meter": "+-+-", "measure": "trochaic.di"}, "line.30": {"text": "Hast genossen als ein weiser Mann.", "tokens": ["Hast", "ge\u00b7nos\u00b7sen", "als", "ein", "wei\u00b7ser", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}