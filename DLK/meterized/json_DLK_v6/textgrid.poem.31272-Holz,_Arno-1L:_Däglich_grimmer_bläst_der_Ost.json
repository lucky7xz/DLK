{"textgrid.poem.31272": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1L: D\u00e4glich grimmer bl\u00e4st der Ost/", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "D\u00e4glich grimmer bl\u00e4st der Ost/", "tokens": ["D\u00e4g\u00b7lich", "grim\u00b7mer", "bl\u00e4st", "der", "Ost", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gla\u00df-Ey\u00df gl\u00fczzt und Zukker-Frost/", "tokens": ["Gla\u00df\u00b7Ey\u00df", "gl\u00fczzt", "und", "Zuk\u00b7ker\u00b7Frost", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "NN", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "die Kindgens schon drompeten", "tokens": ["die", "Kind\u00b7gens", "schon", "drom\u00b7pe\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "auff kleinen Zinn-Corneten.", "tokens": ["auff", "klei\u00b7nen", "Zinn\u00b7Cor\u00b7ne\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Lebk\u00fcchene Soldaten/", "tokens": ["Leb\u00b7k\u00fc\u00b7che\u00b7ne", "Sol\u00b7da\u00b7ten", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "verguldete Mu\u00dfkaten", "tokens": ["ver\u00b7gul\u00b7de\u00b7te", "Mu\u00df\u00b7ka\u00b7ten"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "bezihren bald die T\u00e4nngens/", "tokens": ["be\u00b7zih\u00b7ren", "bald", "die", "T\u00e4nn\u00b7gens", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "darzu Dukahten-M\u00e4nngens.", "tokens": ["dar\u00b7zu", "Du\u00b7kah\u00b7ten\u00b7M\u00e4nn\u00b7gens", "."], "token_info": ["word", "word", "punct"], "pos": ["PAV", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Man zeigt sich kaum noch wo p\u00fcblik", "tokens": ["Man", "zeigt", "sich", "kaum", "noch", "wo", "p\u00fcb\u00b7lik"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PRF", "ADV", "ADV", "PWAV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "und macht im Kehficht Wald-", "tokens": ["und", "macht", "im", "Keh\u00b7ficht", "Wald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "TRUNC"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie lange wird es tauren/", "tokens": ["Wie", "lan\u00b7ge", "wird", "es", "tau\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "dan dr\u00e4ut der Himmel nicht mehr grie\u00df/", "tokens": ["dan", "dr\u00e4ut", "der", "Him\u00b7mel", "nicht", "mehr", "grie\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKNEG", "ADV", "VVFIN", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "dan buzzt f\u00fcr unsren Mauren", "tokens": ["dan", "buzzt", "f\u00fcr", "un\u00b7sren", "Mau\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Dan kombt/ fast \u00fcber Nacht/", "tokens": ["Dan", "kombt", "/", "fast", "\u00fc\u00b7ber", "Nacht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Frau ", "tokens": ["Frau"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "und bringt in ihrem Mihder", "tokens": ["und", "bringt", "in", "ih\u00b7rem", "Mih\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "den gantzen Fr\u00fchling wihder!", "tokens": ["den", "gant\u00b7zen", "Fr\u00fch\u00b7ling", "wih\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Zahrt an eines B\u00e4chleins Rand", "tokens": ["Zahrt", "an", "ei\u00b7nes", "B\u00e4ch\u00b7leins", "Rand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "wird sich dan wer b\u00fckken", "tokens": ["wird", "sich", "dan", "wer", "b\u00fck\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PRF", "ADV", "PWS", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "und mit seiner weissen Hand", "tokens": ["und", "mit", "sei\u00b7ner", "weis\u00b7sen", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schl\u00fcssel-Bl\u00f6hmckens pfl\u00fckken.", "tokens": ["Schl\u00fcs\u00b7sel\u00b7Bl\u00f6hm\u00b7ckens", "pfl\u00fck\u00b7ken", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Jedes kleine Grillgen geigt", "tokens": ["Je\u00b7des", "klei\u00b7ne", "Grill\u00b7gen", "geigt"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "wa\u00df mir dan ihr Mund verschweigt;", "tokens": ["wa\u00df", "mir", "dan", "ihr", "Mund", "ver\u00b7schweigt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "lacht so laut er lachen kan!", "tokens": ["lacht", "so", "laut", "er", "la\u00b7chen", "kan", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ist der Fr\u00fchling dan verronnen/", "tokens": ["Ist", "der", "Fr\u00fch\u00b7ling", "dan", "ver\u00b7ron\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "singt der Sommer/ da\u00df es schallt/", "tokens": ["singt", "der", "Som\u00b7mer", "/", "da\u00df", "es", "schallt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$(", "KOUS", "PPER", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "lihblich rauschen kleine Bronnen", "tokens": ["lih\u00b7blich", "rau\u00b7schen", "klei\u00b7ne", "Bron\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "durch den gr\u00fcnen Sch\u00e4ffer-Wald.", "tokens": ["durch", "den", "gr\u00fc\u00b7nen", "Sch\u00e4f\u00b7fer\u00b7Wald", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kloris steht bi\u00df an die Waden", "tokens": ["Klo\u00b7ris", "steht", "bi\u00df", "an", "die", "Wa\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "zwischen Moon und Akker-Rhaden/", "tokens": ["zwi\u00b7schen", "Moon", "und", "Ak\u00b7ker\u00b7R\u00b7ha\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "heymlig ziht mich in den Klee", "tokens": ["heym\u00b7lig", "ziht", "mich", "in", "den", "Klee"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "die erhizzte Dorile!", "tokens": ["die", "er\u00b7hizz\u00b7te", "Do\u00b7ri\u00b7le", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Dan f\u00e4rbt der Herbst den B\u00e4umgens", "tokens": ["Dan", "f\u00e4rbt", "der", "Herbst", "den", "B\u00e4um\u00b7gens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Violen-blau die Pfl\u00e4umgens/", "tokens": ["Vi\u00b7o\u00b7len\u00b7blau", "die", "Pfl\u00e4um\u00b7gens", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "das letzte Schwalben-P\u00e4rgen fliht/", "tokens": ["das", "letz\u00b7te", "Schwal\u00b7ben\u00b7P\u00e4r\u00b7gen", "fliht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Michel/ Seppel/ Veit und Hann\u00df", "tokens": ["Mi\u00b7chel", "/", "Sep\u00b7pel", "/", "Veit", "und", "Hann\u00df"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$(", "NN", "$(", "NE", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "springen \u00fcmb den Erndte-Krantz/", "tokens": ["sprin\u00b7gen", "\u00fcmb", "den", "Ernd\u00b7te\u00b7Krantz", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "und s\u00e4zzen \u00fcber alle Kost", "tokens": ["und", "s\u00e4z\u00b7zen", "\u00fc\u00b7ber", "al\u00b7le", "Kost"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Zurlezzt kombt gantz darhindter", "tokens": ["Zur\u00b7lezzt", "kombt", "gantz", "dar\u00b7hind\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PAV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "wihder\u00fcmb der Winter", "tokens": ["wih\u00b7de\u00b7r\u00fcmb", "der", "Win\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "und f\u00fcllt uns durchau\u00df bi\u00df ins Bett", "tokens": ["und", "f\u00fcllt", "uns", "durch\u00b7au\u00df", "bi\u00df", "ins", "Bett"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "APPRART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "voll Ambrosin und Nectar-Fett!", "tokens": ["voll", "A\u00b7mbro\u00b7sin", "und", "Nec\u00b7ta\u00b7rFett", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Man juhchtzt/ drutz Ey\u00df und Schnee/", "tokens": ["Man", "juhchtzt", "/", "drutz", "Ey\u00df", "und", "Schnee", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$(", "NE", "NN", "KON", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "und singt sich \u00fcmmer wihder froh", "tokens": ["und", "singt", "sich", "\u00fcm\u00b7mer", "wih\u00b7der", "froh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "auff seinem ", "tokens": ["auff", "sei\u00b7nem"], "token_info": ["word", "word"], "pos": ["APPR", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.7": {"line.1": {"text": "sie dreht sich st\u00e4ts vom Neuen;", "tokens": ["sie", "dreht", "sich", "st\u00e4ts", "vom", "Neu\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "mit j\u00e4dem/ wa\u00df sie stellt/", "tokens": ["mit", "j\u00e4\u00b7dem", "/", "wa\u00df", "sie", "stellt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$(", "KOUS", "PPER", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "will sie uns bloh\u00df erfreuen.", "tokens": ["will", "sie", "uns", "bloh\u00df", "er\u00b7freu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "--