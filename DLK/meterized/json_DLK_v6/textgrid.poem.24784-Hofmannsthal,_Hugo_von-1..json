{"textgrid.poem.24784": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1901, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rtest du denn nicht hinein,", "tokens": ["H\u00f6r\u00b7test", "du", "denn", "nicht", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df Musik das Haus umschlich?", "tokens": ["Da\u00df", "Mu\u00b7sik", "das", "Haus", "um\u00b7schlich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nacht war schwer und ohne Schein,", "tokens": ["Nacht", "war", "schwer", "und", "oh\u00b7ne", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch der sanft auf hartem Stein", "tokens": ["Doch", "der", "sanft", "auf", "har\u00b7tem", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "APPR", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Lag und spielte, das war ich.", "tokens": ["Lag", "und", "spiel\u00b7te", ",", "das", "war", "ich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,", "PDS", "VAFIN", "PPER", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.2": {"line.1": {"text": "Was ich konnte, sprach ich aus:", "tokens": ["Was", "ich", "konn\u00b7te", ",", "sprach", "ich", "aus", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VMFIN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbliebste du, mein Alles du!\u00ab", "tokens": ["\u00bb", "liebs\u00b7te", "du", ",", "mein", "Al\u00b7les", "du", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "PPOSAT", "PIS", "PPER", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00d6stlich brach ein Licht heraus,", "tokens": ["\u00d6st\u00b7lich", "brach", "ein", "Licht", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwerer Tag trieb mich nach Haus,", "tokens": ["Schwe\u00b7rer", "Tag", "trieb", "mich", "nach", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und mein Mund ist wieder zu", "tokens": ["Und", "mein", "Mund", "ist", "wie\u00b7der", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "PTKZU"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}