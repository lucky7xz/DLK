{"textgrid.poem.34218": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Eine Antwort sonder Gleichen", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Finden Sie selber sie gut? So frug ich in Hamburg den J\u00fcngling,", "tokens": ["Fin\u00b7den", "Sie", "sel\u00b7ber", "sie", "gut", "?", "So", "frug", "ich", "in", "Ham\u00b7burg", "den", "J\u00fcng\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PPER", "ADJD", "$.", "ADV", "VVFIN", "PPER", "APPR", "NE", "ART", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Der mir den schwellenden Band seiner Gedichte gebracht.", "tokens": ["Der", "mir", "den", "schwel\u00b7len\u00b7den", "Band", "sei\u00b7ner", "Ge\u00b7dich\u00b7te", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Freilich! versetzt' er mit Ruhe, denn f\u00e4nd' ich sie anders, so h\u00e4tt' ich", "tokens": ["Frei\u00b7lich", "!", "ver\u00b7setzt'", "er", "mit", "Ru\u00b7he", ",", "denn", "f\u00e4nd'", "ich", "sie", "an\u00b7ders", ",", "so", "h\u00e4tt'", "ich"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$.", "VVFIN", "PPER", "APPR", "NN", "$,", "KON", "VVFIN", "PPER", "PPER", "ADV", "$,", "ADV", "VAFIN", "PPER"], "meter": "+--+--+-++--+--+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Sie ja besser gemacht! Ist es nicht einzig, dies Wort?", "tokens": ["Sie", "ja", "bes\u00b7ser", "ge\u00b7macht", "!", "Ist", "es", "nicht", "ein\u00b7zig", ",", "dies", "Wort", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "VVPP", "$.", "VAFIN", "PPER", "PTKNEG", "ADJD", "$,", "PDS", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}}}}