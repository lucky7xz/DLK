{"textgrid.poem.34556": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ja! Die Ebne ruht in satten D\u00fcften,", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja! Die Ebne ruht in satten D\u00fcften,", "tokens": ["Ja", "!", "Die", "Eb\u00b7ne", "ruht", "in", "sat\u00b7ten", "D\u00fcf\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "hingeschmiegt in sanfter H\u00fcgellinie \u2013", "tokens": ["hin\u00b7ge\u00b7schmiegt", "in", "sanf\u00b7ter", "H\u00fc\u00b7gel\u00b7li\u00b7nie", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "dr\u00fcben, hoch in noch durchsonnten L\u00fcften,", "tokens": ["dr\u00fc\u00b7ben", ",", "hoch", "in", "noch", "durch\u00b7sonn\u00b7ten", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "still und einsam-schattend steht die Pinie.", "tokens": ["still", "und", "ein\u00b7sam\u00b7schat\u00b7tend", "steht", "die", "Pi\u00b7nie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Alte Gr\u00e4ber dunkeln, morsche Steine \u2013", "tokens": ["Al\u00b7te", "Gr\u00e4\u00b7ber", "dun\u00b7keln", ",", "mor\u00b7sche", "Stei\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "fern und unsichtbar ber\u00fchrt dich Rom \u2013", "tokens": ["fern", "und", "un\u00b7sicht\u00b7bar", "be\u00b7r\u00fchrt", "dich", "Rom", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVFIN", "PRF", "NE", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "doch empor aus goldnem Abendscheine", "tokens": ["doch", "em\u00b7por", "aus", "gold\u00b7nem", "A\u00b7bend\u00b7schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "steigt die Kuppel von Sanct Peters Dom.", "tokens": ["steigt", "die", "Kup\u00b7pel", "von", "Sanct", "Pe\u00b7ters", "Dom", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "VVFIN", "NE", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Und du ahnest, wie sich Fl\u00fcgel heben,", "tokens": ["Und", "du", "ah\u00b7nest", ",", "wie", "sich", "Fl\u00fc\u00b7gel", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PWAV", "PRF", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "gleich den abendfarbnen Wolkenstreifen,", "tokens": ["gleich", "den", "a\u00b7bend\u00b7farb\u00b7nen", "Wol\u00b7ken\u00b7strei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "denn die Seele will zur H\u00f6he schweben", "tokens": ["denn", "die", "See\u00b7le", "will", "zur", "H\u00f6\u00b7he", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VMFIN", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "und der Wille durch die Fernen schweifen.", "tokens": ["und", "der", "Wil\u00b7le", "durch", "die", "Fer\u00b7nen", "schwei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Wage nur zu lieben, was genossen,", "tokens": ["Wa\u00b7ge", "nur", "zu", "lie\u00b7ben", ",", "was", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKZU", "VVINF", "$,", "PRELS", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "zu behaupten, was dich je entz\u00fcndet \u2013", "tokens": ["zu", "be\u00b7haup\u00b7ten", ",", "was", "dich", "je", "ent\u00b7z\u00fcn\u00b7det", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PWS", "PPER", "ADV", "VVPP", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "alle Wonnen sind in dir beschlossen,", "tokens": ["al\u00b7le", "Won\u00b7nen", "sind", "in", "dir", "be\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "jede Gr\u00f6sse ist in dir begr\u00fcndet.", "tokens": ["je\u00b7de", "Gr\u00f6s\u00b7se", "ist", "in", "dir", "be\u00b7gr\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}