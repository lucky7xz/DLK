{"textgrid.poem.35028": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Meine Legitimation", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gr\u00fc\u00df Gott, du liebes Tr\u00f6pflein Thau!", "tokens": ["Gr\u00fc\u00df", "Gott", ",", "du", "lie\u00b7bes", "Tr\u00f6pf\u00b7lein", "Thau", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "PPER", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So einen Schmuck giebt es wohl nimmer:", "tokens": ["So", "ei\u00b7nen", "Schmuck", "giebt", "es", "wohl", "nim\u00b7mer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Von jedem H\u00e4lmchen auf der Au", "tokens": ["Von", "je\u00b7dem", "H\u00e4lm\u00b7chen", "auf", "der", "Au"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Spitzt es wie Diamantenschimmer.", "tokens": ["Spitzt", "es", "wie", "Di\u00b7a\u00b7man\u00b7ten\u00b7schim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Entstammt der Erde, harrst du froh", "tokens": ["Ent\u00b7stammt", "der", "Er\u00b7de", ",", "harrst", "du", "froh"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "VAFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem holden Morgenlicht entgegen,", "tokens": ["Dem", "hol\u00b7den", "Mor\u00b7gen\u00b7licht", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Tr\u00e4nkst deinen Halm und wirst ihm so", "tokens": ["Tr\u00e4nkst", "dei\u00b7nen", "Halm", "und", "wirst", "ihm", "so"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nicht nur zur Zierde, auch zum Segen.", "tokens": ["Nicht", "nur", "zur", "Zier\u00b7de", ",", "auch", "zum", "Se\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPRART", "NN", "$,", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Kommt dann aus gold-brokatnem Thor", "tokens": ["Kommt", "dann", "aus", "gold\u00b7bro\u00b7kat\u00b7nem", "Thor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die K\u00f6nigin des Tags gestiegen,", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7gin", "des", "Tags", "ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So strebst du sehnsuchtsvoll empor,", "tokens": ["So", "strebst", "du", "sehn\u00b7suchts\u00b7voll", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dich ihrem Strahle anzuschmiegen.", "tokens": ["Dich", "ih\u00b7rem", "Strah\u00b7le", "an\u00b7zu\u00b7schmie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du f\u00fchlst, du bist ihr unterthan,", "tokens": ["Du", "f\u00fchlst", ",", "du", "bist", "ihr", "un\u00b7ter\u00b7than", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du kannst nicht ohne sie bestehen", "tokens": ["Du", "kannst", "nicht", "oh\u00b7ne", "sie", "be\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und wirst gezogen himmelan,", "tokens": ["Und", "wirst", "ge\u00b7zo\u00b7gen", "him\u00b7me\u00b7lan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In ihrem Kusse aufzugehen.", "tokens": ["In", "ih\u00b7rem", "Kus\u00b7se", "auf\u00b7zu\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein solches Tr\u00f6pflein bin auch ich", "tokens": ["Ein", "sol\u00b7ches", "Tr\u00f6pf\u00b7lein", "bin", "auch", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VAFIN", "ADV", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Lebensmorgen einst gewesen,", "tokens": ["Am", "Le\u00b7bens\u00b7mor\u00b7gen", "einst", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Tr\u00f6pflein, das den andern glich,", "tokens": ["Ein", "Tr\u00f6pf\u00b7lein", ",", "das", "den", "an\u00b7dern", "glich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht auserw\u00e4hlt, nicht auserlesen.", "tokens": ["Nicht", "au\u00b7ser\u00b7w\u00e4hlt", ",", "nicht", "au\u00b7ser\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "$,", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich hing nicht hoch, ich wurde nicht", "tokens": ["Ich", "hing", "nicht", "hoch", ",", "ich", "wur\u00b7de", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJD", "$,", "PPER", "VAFIN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von einer Rose stolz getragen;", "tokens": ["Von", "ei\u00b7ner", "Ro\u00b7se", "stolz", "ge\u00b7tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Tief unten sah ich auf zum Licht", "tokens": ["Tief", "un\u00b7ten", "sah", "ich", "auf", "zum", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "VVFIN", "PPER", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und durfte kaum zu hoffen wagen.", "tokens": ["Und", "durf\u00b7te", "kaum", "zu", "hof\u00b7fen", "wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da stieg sie auf, so himmlisch klar,", "tokens": ["Da", "stieg", "sie", "auf", ",", "so", "himm\u00b7lisch", "klar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So gnadenreich, voll Welterbarmen,", "tokens": ["So", "gna\u00b7den\u00b7reich", ",", "voll", "Wel\u00b7ter\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und mich trieb es so wunderbar,", "tokens": ["Und", "mich", "trieb", "es", "so", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Mit ihr die Menschheit zu umarmen.", "tokens": ["Mit", "ihr", "die", "Menschheit", "zu", "um\u00b7ar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Es war, als ob ich beten m\u00fc\u00dft:", "tokens": ["Es", "war", ",", "als", "ob", "ich", "be\u00b7ten", "m\u00fc\u00dft", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbo komm, und stille mein Verlangen!\u00ab", "tokens": ["\u00bb", "o", "komm", ",", "und", "stil\u00b7le", "mein", "Ver\u00b7lan\u00b7gen", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "VVFIN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da hat die Liebe mich gek\u00fc\u00dft,", "tokens": ["Da", "hat", "die", "Lie\u00b7be", "mich", "ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und ich bin in ihr aufgegangen.", "tokens": ["Und", "ich", "bin", "in", "ihr", "auf\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}