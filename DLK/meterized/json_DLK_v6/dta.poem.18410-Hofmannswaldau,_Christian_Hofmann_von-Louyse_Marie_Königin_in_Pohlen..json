{"dta.poem.18410": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Louyse Marie/ K\u00f6nigin in Pohlen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Seit Franckreich diesen stern den Pohlen hat gegeben/", "tokens": ["Seit", "Fran\u00b7ck\u00b7reich", "die\u00b7sen", "stern", "den", "Poh\u00b7len", "hat", "ge\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PDS", "VVFIN", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und ihrer sonnen ihn als monden beygef\u00fchrt/", "tokens": ["Und", "ih\u00b7rer", "son\u00b7nen", "ihn", "als", "mon\u00b7den", "bey\u00b7ge\u00b7f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "PPER", "KOUS", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So wollen ihrer viel in dieser meinung schweben/", "tokens": ["So", "wol\u00b7len", "ih\u00b7rer", "viel", "in", "die\u00b7ser", "mei\u00b7nung", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "ADV", "APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df dieser monden mehr als in der nacht regiert.", "tokens": ["Da\u00df", "die\u00b7ser", "mon\u00b7den", "mehr", "als", "in", "der", "nacht", "re\u00b7giert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PIAT", "KOKOM", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}