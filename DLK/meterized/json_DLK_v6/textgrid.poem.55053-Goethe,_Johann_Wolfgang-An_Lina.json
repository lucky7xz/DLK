{"textgrid.poem.55053": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "An Lina", "genre": "verse", "period": "N.A.", "pub_year": 1799, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebchen, kommen diese Lieder", "tokens": ["Lieb\u00b7chen", ",", "kom\u00b7men", "die\u00b7se", "Lie\u00b7der"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jemals wieder dir zur Hand,", "tokens": ["Je\u00b7mals", "wie\u00b7der", "dir", "zur", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sitze beim Klaviere nieder,", "tokens": ["Sit\u00b7ze", "beim", "Kla\u00b7vie\u00b7re", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Wo der Freund sonst bei dir stand.", "tokens": ["Wo", "der", "Freund", "sonst", "bei", "dir", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00df die Saiten rasch erklingen,", "tokens": ["La\u00df", "die", "Sai\u00b7ten", "rasch", "er\u00b7klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und dann sieh ins Buch hinein;", "tokens": ["Und", "dann", "sieh", "ins", "Buch", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur nicht lesen! immer singen!", "tokens": ["Nur", "nicht", "le\u00b7sen", "!", "im\u00b7mer", "sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVINF", "$.", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ein jedes Blatt ist dein.", "tokens": ["Und", "ein", "je\u00b7des", "Blatt", "ist", "dein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIAT", "NN", "VAFIN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach, wie traurig sieht in Lettern,", "tokens": ["Ach", ",", "wie", "trau\u00b7rig", "sieht", "in", "Let\u00b7tern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ADJD", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwarz auf wei\u00df, das Lied mich an,", "tokens": ["Schwarz", "auf", "wei\u00df", ",", "das", "Lied", "mich", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "VVFIN", "$,", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das aus deinem Mund verg\u00f6ttern,", "tokens": ["Das", "aus", "dei\u00b7nem", "Mund", "ver\u00b7g\u00f6t\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das ein Herz zerrei\u00dfen kann!", "tokens": ["Das", "ein", "Herz", "zer\u00b7rei\u00b7\u00dfen", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}