{"textgrid.poem.43047": {"metadata": {"author": {"name": "Jacoby, Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Winterst\u00e4ndchen", "genre": "verse", "period": "N.A.", "pub_year": 1867, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schnee liegt auf den Gassen weit,", "tokens": ["Schnee", "liegt", "auf", "den", "Gas\u00b7sen", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schnee gl\u00e4nzt von den D\u00e4chern wieder.", "tokens": ["Schnee", "gl\u00e4nzt", "von", "den", "D\u00e4\u00b7chern", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Voll in Winterherrlichkeit", "tokens": ["Voll", "in", "Win\u00b7ter\u00b7herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Strahlt der lichte Mond hernieder,", "tokens": ["Strahlt", "der", "lich\u00b7te", "Mond", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Strahlt auf's Haus, wo Liebchen ruht,", "tokens": ["Strahlt", "auf's", "Haus", ",", "wo", "Lieb\u00b7chen", "ruht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,", "PWAV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo die Engel halten Hut.", "tokens": ["Wo", "die", "En\u00b7gel", "hal\u00b7ten", "Hut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schlaf s\u00fc\u00df, mein Lieb, schlaf s\u00fc\u00df!", "tokens": ["Schlaf", "s\u00fc\u00df", ",", "mein", "Lieb", ",", "schlaf", "s\u00fc\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PPOSAT", "NN", "$,", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Deiner Zukunft Rosenbild", "tokens": ["Dei\u00b7ner", "Zu\u00b7kunft", "Ro\u00b7sen\u00b7bild"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird durch deine Tr\u00e4ume gehen.", "tokens": ["Wird", "durch", "dei\u00b7ne", "Tr\u00e4u\u00b7me", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Goldig gl\u00e4nzend, wundermild", "tokens": ["Gol\u00b7dig", "gl\u00e4n\u00b7zend", ",", "wun\u00b7der\u00b7mild"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJD", "ADJD", "$,", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird ein Stern am Himmel stehen,", "tokens": ["Wird", "ein", "Stern", "am", "Him\u00b7mel", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Den die Liebe sendet dir,", "tokens": ["Den", "die", "Lie\u00b7be", "sen\u00b7det", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Der dir leuchtet f\u00fcr und f\u00fcr.", "tokens": ["Der", "dir", "leuch\u00b7tet", "f\u00fcr", "und", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "KON", "APPR", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schlaf s\u00fc\u00df, mein Lieb, schlaf s\u00fc\u00df!", "tokens": ["Schlaf", "s\u00fc\u00df", ",", "mein", "Lieb", ",", "schlaf", "s\u00fc\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PPOSAT", "NN", "$,", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Horch, der Sturm mit Schneegebraus", "tokens": ["Horch", ",", "der", "Sturm", "mit", "Schnee\u00b7ge\u00b7braus"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00fcttelt sich vor Frostbeschwerde.", "tokens": ["Sch\u00fct\u00b7telt", "sich", "vor", "Frost\u00b7be\u00b7schwer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle Blumen schlummern aus,", "tokens": ["Al\u00b7le", "Blu\u00b7men", "schlum\u00b7mern", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn der Winter deckt die Erde.", "tokens": ["Denn", "der", "Win\u00b7ter", "deckt", "die", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wachst du auf, wird Fr\u00fchling sein", "tokens": ["Wachst", "du", "auf", ",", "wird", "Fr\u00fch\u00b7ling", "sein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "VAFIN", "NN", "PPOSAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Rings um mich durch dich allein.", "tokens": ["Rings", "um", "mich", "durch", "dich", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "APPR", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schlaf s\u00fc\u00df, mein Lieb, schlaf s\u00fc\u00df!", "tokens": ["Schlaf", "s\u00fc\u00df", ",", "mein", "Lieb", ",", "schlaf", "s\u00fc\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PPOSAT", "NN", "$,", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Aber eine innere Stimme sprach zu mir:", "tokens": ["A\u00b7ber", "ei\u00b7ne", "in\u00b7ne\u00b7re", "Stim\u00b7me", "sprach", "zu", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Du warst wie ein Kind, aller Thorheiten voll.", "tokens": ["Du", "warst", "wie", "ein", "Kind", ",", "al\u00b7ler", "Thor\u00b7hei\u00b7ten", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "ART", "NN", "$,", "PIAT", "NN", "ADJD", "$."], "meter": "-+--+--++-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Aber mitten darin, du wei\u00dft es wohl,", "tokens": ["A\u00b7ber", "mit\u00b7ten", "da\u00b7rin", ",", "du", "wei\u00dft", "es", "wohl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PAV", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Habe ich dir immer einen Sto\u00df gegeben,", "tokens": ["Ha\u00b7be", "ich", "dir", "im\u00b7mer", "ei\u00b7nen", "Sto\u00df", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Da\u00df du solltest eingedenk bleiben der Botschaft", "tokens": ["Da\u00df", "du", "soll\u00b7test", "ein\u00b7ge\u00b7denk", "blei\u00b7ben", "der", "Bot\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VMFIN", "PIS", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Und nicht vergessen des Auftrags.", "tokens": ["Und", "nicht", "ver\u00b7ges\u00b7sen", "des", "Auf\u00b7trags", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVPP", "ART", "NN", "$."], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Siehe nun die Zeit ist gekommen,", "tokens": ["Sie\u00b7he", "nun", "die", "Zeit", "ist", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "So r\u00fcttle dich und gehe!", "tokens": ["So", "r\u00fctt\u00b7le", "dich", "und", "ge\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und ich erbebte tief im Herzen und sprach:", "tokens": ["Und", "ich", "er\u00b7beb\u00b7te", "tief", "im", "Her\u00b7zen", "und", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "APPRART", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.10": {"text": "Ich will leben, so la\u00df mich leben!", "tokens": ["Ich", "will", "le\u00b7ben", ",", "so", "la\u00df", "mich", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "ADV", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Aber die Stimme sprach zu mir:", "tokens": ["A\u00b7ber", "die", "Stim\u00b7me", "sprach", "zu", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.12": {"text": "Willst du leben, sieh her,", "tokens": ["Willst", "du", "le\u00b7ben", ",", "sieh", "her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "VVIMP", "PTKVZ", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "Ich ziehe heute den Vorhang von deinen Augen,", "tokens": ["Ich", "zie\u00b7he", "heu\u00b7te", "den", "Vor\u00b7hang", "von", "dei\u00b7nen", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Durch den du die Menschen siehest,", "tokens": ["Durch", "den", "du", "die", "Men\u00b7schen", "sie\u00b7hest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Und durch den die Andern auf Erden die Menschen sehen.", "tokens": ["Und", "durch", "den", "die", "An\u00b7dern", "auf", "Er\u00b7den", "die", "Men\u00b7schen", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ART", "ADJA", "APPR", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}, "line.16": {"text": "Und ich sah hin, \u2013", "tokens": ["Und", "ich", "sah", "hin", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "$,", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Da erschauderte so mein Gebein,", "tokens": ["Da", "er\u00b7schau\u00b7der\u00b7te", "so", "mein", "Ge\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.18": {"text": "Da\u00df ich st\u00fcrzte und schrie:", "tokens": ["Da\u00df", "ich", "st\u00fcrz\u00b7te", "und", "schrie", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.19": {"text": "Halt ein, halt ein! ich will lieber sterben!", "tokens": ["Halt", "ein", ",", "halt", "ein", "!", "ich", "will", "lie\u00b7ber", "ster\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$.", "PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Seitdem ist die Freude von mir genommen.", "tokens": ["Seit\u00b7dem", "ist", "die", "Freu\u00b7de", "von", "mir", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.21": {"text": "Ich bin wie ein Sieb,", "tokens": ["Ich", "bin", "wie", "ein", "Sieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.22": {"text": "So durchgesch\u00fcttelt von Schmerz,", "tokens": ["So", "durch\u00b7ge\u00b7sch\u00fct\u00b7telt", "von", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.23": {"text": "Alles Lustige ist davongeflogen", "tokens": ["Al\u00b7les", "Lus\u00b7ti\u00b7ge", "ist", "da\u00b7von\u00b7ge\u00b7flo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "PAV"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.24": {"text": "Und nur die schweren St\u00fccke sind zur\u00fcckgeblieben.", "tokens": ["Und", "nur", "die", "schwe\u00b7ren", "St\u00fc\u00b7cke", "sind", "zu\u00b7r\u00fcck\u00b7ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Es trieb mich hinauf auf die Waldh\u00f6he,", "tokens": ["Es", "trieb", "mich", "hin\u00b7auf", "auf", "die", "Wald\u00b7h\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+--", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die am Ufer des Meeres liegt.", "tokens": ["Die", "am", "U\u00b7fer", "des", "Mee\u00b7res", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Da erhob sich ein Sturmwind vom Meere her,", "tokens": ["Da", "er\u00b7hob", "sich", "ein", "Sturm\u00b7wind", "vom", "Mee\u00b7re", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Ein solcher war noch niemals heraufgekommen.", "tokens": ["Ein", "sol\u00b7cher", "war", "noch", "nie\u00b7mals", "her\u00b7auf\u00b7ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "VAFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und die lustigen Birken zitterten,", "tokens": ["Und", "die", "lus\u00b7ti\u00b7gen", "Bir\u00b7ken", "zit\u00b7ter\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+-+--", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und die Buchen und Tannen rauschten tief auf vor Angst.", "tokens": ["Und", "die", "Bu\u00b7chen", "und", "Tan\u00b7nen", "rauschten", "tief", "auf", "vor", "Angst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "KON", "NN", "VVFIN", "ADJD", "APPR", "APPR", "NN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Droben aber da stand ein Eichbaum,", "tokens": ["Dro\u00b7ben", "a\u00b7ber", "da", "stand", "ein", "Eich\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Der schien unersch\u00