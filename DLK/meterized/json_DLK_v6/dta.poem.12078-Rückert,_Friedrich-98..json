{"dta.poem.12078": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "98.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In deines Herzen Haus- und Festkalender mag", "tokens": ["In", "dei\u00b7nes", "Her\u00b7zen", "Haus", "und", "Fest\u00b7ka\u00b7len\u00b7der", "mag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "TRUNC", "KON", "NN", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur auch gezeichnet seyn ein Allerseelentag.", "tokens": ["Nur", "auch", "ge\u00b7zeich\u00b7net", "seyn", "ein", "Al\u00b7ler\u00b7see\u00b7len\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "VAINF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gezeichnet soll er seyn nicht mit zu d\u00fcstern Farben,", "tokens": ["Ge\u00b7zeich\u00b7net", "soll", "er", "seyn", "nicht", "mit", "zu", "d\u00fcs\u00b7tern", "Far\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VMFIN", "PPER", "VAINF", "PTKNEG", "APPR", "PTKZU", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch auch zu helle sind f\u00fcr die nicht die da starben.", "tokens": ["Doch", "auch", "zu", "hel\u00b7le", "sind", "f\u00fcr", "die", "nicht", "die", "da", "star\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "VAFIN", "APPR", "PRELS", "PTKNEG", "ART", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mit sanftern Lichtern sei und leisem Schattenschlag", "tokens": ["Mit", "sanf\u00b7tern", "Lich\u00b7tern", "sei", "und", "lei\u00b7sem", "Schat\u00b7ten\u00b7schlag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gezeichnet in dein Herz dein Allerseelentag.", "tokens": ["Ge\u00b7zeich\u00b7net", "in", "dein", "Herz", "dein", "Al\u00b7ler\u00b7see\u00b7len\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ein Allerseelentag, wo du vereint in Frieden", "tokens": ["Ein", "Al\u00b7ler\u00b7see\u00b7len\u00b7tag", ",", "wo", "du", "ver\u00b7eint", "in", "Frie\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit allen Seelen bist, die von dir sind geschieden;", "tokens": ["Mit", "al\u00b7len", "See\u00b7len", "bist", ",", "die", "von", "dir", "sind", "ge\u00b7schie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "$,", "PRELS", "APPR", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wo alle Seelen, die dich aus der Fern' umwallen,", "tokens": ["Wo", "al\u00b7le", "See\u00b7len", ",", "die", "dich", "aus", "der", "Fern'", "um\u00b7wal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "$,", "PRELS", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum Fest versammelt sind in deines Tempels Hallen.", "tokens": ["Zum", "Fest", "ver\u00b7sam\u00b7melt", "sind", "in", "dei\u00b7nes", "Tem\u00b7pels", "Hal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "VAFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Da bete f\u00fcr ihr Heil, und la\u00df sie beten auch", "tokens": ["Da", "be\u00b7te", "f\u00fcr", "ihr", "Heil", ",", "und", "la\u00df", "sie", "be\u00b7ten", "auch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "KON", "VVIMP", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcr deines, denn Gebet ist Seelenlebenshauch.", "tokens": ["F\u00fcr", "dei\u00b7nes", ",", "denn", "Ge\u00b7bet", "ist", "See\u00b7len\u00b7le\u00b7bens\u00b7hauch", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "$,", "KON", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Manch Angedenken zieh hervor, an das sich kn\u00fcpft", "tokens": ["Manch", "An\u00b7ge\u00b7den\u00b7ken", "zieh", "her\u00b7vor", ",", "an", "das", "sich", "kn\u00fcpft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PTKVZ", "$,", "APPR", "PRELS", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Name, zieh es fest, da\u00df er dir nicht entschl\u00fcpft.", "tokens": ["Ein", "Na\u00b7me", ",", "zieh", "es", "fest", ",", "da\u00df", "er", "dir", "nicht", "ent\u00b7schl\u00fcpft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Manch theures Bild auch, eh der Kennzug dir erlischt,", "tokens": ["Manch", "theu\u00b7res", "Bild", "auch", ",", "eh", "der", "Kenn\u00b7zug", "dir", "er\u00b7lischt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ADV", "$,", "KOUS", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sei von der Malerinn Erinnrung angefrischt.", "tokens": ["Sei", "von", "der", "Ma\u00b7le\u00b7rinn", "E\u00b7rinn\u00b7rung", "an\u00b7ge\u00b7frischt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Bedaure du sie nicht, da\u00df sie der Welt entgangen,", "tokens": ["Be\u00b7dau\u00b7re", "du", "sie", "nicht", ",", "da\u00df", "sie", "der", "Welt", "ent\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nicht beneide sie, denn du wirst nachgelangen.", "tokens": ["Und", "nicht", "be\u00b7nei\u00b7de", "sie", ",", "denn", "du", "wirst", "nach\u00b7ge\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVFIN", "PPER", "$,", "KON", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Versichere du nur dich ihrer, da\u00df sie bleiben", "tokens": ["Ver\u00b7si\u00b7che\u00b7re", "du", "nur", "dich", "ih\u00b7rer", ",", "da\u00df", "sie", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "PPOSAT", "$,", "KOUS", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von oben dein Geleit, nach oben dich zu treiben.", "tokens": ["Von", "o\u00b7ben", "dein", "Ge\u00b7leit", ",", "nach", "o\u00b7ben", "dich", "zu", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PPOSAT", "NN", "$,", "APPR", "ADV", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Von oben neigen sie, nach oben zeigen sie", "tokens": ["Von", "o\u00b7ben", "nei\u00b7gen", "sie", ",", "nach", "o\u00b7ben", "zei\u00b7gen", "sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "$,", "APPR", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und deinem Blick voran nach oben steigen sie.", "tokens": ["Und", "dei\u00b7nem", "Blick", "vo\u00b7ran", "nach", "o\u00b7ben", "stei\u00b7gen", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKVZ", "APPR", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Nach oben steigen sie, wo sie dir wollen zeigen,", "tokens": ["Nach", "o\u00b7ben", "stei\u00b7gen", "sie", ",", "wo", "sie", "dir", "wol\u00b7len", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was sie versprechen mit geheimnisvollem Schweigen.", "tokens": ["Was", "sie", "ver\u00b7spre\u00b7chen", "mit", "ge\u00b7heim\u00b7nis\u00b7vol\u00b7lem", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}