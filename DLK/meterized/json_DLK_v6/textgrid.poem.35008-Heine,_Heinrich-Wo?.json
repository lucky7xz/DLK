{"textgrid.poem.35008": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Wo?", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo wird einst des Wanderm\u00fcden", "tokens": ["Wo", "wird", "einst", "des", "Wan\u00b7der\u00b7m\u00fc\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ADV", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Letzte Ruhest\u00e4tte sein?", "tokens": ["Letz\u00b7te", "Ru\u00b7he\u00b7st\u00e4t\u00b7te", "sein", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter Palmen in dem S\u00fcden?", "tokens": ["Un\u00b7ter", "Pal\u00b7men", "in", "dem", "S\u00fc\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unter Linden an dem Rhein?", "tokens": ["Un\u00b7ter", "Lin\u00b7den", "an", "dem", "Rhein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Werd ich wo in einer W\u00fcste", "tokens": ["Werd", "ich", "wo", "in", "ei\u00b7ner", "W\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PWAV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eingescharrt von fremder Hand?", "tokens": ["Ein\u00b7ge\u00b7scharrt", "von", "frem\u00b7der", "Hand", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder ruh ich an der K\u00fcste", "tokens": ["O\u00b7der", "ruh", "ich", "an", "der", "K\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eines Meeres in dem Sand?", "tokens": ["Ei\u00b7nes", "Mee\u00b7res", "in", "dem", "Sand", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Immerhin! Mich wird umgeben", "tokens": ["Im\u00b7mer\u00b7hin", "!", "Mich", "wird", "um\u00b7ge\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$.", "PPER", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gotteshimmel, dort wie hier,", "tokens": ["Got\u00b7te\u00b7shim\u00b7mel", ",", "dort", "wie", "hier", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "KOKOM", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und als Totenlampen schweben", "tokens": ["Und", "als", "To\u00b7ten\u00b7lam\u00b7pen", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nachts die Sterne \u00fcber mir.", "tokens": ["Nachts", "die", "Ster\u00b7ne", "\u00fc\u00b7ber", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}