{"textgrid.poem.62246": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "An denselben", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den ersten Gru\u00df am Morgen", "tokens": ["Den", "ers\u00b7ten", "Gru\u00df", "am", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Empfing ich stets von Dir,", "tokens": ["Emp\u00b7fing", "ich", "stets", "von", "Dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Herz und Geist und Seele", "tokens": ["Und", "Herz", "und", "Geist", "und", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Lag in dem Ton zu mir.", "tokens": ["Lag", "in", "dem", "Ton", "zu", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Du wirst mir immer fehlen,", "tokens": ["Du", "wirst", "mir", "im\u00b7mer", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Stets bange bleibt's nach Dir,", "tokens": ["Stets", "ban\u00b7ge", "bleibt's", "nach", "Dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du s\u00fc\u00dfer Jakob, Kobusch", "tokens": ["Du", "s\u00fc\u00b7\u00dfer", "Ja\u00b7kob", ",", "Ko\u00b7busch"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "ADJA", "NE", "$,", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Bleibst unvergessen hier.", "tokens": ["Bleibst", "un\u00b7ver\u00b7ges\u00b7sen", "hier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Seit zweiundzwanzig Jahren,", "tokens": ["Seit", "zwei\u00b7und\u00b7zwan\u00b7zig", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Seit meiner Mutter Tod,", "tokens": ["Seit", "mei\u00b7ner", "Mut\u00b7ter", "Tod", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Warst Du mein treu Gef\u00e4hrte", "tokens": ["Warst", "Du", "mein", "treu", "Ge\u00b7f\u00e4hr\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Freude, Schmerz und Not!", "tokens": ["In", "Freu\u00b7de", ",", "Schmerz", "und", "Not", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Du bist nicht fortgewichen", "tokens": ["Du", "bist", "nicht", "fort\u00b7ge\u00b7wi\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von ihrem Totenbett", "tokens": ["Von", "ih\u00b7rem", "To\u00b7ten\u00b7bett"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und warst Dein ganzes Leben", "tokens": ["Und", "warst", "Dein", "gan\u00b7zes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Stets geistvoll, klug und nett.", "tokens": ["Stets", "geist\u00b7voll", ",", "klug", "und", "nett", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du wirst mir immer fehlen,", "tokens": ["Du", "wirst", "mir", "im\u00b7mer", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Stets bange bleibt's nach Dir,", "tokens": ["Stets", "ban\u00b7ge", "bleibt's", "nach", "Dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du s\u00fc\u00dfer Jakob, Kobusch", "tokens": ["Du", "s\u00fc\u00b7\u00dfer", "Ja\u00b7kob", ",", "Ko\u00b7busch"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "ADJA", "NE", "$,", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Bleibst unvergessen hier.", "tokens": ["Bleibst", "un\u00b7ver\u00b7ges\u00b7sen", "hier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und mehr warst Du beweinet,", "tokens": ["Und", "mehr", "warst", "Du", "be\u00b7wei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Als mancher Mensch vor Dir,", "tokens": ["Als", "man\u00b7cher", "Mensch", "vor", "Dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O, Koberle, o Jakob,", "tokens": ["O", ",", "Ko\u00b7ber\u00b7le", ",", "o", "Ja\u00b7kob", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "FM", "NE", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Bleibst unverge\u00dflich mir.", "tokens": ["Bleibst", "un\u00b7ver\u00b7ge\u00df\u00b7lich", "mir", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}