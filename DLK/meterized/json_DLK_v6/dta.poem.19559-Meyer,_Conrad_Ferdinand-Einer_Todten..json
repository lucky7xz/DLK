{"dta.poem.19559": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Einer Todten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Wie f\u00fchl' ich heute deine Macht,", "tokens": ["Wie", "f\u00fchl'", "ich", "heu\u00b7te", "dei\u00b7ne", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als ob sich deine Wimper schatte", "tokens": ["Als", "ob", "sich", "dei\u00b7ne", "Wim\u00b7per", "schat\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PRF", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vor mir auf diesem ampelhellen Blatte", "tokens": ["Vor", "mir", "auf", "die\u00b7sem", "am\u00b7pel\u00b7hel\u00b7len", "Blat\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um Mitternacht!", "tokens": ["Um", "Mit\u00b7ter\u00b7nacht", "!"], "token_info": ["word", "word", "punct"], "pos": ["KOUI", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Dein Auge sieht", "tokens": ["Dein", "Au\u00b7ge", "sieht"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Begierig mein entstehend Lied.", "tokens": ["Be\u00b7gie\u00b7rig", "mein", "ent\u00b7ste\u00b7hend", "Lied", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "VVPP", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dein Wesen neigt sich meinem zu,", "tokens": ["Dein", "We\u00b7sen", "neigt", "sich", "mei\u00b7nem", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "PPOSAT", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du bist's! Doch deine Lippen schweigen,", "tokens": ["Du", "bist's", "!", "Doch", "dei\u00b7ne", "Lip\u00b7pen", "schwei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "$.", "KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und liesest du ein Wort, das zart und eigen,", "tokens": ["Und", "lie\u00b7sest", "du", "ein", "Wort", ",", "das", "zart", "und", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bist's wieder du,", "tokens": ["Bist's", "wie\u00b7der", "du", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Dein Herzensblut,", "tokens": ["Dein", "Her\u00b7zens\u00b7blut", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Inde\u00df dein Staub im Grabe ruht.", "tokens": ["In\u00b7de\u00df", "dein", "Staub", "im", "Gra\u00b7be", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mir ist, wann mich dein Athem streift,", "tokens": ["Mir", "ist", ",", "wann", "mich", "dein", "A\u00b7them", "streift", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der ich erstarkt an Kampf und Wunden,", "tokens": ["Der", "ich", "er\u00b7starkt", "an", "Kampf", "und", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als seist in deinen stillen Grabestunden", "tokens": ["Als", "seist", "in", "dei\u00b7nen", "stil\u00b7len", "Gra\u00b7be\u00b7stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auch du gereift", "tokens": ["Auch", "du", "ge\u00b7reift"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PPER", "VVPP"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "An Liebeskraft,", "tokens": ["An", "Lie\u00b7bes\u00b7kraft", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "An Willen und an Leidenschaft.", "tokens": ["An", "Wil\u00b7len", "und", "an", "Lei\u00b7den\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Marmorurne setzten dir", "tokens": ["Die", "Mar\u00b7mo\u00b7rur\u00b7ne", "setz\u00b7ten", "dir"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Deinen \u2014 um dich zu vergessen,", "tokens": ["Die", "Dei\u00b7nen", "um", "dich", "zu", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "$(", "KOUI", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie erbten, bauten, freiten unterdessen,", "tokens": ["Sie", "erb\u00b7ten", ",", "bau\u00b7ten", ",", "frei\u00b7ten", "un\u00b7ter\u00b7des\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du lebst in mir!", "tokens": ["Du", "lebst", "in", "mir", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Wozu beweint?", "tokens": ["Wo\u00b7zu", "be\u00b7weint", "?"], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Du lebst und f\u00fchlst mit mir vereint!", "tokens": ["Du", "lebst", "und", "f\u00fchlst", "mit", "mir", "ver\u00b7eint", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}