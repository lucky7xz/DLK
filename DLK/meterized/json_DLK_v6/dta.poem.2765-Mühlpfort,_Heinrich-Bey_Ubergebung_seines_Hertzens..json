{"dta.poem.2765": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Bey Ubergebung seines Hertzens.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nimm Clytie zu dem Geschencke", "tokens": ["Nimm", "Cly\u00b7tie", "zu", "dem", "Ge\u00b7schen\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mein Hertze/ weil mir Geld gebricht/", "tokens": ["Mein", "Hert\u00b7ze", "/", "weil", "mir", "Geld", "ge\u00b7bricht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "$(", "KOUS", "PPER", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du siehst/ da\u00df ich auff Liebe dencke/", "tokens": ["Du", "siehst", "/", "da\u00df", "ich", "auff", "Lie\u00b7be", "den\u00b7cke", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die aller Sch\u00e4tze Schatz und Licht;", "tokens": ["Die", "al\u00b7ler", "Sch\u00e4t\u00b7ze", "Schatz", "und", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und weil ich leben", "tokens": ["Und", "weil", "ich", "le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Mu\u00df unter dir/", "tokens": ["Mu\u00df", "un\u00b7ter", "dir", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPER", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "So will ich geben", "tokens": ["So", "will", "ich", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Zur Pflicht-Geb\u00fchr", "tokens": ["Zur", "Pflicht\u00b7Ge\u00b7b\u00fchr"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Mein Hertze hier.", "tokens": ["Mein", "Hert\u00b7ze", "hier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Erschrick nicht/ da\u00df es so erzittert/", "tokens": ["Er\u00b7schrick", "nicht", "/", "da\u00df", "es", "so", "er\u00b7zit\u00b7tert", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "$(", "KOUS", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und sich in deinen H\u00e4nden r\u00fchrt/", "tokens": ["Und", "sich", "in", "dei\u00b7nen", "H\u00e4n\u00b7den", "r\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Die Brunst/ von der ein Hertze w\u00fctert", "tokens": ["Die", "Brunst", "/", "von", "der", "ein", "Hert\u00b7ze", "w\u00fc\u00b7tert"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "APPR", "PRELS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Das Liebesflammen in sich f\u00fchrt/", "tokens": ["Das", "Lie\u00b7bes\u00b7flam\u00b7men", "in", "sich", "f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Wird heller brennen/", "tokens": ["Wird", "hel\u00b7ler", "bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Bey dir mein Kind/", "tokens": ["Bey", "dir", "mein", "Kind", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Da\u00df man kan kennen/", "tokens": ["Da\u00df", "man", "kan", "ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VMFIN", "VVINF", "$("], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.17": {"text": "Wie wir entz\u00fcndt", "tokens": ["Wie", "wir", "ent\u00b7z\u00fcndt"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.18": {"text": "In Liebe sind.", "tokens": ["In", "Lie\u00b7be", "sind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "Verwundre nicht die grosse Hitze", "tokens": ["Ver\u00b7wund\u00b7re", "nicht", "die", "gros\u00b7se", "Hit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Die sich in meinem Hertzen regt.", "tokens": ["Die", "sich", "in", "mei\u00b7nem", "Hert\u00b7zen", "reg\u00b7t."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "NN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Empfind ich doch der Sch\u00f6nheit Blitze", "tokens": ["Emp\u00b7find", "ich", "doch", "der", "Sch\u00f6n\u00b7heit", "Blit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Wormit mich stets dein Auge schl\u00e4gt", "tokens": ["Wor\u00b7mit", "mich", "stets", "dein", "Au\u00b7ge", "schl\u00e4gt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Wilst du verdammen/", "tokens": ["Wilst", "du", "ver\u00b7dam\u00b7men", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.24": {"text": "Die linde Glut/", "tokens": ["Die", "lin\u00b7de", "Glut", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.25": {"text": "Da ich doch Flammen", "tokens": ["Da", "ich", "doch", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.26": {"text": "Nehr in dem Blut", "tokens": ["Nehr", "in", "dem", "Blut"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.27": {"text": "Gantz wohlgemuth.", "tokens": ["Gantz", "wohl\u00b7ge\u00b7muth", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "---+", "measure": "unknown.measure.single"}, "line.28": {"text": "Du sprichst/ der Schnee an meinen H\u00e4nden", "tokens": ["Du", "sprichst", "/", "der", "Schnee", "an", "mei\u00b7nen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Zerschmeltzt von diesem Hertzens-Brand.", "tokens": ["Zer\u00b7schmeltzt", "von", "die\u00b7sem", "Hert\u00b7zens\u00b7Brand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Er hat mir Adern/ Marck und Lenden/", "tokens": ["Er", "hat", "mir", "A\u00b7dern", "/", "Marck", "und", "Len\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Ja selbst das Leben umbgewandt.", "tokens": ["Ja", "selbst", "das", "Le\u00b7ben", "umb\u00b7ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Wie eine Kertze", "tokens": ["Wie", "ei\u00b7ne", "Kert\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.33": {"text": "Sich selbst verzehrt/", "tokens": ["Sich", "selbst", "ver\u00b7zehrt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVPP", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.34": {"text": "So ist mein Hertze", "tokens": ["So", "ist", "mein", "Hert\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.35": {"text": "In Staub und Erd", "tokens": ["In", "Staub", "und", "Erd"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.36": {"text": "Durch di\u00df gekehrt.", "tokens": ["Durch", "di\u00df", "ge\u00b7kehrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.37": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.38": {"text": "Da\u00df du solst jedem zeigen an/", "tokens": ["Da\u00df", "du", "solst", "je\u00b7dem", "zei\u00b7gen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "PIS", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Wie ich mein Hertze zu dem deinen", "tokens": ["Wie", "ich", "mein", "Hert\u00b7ze", "zu", "dem", "dei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPOSAT", "VVFIN", "APPR", "ART", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Hab aus verliebtem Sinn gethan.", "tokens": ["Hab", "aus", "ver\u00b7lieb\u00b7tem", "Sinn", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Da\u00df di\u00df Geschencke", "tokens": ["Da\u00df", "di\u00df", "Ge\u00b7schen\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PDS", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.42": {"text": "Man an das Ohr", "tokens": ["Man", "an", "das", "Ohr"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "APPR", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.43": {"text": "Gleich Perlen hencke/", "tokens": ["Gleich", "Per\u00b7len", "hen\u00b7cke", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.44": {"text": "Kommt/ wie ein Mor", "tokens": ["Kommt", "/", "wie", "ein", "Mor"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$(", "KOKOM", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.45": {"text": "Mir seltzam vor.", "tokens": ["Mir", "selt\u00b7zam", "vor", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.46": {"text": "La\u00df andre Diamanten haben/", "tokens": ["La\u00df", "and\u00b7re", "Di\u00b7a\u00b7man\u00b7ten", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Du tr\u00e4gst ein Kleinod das mehr wehrt.", "tokens": ["Du", "tr\u00e4gst", "ein", "Klei\u00b7nod", "das", "mehr", "wehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Gold/ Silber sind des Gl\u00fcckes Gaben.", "tokens": ["Gold", "/", "Sil\u00b7ber", "sind", "des", "Gl\u00fc\u00b7ckes", "Ga\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "Die Liebe wird nur mit beschwehrt.", "tokens": ["Die", "Lie\u00b7be", "wird", "nur", "mit", "be\u00b7schwehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Die Zeit zerreibet", "tokens": ["Die", "Zeit", "zer\u00b7rei\u00b7bet"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.51": {"text": "Der Perlen Zier/", "tokens": ["Der", "Per\u00b7len", "Zier", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.52": {"text": "Mein Hertze bleibet", "tokens": ["Mein", "Hert\u00b7ze", "blei\u00b7bet"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.53": {"text": "In Liebs-Begier", "tokens": ["In", "Liebs\u00b7Be\u00b7gier"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.54": {"text": "Verpflichtet dir.", "tokens": ["Ver\u00b7pflich\u00b7tet", "dir", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.55": {"text": "Gilt doch dein Mund mehr als Corallen/", "tokens": ["Gilt", "doch", "dein", "Mund", "mehr", "als", "Co\u00b7ral\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "PIAT", "KOKOM", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.56": {"text": "Die Lippen mehr als ein Nubin.", "tokens": ["Die", "Lip\u00b7pen", "mehr", "als", "ein", "Nu\u00b7bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "KOKOM", "ART", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.57": {"text": "Kein Demant kan mir so gefallen/", "tokens": ["Kein", "De\u00b7mant", "kan", "mir", "so", "ge\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.58": {"text": "Als deine Augen wenn sie bl\u00fchn.", "tokens": ["Als", "dei\u00b7ne", "Au\u00b7gen", "wenn", "sie", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "KOUS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.59": {"text": "Du bist mein Leben/", "tokens": ["Du", "bist", "mein", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.60": {"text": "Mein h\u00f6chstes Gut;", "tokens": ["Mein", "h\u00f6chs\u00b7tes", "Gut", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.61": {"text": "Der ich gegeben", "tokens": ["Der", "ich", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPER", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.62": {"text": "In treuer Hut/", "tokens": ["In", "treu\u00b7er", "Hut", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.63": {"text": "Geist/ Hertz und Blut.", "tokens": ["Geist", "/", "Hertz", "und", "Blut", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}