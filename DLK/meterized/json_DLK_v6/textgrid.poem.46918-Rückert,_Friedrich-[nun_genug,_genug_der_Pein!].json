{"textgrid.poem.46918": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[nun genug, genug der Pein!]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun genug, genug der Pein!", "tokens": ["Nun", "ge\u00b7nug", ",", "ge\u00b7nug", "der", "Pein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht nach meinen Schulden", "tokens": ["Nicht", "nach", "mei\u00b7nen", "Schul\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Richte, sondern richte fein", "tokens": ["Rich\u00b7te", ",", "son\u00b7dern", "rich\u00b7te", "fein"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "KON", "VVFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich nach deinen Hulden.", "tokens": ["Mich", "nach", "dei\u00b7nen", "Hul\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Da nicht weiter reicht die Kraft", "tokens": ["Da", "nicht", "wei\u00b7ter", "reicht", "die", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Deinen Schlag zu tragen,", "tokens": ["Dei\u00b7nen", "Schlag", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wirst in deiner Wissenschaft", "tokens": ["Wirst", "in", "dei\u00b7ner", "Wis\u00b7sen\u00b7schaft"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Du nicht weiter schlagen.", "tokens": ["Du", "nicht", "wei\u00b7ter", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}