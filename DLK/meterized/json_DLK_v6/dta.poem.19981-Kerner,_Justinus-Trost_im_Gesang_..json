{"dta.poem.19981": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Trost im Gesang .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Wandrer, dem verschwunden", "tokens": ["Der", "Wand\u00b7rer", ",", "dem", "ver\u00b7schwun\u00b7den"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So Sonn' als Mondenlicht,", "tokens": ["So", "Sonn'", "als", "Mon\u00b7den\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KOUS", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der singt ein Lied in's Dunkel", "tokens": ["Der", "singt", "ein", "Lied", "in's", "Dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und h\u00e4rmt sich l\u00e4nger nicht.", "tokens": ["Und", "h\u00e4rmt", "sich", "l\u00e4n\u00b7ger", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er schreitet muthig weiter", "tokens": ["Er", "schrei\u00b7tet", "mut\u00b7hig", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die menschenleere Bahn,", "tokens": ["Die", "men\u00b7schen\u00b7lee\u00b7re", "Bahn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Viel lichte Sangesbilder", "tokens": ["Viel", "lich\u00b7te", "San\u00b7ges\u00b7bil\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die gehen ihm voran.", "tokens": ["Die", "ge\u00b7hen", "ihm", "vo\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nacht ist's auch mir geworden,", "tokens": ["Nacht", "ist's", "auch", "mir", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "PPER", "VAPP", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Die Freunde stehen fern,", "tokens": ["Die", "Freun\u00b7de", "ste\u00b7hen", "fern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von meinem Himmel schwindet", "tokens": ["Von", "mei\u00b7nem", "Him\u00b7mel", "schwin\u00b7det"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der allerletzte Stern,", "tokens": ["Der", "al\u00b7ler\u00b7letz\u00b7te", "Stern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch geh' ich muthig weiter", "tokens": ["Doch", "geh'", "ich", "mut\u00b7hig", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die menschenleere Bahn,", "tokens": ["Die", "men\u00b7schen\u00b7lee\u00b7re", "Bahn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Noch gehen Sangesbilder", "tokens": ["Noch", "ge\u00b7hen", "San\u00b7ges\u00b7bil\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ja mir auch licht voran.", "tokens": ["Ja", "mir", "auch", "licht", "vo\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "ADV", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}