{"textgrid.poem.32837": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: So schweig doch, kleine Schw\u00e4tzerin!", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So schweig doch, kleine Schw\u00e4tzerin!", "tokens": ["So", "schweig", "doch", ",", "klei\u00b7ne", "Schw\u00e4t\u00b7ze\u00b7rin", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hast du noch stets etwas zu sagen?", "tokens": ["Hast", "du", "noch", "stets", "et\u00b7was", "zu", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und immer noch etwas zu fragen?", "tokens": ["Und", "im\u00b7mer", "noch", "et\u00b7was", "zu", "fra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die Zeit fliegt, wie ein Vogel hin.", "tokens": ["Die", "Zeit", "fliegt", ",", "wie", "ein", "Vo\u00b7gel", "hin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Bringt dir das Gestrige Genu\u00df?", "tokens": ["Bringt", "dir", "das", "Ge\u00b7stri\u00b7ge", "Ge\u00b7nu\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Kennst du das Gl\u00fcck vom n\u00e4chsten Morgen?", "tokens": ["Kennst", "du", "das", "Gl\u00fcck", "vom", "n\u00e4chs\u00b7ten", "Mor\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So la\u00df uns lieber daf\u00fcr sorgen,", "tokens": ["So", "la\u00df", "uns", "lie\u00b7ber", "da\u00b7f\u00fcr", "sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was man jetzt thun, nicht, sagen mu\u00df.", "tokens": ["Was", "man", "jetzt", "thun", ",", "nicht", ",", "sa\u00b7gen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADV", "VVINF", "$,", "PTKNEG", "$,", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Bedenk, wie mancher Augenblick", "tokens": ["Be\u00b7denk", ",", "wie", "man\u00b7cher", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist plaudernd, ungen\u00fctzt verschwunden,", "tokens": ["Ist", "plau\u00b7dernd", ",", "un\u00b7ge\u00b7n\u00fctzt", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und Augenblicke werden Stunden,", "tokens": ["Und", "Au\u00b7gen\u00b7bli\u00b7cke", "wer\u00b7den", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Jahre, Stunden voller Gl\u00fcck.", "tokens": ["Und", "Jah\u00b7re", ",", "Stun\u00b7den", "vol\u00b7ler", "Gl\u00fcck", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du machst, da\u00df ich selbst schwatzhaft bin \u2013 \u2013", "tokens": ["Du", "machst", ",", "da\u00df", "ich", "selbst", "schwatz\u00b7haft", "bin", "\u2013", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohlan, la\u00df uns mit hei\u00dfen K\u00fcssen", "tokens": ["Wo\u00b7hlan", ",", "la\u00df", "uns", "mit", "hei\u00b7\u00dfen", "K\u00fcs\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "VVIMP", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den plauderhaften Mund verschlie\u00dfen! \u2013 \u2013", "tokens": ["Den", "plau\u00b7der\u00b7haf\u00b7ten", "Mund", "ver\u00b7schlie\u00b7\u00dfen", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$.", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn wirds, du kleine Schw\u00e4tzerin?", "tokens": ["Wenn", "wirds", ",", "du", "klei\u00b7ne", "Schw\u00e4t\u00b7ze\u00b7rin", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}