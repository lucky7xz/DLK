{"dta.poem.5968": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Bau der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne, die umschr\u00e4nckt in einer gl\u00fchnden R\u00fcnde,", "tokens": ["Die", "Son\u00b7ne", ",", "die", "um\u00b7schr\u00e4nckt", "in", "ei\u00b7ner", "gl\u00fchn\u00b7den", "R\u00fcn\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zieht ihrer Klarheit helle Flammen", "tokens": ["Zieht", "ih\u00b7rer", "Klar\u00b7heit", "hel\u00b7le", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In einen Mittel-Punet zusammen.", "tokens": ["In", "ei\u00b7nen", "Mit\u00b7tel\u00b7Pu\u00b7net", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wer kan doch so zusammen dringen", "tokens": ["Wer", "kan", "doch", "so", "zu\u00b7sam\u00b7men", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ADV", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Solch einen regen Stoff, der sich empor zu schwingen", "tokens": ["Solch", "ei\u00b7nen", "re\u00b7gen", "Stoff", ",", "der", "sich", "em\u00b7por", "zu", "schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "ART", "ADJA", "NN", "$,", "PRELS", "PRF", "PTKVZ", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}