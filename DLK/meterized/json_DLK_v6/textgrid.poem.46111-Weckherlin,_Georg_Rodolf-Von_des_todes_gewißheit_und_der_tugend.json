{"textgrid.poem.46111": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Von des todes gewi\u00dfheit und der tugend", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man findet nichts vollkommen in der welt:", "tokens": ["Man", "fin\u00b7det", "nichts", "voll\u00b7kom\u00b7men", "in", "der", "welt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PIS", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "wir menschen seind mit sorgen, pein und plagen", "tokens": ["wir", "men\u00b7schen", "seind", "mit", "sor\u00b7gen", ",", "pein", "und", "pla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVINF", "VAFIN", "APPR", "NN", "$,", "NE", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "all ort und zeit, in st\u00e4ten, auf dem feld,", "tokens": ["all", "ort", "und", "zeit", ",", "in", "st\u00e4\u00b7ten", ",", "auf", "dem", "feld", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "$,", "APPR", "VVFIN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "von himmel, luft, mer und uns selbs geschlagen:", "tokens": ["von", "him\u00b7mel", ",", "luft", ",", "mer", "und", "uns", "selbs", "ge\u00b7schla\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVFIN", "$,", "ADV", "KON", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "ja auch der g\u00f6tter macht", "tokens": ["ja", "auch", "der", "g\u00f6t\u00b7ter", "macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "hat ihren sitz vollkommen", "tokens": ["hat", "ih\u00b7ren", "sitz", "voll\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "und selig nicht gemacht;", "tokens": ["und", "se\u00b7lig", "nicht", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "wer hat nicht wargenommen,", "tokens": ["wer", "hat", "nicht", "war\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "wie sonn und mon gemein", "tokens": ["wie", "sonn", "und", "mon", "ge\u00b7mein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "KON", "NE", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "verfinstern ihren schein,", "tokens": ["ver\u00b7fins\u00b7tern", "ih\u00b7ren", "schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "und wie des himmels zeichen", "tokens": ["und", "wie", "des", "him\u00b7mels", "zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "(oft mangelhaft) verbleichen?", "tokens": ["(", "oft", "man\u00b7gel\u00b7haft", ")", "ver\u00b7blei\u00b7chen", "?"], "token_info": ["punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "$(", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mit wie vil angst, gefahren, m\u00fch und not", "tokens": ["Mit", "wie", "vil", "angst", ",", "ge\u00b7fah\u00b7ren", ",", "m\u00fch", "und", "not"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "KOKOM", "ADV", "VVPP", "$,", "VVPP", "$,", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "seind ohn abla\u00df wir menschen umgegeben!", "tokens": ["seind", "ohn", "ab\u00b7la\u00df", "wir", "men\u00b7schen", "um\u00b7ge\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "VVFIN", "PPER", "NN", "VVPP", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "des einen list ist oft des andern tod,", "tokens": ["des", "ei\u00b7nen", "list", "ist", "oft", "des", "an\u00b7dern", "tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "des andern herz verkrieget selbs sein leben:", "tokens": ["des", "an\u00b7dern", "herz", "ver\u00b7krie\u00b7get", "selbs", "sein", "le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "der ein aus vil verdru\u00df", "tokens": ["der", "ein", "aus", "vil", "ver\u00b7dru\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "APPR", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "und trauren will verderben;", "tokens": ["und", "trau\u00b7ren", "will", "ver\u00b7der\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "der ander, elend, mu\u00df", "tokens": ["der", "an\u00b7der", ",", "e\u00b7lend", ",", "mu\u00df"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "ADJD", "$,", "ADJD", "$,", "VMFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "in der gef\u00e4ngnus sterben;", "tokens": ["in", "der", "ge\u00b7f\u00e4ng\u00b7nus", "ster\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "ein andrer die armut", "tokens": ["ein", "an\u00b7drer", "die", "ar\u00b7mut"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "verfluchet, suchet gut", "tokens": ["ver\u00b7flu\u00b7chet", ",", "su\u00b7chet", "gut"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "und seinen geiz versinket,", "tokens": ["und", "sei\u00b7nen", "geiz", "ver\u00b7sin\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "wan er im mer verdrinket.", "tokens": ["wan", "er", "im", "mer", "ver\u00b7drin\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "Der ein mit gift, schwert, wasser oder strick", "tokens": ["Der", "ein", "mit", "gift", ",", "schwert", ",", "was\u00b7ser", "o\u00b7der", "strick"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "ART", "APPR", "NN", "$,", "VVFIN", "$,", "PRELS", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "darf \u00fcber sich ein urteil selbs aussprechen,", "tokens": ["darf", "\u00fc\u00b7ber", "sich", "ein", "ur\u00b7teil", "selbs", "aus\u00b7spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PRF", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und rettend sich von druckendem ungl\u00fcck", "tokens": ["und", "ret\u00b7tend", "sich", "von", "dru\u00b7cken\u00b7dem", "un\u00b7gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PRF", "APPR", "PAV", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "vermeinet er sich wider sich zu r\u00e4chen:", "tokens": ["ver\u00b7mei\u00b7net", "er", "sich", "wi\u00b7der", "sich", "zu", "r\u00e4\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "vil kommen auch mit zwang", "tokens": ["vil", "kom\u00b7men", "auch", "mit", "zwang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "in dieses lebens leiden,", "tokens": ["in", "die\u00b7ses", "le\u00b7bens", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "und findend den ausgang", "tokens": ["und", "fin\u00b7dend", "den", "aus\u00b7gang"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "all andre m\u00fch vermeiden;", "tokens": ["all", "and\u00b7re", "m\u00fch", "ver\u00b7mei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PIS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "auch vil sich in ihr grab,", "tokens": ["auch", "vil", "sich", "in", "ihr", "grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "eh da\u00df sie eine gab", "tokens": ["eh", "da\u00df", "sie", "ei\u00b7ne", "gab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "ART", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "des tags und lichts genie\u00dfen,", "tokens": ["des", "tags", "und", "lichts", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "in mutterleib beschlie\u00dfen.", "tokens": ["in", "mut\u00b7ter\u00b7leib", "be\u00b7schlie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es klopfet ja der tod mit einem bein", "tokens": ["Es", "klop\u00b7fet", "ja", "der", "tod", "mit", "ei\u00b7nem", "bein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "an die pall\u00e4st und wolkenhohe schl\u00f6sser", "tokens": ["an", "die", "pal\u00b7l\u00e4st", "und", "wol\u00b7ken\u00b7ho\u00b7he", "schl\u00f6s\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "ADJA", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und armer leut sorglose h\u00fcttelein,", "tokens": ["und", "ar\u00b7mer", "leut", "sorg\u00b7lo\u00b7se", "h\u00fct\u00b7tel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und ist f\u00fcr beed nicht b\u00f6ser und nicht besser:", "tokens": ["und", "ist", "f\u00fcr", "beed", "nicht", "b\u00f6\u00b7ser", "und", "nicht", "bes\u00b7ser", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NE", "PTKNEG", "ADJD", "KON", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "den leib ein tod allein", "tokens": ["den", "leib", "ein", "tod", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "mit wunderbaren plagen,", "tokens": ["mit", "wun\u00b7der\u00b7ba\u00b7ren", "pla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "unmeidenlicher pein,", "tokens": ["un\u00b7mei\u00b7den\u00b7li\u00b7cher", "pein", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "undienstlich langen klagen", "tokens": ["un\u00b7di\u00b7enst\u00b7lich", "lan\u00b7gen", "kla\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "betr\u00fcbet tag und nacht,", "tokens": ["be\u00b7tr\u00fc\u00b7bet", "tag", "und", "nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "und die seel wird gebracht", "tokens": ["und", "die", "seel", "wird", "ge\u00b7bracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.11": {"text": "f\u00fcr Minos, der kein flehen", "tokens": ["f\u00fcr", "Mi\u00b7nos", ",", "der", "kein", "fle\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "PIAT", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.12": {"text": "mehr pfleget anzusehen.", "tokens": ["mehr", "pfle\u00b7get", "an\u00b7zu\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Breit ist der weg zu des todes finsterm haus,", "tokens": ["Breit", "ist", "der", "weg", "zu", "des", "to\u00b7des", "fins\u00b7term", "haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADV", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "ohn th\u00fcr das thor, da man stets hinein gehet,", "tokens": ["ohn", "th\u00fcr", "das", "thor", ",", "da", "man", "stets", "hin\u00b7ein", "ge\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,", "KOUS", "PIS", "ADV", "APZR", "VVFIN", "$,"], "meter": "-+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "sich aber (wert) zu ziehen noch daraus,", "tokens": ["sich", "a\u00b7ber", "(", "wert", ")", "zu", "zie\u00b7hen", "noch", "da\u00b7raus", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "$(", "ADJD", "$(", "PTKZU", "VVINF", "KON", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "hierauf die m\u00fch, hierauf das werk bestehet.", "tokens": ["hier\u00b7auf", "die", "m\u00fch", ",", "hier\u00b7auf", "das", "werk", "be\u00b7ste\u00b7het", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJD", "$,", "PAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "der tugend weg ist schmal,", "tokens": ["der", "tu\u00b7gend", "weg", "ist", "schmal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "mit dornen wol verschlossen,", "tokens": ["mit", "dor\u00b7nen", "wol", "ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "gering auch deren zahl,", "tokens": ["ge\u00b7ring", "auch", "de\u00b7ren", "zahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PRELAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "die mutig, unverdrossen", "tokens": ["die", "mu\u00b7tig", ",", "un\u00b7ver\u00b7dros\u00b7sen"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "ADJD", "$,", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "sich durch der g\u00f6tter gunst", "tokens": ["sich", "durch", "der", "g\u00f6t\u00b7ter", "gunst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "und durch der tugend kunst,", "tokens": ["und", "durch", "der", "