{"textgrid.poem.34192": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Dem Propheten zur Antwort", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wei\u00dft du, wie ich mich sch\u00fctze? Ich habe selber vom Himmel", "tokens": ["Wei\u00dft", "du", ",", "wie", "ich", "mich", "sch\u00fct\u00b7ze", "?", "Ich", "ha\u00b7be", "sel\u00b7ber", "vom", "Him\u00b7mel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "APPRART", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Reichlich empfangen und ganz ohne mein eig'nes Verdienst,", "tokens": ["Reich\u00b7lich", "emp\u00b7fan\u00b7gen", "und", "ganz", "oh\u00b7ne", "mein", "eig'\u00b7nes", "Ver\u00b7dienst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "KON", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Darum gebe ich nie in meinem Namen, ich gebe", "tokens": ["Da\u00b7rum", "ge\u00b7be", "ich", "nie", "in", "mei\u00b7nem", "Na\u00b7men", ",", "ich", "ge\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Immer im Namen des Herrn, wie es dem Menschen geb\u00fchrt,", "tokens": ["Im\u00b7mer", "im", "Na\u00b7men", "des", "Herrn", ",", "wie", "es", "dem", "Men\u00b7schen", "ge\u00b7b\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$,", "PWAV", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Aber, wer selbst nur giebt, um Gott die Schuld zu bezahlen,", "tokens": ["A\u00b7ber", ",", "wer", "selbst", "nur", "giebt", ",", "um", "Gott", "die", "Schuld", "zu", "be\u00b7zah\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWS", "ADV", "ADV", "VVFIN", "$,", "KOUI", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Fragt nicht, ob man ihm dankt oder ihn schm\u00e4hlich betr\u00fcgt!", "tokens": ["Fragt", "nicht", ",", "ob", "man", "ihm", "dankt", "o\u00b7der", "ihn", "schm\u00e4h\u00b7lich", "be\u00b7tr\u00fcgt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "KOUS", "PIS", "PPER", "VVFIN", "KON", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}}}}