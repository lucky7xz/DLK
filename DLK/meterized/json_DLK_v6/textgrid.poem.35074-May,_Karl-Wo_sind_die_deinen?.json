{"textgrid.poem.35074": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Wo sind die deinen?", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Siehst du dort an des Abgrunds Rand", "tokens": ["Siehst", "du", "dort", "an", "des", "Ab\u00b7grunds", "Rand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Schaar der Kinder sorglos schreiten?", "tokens": ["Die", "Schaar", "der", "Kin\u00b7der", "sorg\u00b7los", "schrei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kaum noch die Breite einer Hand,", "tokens": ["Kaum", "noch", "die", "Brei\u00b7te", "ei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So ist der Sturz nicht zu vermeiden.", "tokens": ["So", "ist", "der", "Sturz", "nicht", "zu", "ver\u00b7mei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zu ihren F\u00fc\u00dfen g\u00e4hnt der Tod;", "tokens": ["Zu", "ih\u00b7ren", "F\u00fc\u00b7\u00dfen", "g\u00e4hnt", "der", "Tod", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vor Angst will dir das Herz erkalten,", "tokens": ["Vor", "Angst", "will", "dir", "das", "Herz", "er\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Doch ob er grinst, und ob er droht,", "tokens": ["Doch", "ob", "er", "grinst", ",", "und", "ob", "er", "droht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "KON", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sie werden unsichtbar gehalten.", "tokens": ["Sie", "wer\u00b7den", "un\u00b7sicht\u00b7bar", "ge\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nun steig einmal, du stolzer Mann,", "tokens": ["Nun", "steig", "ein\u00b7mal", ",", "du", "stol\u00b7zer", "Mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hinauf, desselben Wegs zu gehen!", "tokens": ["Hin\u00b7auf", ",", "des\u00b7sel\u00b7ben", "Wegs", "zu", "ge\u00b7hen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PRELAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Warum schaust du mich fragend an?", "tokens": ["Wa\u00b7rum", "schaust", "du", "mich", "fra\u00b7gend", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Warum bleibst du so zagend stehen?", "tokens": ["Wa\u00b7rum", "bleibst", "du", "so", "za\u00b7gend", "ste\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du fragst und zagst ja nie und nicht", "tokens": ["Du", "fragst", "und", "zagst", "ja", "nie", "und", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "ADV", "KON", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf deinen steilen Zweiflerspfaden;", "tokens": ["Auf", "dei\u00b7nen", "stei\u00b7len", "Zweif\u00b7ler\u00b7spfa\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wie kommts, da\u00df jetzt der Muth dir bricht?", "tokens": ["Wie", "kommts", ",", "da\u00df", "jetzt", "der", "Muth", "dir", "bricht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "$,", "KOUS", "ADV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich glaub, ich habe es errathen.", "tokens": ["Ich", "glaub", ",", "ich", "ha\u00b7be", "es", "er\u00b7ra\u00b7then", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du wanderst k\u00fchn von Trug zu Trug", "tokens": ["Du", "wan\u00b7derst", "k\u00fchn", "von", "Trug", "zu", "Trug"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Abgrund deiner geistgen Oede,", "tokens": ["Am", "Ab\u00b7grund", "dei\u00b7ner", "geist\u00b7gen", "O\u00b7e\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Doch hier vor diesem Bergeszug,", "tokens": ["Doch", "hier", "vor", "die\u00b7sem", "Ber\u00b7ges\u00b7zug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da schwindelt dir, da wirst du bl\u00f6de.", "tokens": ["Da", "schwin\u00b7delt", "dir", ",", "da", "wirst", "du", "bl\u00f6\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "VAFIN", "PPER", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nun schau hinauf zum Felsenjoch,", "tokens": ["Nun", "schau", "hin\u00b7auf", "zum", "Fel\u00b7sen\u00b7joch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und sieh den sichern Gang der Kleinen:", "tokens": ["Und", "sieh", "den", "si\u00b7chern", "Gang", "der", "Klei\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sie haben ihre Engel noch;", "tokens": ["Sie", "ha\u00b7ben", "ih\u00b7re", "En\u00b7gel", "noch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Du aber sag, wo sind die deinen?", "tokens": ["Du", "a\u00b7ber", "sag", ",", "wo", "sind", "die", "dei\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "PWAV", "VAFIN", "ART", "PPOSAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}