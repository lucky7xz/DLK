{"textgrid.poem.56392": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "13", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zornig ward Fraya,", "tokens": ["Zor\u00b7nig", "ward", "Fray\u00b7a", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NE", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sie zitterte heftig,", "tokens": ["Sie", "zit\u00b7ter\u00b7te", "hef\u00b7tig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Der ganze Palast", "tokens": ["Der", "gan\u00b7ze", "Pa\u00b7last"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Der G\u00f6tter erbebte,", "tokens": ["Der", "G\u00f6t\u00b7ter", "er\u00b7beb\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Es sprang und entfiel ihr", "tokens": ["Es", "sprang", "und", "ent\u00b7fiel", "ihr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Der funkelnde Halsschmuck:", "tokens": ["Der", "fun\u00b7keln\u00b7de", "Hals\u00b7schmuck", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "\u00bbwohl m\u00f6chtest du meinen,", "tokens": ["\u00bb", "wohl", "m\u00f6ch\u00b7test", "du", "mei\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Da\u00df m\u00e4nnlich ich sei,", "tokens": ["Da\u00df", "m\u00e4nn\u00b7lich", "ich", "sei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "VAFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Wenn beide wir reisten", "tokens": ["Wenn", "bei\u00b7de", "wir", "reis\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Nach Riesenheim.\u00ab", "tokens": ["Nach", "Rie\u00b7sen\u00b7heim", ".", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["APPR", "NE", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}}}}