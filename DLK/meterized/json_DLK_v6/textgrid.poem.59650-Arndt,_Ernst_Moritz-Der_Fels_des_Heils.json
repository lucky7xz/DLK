{"textgrid.poem.59650": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Der Fels des Heils", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df, woran ich glaube,", "tokens": ["Ich", "wei\u00df", ",", "wo\u00b7ran", "ich", "glau\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich wei\u00df, was fest besteht,", "tokens": ["Ich", "wei\u00df", ",", "was", "fest", "be\u00b7steht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wann alles hier im Staube", "tokens": ["Wann", "al\u00b7les", "hier", "im", "Stau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie Sand und Staub verweht;", "tokens": ["Wie", "Sand", "und", "Staub", "ver\u00b7weht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich wei\u00df, was ewig bleibet,", "tokens": ["Ich", "wei\u00df", ",", "was", "e\u00b7wig", "blei\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wo alles wankt und f\u00e4llt,", "tokens": ["Wo", "al\u00b7les", "wankt", "und", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wo Wahn die Weisen treibet", "tokens": ["Wo", "Wahn", "die", "Wei\u00b7sen", "trei\u00b7bet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und Trug die Klugen prellt.", "tokens": ["Und", "Trug", "die", "Klu\u00b7gen", "prellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich wei\u00df, was ewig dauert,", "tokens": ["Ich", "wei\u00df", ",", "was", "e\u00b7wig", "dau\u00b7ert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich wei\u00df, was nimmer l\u00e4\u00dft,", "tokens": ["Ich", "wei\u00df", ",", "was", "nim\u00b7mer", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Diamanten mauert", "tokens": ["Mit", "Di\u00b7a\u00b7man\u00b7ten", "mau\u00b7ert"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir's Gott im Herzen fest.", "tokens": ["Mir's", "Gott", "im", "Her\u00b7zen", "fest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ja, recht mit Edelsteinen", "tokens": ["Ja", ",", "recht", "mit", "E\u00b7del\u00b7stei\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von allerbester Art", "tokens": ["Von", "al\u00b7ler\u00b7bes\u00b7ter", "Art"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hat Gott der Herr den Seinen", "tokens": ["Hat", "Gott", "der", "Herr", "den", "Sei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des Herzens Burg verwahrt.", "tokens": ["Des", "Her\u00b7zens", "Burg", "ver\u00b7wahrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich kenne wohl die Steine,", "tokens": ["Ich", "ken\u00b7ne", "wohl", "die", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die stolze Herzenswehr,", "tokens": ["Die", "stol\u00b7ze", "Her\u00b7zens\u00b7wehr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie funkeln ja mit Scheine", "tokens": ["Sie", "fun\u00b7keln", "ja", "mit", "Schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie Sterne sch\u00f6n und hehr:", "tokens": ["Wie", "Ster\u00b7ne", "sch\u00f6n", "und", "hehr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Steine sind die Worte,", "tokens": ["Die", "Stei\u00b7ne", "sind", "die", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Worte hell und rein,", "tokens": ["Die", "Wor\u00b7te", "hell", "und", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wodurch die schw\u00e4chsten Orte", "tokens": ["Wo\u00b7durch", "die", "schw\u00e4chs\u00b7ten", "Or\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gar feste k\u00f6nnen sein.", "tokens": ["Gar", "fes\u00b7te", "k\u00f6n\u00b7nen", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "VMFIN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Auch kenn' ich wohl den Meister,", "tokens": ["Auch", "kenn'", "ich", "wohl", "den", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der mir die Feste baut,", "tokens": ["Der", "mir", "die", "Fes\u00b7te", "baut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er hei\u00dft der F\u00fcrst der Geister,", "tokens": ["Er", "hei\u00dft", "der", "F\u00fcrst", "der", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf den der Himmel schaut,", "tokens": ["Auf", "den", "der", "Him\u00b7mel", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vor dem die Seraphinen", "tokens": ["Vor", "dem", "die", "Se\u00b7ra\u00b7phi\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Anbetend niederknien,", "tokens": ["An\u00b7be\u00b7tend", "nie\u00b7der\u00b7kni\u00b7en", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Um den die Engel dienen:", "tokens": ["Um", "den", "die", "En\u00b7gel", "die\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ich wei\u00df und kenne ihn.", "tokens": ["Ich", "wei\u00df", "und", "ken\u00b7ne", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Das ist das Licht der H\u00f6he,", "tokens": ["Das", "ist", "das", "Licht", "der", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das ist der Jesus Christ,", "tokens": ["Das", "ist", "der", "Je\u00b7sus", "Christ", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NE", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Fels, auf dem ich stehe,", "tokens": ["Der", "Fels", ",", "auf", "dem", "ich", "ste\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der diamanten ist,", "tokens": ["Der", "di\u00b7a\u00b7man\u00b7ten", "ist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der nimmermehr kann wanken,", "tokens": ["Der", "nim\u00b7mer\u00b7mehr", "kann", "wan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Heiland und der Hort,", "tokens": ["Der", "Hei\u00b7land", "und", "der", "Hort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Leuchte der Gedanken,", "tokens": ["Die", "Leuch\u00b7te", "der", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die leuchten hier und dort.", "tokens": ["Die", "leuch\u00b7ten", "hier", "und", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "So wei\u00df ich, was ich glaube,", "tokens": ["So", "wei\u00df", "ich", ",", "was", "ich", "glau\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich wei\u00df, was fest besteht", "tokens": ["Ich", "wei\u00df", ",", "was", "fest", "be\u00b7steht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ADJD", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und in dem Erdenstaube", "tokens": ["Und", "in", "dem", "Er\u00b7dens\u00b7tau\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht mit als Staub verweht;", "tokens": ["Nicht", "mit", "als", "Staub", "ver\u00b7weht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich wei\u00df, was in dem Grauen", "tokens": ["Ich", "wei\u00df", ",", "was", "in", "dem", "Grau\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Des Todes ewig bleibt", "tokens": ["Des", "To\u00b7des", "e\u00b7wig", "bleibt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und selbst auf Erdenauen", "tokens": ["Und", "selbst", "auf", "Er\u00b7de\u00b7nau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Schon Himmelsblumen treibt.", "tokens": ["Schon", "Him\u00b7mels\u00b7blu\u00b7men", "treibt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}