{"textgrid.poem.55441": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "21", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem Scheidenden ist jede Gabe wert,", "tokens": ["Dem", "Schei\u00b7den\u00b7den", "ist", "je\u00b7de", "Ga\u00b7be", "wert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein d\u00fcrres Blatt, ein Moos, ein Steinchen aus der Quelle,", "tokens": ["Ein", "d\u00fcr\u00b7res", "Blatt", ",", "ein", "Moos", ",", "ein", "Stein\u00b7chen", "aus", "der", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df er des Freunds gedenke, jener Stelle,", "tokens": ["Da\u00df", "er", "des", "Freunds", "ge\u00b7den\u00b7ke", ",", "je\u00b7ner", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wohin er ewig hin und hin begehrt:", "tokens": ["Wo\u00b7hin", "er", "e\u00b7wig", "hin", "und", "hin", "be\u00b7gehrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "PTKVZ", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Zeuge bleibt's, wie sinnig sie gewandelt.", "tokens": ["Ein", "Zeu\u00b7ge", "bleibt's", ",", "wie", "sin\u00b7nig", "sie", "ge\u00b7wan\u00b7delt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So wird ein Nichts zum h\u00f6chsten Schatz verwandelt.", "tokens": ["So", "wird", "ein", "Nichts", "zum", "h\u00f6chs\u00b7ten", "Schatz", "ver\u00b7wan\u00b7delt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "PIS", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn aber solche Gabe tiefen Wert,", "tokens": ["Wenn", "a\u00b7ber", "sol\u00b7che", "Ga\u00b7be", "tie\u00b7fen", "Wert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gestaltet, mit sich f\u00fchrt, f\u00fcr sich allein", "tokens": ["Ge\u00b7stal\u00b7tet", ",", "mit", "sich", "f\u00fchrt", ",", "f\u00fcr", "sich", "al\u00b7lein"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "APPR", "PRF", "VVFIN", "$,", "APPR", "PRF", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dem Sinn des K\u00fcnstlers w\u00fcnschenswert begegnete,", "tokens": ["Dem", "Sinn", "des", "K\u00fcnst\u00b7lers", "w\u00fcn\u00b7schens\u00b7wert", "be\u00b7ge\u00b7gne\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wie mu\u00df das nun ein Schatz der Sch\u00e4tze sein,", "tokens": ["Wie", "mu\u00df", "das", "nun", "ein", "Schatz", "der", "Sch\u00e4t\u00b7ze", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PDS", "ADV", "ART", "NN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenn ihn der Freund im Scheiden treulich segnete!", "tokens": ["Wenn", "ihn", "der", "Freund", "im", "Schei\u00b7den", "treu\u00b7lich", "seg\u00b7ne\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}