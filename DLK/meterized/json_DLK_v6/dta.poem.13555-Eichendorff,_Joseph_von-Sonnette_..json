{"dta.poem.13555": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnette .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht Tr\u00e4ume sind's und leere Wahn-Gesichte,", "tokens": ["Nicht", "Tr\u00e4u\u00b7me", "sin\u00b7d's", "und", "lee\u00b7re", "Wahn\u00b7Ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "NE", "KON", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Was von dem Volk' den Dichter unterscheidet.", "tokens": ["Was", "von", "dem", "Vol\u00b7k'", "den", "Dich\u00b7ter", "un\u00b7ter\u00b7schei\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Was er inbr\u00fcnstig bildet, liebt und leidet,", "tokens": ["Was", "er", "in\u00b7br\u00fcns\u00b7tig", "bil\u00b7det", ",", "liebt", "und", "lei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es ist des Lebens wahrhafte Geschichte.", "tokens": ["Es", "ist", "des", "Le\u00b7bens", "wahr\u00b7haf\u00b7te", "Ge\u00b7schich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}}}}