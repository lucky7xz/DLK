{"textgrid.poem.30759": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich kenn ein Rehe,", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kenn ein Rehe,", "tokens": ["Ich", "kenn", "ein", "Re\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Dem frey thut wehe,", "tokens": ["Dem", "frey", "thut", "we\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Das nach dem fangen", "tokens": ["Das", "nach", "dem", "fan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Tr\u00e4gt ein Verlangen,", "tokens": ["Tr\u00e4gt", "ein", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Das ungejaget", "tokens": ["Das", "un\u00b7ge\u00b7ja\u00b7get"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Ins Garn sich waget,", "tokens": ["Ins", "Garn", "sich", "wa\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Das ihm ein Netze", "tokens": ["Das", "ihm", "ein", "Net\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "F\u00fcr Zierath sch\u00e4tze,", "tokens": ["F\u00fcr", "Zie\u00b7rath", "sch\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Das seinen J\u00e4ger", "tokens": ["Das", "sei\u00b7nen", "J\u00e4\u00b7ger"], "token_info": ["word", "word", "word"], "pos": ["PDS", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Nimmt in sein L\u00e4ger.", "tokens": ["Nimmt", "in", "sein", "L\u00e4\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Ihr Jungfern, stille,", "tokens": ["Ihr", "Jung\u00b7fern", ",", "stil\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Was euer Wille!", "tokens": ["Was", "eu\u00b7er", "Wil\u00b7le", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}