{"dta.poem.17742": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "364.  Wer in dem Wirken ruht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Der Weise welcher sich hat \u00fcbersich gebracht/", "tokens": ["Der", "Wei\u00b7se", "wel\u00b7cher", "sich", "hat", "\u00fc\u00b7ber\u00b7sich", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRELS", "PRF", "VAFIN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der ruhet wenn er laufft und wirkt wenn er betracht.", "tokens": ["Der", "ru\u00b7het", "wenn", "er", "laufft", "und", "wirkt", "wenn", "er", "be\u00b7tracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KOUS", "PPER", "VVFIN", "KON", "VVFIN", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}