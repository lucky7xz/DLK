{"textgrid.poem.35873": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "Der s\u00fc\u00dfesten Mutter", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du tr\u00e4gst auf Mutterarmen", "tokens": ["Du", "tr\u00e4gst", "auf", "Mut\u00b7ter\u00b7ar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den gro\u00dfen Wunderheld,", "tokens": ["Den", "gro\u00b7\u00dfen", "Wun\u00b7der\u00b7held", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Gnade, das Erbarmen,", "tokens": ["Die", "Gna\u00b7de", ",", "das", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Trost der ganzen Welt.", "tokens": ["Den", "Trost", "der", "gan\u00b7zen", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O Jungfrau! la\u00df uns sehen,", "tokens": ["O", "Jung\u00b7frau", "!", "la\u00df", "uns", "se\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVIMP", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was Gott durch Dich uns giebt,", "tokens": ["Was", "Gott", "durch", "Dich", "uns", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "APPR", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Damit wir ganz verstehen,", "tokens": ["Da\u00b7mit", "wir", "ganz", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie sehr Er uns geliebt!", "tokens": ["Wie", "sehr", "Er", "uns", "ge\u00b7liebt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Lehr' uns vor Ihm nur wallen,", "tokens": ["Lehr'", "uns", "vor", "Ihm", "nur", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf Ihn allein nur sehn,", "tokens": ["Auf", "Ihn", "al\u00b7lein", "nur", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis Er mit Wohlgefallen", "tokens": ["Bis", "Er", "mit", "Wohl\u00b7ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "H\u00f6rt Seiner Kinder Flehn.", "tokens": ["H\u00f6rt", "Sei\u00b7ner", "Kin\u00b7der", "Flehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Lehr' uns wie Du Ihn lieben", "tokens": ["Lehr'", "uns", "wie", "Du", "Ihn", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "KOKOM", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Freude und in Schmerz,", "tokens": ["In", "Freu\u00b7de", "und", "in", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und lege uns dort dr\u00fcben", "tokens": ["Und", "le\u00b7ge", "uns", "dort", "dr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Einst an Sein Bruderherz.", "tokens": ["Einst", "an", "Sein", "Bru\u00b7der\u00b7herz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}