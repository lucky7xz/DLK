{"textgrid.poem.33246": {"metadata": {"author": {"name": "Poe, Edgar Allan", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich sah dich einmal \u2013 einmal nur \u2013 vor Jahren:", "genre": "verse", "period": "N.A.", "pub_year": 1829, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah dich einmal \u2013 einmal nur \u2013 vor Jahren:", "tokens": ["Ich", "sah", "dich", "ein\u00b7mal", "\u2013", "ein\u00b7mal", "nur", "\u2013", "vor", "Jah\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$(", "ADV", "ADV", "$(", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich sage nicht ", "tokens": ["Ich", "sa\u00b7ge", "nicht"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Es war in Julinacht, und aus dem vollen", "tokens": ["Es", "war", "in", "Ju\u00b7li\u00b7nacht", ",", "und", "aus", "dem", "vol\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "$,", "KON", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kreisrunden Mond, der gleich wie deine Seele", "tokens": ["Kreis\u00b7run\u00b7den", "Mond", ",", "der", "gleich", "wie", "dei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "ADV", "KOKOM", "PPOSAT", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Den steilsten Weg hinauf zum Himmel suchte,", "tokens": ["Den", "steils\u00b7ten", "Weg", "hin\u00b7auf", "zum", "Him\u00b7mel", "such\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Fiel sanft ein silberseidner Schleier Licht \u2013", "tokens": ["Fiel", "sanft", "ein", "sil\u00b7ber\u00b7seid\u00b7ner", "Schlei\u00b7er", "Licht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Fiel still und schw\u00fcl und schlummerselig nieder", "tokens": ["Fiel", "still", "und", "schw\u00fcl", "und", "schlum\u00b7mer\u00b7se\u00b7lig", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "KON", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Auf tausend Rosen, die nach oben schauten", "tokens": ["Auf", "tau\u00b7send", "Ro\u00b7sen", ",", "die", "nach", "o\u00b7ben", "schau\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "$,", "PRELS", "APPR", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und die in einem Zaubergarten wuchsen,", "tokens": ["Und", "die", "in", "ei\u00b7nem", "Zau\u00b7berg\u00b7ar\u00b7ten", "wuch\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wo Wind auf Zehen nur sich r\u00fchren durfte \u2013", "tokens": ["Wo", "Wind", "auf", "Ze\u00b7hen", "nur", "sich", "r\u00fch\u00b7ren", "durf\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "CARD", "ADV", "PRF", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Auf Rosen fiel er, die nach oben schauten,", "tokens": ["Auf", "Ro\u00b7sen", "fiel", "er", ",", "die", "nach", "o\u00b7ben", "schau\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$,", "PRELS", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Die ihre Seelen in verz\u00fccktem Sterben", "tokens": ["Die", "ih\u00b7re", "See\u00b7len", "in", "ver\u00b7z\u00fcck\u00b7tem", "Ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Als Duft aushauchten in das Liebe-Licht \u2013", "tokens": ["Als", "Duft", "aus\u00b7hauch\u00b7ten", "in", "das", "Lie\u00b7be\u00b7Licht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Auf Rosen fiel er, die nach oben schauten,", "tokens": ["Auf", "Ro\u00b7sen", "fiel", "er", ",", "die", "nach", "o\u00b7ben", "schau\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$,", "PRELS", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Die l\u00e4chelten und starben, wie verzaubert", "tokens": ["Die", "l\u00e4\u00b7chel\u00b7ten", "und", "star\u00b7ben", ",", "wie", "ver\u00b7zau\u00b7bert"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "KON", "VVFIN", "$,", "PWAV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Von dir und deines Wesens Poesie.", "tokens": ["Von", "dir", "und", "dei\u00b7nes", "We\u00b7sens", "Poe\u00b7sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich sah dich ganz in Wei\u00df, auf Veilchenbeet;", "tokens": ["Ich", "sah", "dich", "ganz", "in", "Wei\u00df", ",", "auf", "Veil\u00b7chen\u00b7beet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NE", "$,", "APPR", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf offne Rosen, die nach oben schauten,", "tokens": ["Auf", "off\u00b7ne", "Ro\u00b7sen", ",", "die", "nach", "o\u00b7ben", "schau\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Fiel hell der Mond \u2013 und auch auf dein Gesicht,", "tokens": ["Fiel", "hell", "der", "Mond", "\u2013", "und", "auch", "auf", "dein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$(", "KON", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das aufw\u00e4rts schaute \u2013 schaute, ach, in Leid.", "tokens": ["Das", "auf\u00b7w\u00e4rts", "schau\u00b7te", "\u2013", "schau\u00b7te", ",", "ach", ",", "in", "Leid", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "$(", "VVFIN", "$,", "ITJ", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "War das nicht Schicksal, das in dieser Nacht \u2013", "tokens": ["War", "das", "nicht", "Schick\u00b7sal", ",", "das", "in", "die\u00b7ser", "Nacht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "NN", "$,", "PRELS", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "War das nicht Schicksal (das auch Leiden hei\u00dft),", "tokens": ["War", "das", "nicht", "Schick\u00b7sal", "(", "das", "auch", "Lei\u00b7den", "hei\u00dft", ")", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "NN", "$(", "PDS", "ADV", "NN", "VVFIN", "$(", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das mir vorm Gartentore Halt gebot,", "tokens": ["Das", "mir", "vorm", "Gar\u00b7ten\u00b7to\u00b7re", "Halt", "ge\u00b7bot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPRART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den Schlummerduft der Rosen einzuatmen?", "tokens": ["Den", "Schlum\u00b7mer\u00b7duft", "der", "Ro\u00b7sen", "ein\u00b7zu\u00b7at\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Kein Schritt: in Schlaf lag die verha\u00dfte Welt;", "tokens": ["Kein", "Schritt", ":", "in", "Schlaf", "lag", "die", "ver\u00b7ha\u00df\u00b7te", "Welt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nur du und ich \u2013 (o Gott, wie schl\u00e4gt mein Herz,", "tokens": ["Nur", "du", "und", "ich", "\u2013", "(", "o", "Gott", ",", "wie", "schl\u00e4gt", "mein", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "KON", "PPER", "$(", "$(", "FM", "NN", "$,", "PWAV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da ich zusammen die zwei Worte nenne!) \u2013", "tokens": ["Da", "ich", "zu\u00b7sam\u00b7men", "die", "zwei", "Wor\u00b7te", "nen\u00b7ne", "!", ")", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "CARD", "NN", "VVFIN", "$.", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nur wachend du und ich. Ich stand, ich blickte \u2013", "tokens": ["Nur", "wa\u00b7chend", "du", "und", "ich", ".", "Ich", "stand", ",", "ich", "blick\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "KON", "PPER", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und pl\u00f6tzlich loschen alle Dinge aus.", "tokens": ["Und", "pl\u00f6tz\u00b7lich", "lo\u00b7schen", "al\u00b7le", "Din\u00b7ge", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "(bedenkt es wohl, es war ein Zaubergarten!)", "tokens": ["(", "be\u00b7denkt", "es", "wohl", ",", "es", "war", "ein", "Zau\u00b7berg\u00b7ar\u00b7ten", "!", ")"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Der Perlenglanz des Monds erlosch, die Beete,", "tokens": ["Der", "Per\u00b7len\u00b7glanz", "des", "Monds", "er\u00b7losch", ",", "die", "Bee\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Die moosigen Beete und gewundnen Pfade,", "tokens": ["Die", "moo\u00b7si\u00b7gen", "Bee\u00b7te", "und", "ge\u00b7wund\u00b7nen", "Pfa\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Die frohen Blumen, s\u00e4ftevollen B\u00e4ume \u2013", "tokens": ["Die", "fro\u00b7hen", "Blu\u00b7men", ",", "s\u00e4f\u00b7te\u00b7vol\u00b7len", "B\u00e4u\u00b7me", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Nichts sah man mehr; und selbst der Duft den Rosen", "tokens": ["Nichts", "sah", "man", "mehr", ";", "und", "selbst", "der", "Duft", "den", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PIS", "ADV", "$.", "KON", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Erstarb im Arm anbetend stiller L\u00fcfte.", "tokens": ["Er\u00b7starb", "im", "Arm", "an\u00b7be\u00b7tend", "stil\u00b7ler", "L\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "VVPP", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "All alles au\u00dfer dir verschied, verhauchte,", "tokens": ["All", "al\u00b7les", "au\u00b7\u00dfer", "dir", "ver\u00b7schied", ",", "ver\u00b7hauch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "PIS", "APPR", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Nichts blieb als du \u2013 als weniger denn du:", "tokens": ["Nichts", "blieb", "als", "du", "\u2013", "als", "we\u00b7ni\u00b7ger", "denn", "du", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KOUS", "PPER", "$(", "KOKOM", "ADV", "KON", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Als nur das Himmelslicht in deinen Augen \u2013", "tokens": ["Als", "nur", "das", "Him\u00b7mels\u00b7licht", "in", "dei\u00b7nen", "Au\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Als deine Seele nur in deinen Augen.", "tokens": ["Als", "dei\u00b7ne", "See\u00b7le", "nur", "in", "dei\u00b7nen", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Ich sah nur sie \u2013 sie waren mir die Welt.", "tokens": ["Ich", "sah", "nur", "sie", "\u2013", "sie", "wa\u00b7ren", "mir", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "$(", "PPER", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Ich sah nur sie \u2013 sah stundenlang nur sie \u2013", "tokens": ["Ich", "sah", "nur", "sie", "\u2013", "sah", "stun\u00b7den\u00b7lang", "nur", "sie", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "$(", "VVFIN", "ADJD", "ADV", "PPER", "$("], "meter": "-+-+++-+-+", "measure": "zehnsilber"}, "line.22": {"text": "Sah nichts als sie, bis da\u00df der Mond sich senkte.", "tokens": ["Sah", "nichts", "als", "sie", ",", "bis", "da\u00df", "der", "Mond", "sich", "senk\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "KOKOM", "PPER", "$,", "KON", "KOUS", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.23": {"text": "Welch wundersame Herzgeschichten sprachen", "tokens": ["Welch", "wun\u00b7der\u00b7sa\u00b7me", "Herz\u00b7ge\u00b7schich\u00b7ten", "spra\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Aus jenen himmlischen kristallnen Kugeln!", "tokens": ["Aus", "je\u00b7nen", "himm\u00b7li\u00b7schen", "kris\u00b7tall\u00b7nen", "Ku\u00b7geln", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Welch dunkles Weh! Und doch welch hehres Hoffen!", "tokens": ["Welch", "dunk\u00b7les", "Weh", "!", "Und", "doch", "welch", "heh\u00b7res", "Hof\u00b7fen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$.", "KON", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Welch heiter schweigend Meer erhabnen Stolzes!", "tokens": ["Welch", "hei\u00b7ter", "schwei\u00b7gend", "Meer", "er\u00b7hab\u00b7nen", "Stol\u00b7zes", "!"], "token_info": ["word", "word", "word", "word", "word",