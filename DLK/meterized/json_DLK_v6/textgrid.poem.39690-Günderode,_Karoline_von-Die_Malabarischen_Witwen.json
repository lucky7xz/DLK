{"textgrid.poem.39690": {"metadata": {"author": {"name": "G\u00fcnderode, Karoline von", "birth": "N.A.", "death": "N.A."}, "title": "Die Malabarischen Witwen", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum Flammentode gehn an Indusstranden", "tokens": ["Zum", "Flam\u00b7men\u00b7to\u00b7de", "gehn", "an", "In\u00b7dus\u00b7stran\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit dem Gemahl, in Jugendherrlichkeit,", "tokens": ["Mit", "dem", "Ge\u00b7mahl", ",", "in", "Ju\u00b7gend\u00b7herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Frauen, ohne Zagen, ohne Leid,", "tokens": ["Die", "Frau\u00b7en", ",", "oh\u00b7ne", "Za\u00b7gen", ",", "oh\u00b7ne", "Leid", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUI", "NN", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Geschm\u00fccket festlich, wie in Brautgewanden.", "tokens": ["Ge\u00b7schm\u00fc\u00b7cket", "fest\u00b7lich", ",", "wie", "in", "Braut\u00b7ge\u00b7wan\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "PWAV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Sitte hat der Liebe Sinn verstanden,", "tokens": ["Die", "Sit\u00b7te", "hat", "der", "Lie\u00b7be", "Sinn", "ver\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie von der Trennung harter Schmach befreit", "tokens": ["Sie", "von", "der", "Tren\u00b7nung", "har\u00b7ter", "Schmach", "be\u00b7freit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu ihrem Priester selbst den Tod geweiht,", "tokens": ["Zu", "ih\u00b7rem", "Pries\u00b7ter", "selbst", "den", "Tod", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Unsterblichkeit gegeben ihren Banden.", "tokens": ["U\u00b7nsterb\u00b7lich\u00b7keit", "ge\u00b7ge\u00b7ben", "ih\u00b7ren", "Ban\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Nicht Trennung ferner solchem Bunde droht,", "tokens": ["Nicht", "Tren\u00b7nung", "fer\u00b7ner", "sol\u00b7chem", "Bun\u00b7de", "droht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Denn die vorhin entzweiten Liebesflammen", "tokens": ["Denn", "die", "vor\u00b7hin", "ent\u00b7zwei\u00b7ten", "Lie\u00b7bes\u00b7flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In einer schlagen br\u00fcnstig sie zusammen.", "tokens": ["In", "ei\u00b7ner", "schla\u00b7gen", "br\u00fcns\u00b7tig", "sie", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJD", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Zur s\u00fc\u00dfen Liebesfeyer wird der Tod,", "tokens": ["Zur", "s\u00fc\u00b7\u00dfen", "Lie\u00b7bes\u00b7fe\u00b7yer", "wird", "der", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vereinet die getrennten Elemente,", "tokens": ["Ver\u00b7ei\u00b7net", "die", "ge\u00b7trenn\u00b7ten", "E\u00b7le\u00b7men\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Zum Lebensgipfel wird des Daseins Ende.", "tokens": ["Zum", "Le\u00b7bens\u00b7gip\u00b7fel", "wird", "des", "Da\u00b7seins", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}