{"textgrid.poem.31534": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mich d\u00fcrstet: da\u00df ich euch an Freud kan truncken machen /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mich d\u00fcrstet: da\u00df ich euch an Freud kan truncken machen /", "tokens": ["Mich", "d\u00fcrs\u00b7tet", ":", "da\u00df", "ich", "euch", "an", "Freud", "kan", "trun\u00b7cken", "ma\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "KOUS", "PPER", "PRF", "APPR", "NN", "VMFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "da\u00df ihr vor gutem Muht jauchzt in der Ewigkeit.", "tokens": ["da\u00df", "ihr", "vor", "gu\u00b7tem", "Muht", "jauchzt", "in", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Blut / so d\u00fcrstig ist / da\u00df es euch Ruh bereit /", "tokens": ["Mein", "Blut", "/", "so", "d\u00fcrs\u00b7tig", "ist", "/", "da\u00df", "es", "euch", "Ruh", "be\u00b7reit", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ADV", "ADJD", "VAFIN", "$(", "KOUS", "PPER", "PPER", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "da\u00df seiner tropffen Schwei\u00df wie Purpur-Thau herbrachen.", "tokens": ["da\u00df", "sei\u00b7ner", "tropf\u00b7fen", "Schwei\u00df", "wie", "Pur\u00b7pur\u00b7\u00b7T\u00b7hau", "her\u00b7bra\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Es d\u00fcrstet nach dem Durst der fast verschmachten Schwachen.", "tokens": ["Es", "d\u00fcrs\u00b7tet", "nach", "dem", "Durst", "der", "fast", "ver\u00b7schmach\u00b7ten", "Schwa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "und da\u00df es ihnen selbst k\u00f6nnt werden mit der Zeit", "tokens": ["und", "da\u00df", "es", "ih\u00b7nen", "selbst", "k\u00f6nnt", "wer\u00b7den", "mit", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "ein Trank: ihr werdt dadurch des Seelen-Dursts befreyt.", "tokens": ["ein", "Trank", ":", "ihr", "werdt", "da\u00b7durch", "des", "See\u00b7len\u00b7Dursts", "be\u00b7freyt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PPER", "VAFIN", "PAV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Durst ist / nicht nach Wein / nach Herz-Erquickungs Sachen.", "tokens": ["Der", "Durst", "ist", "/", "nicht", "nach", "Wein", "/", "nach", "Her\u00b7zEr\u00b7qui\u00b7ckungs", "Sa\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$(", "PTKNEG", "APPR", "NN", "$(", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich k\u00f6nt den Felsen auch wol schlagen / wann ich wolt /", "tokens": ["Ich", "k\u00f6nt", "den", "Fel\u00b7sen", "auch", "wol", "schla\u00b7gen", "/", "wann", "ich", "wolt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADV", "VVINF", "$(", "PWAV", "PPER", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "ich selbst der Lebens-Brunn k\u00f6nt frische Quellen schaffen /", "tokens": ["ich", "selbst", "der", "Le\u00b7bens\u00b7Brunn", "k\u00f6nt", "fri\u00b7sche", "Quel\u00b7len", "schaf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "ADJD", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "ja da\u00df mir in den Mund ein B\u00e4chlein rinnen solt.", "tokens": ["ja", "da\u00df", "mir", "in", "den", "Mund", "ein", "B\u00e4ch\u00b7lein", "rin\u00b7nen", "solt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "APPR", "ART", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "da\u00df ihr wurd Ewig satt / mich alle M\u00e4ngel traffen.", "tokens": ["da\u00df", "ihr", "wurd", "E\u00b7wig", "satt", "/", "mich", "al\u00b7le", "M\u00e4n\u00b7gel", "traf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "ADV", "ADJD", "$(", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Schau / alles di\u00df / O Mensch / ich willig leid vor dich.", "tokens": ["Schau", "/", "al\u00b7les", "di\u00df", "/", "O", "Mensch", "/", "ich", "wil\u00b7lig", "leid", "vor", "dich", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PIS", "PDS", "$(", "NE", "NN", "$(", "PPER", "ADJD", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Mit Buse-Thr\u00e4nen solst du wider tr\u00e4ncken mich.", "tokens": ["Mit", "Bu\u00b7se\u00b7Thr\u00e4\u00b7nen", "solst", "du", "wi\u00b7der", "tr\u00e4n\u00b7cken", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "PTKVZ", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}