{"textgrid.poem.44590": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es fiel einer in eine Grube", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es fiel einer in eine Grube", "tokens": ["Es", "fiel", "ei\u00b7ner", "in", "ei\u00b7ne", "Gru\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "APPR", "ART", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und brach dabei ein Bein,", "tokens": ["Und", "brach", "da\u00b7bei", "ein", "Bein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Statt die Grube zuzusch\u00fctten,", "tokens": ["Statt", "die", "Gru\u00b7be", "zu\u00b7zu\u00b7sch\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Beschlo\u00df er, achtsamer zu sein,", "tokens": ["Be\u00b7schlo\u00df", "er", ",", "acht\u00b7sa\u00b7mer", "zu", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch nachts brach Feuer aus,", "tokens": ["Doch", "nachts", "brach", "Feu\u00b7er", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Verwirrt von der Flamme Schein,", "tokens": ["Ver\u00b7wirrt", "von", "der", "Flam\u00b7me", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Verga\u00df er auf die Grube", "tokens": ["Ver\u00b7ga\u00df", "er", "auf", "die", "Gru\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und fiel zum zweitenmal hinein.", "tokens": ["Und", "fiel", "zum", "zwei\u00b7ten\u00b7mal", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was er nun da zerbrochen,", "tokens": ["Was", "er", "nun", "da", "zer\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Macht andern wenig Pein,", "tokens": ["Macht", "an\u00b7dern", "we\u00b7nig", "Pein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Recht w\u00e4rs der Sch\u00e4del gewesen,", "tokens": ["Mit", "Recht", "w\u00e4rs", "der", "Sch\u00e4\u00b7del", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "VAPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "War doch auch der Fehler sein.", "tokens": ["War", "doch", "auch", "der", "Feh\u00b7ler", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}