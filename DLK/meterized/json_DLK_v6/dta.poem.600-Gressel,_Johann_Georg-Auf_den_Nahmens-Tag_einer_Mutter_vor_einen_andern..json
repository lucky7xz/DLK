{"dta.poem.600": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Nahmens-Tag einer Mutter/  \n vor einen andern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Hertz und Geist ermuntert euch", "tokens": ["Mein", "Hertz", "und", "Geist", "er\u00b7mun\u00b7tert", "euch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jhr m\u00fc\u00dft bey Arbeit und den B\u00fcchern", "tokens": ["Ihr", "m\u00fc\u00dft", "bey", "Ar\u00b7beit", "und", "den", "B\u00fc\u00b7chern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Durch dieses Opffer heut zugleich", "tokens": ["Durch", "die\u00b7ses", "Opf\u00b7fer", "heut", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch eure Freude mit versichern/", "tokens": ["Auch", "eu\u00b7re", "Freu\u00b7de", "mit", "ver\u00b7si\u00b7chern", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Treue treibt euch dazu an/", "tokens": ["Die", "Treu\u00b7e", "treibt", "euch", "da\u00b7zu", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PAV", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Liebe nennt es wohlgethan.", "tokens": ["Und", "Lie\u00b7be", "nennt", "es", "wohl\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mein Hertz und Geist ermuntert euch.", "tokens": ["Mein", "Hertz", "und", "Geist", "er\u00b7mun\u00b7tert", "euch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}