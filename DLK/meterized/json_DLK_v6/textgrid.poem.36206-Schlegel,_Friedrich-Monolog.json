{"textgrid.poem.36206": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Monolog", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, ich f\u00fchle mich gezwungen", "tokens": ["Ja", ",", "ich", "f\u00fch\u00b7le", "mich", "ge\u00b7zwun\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Endlich dir mein Leid zu klagen.", "tokens": ["End\u00b7lich", "dir", "mein", "Leid", "zu", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00fcfte! k\u00f6nnt ihr mir nicht sagen,", "tokens": ["L\u00fcf\u00b7te", "!", "k\u00f6nnt", "ihr", "mir", "nicht", "sa\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo so lange bleibt die Meine?", "tokens": ["Wo", "so", "lan\u00b7ge", "bleibt", "die", "Mei\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df ich irren mu\u00df alleine,", "tokens": ["Da\u00df", "ich", "ir\u00b7ren", "mu\u00df", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seufzen nach der Unbekannten;", "tokens": ["Seuf\u00b7zen", "nach", "der", "Un\u00b7be\u00b7kann\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nein, der nur zu wohl Bekannten,", "tokens": ["Nein", ",", "der", "nur", "zu", "wohl", "Be\u00b7kann\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "ADV", "APPR", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die in jedem Traum ich sah,", "tokens": ["Die", "in", "je\u00b7dem", "Traum", "ich", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Deren Bild mir immer nah,", "tokens": ["De\u00b7ren", "Bild", "mir", "im\u00b7mer", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Doch vor allem hier im Tal;", "tokens": ["Doch", "vor", "al\u00b7lem", "hier", "im", "Tal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Hier erfreu' ich mich der Qual,", "tokens": ["Hier", "er\u00b7freu'", "ich", "mich", "der", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Hier ergeb' ich mich den Tr\u00e4umen,", "tokens": ["Hier", "er\u00b7geb'", "ich", "mich", "den", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Irrend auf den gr\u00fcnen R\u00e4umen,", "tokens": ["Ir\u00b7rend", "auf", "den", "gr\u00fc\u00b7nen", "R\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Wo der Waldbach rauschend flie\u00dft.", "tokens": ["Wo", "der", "Wald\u00b7bach", "rau\u00b7schend", "flie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Wenn sich im Gesang ergie\u00dft", "tokens": ["Wenn", "sich", "im", "Ge\u00b7sang", "er\u00b7gie\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Klage hoher Nachtigallen,", "tokens": ["Kla\u00b7ge", "ho\u00b7her", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Sie ins tiefe Herz mir schallen,", "tokens": ["Sie", "ins", "tie\u00b7fe", "Herz", "mir", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "\u00d6ffnet sich der stumme Mund,", "tokens": ["\u00d6ff\u00b7net", "sich", "der", "stum\u00b7me", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Tut mir selbst mein Leiden kund,", "tokens": ["Tut", "mir", "selbst", "mein", "Lei\u00b7den", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Rufend dich, du lang Ersehnte!", "tokens": ["Ru\u00b7fend", "dich", ",", "du", "lang", "Er\u00b7sehn\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "$,", "PPER", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Alles, was ich Freude w\u00e4hnte,", "tokens": ["Al\u00b7les", ",", "was", "ich", "Freu\u00b7de", "w\u00e4hn\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PWS", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Kann mir keine Lust mehr schaffen.", "tokens": ["Kann", "mir", "kei\u00b7ne", "Lust", "mehr", "schaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Fremd sind mir die teuren Waffen,", "tokens": ["Fremd", "sind", "mir", "die", "teu\u00b7ren", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Nichts der Rosse k\u00fchnes Spiel;", "tokens": ["Nichts", "der", "Ros\u00b7se", "k\u00fch\u00b7nes", "Spiel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Ach und was mir so gefiel,", "tokens": ["Ach", "und", "was", "mir", "so", "ge\u00b7fiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KON", "PWS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Lieber Freunde mutig Scherzen,", "tokens": ["Lie\u00b7ber", "Freun\u00b7de", "mu\u00b7tig", "Scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Das verwirrt mir nur die Schmerzen,", "tokens": ["Das", "ver\u00b7wirrt", "mir", "nur", "die", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Seit ich hin, nicht mehr mein eigen.", "tokens": ["Seit", "ich", "hin", ",", "nicht", "mehr", "mein", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKVZ", "$,", "PTKNEG", "ADV", "PPOSAT", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.29": {"text": "Komm, Geliebte, dich zu zeigen,", "tokens": ["Komm", ",", "Ge\u00b7lieb\u00b7te", ",", "dich", "zu", "zei\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Da\u00df ich dich mit Glanz umkr\u00f6ne!", "tokens": ["Da\u00df", "ich", "dich", "mit", "Glanz", "um\u00b7kr\u00f6\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Komm, und gib mir deine Sch\u00f6ne", "tokens": ["Komm", ",", "und", "gib", "mir", "dei\u00b7ne", "Sch\u00f6\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KON", "VVIMP", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Mein zu sein, mein Heiligtum!", "tokens": ["Mein", "zu", "sein", ",", "mein", "Hei\u00b7lig\u00b7tum", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "PTKZU", "VAINF", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Bin ich darum reich an Ruhm,", "tokens": ["Bin", "ich", "da\u00b7rum", "reich", "an", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PAV", "ADJD", "APPR", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.34": {"text": "Aller Jugend Kron' und Zier,", "tokens": ["Al\u00b7ler", "Ju\u00b7gend", "Kron'", "und", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Da\u00df ich krank und einsam hier", "tokens": ["Da\u00df", "ich", "krank", "und", "ein\u00b7sam", "hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "KON", "ADJD", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.36": {"text": "Soll umsonst nach Freude schmachten?", "tokens": ["Soll", "um\u00b7sonst", "nach", "Freu\u00b7de", "schmach\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Jede Kunde, die mir brachten", "tokens": ["Je\u00b7de", "Kun\u00b7de", ",", "die", "mir", "brach\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ferne M\u00e4nner, stille Boten,", "tokens": ["Fer\u00b7ne", "M\u00e4n\u00b7ner", ",", "stil\u00b7le", "Bo\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was du mir geheim entboten,", "tokens": ["Was", "du", "mir", "ge\u00b7heim", "ent\u00b7bo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was so manche Sommernacht,", "tokens": ["Was", "so", "man\u00b7che", "Som\u00b7mer\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die ich gl\u00fchend durchgewacht,", "tokens": ["Die", "ich", "gl\u00fc\u00b7hend", "durch\u00b7ge\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch die W\u00e4lder mich getrieben,", "tokens": ["Durch", "die", "W\u00e4l\u00b7der", "mich", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Das ist in der Brust geblieben,", "tokens": ["Das", "ist", "in", "der", "Brust", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Alles andre gar verschwunden.", "tokens": ["Al\u00b7les", "and\u00b7re", "gar", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PIS", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Keine hatt' ich noch gefunden,", "tokens": ["Kei\u00b7ne", "hatt'", "ich", "noch", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Die wie ich der Glut ergeben.", "tokens": ["Die", "wie", "ich", "der", "Glut", "er\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PWAV", "PPER", "ART", "NN", "VVPP", "$."], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.11": {"text": "Ach, sie f\u00fchlten nicht das Leben,", "tokens": ["Ach", ",", "sie", "f\u00fchl\u00b7ten", "nicht", "das", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Das der vollen Seel' entquillt!", "tokens": ["Das", "der", "vol\u00b7len", "Seel'", "ent\u00b7quillt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Keine hat das Herz gestillt,", "tokens": ["Kei\u00b7ne", "hat", "das", "Herz", "ge\u00b7stillt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Das die Kraft so gern verschwendet.", "tokens": ["Das", "die", "Kraft", "so", "gern", "ver\u00b7schwen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Du allein hast Trost gesendet,", "tokens": ["Du", "al\u00b7lein", "hast", "Trost", "ge\u00b7sen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "S\u00fc\u00dfe Herrin, du alleine,", "tokens": ["S\u00fc\u00b7\u00dfe", "Her\u00b7rin", ",", "du", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Du an Sch\u00f6nheit einzig Eine,", "tokens": ["Du", "an", "Sch\u00f6n\u00b7heit", "ein\u00b7zig", "Ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "ADJD", "ART", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Stolze Heldin, herrlich Wesen,", "tokens": ["Stol\u00b7ze", "Hel\u00b7din", ",", "herr\u00b7lich", "We\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Die ein Gott wohl auserlesen,", "tokens": ["Die", "ein", "Gott", "wohl", "au\u00b7ser\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Da\u00df mir k\u00fchne Hoffnung bliebe,", "tokens": ["Da\u00df", "mir", "k\u00fch\u00b7ne", "Hoff\u00b7nung", "blie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Wahr zu finden meine Liebe.", "tokens": ["Wahr", "zu", "fin\u00b7den", "mei\u00b7ne", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}