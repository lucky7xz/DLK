{"textgrid.poem.53335": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Als der Edle, Rottger von Tieffenbrock, aus Lieffland b\u00fcrtig, etc. diese Welt gesegnet, zu K\u00f6nigsberg in Preussen", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nimm dich, O meine Seel', in acht,", "tokens": ["Nimm", "dich", ",", "O", "mei\u00b7ne", "Seel'", ",", "in", "acht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NE", "PPOSAT", "NN", "$,", "APPR", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du must schon hier in diesem Leben", "tokens": ["Du", "must", "schon", "hier", "in", "die\u00b7sem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nach dem, was ewig seelig macht,", "tokens": ["Nach", "dem", ",", "was", "e\u00b7wig", "see\u00b7lig", "macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht aller erst im Tode, streben.", "tokens": ["Nicht", "al\u00b7ler", "erst", "im", "To\u00b7de", ",", "stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "PIAT", "ADV", "APPRART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sind es die Sch\u00e4tze dieser Welt", "tokens": ["Sind", "es", "die", "Sch\u00e4t\u00b7ze", "die\u00b7ser", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die dich im Himmel auch versorgen,", "tokens": ["Die", "dich", "im", "Him\u00b7mel", "auch", "ver\u00b7sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So renn vnd lauff, vnd samle Geld", "tokens": ["So", "renn", "vnd", "lauff", ",", "vnd", "sam\u00b7le", "Geld"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "NN", "$,", "KON", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vmb Mittag, Abend, Nachtzeit, Morgen!", "tokens": ["Vmb", "Mit\u00b7tag", ",", "A\u00b7bend", ",", "Nacht\u00b7zeit", ",", "Mor\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was aber ist der Schrifft Bericht?", "tokens": ["Was", "a\u00b7ber", "ist", "der", "Schrifft", "Be\u00b7richt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Welt-Krei\u00df, sagt sie, wird vergehen,", "tokens": ["Der", "Welt\u00b7Krei\u00df", ",", "sagt", "sie", ",", "wird", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "$,", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So k\u00f6nnen ia die G\u00fcter nicht", "tokens": ["So", "k\u00f6n\u00b7nen", "i\u00b7a", "die", "G\u00fc\u00b7ter", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "ART", "NN", "PTKNEG"], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.4": {"text": "Den Fall des Himmels \u00fcberstehen.", "tokens": ["Den", "Fall", "des", "Him\u00b7mels", "\u00fc\u00b7bers\u00b7te\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Vnd wirst du t\u00e4glich nicht gewahr", "tokens": ["Vnd", "wirst", "du", "t\u00e4g\u00b7lich", "nicht", "ge\u00b7wahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "PTKNEG", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie viel man hin tr\u00e4gt nach der Erden?", "tokens": ["Wie", "viel", "man", "hin", "tr\u00e4gt", "nach", "der", "Er\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIS", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die hebt man nackt da auff die Bahr,", "tokens": ["Die", "hebt", "man", "nackt", "da", "auff", "die", "Bahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "ADJD", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Geld vnd Gut mus andern werden.", "tokens": ["Ihr", "Geld", "vnd", "Gut", "mus", "an\u00b7dern", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VMFIN", "PIS", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Was hat die Erde sonst vor dich?", "tokens": ["Was", "hat", "die", "Er\u00b7de", "sonst", "vor", "dich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was kanst du mit von hinnen bringen?", "tokens": ["Was", "kanst", "du", "mit", "von", "hin\u00b7nen", "brin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht Pracht noch Hoheit h\u00e4lt den Stich,", "tokens": ["Nicht", "Pracht", "noch", "Ho\u00b7heit", "h\u00e4lt", "den", "Stich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ADV", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verg\u00e4ngnis herscht in allen Dingen.", "tokens": ["Ver\u00b7g\u00e4ng\u00b7nis", "herscht", "in", "al\u00b7len", "Din\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Himmel hat dein wahres Gut", "tokens": ["Der", "Him\u00b7mel", "hat", "dein", "wah\u00b7res", "Gut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach dem du iederzeit solst trachten,", "tokens": ["Nach", "dem", "du", "ie\u00b7der\u00b7zeit", "solst", "trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Daselbst hin schick du deinen Muth", "tokens": ["Da\u00b7selbst", "hin", "schick", "du", "dei\u00b7nen", "Muth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd lern die Erde bald verachten.", "tokens": ["Vnd", "lern", "die", "Er\u00b7de", "bald", "ver\u00b7ach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "O wer beschreibt den Reichthum mir", "tokens": ["O", "wer", "be\u00b7schreibt", "den", "Reicht\u00b7hum", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "PWS", "VVFIN", "ART", "NN", "PPER"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Der dort ist beygelegt den Frommen?", "tokens": ["Der", "dort", "ist", "bey\u00b7ge\u00b7legt", "den", "From\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VAFIN", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wer alle Lust, zu welcher wir,", "tokens": ["Wer", "al\u00b7le", "Lust", ",", "zu", "wel\u00b7cher", "wir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "$,", "APPR", "PRELS", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn wir die Welt verachten, kommen?", "tokens": ["Wenn", "wir", "die", "Welt", "ver\u00b7ach\u00b7ten", ",", "kom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Kein Ohr vnd Aug' hat ie erkant", "tokens": ["Kein", "Ohr", "vnd", "Aug'", "hat", "ie", "er\u00b7kant"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "NN", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd keines Hertz hat noch empfunden", "tokens": ["Vnd", "kei\u00b7nes", "Hertz", "hat", "noch", "emp\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Seelen Rhu vnd Frewden-Standt,", "tokens": ["Der", "See\u00b7len", "Rhu", "vnd", "Fre\u00b7wden\u00b7Standt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die alles selig \u00fcberwunden.", "tokens": ["Die", "al\u00b7les", "se\u00b7lig", "\u00fc\u00b7berw\u00b7un\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Was hemmt die Erde meinen Lauff?", "tokens": ["Was", "hemmt", "die", "Er\u00b7de", "mei\u00b7nen", "Lauff", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was h\u00e4lt sie mich mit Zaum vnd Z\u00fcgel?", "tokens": ["Was", "h\u00e4lt", "sie", "mich", "mit", "Zaum", "vnd", "Z\u00fc\u00b7gel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich sehne hertzlich mich hienauff,", "tokens": ["Ich", "seh\u00b7ne", "hertz\u00b7lich", "mich", "hien\u00b7auff", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PRF", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer giebt mir hie zu Adlers Fl\u00fcgel?", "tokens": ["Wer", "giebt", "mir", "hie", "zu", "Ad\u00b7lers", "Fl\u00fc\u00b7gel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Komm, Jesu, nimm mich zu Dir ein,", "tokens": ["Komm", ",", "Je\u00b7su", ",", "nimm", "mich", "zu", "Dir", "ein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NE", "$,", "VVIMP", "PPER", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Komm, seum mich nicht in meinen Frewden!", "tokens": ["Komm", ",", "seum", "mich", "nicht", "in", "mei\u00b7nen", "Frew\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUI", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich habe Lust bey Dir zu seyn", "tokens": ["Ich", "ha\u00b7be", "Lust", "bey", "Dir", "zu", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "APPR", "PPER", "PTKZU", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd darumb selig abzuscheiden.", "tokens": ["Vnd", "da\u00b7rumb", "se\u00b7lig", "ab\u00b7zu\u00b7schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}