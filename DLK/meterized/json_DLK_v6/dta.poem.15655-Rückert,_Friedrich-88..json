{"dta.poem.15655": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "88.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn zwei zu gleicher Zeit, der hier aus flachem Thal,", "tokens": ["Wenn", "zwei", "zu", "glei\u00b7cher", "Zeit", ",", "der", "hier", "aus", "fla\u00b7chem", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "CARD", "APPR", "ADJA", "NN", "$,", "PRELS", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der dort vom h\u00f6chsten Thurm, sehn eines Sternes Stral;", "tokens": ["Der", "dort", "vom", "h\u00f6chs\u00b7ten", "Thurm", ",", "sehn", "ei\u00b7nes", "Ster\u00b7nes", "Stral", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "ADJA", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wird jener niedriger deswegen etwa sehn,", "tokens": ["Wird", "je\u00b7ner", "nied\u00b7ri\u00b7ger", "des\u00b7we\u00b7gen", "et\u00b7wa", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "ADJD", "PAV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und h\u00f6her dieser hier den Stern am Himmel stehn?", "tokens": ["Und", "h\u00f6\u00b7her", "die\u00b7ser", "hier", "den", "Stern", "am", "Him\u00b7mel", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PDAT", "ADV", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nein, gleichhoch setzen ihn die beiden, und empfinden,", "tokens": ["Nein", ",", "gleich\u00b7hoch", "set\u00b7zen", "ihn", "die", "bei\u00b7den", ",", "und", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "PPER", "ART", "PIAT", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df Erdabst\u00e4nde vorm Unendlichen verschwinden.", "tokens": ["Da\u00df", "Erd\u00b7ab\u00b7st\u00e4n\u00b7de", "vorm", "Un\u00b7end\u00b7li\u00b7chen", "ver\u00b7schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}