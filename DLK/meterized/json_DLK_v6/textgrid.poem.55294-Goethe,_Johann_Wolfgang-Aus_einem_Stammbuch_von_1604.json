{"textgrid.poem.55294": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Aus einem Stammbuch von 1604", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hoffnung beschwingt Gedanken, Liebe Hoffnung,", "tokens": ["Hoff\u00b7nung", "be\u00b7schwingt", "Ge\u00b7dan\u00b7ken", ",", "Lie\u00b7be", "Hoff\u00b7nung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "In klarster Nacht hinauf zu Cynthien, Liebe!", "tokens": ["In", "klars\u00b7ter", "Nacht", "hin\u00b7auf", "zu", "Cyn\u00b7thien", ",", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "APPR", "NE", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sprich: wie sie sich oben umgestaltet,", "tokens": ["Und", "sprich", ":", "wie", "sie", "sich", "o\u00b7ben", "um\u00b7ge\u00b7stal\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$.", "PWAV", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So auf der Erde schwindet, w\u00e4chst mein Gl\u00fcck.", "tokens": ["So", "auf", "der", "Er\u00b7de", "schwin\u00b7det", ",", "w\u00e4chst", "mein", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und wispere sanft-bescheiden ihr ans Ohr,", "tokens": ["Und", "wis\u00b7pe\u00b7re", "sanft\u00b7be\u00b7schei\u00b7den", "ihr", "ans", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPOSAT", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Wie Zweifel oft das Haupt hing, Treue tr\u00e4nte.", "tokens": ["Wie", "Zwei\u00b7fel", "oft", "das", "Haupt", "hing", ",", "Treu\u00b7e", "tr\u00e4n\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "ART", "NN", "VVFIN", "$,", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und ihr, Gedanken, mi\u00dfzutraun geneigt,", "tokens": ["Und", "ihr", ",", "Ge\u00b7dan\u00b7ken", ",", "mi\u00df\u00b7zu\u00b7traun", "ge\u00b7neigt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Beschilt euch die Geliebte dessenthalb,", "tokens": ["Be\u00b7schilt", "euch", "die", "Ge\u00b7lieb\u00b7te", "des\u00b7sent\u00b7halb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "So sagt: ihr wechselt zwar, doch \u00e4ndert nicht,", "tokens": ["So", "sagt", ":", "ihr", "wech\u00b7selt", "zwar", ",", "doch", "\u00e4n\u00b7dert", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "$,", "ADV", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wie sie dieselbe bleibt und immer wechselt.", "tokens": ["Wie", "sie", "die\u00b7sel\u00b7be", "bleibt", "und", "im\u00b7mer", "wech\u00b7selt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PDAT", "VVFIN", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Untrauen tritt ins Herz, vergiftet's nicht,", "tokens": ["Un\u00b7trau\u00b7en", "tritt", "ins", "Herz", ",", "ver\u00b7gif\u00b7tet's", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Denn Lieb ist s\u00fc\u00dfer, von Verdacht gew\u00fcrzt.", "tokens": ["Denn", "Lieb", "ist", "s\u00fc\u00b7\u00dfer", ",", "von", "Ver\u00b7dacht", "ge\u00b7w\u00fcrzt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADJD", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Wenn sie verdrie\u00dflich dann das Aug umw\u00f6lkt,", "tokens": ["Wenn", "sie", "ver\u00b7drie\u00df\u00b7lich", "dann", "das", "Aug", "um\u00b7w\u00f6lkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Des Himmels Kl\u00e4re widerw\u00e4rtig schw\u00e4rzt,", "tokens": ["Des", "Him\u00b7mels", "Kl\u00e4\u00b7re", "wi\u00b7der\u00b7w\u00e4r\u00b7tig", "schw\u00e4rzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Dann, Seufzerwinde, scheucht die Wolken weg,", "tokens": ["Dann", ",", "Seuf\u00b7zer\u00b7win\u00b7de", ",", "scheucht", "die", "Wol\u00b7ken", "weg", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Tr\u00e4nt nieder, sie in Regen aufzul\u00f6sen.", "tokens": ["Tr\u00e4nt", "nie\u00b7der", ",", "sie", "in", "Re\u00b7gen", "auf\u00b7zu\u00b7l\u00f6\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Gedanke, Hoffnung, Liebe bleibt nur dort,", "tokens": ["Ge\u00b7dan\u00b7ke", ",", "Hoff\u00b7nung", ",", "Lie\u00b7be", "bleibt", "nur", "dort", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Bis Cynthia scheint, wie sie mir sonst getan.", "tokens": ["Bis", "Cyn\u00b7thia", "scheint", ",", "wie", "sie", "mir", "sonst", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "$,", "PWAV", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}