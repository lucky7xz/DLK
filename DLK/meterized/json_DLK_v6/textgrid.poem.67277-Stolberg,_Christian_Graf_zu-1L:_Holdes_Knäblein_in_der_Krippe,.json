{"textgrid.poem.67277": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Holdes Kn\u00e4blein in der Krippe,", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Holdes Kn\u00e4blein in der Krippe,", "tokens": ["Hol\u00b7des", "Kn\u00e4\u00b7blein", "in", "der", "Krip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4chelnd nimm zum Weihnachtsgru\u00df", "tokens": ["L\u00e4\u00b7chelnd", "nimm", "zum", "Weih\u00b7nachts\u00b7gru\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihn, den bietet meine Lippe", "tokens": ["Ihn", ",", "den", "bie\u00b7tet", "mei\u00b7ne", "Lip\u00b7pe"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "ART", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deiner H\u00fclle Saum, den Ku\u00df.", "tokens": ["Dei\u00b7ner", "H\u00fcl\u00b7le", "Saum", ",", "den", "Ku\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wort des Anfangs, dessen Werde,", "tokens": ["Wort", "des", "An\u00b7fangs", ",", "des\u00b7sen", "Wer\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PDS", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gottheitsf\u00fclle wohnt in Dir,", "tokens": ["Got\u00b7theits\u00b7f\u00fcl\u00b7le", "wohnt", "in", "Dir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sonne, Sterne, Mond und Erde", "tokens": ["Son\u00b7ne", ",", "Ster\u00b7ne", ",", "Mond", "und", "Er\u00b7de"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schuf, und Leben gab auch mir!", "tokens": ["Schuf", ",", "und", "Le\u00b7ben", "gab", "auch", "mir", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "NN", "VVFIN", "ADV", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Bethlems angef\u00fcllte H\u00fctte", "tokens": ["Beth\u00b7lems", "an\u00b7ge\u00b7f\u00fcll\u00b7te", "H\u00fct\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lie\u00df im Stalle Raum Dir nur,", "tokens": ["Lie\u00df", "im", "Stal\u00b7le", "Raum", "Dir", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "NN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Selbst des Neugebornen Tritte", "tokens": ["Selbst", "des", "Neu\u00b7ge\u00b7bor\u00b7nen", "Trit\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wallten schon auf Kreuzes-Spur.", "tokens": ["Wall\u00b7ten", "schon", "auf", "Kreu\u00b7zes\u00b7Spur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Der des Aufgangs Fackel z\u00fcndet,", "tokens": ["Der", "des", "Auf\u00b7gangs", "Fa\u00b7ckel", "z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spangt Orions Flammenschwert,", "tokens": ["Spangt", "O\u00b7rions", "Flam\u00b7men\u00b7schwert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Darbt in Noth und Kummer, findet", "tokens": ["Darbt", "in", "Noth", "und", "Kum\u00b7mer", ",", "fin\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PAV", "APPR", "NN", "KON", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihn zu w\u00e4rmen keinen Heerd!", "tokens": ["Ihn", "zu", "w\u00e4r\u00b7men", "kei\u00b7nen", "Heerd", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Scholl alsbald nicht Juda's Meistern", "tokens": ["Scholl", "als\u00b7bald", "nicht", "Ju\u00b7da's", "Meis\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "PTKNEG", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wunderkund' in's Forscherohr,", "tokens": ["Wun\u00b7der\u00b7kund'", "in's", "For\u00b7scher\u00b7ohr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Strahlte Glanz von Heroldsgeistern", "tokens": ["Strahl\u00b7te", "Glanz", "von", "He\u00b7rolds\u00b7geis\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihnen nicht und Levi's Chor?", "tokens": ["Ih\u00b7nen", "nicht", "und", "Le\u00b7vi's", "Chor", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "KON", "NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Unschuld! Einfalt! Hirten waren", "tokens": ["Un\u00b7schuld", "!", "Ein\u00b7falt", "!", "Hir\u00b7ten", "wa\u00b7ren"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["ADJD", "$.", "NN", "$.", "NN", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ersterkohrne \u2013 \u2013 \u2013 Auf sich schlo\u00df", "tokens": ["Ers\u00b7ter\u00b7kohr\u00b7ne", "\u2013", "\u2013", "\u2013", "Auf", "sich", "schlo\u00df"], "token_info": ["word", "punct", "punct", "punct", "word", "word", "word"], "pos": ["ADV", "$(", "$(", "$(", "APPR", "PRF", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gottes Himmel, Engelschaaren", "tokens": ["Got\u00b7tes", "Him\u00b7mel", ",", "En\u00b7gel\u00b7schaa\u00b7ren"], "token_info": ["word", "word", "punct", "word"], "pos": ["NN", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwebten, Licht sich rings ergo\u00df.", "tokens": ["Schweb\u00b7ten", ",", "Licht", "sich", "rings", "er\u00b7go\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "PRF", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Eines Stimme, furchtbezwingend,", "tokens": ["Ei\u00b7nes", "Stim\u00b7me", ",", "furcht\u00b7be\u00b7zwin\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deutet auf das Kn\u00e4blein, bald", "tokens": ["Deu\u00b7tet", "auf", "das", "Kn\u00e4\u00b7blein", ",", "bald"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aller Hymnus Ihn lobsingend,", "tokens": ["Al\u00b7ler", "Hym\u00b7nus", "Ihn", "lob\u00b7sin\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um die Staunenden erschallt.", "tokens": ["Um", "die", "Stau\u00b7nen\u00b7den", "er\u00b7schallt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "\u00bbpreis Gott in der H\u00f6h', hienieden,\u00ab", "tokens": ["\u00bb", "preis", "Gott", "in", "der", "H\u00f6h'", ",", "hien\u00b7ie\u00b7den", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "ADJA", "NN", "APPR", "ART", "NN", "$,", "ADV", "$,", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Heilverk\u00fcndend t\u00f6nt's ihr Mund,", "tokens": ["Heil\u00b7ver\u00b7k\u00fcn\u00b7dend", "t\u00f6nt's", "ihr", "Mund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbgro\u00dfe Freud' und Gottes Frieden,", "tokens": ["\u00bb", "gro\u00b7\u00dfe", "Freud'", "und", "Got\u00b7tes", "Frie\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "KON", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Adams Stamm der Gnade Bund.\u00ab", "tokens": ["A\u00b7dams", "Stamm", "der", "Gna\u00b7de", "Bund", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "ART", "NN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Wer sind, die aus weiter Ferne", "tokens": ["Wer", "sind", ",", "die", "aus", "wei\u00b7ter", "Fer\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "APPR", "ADV", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ihm mit Opfergaben nah'n,", "tokens": ["Ihm", "mit", "Op\u00b7fer\u00b7ga\u00b7ben", "nah'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die, gef\u00fchrt von Seinem Sterne,", "tokens": ["Die", ",", "ge\u00b7f\u00fchrt", "von", "Sei\u00b7nem", "Ster\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wall'n die hochgeweihte Bahn?", "tokens": ["Wall'n", "die", "hoch\u00b7ge\u00b7weih\u00b7te", "Bahn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Wahrer Weisheit J\u00fcnger schauen", "tokens": ["Wah\u00b7rer", "Weis\u00b7heit", "J\u00fcn\u00b7ger", "schau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schmachtend himmelw\u00e4rts nach Licht,", "tokens": ["Schmach\u00b7tend", "him\u00b7mel\u00b7w\u00e4rts", "nach", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Glaubensvoll und ihr Vertrauen", "tokens": ["Glau\u00b7bens\u00b7voll", "und", "ihr", "Ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "T\u00e4uscht Er, der's erweckte, nicht.", "tokens": ["T\u00e4uscht", "Er", ",", "der's", "er\u00b7weck\u00b7te", ",", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PIS", "VVFIN", "$,", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Herr, wir schaun Dich nicht! doch selig", "tokens": ["Herr", ",", "wir", "schaun", "Dich", "nicht", "!", "doch", "se\u00b7lig"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$.", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind die glauben und nicht sehn,", "tokens": ["Sind", "die", "glau\u00b7ben", "und", "nicht", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "VVINF", "KON", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die, Dir allvertrauend, fr\u00f6hlich", "tokens": ["Die", ",", "Dir", "all\u00b7ver\u00b7trau\u00b7end", ",", "fr\u00f6h\u00b7lich"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "$,", "PPER", "ADJD", "$,", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hellerm Schaun entgegen gehn!", "tokens": ["Hel\u00b7lerm", "Schaun", "ent\u00b7ge\u00b7gen", "gehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKVZ", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}