{"textgrid.poem.66274": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Abendlied eines Gefangenen", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Tages tr\u00fcbe Stunden", "tokens": ["Des", "Ta\u00b7ges", "tr\u00fc\u00b7be", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sind wieder weggeschwunden,", "tokens": ["Sind", "wie\u00b7der", "weg\u00b7ge\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es gl\u00e4nzt der Abendstern", "tokens": ["Es", "gl\u00e4nzt", "der", "A\u00b7bends\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+----", "measure": "dactylic.init"}, "line.4": {"text": "An blauen Himmelsh\u00f6hen,", "tokens": ["An", "blau\u00b7en", "Him\u00b7mels\u00b7h\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Von mir zwar ungesehen,", "tokens": ["Von", "mir", "zwar", "un\u00b7ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Doch steigt mein Nachtgesang zum Herrn.", "tokens": ["Doch", "steigt", "mein", "Nacht\u00b7ge\u00b7sang", "zum", "Herrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er half mir wieder tragen", "tokens": ["Er", "half", "mir", "wie\u00b7der", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der langen Knechtschaft Plagen", "tokens": ["Der", "lan\u00b7gen", "Knecht\u00b7schaft", "Pla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und h\u00fcllt nun meine Pein,", "tokens": ["Und", "h\u00fcllt", "nun", "mei\u00b7ne", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Lasten meines Kummers,", "tokens": ["Die", "Las\u00b7ten", "mei\u00b7nes", "Kum\u00b7mers", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In Wolken sanftes Schlummers", "tokens": ["In", "Wol\u00b7ken", "sanf\u00b7tes", "Schlum\u00b7mers"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit allen meinen Thr\u00e4nen ein.", "tokens": ["Mit", "al\u00b7len", "mei\u00b7nen", "Thr\u00e4\u00b7nen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ja, danken will ich, danken,", "tokens": ["Ja", ",", "dan\u00b7ken", "will", "ich", ",", "dan\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKANT", "$,", "VVINF", "VMFIN", "PPER", "$,", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Denn mich verlassnen Kranken,", "tokens": ["Denn", "mich", "ver\u00b7lass\u00b7nen", "Kran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Erbarmer, st\u00e4rktest du!", "tokens": ["Er\u00b7bar\u00b7mer", ",", "st\u00e4rk\u00b7test", "du", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Du sah'st des Leibes Schw\u00e4che,", "tokens": ["Du", "sah'st", "des", "Lei\u00b7bes", "Schw\u00e4\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und go\u00df'st, wie Lebensb\u00e4che,", "tokens": ["Und", "go\u00df'st", ",", "wie", "Le\u00b7bens\u00b7b\u00e4\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In meine m\u00fcden Glieder Ruh'.", "tokens": ["In", "mei\u00b7ne", "m\u00fc\u00b7den", "Glie\u00b7der", "Ruh'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Wunden meiner Seele", "tokens": ["Die", "Wun\u00b7den", "mei\u00b7ner", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hast du, als wie mit Oele,", "tokens": ["Hast", "du", ",", "als", "wie", "mit", "O\u00b7e\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "KOUS", "KOKOM", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus deinem Wort betr\u00e4uft;", "tokens": ["Aus", "dei\u00b7nem", "Wort", "be\u00b7tr\u00e4uft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Den Engel sah ich strahlen,", "tokens": ["Den", "En\u00b7gel", "sah", "ich", "strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der nach des Elends Qualen", "tokens": ["Der", "nach", "des", "E\u00b7lends", "Qua\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Bande von den H\u00e4nden streift.", "tokens": ["Die", "Ban\u00b7de", "von", "den", "H\u00e4n\u00b7den", "streift", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Von Menschen, die mich hassen,", "tokens": ["Von", "Men\u00b7schen", ",", "die", "mich", "has\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Freunden selbst verlassen", "tokens": ["Von", "Freun\u00b7den", "selbst", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In \u00f6der Einsamkeit,", "tokens": ["In", "\u00f6\u00b7der", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Erbarmtest du dich meiner;", "tokens": ["Er\u00b7barm\u00b7test", "du", "dich", "mei\u00b7ner", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PPOSAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nur du, mein Gott, sonst keiner,", "tokens": ["Nur", "du", ",", "mein", "Gott", ",", "sonst", "kei\u00b7ner", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "PPOSAT", "NN", "$,", "ADV", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hast mich mit deinem Trost erfreut.", "tokens": ["Hast", "mich", "mit", "dei\u00b7nem", "Trost", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "In dieser Gr\u00e4berstille,", "tokens": ["In", "die\u00b7ser", "Gr\u00e4\u00b7ber\u00b7stil\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit dieses Herzens F\u00fclle", "tokens": ["Mit", "die\u00b7ses", "Her\u00b7zens", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Komm' ich, mein Gott, zu dir!", "tokens": ["Komm'", "ich", ",", "mein", "Gott", ",", "zu", "dir", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ist alles mir entrissen,", "tokens": ["Ist", "al\u00b7les", "mir", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So will ich's gerne missen,", "tokens": ["So", "will", "ich's", "ger\u00b7ne", "mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Denn alles, alles bist du mir!", "tokens": ["Denn", "al\u00b7les", ",", "al\u00b7les", "bist", "du", "mir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PIS", "VAFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Vergib mir meine Schulden,", "tokens": ["Ver\u00b7gib", "mir", "mei\u00b7ne", "Schul\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schenk mir die Kraft zu dulden,", "tokens": ["Schenk", "mir", "die", "Kraft", "zu", "dul\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gib Herzensreinigkeit!", "tokens": ["Gib", "Her\u00b7zen\u00b7srei\u00b7nig\u00b7keit", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVIMP", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Lehr mich im Elend danken,", "tokens": ["Lehr", "mich", "im", "E\u00b7lend", "dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Und will mein Glaube wanken,", "tokens": ["Und", "will", "mein", "Glau\u00b7be", "wan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So gib ihm wieder Festigkeit.", "tokens": ["So", "gib", "ihm", "wie\u00b7der", "Fes\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Willst du mir noch im Leben", "tokens": ["Willst", "du", "mir", "noch", "im", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Freiheit wieder geben;", "tokens": ["Die", "Frei\u00b7heit", "wie\u00b7der", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So h\u00f6r' den gro\u00dfen Schwur:", "tokens": ["So", "h\u00f6r'", "den", "gro\u00b7\u00dfen", "Schwur", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dir soll mein Herz nur klopfen,", "tokens": ["Dir", "soll", "mein", "Herz", "nur", "klop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und alle Lebenstropfen", "tokens": ["Und", "al\u00b7le", "Le\u00b7bens\u00b7trop\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Verstr\u00f6men dir zur Ehre nur.", "tokens": ["Ver\u00b7str\u00f6\u00b7men", "dir", "zur", "Eh\u00b7re", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Doch soll nach tausend N\u00f6then", "tokens": ["Doch", "soll", "nach", "tau\u00b7send", "N\u00f6\u00b7then"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APPR", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Langsamer Tod mich t\u00f6dten;", "tokens": ["Lang\u00b7sa\u00b7mer", "Tod", "mich", "t\u00f6d\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "So h\u00f6r' den gro\u00dfen Schwur:", "tokens": ["So", "h\u00f6r'", "den", "gro\u00b7\u00dfen", "Schwur", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dich will ich stammelnd preisen", "tokens": ["Dich", "will", "ich", "stam\u00b7melnd", "prei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In meinen letzten Schwei\u00dfen;", "tokens": ["In", "mei\u00b7nen", "letz\u00b7ten", "Schwei\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Auf Jesum Christum sterb' ich nur!", "tokens": ["Auf", "Je\u00b7sum", "Chris\u00b7tum", "sterb'", "ich", "nur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "So tr\u00e4ufle Ruh' und Frieden,", "tokens": ["So", "tr\u00e4uf\u00b7le", "Ruh'", "und", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O Gott! auf alle M\u00fcden", "tokens": ["O", "Gott", "!", "auf", "al\u00b7le", "M\u00fc\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$.", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vom stillen Mond herab;", "tokens": ["Vom", "stil\u00b7len", "Mond", "her\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In sanftem S\u00e4useln falle", "tokens": ["In", "sanf\u00b7tem", "S\u00e4u\u00b7seln", "fal\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dein s\u00fc\u00dfer Schlaf auf alle", "tokens": ["Dein", "s\u00fc\u00b7\u00dfer", "Schlaf", "auf", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die m\u00fcden Pilgrime zum Grab.", "tokens": ["Die", "m\u00fc\u00b7den", "Pil\u00b7gri\u00b7me", "zum", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.11": {"line.1": {"text": "Den Sklaven wilder L\u00fcste", "tokens": ["Den", "Skla\u00b7ven", "wil\u00b7der", "L\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In wasserloser W\u00fcste", "tokens": ["In", "was\u00b7ser\u00b7lo\u00b7ser", "W\u00fcs\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "N