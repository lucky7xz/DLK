{"textgrid.poem.24726": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wo kleine Felsen, kleine Fichten", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo kleine Felsen, kleine Fichten", "tokens": ["Wo", "klei\u00b7ne", "Fel\u00b7sen", ",", "klei\u00b7ne", "Fich\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gegen freien Himmel stehen,", "tokens": ["Ge\u00b7gen", "frei\u00b7en", "Him\u00b7mel", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6nnt ihr kommen, k\u00f6nnt ihr sehen,", "tokens": ["K\u00f6nnt", "ihr", "kom\u00b7men", ",", "k\u00f6nnt", "ihr", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie wir, trunken von Gedichten,", "tokens": ["Wie", "wir", ",", "trun\u00b7ken", "von", "Ge\u00b7dich\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kindlich schmale Pfade wandern.", "tokens": ["Kind\u00b7lich", "schma\u00b7le", "Pfa\u00b7de", "wan\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sind nicht wir vor allen andern", "tokens": ["Sind", "nicht", "wir", "vor", "al\u00b7len", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "PPER", "APPR", "PIAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch die unber\u00fchrten Kinder?", "tokens": ["Doch", "die", "un\u00b7be\u00b7r\u00fchr\u00b7ten", "Kin\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sind es nicht die Knaben minder", "tokens": ["Sind", "es", "nicht", "die", "Kna\u00b7ben", "min\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "ART", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und die M\u00e4dchen, jene andern?", "tokens": ["Und", "die", "M\u00e4d\u00b7chen", ",", "je\u00b7ne", "an\u00b7dern", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PDAT", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Sind sie wahr in ihren Spielen,", "tokens": ["Sind", "sie", "wahr", "in", "ih\u00b7ren", "Spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Jene andern, jene vielen?", "tokens": ["Je\u00b7ne", "an\u00b7dern", ",", "je\u00b7ne", "vie\u00b7len", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "$,", "PDAT", "PIAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}