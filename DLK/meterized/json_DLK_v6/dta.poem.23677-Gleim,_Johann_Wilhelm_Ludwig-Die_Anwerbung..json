{"dta.poem.23677": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Anwerbung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo wilst du k&#252;nftig wohnen?", "tokens": ["Wo", "wilst", "du", "k", "&#252;", "nf\u00b7tig", "woh\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVFIN", "$(", "ADJD", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Im Garten meines M\u00e4dchen.", "tokens": ["Im", "Gar\u00b7ten", "mei\u00b7nes", "M\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo wilst du k\u00fcnftig schlafen?", "tokens": ["Wo", "wilst", "du", "k\u00fcnf\u00b7tig", "schla\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Arme meines M\u00e4dchen.", "tokens": ["Im", "Ar\u00b7me", "mei\u00b7nes", "M\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wo wilst du k\u00fcnftig spielen?", "tokens": ["Wo", "wilst", "du", "k\u00fcnf\u00b7tig", "spie\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "An meinem ganzen M\u00e4dchen.", "tokens": ["An", "mei\u00b7nem", "gan\u00b7zen", "M\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wo wilst du k\u00fcnftig sterben?", "tokens": ["Wo", "wilst", "du", "k\u00fcnf\u00b7tig", "ster\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Auf seinem sanften Schoosse.", "tokens": ["Auf", "sei\u00b7nem", "sanf\u00b7ten", "Schoos\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Wer soll dich denn begraben?", "tokens": ["Wer", "soll", "dich", "denn", "be\u00b7gra\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Mein M\u00e4dchen und sein Enkel.", "tokens": ["Mein", "M\u00e4d\u00b7chen", "und", "sein", "En\u00b7kel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Wo hast du denn das M\u00e4dchen?", "tokens": ["Wo", "hast", "du", "denn", "das", "M\u00e4d\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Wo habt ihr denn die Tochter?", "tokens": ["Wo", "habt", "ihr", "denn", "die", "Toch\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}