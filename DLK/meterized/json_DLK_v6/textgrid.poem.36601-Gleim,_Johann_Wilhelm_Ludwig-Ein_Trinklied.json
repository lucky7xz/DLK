{"textgrid.poem.36601": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Ein Trinklied", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was soll die Zauberei? ihr Br\u00fcder!", "tokens": ["Was", "soll", "die", "Zau\u00b7be\u00b7rei", "?", "ihr", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "$.", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kurz ist die Stunde, singet Lieder", "tokens": ["Kurz", "ist", "die", "Stun\u00b7de", ",", "sin\u00b7get", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "VVFIN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und trinkt und leert da volle Fa\u00df!", "tokens": ["Und", "trinkt", "und", "leert", "da", "vol\u00b7le", "Fa\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Zeit hat allzustarke Schwingen,", "tokens": ["Die", "Zeit", "hat", "all\u00b7zu\u00b7star\u00b7ke", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wer kann sie halten! La\u00dft uns singen,", "tokens": ["Wer", "kann", "sie", "hal\u00b7ten", "!", "La\u00dft", "uns", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "$.", "VVIMP", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein jeder f\u00fclle sein Glas!", "tokens": ["Ein", "je\u00b7der", "f\u00fcl\u00b7le", "sein", "Glas", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Kurz ist die Stunde! diesem Weine", "tokens": ["Kurz", "ist", "die", "Stun\u00b7de", "!", "die\u00b7sem", "Wei\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$.", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gab unser guter Wirt nur Eine,", "tokens": ["Gab", "un\u00b7ser", "gu\u00b7ter", "Wirt", "nur", "Ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "ART", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nur eine gab er Einem Fa\u00df!", "tokens": ["Nur", "ei\u00b7ne", "gab", "er", "Ei\u00b7nem", "Fa\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der uns die Eine nur gegeben,", "tokens": ["Der", "uns", "die", "Ei\u00b7ne", "nur", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ART", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der soll noch hundert Jahre leben,", "tokens": ["Der", "soll", "noch", "hun\u00b7dert", "Jah\u00b7re", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein jeder leere sein Glas!", "tokens": ["Ein", "je\u00b7der", "lee\u00b7re", "sein", "Glas", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Ein jeder hat sein Glas geleeret,", "tokens": ["Ein", "je\u00b7der", "hat", "sein", "Glas", "ge\u00b7lee\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur der nicht, dem der Wein geh\u00f6ret,", "tokens": ["Nur", "der", "nicht", ",", "dem", "der", "Wein", "ge\u00b7h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "PTKNEG", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "O b\u00f6ser Wirt! was hei\u00dfet das?", "tokens": ["O", "b\u00f6\u00b7ser", "Wirt", "!", "was", "hei\u00b7\u00dfet", "das", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "PWS", "VVFIN", "PDS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Soll dein Exempel uns verf\u00fchren?", "tokens": ["Soll", "dein", "Ex\u00b7em\u00b7pel", "uns", "ver\u00b7f\u00fch\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Kein Augenblick ist zu verlieren.", "tokens": ["Kein", "Au\u00b7gen\u00b7blick", "ist", "zu", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein jeder f\u00fclle sein Glas!", "tokens": ["Ein", "je\u00b7der", "f\u00fcl\u00b7le", "sein", "Glas", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Wir trinken, unsern Durst zu stillen;", "tokens": ["Wir", "trin\u00b7ken", ",", "un\u00b7sern", "Durst", "zu", "stil\u00b7len", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Gl\u00e4ser leeren, wieder f\u00fcllen,", "tokens": ["Die", "Gl\u00e4\u00b7ser", "lee\u00b7ren", ",", "wie\u00b7der", "f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und wieder leeren, leert das Fa\u00df!", "tokens": ["Und", "wie\u00b7der", "lee\u00b7ren", ",", "leert", "das", "Fa\u00df", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das leere Fa\u00df bek\u00f6mmst du wieder,", "tokens": ["Das", "lee\u00b7re", "Fa\u00df", "be\u00b7k\u00f6mmst", "du", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Herr Wirt, Geduld! o Br\u00fcder! Br\u00fcder!", "tokens": ["Herr", "Wirt", ",", "Ge\u00b7duld", "!", "o", "Br\u00fc\u00b7der", "!", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "$.", "FM", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein jeder leere sein Glas!", "tokens": ["Ein", "je\u00b7der", "lee\u00b7re", "sein", "Glas", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Die Zeit hat allzuschnelle Schwingen,", "tokens": ["Die", "Zeit", "hat", "all\u00b7zu\u00b7schnel\u00b7le", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Augenblick ist zu versingen,", "tokens": ["Kein", "Au\u00b7gen\u00b7blick", "ist", "zu", "ver\u00b7sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Trinkt, Br\u00fcder, trinkt, bezwingt das Fa\u00df!", "tokens": ["Trinkt", ",", "Br\u00fc\u00b7der", ",", "trinkt", ",", "be\u00b7zwingt", "das", "Fa\u00df", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und du Gesang, den Bacchuss\u00f6hnen", "tokens": ["Und", "du", "Ge\u00b7sang", ",", "den", "Bac\u00b7chus\u00b7s\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gef\u00e4hrlich, weg mit deinen T\u00f6nen!", "tokens": ["Ge\u00b7f\u00e4hr\u00b7lich", ",", "weg", "mit", "dei\u00b7nen", "T\u00f6\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Am besten t\u00f6net das Glas!", "tokens": ["Am", "bes\u00b7ten", "t\u00f6\u00b7net", "das", "Glas", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}