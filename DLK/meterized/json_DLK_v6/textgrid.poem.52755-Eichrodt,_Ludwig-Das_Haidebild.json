{"textgrid.poem.52755": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Das Haidebild", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zigeuner mit alten Geigen,", "tokens": ["Zi\u00b7geu\u00b7ner", "mit", "al\u00b7ten", "Gei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Grafen mit schwarzen Dirnen,", "tokens": ["Gra\u00b7fen", "mit", "schwar\u00b7zen", "Dir\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Reizender, schamloser Reigen,", "tokens": ["Rei\u00b7zen\u00b7der", ",", "scham\u00b7lo\u00b7ser", "Rei\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Finsteres, sprechendes Schweigen,", "tokens": ["Fins\u00b7te\u00b7res", ",", "spre\u00b7chen\u00b7des", "Schwei\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Schwei\u00dfperlende Stirnen.", "tokens": ["Schwei\u00df\u00b7per\u00b7len\u00b7de", "Stir\u00b7nen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+---+-", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Unheimlich alte Weiber,", "tokens": ["Un\u00b7heim\u00b7lich", "al\u00b7te", "Wei\u00b7ber", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gl\u00fchende Gluthweinbecher,", "tokens": ["Gl\u00fc\u00b7hen\u00b7de", "Gluth\u00b7wein\u00b7be\u00b7cher", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Zotende Rossetreiber,", "tokens": ["Zo\u00b7ten\u00b7de", "Ros\u00b7se\u00b7trei\u00b7ber", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Mitunter R\u00e4uber,", "tokens": ["Mi\u00b7tun\u00b7ter", "R\u00e4u\u00b7ber", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Unma\u00dfgeblicher Beschreiber,", "tokens": ["Un\u00b7ma\u00df\u00b7geb\u00b7li\u00b7cher", "Be\u00b7schrei\u00b7ber", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Je interessanter je frecher.", "tokens": ["Je", "in\u00b7ter\u00b7es\u00b7san\u00b7ter", "je", "fre\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ADJD", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}