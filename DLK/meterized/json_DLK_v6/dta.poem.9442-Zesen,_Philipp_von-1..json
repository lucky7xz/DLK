{"dta.poem.9442": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Liebster ist wei\u00dflecht mit Rosen besprenget/", "tokens": ["Mein", "Liebs\u00b7ter", "ist", "wei\u00df\u00b7lecht", "mit", "Ro\u00b7sen", "be\u00b7spren\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "APPR", "NN", "VVFIN", "$("], "meter": "-+--++-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der unter viel tausenden leuchtet erf\u00fcr/", "tokens": ["Der", "un\u00b7ter", "viel", "tau\u00b7sen\u00b7den", "leuch\u00b7tet", "er\u00b7f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "ADJA", "VVFIN", "NE", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Am g\u00fcldenen henpte stolzieret und h\u00e4nget", "tokens": ["Am", "g\u00fcl\u00b7de\u00b7nen", "henp\u00b7te", "stol\u00b7zie\u00b7ret", "und", "h\u00e4n\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der schw\u00e4rtzlichten Haare gestam\u0303ete zier/", "tokens": ["Der", "schw\u00e4rtz\u00b7lich\u00b7ten", "Haa\u00b7re", "ge\u00b7stam\u0303e\u00b7te", "zier", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Die artlich erhaben", "tokens": ["Die", "art\u00b7lich", "er\u00b7ha\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "und gehen den Raben", "tokens": ["und", "ge\u00b7hen", "den", "Ra\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "An farbe noch f\u00fcr:", "tokens": ["An", "far\u00b7be", "noch", "f\u00fcr", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "APPR", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Die augen im dunckeln", "tokens": ["Die", "au\u00b7gen", "im", "dun\u00b7ckeln"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Nach Tauben-art funckeln", "tokens": ["Nach", "Tau\u00b7ben\u00b7art", "fun\u00b7ckeln"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Mit Milche gewaschen/ in v\u00f6lliger zier.", "tokens": ["Mit", "Mil\u00b7che", "ge\u00b7wa\u00b7schen", "/", "in", "v\u00f6l\u00b7li\u00b7ger", "zier", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$(", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Den Beeten/ da allerley Blumen auf schie\u00dfen/", "tokens": ["Den", "Bee\u00b7ten", "/", "da", "al\u00b7ler\u00b7ley", "Blu\u00b7men", "auf", "schie\u00b7\u00dfen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "PIAT", "NN", "APPR", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Seyn \u00e4hnlich die Wangen und gehen noch f\u00fcr/", "tokens": ["Seyn", "\u00e4hn\u00b7lich", "die", "Wan\u00b7gen", "und", "ge\u00b7hen", "noch", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ART", "NN", "KON", "VVFIN", "ADV", "APPR", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die Lippen wie Rosen/ da Myrrhen aufflie\u00dfen/", "tokens": ["Die", "Lip\u00b7pen", "wie", "Ro\u00b7sen", "/", "da", "Myr\u00b7rhen", "auf\u00b7flie\u00b7\u00dfen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "NN", "$(", "KOUS", "NN", "VVINF", "$("], "meter": "-+--+--++-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Seyn jmmer erf\u00fcllet mit freundlicher zier.", "tokens": ["Seyn", "jm\u00b7mer", "er\u00b7f\u00fcl\u00b7let", "mit", "freund\u00b7li\u00b7cher", "zier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Die Finger ingleichen", "tokens": ["Die", "Fin\u00b7ger", "in\u00b7glei\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Den Ringen nicht weichen", "tokens": ["Den", "Rin\u00b7gen", "nicht", "wei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PTKNEG", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Mit Steinen geziert;", "tokens": ["Mit", "Stei\u00b7nen", "ge\u00b7ziert", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Wie sch\u00f6ne Saffieren", "tokens": ["Wie", "sch\u00f6\u00b7ne", "Saf\u00b7fie\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Das Helffenbein zieren/", "tokens": ["Das", "Helf\u00b7fen\u00b7bein", "zie\u00b7ren", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Solch Gl\u00e4ntzen am Leibe mein Leben auch f\u00fchre.", "tokens": ["Solch", "Gl\u00e4nt\u00b7zen", "am", "Lei\u00b7be", "mein", "Le\u00b7ben", "auch", "f\u00fch\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Die Beine wie Marmor mit Golde geschm\u00fccket", "tokens": ["Die", "Bei\u00b7ne", "wie", "Mar\u00b7mor", "mit", "Gol\u00b7de", "ge\u00b7schm\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KOKOM", "NN", "APPR", "NN", "VVPP"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Seyn hurtig zum tantzen und sauber wie schn\u00e9e/", "tokens": ["Seyn", "hur\u00b7tig", "zum", "tant\u00b7zen", "und", "sau\u00b7ber", "wie", "schn\u00e9e", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "APPRART", "VVINF", "KON", "ADJD", "KOKOM", "VVFIN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Sein lieblich Anschauen mich eben anblicket", "tokens": ["Sein", "lieb\u00b7lich", "An\u00b7schau\u00b7en", "mich", "e\u00b7ben", "an\u00b7bli\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "NN", "PPER", "ADV", "VVFIN"], "meter": "-+-++--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wie Libanons Cedern und herrliche H\u00f6h.", "tokens": ["Wie", "Li\u00b7ba\u00b7nons", "Ce\u00b7dern", "und", "herr\u00b7li\u00b7che", "H\u00f6h", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}