{"dta.poem.3142": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "187.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sonst da mich jeder schalt, und keiner fast mich lobte,", "tokens": ["Sonst", "da", "mich", "je\u00b7der", "schalt", ",", "und", "kei\u00b7ner", "fast", "mich", "lob\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PIS", "VVFIN", "$,", "KON", "PIS", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich dachte Wunder welch ein Ungl\u00fcck ich erprobte.", "tokens": ["Ich", "dach\u00b7te", "Wun\u00b7der", "welch", "ein", "Un\u00b7gl\u00fcck", "ich", "er\u00b7prob\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PWAT", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nun jeder fast mich lobt, und keiner mehr mich schilt;", "tokens": ["Nun", "je\u00b7der", "fast", "mich", "lobt", ",", "und", "kei\u00b7ner", "mehr", "mich", "schilt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "VVFIN", "$,", "KON", "PIS", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht wenig kostet mich, was mir so wenig gilt.", "tokens": ["Nicht", "we\u00b7nig", "kos\u00b7tet", "mich", ",", "was", "mir", "so", "we\u00b7nig", "gilt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "PPER", "$,", "PWS", "PPER", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Denn wenn ich durfte sonst doch, die mich schalten, schelten,", "tokens": ["Denn", "wenn", "ich", "durf\u00b7te", "sonst", "doch", ",", "die", "mich", "schal\u00b7ten", ",", "schel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VMFIN", "ADV", "ADV", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So mu\u00df ich jetzo, die mich loben, lassen gelten.", "tokens": ["So", "mu\u00df", "ich", "jet\u00b7zo", ",", "die", "mich", "lo\u00b7ben", ",", "las\u00b7sen", "gel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "$,", "PRELS", "PPER", "VVINF", "$,", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}