{"textgrid.poem.43764": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Heute will ich fr\u00f6hlich sein,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heute will ich fr\u00f6hlich sein,", "tokens": ["Heu\u00b7te", "will", "ich", "fr\u00f6h\u00b7lich", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Heute guter Dinge,", "tokens": ["Heu\u00b7te", "gu\u00b7ter", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und auch du, mein D\u00f6ckelein,", "tokens": ["Und", "auch", "du", ",", "mein", "D\u00f6\u00b7ckel\u00b7ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Komm' und tanz' und springe!", "tokens": ["Komm'", "und", "tanz'", "und", "sprin\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Ist der heil'ge Christ ins Haus", "tokens": ["Ist", "der", "heil'\u00b7ge", "Christ", "ins", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Heut schon eingekehret?", "tokens": ["Heut", "schon", "ein\u00b7ge\u00b7keh\u00b7ret", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Hat der heil'ge Nikolaus", "tokens": ["Hat", "der", "heil'\u00b7ge", "Ni\u00b7ko\u00b7laus"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Uns was einbescheeret?", "tokens": ["Uns", "was", "ein\u00b7be\u00b7schee\u00b7ret", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Nein, so h\u00f6rt es denn von mir,", "tokens": ["Nein", ",", "so", "h\u00f6rt", "es", "denn", "von", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6rt es, lieben Leute!", "tokens": ["H\u00f6rt", "es", ",", "lie\u00b7ben", "Leu\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Den Geburtstag feiern wir", "tokens": ["Den", "Ge\u00b7burts\u00b7tag", "fei\u00b7ern", "wir"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unsers Papa's heute.", "tokens": ["Un\u00b7sers", "Pa\u00b7pa's", "heu\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Darum will ich fr\u00f6hlich sein,", "tokens": ["Da\u00b7rum", "will", "ich", "fr\u00f6h\u00b7lich", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hei\u00dfa! guter Dinge,", "tokens": ["Hei\u00b7\u00dfa", "!", "gu\u00b7ter", "Din\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und auch du, mein D\u00f6ckelein,", "tokens": ["Und", "auch", "du", ",", "mein", "D\u00f6\u00b7ckel\u00b7ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Komm' und tanz' und springe!", "tokens": ["Komm'", "und", "tanz'", "und", "sprin\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}