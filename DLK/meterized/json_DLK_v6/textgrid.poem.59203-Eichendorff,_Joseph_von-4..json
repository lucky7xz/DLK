{"textgrid.poem.59203": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer einmal tief und durstig hat getrunken,", "tokens": ["Wer", "ein\u00b7mal", "tief", "und", "durs\u00b7tig", "hat", "ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "KON", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den zieht zu sich hinab die Wunderquelle,", "tokens": ["Den", "zieht", "zu", "sich", "hin\u00b7ab", "die", "Wun\u00b7der\u00b7quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PRF", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df er melodisch mitzieht, selbst als Welle,", "tokens": ["Da\u00df", "er", "me\u00b7lo\u00b7disch", "mit\u00b7zieht", ",", "selbst", "als", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "$,", "ADV", "KOUS", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Auf der die Welt sich bricht in tausend Funken.", "tokens": ["Auf", "der", "die", "Welt", "sich", "bricht", "in", "tau\u00b7send", "Fun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "PRF", "VVFIN", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es w\u00e4chst sehns\u00fcchtig, st\u00fcrzt und leuchtet trunken", "tokens": ["Es", "w\u00e4chst", "sehn\u00b7s\u00fcch\u00b7tig", ",", "st\u00fcrzt", "und", "leuch\u00b7tet", "trun\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "VVFIN", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Jauchzend im Innersten die heil'ge Quelle,", "tokens": ["Jauch\u00b7zend", "im", "In\u00b7ners\u00b7ten", "die", "heil'\u00b7ge", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+---+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Bald Bahn sich brechend durch die Kluft zur Helle,", "tokens": ["Bald", "Bahn", "sich", "bre\u00b7chend", "durch", "die", "Kluft", "zur", "Hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PRF", "ADJD", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bald k\u00fchle rauschend dann in Nacht versunken.", "tokens": ["Bald", "k\u00fch\u00b7le", "rau\u00b7schend", "dann", "in", "Nacht", "ver\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So la\u00df es ungeduldig brausen, dr\u00e4ngen!", "tokens": ["So", "la\u00df", "es", "un\u00b7ge\u00b7dul\u00b7dig", "brau\u00b7sen", ",", "dr\u00e4n\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADJD", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hoch schwebt der Dichter drauf in goldnem Nachen,", "tokens": ["Hoch", "schwebt", "der", "Dich\u00b7ter", "drauf", "in", "gold\u00b7nem", "Na\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PAV", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Sich selber heilig opfernd in Ges\u00e4ngen.", "tokens": ["Sich", "sel\u00b7ber", "hei\u00b7lig", "op\u00b7fernd", "in", "Ge\u00b7s\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADJD", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die alten Felsen spalten sich mit Krachen,", "tokens": ["Die", "al\u00b7ten", "Fel\u00b7sen", "spal\u00b7ten", "sich", "mit", "Kra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von dr\u00fcben gr\u00fc\u00dfen schon verwandte Lieder,", "tokens": ["Von", "dr\u00fc\u00b7ben", "gr\u00fc\u00b7\u00dfen", "schon", "ver\u00b7wand\u00b7te", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zum ew'gen Meere f\u00fchrt er alle wieder.", "tokens": ["Zum", "ew'\u00b7gen", "Mee\u00b7re", "f\u00fchrt", "er", "al\u00b7le", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}