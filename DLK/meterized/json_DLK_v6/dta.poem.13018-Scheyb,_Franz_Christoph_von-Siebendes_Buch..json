{"dta.poem.13018": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In solcher Aenderung befande sich der Saal,", "tokens": ["In", "sol\u00b7cher", "A\u00b7en\u00b7de\u00b7rung", "be\u00b7fan\u00b7de", "sich", "der", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Bis jemand von dem Rath Gelassenheit befahl.", "tokens": ["Bis", "je\u00b7mand", "von", "dem", "Rath", "Ge\u00b7las\u00b7sen\u00b7heit", "be\u00b7fahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es war die Majest\u00e4t, die mit erhabnen Blicken", "tokens": ["Es", "war", "die", "Ma\u00b7jes\u00b7t\u00e4t", ",", "die", "mit", "er\u00b7hab\u00b7nen", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Streit zu stillen wu\u00dft, die Zungen zu bestricken.", "tokens": ["Den", "Streit", "zu", "stil\u00b7len", "wu\u00dft", ",", "die", "Zun\u00b7gen", "zu", "be\u00b7stri\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jhr Ernst-erf\u00fclltes Aug in ihrer Hoheit Pracht", "tokens": ["Ihr", "Ernst\u00b7er\u00b7f\u00fcll\u00b7tes", "Aug", "in", "ih\u00b7rer", "Ho\u00b7heit", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "90Wand sich im Krei\u00df herum und nahm den Streit in acht,", "tokens": ["sich", "im", "Krei\u00df", "he\u00b7rum", "und", "nahm", "den", "Streit", "in", "acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "PTKVZ", "KON", "VVFIN", "ART", "NN", "APPR", "CARD", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "Verfinsterte die Stirn, und winckte mit den H\u00e4nden,", "tokens": ["Ver\u00b7fins\u00b7ter\u00b7te", "die", "Stirn", ",", "und", "win\u00b7ck\u00b7te", "mit", "den", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Man solle des Gespr\u00e4chs Unartigkeiten enden.", "tokens": ["Man", "sol\u00b7le", "des", "Ge\u00b7spr\u00e4chs", "Un\u00b7ar\u00b7tig\u00b7kei\u00b7ten", "en\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So tief man im Geschw\u00e4z, und in dem Eifer war,", "tokens": ["So", "tief", "man", "im", "Ge\u00b7schw\u00e4z", ",", "und", "in", "dem", "Ei\u00b7fer", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PIS", "APPRART", "NN", "$,", "KON", "APPR", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So bald ward alles still, wie nach dem Regen klar;", "tokens": ["So", "bald", "ward", "al\u00b7les", "still", ",", "wie", "nach", "dem", "Re\u00b7gen", "klar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PIS", "PTKVZ", "$,", "PWAV", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "95Der Saal bequemte sich den Unfug abzulegen:", "tokens": ["Saal", "be\u00b7quem\u00b7te", "sich", "den", "Un\u00b7fug", "ab\u00b7zu\u00b7le\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ART", "NN", "VVIZU", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.12": {"text": "Jmmittelst sahen wir die ", "tokens": ["Jm\u00b7mit\u00b7telst", "sa\u00b7hen", "wir", "die"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}