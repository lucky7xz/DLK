{"textgrid.poem.28496": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einen Greiner, einen Schreyer,", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einen Greiner, einen Schreyer,", "tokens": ["Ei\u00b7nen", "Grei\u00b7ner", ",", "ei\u00b7nen", "Schre\u00b7yer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Einen Praler, einen Dr\u00e4uer,", "tokens": ["Ei\u00b7nen", "Pra\u00b7ler", ",", "ei\u00b7nen", "Dr\u00e4u\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Einen Buhler, einen Z\u00e4ncker,", "tokens": ["Ei\u00b7nen", "Buh\u00b7ler", ",", "ei\u00b7nen", "Z\u00e4n\u00b7cker", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen Balger, einen St\u00e4ncker,", "tokens": ["Ei\u00b7nen", "Bal\u00b7ger", ",", "ei\u00b7nen", "St\u00e4n\u00b7cker", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Einen Herren, einen Narren,", "tokens": ["Ei\u00b7nen", "Her\u00b7ren", ",", "ei\u00b7nen", "Nar\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Einen Richter, einen Pfarren,", "tokens": ["Ei\u00b7nen", "Rich\u00b7ter", ",", "ei\u00b7nen", "Pfar\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Einen Doctor, einen Simpel,", "tokens": ["Ei\u00b7nen", "Doc\u00b7tor", ",", "ei\u00b7nen", "Sim\u00b7pel", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Einen Witzel, einen G\u00fcmpel,", "tokens": ["Ei\u00b7nen", "Wit\u00b7zel", ",", "ei\u00b7nen", "G\u00fcm\u00b7pel", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Einen T\u00e4ntzer, einen Singer,", "tokens": ["Ei\u00b7nen", "T\u00e4nt\u00b7zer", ",", "ei\u00b7nen", "Sin\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Einen Schl\u00e4fer, einen Springer,", "tokens": ["Ei\u00b7nen", "Schl\u00e4\u00b7fer", ",", "ei\u00b7nen", "Sprin\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Einen M\u00f6rder, einen Stehler,", "tokens": ["Ei\u00b7nen", "M\u00f6r\u00b7der", ",", "ei\u00b7nen", "Steh\u00b7ler", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Einen W\u00e4scher, einen H\u00f6ler,", "tokens": ["Ei\u00b7nen", "W\u00e4\u00b7scher", ",", "ei\u00b7nen", "H\u00f6\u00b7ler", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Einen L\u00fcgner, einen Trieger,", "tokens": ["Ei\u00b7nen", "L\u00fcg\u00b7ner", ",", "ei\u00b7nen", "Trie\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Einen Schmeichler, einen R\u00fcger,", "tokens": ["Ei\u00b7nen", "Schmeich\u00b7ler", ",", "ei\u00b7nen", "R\u00fc\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Einen Flucher, einen Beter,", "tokens": ["Ei\u00b7nen", "Fluc\u00b7her", ",", "ei\u00b7nen", "Be\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Aller Laster einen Th\u00e4ter", "tokens": ["Al\u00b7ler", "Las\u00b7ter", "ei\u00b7nen", "Th\u00e4\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Hat in dem man zu erkennen,", "tokens": ["Hat", "in", "dem", "man", "zu", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PRELS", "PIS", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Den man kan versoffen nennen.", "tokens": ["Den", "man", "kan", "ver\u00b7sof\u00b7fen", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}