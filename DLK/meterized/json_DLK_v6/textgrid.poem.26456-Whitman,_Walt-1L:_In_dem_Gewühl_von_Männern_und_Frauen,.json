{"textgrid.poem.26456": {"metadata": {"author": {"name": "Whitman, Walt", "birth": "N.A.", "death": "N.A."}, "title": "1L: In dem Gew\u00fchl von M\u00e4nnern und Frauen,", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In dem Gew\u00fchl von M\u00e4nnern und Frauen,", "tokens": ["In", "dem", "Ge\u00b7w\u00fchl", "von", "M\u00e4n\u00b7nern", "und", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "im Gedr\u00e4nge der Menge", "tokens": ["im", "Ge\u00b7dr\u00e4n\u00b7ge", "der", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "f\u00fchle ich, wie durch geheime", "tokens": ["f\u00fch\u00b7le", "ich", ",", "wie", "durch", "ge\u00b7hei\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "g\u00f6ttliche Zeichen", "tokens": ["g\u00f6tt\u00b7li\u00b7che", "Zei\u00b7chen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Einer mich ausw\u00e4hlt.", "tokens": ["Ei\u00b7ner", "mich", "aus\u00b7w\u00e4hlt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "PPER", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Ihm bin ich n\u00e4her verwandt", "tokens": ["Ihm", "bin", "ich", "n\u00e4\u00b7her", "ver\u00b7wandt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVPP"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "als meinen Eltern und Geschwistern,", "tokens": ["als", "mei\u00b7nen", "El\u00b7tern", "und", "Ge\u00b7schwis\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Braut, Eheweib oder Kindern.", "tokens": ["Braut", ",", "E\u00b7he\u00b7weib", "o\u00b7der", "Kin\u00b7dern", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Andere z\u00f6gern und zweifeln noch,", "tokens": ["An\u00b7de\u00b7re", "z\u00f6\u00b7gern", "und", "zwei\u00b7feln", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVINF", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "aber dieser", "tokens": ["a\u00b7ber", "die\u00b7ser"], "token_info": ["word", "word"], "pos": ["ADV", "PDAT"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "z\u00f6gert und zweifelt nicht,", "tokens": ["z\u00f6\u00b7gert", "und", "zwei\u00b7felt", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PTKNEG", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "denn er kennt mich.", "tokens": ["denn", "er", "kennt", "mich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "O Geliebter und ganz Gleichartiger!", "tokens": ["O", "Ge\u00b7lieb\u00b7ter", "und", "ganz", "Gleich\u00b7ar\u00b7ti\u00b7ger", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "ADV", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Du wirst mich entdecken", "tokens": ["Du", "wirst", "mich", "ent\u00b7de\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "und erkennen", "tokens": ["und", "er\u00b7ken\u00b7nen"], "token_info": ["word", "word"], "pos": ["KON", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "an meinen zartesten Zeichen,", "tokens": ["an", "mei\u00b7nen", "zar\u00b7tes\u00b7ten", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "und ich will dich entdecken und erkennen,", "tokens": ["und", "ich", "will", "dich", "ent\u00b7de\u00b7cken", "und", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PRF", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "wenn wir uns begegnen,", "tokens": ["wenn", "wir", "uns", "be\u00b7geg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "an den gleichen Zeichen,", "tokens": ["an", "den", "glei\u00b7chen", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "im Gedr\u00e4nge der Menge.", "tokens": ["im", "Ge\u00b7dr\u00e4n\u00b7ge", "der", "Men\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}}}}