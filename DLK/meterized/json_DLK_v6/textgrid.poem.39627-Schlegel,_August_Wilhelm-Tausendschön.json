{"textgrid.poem.39627": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Tausendsch\u00f6n", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, herrlich ist der Griechen Alterthum,", "tokens": ["Ja", ",", "herr\u00b7lich", "ist", "der", "Grie\u00b7chen", "Al\u00b7ter\u00b7thum", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ist wahre Sch\u00f6pfung zu der Menschheit Ruhm.", "tokens": ["Ist", "wah\u00b7re", "Sch\u00f6p\u00b7fung", "zu", "der", "Menschheit", "Ruhm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Du m\u00f6chtest schildern gern in deinen dicken B\u00e4nden", "tokens": ["Du", "m\u00f6ch\u00b7test", "schil\u00b7dern", "gern", "in", "dei\u00b7nen", "di\u00b7cken", "B\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des Genius Kosmogonie;", "tokens": ["Des", "Ge\u00b7nius", "Kos\u00b7mo\u00b7go\u00b7nie", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch Ebenma\u00df, Gestalt und Ordnung triffst du nie,", "tokens": ["Doch", "E\u00b7ben\u00b7ma\u00df", ",", "Ge\u00b7stalt", "und", "Ord\u00b7nung", "triffst", "du", "nie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "KON", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und mit dem Chaos wirst du enden.", "tokens": ["Und", "mit", "dem", "Chaos", "wirst", "du", "en\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Du kannst Latein nicht ohne Drucksen sprechen:", "tokens": ["Du", "kannst", "La\u00b7tein", "nicht", "oh\u00b7ne", "Druck\u00b7sen", "spre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "PTKNEG", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Philologen steht die St\u00fcmperei nicht fein.", "tokens": ["Dem", "Phi\u00b7lo\u00b7lo\u00b7gen", "steht", "die", "St\u00fcm\u00b7pe\u00b7rei", "nicht", "fein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man h\u00f6rt dich ebenfalls das Deutsche radebrechen;", "tokens": ["Man", "h\u00f6rt", "dich", "e\u00b7ben\u00b7falls", "das", "Deut\u00b7sche", "ra\u00b7de\u00b7bre\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dir ist die Alalie nat\u00fcrlich, allgemein.", "tokens": ["Dir", "ist", "die", "A\u00b7la\u00b7lie", "na\u00b7t\u00fcr\u00b7lich", ",", "all\u00b7ge\u00b7mein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NE", "ADV", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch deine Feder schwatzt gar viel in Einem Odem", "tokens": ["Doch", "dei\u00b7ne", "Fe\u00b7der", "schwatzt", "gar", "viel", "in", "Ei\u00b7nem", "O\u00b7dem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und spinnt den leersten Satz zu langen Perioden.", "tokens": ["Und", "spinnt", "den", "leers\u00b7ten", "Satz", "zu", "lan\u00b7gen", "Pe\u00b7ri\u00b7o\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Alten, sagst du, sind im Krei\u00df", "tokens": ["Die", "Al\u00b7ten", ",", "sagst", "du", ",", "sind", "im", "Krei\u00df"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "$,", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herumgegangen.", "tokens": ["Her\u00b7um\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Kein Andrer wei\u00df,", "tokens": ["Kein", "A\u00b7ndrer", "wei\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Wie du, die Schlangen", "tokens": ["Wie", "du", ",", "die", "Schlan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PWAV", "PPER", "$,", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Bei'm Schwanz zu fangen.", "tokens": ["Bei'm", "Schwanz", "zu", "fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Von hinten les' ich's oder von vorn,", "tokens": ["Von", "hin\u00b7ten", "les'", "ich's", "o\u00b7der", "von", "vorn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PIS", "KON", "APPR", "ADV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Es bleibt mir gleicherma\u00dfen verworr'n.", "tokens": ["Es", "bleibt", "mir", "glei\u00b7cher\u00b7ma\u00b7\u00dfen", "ver\u00b7worr'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVFIN", "NE"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Wahrheit fehlt dem Mythologen,", "tokens": ["Wahr\u00b7heit", "fehlt", "dem", "My\u00b7tho\u00b7lo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kunstsinn dem Arch\u00e4ologen,", "tokens": ["Kunst\u00b7sinn", "dem", "Ar\u00b7ch\u00e4\u00b7o\u00b7lo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--++-+-", "measure": "dactylic.init"}, "line.3": {"text": "Red' und Schrift dem Philologen.", "tokens": ["Red'", "und", "Schrift", "dem", "Phi\u00b7lo\u00b7lo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}