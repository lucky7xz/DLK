{"textgrid.poem.26224": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Bis weit nach Asien zeigt das All", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bis weit nach Asien zeigt das All", "tokens": ["Bis", "weit", "nach", "A\u00b7sien", "zeigt", "das", "All"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "APPR", "NE", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des gro\u00dfen Krieges Widerhall.", "tokens": ["Des", "gro\u00b7\u00dfen", "Krie\u00b7ges", "Wi\u00b7der\u00b7hall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Sonne geht mit blassem Leibe", "tokens": ["Die", "Son\u00b7ne", "geht", "mit", "blas\u00b7sem", "Lei\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ums Atapdach.", "tokens": ["Ums", "A\u00b7tap\u00b7dach", "."], "token_info": ["word", "word", "punct"], "pos": ["KOUI", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Sie zeigt kaum noch die bange Scheibe,", "tokens": ["Sie", "zeigt", "kaum", "noch", "die", "ban\u00b7ge", "Schei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr Strahl ward schwach.", "tokens": ["Ihr", "Strahl", "ward", "schwach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Versteckt im Dunst, bleibt sie verschwunden,", "tokens": ["Ver\u00b7steckt", "im", "Dunst", ",", "bleibt", "sie", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Mittag gleicht den Abendstunden.", "tokens": ["Der", "Mit\u00b7tag", "gleicht", "den", "A\u00b7bends\u00b7tun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Fern in Europa ging durch Blut", "tokens": ["Fern", "in", "Eu\u00b7ro\u00b7pa", "ging", "durch", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NE", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ihr Strahl seit Tagen,", "tokens": ["Ihr", "Strahl", "seit", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Nun fehlt zum Leuchten ihr der Mut.", "tokens": ["Nun", "fehlt", "zum", "Leuch\u00b7ten", "ihr", "der", "Mut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Sie will nicht immer Licht nur tragen", "tokens": ["Sie", "will", "nicht", "im\u00b7mer", "Licht", "nur", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Zum T\u00f6ten und zum Wundenschlagen.", "tokens": ["Zum", "T\u00f6\u00b7ten", "und", "zum", "Wun\u00b7den\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Sie macht den Weg tief grau verh\u00fcllt,", "tokens": ["Sie", "macht", "den", "Weg", "tief", "grau", "ver\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Bis da\u00df der Friede sich erf\u00fcllt.", "tokens": ["Bis", "da\u00df", "der", "Frie\u00b7de", "sich", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}