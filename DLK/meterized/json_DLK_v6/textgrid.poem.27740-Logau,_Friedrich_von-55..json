{"textgrid.poem.27740": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "55.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kunimundus giebt sich an,", "tokens": ["Ku\u00b7ni\u00b7mun\u00b7dus", "giebt", "sich", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Manche Stunde seinen Mann", "tokens": ["Man\u00b7che", "Stun\u00b7de", "sei\u00b7nen", "Mann"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu bestehen, das ist viel.", "tokens": ["Zu", "be\u00b7ste\u00b7hen", ",", "das", "ist", "viel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PDS", "VAFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "O es ist bedinget worden,", "tokens": ["O", "es", "ist", "be\u00b7din\u00b7get", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VAFIN", "VVPP", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df er weder selbst ermorden,", "tokens": ["Da\u00df", "er", "we\u00b7der", "selbst", "er\u00b7mor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KON", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Noch ermordet werden wil.", "tokens": ["Noch", "er\u00b7mor\u00b7det", "wer\u00b7den", "wil", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}