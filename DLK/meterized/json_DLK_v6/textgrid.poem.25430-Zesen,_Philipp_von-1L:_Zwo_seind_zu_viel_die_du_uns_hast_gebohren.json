{"textgrid.poem.25430": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zwo seind zu viel/ die du uns hast gebohren/", "genre": "verse", "period": "N.A.", "pub_year": 1654, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwo seind zu viel/ die du uns hast gebohren/", "tokens": ["Zwo", "seind", "zu", "viel", "/", "die", "du", "uns", "hast", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PIAT", "$(", "PRELS", "PPER", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ver\u00e4nderung. Verlohren/", "tokens": ["Ver\u00b7\u00e4n\u00b7de\u00b7rung", ".", "Ver\u00b7loh\u00b7ren", "/"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "verlohren geht die Freude/ wann das leiden/", "tokens": ["ver\u00b7loh\u00b7ren", "geht", "die", "Freu\u00b7de", "/", "wann", "das", "lei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "$(", "PWAV", "PDS", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "das bast\u00e4rt/ sie macht scheiden.", "tokens": ["das", "bas\u00b7t\u00e4rt", "/", "sie", "macht", "schei\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$(", "PPER", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein kind ist guht:", "tokens": ["Ein", "kind", "ist", "guht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "das andre st\u00fcrmt/ und t\u00e4mmt den muht.", "tokens": ["das", "and\u00b7re", "st\u00fcrmt", "/", "und", "t\u00e4mmt", "den", "muht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$(", "KON", "VVFIN", "ART", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Bringt uns ein mahnd der Mannen und der M\u00e4rsen", "tokens": ["Bringt", "uns", "ein", "mahnd", "der", "Man\u00b7nen", "und", "der", "M\u00e4r\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "VVFIN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die fr\u00fche lilj' aus Persen;", "tokens": ["die", "fr\u00fc\u00b7he", "lil\u00b7j'", "aus", "Per\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "so folgen nach die bunten oster-wochen/", "tokens": ["so", "fol\u00b7gen", "nach", "die", "bun\u00b7ten", "os\u00b7ter\u00b7wo\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und kommen aus-gebrochen", "tokens": ["und", "kom\u00b7men", "aus\u00b7ge\u00b7bro\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVINF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "in froher tracht", "tokens": ["in", "fro\u00b7her", "tracht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "mit vielgef\u00e4rbter tulpen-tracht.", "tokens": ["mit", "viel\u00b7ge\u00b7f\u00e4rb\u00b7ter", "tul\u00b7pen\u00b7tracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Drauf brechen an des Rosen-mahndes l\u00fcste/", "tokens": ["Drauf", "bre\u00b7chen", "an", "des", "Ro\u00b7sen\u00b7mahn\u00b7des", "l\u00fcs\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die zier der frohen br\u00fcste/", "tokens": ["die", "zier", "der", "fro\u00b7hen", "br\u00fcs\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "ADJA", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "in keuscher schaam mit milch' und bluht bespr\u00fchet:", "tokens": ["in", "keu\u00b7scher", "schaam", "mit", "milch'", "und", "bluht", "be\u00b7spr\u00fc\u00b7het", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NE", "KON", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "dan milcht die ros' und bl\u00fchet;", "tokens": ["dan", "milcht", "die", "ros'", "und", "bl\u00fc\u00b7het", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NE", "KON", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "dan bluhten sie", "tokens": ["dan", "bluh\u00b7ten", "sie"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "die lieblichen/ die sch\u00f6nen Rosen die.", "tokens": ["die", "lieb\u00b7li\u00b7chen", "/", "die", "sch\u00f6\u00b7nen", "Ro\u00b7sen", "die", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$(", "ART", "ADJA", "NN", "ART", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sie tauren noch am l\u00e4ngsten unter allen/", "tokens": ["Sie", "tau\u00b7ren", "noch", "am", "l\u00e4ngs\u00b7ten", "un\u00b7ter", "al\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJA", "APPR", "PIAT", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und wan sie ja verfallen;", "tokens": ["und", "wan", "sie", "ja", "ver\u00b7fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "dan fallen auch darnieder alle freuden/", "tokens": ["dan", "fal\u00b7len", "auch", "dar\u00b7nie\u00b7der", "al\u00b7le", "freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PAV", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "ach weh! verkehrt in leiden.", "tokens": ["ach", "weh", "!", "ver\u00b7kehrt", "in", "lei\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$.", "VVFIN", "APPR", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "dan stirbt zur stund", "tokens": ["dan", "stirbt", "zur", "stund"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN"], "meter": "---+", "measure": "unknown.measure.single"}, "line.6": {"text": "die gantze lust mit Rosemund.", "tokens": ["die", "gant\u00b7ze", "lust", "mit", "Ro\u00b7se\u00b7mund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}