{"dta.poem.20647": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "1763 .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus allen himmlischen Bezirken                   ", "tokens": ["Aus", "al\u00b7len", "himm\u00b7li\u00b7schen", "Be\u00b7zir\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Versammleten die B\u00fcrger j\u00fcngst", "tokens": ["Ver\u00b7samm\u00b7le\u00b7ten", "die", "B\u00fcr\u00b7ger", "j\u00fcngst"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich in der Luft zu sehn, wie Du mit jungen T\u00fcrken", "tokens": ["Sich", "in", "der", "Luft", "zu", "sehn", ",", "wie", "Du", "mit", "jun\u00b7gen", "T\u00fcr\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,", "PWAV", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Bacchanalien begingst.", "tokens": ["Die", "Bac\u00b7cha\u00b7na\u00b7li\u00b7en", "be\u00b7gingst", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du, der bey rauschenden Pok\u00e4len", "tokens": ["Du", ",", "der", "bey", "rau\u00b7schen\u00b7den", "Po\u00b7k\u00e4\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So tapfer ist, und so ge\u00fcbt", "tokens": ["So", "tap\u00b7fer", "ist", ",", "und", "so", "ge\u00b7\u00fcbt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "$,", "KON", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als einer Legion den Angriff zu befehlen,", "tokens": ["Als", "ei\u00b7ner", "Le\u00b7gi\u00b7on", "den", "An\u00b7griff", "zu", "be\u00b7feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn Mars die Loosung schrecklich giebt.", "tokens": ["Wenn", "Mars", "die", "Loo\u00b7sung", "schreck\u00b7lich", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du, von dem K\u00f6nig und dem Volke", "tokens": ["Du", ",", "von", "dem", "K\u00f6\u00b7nig", "und", "dem", "Vol\u00b7ke"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "APPR", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zugleich geliebter ", "tokens": ["Zu\u00b7gleich", "ge\u00b7lieb\u00b7ter"], "token_info": ["word", "word"], "pos": ["ADV", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ob nicht Ly\u00e4us selbst in einer Weinduftwolke", "tokens": ["Ob", "nicht", "Ly\u00b7\u00e4us", "selbst", "in", "ei\u00b7ner", "Wein\u00b7duft\u00b7wol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "NE", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Umschwebte Deinen Tisch und Dich?", "tokens": ["Um\u00b7schweb\u00b7te", "Dei\u00b7nen", "Tisch", "und", "Dich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ob nicht der sch\u00f6ne G\u00f6tterschenke", "tokens": ["Ob", "nicht", "der", "sch\u00f6\u00b7ne", "G\u00f6t\u00b7ter\u00b7schen\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In Deinem Achmet sich versteckt,", "tokens": ["In", "Dei\u00b7nem", "Ach\u00b7met", "sich", "ver\u00b7steckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Eure Becher voll gegossen mit Getr\u00e4nke,", "tokens": ["Der", "Eu\u00b7re", "Be\u00b7cher", "voll", "ge\u00b7gos\u00b7sen", "mit", "Ge\u00b7tr\u00e4n\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das k\u00f6stlich wie der Nektar schmeckt.", "tokens": ["Das", "k\u00f6st\u00b7lich", "wie", "der", "Nek\u00b7tar", "schmeckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Muthemp\u00f6rende Trompete,", "tokens": ["Die", "Mut\u00b7hem\u00b7p\u00f6\u00b7ren\u00b7de", "Trom\u00b7pe\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der l\u00e4rmerischen Pauken Schall", "tokens": ["Der", "l\u00e4r\u00b7me\u00b7ri\u00b7schen", "Pau\u00b7ken", "Schall"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Beflammten das Gesicht mit hoher Purpurr\u00f6the,", "tokens": ["Be\u00b7flamm\u00b7ten", "das", "Ge\u00b7sicht", "mit", "ho\u00b7her", "Pur\u00b7pur\u00b7r\u00f6\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als h\u00f6rtet ihr Kanonenknall.", "tokens": ["Als", "h\u00f6r\u00b7tet", "ihr", "Ka\u00b7no\u00b7nen\u00b7knall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ihr spranget auf mit Trinkerhitze,", "tokens": ["Ihr", "spran\u00b7get", "auf", "mit", "Trin\u00b7ker\u00b7hit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So sprang der Thracier, und flog", "tokens": ["So", "sprang", "der", "Thra\u00b7cier", ",", "und", "flog"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Voll Feuer in der Stirn bis an des Heeres Spitze,", "tokens": ["Voll", "Feu\u00b7er", "in", "der", "Stirn", "bis", "an", "des", "Hee\u00b7res", "Spit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "ART", "NN", "APPR", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das feindlich ihm entgegen zog.", "tokens": ["Das", "feind\u00b7lich", "ihm", "ent\u00b7ge\u00b7gen", "zog", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "PPER", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ihr folgt mit Fackeln durch die Stra\u00dfen;", "tokens": ["Ihr", "folgt", "mit", "Fa\u00b7ckeln", "durch", "die", "Stra\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein junger Hirsch fliegt kaum so schnell,", "tokens": ["Ein", "jun\u00b7ger", "Hirsch", "fliegt", "kaum", "so", "schnell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der stolz auf sein Geweyh die Mutter hat verlassen", "tokens": ["Der", "stolz", "auf", "sein", "Ge\u00b7weyh", "die", "Mut\u00b7ter", "hat", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "PPOSAT", "NN", "ART", "NN", "VAFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und selber suchet Gras und Quell.", "tokens": ["Und", "sel\u00b7ber", "su\u00b7chet", "Gras", "und", "Quell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Euch folgten alle Turbantrager,", "tokens": ["Euch", "folg\u00b7ten", "al\u00b7le", "Tur\u00b7ban\u00b7tra\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Centauren waret Ihr, und lieft", "tokens": ["Cen\u00b7tau\u00b7ren", "wa\u00b7ret", "Ihr", ",", "und", "lieft"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An alle Th\u00fcren Sturm; das M\u00e4dchen sprang vom Lager", "tokens": ["An", "al\u00b7le", "Th\u00fc\u00b7ren", "Sturm", ";", "das", "M\u00e4d\u00b7chen", "sprang", "vom", "La\u00b7ger"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "NN", "$.", "ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und bebte, wenn Ihr Hussay! rieft.", "tokens": ["Und", "beb\u00b7te", ",", "wenn", "Ihr", "Hus\u00b7say", "!", "rieft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "$.", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Umarmte M\u00e4nner fuhren pl\u00f6tzlich", "tokens": ["Um\u00b7arm\u00b7te", "M\u00e4n\u00b7ner", "fuh\u00b7ren", "pl\u00f6tz\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von hei\u00dfgek\u00fc\u00dften Lippen ab,", "tokens": ["Von", "hei\u00df\u00b7ge\u00b7k\u00fc\u00df\u00b7ten", "Lip\u00b7pen", "ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und horchten das Geschrey, dem Gott des Weins", "tokens": ["Und", "horch\u00b7ten", "das", "Ge\u00b7schrey", ",", "dem", "Gott", "des", "Weins"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "erg\u00f6tzlich,", "tokens": ["er\u00b7g\u00f6tz\u00b7lich", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Der m\u00e4chtig Euch Befehle gab.", "tokens": ["Der", "m\u00e4ch\u00b7tig", "Euch", "Be\u00b7feh\u00b7le", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Im Glanz des Morgensterns gekleidet", "tokens": ["Im", "Glanz", "des", "Mor\u00b7gens\u00b7terns", "ge\u00b7klei\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Trat Venus schon Auroren vor.", "tokens": ["Trat", "Ve\u00b7nus", "schon", "Au\u00b7ro\u00b7ren", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit diamantnem Blick, der Nacht und Tag entscheidet,", "tokens": ["Mit", "di\u00b7a\u00b7mant\u00b7nem", "Blick", ",", "der", "Nacht", "und", "Tag", "ent\u00b7schei\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "ART", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Besahe sie das Bachuschor,", "tokens": ["Be\u00b7sa\u00b7he", "sie", "das", "Ba\u00b7chu\u00b7schor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Und sagte l\u00e4chelnd zu Ly\u00e4en:", "tokens": ["Und", "sag\u00b7te", "l\u00e4\u00b7chelnd", "zu", "Ly\u00b7\u00e4\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vernimm, o Lieber! dies Geschrey", "tokens": ["Ver\u00b7nimm", ",", "o", "Lie\u00b7ber", "!", "dies", "Ge\u00b7schrey"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "$,", "FM", "ADJD", "$.", "PDS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Helden, welche sonst der Schlacht entgegen stehen,", "tokens": ["Von", "Hel\u00b7den", ",", "wel\u00b7che", "sonst", "der", "Schlacht", "ent\u00b7ge\u00b7gen", "ste\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADV", "ART", "NN", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und frag: wer jezt ihr Feldherr sey?", "tokens": ["Und", "frag", ":", "wer", "jezt", "ihr", "Feld\u00b7herr", "sey", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "PWS", "ADV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Nicht Mavors, der sie vormals weckte,", "tokens": ["Nicht", "Ma\u00b7vors", ",", "der", "sie", "vor\u00b7mals", "weck\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NE", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn ihn Bellonens Wagen trug;", "tokens": ["Wenn", "ihn", "Bel\u00b7lo\u00b7nens", "Wa\u00b7gen", "trug", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nein, du! dem Cerberus die F\u00fc\u00dfe freundlich leckte,", "tokens": ["Nein", ",", "du", "!", "dem", "Cer\u00b7be\u00b7rus", "die", "F\u00fc\u00b7\u00dfe", "freund\u00b7lich", "leck\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "$.", "ART", "NN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du, der die Himmelsst\u00fcrmer schlug!", "tokens": ["Du", ",", "der", "die", "Him\u00b7mels\u00b7st\u00fcr\u00b7mer", "schlug", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Du s\u00fc\u00dfer, s\u00fc\u00dfer Gott der Reben!", "tokens": ["Du", "s\u00fc\u00b7\u00dfer", ",", "s\u00fc\u00b7\u00dfer", "Gott", "der", "Re\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Begeisterst selbst des Rosses Tritt,", "tokens": ["Be\u00b7geis\u00b7terst", "selbst", "des", "Ros\u00b7ses", "Tritt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der nach dem Takte f\u00e4llt, da\u00df B\u00e4ume sich erheben,", "tokens": ["Der", "nach", "dem", "Tak\u00b7te", "f\u00e4llt", ",", "da\u00df", "B\u00e4u\u00b7me", "sich", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,", "KOUS", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und alle Pfeiler h\u00fcpfen mit.", "tokens": ["Und", "al\u00b7le", "Pfei\u00b7ler", "h\u00fcp\u00b7fen", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}