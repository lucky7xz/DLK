{"dta.poem.12686": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf eines buchf\u00fchrers hochzeit  \n mit Jgfr. Keimannin.  \n  D.  V. A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der seelge Keimann hat der b\u00fccher viel geschrieben.", "tokens": ["Der", "seel\u00b7ge", "Kei\u00b7mann", "hat", "der", "b\u00fc\u00b7cher", "viel", "ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "ADJA", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der wohl-ber\u00fchmte mann: nur eins ist \u00fcbrig blieben,", "tokens": ["Der", "wohl\u00b7be\u00b7r\u00fchm\u00b7te", "mann", ":", "nur", "eins", "ist", "\u00fcb\u00b7rig", "blie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ADV", "PIS", "VAFIN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das legt zum erstenmal, Herr Br\u00e4utgam! er itzt auf.", "tokens": ["Das", "legt", "zum", "ers\u00b7ten\u00b7mal", ",", "Herr", "Br\u00e4ut\u00b7gam", "!", "er", "itzt", "auf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "ADV", "$,", "NN", "NE", "$.", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch ist es so ein buch, da\u00df ihm nur selbst zu kauff", "tokens": ["Doch", "ist", "es", "so", "ein", "buch", ",", "da\u00df", "ihm", "nur", "selbst", "zu", "kauff"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "NN", "$,", "KOUS", "PPER", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und keinem andern steht. Er wird es auch behalten.", "tokens": ["Und", "kei\u00b7nem", "an\u00b7dern", "steht", ".", "Er", "wird", "es", "auch", "be\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PIS", "VVFIN", "$.", "PPER", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Weil es so reinlich ist, und ohne fleck und falten.", "tokens": ["Weil", "es", "so", "rein\u00b7lich", "ist", ",", "und", "oh\u00b7ne", "fleck", "und", "fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$,", "KON", "APPR", "NN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der wucher wird auch schon all-j\u00e4hrlich lauffen ein,", "tokens": ["Der", "wu\u00b7cher", "wird", "auch", "schon", "all\u00b7j\u00e4hr\u00b7lich", "lauf\u00b7fen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ADV", "ADV", "ADJD", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein wucher, der nicht wird ohn h\u00e4nd und f\u00fcsse seyn.", "tokens": ["Ein", "wu\u00b7cher", ",", "der", "nicht", "wird", "ohn", "h\u00e4nd", "und", "f\u00fcs\u00b7se", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "PTKNEG", "VAFIN", "APPR", "NE", "KON", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}