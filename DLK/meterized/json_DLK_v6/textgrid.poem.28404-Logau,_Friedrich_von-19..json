{"textgrid.poem.28404": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Uns \u00c4cker sind ietzt nichts als Wege, Steine, Hecken;", "tokens": ["Uns", "\u00c4\u00b7cker", "sind", "ietzt", "nichts", "als", "We\u00b7ge", ",", "Stei\u00b7ne", ",", "He\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "ADV", "PIS", "KOKOM", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sorg, Abfall, Sicherheit wil uns wie gar erstecken.", "tokens": ["Sorg", ",", "Ab\u00b7fall", ",", "Si\u00b7cher\u00b7heit", "wil", "uns", "wie", "gar", "er\u00b7ste\u00b7cken", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "VMFIN", "PPER", "KOKOM", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gib, Gott, da\u00df Korn im Feld, in uns dein Wort bekleibe,", "tokens": ["Gib", ",", "Gott", ",", "da\u00df", "Korn", "im", "Feld", ",", "in", "uns", "dein", "Wort", "be\u00b7klei\u00b7be", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "$,", "KOUS", "NN", "APPRART", "NN", "$,", "APPR", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df wir theils haben Brot der Seele, theils dem Leibe!", "tokens": ["Da\u00df", "wir", "theils", "ha\u00b7ben", "Brot", "der", "See\u00b7le", ",", "theils", "dem", "Lei\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "NN", "ART", "NN", "$,", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}