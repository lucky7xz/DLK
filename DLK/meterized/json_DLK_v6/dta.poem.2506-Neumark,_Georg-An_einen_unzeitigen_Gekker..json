{"dta.poem.2506": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "An einen unzeitigen Gekker.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Du fragst mich Koridon wie du dich solst befreyen/", "tokens": ["Du", "fragst", "mich", "Ko\u00b7ri\u00b7don", "wie", "du", "dich", "solst", "be\u00b7fre\u00b7yen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KOKOM", "PPER", "PPER", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Damit es dich nicht m\u00f6cht\u2019 ins k\u00fcnfftige gercuen/", "tokens": ["Da\u00b7mit", "es", "dich", "nicht", "m\u00f6cht'", "ins", "k\u00fcnff\u00b7ti\u00b7ge", "ge\u00b7rcu\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VMFIN", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die\u00df ist mein Raht: Schau zu da\u00df du kanst etwa sehn/", "tokens": ["Die\u00df", "ist", "mein", "Raht", ":", "Schau", "zu", "da\u00df", "du", "kanst", "et\u00b7wa", "sehn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$.", "NN", "APPR", "KOUS", "PPER", "VMFIN", "ADV", "VVINF", "$("], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.4": {"text": "Die Jung/ die Keusch/ die Reich/ die \u00e4del/ die auch sch\u00f6n", "tokens": ["Die", "Jung", "/", "die", "Keusch", "/", "die", "Reich", "/", "die", "\u00e4\u00b7del", "/", "die", "auch", "sch\u00f6n"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ART", "NN", "$(", "ART", "NN", "$(", "ART", "ADJD", "$(", "PRELS", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jung soll sie seyn/ datz Sie dir Freud\u2019 und Luft erwekke/", "tokens": ["Jung", "soll", "sie", "seyn", "/", "datz", "Sie", "dir", "Freud'", "und", "Luft", "er\u00b7wek\u00b7ke", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "VAINF", "$(", "KOUS", "PPER", "PPER", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Keusch dar\u00fcm/ da\u00df sie nicht dein Ehrenbett beflekke/", "tokens": ["Keusch", "da\u00b7r\u00fcm", "/", "da\u00df", "sie", "nicht", "dein", "Eh\u00b7ren\u00b7bett", "be\u00b7flek\u00b7ke", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PAV", "$(", "KOUS", "PPER", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$("], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Reich/ da\u00df du allzeit wol au\u00df vollem Beutel lebst/", "tokens": ["Reich", "/", "da\u00df", "du", "all\u00b7zeit", "wol", "au\u00df", "vol\u00b7lem", "Beu\u00b7tel", "lebst", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PPER", "ADV", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Vom Adel/ da\u00df du auch mit ihr in Ehren schwebst/", "tokens": ["Vom", "A\u00b7del", "/", "da\u00df", "du", "auch", "mit", "ihr", "in", "Eh\u00b7ren", "schwebst", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$(", "KOUS", "PPER", "ADV", "APPR", "PPOSAT", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Schon/ da\u00df Sie dir gef\u00e4llt. Die\u00df sind die besten Gaben", "tokens": ["Schon", "/", "da\u00df", "Sie", "dir", "ge\u00b7f\u00e4llt", ".", "Die\u00df", "sind", "die", "bes\u00b7ten", "Ga\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$(", "KOUS", "PPER", "PPER", "VVPP", "$.", "PDS", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wie mich bed\u00fcnkt. Wolan! Kanstu nun solche haben", "tokens": ["Wie", "mich", "be\u00b7d\u00fcnkt", ".", "Wo\u00b7lan", "!", "Kans\u00b7tu", "nun", "sol\u00b7che", "ha\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "$.", "ADV", "$.", "VMFIN", "ADV", "PIS", "VAFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So frey Sie immer weg. Doch frag auch ob Sie wil/", "tokens": ["So", "frey", "Sie", "im\u00b7mer", "weg", ".", "Doch", "frag", "auch", "ob", "Sie", "wil", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "PTKVZ", "$.", "KON", "VVIMP", "ADV", "KOUS", "PPER", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Bek\u00f6mstu einen Korb/ so geh und schweige still.", "tokens": ["Be\u00b7k\u00f6ms\u00b7tu", "ei\u00b7nen", "Korb", "/", "so", "geh", "und", "schwei\u00b7ge", "still", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$(", "ADV", "VVFIN", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}