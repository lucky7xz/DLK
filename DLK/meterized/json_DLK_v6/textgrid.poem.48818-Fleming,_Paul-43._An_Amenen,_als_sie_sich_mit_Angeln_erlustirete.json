{"textgrid.poem.48818": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "43. An Amenen, als sie sich mit Angeln erlustirete", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lieb, du hast deine Lust um diesen sch\u00f6nen Flu\u00df,", "tokens": ["Lieb", ",", "du", "hast", "dei\u00b7ne", "Lust", "um", "die\u00b7sen", "sch\u00f6\u00b7nen", "Flu\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "da manche Nais schwimmt, da Chloris mit Napeen", "tokens": ["da", "man\u00b7che", "Nais", "schwimmt", ",", "da", "Chlo\u00b7ris", "mit", "Na\u00b7peen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "$,", "KOUS", "NE", "APPR", "NN"], "meter": "-+-++-+-+-+", "measure": "unknown.measure.hexa"}, "line.3": {"text": "um seines Ufers Rand und tausent Blumen gehen.", "tokens": ["um", "sei\u00b7nes", "U\u00b7fers", "Rand", "und", "tau\u00b7sent", "Blu\u00b7men", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "NN", "KON", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und da\u00df der hei\u00dfe Tag dir mache nicht Verdru\u00df,", "tokens": ["Und", "da\u00df", "der", "hei\u00b7\u00dfe", "Tag", "dir", "ma\u00b7che", "nicht", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "PPER", "VVFIN", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "so beut die Weide dir zum Sessel ihren Fu\u00df,", "tokens": ["so", "beut", "die", "Wei\u00b7de", "dir", "zum", "Ses\u00b7sel", "ih\u00b7ren", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "zum Schirm' ihr laubicht Haupt. Die Etesinnen wehen.", "tokens": ["zum", "Schirm'", "ihr", "lau\u00b7bicht", "Haupt", ".", "Die", "E\u00b7te\u00b7sin\u00b7nen", "we\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "NN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So siehest du vor dir die stummen Schaaren gehen,", "tokens": ["So", "sie\u00b7hest", "du", "vor", "dir", "die", "stum\u00b7men", "Schaa\u00b7ren", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "die deiner Angel auch verehren ihren Ku\u00df.", "tokens": ["die", "dei\u00b7ner", "An\u00b7gel", "auch", "ver\u00b7eh\u00b7ren", "ih\u00b7ren", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So spielst du, sch\u00f6nes Kind, und l\u00e4ssest unerwogen,", "tokens": ["So", "spielst", "du", ",", "sch\u00f6\u00b7nes", "Kind", ",", "und", "l\u00e4s\u00b7sest", "un\u00b7er\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "was f\u00fcr Gedanken mir hier werden zugezogen.", "tokens": ["was", "f\u00fcr", "Ge\u00b7dan\u00b7ken", "mir", "hier", "wer\u00b7den", "zu\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "PPER", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gleich so spielt Amor auch mit mir und kr\u00e4nkt mich frisch.", "tokens": ["Gleich", "so", "spielt", "A\u00b7mor", "auch", "mit", "mir", "und", "kr\u00e4nkt", "mich", "frisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "NE", "ADV", "APPR", "PPER", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-++-+-+-+-+", "measure": "unknown.measure.septa"}}, "stanza.4": {"line.1": {"text": "Sein' Angel ist das Lob von deiner edlen Tugend,", "tokens": ["Sein'", "An\u00b7gel", "ist", "das", "Lob", "von", "dei\u00b7ner", "ed\u00b7len", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "das Garn der Augen Licht, die Rute deine Jugend,", "tokens": ["das", "Garn", "der", "Au\u00b7gen", "Licht", ",", "die", "Ru\u00b7te", "dei\u00b7ne", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die Speise deine Zier und ich der schwache Fisch.", "tokens": ["die", "Spei\u00b7se", "dei\u00b7ne", "Zier", "und", "ich", "der", "schwa\u00b7che", "Fisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "KON", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}