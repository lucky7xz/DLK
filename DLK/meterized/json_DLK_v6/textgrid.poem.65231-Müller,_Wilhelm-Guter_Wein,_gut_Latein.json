{"textgrid.poem.65231": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Guter Wein, gut Latein", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Guter Wein lehrt gut Latein.", "tokens": ["Gu\u00b7ter", "Wein", "lehrt", "gut", "La\u00b7tein."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ADJA", "NN", "VVFIN", "ADJD", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Sitz' ich bei dem vollen Glase,", "tokens": ["Sitz'", "ich", "bei", "dem", "vol\u00b7len", "Gla\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mein' ich, ein Apoll zu sein,", "tokens": ["Mein'", "ich", ",", "ein", "A\u00b7poll", "zu", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "ART", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und es hebt sich meine Nase", "tokens": ["Und", "es", "hebt", "sich", "mei\u00b7ne", "Na\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In die Wolken fast hinein.", "tokens": ["In", "die", "Wol\u00b7ken", "fast", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Z\u00f6pfe, Beutel und Perr\u00fccken", "tokens": ["Z\u00f6p\u00b7fe", ",", "Beu\u00b7tel", "und", "Per\u00b7r\u00fc\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wachsen flugs auf meinem Haupt,", "tokens": ["Wach\u00b7sen", "flugs", "auf", "mei\u00b7nem", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Es mit Ehren auszuschm\u00fccken,", "tokens": ["Es", "mit", "Eh\u00b7ren", "aus\u00b7zu\u00b7schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Die kein S\u00e4kulum ihm raubt.", "tokens": ["Die", "kein", "S\u00e4\u00b7ku\u00b7lum", "ihm", "raubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Guter Wein lehrt gut Latein.", "tokens": ["Gu\u00b7ter", "Wein", "lehrt", "gut", "La\u00b7tein."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ADJA", "NN", "VVFIN", "ADJD", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Seh' ich schon der Flasche Boden,", "tokens": ["Seh'", "ich", "schon", "der", "Fla\u00b7sche", "Bo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist mir auch Apoll zu klein;", "tokens": ["Ist", "mir", "auch", "A\u00b7poll", "zu", "klein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "NE", "PTKA", "ADJD", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "K\u00fchner, als die k\u00fchnsten Oden,", "tokens": ["K\u00fch\u00b7ner", ",", "als", "die", "k\u00fchns\u00b7ten", "O\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "St\u00fcrm' ich in die Welt hinein.", "tokens": ["St\u00fcrm'", "ich", "in", "die", "Welt", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und nach meinem Saitenspiele", "tokens": ["Und", "nach", "mei\u00b7nem", "Sai\u00b7ten\u00b7spie\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Lass' ich sich die Reiche drehn;", "tokens": ["Lass'", "ich", "sich", "die", "Rei\u00b7che", "drehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "ART", "NE", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Liberale und Servile", "tokens": ["Li\u00b7be\u00b7ra\u00b7le", "und", "Ser\u00b7vi\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "M\u00fcssen Musterung bestehn.", "tokens": ["M\u00fcs\u00b7sen", "Mus\u00b7te\u00b7rung", "be\u00b7stehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Guter Wein lehrt gut Latein.", "tokens": ["Gu\u00b7ter", "Wein", "lehrt", "gut", "La\u00b7tein."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ADJA", "NN", "VVFIN", "ADJD", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Ist der Tisch erst na\u00df geworden,", "tokens": ["Ist", "der", "Tisch", "erst", "na\u00df", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ADJD", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Werd' ich gar ein Taktikus,", "tokens": ["Werd'", "ich", "gar", "ein", "Tak\u00b7ti\u00b7kus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lasse nach der Regel morden,", "tokens": ["Las\u00b7se", "nach", "der", "Re\u00b7gel", "mor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und es geht auf Hieb und Schu\u00df.", "tokens": ["Und", "es", "geht", "auf", "Hieb", "und", "Schu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit dem Finger mal' ich Fl\u00fcsse,", "tokens": ["Mit", "dem", "Fin\u00b7ger", "mal'", "ich", "Fl\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Seen mit der ganzen Hand;", "tokens": ["Seen", "mit", "der", "gan\u00b7zen", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "Meines rothen Weines G\u00fcsse", "tokens": ["Mei\u00b7nes", "ro\u00b7then", "Wei\u00b7nes", "G\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Str\u00f6men f\u00fcr das Vaterland.", "tokens": ["Str\u00f6\u00b7men", "f\u00fcr", "das", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Guter Wein lehrt gut Latein.", "tokens": ["Gu\u00b7ter", "Wein", "lehrt", "gut", "La\u00b7tein."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ADJA", "NN", "VVFIN", "ADJD", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Ist der Tisch dann abgewaschen,", "tokens": ["Ist", "der", "Tisch", "dann", "ab\u00b7ge\u00b7wa\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Steck' ich ein das Schwert inde\u00df,", "tokens": ["Steck'", "ich", "ein", "das", "Schwert", "in\u00b7de\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und vor meinen leeren Flaschen", "tokens": ["Und", "vor", "mei\u00b7nen", "lee\u00b7ren", "Fla\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Halt' ich friedlichen Kongre\u00df.", "tokens": ["Halt'", "ich", "fried\u00b7li\u00b7chen", "Kon\u00b7gre\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "L\u00e4nder rei\u00df' ich flugs in St\u00fccken,", "tokens": ["L\u00e4n\u00b7der", "rei\u00df'", "ich", "flugs", "in", "St\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Kann mit einer neuen Nath", "tokens": ["Kann", "mit", "ei\u00b7ner", "neu\u00b7en", "Nath"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Alte Fetzen wieder flicken \u2013", "tokens": ["Al\u00b7te", "Fet\u00b7zen", "wie\u00b7der", "fli\u00b7cken", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Bin ich nicht ein Diplomat?", "tokens": ["Bin", "ich", "nicht", "ein", "Dip\u00b7lo\u00b7mat", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Guter Wein lehrt gut Latein.", "tokens": ["Gu\u00b7ter", "Wein", "lehrt", "gut", "La\u00b7tein."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ADJA", "NN", "VVFIN", "ADJD", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Komm' ich an die letzten Tropfen,", "tokens": ["Komm'", "ich", "an", "die", "letz\u00b7ten", "Trop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist mir nichts mehr gut genug;", "tokens": ["Ist", "mir", "nichts", "mehr", "gut", "ge\u00b7nug", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIS", "ADV", "ADJD", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich riech' an meinem Pfropfen,", "tokens": ["Und", "ich", "riech'", "an", "mei\u00b7nem", "Pfrop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kritisire den Geruch.", "tokens": ["Kri\u00b7ti\u00b7si\u00b7re", "den", "Ge\u00b7ruch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Leer ist meine Westentasche,", "tokens": ["Leer", "ist", "mei\u00b7ne", "Wes\u00b7ten\u00b7ta\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und der Wirth liebt baares Geld. \u2013", "tokens": ["Und", "der", "Wirth", "liebt", "baa\u00b7res", "Geld", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJA", "NN", "$.", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "Schafft mir eine neue Flasche,", "tokens": ["Schafft", "mir", "ei\u00b7ne", "neu\u00b7e", "Fla\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Oder eine neue Welt!", "tokens": ["O\u00b7der", "ei\u00b7ne", "neu\u00b7e", "Welt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}