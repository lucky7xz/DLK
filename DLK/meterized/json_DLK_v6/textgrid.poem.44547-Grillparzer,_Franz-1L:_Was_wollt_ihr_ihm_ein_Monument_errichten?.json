{"textgrid.poem.44547": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was wollt ihr ihm ein Monument errichten?", "genre": "verse", "period": "N.A.", "pub_year": 1846, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was wollt ihr ihm ein Monument errichten?", "tokens": ["Was", "wollt", "ihr", "ihm", "ein", "Mo\u00b7nu\u00b7ment", "er\u00b7rich\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es steht schon lange, wo ihrs alle kennt,", "tokens": ["Es", "steht", "schon", "lan\u00b7ge", ",", "wo", "ihrs", "al\u00b7le", "kennt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "PWAV", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Am Josephsplatz, wo sich die Stra\u00dfen lichten,", "tokens": ["Am", "Jo\u00b7se\u00b7phsplatz", ",", "wo", "sich", "die", "Stra\u00b7\u00dfen", "lich\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "PRF", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Steht schon seit lange Franzens Monument.", "tokens": ["Steht", "schon", "seit", "lan\u00b7ge", "Fran\u00b7zens", "Mo\u00b7nu\u00b7ment", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Als er, bedr\u00e4ngt von st\u00fcrmenden Gewalten", "tokens": ["Als", "er", ",", "be\u00b7dr\u00e4ngt", "von", "st\u00fcr\u00b7men\u00b7den", "Ge\u00b7wal\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des m\u00e4chtgen Korsen, der nun auch dahin,", "tokens": ["Des", "m\u00e4cht\u00b7gen", "Kor\u00b7sen", ",", "der", "nun", "auch", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ADV", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verzweifelnd an der Zaubermacht des Alten", "tokens": ["Ver\u00b7zwei\u00b7felnd", "an", "der", "Zau\u00b7ber\u00b7macht", "des", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sich schwer entschlo\u00df zu neuen Laufs Beginn,", "tokens": ["Sich", "schwer", "ent\u00b7schlo\u00df", "zu", "neu\u00b7en", "Laufs", "Be\u00b7ginn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da setzt er, halb besorgt, sein Wort zu brechen,", "tokens": ["Da", "setzt", "er", ",", "halb", "be\u00b7sorgt", ",", "sein", "Wort", "zu", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "VVPP", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das nur die Not gegeben, nicht das Herz,", "tokens": ["Das", "nur", "die", "Not", "ge\u00b7ge\u00b7ben", ",", "nicht", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "VVPP", "$,", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als bindend Siegel unter das Versprechen,", "tokens": ["Als", "bin\u00b7dend", "Sie\u00b7gel", "un\u00b7ter", "das", "Ver\u00b7spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem gro\u00dfen Ohm ein Standbild fest von Erz.", "tokens": ["Dem", "gro\u00b7\u00dfen", "Ohm", "ein", "Stand\u00b7bild", "fest", "von", "Erz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "...", "tokens": ["..."], "token_info": ["punct"], "pos": ["$("]}}}}}