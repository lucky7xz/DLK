{"textgrid.poem.28645": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "60.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer schl\u00e4ft, der schnarcht wol offt, beist aber dennoch nicht;", "tokens": ["Wer", "schl\u00e4ft", ",", "der", "schnarcht", "wol", "offt", ",", "beist", "a\u00b7ber", "den\u00b7noch", "nicht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PRELS", "VVFIN", "ADV", "ADV", "$,", "ADV", "ADV", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Redligkeit verlacht, was ihr Verfolger spricht;", "tokens": ["Die", "Red\u00b7lig\u00b7keit", "ver\u00b7lacht", ",", "was", "ihr", "Ver\u00b7fol\u00b7ger", "spricht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Biedermann steht stets, nicht lang ein B\u00f6sewicht.", "tokens": ["Ein", "Bie\u00b7der\u00b7mann", "steht", "stets", ",", "nicht", "lang", "ein", "B\u00f6\u00b7se\u00b7wicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "PTKNEG", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}