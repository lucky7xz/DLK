{"textgrid.poem.31426": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gedult / gedult ist noht / in hohen schweren dingen:", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gedult / gedult ist noht / in hohen schweren dingen:", "tokens": ["Ge\u00b7dult", "/", "ge\u00b7dult", "ist", "noht", "/", "in", "ho\u00b7hen", "schwe\u00b7ren", "din\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVPP", "VAFIN", "NN", "$(", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "zu sehen / bi\u00df der Raht de\u00df h\u00f6chsten wird vollbracht.", "tokens": ["zu", "se\u00b7hen", "/", "bi\u00df", "der", "Raht", "de\u00df", "h\u00f6chs\u00b7ten", "wird", "voll\u00b7bracht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$(", "APPR", "ART", "NN", "ART", "ADJA", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Offt wird der Sinn verruckt / Vernunfft ganz blind gemacht;", "tokens": ["Offt", "wird", "der", "Sinn", "ver\u00b7ruckt", "/", "Ver\u00b7nunfft", "ganz", "blind", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$(", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "auf unvermeinte Krei\u00df die Ausgangs Fl\u00fcg sich schwingen /", "tokens": ["auf", "un\u00b7ver\u00b7mein\u00b7te", "Krei\u00df", "die", "Aus\u00b7gangs", "Fl\u00fcg", "sich", "schwin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "NN", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "entfernen sich / wann sie es solten itzt vollbringen /", "tokens": ["ent\u00b7fer\u00b7nen", "sich", "/", "wann", "sie", "es", "sol\u00b7ten", "itzt", "voll\u00b7brin\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$(", "PWAV", "PPER", "PPER", "VMFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "vollziehen allererst wann man nit drauf gedacht:", "tokens": ["voll\u00b7zie\u00b7hen", "al\u00b7le\u00b7rerst", "wann", "man", "nit", "drauf", "ge\u00b7dacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADV", "PWAV", "PIS", "PTKNEG", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "da\u00df unbegreiflich werd sein hoher Raht geacht /", "tokens": ["da\u00df", "un\u00b7be\u00b7greif\u00b7lich", "werd", "sein", "ho\u00b7her", "Raht", "ge\u00b7acht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "und doch dem Glauben m\u00fc\u00df sein Ziel und Sieg gelingen.", "tokens": ["und", "doch", "dem", "Glau\u00b7ben", "m\u00fc\u00df", "sein", "Ziel", "und", "Sieg", "ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VMFIN", "PPOSAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Des H\u00f6llen-Drachen Rach / speyt Feuer da\u00df es blitzt /", "tokens": ["Des", "H\u00f6l\u00b7len\u00b7Dra\u00b7chen", "Rach", "/", "speyt", "Feu\u00b7er", "da\u00df", "es", "blitzt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "VVFIN", "NN", "KOUS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "er ist jetzt zu dem End / der b\u00f6sen zeit erhitzt:", "tokens": ["er", "ist", "jetzt", "zu", "dem", "End", "/", "der", "b\u00f6\u00b7sen", "zeit", "er\u00b7hitzt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "$(", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "doch schad sein toben nichts. Des Himmels schlu\u00df bestehet:", "tokens": ["doch", "schad", "sein", "to\u00b7ben", "nichts", ".", "Des", "Him\u00b7mels", "schlu\u00df", "be\u00b7ste\u00b7het", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAINF", "VVFIN", "PIS", "$.", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "was in Abgrund verst\u00fcrzt / empor mit freuden gehet.", "tokens": ["was", "in", "Ab\u00b7grund", "ver\u00b7st\u00fcrzt", "/", "em\u00b7por", "mit", "freu\u00b7den", "ge\u00b7het", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "VVPP", "$(", "PTKVZ", "APPR", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Dem Gottes-willen All-All-Alles giebet nach.", "tokens": ["Dem", "Got\u00b7tes\u00b7wil\u00b7len", "All\u00b7All\u00b7Al\u00b7les", "gie\u00b7bet", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Selbst Aller widerstand bef\u00f6rdert seine Sach!", "tokens": ["Selbst", "Al\u00b7ler", "wi\u00b7der\u00b7stand", "be\u00b7f\u00f6r\u00b7dert", "sei\u00b7ne", "Sach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}