{"dta.poem.11794": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "80.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zweideutig ist, o Mensch, vernimm auch diese Lehre,", "tokens": ["Zwei\u00b7deu\u00b7tig", "ist", ",", "o", "Mensch", ",", "ver\u00b7nimm", "auch", "die\u00b7se", "Leh\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "FM", "NN", "$,", "VVIMP", "ADV", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein Wesen, wie der Sinn von Leichtigkeit und Schwere.", "tokens": ["Dein", "We\u00b7sen", ",", "wie", "der", "Sinn", "von", "Leich\u00b7tig\u00b7keit", "und", "Schwe\u00b7re", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn wo das Schwere sich macht gelten als das Wichtige,", "tokens": ["Denn", "wo", "das", "Schwe\u00b7re", "sich", "macht", "gel\u00b7ten", "als", "das", "Wich\u00b7ti\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PRF", "VVFIN", "VVINF", "KOKOM", "ART", "ADJA", "$,"], "meter": "-+-+--++-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Erscheint das Leichte nur dagegen als das Nichtige.", "tokens": ["Er\u00b7scheint", "das", "Leich\u00b7te", "nur", "da\u00b7ge\u00b7gen", "als", "das", "Nich\u00b7ti\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PAV", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "Doch ist das Leichte dann das Himmelstrebende,", "tokens": ["Doch", "ist", "das", "Leich\u00b7te", "dann", "das", "Him\u00b7mel\u00b7stre\u00b7ben\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.2": {"text": "So ist das Schwere das am Boden klebende.", "tokens": ["So", "ist", "das", "Schwe\u00b7re", "das", "am", "Bo\u00b7den", "kle\u00b7ben\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "APPRART", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}}, "stanza.4": {"line.1": {"text": "Wo Schwerkraft fehlt, da ists ein Leichtes aufwertsfliegen,", "tokens": ["Wo", "Schwer\u00b7kraft", "fehlt", ",", "da", "ists", "ein", "Leich\u00b7tes", "auf\u00b7werts\u00b7flie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "$,", "ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch schwer ists ohne sie im Gleichgewicht sich wiegen.", "tokens": ["Doch", "schwer", "ists", "oh\u00b7ne", "sie", "im", "Gleich\u00b7ge\u00b7wicht", "sich", "wie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "APPR", "PPER", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch wo die Schnellkraft fehlt, der Schwung der Leidenschaft,", "tokens": ["Doch", "wo", "die", "Schnell\u00b7kraft", "fehlt", ",", "der", "Schwung", "der", "Lei\u00b7den\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da ist zum Guten nicht, noch auch zum B\u00f6sen Kraft.", "tokens": ["Da", "ist", "zum", "Gu\u00b7ten", "nicht", ",", "noch", "auch", "zum", "B\u00f6\u00b7sen", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "PTKNEG", "$,", "ADV", "ADV", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Das Gute selber ist schwer anfangs, leicht zuletzt,", "tokens": ["Das", "Gu\u00b7te", "sel\u00b7ber", "ist", "schwer", "an\u00b7fangs", ",", "leicht", "zu\u00b7letzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADJD", "ADV", "$,", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Seit G\u00f6tter Schwierigkeit der Tugend vorgesetzt.", "tokens": ["Seit", "G\u00f6t\u00b7ter", "Schwie\u00b7rig\u00b7keit", "der", "Tu\u00b7gend", "vor\u00b7ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wer sich das Leichte w\u00e4hlt, erreicht es leicht villeicht,", "tokens": ["Wer", "sich", "das", "Leich\u00b7te", "w\u00e4hlt", ",", "er\u00b7reicht", "es", "leicht", "vil\u00b7leicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch schwerlich neidet ihn, wer Schweres schwer erreicht.", "tokens": ["Doch", "schwer\u00b7lich", "nei\u00b7det", "ihn", ",", "wer", "Schwe\u00b7res", "schwer", "er\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$,", "PWS", "NE", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Wol leichter fertig ist nichts als Leichtfertigkeit,", "tokens": ["Wol", "leich\u00b7ter", "fer\u00b7tig", "ist", "nichts", "als", "Leicht\u00b7fer\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "VAFIN", "PIS", "KOKOM", "NN", "$,"], "meter": "+--+-++-+--+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Doch schwer ist leichter Muth in Widerw\u00e4rtigkeit.", "tokens": ["Doch", "schwer", "ist", "leich\u00b7ter", "Muth", "in", "Wi\u00b7der\u00b7w\u00e4r\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ADJD", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Dir gebe Gott, da\u00df nie dein Leichtes werde fl\u00fcchtig,", "tokens": ["Dir", "ge\u00b7be", "Gott", ",", "da\u00df", "nie", "dein", "Leich\u00b7tes", "wer\u00b7de", "fl\u00fcch\u00b7tig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "KOUS", "ADV", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Und da\u00df ein Schweres stets gehaltig sei und t\u00fcchtig.", "tokens": ["Und", "da\u00df", "ein", "Schwe\u00b7res", "stets", "ge\u00b7hal\u00b7tig", "sei", "und", "t\u00fcch\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "ADJD", "VAFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Wer weder scheinen will schwerf\u00e4llig noch leichtsinnig,", "tokens": ["Wer", "we\u00b7der", "schei\u00b7nen", "will", "schwer\u00b7f\u00e4l\u00b7lig", "noch", "leicht\u00b7sin\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "KON", "VVINF", "VMFIN", "ADJD", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der zeige sich zugleich gef\u00e4llig und herzinnig.", "tokens": ["Der", "zei\u00b7ge", "sich", "zu\u00b7gleich", "ge\u00b7f\u00e4l\u00b7lig", "und", "her\u00b7zin\u00b7nig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}