{"textgrid.poem.37410": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Abschied", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die B\u00e4ume h\u00f6ren auf zu bl\u00fchn,", "tokens": ["Die", "B\u00e4u\u00b7me", "h\u00f6\u00b7ren", "auf", "zu", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Schatz will in die Fremde ziehn;", "tokens": ["Mein", "Schatz", "will", "in", "die", "Frem\u00b7de", "ziehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Schatz der sprach ein bittres Wort:", "tokens": ["Mein", "Schatz", "der", "sprach", "ein", "bitt\u00b7res", "Wort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du bleibst nun hier, aber ich mu\u00df fort.", "tokens": ["Du", "bleibst", "nun", "hier", ",", "a\u00b7ber", "ich", "mu\u00df", "fort", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "KON", "PPER", "VMFIN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Leb wohl, mein Schatz, ich bleib dir treu,", "tokens": ["Leb", "wohl", ",", "mein", "Schatz", ",", "ich", "bleib", "dir", "treu", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo du auch bist, wo ich auch sei.", "tokens": ["Wo", "du", "auch", "bist", ",", "wo", "ich", "auch", "sei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VAFIN", "$,", "PWAV", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bei Regen und bei Sonnenschein,", "tokens": ["Bei", "Re\u00b7gen", "und", "bei", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So lang ich lebe, gedenk ich dein.", "tokens": ["So", "lang", "ich", "le\u00b7be", ",", "ge\u00b7denk", "ich", "dein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "So lang ich lebe, lieb ich dich,", "tokens": ["So", "lang", "ich", "le\u00b7be", ",", "lieb", "ich", "dich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wenn ich sterbe, bet f\u00fcr mich,", "tokens": ["Und", "wenn", "ich", "ster\u00b7be", ",", "bet", "f\u00fcr", "mich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn du kommst zu meinem Grab,", "tokens": ["Und", "wenn", "du", "kommst", "zu", "mei\u00b7nem", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So denk, da\u00df ich dich geliebet hab.", "tokens": ["So", "denk", ",", "da\u00df", "ich", "dich", "ge\u00b7lie\u00b7bet", "hab", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPER", "PRF", "VVPP", "VAFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}