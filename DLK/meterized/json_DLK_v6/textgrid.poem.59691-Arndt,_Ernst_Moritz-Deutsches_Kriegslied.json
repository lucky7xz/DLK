{"textgrid.poem.59691": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Deutsches Kriegslied", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcrs Vaterland, f\u00fcrs Vaterland", "tokens": ["F\u00fcrs", "Va\u00b7ter\u00b7land", ",", "f\u00fcrs", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "All-Deutschland frisch und fr\u00f6hlich auf!", "tokens": ["All\u00b7Deut\u00b7schland", "frisch", "und", "fr\u00f6h\u00b7lich", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vom Ostseestrand, vom Nordseestrand,", "tokens": ["Vom", "Ost\u00b7see\u00b7strand", ",", "vom", "Nord\u00b7sees\u00b7trand", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus Berg und Tal All-Deutschland auf!", "tokens": ["Aus", "Berg", "und", "Tal", "All\u00b7Deut\u00b7schland", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf! Auf! Was kann die Stange tragen,", "tokens": ["Auf", "!", "Auf", "!", "Was", "kann", "die", "Stan\u00b7ge", "tra\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "APPR", "$.", "PWS", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und was von deutschen Ehren wei\u00df!", "tokens": ["Und", "was", "von", "deut\u00b7schen", "Eh\u00b7ren", "wei\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und was ein deutsches Herz f\u00fchlt schlagen,", "tokens": ["Und", "was", "ein", "deut\u00b7sches", "Herz", "f\u00fchlt", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJA", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dem gl\u00fch' das Herz heut doppelt hei\u00df!", "tokens": ["Dem", "gl\u00fch'", "das", "Herz", "heut", "dop\u00b7pelt", "hei\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "F\u00fcrs Vaterland, f\u00fcrs Vaterland!", "tokens": ["F\u00fcrs", "Va\u00b7ter\u00b7land", ",", "f\u00fcrs", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "All-Deutschland frisch und fr\u00f6hlich auf!", "tokens": ["All\u00b7Deut\u00b7schland", "frisch", "und", "fr\u00f6h\u00b7lich", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf gegen welschen L\u00fcgentand", "tokens": ["Auf", "ge\u00b7gen", "wel\u00b7schen", "L\u00fc\u00b7gen\u00b7tand"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPR", "PWAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Sturmesschritt im Sprung und Lauf!", "tokens": ["Mit", "Stur\u00b7mes\u00b7schritt", "im", "Sprung", "und", "Lauf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ha! H\u00f6rt ihr frech die Welschen t\u00f6nen?", "tokens": ["Ha", "!", "H\u00f6rt", "ihr", "frech", "die", "Wel\u00b7schen", "t\u00f6\u00b7nen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVIMP", "PPER", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbf\u00fcr uns das Land, f\u00fcr uns der Rhein!", "tokens": ["\u00bb", "f\u00fcr", "uns", "das", "Land", ",", "f\u00fcr", "uns", "der", "Rhein", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPER", "ART", "NN", "$,", "APPR", "PPER", "ART", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Sieg ist Galliens tapfern S\u00f6hnen,", "tokens": ["Der", "Sieg", "ist", "Gal\u00b7li\u00b7ens", "tap\u00b7fern", "S\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NE", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Drum, stiller Deutscher, gib dich drein!\u00ab", "tokens": ["Drum", ",", "stil\u00b7ler", "Deut\u00b7scher", ",", "gib", "dich", "drein", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "$,", "ADJA", "NN", "$,", "VVIMP", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "F\u00fcrs Vaterland! F\u00fcrs Vaterland!", "tokens": ["F\u00fcrs", "Va\u00b7ter\u00b7land", "!", "F\u00fcrs", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Horch', Welschland! H\u00f6r' ein Gegenlied:", "tokens": ["Horch'", ",", "Wel\u00b7schland", "!", "H\u00f6r'", "ein", "Ge\u00b7gen\u00b7lied", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$.", "NE", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Volk, ein Heer, ein Herz und Hand,", "tokens": ["Ein", "Volk", ",", "ein", "Heer", ",", "ein", "Herz", "und", "Hand", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was gegen euch den Degen zieht \u2013", "tokens": ["Was", "ge\u00b7gen", "euch", "den", "De\u00b7gen", "zieht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sind all zu Schild und Helm geboren,", "tokens": ["Sind", "all", "zu", "Schild", "und", "Helm", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das freie, tapfre Teutsgeschlecht,", "tokens": ["Das", "frei\u00b7e", ",", "tapf\u00b7re", "Teuts\u00b7ge\u00b7schlecht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zu edlem Tode aufgeschworen,", "tokens": ["Zu", "ed\u00b7lem", "To\u00b7de", "auf\u00b7ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zum Kampf f\u00fcr Freiheit, Licht und Recht.", "tokens": ["Zum", "Kampf", "f\u00fcr", "Frei\u00b7heit", ",", "Licht", "und", "Recht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "F\u00fcrs Vaterland! F\u00fcrs Vaterland!", "tokens": ["F\u00fcrs", "Va\u00b7ter\u00b7land", "!", "F\u00fcrs", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drum alle frisch und fr\u00f6hlich drein!", "tokens": ["Drum", "al\u00b7le", "frisch", "und", "fr\u00f6h\u00b7lich", "drein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf welschen Trotz ins welsche Land!", "tokens": ["Auf", "wel\u00b7schen", "Trotz", "ins", "wel\u00b7sche", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr unsern Rhein frisch \u00fcbern Rhein!", "tokens": ["F\u00fcr", "un\u00b7sern", "Rhein", "frisch", "\u00fc\u00b7bern", "Rhein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NE", "ADJD", "VVFIN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit Gott dem Herrn, dem Gott der Freien,", "tokens": ["Mit", "Gott", "dem", "Herrn", ",", "dem", "Gott", "der", "Frei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Drum alle frisch und fr\u00f6hlich drein!", "tokens": ["Drum", "al\u00b7le", "frisch", "und", "fr\u00f6h\u00b7lich", "drein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und was die Prahler dr\u00e4un und schreien,", "tokens": ["Und", "was", "die", "Prah\u00b7ler", "dr\u00e4un", "und", "schrei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Es mu\u00df durch Gott zerstoben sein.", "tokens": ["Es", "mu\u00df", "durch", "Gott", "zer\u00b7sto\u00b7ben", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}