{"dta.poem.11618": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "IiI.  \n  Zwei Liebchen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Schifflein auf der Donau schwamm,", "tokens": ["Ein", "Schif\u00b7flein", "auf", "der", "Do\u00b7nau", "schwamm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drin sa\u00dfen Braut und Br\u00e4utigam,", "tokens": ["Drin", "sa\u00b7\u00dfen", "Braut", "und", "Br\u00e4u\u00b7ti\u00b7gam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er h\u00fcben und sie dr\u00fcben.", "tokens": ["Er", "h\u00fc\u00b7ben", "und", "sie", "dr\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie sprach: Herzliebster, sage mir,", "tokens": ["Sie", "sprach", ":", "Herz\u00b7liebs\u00b7ter", ",", "sa\u00b7ge", "mir", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Angebind' was geb' ich dir?", "tokens": ["Zum", "An\u00b7ge\u00b7bind'", "was", "geb'", "ich", "dir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PWS", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie streift zur\u00fcck ihr Aermelein,", "tokens": ["Sie", "streift", "zu\u00b7r\u00fcck", "ihr", "A\u00b7er\u00b7me\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Sie greift in's Wasser frisch hinein.", "tokens": ["Sie", "greift", "in's", "Was\u00b7ser", "frisch", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Knabe, der th\u00e4t gleich also,", "tokens": ["Der", "Kna\u00b7be", ",", "der", "th\u00e4t", "gleich", "al\u00b7so", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und scherzt mit ihr und lacht so froh.", "tokens": ["Und", "scherzt", "mit", "ihr", "und", "lacht", "so", "froh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "KON", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ach, sch\u00f6ne Frau Done, geb' sie mir", "tokens": ["Ach", ",", "sch\u00f6\u00b7ne", "Frau", "Do\u00b7ne", ",", "geb'", "sie", "mir"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "ADJA", "NN", "NE", "$,", "VVFIN", "PPER", "PPER"], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.2": {"text": "F\u00fcr meinen Schatz eine h\u00fcbsche Zier!", "tokens": ["F\u00fcr", "mei\u00b7nen", "Schatz", "ei\u00b7ne", "h\u00fcb\u00b7sche", "Zier", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Sie zog heraus ein sch\u00f6nes Schwert,", "tokens": ["Sie", "zog", "he\u00b7raus", "ein", "sch\u00f6\u00b7nes", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Knab' h\u00e4tt' lang so eins begehrt.", "tokens": ["Der", "Knab'", "h\u00e4tt'", "lang", "so", "eins", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ADV", "PIS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Der Knab', was h\u00e4lt er in der Hand?", "tokens": ["Der", "Knab'", ",", "was", "h\u00e4lt", "er", "in", "der", "Hand", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Milchwei\u00df ein k\u00f6stlich Perlenband.", "tokens": ["Milch\u00b7wei\u00df", "ein", "k\u00f6st\u00b7lich", "Per\u00b7len\u00b7band", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Er legt's ihr um ihr schwarzes Haar,", "tokens": ["Er", "legt's", "ihr", "um", "ihr", "schwar\u00b7zes", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie sah wie eine F\u00fcrstin gar.", "tokens": ["Sie", "sah", "wie", "ei\u00b7ne", "F\u00fcrs\u00b7tin", "gar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ach, sch\u00f6ne Frau Done, geb' sie mir", "tokens": ["Ach", ",", "sch\u00f6\u00b7ne", "Frau", "Do\u00b7ne", ",", "geb'", "sie", "mir"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "ADJA", "NN", "NE", "$,", "VVFIN", "PPER", "PPER"], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.2": {"text": "F\u00fcr meinen Schatz eine h\u00fcbsche Zier!", "tokens": ["F\u00fcr", "mei\u00b7nen", "Schatz", "ei\u00b7ne", "h\u00fcb\u00b7sche", "Zier", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Sie langt hinein zum andern Mal,", "tokens": ["Sie", "langt", "hin\u00b7ein", "zum", "an\u00b7dern", "Mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fa\u00dft einen Helm von lichtem Stahl.", "tokens": ["Fa\u00dft", "ei\u00b7nen", "Helm", "von", "lich\u00b7tem", "Stahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Der Knab' vor Freud' entsezt sich schier,", "tokens": ["Der", "Knab'", "vor", "Freud'", "ent\u00b7sezt", "sich", "schier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fischt ihr einen goldnen Kamm daf\u00fcr.", "tokens": ["Fischt", "ihr", "ei\u00b7nen", "gold\u00b7nen", "Kamm", "da\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PAV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Zum Dritten sie in's Wasser griff:", "tokens": ["Zum", "Drit\u00b7ten", "sie", "in's", "Was\u00b7ser", "griff", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ach weh! da f\u00e4llt sie aus dem Schiff.", "tokens": ["Ach", "weh", "!", "da", "f\u00e4llt", "sie", "aus", "dem", "Schiff", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$.", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Er springt ihr nach, er fa\u00dft sie keck,", "tokens": ["Er", "springt", "ihr", "nach", ",", "er", "fa\u00dft", "sie", "keck", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Frau Done rei\u00dft sie Beide weg;", "tokens": ["Frau", "Do\u00b7ne", "rei\u00dft", "sie", "Bei\u00b7de", "weg", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "PPER", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Frau Done hat ihr Schmuck gereut,", "tokens": ["Frau", "Do\u00b7ne", "hat", "ihr", "Schmuck", "ge\u00b7reut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das b\u00fc\u00dft der J\u00fcngling und die Maid.", "tokens": ["Das", "b\u00fc\u00dft", "der", "J\u00fcng\u00b7ling", "und", "die", "Maid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Das Schifflein leer hinunter wallt;", "tokens": ["Das", "Schif\u00b7flein", "leer", "hin\u00b7un\u00b7ter", "wallt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sonne sinkt hinter die Berge bald.", "tokens": ["Die", "Son\u00b7ne", "sinkt", "hin\u00b7ter", "die", "Ber\u00b7ge", "bald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.16": {"line.1": {"text": "Und als der Mond am Himmel stand,", "tokens": ["Und", "als", "der", "Mond", "am", "Him\u00b7mel", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Liebchen schwimmen todt an's Land,", "tokens": ["Die", "Lieb\u00b7chen", "schwim\u00b7men", "todt", "an's", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er h\u00fcben und sie dr\u00fcben.", "tokens": ["Er", "h\u00fc\u00b7ben", "und", "sie", "dr\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}