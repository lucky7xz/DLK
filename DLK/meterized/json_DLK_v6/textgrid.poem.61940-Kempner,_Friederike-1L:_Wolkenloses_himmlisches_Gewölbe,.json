{"textgrid.poem.61940": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wolkenloses himmlisches Gew\u00f6lbe,", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wolkenloses himmlisches Gew\u00f6lbe,", "tokens": ["Wol\u00b7ken\u00b7lo\u00b7ses", "himm\u00b7li\u00b7sches", "Ge\u00b7w\u00f6l\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Unter m\u00e4cht'gen Palmen Purpurzelt,", "tokens": ["Un\u00b7ter", "m\u00e4cht'\u00b7gen", "Pal\u00b7men", "Pur\u00b7pur\u00b7zelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Eine Reiter-Karawane h\u00e4lt,", "tokens": ["Ei\u00b7ne", "Rei\u00b7ter\u00b7Ka\u00b7ra\u00b7wa\u00b7ne", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Auf dem Boden W\u00fcstensand, der gelbe.", "tokens": ["Auf", "dem", "Bo\u00b7den", "W\u00fcs\u00b7ten\u00b7sand", ",", "der", "gel\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "ART", "ADJA", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Krachend unterirdisches Gew\u00f6lbe,", "tokens": ["Kra\u00b7chend", "un\u00b7ter\u00b7ir\u00b7di\u00b7sches", "Ge\u00b7w\u00f6l\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "F\u00fcnfzehnhundert Leichen, tiefentstellt, \u2013", "tokens": ["F\u00fcnf\u00b7zehn\u00b7hun\u00b7dert", "Lei\u00b7chen", ",", "tie\u00b7fent\u00b7stellt", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "punct"], "pos": ["CARD", "NN", "$,", "ADJD", "$,", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Jede Leiche war ein wackrer Held, \u2013", "tokens": ["Je\u00b7de", "Lei\u00b7che", "war", "ein", "wack\u00b7rer", "Held", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$,", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Speit die Flamme rasselnd aus, die gelbe.", "tokens": ["Speit", "die", "Flam\u00b7me", "ras\u00b7selnd", "aus", ",", "die", "gel\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "PTKVZ", "$,", "ART", "ADJA", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Solch' ein Traumbild Abdelkader gr\u00fc\u00dfte,", "tokens": ["Solch'", "ein", "Traum\u00b7bild", "Ab\u00b7del\u00b7ka\u00b7der", "gr\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Trunken er der Heimat Boden k\u00fc\u00dfte:", "tokens": ["Trun\u00b7ken", "er", "der", "Hei\u00b7mat", "Bo\u00b7den", "k\u00fc\u00df\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "\u00bballah, Allah\u00ab \u2013 ruft er, \u2013 \u00bbmeine W\u00fcste!\u00ab", "tokens": ["\u00bb", "al\u00b7lah", ",", "Al\u00b7lah", "\u00ab", "\u2013", "ruft", "er", ",", "\u2013", "\u00bb", "mei\u00b7ne", "W\u00fcs\u00b7te", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "punct", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "NN", "$(", "$(", "VVFIN", "PPER", "$,", "$(", "$(", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbpellissier, Dein f\u00fcrchterlicher Brand!\u00ab \u2013", "tokens": ["\u00bb", "pel\u00b7lis\u00b7sier", ",", "Dein", "f\u00fcrch\u00b7ter\u00b7li\u00b7cher", "Brand", "!", "\u00ab", "\u2013"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "FM.la", "$,", "PPOSAT", "ADJA", "NN", "$.", "$(", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Pl\u00f6tzlich sich der Held im Traum ermannt,", "tokens": ["Pl\u00f6tz\u00b7lich", "sich", "der", "Held", "im", "Traum", "er\u00b7mannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Seine Blicke trafen Kerkerswand! \u2013", "tokens": ["Sei\u00b7ne", "Bli\u00b7cke", "tra\u00b7fen", "Ker\u00b7kers\u00b7wand", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}