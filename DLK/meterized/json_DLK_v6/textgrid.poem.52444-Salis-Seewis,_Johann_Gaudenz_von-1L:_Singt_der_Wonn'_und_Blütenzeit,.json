{"textgrid.poem.52444": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Singt der Wonn' und Bl\u00fctenzeit,", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Singt der Wonn' und Bl\u00fctenzeit,", "tokens": ["Singt", "der", "Wonn'", "und", "Bl\u00fc\u00b7ten\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Pflanzt die gr\u00fcnen Maien!", "tokens": ["Pflanzt", "die", "gr\u00fc\u00b7nen", "Mai\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Selig, wer des Mais sich freut,", "tokens": ["Se\u00b7lig", ",", "wer", "des", "Mais", "sich", "freut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie uns die Natur gebeut,", "tokens": ["Wie", "uns", "die", "Na\u00b7tur", "ge\u00b7beut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zu Zweien! Zu Zweien! Zu Zweien!", "tokens": ["Zu", "Zwei\u00b7en", "!", "Zu", "Zwei\u00b7en", "!", "Zu", "Zwei\u00b7en", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "APPR", "NN", "$.", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Zu der T\u00e4nze Melodei", "tokens": ["Zu", "der", "T\u00e4n\u00b7ze", "Me\u00b7lo\u00b7dei"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wirbelt das Gest\u00e4ude;", "tokens": ["Wir\u00b7belt", "das", "Ge\u00b7st\u00e4u\u00b7de", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Waldgesang und Dorfschalmei", "tokens": ["Wald\u00b7ge\u00b7sang", "und", "Dorf\u00b7schal\u00b7mei"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jubeln: Pflicht und Weisheit sei", "tokens": ["Ju\u00b7beln", ":", "Pflicht", "und", "Weis\u00b7heit", "sei"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "NN", "KON", "NN", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die Freude! Die Freude! Die Freude!", "tokens": ["Die", "Freu\u00b7de", "!", "Die", "Freu\u00b7de", "!", "Die", "Freu\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "NN", "$.", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Kr\u00e4nzt, Verlobte, kr\u00e4nzt das Haar", "tokens": ["Kr\u00e4nzt", ",", "Ver\u00b7lob\u00b7te", ",", "kr\u00e4nzt", "das", "Haar"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Froh mit Myrtenzweigen!", "tokens": ["Froh", "mit", "Myr\u00b7ten\u00b7zwei\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "So, wie bald am Brautaltar,", "tokens": ["So", ",", "wie", "bald", "am", "Brau\u00b7tal\u00b7tar", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ADV", "APPRART", "NN", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.4": {"text": "Steht hier alles Paar um Paar", "tokens": ["Steht", "hier", "al\u00b7les", "Paar", "um", "Paar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PIAT", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Im Reigen! Im Reigen! Im Reigen!", "tokens": ["Im", "Rei\u00b7gen", "!", "Im", "Rei\u00b7gen", "!", "Im", "Rei\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "APPRART", "NN", "$.", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Amor l\u00e4\u00dft am Maienfest", "tokens": ["A\u00b7mor", "l\u00e4\u00dft", "am", "Mai\u00b7en\u00b7fest"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Jede Spr\u00f6de b\u00fc\u00dfen!", "tokens": ["Je\u00b7de", "Spr\u00f6\u00b7de", "b\u00fc\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Philomele baut ihr Nest!", "tokens": ["Phi\u00b7lo\u00b7me\u00b7le", "baut", "ihr", "Nest", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles Holde liebt und l\u00e4\u00dft", "tokens": ["Al\u00b7les", "Hol\u00b7de", "liebt", "und", "l\u00e4\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sich k\u00fcssen! Sich k\u00fcssen! Sich k\u00fcssen!", "tokens": ["Sich", "k\u00fcs\u00b7sen", "!", "Sich", "k\u00fcs\u00b7sen", "!", "Sich", "k\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "VVINF", "$.", "PRF", "VVINF", "$.", "PRF", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}