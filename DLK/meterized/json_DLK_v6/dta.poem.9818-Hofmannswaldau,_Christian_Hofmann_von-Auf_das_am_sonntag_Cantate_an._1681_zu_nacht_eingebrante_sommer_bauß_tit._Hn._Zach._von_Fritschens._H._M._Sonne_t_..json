{"dta.poem.9818": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf das am sonntag Cantate an. 1681 zu nacht  \n eingebrante sommer bau\u00df tit. Hn. Zach. von  \n Fritschens.  \n H. M.  \n  Sonne t .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Brenn immer wie du wilt/ du altes sommer-hau\u00df/", "tokens": ["Brenn", "im\u00b7mer", "wie", "du", "wilt", "/", "du", "al\u00b7tes", "som\u00b7mer\u00b7hau\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "KOKOM", "PPER", "VMFIN", "$(", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den freunden nur zur lust/ den nachbarn nicht zum schrecken/", "tokens": ["Den", "freun\u00b7den", "nur", "zur", "lust", "/", "den", "nach\u00b7barn", "nicht", "zum", "schre\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "APPRART", "NN", "$(", "ART", "NN", "PTKNEG", "APPRART", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es darff kein w\u00e4chter hier den brand zu l\u00f6schen wecken:", "tokens": ["Es", "darff", "kein", "w\u00e4ch\u00b7ter", "hier", "den", "brand", "zu", "l\u00f6\u00b7schen", "we\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "ADV", "ART", "NN", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn ob du gleich anitzt vergehst in asch und grau\u00df/", "tokens": ["Denn", "ob", "du", "gleich", "a\u00b7nitzt", "ver\u00b7gehst", "in", "asch", "und", "grau\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "VVFIN", "APPR", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So baut der herr von Fritsch dich sch\u00f6ner wieder aus/", "tokens": ["So", "baut", "der", "herr", "von", "Fritsch", "dich", "sch\u00f6\u00b7ner", "wie\u00b7der", "aus", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "PPER", "ADJD", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Venus will dich selbst mit ihren rosen decken/", "tokens": ["Und", "Ve\u00b7nus", "will", "dich", "selbst", "mit", "ih\u00b7ren", "ro\u00b7sen", "de\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ja/ statt der ziegel schenckt sie ihre purpur-schnecken/", "tokens": ["Ja", "/", "statt", "der", "zie\u00b7gel", "schenckt", "sie", "ih\u00b7re", "pur\u00b7pur\u00b7schne\u00b7cken", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "APPR", "ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gedenckt auch alle jahr zu halten einen schmau\u00df.", "tokens": ["Ge\u00b7denckt", "auch", "al\u00b7le", "jahr", "zu", "hal\u00b7ten", "ei\u00b7nen", "schmau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "PIAT", "NN", "PTKZU", "VVINF", "ART", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Nymphen sch\u00f6nes volck wil dich hinfort bewohnen/", "tokens": ["Der", "Nym\u00b7phen", "sch\u00f6\u00b7nes", "volck", "wil", "dich", "hin\u00b7fort", "be\u00b7woh\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VMFIN", "PRF", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die nackten Amor\u2019n gehn und legen zwibeln ein/", "tokens": ["Die", "nack\u00b7ten", "A\u00b7mor'n", "gehn", "und", "le\u00b7gen", "zwi\u00b7beln", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "KON", "VVFIN", "APPR", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df k\u00fcnftig wachsen auf narcissen/ anemonen/", "tokens": ["Da\u00df", "k\u00fcnf\u00b7tig", "wach\u00b7sen", "auf", "nar\u00b7cis\u00b7sen", "/", "a\u00b7ne\u00b7mo\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ADJD", "VVFIN", "APPR", "NN", "$(", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und da\u00df ein ewig lentz um deine burg mu\u00df seyn.", "tokens": ["Und", "da\u00df", "ein", "e\u00b7wig", "lentz", "um", "dei\u00b7ne", "burg", "mu\u00df", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer aber/ frag\u2019 ich auch aus frevel nicht vor witze/", "tokens": ["Wer", "a\u00b7ber", "/", "frag'", "ich", "auch", "aus", "fre\u00b7vel", "nicht", "vor", "wit\u00b7ze", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "$(", "VVIMP", "PPER", "ADV", "APPR", "NE", "PTKNEG", "APPR", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Versetzte dich in brand? ach! ach! der spieler hitze!", "tokens": ["Ver\u00b7setz\u00b7te", "dich", "in", "brand", "?", "ach", "!", "ach", "!", "der", "spie\u00b7ler", "hit\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$.", "XY", "$.", "XY", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}