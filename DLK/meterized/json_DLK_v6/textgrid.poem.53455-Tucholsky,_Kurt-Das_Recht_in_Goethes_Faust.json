{"textgrid.poem.53455": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Das Recht in Goethes Faust", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf! oder ihr seid verloren.", "tokens": ["Auf", "!", "o\u00b7der", "ihr", "seid", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "KON", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unn\u00fctzes Zagen! Zaudern und Plaudern!", "tokens": ["Un\u00b7n\u00fct\u00b7zes", "Za\u00b7gen", "!", "Zau\u00b7dern", "und", "Plau\u00b7dern", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Meine Pferde schaudern, der Morgen d\u00e4mmert auf.", "tokens": ["Mei\u00b7ne", "Pfer\u00b7de", "schau\u00b7dern", ",", "der", "Mor\u00b7gen", "d\u00e4m\u00b7mert", "auf", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Ja, gute Frau, durch zweier Zeugen Mund", "tokens": ["Ja", ",", "gu\u00b7te", "Frau", ",", "durch", "zwei\u00b7er", "Zeu\u00b7gen", "Mund"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wird allewegs die Wahrheit kund . . .", "tokens": ["Wird", "al\u00b7le\u00b7wegs", "die", "Wahr\u00b7heit", "kund", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PTKVZ", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Keinen Blumenflor beneid ich,", "tokens": ["Kei\u00b7nen", "Blu\u00b7men\u00b7flor", "be\u00b7neid", "ich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Allen Widerstreit vermeid ich;", "tokens": ["Al\u00b7len", "Wi\u00b7der\u00b7streit", "ver\u00b7meid", "ich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mir ists gegen die Natur:", "tokens": ["Mir", "ists", "ge\u00b7gen", "die", "Na\u00b7tur", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bin ich doch das Mark vom Lande", "tokens": ["Bin", "ich", "doch", "das", "Mark", "vom", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und zum sicheren Unterpfande", "tokens": ["Und", "zum", "si\u00b7che\u00b7ren", "Un\u00b7ter\u00b7pfan\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Friedenszeichen jeder Flur.", "tokens": ["Frie\u00b7dens\u00b7zei\u00b7chen", "je\u00b7der", "Flur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}