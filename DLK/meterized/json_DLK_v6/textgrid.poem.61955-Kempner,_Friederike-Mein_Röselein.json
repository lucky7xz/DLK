{"textgrid.poem.61955": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Mein R\u00f6selein", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gr\u00fc\u00df' Dich Gott, mein R\u00f6selein,", "tokens": ["Gr\u00fc\u00df'", "Dich", "Gott", ",", "mein", "R\u00f6\u00b7se\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6n und klein und sanft Du bist:", "tokens": ["Sch\u00f6n", "und", "klein", "und", "sanft", "Du", "bist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "KON", "VVFIN", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie sie so anmutig ist!", "tokens": ["Wie", "sie", "so", "an\u00b7mu\u00b7tig", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+----+", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "R\u00f6selein, gern seh' ich Dich!", "tokens": ["R\u00f6\u00b7se\u00b7lein", ",", "gern", "seh'", "ich", "Dich", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bleib' so still und lieb und rein:", "tokens": ["Bleib'", "so", "still", "und", "lieb", "und", "rein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bleib' so ewig jung und mein!", "tokens": ["Bleib'", "so", "e\u00b7wig", "jung", "und", "mein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "ADJD", "KON", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "R\u00f6slein mein, o denk' an mich!", "tokens": ["R\u00f6s\u00b7lein", "mein", ",", "o", "denk'", "an", "mich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "$,", "FM", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Purpurrot und gr\u00fcn Dein Stiel:", "tokens": ["Pur\u00b7pur\u00b7rot", "und", "gr\u00fcn", "Dein", "Stiel", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Geist und Anmut hat sie viel!", "tokens": ["Geist", "und", "An\u00b7mut", "hat", "sie", "viel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "R\u00f6slein, Dich, Dich liebe ich!", "tokens": ["R\u00f6s\u00b7lein", ",", "Dich", ",", "Dich", "lie\u00b7be", "ich", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zart dr\u00fcck' ich Dich an den Mund:", "tokens": ["Zart", "dr\u00fcck", "ich", "Dich", "an", "den", "Mund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Nehme Abschied, bleib' gesund!", "tokens": ["Neh\u00b7me", "Ab\u00b7schied", ",", "bleib'", "ge\u00b7sund", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Bl\u00e4ttlein klein, o bleibet frisch,", "tokens": ["Bl\u00e4tt\u00b7lein", "klein", ",", "o", "blei\u00b7bet", "frisch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "FM", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihres Zweiges dunkelgr\u00fcn:", "tokens": ["Ih\u00b7res", "Zwei\u00b7ges", "dun\u00b7kel\u00b7gr\u00fcn", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, ich mu\u00df von dannen zieh'n!", "tokens": ["Ach", ",", "ich", "mu\u00df", "von", "dan\u00b7nen", "zieh'n", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VMFIN", "APPR", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "R\u00f6slein, nein, es war nur Scherz:", "tokens": ["R\u00f6s\u00b7lein", ",", "nein", ",", "es", "war", "nur", "Scherz", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKANT", "$,", "PPER", "VAFIN", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ewig, ewig bleib' ich Dein!", "tokens": ["E\u00b7wig", ",", "e\u00b7wig", "bleib'", "ich", "Dein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ewig bleibst Du lieb und fein!", "tokens": ["E\u00b7wig", "bleibst", "Du", "lieb", "und", "fein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "R\u00f6selein, o gr\u00fc\u00df' Dich Gott,", "tokens": ["R\u00f6\u00b7se\u00b7lein", ",", "o", "gr\u00fc\u00df'", "Dich", "Gott", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6n und frisch und mein Du bist:", "tokens": ["Sch\u00f6n", "und", "frisch", "und", "mein", "Du", "bist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "KON", "PPOSAT", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Voll mein Herz vor Freuden ist!", "tokens": ["Voll", "mein", "Herz", "vor", "Freu\u00b7den", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "APPR", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}