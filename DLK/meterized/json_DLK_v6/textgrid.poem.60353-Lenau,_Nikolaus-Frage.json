{"textgrid.poem.60353": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Frage", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bist du noch nie beim Morgenschein erwacht", "tokens": ["Bist", "du", "noch", "nie", "beim", "Mor\u00b7gen\u00b7schein", "er\u00b7wacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit schwerem Herzen, traurig und beklommen,", "tokens": ["Mit", "schwe\u00b7rem", "Her\u00b7zen", ",", "trau\u00b7rig", "und", "be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wu\u00dftest nicht, wie du auch nachgedacht,", "tokens": ["Und", "wu\u00df\u00b7test", "nicht", ",", "wie", "du", "auch", "nach\u00b7ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Woher ins Herz der Gram dir war gekommen?", "tokens": ["Wo\u00b7her", "ins", "Herz", "der", "Gram", "dir", "war", "ge\u00b7kom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du f\u00fchltest nur: ein Traum wars in der Nacht;", "tokens": ["Du", "f\u00fchl\u00b7test", "nur", ":", "ein", "Traum", "wars", "in", "der", "Nacht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "ART", "NN", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des Traumes Bilder waren dir verschwommen,", "tokens": ["Des", "Trau\u00b7mes", "Bil\u00b7der", "wa\u00b7ren", "dir", "ver\u00b7schwom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch hat nachwirkend ihre dunkle Macht", "tokens": ["Doch", "hat", "nach\u00b7wir\u00b7kend", "ih\u00b7re", "dunk\u00b7le", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "VVPP", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dich, da\u00df du weinen mu\u00dftest, \u00fcbernommen.", "tokens": ["Dich", ",", "da\u00df", "du", "wei\u00b7nen", "mu\u00df\u00b7test", ",", "\u00fc\u00b7ber\u00b7nom\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hast du dich einst der Erdennacht entschwungen,", "tokens": ["Hast", "du", "dich", "einst", "der", "Er\u00b7den\u00b7nacht", "ent\u00b7schwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und werden, wie du meinst, am hellen Tage", "tokens": ["Und", "wer\u00b7den", ",", "wie", "du", "meinst", ",", "am", "hel\u00b7len", "Ta\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VAINF", "$,", "PWAV", "PPER", "ADV", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verloren sein des Traums Erinnerungen:", "tokens": ["Ver\u00b7lo\u00b7ren", "sein", "des", "Traums", "E\u00b7rin\u00b7ne\u00b7run\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wer wei\u00df, ob nicht so deine Schuld hienieden", "tokens": ["Wer", "wei\u00df", ",", "ob", "nicht", "so", "dei\u00b7ne", "Schuld", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PTKNEG", "ADV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nachwirken wird als eine dunkle Klage", "tokens": ["Nach\u00b7wir\u00b7ken", "wird", "als", "ei\u00b7ne", "dunk\u00b7le", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und dort der Seele st\u00f6ren ihren Frieden?", "tokens": ["Und", "dort", "der", "See\u00b7le", "st\u00f6\u00b7ren", "ih\u00b7ren", "Frie\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}