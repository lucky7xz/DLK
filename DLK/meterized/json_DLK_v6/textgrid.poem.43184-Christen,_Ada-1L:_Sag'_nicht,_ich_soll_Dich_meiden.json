{"textgrid.poem.43184": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sag' nicht, ich soll Dich meiden", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sag' nicht, ich soll Dich meiden", "tokens": ["Sag'", "nicht", ",", "ich", "soll", "Dich", "mei\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "$,", "PPER", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und nimmer sehn,", "tokens": ["Und", "nim\u00b7mer", "sehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wollt' ich Dich auch verlassen,", "tokens": ["Wollt'", "ich", "Dich", "auch", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wohin sollt' ich gehn? \u2013", "tokens": ["Wo\u00b7hin", "sollt'", "ich", "gehn", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "--+-+", "measure": "anapaest.init"}, "line.5": {"text": "Du wei\u00dft es ja, ich habe", "tokens": ["Du", "wei\u00dft", "es", "ja", ",", "ich", "ha\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Keine Heimath dann \u2013", "tokens": ["Kei\u00b7ne", "Hei\u00b7math", "dann", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Kein Gl\u00fcck \u2013 und keine St\u00e4tte,", "tokens": ["Kein", "Gl\u00fcck", "\u2013", "und", "kei\u00b7ne", "St\u00e4t\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$(", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wo ich ruhen kann ...", "tokens": ["Wo", "ich", "ru\u00b7hen", "kann", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "VMFIN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}