{"dta.poem.4135": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Juni.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich schlaf' in einem Bett' von Rosenduft,", "tokens": ["Ich", "schlaf'", "in", "ei\u00b7nem", "Bett'", "von", "Ro\u00b7sen\u00b7duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bis mich der rosenrothe Morgen ruft,", "tokens": ["Bis", "mich", "der", "ro\u00b7sen\u00b7ro\u00b7the", "Mor\u00b7gen", "ruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein St\u00fcndlein in den Knospen zu verscherzen.", "tokens": ["Ein", "St\u00fcnd\u00b7lein", "in", "den", "Knos\u00b7pen", "zu", "ver\u00b7scher\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Mittag liebt ein herzlicheres Herzen,", "tokens": ["Der", "Mit\u00b7tag", "liebt", "ein", "herz\u00b7li\u00b7che\u00b7res", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dringt hei\u00df bis in des Kelches tiefste Kluft:", "tokens": ["Dringt", "hei\u00df", "bis", "in", "des", "Kel\u00b7ches", "tiefs\u00b7te", "Kluft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da fliegt manch Rosenbl\u00e4ttchen durch die Luft,", "tokens": ["Da", "fliegt", "manch", "Ro\u00b7sen\u00b7bl\u00e4tt\u00b7chen", "durch", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und seufzt von Minnelust und Minneschmerzen.", "tokens": ["Und", "seufzt", "von", "Min\u00b7ne\u00b7lust", "und", "Min\u00b7nesc\u00b7hmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Abend kommt, den Blumen Trost zu geben,", "tokens": ["Der", "A\u00b7bend", "kommt", ",", "den", "Blu\u00b7men", "Trost", "zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die matt und bla\u00df in seinem Thau sich baden,", "tokens": ["Die", "matt", "und", "bla\u00df", "in", "sei\u00b7nem", "Thau", "sich", "ba\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "APPR", "PPOSAT", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bis allen ihren Zorn sie ausgek\u00fchlt.", "tokens": ["Bis", "al\u00b7len", "ih\u00b7ren", "Zorn", "sie", "aus\u00b7ge\u00b7k\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Behagt dir, Freund, dies rothe Rosenleben,", "tokens": ["Be\u00b7hagt", "dir", ",", "Freund", ",", "dies", "ro\u00b7the", "Ro\u00b7sen\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "PDS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So sei von mir auf morgen eingeladen,", "tokens": ["So", "sei", "von", "mir", "auf", "mor\u00b7gen", "ein\u00b7ge\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "APPR", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn alle Tage wird solch Spiel gespielt.", "tokens": ["Denn", "al\u00b7le", "Ta\u00b7ge", "wird", "solch", "Spiel", "ge\u00b7spielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}