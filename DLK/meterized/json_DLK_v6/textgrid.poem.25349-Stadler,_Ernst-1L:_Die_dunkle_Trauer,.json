{"textgrid.poem.25349": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die dunkle Trauer,", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die dunkle Trauer,", "tokens": ["Die", "dunk\u00b7le", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "die um aller Dinge Stirnen todess\u00fcchtig wittert,", "tokens": ["die", "um", "al\u00b7ler", "Din\u00b7ge", "Stir\u00b7nen", "to\u00b7des\u00b7s\u00fcch\u00b7tig", "wit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "NN", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "Hebt sachte deiner Fl\u00f6te Klingen auf,", "tokens": ["Hebt", "sach\u00b7te", "dei\u00b7ner", "Fl\u00f6\u00b7te", "Klin\u00b7gen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "das mitt\u00e4glich im braunen Haider\u00f6hricht zittert.", "tokens": ["das", "mit\u00b7t\u00e4g\u00b7lich", "im", "brau\u00b7nen", "Hai\u00b7de\u00b7r\u00f6h\u00b7richt", "zit\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Die Schwermut aller Blumen,", "tokens": ["Die", "Schwer\u00b7mut", "al\u00b7ler", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "aller Gr\u00e4ser, Steine, Schilfe, B\u00e4ume stummes Klagen", "tokens": ["al\u00b7ler", "Gr\u00e4\u00b7ser", ",", "Stei\u00b7ne", ",", "Schil\u00b7fe", ",", "B\u00e4u\u00b7me", "stum\u00b7mes", "Kla\u00b7gen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.7": {"text": "Saugt es in sich und will sie demutsvoll", "tokens": ["Saugt", "es", "in", "sich", "und", "will", "sie", "de\u00b7muts\u00b7voll"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PRF", "KON", "VMFIN", "PPER", "ADJD"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "in blaue Sommerhimmel tragen.", "tokens": ["in", "blau\u00b7e", "Som\u00b7mer\u00b7him\u00b7mel", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die M\u00fcdigkeit der Stunden,", "tokens": ["Die", "M\u00fc\u00b7dig\u00b7keit", "der", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "wenn der Tag durch gelbe D\u00e4mmernebel raucht,", "tokens": ["wenn", "der", "Tag", "durch", "gel\u00b7be", "D\u00e4m\u00b7mer\u00b7ne\u00b7bel", "raucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.11": {"text": "Heimstr\u00f6mend alles Licht", "tokens": ["Heims\u00b7tr\u00f6\u00b7mend", "al\u00b7les", "Licht"], "token_info": ["word", "word", "word"], "pos": ["NN", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "im m\u00fctterlichen Scho\u00df der Nacht sich untertaucht,", "tokens": ["im", "m\u00fct\u00b7ter\u00b7li\u00b7chen", "Scho\u00df", "der", "Nacht", "sich", "un\u00b7ter\u00b7taucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Verlorne Wehmut kleiner Lieder, die ein M\u00e4dchen", "tokens": ["Ver\u00b7lor\u00b7ne", "Weh\u00b7mut", "klei\u00b7ner", "Lie\u00b7der", ",", "die", "ein", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "tanzend sich auf Sommerwiesen singt,", "tokens": ["tan\u00b7zend", "sich", "auf", "Som\u00b7mer\u00b7wie\u00b7sen", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "Glockengel\u00e4ut, das heimwehrauschend", "tokens": ["Glo\u00b7cken\u00b7ge\u00b7l\u00e4ut", ",", "das", "heim\u00b7wehr\u00b7au\u00b7schend"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PRELS", "ADJD"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.16": {"text": "\u00fcber sonnenrote Abendh\u00fcgel dringt,", "tokens": ["\u00fc\u00b7ber", "son\u00b7nen\u00b7ro\u00b7te", "A\u00b7bend\u00b7h\u00fc\u00b7gel", "dringt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.17": {"text": "Die gro\u00dfe Traurigkeit des Meers, das sich", "tokens": ["Die", "gro\u00b7\u00dfe", "Trau\u00b7rig\u00b7keit", "des", "Meers", ",", "das", "sich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,", "PRELS", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "an grauer K\u00fcsten Damm die Brust zerschl\u00e4gt", "tokens": ["an", "grau\u00b7er", "K\u00fcs\u00b7ten", "Damm", "die", "Brust", "zer\u00b7schl\u00e4gt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Und auf gebeugtem R\u00fccken endlos die Verg\u00e4nglichkeit", "tokens": ["Und", "auf", "ge\u00b7beug\u00b7tem", "R\u00fc\u00b7cken", "end\u00b7los", "die", "Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.20": {"text": "vom Sommer in den jungen Fr\u00fchling tr\u00e4gt \u2013", "tokens": ["vom", "Som\u00b7mer", "in", "den", "jun\u00b7gen", "Fr\u00fch\u00b7ling", "tr\u00e4gt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Sinkt in dein Spiel, schwerm\u00fctig helle Bl\u00fcte,", "tokens": ["Sinkt", "in", "dein", "Spiel", ",", "schwer\u00b7m\u00fc\u00b7tig", "hel\u00b7le", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "die in dunkle Brunnen glitt ...", "tokens": ["die", "in", "dunk\u00b7le", "Brun\u00b7nen", "glitt", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Und alle stummen Dinge sprechen leise gl\u00fchend", "tokens": ["Und", "al\u00b7le", "stum\u00b7men", "Din\u00b7ge", "spre\u00b7chen", "lei\u00b7se", "gl\u00fc\u00b7hend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN", "VVFIN", "ADJD", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "ihrer Seelen wehste Litaneien mit.", "tokens": ["ih\u00b7rer", "See\u00b7len", "wehs\u00b7te", "Li\u00b7ta\u00b7nei\u00b7en", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.25": {"text": "Du aber l\u00e4chelst, l\u00e4chelst ... Deine Augen", "tokens": ["Du", "a\u00b7ber", "l\u00e4\u00b7chelst", ",", "l\u00e4\u00b7chelst", "...", "Dei\u00b7ne", "Au\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "$,", "VVFIN", "$(", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "beugen sich vergessen, weltenweit entr\u00fcckt", "tokens": ["beu\u00b7gen", "sich", "ver\u00b7ges\u00b7sen", ",", "wel\u00b7ten\u00b7weit", "ent\u00b7r\u00fcckt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PRF", "VVPP", "$,", "NN", "VVPP"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.27": {"text": "\u00dcber die Tiefen,", "tokens": ["\u00dc\u00b7ber", "die", "Tie\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.28": {"text": "draus dein Rohr die gro\u00dfe Wunderblume pfl\u00fcckt.", "tokens": ["draus", "dein", "Rohr", "die", "gro\u00b7\u00dfe", "Wun\u00b7der\u00b7blu\u00b7me", "pfl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}