{"textgrid.poem.60483": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Erinnerung", "genre": "verse", "period": "N.A.", "pub_year": 1823, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Selige Stunde! Da mir meine Bertha", "tokens": ["Se\u00b7li\u00b7ge", "Stun\u00b7de", "!", "Da", "mir", "mei\u00b7ne", "Bert\u00b7ha"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "KOUS", "PPER", "PPOSAT", "NE"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "M\u00e4chtig ergriffen von der Liebe Sehnen", "tokens": ["M\u00e4ch\u00b7tig", "er\u00b7grif\u00b7fen", "von", "der", "Lie\u00b7be", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "APPR", "ART", "NN", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "An den bewegten, ihr allein geweihten", "tokens": ["An", "den", "be\u00b7weg\u00b7ten", ",", "ihr", "al\u00b7lein", "ge\u00b7weih\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "PPER", "ADV", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Busen gesunken.", "tokens": ["Bu\u00b7sen", "ge\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "N\u00e4chtliche Stille lag auf Flur und Hain, es", "tokens": ["N\u00e4cht\u00b7li\u00b7che", "Stil\u00b7le", "lag", "auf", "Flur", "und", "Hain", ",", "es"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,", "PPER"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ruhten die Weste, um die leisen Seufzer", "tokens": ["Ruh\u00b7ten", "die", "Wes\u00b7te", ",", "um", "die", "lei\u00b7sen", "Seuf\u00b7zer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "KOUI", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Nicht zu verwehn, dem Pochen unsrer Herzen", "tokens": ["Nicht", "zu", "ver\u00b7wehn", ",", "dem", "Po\u00b7chen", "uns\u00b7rer", "Her\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "PTKZU", "VVINF", "$,", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Lauschten die Sterne", "tokens": ["Lauschten", "die", "Ster\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Gl\u00fchende K\u00fcsse bebten durch die Seele,", "tokens": ["Gl\u00fc\u00b7hen\u00b7de", "K\u00fcs\u00b7se", "beb\u00b7ten", "durch", "die", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Innig umschlungen hielt ich dich, Geliebte!", "tokens": ["In\u00b7nig", "um\u00b7schlun\u00b7gen", "hielt", "ich", "dich", ",", "Ge\u00b7lieb\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVINF", "VVFIN", "PPER", "PRF", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "G\u00f6ttliche Bertha! Zierde meines Lebens!", "tokens": ["G\u00f6tt\u00b7li\u00b7che", "Bert\u00b7ha", "!", "Zier\u00b7de", "mei\u00b7nes", "Le\u00b7bens", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "NN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Selige Stunde!", "tokens": ["Se\u00b7li\u00b7ge", "Stun\u00b7de", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}