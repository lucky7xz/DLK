{"textgrid.poem.24404": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr Reichen lasset euch den Reichthum nicht gel\u00fcsten/", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Reichen lasset euch den Reichthum nicht gel\u00fcsten/", "tokens": ["Ihr", "Rei\u00b7chen", "las\u00b7set", "euch", "den", "Reicht\u00b7hum", "nicht", "ge\u00b7l\u00fcs\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kauffe euch kein Zucht-Hau\u00df nicht durch euer eigen Geld.", "tokens": ["Kauf\u00b7fe", "euch", "kein", "Zucht\u00b7Hau\u00df", "nicht", "durch", "eu\u00b7er", "ei\u00b7gen", "Geld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "PTKNEG", "APPR", "PPOSAT", "ADJD", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Bey gro\u00dfem Gute dient kein Angst und Sorgen Held/", "tokens": ["Bey", "gro\u00b7\u00dfem", "Gu\u00b7te", "dient", "kein", "Angst", "und", "Sor\u00b7gen", "Held", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PIAT", "NN", "KON", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und nur ein Sclaven Hertz verschlie\u00dft sich in die K\u00fcsten.", "tokens": ["Und", "nur", "ein", "Scla\u00b7ven", "Hertz", "ver\u00b7schlie\u00dft", "sich", "in", "die", "K\u00fcs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein M\u00f6rder saugt sein Blut aus seinen eignen Br\u00fcsten.", "tokens": ["Ein", "M\u00f6r\u00b7der", "saugt", "sein", "Blut", "aus", "sei\u00b7nen", "eig\u00b7nen", "Br\u00fcs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein Kluger lebt vergn\u00fcgt/ und der besiegt die Welt/", "tokens": ["Ein", "Klu\u00b7ger", "lebt", "ver\u00b7gn\u00fcgt", "/", "und", "der", "be\u00b7siegt", "die", "Welt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$(", "KON", "ART", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wenn/ welche Schande doch! durch sie ein Geitzhal\u00df f\u00e4llt/", "tokens": ["Wenn", "/", "wel\u00b7che", "Schan\u00b7de", "doch", "!", "durch", "sie", "ein", "Geitz\u00b7hal\u00df", "f\u00e4llt", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "PWAT", "NN", "ADV", "$.", "APPR", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als ob Metall und Koth ihn zu begl\u00fccken w\u00fc\u00dften.", "tokens": ["Als", "ob", "Me\u00b7tall", "und", "Koth", "ihn", "zu", "be\u00b7gl\u00fc\u00b7cken", "w\u00fc\u00df\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "NN", "KON", "NN", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "In Banden legt er sich; ob sie nun g\u00fcldenseyn/", "tokens": ["In", "Ban\u00b7den", "legt", "er", "sich", ";", "ob", "sie", "nun", "g\u00fcl\u00b7den\u00b7seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PRF", "$.", "KOUS", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So schl\u00e4gt der gro\u00dfe Narr doch seine Seele nein/", "tokens": ["So", "schl\u00e4gt", "der", "gro\u00b7\u00dfe", "Narr", "doch", "sei\u00b7ne", "See\u00b7le", "nein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADV", "PPOSAT", "NN", "PTKANT", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die \u00fcber alles soll auf Erden triumphiren.", "tokens": ["Die", "\u00fc\u00b7ber", "al\u00b7les", "soll", "auf", "Er\u00b7den", "tri\u00b7um\u00b7phi\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIS", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "An Stricken pfleget man die Hunde nur zu f\u00fchren.", "tokens": ["An", "Stri\u00b7cken", "pfle\u00b7get", "man", "die", "Hun\u00b7de", "nur", "zu", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIS", "ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ein Weiser braucht sein Geld/ wird nicht vor Sorgen bleich/", "tokens": ["Ein", "Wei\u00b7ser", "braucht", "sein", "Geld", "/", "wird", "nicht", "vor", "Sor\u00b7gen", "bleich", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$(", "VAFIN", "PTKNEG", "APPR", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Bey Reichthum ist er froh/ und in der Armuth reich.", "tokens": ["Bey", "Reicht\u00b7hum", "ist", "er", "froh", "/", "und", "in", "der", "Ar\u00b7muth", "reich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADJD", "$(", "KON", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}}}}}