{"textgrid.poem.48012": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da ri\u00df der Geist mich fort auf Sturmesfl\u00fcgeln", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da ri\u00df der Geist mich fort auf Sturmesfl\u00fcgeln", "tokens": ["Da", "ri\u00df", "der", "Geist", "mich", "fort", "auf", "Stur\u00b7mes\u00b7fl\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und trug mich \u00fcber Mauer, Tor und Strom", "tokens": ["und", "trug", "mich", "\u00fc\u00b7ber", "Mau\u00b7er", ",", "Tor", "und", "Strom"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "$,", "NE", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "zur alten heiligen Stadt auf sieben H\u00fcgeln.", "tokens": ["zur", "al\u00b7ten", "hei\u00b7li\u00b7gen", "Stadt", "auf", "sie\u00b7ben", "H\u00fc\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "APPR", "CARD", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Die tolle Nacht zum Aschermittwoch sank", "tokens": ["Die", "tol\u00b7le", "Nacht", "zum", "A\u00b7scher\u00b7mitt\u00b7woch", "sank"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "auf Rom herab, das, Rosen in den Locken,", "tokens": ["auf", "Rom", "her\u00b7ab", ",", "das", ",", "Ro\u00b7sen", "in", "den", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "$,", "PDS", "$,", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "berauschten Sinns den Kelch der Freude trank.", "tokens": ["be\u00b7rauschten", "Sinns", "den", "Kelch", "der", "Freu\u00b7de", "trank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Mich aber zog es aus dem L\u00e4rm der Gassen,", "tokens": ["Mich", "a\u00b7ber", "zog", "es", "aus", "dem", "L\u00e4rm", "der", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "der grellen Farbenpracht des Karnevals", "tokens": ["der", "grel\u00b7len", "Far\u00b7ben\u00b7pracht", "des", "Kar\u00b7ne\u00b7vals"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "zu einer Villa, lichtlos und verlassen.", "tokens": ["zu", "ei\u00b7ner", "Vil\u00b7la", ",", "licht\u00b7los", "und", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie kam's, da\u00df sie, die nur der Lust geweiht,", "tokens": ["Wie", "kam's", ",", "da\u00df", "sie", ",", "die", "nur", "der", "Lust", "ge\u00b7weiht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "$,", "KOUS", "PPER", "$,", "PRELS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "die freudenreiche Villa Cavalotti,", "tokens": ["die", "freu\u00b7den\u00b7rei\u00b7che", "Vil\u00b7la", "Ca\u00b7va\u00b7lot\u00b7ti", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "am Fasching sank in Nacht und Dunkelheit?", "tokens": ["am", "Fa\u00b7sching", "sank", "in", "Nacht", "und", "Dun\u00b7kel\u00b7heit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wohl mag sie trauern, da ihr Stern erblich;", "tokens": ["Wohl", "mag", "sie", "trau\u00b7ern", ",", "da", "ihr", "Stern", "er\u00b7blich", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "KOUS", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "mit leisem Seufzen nur um ihre Mauern", "tokens": ["mit", "lei\u00b7sem", "Seuf\u00b7zen", "nur", "um", "ih\u00b7re", "Mau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "gespensterhaft der irre Nachtwind strich:", "tokens": ["ge\u00b7spens\u00b7ter\u00b7haft", "der", "ir\u00b7re", "Nacht\u00b7wind", "strich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Denn er, der r\u00f6mischen Jugend ein Verderben,", "tokens": ["Denn", "er", ",", "der", "r\u00f6\u00b7mi\u00b7schen", "Ju\u00b7gend", "ein", "Ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "des Bundes Meister und der Kunst M\u00e4cen,", "tokens": ["des", "Bun\u00b7des", "Meis\u00b7ter", "und", "der", "Kunst", "M\u00e4\u00b7cen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ernesto Cavalotti, kam zum Sterben.", "tokens": ["Er\u00b7nes\u00b7to", "Ca\u00b7va\u00b7lot\u00b7ti", ",", "kam", "zum", "Ster\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "In Seide ruht er und auf Purpurpf\u00fchlen, \u2013", "tokens": ["In", "Sei\u00b7de", "ruht", "er", "und", "auf", "Pur\u00b7pur\u00b7pf\u00fch\u00b7len", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPER", "KON", "APPR", "NN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und keine Hand erhebt sich, ihm die Stirn,", "tokens": ["und", "kei\u00b7ne", "Hand", "er\u00b7hebt", "sich", ",", "ihm", "die", "Stirn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PRF", "$,", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "die fiebergl\u00fchende, mitleidsvoll zu k\u00fchlen?!", "tokens": ["die", "fie\u00b7berg\u00b7l\u00fc\u00b7hen\u00b7de", ",", "mit\u00b7le\u00b7ids\u00b7voll", "zu", "k\u00fch\u00b7len", "?!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Weilt von den Frauen, die sein Ku\u00df begl\u00fcckt,", "tokens": ["Weilt", "von", "den", "Frau\u00b7en", ",", "die", "sein", "Ku\u00df", "be\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "nicht eine hier, da\u00df sie in Todeswehen", "tokens": ["nicht", "ei\u00b7ne", "hier", ",", "da\u00df", "sie", "in", "To\u00b7des\u00b7we\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "ADV", "$,", "KOUS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "dem vielgeliebten Mann die Kissen r\u00fcckt? \u2013", "tokens": ["dem", "viel\u00b7ge\u00b7lieb\u00b7ten", "Mann", "die", "Kis\u00b7sen", "r\u00fcckt", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Hielt keiner seiner Freunde bei ihm stand? \u2013", "tokens": ["Hielt", "kei\u00b7ner", "sei\u00b7ner", "Freun\u00b7de", "bei", "ihm", "stand", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PIS", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sind schon die Funken, die sein Geist verspr\u00fchte,", "tokens": ["Sind", "schon", "die", "Fun\u00b7ken", ",", "die", "sein", "Geist", "ver\u00b7spr\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "bevor sein Blick erloschen, ausgebrannt?! \u2013 \u2013", "tokens": ["be\u00b7vor", "sein", "Blick", "er\u00b7lo\u00b7schen", ",", "aus\u00b7ge\u00b7brannt", "?!", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVINF", "$,", "VVPP", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Die Lust des Faschings rief sie alle \u2013 alle \u2013;", "tokens": ["Die", "Lust", "des", "Fa\u00b7schings", "rief", "sie", "al\u00b7le", "\u2013", "al\u00b7le", "\u2013", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "PIAT", "$(", "PIAT", "$(", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "ein Diener nur mit welkem Angesicht", "tokens": ["ein", "Die\u00b7ner", "nur", "mit", "wel\u00b7kem", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "PWAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "lehnt einsam wachend in der S\u00e4ulenhalle,", "tokens": ["lehnt", "ein\u00b7sam", "wa\u00b7chend", "in", "der", "S\u00e4u\u00b7len\u00b7hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "indes der kranke Herr sich unruhvoll", "tokens": ["in\u00b7des", "der", "kran\u00b7ke", "Herr", "sich", "un\u00b7ruh\u00b7voll"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "PRF", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "auf seinem Lager dehnt und von der Gasse", "tokens": ["auf", "sei\u00b7nem", "La\u00b7ger", "dehnt", "und", "von", "der", "Gas\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "das Fastnachtstreiben laut und lauter scholl.", "tokens": ["das", "Fast\u00b7nacht\u00b7strei\u00b7ben", "laut", "und", "lau\u00b7ter", "scholl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Da pl\u00f6tzlich ", "tokens": ["Da", "pl\u00f6tz\u00b7lich"], "token_info": ["word", "word"], "pos": ["ADV", "ADJD"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "es ist, als habe ein Trompetensto\u00df", "tokens": ["es", "ist", ",", "als", "ha\u00b7be", "ein", "Trom\u00b7pe\u00b7ten\u00b7sto\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "den fliehenden Geist belebt zur letzten L\u00fcge.", "tokens": ["den", "flie\u00b7hen\u00b7den", "Geist", "be\u00b7lebt", "zur", "letz\u00b7ten", "L\u00fc\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.13": {"line.1": {"text": "Er strebt empor und winkt dem alten Mann:", "tokens": ["Er", "strebt", "em\u00b7por", "und", "winkt", "dem", "al\u00b7ten", "Mann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbdie andern sind zerstoben und verflogen \u2013", "tokens": ["\u00bb", "die", "an\u00b7dern", "sind", "zer\u00b7sto\u00b7ben", "und", "ver\u00b7flo\u00b7gen", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "VAFIN", "VVPP", "KON", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du bliebst mir treu; nun h\u00f6r mein Letztes an.", "tokens": ["Du", "bliebst", "mir", "treu", ";", "nun", "h\u00f6r", "mein", "Letz\u00b7tes", "an", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$.", "ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Mein Lebenlang hab ich gewirkt im Dienste", "tokens": ["Mein", "Le\u00b7ben\u00b7lang", "hab", "ich", "ge\u00b7wirkt", "im", "Diens\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "der freien Wahrheit, hab verh\u00f6hnt, verflucht", "tokens": ["der", "frei\u00b7en", "Wahr\u00b7heit", ",", "hab", "ver\u00b7h\u00f6hnt", ",", "ver\u00b7flucht"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VAFIN", "VVPP", "$,", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "der Priesterweisheit taube Hirngespinste.", "tokens": ["der", "Pries\u00b7ter\u00b7weis\u00b7heit", "tau\u00b7be", "Hirn\u00b7ge\u00b7spins\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.15": {"line.1": {"text": "Das Kreuz, davor der feige P\u00f6bel kriecht,", "tokens": ["Das", "Kreuz", ",", "da\u00b7vor", "der", "fei\u00b7ge", "P\u00f6\u00b7bel", "kriecht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "mit F\u00fc\u00dfen trat ich's und zerri\u00df der Dornen", "tokens": ["mit", "F\u00fc\u00b7\u00dfen", "trat", "ich's", "und", "zer\u00b7ri\u00df", "der", "Dor\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PIS", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gewinde, das um seinen Stamm sich flicht.", "tokens": ["Ge\u00b7win\u00b7de", ",", "das", "um", "sei\u00b7nen", "Stamm", "sich", "flicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.16": {"line.1": {"text": "Die Wahngebilde, ich bezwang sie alle \u2013", "tokens": ["Die", "Wahn\u00b7ge\u00b7bil\u00b7de", ",", "ich", "be\u00b7zwang", "sie", "al\u00b7le", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "PPER", "PIAT", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "nun kommt der schwerste Kampf, der st\u00e4rkste Feind", "tokens": ["nun", "kommt", "der", "schwers\u00b7te", "Kampf", ",", "der", "st\u00e4rks\u00b7te", "Feind"], "token_info": ["word", "word", "word", "word", "word",