{"textgrid.poem.55726": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zur Abendzeit, wenn sich die Wolken f\u00e4rben,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zur Abendzeit, wenn sich die Wolken f\u00e4rben,", "tokens": ["Zur", "A\u00b7bend\u00b7zeit", ",", "wenn", "sich", "die", "Wol\u00b7ken", "f\u00e4r\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn Alles gl\u00fcht in r\u00f6tlich-gelbem Licht,", "tokens": ["Wenn", "Al\u00b7les", "gl\u00fcht", "in", "r\u00f6t\u00b7lich\u00b7gel\u00b7bem", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und keusch und lauter meine Seele spricht, \u2013", "tokens": ["Und", "keusch", "und", "lau\u00b7ter", "mei\u00b7ne", "See\u00b7le", "spricht", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "KON", "PIAT", "PPOSAT", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zur Abendzeit, wenn sich die Wolken f\u00e4rben.", "tokens": ["Zur", "A\u00b7bend\u00b7zeit", ",", "wenn", "sich", "die", "Wol\u00b7ken", "f\u00e4r\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und es erwacht in ihr ein tiefes Sehnen,", "tokens": ["Und", "es", "er\u00b7wacht", "in", "ihr", "ein", "tie\u00b7fes", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach keuscher Liebe zittert ihr Gebet,", "tokens": ["Nach", "keu\u00b7scher", "Lie\u00b7be", "zit\u00b7tert", "ihr", "Ge\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und niederrieseln \u00e4tzend-scharfe Tr\u00e4nen,", "tokens": ["Und", "nie\u00b7der\u00b7rie\u00b7seln", "\u00e4t\u00b7zen\u00b7dscha\u00b7rfe", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie wei\u00df es wohl, da\u00df es zu sp\u00e4t, zu sp\u00e4t!", "tokens": ["Sie", "wei\u00df", "es", "wohl", ",", "da\u00df", "es", "zu", "sp\u00e4t", ",", "zu", "sp\u00e4t", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "PTKA", "ADJD", "$,", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und immer qualenreicher wird das Sehnen.", "tokens": ["Und", "im\u00b7mer", "qua\u00b7len\u00b7rei\u00b7cher", "wird", "das", "Seh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mit stiller Freude sehn es die D\u00e4monen", "tokens": ["Mit", "stil\u00b7ler", "Freu\u00b7de", "sehn", "es", "die", "D\u00e4\u00b7mo\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und fl\u00fcstern s\u00fc\u00df-verlockend ihr ins Ohr:", "tokens": ["Und", "fl\u00fcs\u00b7tern", "s\u00fc\u00df\u00b7ver\u00b7lo\u00b7ckend", "ihr", "ins", "Ohr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir wollen Dich f\u00fcr jede Qual entlohnen,", "tokens": ["Wir", "wol\u00b7len", "Dich", "f\u00fcr", "je\u00b7de", "Qual", "ent\u00b7loh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf, auf, sei stark und raffe Dich empor!", "tokens": ["Auf", ",", "auf", ",", "sei", "stark", "und", "raf\u00b7fe", "Dich", "em\u00b7por", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PTKVZ", "$,", "VAFIN", "ADJD", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und immer s\u00fc\u00dfer locken die D\u00e4monen", "tokens": ["Und", "im\u00b7mer", "s\u00fc\u00b7\u00dfer", "lo\u00b7cken", "die", "D\u00e4\u00b7mo\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Erstorbne Gluten wieder sich entflammen,", "tokens": ["E\u00b7rstorb\u00b7ne", "Glu\u00b7ten", "wie\u00b7der", "sich", "ent\u00b7flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es st\u00f6hnt das Herz nach jener Trunkenheit,", "tokens": ["Es", "st\u00f6hnt", "das", "Herz", "nach", "je\u00b7ner", "Trun\u00b7ken\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In welcher das Bewu\u00dftsein bricht zusammen,", "tokens": ["In", "wel\u00b7cher", "das", "Be\u00b7wu\u00df\u00b7tsein", "bricht", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nach Wollustkr\u00e4mpfen meine Seele schreit;", "tokens": ["Nach", "Wol\u00b7lust\u00b7kr\u00e4mp\u00b7fen", "mei\u00b7ne", "See\u00b7le", "schreit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Erstorbne Gluten wieder sich entflammen!", "tokens": ["E\u00b7rstorb\u00b7ne", "Glu\u00b7ten", "wie\u00b7der", "sich", "ent\u00b7flam\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}