{"textgrid.poem.56964": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vorfr\u00fchling seufzt in weiter Nacht,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vorfr\u00fchling seufzt in weiter Nacht,", "tokens": ["Vor\u00b7fr\u00fch\u00b7ling", "seufzt", "in", "wei\u00b7ter", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "da\u00df mir das Herze brechen will;", "tokens": ["da\u00df", "mir", "das", "Her\u00b7ze", "bre\u00b7chen", "will", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VVFIN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "die Lande ruhn so menschenstill,", "tokens": ["die", "Lan\u00b7de", "ruhn", "so", "men\u00b7schen\u00b7still", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "nur ich bin aufgewacht.", "tokens": ["nur", "ich", "bin", "auf\u00b7ge\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Oh horch, nun bricht des Eises Wall", "tokens": ["Oh", "horch", ",", "nun", "bricht", "des", "Ei\u00b7ses", "Wall"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ITJ", "$,", "ADV", "VVFIN", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "auf allen Str\u00f6men, allen Seen;", "tokens": ["auf", "al\u00b7len", "Str\u00f6\u00b7men", ",", "al\u00b7len", "Seen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "mir ist, ich m\u00fc\u00dfte mit vergehn", "tokens": ["mir", "ist", ",", "ich", "m\u00fc\u00df\u00b7te", "mit", "ver\u00b7gehn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VMFIN", "APPR", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und, Woge, wieder auferstehn", "tokens": ["und", ",", "Wo\u00b7ge", ",", "wie\u00b7der", "auf\u00b7er\u00b7stehn"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "$,", "NN", "$,", "ADV", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "zu neuem Klippenfall.", "tokens": ["zu", "neu\u00b7em", "Klip\u00b7pen\u00b7fall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Lande ruhn so menschenstill;", "tokens": ["Die", "Lan\u00b7de", "ruhn", "so", "men\u00b7schen\u00b7still", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "nur hier und dort ist wer erwacht,", "tokens": ["nur", "hier", "und", "dort", "ist", "wer", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und seine Seele weint und lacht,", "tokens": ["und", "sei\u00b7ne", "See\u00b7le", "weint", "und", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "wie es der Tauwind will.", "tokens": ["wie", "es", "der", "Tau\u00b7wind", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}