{"dta.poem.16259": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "313.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man sagt wol, ein Ersatz, ein zeit'ger L\u00fcckenb\u00fc\u00dfer,", "tokens": ["Man", "sagt", "wol", ",", "ein", "Er\u00b7satz", ",", "ein", "zeit'\u00b7ger", "L\u00fc\u00b7cken\u00b7b\u00fc\u00b7\u00dfer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "$,", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Nicht jeder Forderung gen\u00fcgen soll' und m\u00fc\u00df' er.", "tokens": ["Nicht", "je\u00b7der", "For\u00b7de\u00b7rung", "ge\u00b7n\u00fc\u00b7gen", "soll'", "und", "m\u00fc\u00df'", "er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "VVINF", "VMFIN", "KON", "VMFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch wenn er wirklichem Bed\u00fcrfnis nicht gen\u00fcgt,", "tokens": ["Doch", "wenn", "er", "wirk\u00b7li\u00b7chem", "Be\u00b7d\u00fcrf\u00b7nis", "nicht", "ge\u00b7n\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJA", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ists besser da\u00df man nicht den Wunsch mit ihm betr\u00fcgt.", "tokens": ["Ists", "bes\u00b7ser", "da\u00df", "man", "nicht", "den", "Wunsch", "mit", "ihm", "be\u00b7tr\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOUS", "PIS", "PTKNEG", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Denn das Bed\u00fcrfnis w\u00fcrkt, solang die L\u00fcck' ist offen;", "tokens": ["Denn", "das", "Be\u00b7d\u00fcrf\u00b7nis", "w\u00fcrkt", ",", "so\u00b7lang", "die", "L\u00fcck", "ist", "of\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "KOUS", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist sie zum Schein gef\u00fcllt, bleibt Bessrung nicht zu hoffen.", "tokens": ["Ist", "sie", "zum", "Schein", "ge\u00b7f\u00fcllt", ",", "bleibt", "Bess\u00b7rung", "nicht", "zu", "hof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "VVPP", "$,", "VVFIN", "NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}