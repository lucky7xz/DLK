{"dta.poem.13607": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Treue .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Frisch auf, mein Herz! wie hei\u00df auch das Gedr\u00e4nge,", "tokens": ["Frisch", "auf", ",", "mein", "Herz", "!", "wie", "hei\u00df", "auch", "das", "Ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "PPOSAT", "NN", "$.", "PWAV", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bewahr' ich doch mir k\u00fchl und frei die Brust!", "tokens": ["Be\u00b7wahr'", "ich", "doch", "mir", "k\u00fchl", "und", "frei", "die", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "ADJD", "KON", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Schickt Wald und Flur doch noch die alten Kl\u00e4nge,", "tokens": ["Schickt", "Wald", "und", "Flur", "doch", "noch", "die", "al\u00b7ten", "Kl\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ersch\u00fctternd mich mit wunderbarer Lust.", "tokens": ["Er\u00b7sch\u00fct\u00b7ternd", "mich", "mit", "wun\u00b7der\u00b7ba\u00b7rer", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und ob die Woge feindlich mit mir r\u00e4nge:", "tokens": ["Und", "ob", "die", "Wo\u00b7ge", "feind\u00b7lich", "mit", "mir", "r\u00e4n\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So fr\u00f6mmer nur sing' ich aus treuer Brust;", "tokens": ["So", "fr\u00f6m\u00b7mer", "nur", "sing'", "ich", "aus", "treu\u00b7er", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da bleicht das Wetter, Himmelblau scheint helle,", "tokens": ["Da", "bleicht", "das", "Wet\u00b7ter", ",", "Him\u00b7mel\u00b7blau", "scheint", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "NN", "VVFIN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Das Meer wird still und zum Delphin die Welle.", "tokens": ["Das", "Meer", "wird", "still", "und", "zum", "Del\u00b7phin", "die", "Wel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "\u201ewas wollt' Ihr doch mit Euer'm Lieder-Spa\u00dfe!", "tokens": ["\u201e", "was", "wollt'", "Ihr", "doch", "mit", "Eu\u00b7er'm", "Lie\u00b7der\u00b7Spa\u00b7\u00dfe", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des W\u00fcrd'gern beut die gro\u00dfe Zeit so viel!\u201c", "tokens": ["Des", "W\u00fcrd'\u00b7gern", "beut", "die", "gro\u00b7\u00dfe", "Zeit", "so", "viel", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So schallt's hoff\u00e4rtig nun auf jeder Gasse,", "tokens": ["So", "schallt's", "hof\u00b7f\u00e4r\u00b7tig", "nun", "auf", "je\u00b7der", "Gas\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und jeder steckt sich dreist sein gl\u00e4nzend Ziel.", "tokens": ["Und", "je\u00b7der", "steckt", "sich", "dreist", "sein", "gl\u00e4n\u00b7zend", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PRF", "ADJD", "VAINF", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Lieder, die ich stammelnd h\u00f6ren lasse,", "tokens": ["Die", "Lie\u00b7der", ",", "die", "ich", "stam\u00b7melnd", "h\u00f6\u00b7ren", "las\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ew'ger Gef\u00fchle schwaches Wiederspiel, \u2014", "tokens": ["Ew'\u00b7ger", "Ge\u00b7f\u00fch\u00b7le", "schwa\u00b7ches", "Wie\u00b7der\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Sie sind es wahrlich auch nicht, was ich meine,", "tokens": ["Sie", "sind", "es", "wahr\u00b7lich", "auch", "nicht", ",", "was", "ich", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Denn ewig unerreichbar ist das Eine.", "tokens": ["Denn", "e\u00b7wig", "un\u00b7er\u00b7reich\u00b7bar", "ist", "das", "Ei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch lieben oft, der Sehnsucht Gluth zu mildern,", "tokens": ["Doch", "lie\u00b7ben", "oft", ",", "der", "Sehn\u00b7sucht", "Gluth", "zu", "mil\u00b7dern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "ART", "NN", "NN", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gefang'ne wohl, das ferne Vaterland", "tokens": ["Ge\u00b7fang'\u00b7ne", "wohl", ",", "das", "fer\u00b7ne", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "An ihres Kerkers Mauern abzuschildern.", "tokens": ["An", "ih\u00b7res", "Ker\u00b7kers", "Mau\u00b7ern", "ab\u00b7zu\u00b7schil\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Himmelsstrahl f\u00e4llt schweifend auf die Wand,", "tokens": ["Ein", "Him\u00b7mels\u00b7strahl", "f\u00e4llt", "schwei\u00b7fend", "auf", "die", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da r\u00fchrt's lebendig sich in allen Bildern. \u2014", "tokens": ["Da", "r\u00fchrt's", "le\u00b7ben\u00b7dig", "sich", "in", "al\u00b7len", "Bil\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PRF", "APPR", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dem Auge scheint's ein lieblich bunter Tand \u2014", "tokens": ["Dem", "Au\u00b7ge", "scheint's", "ein", "lieb\u00b7lich", "bun\u00b7ter", "Tand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Doch wer der lichten Heimath recht zu eigen,", "tokens": ["Doch", "wer", "der", "lich\u00b7ten", "Hei\u00b7math", "recht", "zu", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJA", "NN", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dem wird der Bilder ernster Geist sich zeigen.", "tokens": ["Dem", "wird", "der", "Bil\u00b7der", "erns\u00b7ter", "Geist", "sich", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So wachse denn und treibe fr\u00f6hlich Bl\u00fcthe,", "tokens": ["So", "wach\u00b7se", "denn", "und", "trei\u00b7be", "fr\u00f6h\u00b7lich", "Bl\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "KON", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du kr\u00e4ftig gr\u00fcner deutscher Sangesbaum!", "tokens": ["Du", "kr\u00e4f\u00b7tig", "gr\u00fc\u00b7ner", "deut\u00b7scher", "San\u00b7ges\u00b7baum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Rausch' nur erfrischend fort mir in's Gem\u00fcthe", "tokens": ["Rausch'", "nur", "er\u00b7fri\u00b7schend", "fort", "mir", "in's", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADJD", "PTKVZ", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus Deiner Wipfel klarem Himmelsraum!", "tokens": ["Aus", "Dei\u00b7ner", "Wip\u00b7fel", "kla\u00b7rem", "Him\u00b7mels\u00b7raum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du aber, wunderbare, ew'ge G\u00fcte,", "tokens": ["Du", "a\u00b7ber", ",", "wun\u00b7der\u00b7ba\u00b7re", ",", "ew'\u00b7ge", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die mir den Himmel wies im sch\u00f6nen Traum,", "tokens": ["Die", "mir", "den", "Him\u00b7mel", "wies", "im", "sch\u00f6\u00b7nen", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Erhalt' auf Erden r\u00fcstig mir die Seele,", "tokens": ["Er\u00b7halt'", "auf", "Er\u00b7den", "r\u00fcs\u00b7tig", "mir", "die", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ADJD", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df ich, wo's immer ehrlich gilt, nicht fehle!", "tokens": ["Da\u00df", "ich", ",", "wo's", "im\u00b7mer", "ehr\u00b7lich", "gilt", ",", "nicht", "feh\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWAV", "ADV", "ADJD", "VVFIN", "$,", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}