{"dta.poem.13215": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z ehndes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es hielten viere noch der andern Kinder Flor,", "tokens": ["Es", "hiel\u00b7ten", "vie\u00b7re", "noch", "der", "an\u00b7dern", "Kin\u00b7der", "Flor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADV", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "390Die schw\u00e4zt- und lispelten einander in das Ohr.", "tokens": ["schw\u00e4z\u00b7t", "und", "lis\u00b7pel\u00b7ten", "ein\u00b7an\u00b7der", "in", "das", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Hat eines Fr\u00fchlings Reiz was liebliches zu zeigen,", "tokens": ["Hat", "ei\u00b7nes", "Fr\u00fch\u00b7lings", "Reiz", "was", "lieb\u00b7li\u00b7ches", "zu", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "PWS", "ADJA", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So war es hier gewi\u00df der Kinder Anblick eigen.", "tokens": ["So", "war", "es", "hier", "ge\u00b7wi\u00df", "der", "Kin\u00b7der", "An\u00b7blick", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wann diesen (d\u00fcnckte mich) GOtt keine Kron\u2019 erlaubt,", "tokens": ["Wann", "die\u00b7sen", "(", "d\u00fcnck\u00b7te", "mich", ")", "Gott", "kei\u00b7ne", "Kron'", "er\u00b7laubt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "$(", "VVFIN", "PPER", "$(", "NN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So bleibet mancher Thron des gr\u00f6\u00dften Schmucks beraubt.", "tokens": ["So", "blei\u00b7bet", "man\u00b7cher", "Thron", "des", "gr\u00f6\u00df\u00b7ten", "Schmucks", "be\u00b7raubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}