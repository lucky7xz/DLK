{"textgrid.poem.33184": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "2106.", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O allerliebster Herr Jesu Christ! etc.", "tokens": ["O", "al\u00b7ler\u00b7liebs\u00b7ter", "Herr", "Je\u00b7su", "Christ", "!", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "word", "punct", "abbreviation"], "pos": ["NE", "ADJA", "NN", "NE", "NE", "$.", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Die sache k\u00f6mt w\u00fcrklich etc.", "tokens": ["Die", "sa\u00b7che", "k\u00f6mt", "w\u00fcrk\u00b7lich", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ART", "ADJA", "VVFIN", "ADJD", "ADV"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Ein armes s\u00fcnder-herz, etc.", "tokens": ["Ein", "ar\u00b7mes", "s\u00fcn\u00b7der\u00b7herz", ",", "et\u00b7c."], "token_info": ["word", "word", "word", "punct", "abbreviation"], "pos": ["ART", "ADJA", "NN", "$,", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Lamm deiner wunden-maal etc.", "tokens": ["Lamm", "dei\u00b7ner", "wun\u00b7den\u00b7maal", "et\u00b7c."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["NN", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Herr Jesu! habe ewig preis und ehr; wo w\u00e4ren wir doch, wenn kein Jesus war?", "tokens": ["Herr", "Je\u00b7su", "!", "ha\u00b7be", "e\u00b7wig", "preis", "und", "ehr", ";", "wo", "w\u00e4\u00b7ren", "wir", "doch", ",", "wenn", "kein", "Je\u00b7sus", "war", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "VAFIN", "ADJD", "PTKVZ", "KON", "NN", "$.", "PWAV", "VAFIN", "PPER", "ADV", "$,", "KOUS", "PIAT", "NE", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Das ist meine lust, etc.", "tokens": ["Das", "ist", "mei\u00b7ne", "lust", ",", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "punct", "abbreviation"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Sey gelobt, Herr Jesu Christ, da\u00df du mensch gebohren bist!", "tokens": ["Sey", "ge\u00b7lobt", ",", "Herr", "Je\u00b7su", "Christ", ",", "da\u00df", "du", "mensch", "ge\u00b7boh\u00b7ren", "bist", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "NN", "NE", "NE", "$,", "KOUS", "PPER", "ADJD", "VVPP", "VAFIN", "$."], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}}}}}