{"textgrid.poem.45604": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Ultimatum", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein kecker Schritt, der ohne Folgen blieb,", "tokens": ["Ein", "ke\u00b7cker", "Schritt", ",", "der", "oh\u00b7ne", "Fol\u00b7gen", "blieb", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hat schnell den Freundschaftsbund gebrochen.", "tokens": ["Hat", "schnell", "den", "Freund\u00b7schafts\u00b7bund", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Hinterpf\u00f6rtchen ist jedem lieb,", "tokens": ["Ein", "Hin\u00b7ter\u00b7pf\u00f6rt\u00b7chen", "ist", "je\u00b7dem", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der vor der eignen Courage erschrocken.", "tokens": ["Der", "vor", "der", "eig\u00b7nen", "Cou\u00b7ra\u00b7ge", "er\u00b7schro\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}