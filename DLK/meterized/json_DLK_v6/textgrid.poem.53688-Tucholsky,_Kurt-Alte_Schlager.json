{"textgrid.poem.53688": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Alte Schlager", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Braun sind schon die Felder", "tokens": ["Braun", "sind", "schon", "die", "Fel\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ADV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Gelb die Stoppelfelder,", "tokens": ["Gelb", "die", "Stop\u00b7pel\u00b7fel\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und der Herbst beginnt.", "tokens": ["Und", "der", "Herbst", "be\u00b7ginnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "--+-+", "measure": "anapaest.init"}, "line.4": {"text": "Rote Bl\u00e4tter fallen,", "tokens": ["Ro\u00b7te", "Bl\u00e4t\u00b7ter", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Graue Nebel wallen,", "tokens": ["Grau\u00b7e", "Ne\u00b7bel", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "K\u00fchler weht der Wind.", "tokens": ["K\u00fch\u00b7ler", "weht", "der", "Wind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Mit dem reichen Schlu\u00dfvers:", "tokens": ["Mit", "dem", "rei\u00b7chen", "Schlu\u00df\u00b7vers", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Geige t\u00f6nt und Fl\u00f6te", "tokens": ["Gei\u00b7ge", "t\u00f6nt", "und", "Fl\u00f6\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Bei der Abendr\u00f6te", "tokens": ["Bei", "der", "A\u00b7bend\u00b7r\u00f6\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und bei Mondenglanz;", "tokens": ["Und", "bei", "Mon\u00b7den\u00b7glanz", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Sch\u00f6ne Winzerinnen", "tokens": ["Sch\u00f6\u00b7ne", "Win\u00b7ze\u00b7rin\u00b7nen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Winken und beginnen", "tokens": ["Win\u00b7ken", "und", "be\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Deutschen Ringeltanz.", "tokens": ["Deut\u00b7schen", "Rin\u00b7gel\u00b7tanz", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Als der Gro\u00dfvater die Gro\u00dfmutter nahm,", "tokens": ["Als", "der", "Gro\u00df\u00b7va\u00b7ter", "die", "Gro\u00df\u00b7mut\u00b7ter", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--++-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Da wu\u00dfte man nichts von Mamsell und Madam;", "tokens": ["Da", "wu\u00df\u00b7te", "man", "nichts", "von", "Mam\u00b7sell", "und", "Ma\u00b7dam", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PIS", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die z\u00fcchtige Jungfrau, das h\u00e4usliche Weib,", "tokens": ["Die", "z\u00fcch\u00b7ti\u00b7ge", "Jung\u00b7frau", ",", "das", "h\u00e4us\u00b7li\u00b7che", "Weib", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Sie waren echt deutsch noch an Seel und Leib.", "tokens": ["Sie", "wa\u00b7ren", "echt", "deutsch", "noch", "an", "Seel", "und", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADJD", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Als der Gro\u00dfvater die Gro\u00dfmutter nahm,", "tokens": ["Als", "der", "Gro\u00df\u00b7va\u00b7ter", "die", "Gro\u00df\u00b7mut\u00b7ter", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--++-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Da herrschte noch sittig verschleierte Scham . . .", "tokens": ["Da", "herrschte", "noch", "sit\u00b7tig", "ver\u00b7schlei\u00b7er\u00b7te", "Scham", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "ADJA", "NN", "$.", "$.", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Aber als er sie wirklich nahm \u2013 im Jahre 1747 \u2013, da sang Hagedorn:", "tokens": ["A\u00b7ber", "als", "er", "sie", "wirk\u00b7lich", "nahm", "\u2013", "im", "Jah\u00b7re", "1747", "\u2013", ",", "da", "sang", "Ha\u00b7ge\u00b7dorn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "number", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$(", "APPRART", "NN", "CARD", "$(", "$,", "ADV", "VVFIN", "NE", "$."], "meter": "+--+-+-+-+-+++-+", "measure": "iambic.octa.plus.invert"}}, "stanza.7": {"line.1": {"text": "Zu meiner Zeit", "tokens": ["Zu", "mei\u00b7ner", "Zeit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Bestand noch Recht und Billigkeit.", "tokens": ["Be\u00b7stand", "noch", "Recht", "und", "Bil\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da wurden auch aus Kindern Leute,", "tokens": ["Da", "wur\u00b7den", "auch", "aus", "Kin\u00b7dern", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da wurden auch aus Jungfern Br\u00e4ute,", "tokens": ["Da", "wur\u00b7den", "auch", "aus", "Jung\u00b7fern", "Br\u00e4u\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch alles mit Bescheidenheit . . .", "tokens": ["Doch", "al\u00b7les", "mit", "Be\u00b7schei\u00b7den\u00b7heit", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "PIS", "APPR", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ein schwaches Volk ist stets ein K\u00f6nig Lear!", "tokens": ["Ein", "schwa\u00b7ches", "Volk", "ist", "stets", "ein", "K\u00f6\u00b7nig", "Le\u00b7ar", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Die Jugend ist um ihretwillen hier;", "tokens": ["Die", "Ju\u00b7gend", "ist", "um", "ih\u00b7ret\u00b7wil\u00b7len", "hier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPOSAT", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es w\u00e4re t\u00f6richt zu verlangen:", "tokens": ["Es", "w\u00e4\u00b7re", "t\u00f6\u00b7richt", "zu", "ver\u00b7lan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Komm, \u00e4ltle du mit mir.", "tokens": ["Komm", ",", "\u00e4lt\u00b7le", "du", "mit", "mir", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}