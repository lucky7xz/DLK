{"textgrid.poem.50008": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "40. An den h\u00f6hnischen Damon", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dass dir missf\u00e4llt, wenn Thrax mit ", "tokens": ["Dass", "dir", "miss\u00b7f\u00e4llt", ",", "wenn", "Thrax", "mit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "NE", "APPR"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Giebst du durch nichts uns zu verstehn,", "tokens": ["Giebst", "du", "durch", "nichts", "uns", "zu", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIS", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als durch ", "tokens": ["Als", "durch"], "token_info": ["word", "word"], "pos": ["KOUS", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Ich lach', und die Gesellschafft find't,", "tokens": ["Ich", "lach'", ",", "und", "die", "Ge\u00b7sell\u00b7schafft", "find't", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dass ", "tokens": ["Dass"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}}}}}