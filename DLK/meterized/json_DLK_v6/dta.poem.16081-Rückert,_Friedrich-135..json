{"dta.poem.16081": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "135.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Gute mu\u00dft du hin, wo's angewandt ist, wenden;", "tokens": ["Das", "Gu\u00b7te", "mu\u00dft", "du", "hin", ",", "wo's", "an\u00b7ge\u00b7wandt", "ist", ",", "wen\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKVZ", "$,", "PWAV", "VVPP", "VAFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo sie ist wohlgethan, mu\u00dft du die Wohlthat spenden.", "tokens": ["Wo", "sie", "ist", "wohl\u00b7ge\u00b7than", ",", "mu\u00dft", "du", "die", "Wohlt\u00b7hat", "spen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "ADJD", "$,", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn mancher schlechte hat so einen schlechten Magen,", "tokens": ["Denn", "man\u00b7cher", "schlech\u00b7te", "hat", "so", "ei\u00b7nen", "schlech\u00b7ten", "Ma\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was wohl dem Guten thut, das kann er nicht vertragen.", "tokens": ["Was", "wohl", "dem", "Gu\u00b7ten", "thut", ",", "das", "kann", "er", "nicht", "ver\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVFIN", "$,", "PDS", "VMFIN", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}