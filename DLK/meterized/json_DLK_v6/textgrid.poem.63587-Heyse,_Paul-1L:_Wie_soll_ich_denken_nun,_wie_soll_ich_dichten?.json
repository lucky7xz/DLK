{"textgrid.poem.63587": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie soll ich denken nun, wie soll ich dichten?", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie soll ich denken nun, wie soll ich dichten?", "tokens": ["Wie", "soll", "ich", "den\u00b7ken", "nun", ",", "wie", "soll", "ich", "dich\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVFIN", "ADV", "$,", "PWAV", "VMFIN", "PPER", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich war verw\u00f6hnt, hinweg von meinem Blatte", "tokens": ["Ich", "war", "ver\u00b7w\u00f6hnt", ",", "hin\u00b7weg", "von", "mei\u00b7nem", "Blat\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Oft auf ein kleines Haupt den Blick zu richten;", "tokens": ["Oft", "auf", "ein", "klei\u00b7nes", "Haupt", "den", "Blick", "zu", "rich\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wenn das Sinnen mich erm\u00fcdet hatte,", "tokens": ["Und", "wenn", "das", "Sin\u00b7nen", "mich", "er\u00b7m\u00fc\u00b7det", "hat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "An seinem Schlaf den wachen Geist zu st\u00e4rken,", "tokens": ["An", "sei\u00b7nem", "Schlaf", "den", "wa\u00b7chen", "Geist", "zu", "st\u00e4r\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Stirne k\u00fcssend ihm, die lilienglatte;", "tokens": ["Die", "Stir\u00b7ne", "k\u00fcs\u00b7send", "ihm", ",", "die", "li\u00b7li\u00b7en\u00b7glat\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PPER", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Auf seines Atems Ebb' und Flut zu merken,", "tokens": ["Auf", "sei\u00b7nes", "A\u00b7tems", "Ebb'", "und", "Flut", "zu", "mer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Als ob ein Hauch von unbewu\u00dftem Sein", "tokens": ["Als", "ob", "ein", "Hauch", "von", "un\u00b7be\u00b7wu\u00df\u00b7tem", "Sein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Sich mische so den wohlbedachten Werken.", "tokens": ["Sich", "mi\u00b7sche", "so", "den", "wohl\u00b7be\u00b7dach\u00b7ten", "Wer\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und wacht' es auf, so lief's zu mir herein", "tokens": ["Und", "wacht'", "es", "auf", ",", "so", "lie\u00b7f's", "zu", "mir", "her\u00b7ein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "ADJD", "APPR", "PPER", "PTKVZ"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Und wollte mich durchaus zum Spielgesellen", "tokens": ["Und", "woll\u00b7te", "mich", "durc\u00b7haus", "zum", "Spiel\u00b7ge\u00b7sel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und ruhte nicht, bis wir den Ball zu zwein", "tokens": ["Und", "ruh\u00b7te", "nicht", ",", "bis", "wir", "den", "Ball", "zu", "zwein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Hin \u00fcber meinen Teppich lie\u00dfen schnellen,", "tokens": ["Hin", "\u00fc\u00b7ber", "mei\u00b7nen", "Tep\u00b7pich", "lie\u00b7\u00dfen", "schnel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Auch wohl, wenn ich beharrlich weiter schrieb,", "tokens": ["Auch", "wohl", ",", "wenn", "ich", "be\u00b7harr\u00b7lich", "wei\u00b7ter", "schrieb", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Kramt' es die B\u00fccher mir von den Gestellen,", "tokens": ["Kramt'", "es", "die", "B\u00fc\u00b7cher", "mir", "von", "den", "Ge\u00b7stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+--+-+++-+-", "measure": "iambic.hexa.invert"}, "line.16": {"text": "Holdselig lachend, der verschmitzte Dieb,", "tokens": ["Hold\u00b7se\u00b7lig", "la\u00b7chend", ",", "der", "ver\u00b7schmitz\u00b7te", "Dieb", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.17": {"text": "Droht' ich, den Raub ihm wieder abzujagen,", "tokens": ["Droht'", "ich", ",", "den", "Raub", "ihm", "wie\u00b7der", "ab\u00b7zu\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "PPER", "ADV", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Bis sorglich ihn die W\u00e4rterin vertrieb:", "tokens": ["Bis", "sorg\u00b7lich", "ihn", "die", "W\u00e4r\u00b7te\u00b7rin", "ver\u00b7trieb", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "\u00bbdu st\u00f6rst Papa; la\u00df zur Mama dich tragen!\u00ab", "tokens": ["\u00bb", "du", "st\u00f6rst", "Pa\u00b7pa", ";", "la\u00df", "zur", "Ma\u00b7ma", "dich", "tra\u00b7gen", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "NN", "$.", "VVIMP", "APPRART", "NN", "PPER", "VVINF", "$.", "$("], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Nun \u2013 bittrer Hohn! \u2013 nun st\u00f6rt mich niemand mehr;", "tokens": ["Nun", "\u2013", "bit\u00b7trer", "Hohn", "!", "\u2013", "nun", "st\u00f6rt", "mich", "nie\u00b7mand", "mehr", ";"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "ADJA", "NN", "$.", "$(", "ADV", "VVFIN", "PPER", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Das Leid darf ungest\u00f6rt am Herzen nagen.", "tokens": ["Das", "Leid", "darf", "un\u00b7ge\u00b7st\u00f6rt", "am", "Her\u00b7zen", "na\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Nun hab' ich Ruhe, doch die Ruh' ist leer.", "tokens": ["Nun", "hab'", "ich", "Ru\u00b7he", ",", "doch", "die", "Ruh'", "ist", "leer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "ADV", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Der Faden, den die s\u00fc\u00dfen Kinderh\u00e4nde", "tokens": ["Der", "Fa\u00b7den", ",", "den", "die", "s\u00fc\u00b7\u00dfen", "Kin\u00b7der\u00b7h\u00e4n\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Mir oft zerrissen, flattert um mich her.", "tokens": ["Mir", "oft", "zer\u00b7ris\u00b7sen", ",", "flat\u00b7tert", "um", "mich", "her", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$,", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Fortspinnen k\u00f6nnt' ich ihn getrost ohn' Ende,", "tokens": ["Forts\u00b7pin\u00b7nen", "k\u00f6nnt'", "ich", "ihn", "ge\u00b7trost", "ohn'", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Doch l\u00e4uft er grau in grau, gleich Spinneweben,", "tokens": ["Doch", "l\u00e4uft", "er", "grau", "in", "grau", ",", "gleich", "Spin\u00b7ne\u00b7we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "ADJD", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Ein Tun, daran kein Mensch Gefallen f\u00e4nde.", "tokens": ["Ein", "Tun", ",", "da\u00b7ran", "kein", "Mensch", "Ge\u00b7fal\u00b7len", "f\u00e4n\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "PIAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Versiegt ist nun der Born von jungem Leben,", "tokens": ["Ver\u00b7siegt", "ist", "nun", "der", "Born", "von", "jun\u00b7gem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Drin ich die F\u00e4den eingetaucht, der Quell", "tokens": ["Drin", "ich", "die", "F\u00e4\u00b7den", "ein\u00b7ge\u00b7taucht", ",", "der", "Quell"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PPER", "ART", "NN", "VVPP", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Ew'ger Natur, der ihnen Halt gegeben.", "tokens": ["Ew'\u00b7ger", "Na\u00b7tur", ",", "der", "ih\u00b7nen", "Halt", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.31": {"text": "Kein Spiel erg\u00f6tzt mich, seit mein Spielgesell", "tokens": ["Kein", "Spiel", "er\u00b7g\u00f6tzt", "mich", ",", "seit", "mein", "Spiel\u00b7ge\u00b7sell"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Mir untreu ward, und die den Ernst mir weihte,", "tokens": ["Mir", "un\u00b7treu", "ward", ",", "und", "die", "den", "Ernst", "mir", "weih\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VAFIN", "$,", "KON", "ART", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Die Freude fehlt, dies Lachen silberhell,", "tokens": ["Die", "Freu\u00b7de", "fehlt", ",", "dies", "La\u00b7chen", "sil\u00b7ber\u00b7hell", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PDS", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Das mir in der Gedanken Widerstreite", "tokens": ["Das", "mir", "in", "der", "Ge\u00b7dan\u00b7ken", "Wi\u00b7der\u00b7strei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Aufblitzte, wie ein Licht in Finsternissen,", "tokens": ["Auf\u00b7blitz\u00b7te", ",", "wie", "ein", "Licht", "in", "Fins\u00b7ter\u00b7nis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Das den verworrnen Geist zum Ziele leite.", "tokens": ["Das", "den", "ver\u00b7worr\u00b7nen", "Geist", "zum", "Zie\u00b7le", "lei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Wertlos ward mir das Bilden, schal das Wissen.", "tokens": ["Wert\u00b7los", "ward", "mir", "das", "Bil\u00b7den", ",", "schal", "das", "Wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ART", "NN", "$,", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Die B\u00fccher stehn wie tot in ihren Reihn,", "tokens": ["Die", "B\u00fc\u00b7cher", "stehn", "wie", "tot", "in", "ih\u00b7ren", "Reihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KOKOM", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Und was ich sonst bedurft, nun kann ich's missen \u2013", "tokens": ["Und", "was", "ich", "sonst", "be\u00b7durft", ",", "nun", "kann", "ich's", "mis\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVFIN", "$,", "ADV", "VMFIN", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "In all mein Leben grinst der Tod hinein.", "tokens": ["In", "all", "mein", "Le\u00b7ben", "grinst", "der", "Tod", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}