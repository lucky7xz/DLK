{"textgrid.poem.67420": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Ursprung des Ideals", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In sch\u00f6nster Anmuth gl\u00e4nzten Mond und Sonne", "tokens": ["In", "sch\u00f6ns\u00b7ter", "An\u00b7muth", "gl\u00e4nz\u00b7ten", "Mond", "und", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Sitze der Natur im Paradiese,", "tokens": ["Dem", "Sit\u00b7ze", "der", "Na\u00b7tur", "im", "Pa\u00b7ra\u00b7die\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als sie Dein Antlitz schuf, o C\u00f6lirosa,", "tokens": ["Als", "sie", "Dein", "Ant\u00b7litz", "schuf", ",", "o", "C\u00f6\u00b7li\u00b7ro\u00b7sa", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,", "FM", "FM", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und es mit Reiz und allen Z\u00fcgen schm\u00fcckte.", "tokens": ["Und", "es", "mit", "Reiz", "und", "al\u00b7len", "Z\u00fc\u00b7gen", "schm\u00fcck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Lust war heiter, wolkenlos der Himmel,", "tokens": ["Die", "Lust", "war", "hei\u00b7ter", ",", "wol\u00b7ken\u00b7los", "der", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Heiterkeit mit allen G\u00f6ttern schwebte", "tokens": ["Und", "Hei\u00b7ter\u00b7keit", "mit", "al\u00b7len", "G\u00f6t\u00b7tern", "schweb\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu Dir hernieder. Also w\u00f6lbte sich", "tokens": ["Zu", "Dir", "her\u00b7nie\u00b7der", ".", "Al\u00b7so", "w\u00f6lb\u00b7te", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "PTKVZ", "$.", "ADV", "VVFIN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Stirn, das Auge; Pallas r\u00fchrte sie,", "tokens": ["Die", "Stirn", ",", "das", "Au\u00b7ge", ";", "Pal\u00b7las", "r\u00fchr\u00b7te", "sie", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$.", "NE", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Wange Kypris, und die Lippen Pitho,", "tokens": ["Die", "Wan\u00b7ge", "Kyp\u00b7ris", ",", "und", "die", "Lip\u00b7pen", "Pi\u00b7tho", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "KON", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Das Augenbran Aglaia, und Eros,", "tokens": ["Das", "Au\u00b7gen\u00b7bran", "Ag\u00b7lai\u00b7a", ",", "und", "E\u00b7ros", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "KON", "NE", "$,"], "meter": "-+--++--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Er wiegte sich auf Deinen Augenlidern.", "tokens": ["Er", "wieg\u00b7te", "sich", "auf", "Dei\u00b7nen", "Au\u00b7gen\u00b7li\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und Zeus und Juno sahen freundlich an", "tokens": ["Und", "Zeus", "und", "Ju\u00b7no", "sa\u00b7hen", "freund\u00b7lich", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "KON", "NN", "VVFIN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das ungesehne Werk voll s\u00fc\u00dfer Einfalt", "tokens": ["Das", "un\u00b7ge\u00b7seh\u00b7ne", "Werk", "voll", "s\u00fc\u00b7\u00dfer", "Ein\u00b7falt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und nannten's ", "tokens": ["Und", "nann\u00b7ten's"], "token_info": ["word", "word"], "pos": ["KON", "NE"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Den Menschen es zum sch\u00f6nen ", "tokens": ["Den", "Men\u00b7schen", "es", "zum", "sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "APPRART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "La\u00df andre K\u00fcnstler, Dichter sch\u00f6ne Glieder,", "tokens": ["La\u00df", "and\u00b7re", "K\u00fcnst\u00b7ler", ",", "Dich\u00b7ter", "sch\u00f6\u00b7ne", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "$,", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Eine dies, der Andre jenes preisen,", "tokens": ["Der", "Ei\u00b7ne", "dies", ",", "der", "And\u00b7re", "je\u00b7nes", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "PDS", "$,", "PRELS", "PIS", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In Dir vereinte sich des Sch\u00f6nen Sch\u00f6nstes.", "tokens": ["In", "Dir", "ver\u00b7ein\u00b7te", "sich", "des", "Sch\u00f6\u00b7nen", "Sch\u00f6ns\u00b7tes", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PRF", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}