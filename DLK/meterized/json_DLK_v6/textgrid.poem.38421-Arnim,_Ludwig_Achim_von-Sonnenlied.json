{"textgrid.poem.38421": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnenlied", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sonne, Sonne, scheine,", "tokens": ["Son\u00b7ne", ",", "Son\u00b7ne", ",", "schei\u00b7ne", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Fahr \u00fcber Rheine,", "tokens": ["Fahr", "\u00fc\u00b7ber", "Rhei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Fahr \u00fcbers Glockenhaus,", "tokens": ["Fahr", "\u00fc\u00b7bers", "Glo\u00b7cken\u00b7haus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Gucken drey sch\u00f6ne Puppen raus,", "tokens": ["Gu\u00b7cken", "drey", "sch\u00f6\u00b7ne", "Pup\u00b7pen", "raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Eine die spinnt Seiden,", "tokens": ["Ei\u00b7ne", "die", "spinnt", "Sei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "VVFIN", "NE", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Die andre wickelt Weiden,", "tokens": ["Die", "and\u00b7re", "wi\u00b7ckelt", "Wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die andre geht ans Br\u00fcnnchen,", "tokens": ["Die", "and\u00b7re", "geht", "ans", "Br\u00fcnn\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Findt ein goldig Kindchen;", "tokens": ["Findt", "ein", "gol\u00b7dig", "Kind\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Wer solls heben?", "tokens": ["Wer", "solls", "he\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "VVINF", "$."], "meter": "-+--", "measure": "dactylic.init"}, "line.10": {"text": "Die T\u00f6chter aus dem L\u00f6wen.", "tokens": ["Die", "T\u00f6ch\u00b7ter", "aus", "dem", "L\u00f6\u00b7wen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Wer soll die Windeln w\u00e4schen?", "tokens": ["Wer", "soll", "die", "Win\u00b7deln", "w\u00e4\u00b7schen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Die alte Schneppert\u00e4schen.", "tokens": ["Die", "al\u00b7te", "Schnep\u00b7per\u00b7t\u00e4s\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}