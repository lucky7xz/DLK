{"textgrid.poem.55865": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Abend", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Abend wechselt langsam die Gew\u00e4nder,", "tokens": ["Der", "A\u00b7bend", "wech\u00b7selt", "lang\u00b7sam", "die", "Ge\u00b7w\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die ihm ein Rand von alten B\u00e4umen h\u00e4lt;", "tokens": ["die", "ihm", "ein", "Rand", "von", "al\u00b7ten", "B\u00e4u\u00b7men", "h\u00e4lt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "du schaust: und von dir scheiden sich die L\u00e4nder,", "tokens": ["du", "schaust", ":", "und", "von", "dir", "schei\u00b7den", "sich", "die", "L\u00e4n\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "KON", "APPR", "PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "ein himmelfahrendes und eins, das f\u00e4llt;", "tokens": ["ein", "him\u00b7mel\u00b7fah\u00b7ren\u00b7des", "und", "eins", ",", "das", "f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "PIS", "$,", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "und lassen dich, zu keinem ganz geh\u00f6rend,", "tokens": ["und", "las\u00b7sen", "dich", ",", "zu", "kei\u00b7nem", "ganz", "ge\u00b7h\u00f6\u00b7rend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "APPR", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "nicht ganz so dunkel wie das Haus, das schweigt,", "tokens": ["nicht", "ganz", "so", "dun\u00b7kel", "wie", "das", "Haus", ",", "das", "schweigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "ADJD", "KOKOM", "ART", "NN", "$,", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "nicht ganz so sicher Ewiges beschw\u00f6rend", "tokens": ["nicht", "ganz", "so", "si\u00b7cher", "E\u00b7wi\u00b7ges", "be\u00b7schw\u00f6\u00b7rend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ADV", "ADJD", "ADJA", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "wie das, was Stern wird jede Nacht und steigt \u2013", "tokens": ["wie", "das", ",", "was", "Stern", "wird", "je\u00b7de", "Nacht", "und", "steigt", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "$,", "PWS", "NN", "VAFIN", "PIAT", "NN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "und lassen dir (uns\u00e4glich zu entwirrn)", "tokens": ["und", "las\u00b7sen", "dir", "(", "un\u00b7s\u00e4g\u00b7lich", "zu", "ent\u00b7wirrn", ")"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$(", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "dein Leben bang und riesenhaft und reifend,", "tokens": ["dein", "Le\u00b7ben", "bang", "und", "rie\u00b7sen\u00b7haft", "und", "rei\u00b7fend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "so da\u00df es, bald begrenzt und bald begreifend,", "tokens": ["so", "da\u00df", "es", ",", "bald", "be\u00b7grenzt", "und", "bald", "be\u00b7grei\u00b7fend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "$,", "ADV", "VVPP", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "abwechselnd Stein in dir wird und Gestirn.", "tokens": ["ab\u00b7wech\u00b7selnd", "Stein", "in", "dir", "wird", "und", "Ge\u00b7stirn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "PPER", "VAFIN", "KON", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}