{"textgrid.poem.57659": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Scharf pfeift der Wind ...", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Scharf pfeift der Wind am Holzrand her,", "tokens": ["Scharf", "pfeift", "der", "Wind", "am", "Holz\u00b7rand", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wir gehen fest umschlungen,", "tokens": ["Wir", "ge\u00b7hen", "fest", "um\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Leer ist der Wald, der letzte Ton", "tokens": ["Leer", "ist", "der", "Wald", ",", "der", "letz\u00b7te", "Ton"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Abendglocke verklungen.", "tokens": ["Der", "A\u00b7bend\u00b7glo\u00b7cke", "ver\u00b7klun\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Zu schwarzen Massen ballen sich", "tokens": ["Zu", "schwar\u00b7zen", "Mas\u00b7sen", "bal\u00b7len", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Eichen kahle Zweige,", "tokens": ["Der", "Ei\u00b7chen", "kah\u00b7le", "Zwei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Waldkauz ruft so dumpf und hohl,", "tokens": ["Der", "Wald\u00b7kauz", "ruft", "so", "dumpf", "und", "hohl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Tag geht auf die Neige.", "tokens": ["Der", "Tag", "geht", "auf", "die", "Nei\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wir lachen in die Finsternis", "tokens": ["Wir", "la\u00b7chen", "in", "die", "Fins\u00b7ter\u00b7nis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und gehen auf schwarzen Wegen", "tokens": ["Und", "ge\u00b7hen", "auf", "schwar\u00b7zen", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mit frohen Augen leichten Schritts", "tokens": ["Mit", "fro\u00b7hen", "Au\u00b7gen", "leich\u00b7ten", "Schritts"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Morgenrot entgegen.", "tokens": ["Dem", "Mor\u00b7gen\u00b7rot", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}