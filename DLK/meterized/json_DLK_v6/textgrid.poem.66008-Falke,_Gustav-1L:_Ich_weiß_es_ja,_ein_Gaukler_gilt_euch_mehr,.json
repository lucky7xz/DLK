{"textgrid.poem.66008": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich wei\u00df es ja, ein Gaukler gilt euch mehr,", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df es ja, ein Gaukler gilt euch mehr,", "tokens": ["Ich", "wei\u00df", "es", "ja", ",", "ein", "Gauk\u00b7ler", "gilt", "euch", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und zehnmal mehr ein reicher Kaffeemakler,", "tokens": ["Und", "zehn\u00b7mal", "mehr", "ein", "rei\u00b7cher", "Kaf\u00b7fee\u00b7mak\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich laufe nur so mit im gro\u00dfen Heer,", "tokens": ["Ich", "lau\u00b7fe", "nur", "so", "mit", "im", "gro\u00b7\u00dfen", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mich \u00fcberschreit ein windiger Spektakler.", "tokens": ["Mich", "\u00fc\u00b7bersc\u00b7hreit", "ein", "win\u00b7di\u00b7ger", "Spek\u00b7tak\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Lorbeerkranz mit breitem Atlasband", "tokens": ["Ein", "Lor\u00b7beer\u00b7kranz", "mit", "brei\u00b7tem", "At\u00b7las\u00b7band"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Mimen, Clowns und B\u00f6rsenjubilaren.", "tokens": ["Den", "Mi\u00b7men", ",", "Clowns", "und", "B\u00f6r\u00b7sen\u00b7ju\u00b7bi\u00b7la\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Dichter steht dabei mit leerer Hand,", "tokens": ["Der", "Dich\u00b7ter", "steht", "da\u00b7bei", "mit", "lee\u00b7rer", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bis elend in die Grube er gefahren.", "tokens": ["Bis", "e\u00b7lend", "in", "die", "Gru\u00b7be", "er", "ge\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "APPR", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Pocht nicht auf S\u00e4ulen, die ihr Toten setzt.", "tokens": ["Pocht", "nicht", "auf", "S\u00e4u\u00b7len", ",", "die", "ihr", "To\u00b7ten", "setzt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was soll die Farce noch, ihr eitlen Thoren?", "tokens": ["Was", "soll", "die", "Far\u00b7ce", "noch", ",", "ihr", "eit\u00b7len", "Tho\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer euch im Leben immer kam zuletzt,", "tokens": ["Wer", "euch", "im", "Le\u00b7ben", "im\u00b7mer", "kam", "zu\u00b7letzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den lasst nun auch im Grabe ungeschoren.", "tokens": ["Den", "lasst", "nun", "auch", "im", "Gra\u00b7be", "un\u00b7ge\u00b7scho\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADV", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}