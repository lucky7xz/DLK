{"dta.poem.17897": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "140.  Wer nicht hasst hat nicht verlassen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Du st\u00e4kst im falschen Wahn; kanstu die Welt nicht", "tokens": ["Du", "st\u00e4kst", "im", "fal\u00b7schen", "Wahn", ";", "kans\u00b7tu", "die", "Welt", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$.", "VMFIN", "ART", "NN", "PTKNEG"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "hassen/", "tokens": ["has\u00b7sen", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "F\u00fcrwahr du hast nicht sie/ sie hat nur dich verlassen.", "tokens": ["F\u00fcr\u00b7wahr", "du", "hast", "nicht", "sie", "/", "sie", "hat", "nur", "dich", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VAFIN", "PTKNEG", "PPER", "$(", "PPER", "VAFIN", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}