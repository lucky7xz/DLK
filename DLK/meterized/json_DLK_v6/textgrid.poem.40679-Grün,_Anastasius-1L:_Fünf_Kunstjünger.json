{"textgrid.poem.40679": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: F\u00fcnf Kunstj\u00fcnger", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcnf Kunstj\u00fcnger", "tokens": ["F\u00fcnf", "Kunst\u00b7j\u00fcn\u00b7ger"], "token_info": ["word", "word"], "pos": ["CARD", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Sind deine Finger,", "tokens": ["Sind", "dei\u00b7ne", "Fin\u00b7ger", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Gar feine, gescheidte", "tokens": ["Gar", "fei\u00b7ne", ",", "ge\u00b7scheid\u00b7te"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADV", "ADJA", "$,", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Manierliche Leute,", "tokens": ["Ma\u00b7nier\u00b7li\u00b7che", "Leu\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Gelehrig und biegsam,", "tokens": ["Ge\u00b7leh\u00b7rig", "und", "bieg\u00b7sam", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Gef\u00e4llig und schmiegsam,", "tokens": ["Ge\u00b7f\u00e4l\u00b7lig", "und", "schmieg\u00b7sam", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Der Arbeit zu dienen", "tokens": ["Der", "Ar\u00b7beit", "zu", "die\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Ein sch\u00f6ner Verein,", "tokens": ["Ein", "sch\u00f6\u00b7ner", "Ver\u00b7ein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Ob jeder von ihnen", "tokens": ["Ob", "je\u00b7der", "von", "ih\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "PPER"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Auch f\u00fcr sich allein.", "tokens": ["Auch", "f\u00fcr", "sich", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Doch wenn sie sich rotten", "tokens": ["Doch", "wenn", "sie", "sich", "rot\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PRF", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Und ballen zum Kn\u00e4uel,", "tokens": ["Und", "bal\u00b7len", "zum", "Kn\u00e4u\u00b7el", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Der Sitte zu spotten,", "tokens": ["Der", "Sit\u00b7te", "zu", "spot\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Der Satzung und Regel;", "tokens": ["Der", "Sat\u00b7zung", "und", "Re\u00b7gel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Dann wird draus, \u2013 o Gr\u00e4uel,", "tokens": ["Dann", "wird", "draus", ",", "\u2013", "o", "Gr\u00e4u\u00b7el", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PAV", "$,", "$(", "FM", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Vor dem dir graust!", "tokens": ["Vor", "dem", "dir", "graust", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Ein grober Flegel:", "tokens": ["Ein", "gro\u00b7ber", "Fle\u00b7gel", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Die Faust!", "tokens": ["Die", "Faust", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}}}}}