{"textgrid.poem.24176": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sie machen die Luft dir dumpf und schwer,", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie machen die Luft dir dumpf und schwer,", "tokens": ["Sie", "ma\u00b7chen", "die", "Luft", "dir", "dumpf", "und", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die kreischenden Zwerge?", "tokens": ["Die", "krei\u00b7schen\u00b7den", "Zwer\u00b7ge", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Lach ihnen Abschied! Fahr \u00fcber das Meer!", "tokens": ["Lach", "ih\u00b7nen", "Ab\u00b7schied", "!", "Fahr", "\u00fc\u00b7ber", "das", "Meer", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$.", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Steig \u00fcber die Berge!", "tokens": ["Steig", "\u00fc\u00b7ber", "die", "Ber\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Doch, ehe du gehst, nimm einen am Ohr", "tokens": ["Doch", ",", "e\u00b7he", "du", "gehst", ",", "nimm", "ei\u00b7nen", "am", "Ohr"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "VVFIN", "$,", "VVIMP", "ART", "APPRART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und sch\u00fcttel ihn leise.", "tokens": ["Und", "sch\u00fct\u00b7tel", "ihn", "lei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Verloren ist, wer den Humor verlor.", "tokens": ["Ver\u00b7lo\u00b7ren", "ist", ",", "wer", "den", "Hu\u00b7mor", "ver\u00b7lor", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "PWS", "ART", "NN", "VVFIN", "$."], "meter": "-+-++-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Gl\u00fcck auf die Reise!", "tokens": ["Gl\u00fcck", "auf", "die", "Rei\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}