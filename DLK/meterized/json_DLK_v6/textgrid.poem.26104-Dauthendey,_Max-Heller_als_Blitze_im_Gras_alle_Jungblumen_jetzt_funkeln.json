{"textgrid.poem.26104": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Heller als Blitze im Gras alle Jungblumen jetzt funkeln", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Regen um Regen fiel hin, und alle Bl\u00fcten erschienen,", "tokens": ["Re\u00b7gen", "um", "Re\u00b7gen", "fiel", "hin", ",", "und", "al\u00b7le", "Bl\u00fc\u00b7ten", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "PTKVZ", "$,", "KON", "PIAT", "NN", "VVINF", "$,"], "meter": "+--+----+-+--+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Mairegen umarmt auflebende Blumen an allen Wegen,", "tokens": ["Mai\u00b7re\u00b7gen", "um\u00b7armt", "auf\u00b7le\u00b7ben\u00b7de", "Blu\u00b7men", "an", "al\u00b7len", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJA", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "+---+-+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Und sie alle behalten im Regen die festlichen Mienen.", "tokens": ["Und", "sie", "al\u00b7le", "be\u00b7hal\u00b7ten", "im", "Re\u00b7gen", "die", "fest\u00b7li\u00b7chen", "Mie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PIS", "VVINF", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}}, "stanza.2": {"line.1": {"text": "Keine Wolke kann mehr die bl\u00fchende Wiese verdunkeln,", "tokens": ["Kei\u00b7ne", "Wol\u00b7ke", "kann", "mehr", "die", "bl\u00fc\u00b7hen\u00b7de", "Wie\u00b7se", "ver\u00b7dun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Ziehen auch Wolken heran, springend wie rauchige Riesen,", "tokens": ["Zie\u00b7hen", "auch", "Wol\u00b7ken", "he\u00b7ran", ",", "sprin\u00b7gend", "wie", "rau\u00b7chi\u00b7ge", "Rie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "PTKVZ", "$,", "ADJD", "KOKOM", "ADJA", "NN", "$,"], "meter": "+--+---+--+--+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Heller als Blitze im Gras alle Jungblumen jetzt funkeln.", "tokens": ["Hel\u00b7ler", "als", "Blit\u00b7ze", "im", "Gras", "al\u00b7le", "Jung\u00b7blu\u00b7men", "jetzt", "fun\u00b7keln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "NN", "APPRART", "NN", "PIAT", "NN", "ADV", "VVINF", "$."], "meter": "+--+--+---+--+-", "measure": "dactylic.tri.plus"}}, "stanza.3": {"line.1": {"text": "Und wird sternlos die Nacht, wild vom Gewitter verhangen,", "tokens": ["Und", "wird", "stern\u00b7los", "die", "Nacht", ",", "wild", "vom", "Ge\u00b7wit\u00b7ter", "ver\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "ART", "NN", "$,", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "--+--+---+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Stark sind Baumd\u00fcfte, wie Sehnsucht s\u00fc\u00df bald und bitter,", "tokens": ["Stark", "sind", "Baum\u00b7d\u00fcf\u00b7te", ",", "wie", "Sehn\u00b7sucht", "s\u00fc\u00df", "bald", "und", "bit\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$,", "PWAV", "NN", "VVFIN", "ADV", "KON", "ADJD", "$,"], "meter": "+-++--+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "St\u00e4rker als Donner ersch\u00fctternd \u00fcber das Nachtgras gegangen.", "tokens": ["St\u00e4r\u00b7ker", "als", "Don\u00b7ner", "er\u00b7sch\u00fct\u00b7ternd", "\u00fc\u00b7ber", "das", "Nacht\u00b7gras", "ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NN", "VVPP", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}}}}}