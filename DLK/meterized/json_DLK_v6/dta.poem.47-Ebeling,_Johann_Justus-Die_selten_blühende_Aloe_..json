{"dta.poem.47": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n  selten bl\u00fchende Aloe .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das pr\u00e4chtige Gew\u00e4chs, die seltne\nAloe,", "tokens": ["Das", "pr\u00e4ch\u00b7ti\u00b7ge", "Ge\u00b7w\u00e4chs", ",", "die", "selt\u00b7ne", "A\u00b7loe", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schlingt sich bewunderns-werth,", "tokens": ["Schlingt", "sich", "be\u00b7wun\u00b7derns\u00b7werth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Als keine Pflanze thut, und ihre rare Bl\u00fcte,", "tokens": ["Als", "kei\u00b7ne", "Pflan\u00b7ze", "thut", ",", "und", "ih\u00b7re", "ra\u00b7re", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVFIN", "$,", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zeigt manches Sinnenbild, dem forschenden Ge-", "tokens": ["Zeigt", "man\u00b7ches", "Sin\u00b7nen\u00b7bild", ",", "dem", "for\u00b7schen\u00b7den", "Ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sie w\u00e4chset eigentlich in einem fremden Land", "tokens": ["Sie", "w\u00e4ch\u00b7set", "ei\u00b7gent\u00b7lich", "in", "ei\u00b7nem", "frem\u00b7den", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Jm heissen Africa, allwo der Sonnenbrand", "tokens": ["Jm", "heis\u00b7sen", "A\u00b7fri\u00b7ca", ",", "all\u00b7wo", "der", "Son\u00b7nen\u00b7brand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NE", "$,", "PWAV", "ART", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Viel feuriger als hie in unsrer Gegend gl\u00fchet,", "tokens": ["Viel", "feu\u00b7ri\u00b7ger", "als", "hie", "in", "uns\u00b7rer", "Ge\u00b7gend", "gl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der eine Aloe zu seiner H\u00f6he ziehet.", "tokens": ["Der", "ei\u00b7ne", "A\u00b7loe", "zu", "sei\u00b7ner", "H\u00f6\u00b7he", "zie\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Man findet sie auch da, in jener neuen Welt,", "tokens": ["Man", "fin\u00b7det", "sie", "auch", "da", ",", "in", "je\u00b7ner", "neu\u00b7en", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "ADV", "$,", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die man vors reichste Theil des Erden-Kreises", "tokens": ["Die", "man", "vors", "reichs\u00b7te", "Theil", "des", "Er\u00b7den\u00b7Krei\u00b7ses"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Allwo sie nuzbar ist; wo sie zum frohen Leben,", "tokens": ["All\u00b7wo", "sie", "nuz\u00b7bar", "ist", ";", "wo", "sie", "zum", "fro\u00b7hen", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VAFIN", "$.", "PWAV", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Kan Kleidung, Nahrung, Haus, und was man", "tokens": ["Kan", "Klei\u00b7dung", ",", "Nah\u00b7rung", ",", "Haus", ",", "und", "was", "man"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "NN", "$,", "NN", "$,", "NN", "$,", "KON", "PWS", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Der dikke Stamm reicht Holz zu einer H\u00fctte dar.", "tokens": ["Der", "dik\u00b7ke", "Stamm", "reicht", "Holz", "zu", "ei\u00b7ner", "H\u00fct\u00b7te", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die Bl\u00e4tter geben Scheu, die F\u00e4dgen geben gar,", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "ge\u00b7ben", "Scheu", ",", "die", "F\u00e4d\u00b7gen", "ge\u00b7ben", "gar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$,", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wie Woll und Flachs bei uns, die allersch\u00f6nsten", "tokens": ["Wie", "Woll", "und", "Flachs", "bei", "uns", ",", "die", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADJD", "KON", "NN", "APPR", "PPER", "$,", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Worin die Mohren sich, als in ein Kleid verstek-", "tokens": ["Wo\u00b7rin", "die", "Moh\u00b7ren", "sich", ",", "als", "in", "ein", "Kleid", "vers\u00b7tek"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PRF", "$,", "KOUS", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die Stacheln die daran, sind denen Nageln gleich:", "tokens": ["Die", "Sta\u00b7cheln", "die", "da\u00b7ran", ",", "sind", "de\u00b7nen", "Na\u00b7geln", "gleich", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PAV", "$,", "VAFIN", "PDS", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Es ist die Aloe von sch\u00f6nen S\u00e4ften reich", "tokens": ["Es", "ist", "die", "A\u00b7loe", "von", "sch\u00f6\u00b7nen", "S\u00e4f\u00b7ten", "reich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "ADJD"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Daraus entsteht ein Wein; aus dessen S\u00fc\u00dfigkeiten,", "tokens": ["Da\u00b7raus", "ent\u00b7steht", "ein", "Wein", ";", "aus", "des\u00b7sen", "S\u00fc\u00b7\u00dfig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "$.", "APPR", "PRELAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Auch wenn sie klebricht hart, ein Zukker herzulei-", "tokens": ["Auch", "wenn", "sie", "kleb\u00b7richt", "hart", ",", "ein", "Zuk\u00b7ker", "her\u00b7zu\u00b7lei"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "VVFIN", "ADJD", "$,", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der Saft wird durch die Sonn, zum E\u00dfig-Trank", "tokens": ["Der", "Saft", "wird", "durch", "die", "Sonn", ",", "zum", "E\u00b7\u00dfig\u00b7Trank"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Da aus der S\u00fc\u00dfigkeit ein Saur herf\u00fcrgebracht.", "tokens": ["Da", "aus", "der", "S\u00fc\u00b7\u00dfig\u00b7keit", "ein", "Saur", "her\u00b7f\u00fcr\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die Wurzeln dienen da mit ihren langen Str\u00e4n-", "tokens": ["Die", "Wur\u00b7zeln", "die\u00b7nen", "da", "mit", "ih\u00b7ren", "lan\u00b7gen", "Str\u00e4n"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wie Strikke die man braucht, zum Binden, zum", "tokens": ["Wie", "Strik\u00b7ke", "die", "man", "braucht", ",", "zum", "Bin\u00b7den", ",", "zum"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PWAV", "NN", "ART", "PIS", "VVFIN", "$,", "APPRART", "NN", "$,", "APPRART"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Und andern Dingen mehr. Der hart und dikker", "tokens": ["Und", "an\u00b7dern", "Din\u00b7gen", "mehr", ".", "Der", "hart", "und", "dik\u00b7ker"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "ADV", "$.", "ART", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Der in den Bl\u00e4ttern liegt, in ihr Geweb gefa\u00dft,", "tokens": ["Der", "in", "den", "Bl\u00e4t\u00b7tern", "liegt", ",", "in", "ihr", "Ge\u00b7web", "ge\u00b7fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Giebt eine s\u00fcsse Kost, durch ein gekochtes G\u00e4hren,", "tokens": ["Giebt", "ei\u00b7ne", "s\u00fcs\u00b7se", "Kost", ",", "durch", "ein", "ge\u00b7koch\u00b7tes", "G\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Womit die Mohren sich als einer Speise n\u00e4hren.", "tokens": ["Wo\u00b7mit", "die", "Moh\u00b7ren", "sich", "als", "ei\u00b7ner", "Spei\u00b7se", "n\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "KOUS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wie Wundernsw\u00fcrdig ist wenn man erwegt, be-", "tokens": ["Wie", "Wun\u00b7derns\u00b7w\u00fcr\u00b7dig", "ist", "wenn", "man", "er\u00b7wegt", ",", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "KOUS", "PIS", "VVFIN", "$,", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Da\u00df ", "tokens": ["Da\u00df"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.31": {"text": "Da\u00df jene Wilden gnug, zur Kleidung, sich zu la-", "tokens": ["Da\u00df", "je\u00b7ne", "Wil\u00b7den", "gnug", ",", "zur", "Klei\u00b7dung", ",", "sich", "zu", "la"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PDAT", "NN", "ADV", "$,", "APPRART", "NN", "$,", "PRF", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und mit der Aloe des Lebens Nothdurft haben!", "tokens": ["Und", "mit", "der", "A\u00b7loe", "des", "Le\u00b7bens", "Noth\u00b7durft", "ha\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.33": {"text": "Der sch\u00f6ne Wunderbau, die wollgeorndte Pracht,", "tokens": ["Der", "sch\u00f6\u00b7ne", "Wun\u00b7der\u00b7bau", ",", "die", "woll\u00b7ge\u00b7ornd\u00b7te", "Pracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die diese Pflanze schm\u00fckt, den Augen herrlich", "tokens": ["Die", "die\u00b7se", "Pflan\u00b7ze", "schm\u00fckt", ",", "den", "Au\u00b7gen", "herr\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PDAT", "NN", "VVFIN", "$,", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Hat die Begierd erregt, da\u00df sie in unsrer Erde,", "tokens": ["Hat", "die", "Be\u00b7gierd", "er\u00b7regt", ",", "da\u00df", "sie", "in", "uns\u00b7rer", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Als eine Seltenheit mit Kunst verpflanzet werde.", "tokens": ["Als", "ei\u00b7ne", "Sel\u00b7ten\u00b7heit", "mit", "Kunst", "ver\u00b7pflan\u00b7zet", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Es ist der Kunst gegl\u00fckt. Man stellet ihre Zier,", "tokens": ["Es", "ist", "der", "Kunst", "ge\u00b7gl\u00fckt", ".", "Man", "stel\u00b7let", "ih\u00b7re", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$.", "PIS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "In F\u00fcrsten G\u00e4rten auf; allwo die Neubegier,", "tokens": ["In", "F\u00fcrs\u00b7ten", "G\u00e4r\u00b7ten", "auf", ";", "all\u00b7wo", "die", "Neu\u00b7be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "PTKVZ", "$.", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Nach ihrer Bl\u00fcthe hoft, die sie in langen Jahren,", "tokens": ["Nach", "ih\u00b7rer", "Bl\u00fc\u00b7the", "hoft", ",", "die", "sie", "in", "lan\u00b7gen", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "PRELS", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wenn sie auch wol gepflegt, mit kurzer Lust erfah-", "tokens": ["Wenn", "sie", "auch", "wol", "ge\u00b7pflegt", ",", "mit", "kur\u00b7zer", "Lust", "er\u00b7fah"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVPP", "$,", "APPR", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Des Stammes Untertheil ist sch\u00f6n und Bl\u00e4tter-", "tokens": ["Des", "Stam\u00b7mes", "Un\u00b7ter\u00b7theil", "ist", "sch\u00f6n", "und", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "KON", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Die Bl\u00e4tter sind sehr dik und lang als wie ein Zweig", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "sind", "sehr", "dik", "und", "lang", "als", "wie", "ein", "Zweig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "KOKOM", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Und h\u00e4ngen um den Stam, bis an die Erde nieder,", "tokens": ["Und", "h\u00e4n\u00b7gen", "um", "den", "Stam", ",", "bis", "an", "die", "Er\u00b7de", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,", "KOUS", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Der Stam geht in die H\u00f6h woran nur hin und", "tokens": ["Der", "Stam", "geht", "in", "die", "H\u00f6h", "wo\u00b7ran", "nur", "hin", "und"], "to