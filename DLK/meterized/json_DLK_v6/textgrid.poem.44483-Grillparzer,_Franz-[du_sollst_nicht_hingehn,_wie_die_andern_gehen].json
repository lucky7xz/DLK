{"textgrid.poem.44483": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[du sollst nicht hingehn, wie die andern gehen]", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du sollst nicht hingehn, wie die andern gehen,", "tokens": ["Du", "sollst", "nicht", "hin\u00b7gehn", ",", "wie", "die", "an\u00b7dern", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "PWAV", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Wort bezeichne dauernd den Verlust,", "tokens": ["Ein", "Wort", "be\u00b7zeich\u00b7ne", "dau\u00b7ernd", "den", "Ver\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wenn wir Gut und Schlecht vergessen sehen,", "tokens": ["Und", "wenn", "wir", "Gut", "und", "Schlecht", "ver\u00b7ges\u00b7sen", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "KON", "NN", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So sei dein Wert zum mindesten gewu\u00dft.", "tokens": ["So", "sei", "dein", "Wert", "zum", "min\u00b7des\u00b7ten", "ge\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPRART", "ADJA", "VVPP", "$."], "meter": "-+-+-+---+", "measure": "zehnsilber"}}, "stanza.2": {"line.1": {"text": "Er war nicht eins, dein Wert mit deiner Stelle \u2013", "tokens": ["Er", "war", "nicht", "eins", ",", "dein", "Wert", "mit", "dei\u00b7ner", "Stel\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "PIS", "$,", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So wie der Wipfel, gleich den Zweigen, Holz,", "tokens": ["So", "wie", "der", "Wip\u00b7fel", ",", "gleich", "den", "Zwei\u00b7gen", ",", "Holz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "$,", "ADV", "ART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und nur weil ferner von des Lebens Schwelle,", "tokens": ["Und", "nur", "weil", "fer\u00b7ner", "von", "des", "Le\u00b7bens", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um so viel h\u00f6her, um so h\u00f6hrer Stolz.", "tokens": ["Um", "so", "viel", "h\u00f6\u00b7her", ",", "um", "so", "h\u00f6h\u00b7rer", "Stolz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADV", "ADJD", "$,", "KOUI", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "...", "tokens": ["..."], "token_info": ["punct"], "pos": ["$("]}}}}}