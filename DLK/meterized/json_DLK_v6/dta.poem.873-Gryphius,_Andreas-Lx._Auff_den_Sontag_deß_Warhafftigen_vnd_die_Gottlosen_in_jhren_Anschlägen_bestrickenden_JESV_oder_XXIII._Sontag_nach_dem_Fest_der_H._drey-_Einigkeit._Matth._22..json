{"dta.poem.873": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Lx.  Auff den Sontag de\u00df Warhafftigen/ vnd  \n die Gottlosen in jhren Anschl\u00e4gen bestrickenden  \n JESV/ oder  XXIII.  Sontag nach dem Fest der H. drey-  \n Einigkeit.  Matth.  22.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin der L\u00fcgen-Kind/ ein Sohn der Vn-warheiten/", "tokens": ["Ich", "bin", "der", "L\u00fc\u00b7gen\u00b7Kind", "/", "ein", "Sohn", "der", "Vn\u00b7wa\u00b7rhei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "De\u00df Zung vnd eitler Mund nur L\u00fcgen-Wort au\u00dfschiest.", "tokens": ["De\u00df", "Zung", "vnd", "eit\u00b7ler", "Mund", "nur", "L\u00fc\u00b7gen\u00b7Wort", "au\u00df\u00b7schiest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJA", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein Mund/ mein JESV/ ist v\u00f5 warheit gantzdurchs\u00fcst/", "tokens": ["Dein", "Mund", "/", "mein", "JeSV", "/", "ist", "v\u00f5", "war\u00b7heit", "gantz\u00b7durch\u00b7s\u00fcst", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "NE", "$(", "VAFIN", "ADJD", "VAFIN", "ADV", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Du kanst den Himmel-weg den Menschen zu bereiten/", "tokens": ["Du", "kanst", "den", "Him\u00b7mel\u00b7weg", "den", "Men\u00b7schen", "zu", "be\u00b7rei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du achtest keinen nicht/ du zeigst/ zu allen zeiten/", "tokens": ["Du", "ach\u00b7test", "kei\u00b7nen", "nicht", "/", "du", "zeigst", "/", "zu", "al\u00b7len", "zei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "PTKNEG", "$(", "PPER", "VVFIN", "$(", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie man recht leben soll. Wol! wer der Lehr geniest/", "tokens": ["Wie", "man", "recht", "le\u00b7ben", "soll", ".", "Wol", "!", "wer", "der", "Lehr", "ge\u00b7niest", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJD", "VVINF", "VMFIN", "$.", "ADV", "$.", "PWS", "ART", "NN", "VVPP", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ja; Seelig ist der Mensch/ der deine Zung begr\u00fcst/", "tokens": ["Ja", ";", "See\u00b7lig", "ist", "der", "Mensch", "/", "der", "dei\u00b7ne", "Zung", "be\u00b7gr\u00fcst", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADJD", "VAFIN", "ART", "NN", "$(", "ART", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der steiget sicher an den Sitz der Ewigkeiten.", "tokens": ["Der", "stei\u00b7get", "si\u00b7cher", "an", "den", "Sitz", "der", "E\u00b7wig\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "La\u00df gleich die Schlangen Art/ das Belials-Geschlecht/", "tokens": ["La\u00df", "gleich", "die", "Schlan\u00b7gen", "Art", "/", "das", "Be\u00b7li\u00b7als\u00b7Ge\u00b7schlecht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "NN", "$(", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dahin bem\u016fhet seyn/ da\u00df/ wieder allem Recht/", "tokens": ["Da\u00b7hin", "bem\u016fhet", "seyn", "/", "da\u00df", "/", "wie\u00b7der", "al\u00b7lem", "Recht", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "VAINF", "$(", "KOUS", "$(", "ADV", "PIS", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vnd wahrer Billigkeit/ sie diesen Jesum fangen/", "tokens": ["Vnd", "wah\u00b7rer", "Bil\u00b7lig\u00b7keit", "/", "sie", "die\u00b7sen", "Je\u00b7sum", "fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$(", "PPER", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So ist es doch vmbsonst. O nein! die\u00df nimmer gl\u00fcckt/", "tokens": ["So", "ist", "es", "doch", "vmbsonst", ".", "O", "nein", "!", "die\u00df", "nim\u00b7mer", "gl\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "$.", "NE", "PTKANT", "$.", "PDS", "ADV", "VVFIN", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In jhrer Falschheit Netz h\u00e4lt er sie schon bestrickt.", "tokens": ["In", "jhrer", "Falschheit", "Netz", "h\u00e4lt", "er", "sie", "schon", "be\u00b7strickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dr\u00fcm wil ich nur an dir/ HErr JESV/ bleiben hangen.", "tokens": ["Dr\u00fcm", "wil", "ich", "nur", "an", "dir", "/", "Herr", "JeSV", "/", "blei\u00b7ben", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "PPER", "$(", "NN", "NE", "$(", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}