{"dta.poem.2948": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "147.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dem Kinde magst du schwer den Mond am Himmel zeigen,", "tokens": ["Dem", "Kin\u00b7de", "magst", "du", "schwer", "den", "Mond", "am", "Him\u00b7mel", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADJD", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es ist als k\u00f6nne nicht sein Blick die H\u00f6h' ersteigen.", "tokens": ["Es", "ist", "als", "k\u00f6n\u00b7ne", "nicht", "sein", "Blick", "die", "H\u00f6h'", "er\u00b7stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "VMFIN", "PTKNEG", "PPOSAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Den Vater selber, der herab vom Fenster schaut,", "tokens": ["Den", "Va\u00b7ter", "sel\u00b7ber", ",", "der", "her\u00b7ab", "vom", "Fens\u00b7ter", "schaut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Entdeckt es nicht, wiewol es kennt der Stimme Laut.", "tokens": ["Ent\u00b7deckt", "es", "nicht", ",", "wie\u00b7wol", "es", "kennt", "der", "Stim\u00b7me", "Laut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "VVFIN", "ART", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Vom Anfang ist der Blick der Erde zugekehrt,", "tokens": ["Vom", "An\u00b7fang", "ist", "der", "Blick", "der", "Er\u00b7de", "zu\u00b7ge\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wird nur nach und nach emporzuschaun gelehrt.", "tokens": ["Und", "wird", "nur", "nach", "und", "nach", "em\u00b7por\u00b7zu\u00b7schaun", "ge\u00b7lehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPR", "KON", "APPR", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}