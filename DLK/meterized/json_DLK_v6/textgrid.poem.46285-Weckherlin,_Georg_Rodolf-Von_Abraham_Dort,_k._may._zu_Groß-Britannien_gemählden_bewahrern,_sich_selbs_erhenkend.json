{"textgrid.poem.46285": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Von Abraham Dort, k. may. zu Gro\u00df-Britannien gem\u00e4hlden bewahrern, sich selbs erhenkend", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nachdem der arme bub von Dort", "tokens": ["Nach\u00b7dem", "der", "ar\u00b7me", "bub", "von", "Dort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "sein amt recht zu thun sich bekr\u00e4nket,", "tokens": ["sein", "amt", "recht", "zu", "thun", "sich", "be\u00b7kr\u00e4n\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "PTKZU", "VVINF", "PRF", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "hat er sich hie an disem ort", "tokens": ["hat", "er", "sich", "hie", "an", "di\u00b7sem", "ort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "APPR", "PDAT", "NN"], "meter": "+--+---+", "measure": "dactylic.di.plus"}, "line.4": {"text": "nach den gem\u00e4hlden selbs gehenket:", "tokens": ["nach", "den", "ge\u00b7m\u00e4hl\u00b7den", "selbs", "ge\u00b7hen\u00b7ket", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hat also er dort gleiches gl\u00fcck", "tokens": ["Hat", "al\u00b7so", "er", "dort", "glei\u00b7ches", "gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPER", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "als die gem\u00e4hlde hie empfangen,", "tokens": ["als", "die", "ge\u00b7m\u00e4hl\u00b7de", "hie", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "dan dort sah man manch sch\u00f6nes st\u00fcck,", "tokens": ["dan", "dort", "sah", "man", "manch", "sch\u00f6\u00b7nes", "st\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PIS", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "hie aber Dort selbs sch\u00e4ndlich hangen.", "tokens": ["hie", "a\u00b7ber", "Dort", "selbs", "sch\u00e4nd\u00b7lich", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}