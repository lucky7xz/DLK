{"dta.poem.14021": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "M\u00e4dchen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Gar oft schon f\u00fchlt' ich's tief, des M\u00e4dchens Seele", "tokens": ["Gar", "oft", "schon", "f\u00fchlt'", "ich's", "tief", ",", "des", "M\u00e4d\u00b7chens", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "PIS", "ADJD", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wird nicht sich selbst, dem Liebsten nur geboren.", "tokens": ["Wird", "nicht", "sich", "selbst", ",", "dem", "Liebs\u00b7ten", "nur", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PRF", "ADV", "$,", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da irrt sie nun versto\u00dfen und verloren,", "tokens": ["Da", "irrt", "sie", "nun", "ver\u00b7sto\u00b7\u00dfen", "und", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVINF", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schickt heimlich Blicke sch\u00f6n als Boten aus,", "tokens": ["Schickt", "heim\u00b7lich", "Bli\u00b7cke", "sch\u00f6n", "als", "Bo\u00b7ten", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "ADJD", "KOKOM", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df sie auf Erden suchen ihr ihr Haus.", "tokens": ["Da\u00df", "sie", "auf", "Er\u00b7den", "su\u00b7chen", "ihr", "ihr", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sie schlummert in der Schw\u00fcle, leicht bedeckt,", "tokens": ["Sie", "schlum\u00b7mert", "in", "der", "Schw\u00fc\u00b7le", ",", "leicht", "be\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "L\u00e4chelt im Schlafe, athmet warm und leise,", "tokens": ["L\u00e4\u00b7chelt", "im", "Schla\u00b7fe", ",", "ath\u00b7met", "warm", "und", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.8": {"text": "Doch die Gedanken sind fern auf der Reise,", "tokens": ["Doch", "die", "Ge\u00b7dan\u00b7ken", "sind", "fern", "auf", "der", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und auf den Wangen flattert tr\u00e4um'risch Feuer,", "tokens": ["Und", "auf", "den", "Wan\u00b7gen", "flat\u00b7tert", "tr\u00e4um'\u00b7risch", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Hebt buhlend oft der Wind den zarten Schleier.", "tokens": ["Hebt", "buh\u00b7lend", "oft", "der", "Wind", "den", "zar\u00b7ten", "Schlei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Der Mann, der da zum erstenmal sie weckt,", "tokens": ["Der", "Mann", ",", "der", "da", "zum", "ers\u00b7ten\u00b7mal", "sie", "weckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPRART", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Zuerst hinunterlangt in diese Stille,", "tokens": ["Zu\u00b7erst", "hin\u00b7un\u00b7ter\u00b7langt", "in", "die\u00b7se", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Dem f\u00e4llt sie um den Hals vor Freude bang", "tokens": ["Dem", "f\u00e4llt", "sie", "um", "den", "Hals", "vor", "Freu\u00b7de", "bang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Und l\u00e4\u00dft ihn nicht mehr all' ihr Lebelang.", "tokens": ["Und", "l\u00e4\u00dft", "ihn", "nicht", "mehr", "all'", "ihr", "Le\u00b7be\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADV", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}