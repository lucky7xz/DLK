{"textgrid.poem.59077": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "296. Wie du ihm, so ist er dir", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott scheint dir fern und unbekannt;", "tokens": ["Gott", "scheint", "dir", "fern", "und", "un\u00b7be\u00b7kannt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Wunder, du stehst abgewandt.", "tokens": ["Kein", "Wun\u00b7der", ",", "du", "stehst", "ab\u00b7ge\u00b7wandt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PPER", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sei ganz mit Gott, hab ihn alleine,", "tokens": ["Sei", "ganz", "mit", "Gott", ",", "hab", "ihn", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "$,", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dann ist er ganz mit dir und deine!", "tokens": ["Dann", "ist", "er", "ganz", "mit", "dir", "und", "dei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPER", "KON", "PPOSAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}