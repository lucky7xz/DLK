{"textgrid.poem.43634": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Mein Blumentisch", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf meinem Blumentische", "tokens": ["Auf", "mei\u00b7nem", "Blu\u00b7men\u00b7ti\u00b7sche"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Will's immer Fr\u00fchling sein:", "tokens": ["Will's", "im\u00b7mer", "Fr\u00fch\u00b7ling", "sein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Glas die goldnen Fische,", "tokens": ["Im", "Glas", "die", "gold\u00b7nen", "Fi\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Umgl\u00e4nzt vom Sonnenschein,", "tokens": ["Um\u00b7gl\u00e4nzt", "vom", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und rings in voller Frische", "tokens": ["Und", "rings", "in", "vol\u00b7ler", "Fri\u00b7sche"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So manches Bl\u00fcmelein \u2013", "tokens": ["So", "man\u00b7ches", "Bl\u00fc\u00b7me\u00b7lein", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Auf meinem Blumentische!", "tokens": ["Auf", "mei\u00b7nem", "Blu\u00b7men\u00b7ti\u00b7sche", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Des Morgens in der Fr\u00fche", "tokens": ["Des", "Mor\u00b7gens", "in", "der", "Fr\u00fc\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bin ich vergn\u00fcgt und frisch,", "tokens": ["Bin", "ich", "ver\u00b7gn\u00fcgt", "und", "frisch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn keine Sorg' und M\u00fche", "tokens": ["Denn", "kei\u00b7ne", "Sor\u00b7g'", "und", "M\u00fc\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Macht mir mein Blumentisch.", "tokens": ["Macht", "mir", "mein", "Blu\u00b7men\u00b7tisch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.5": {"text": "Das Bl\u00fcmlein sagt: ich bl\u00fche!", "tokens": ["Das", "Bl\u00fcm\u00b7lein", "sagt", ":", "ich", "bl\u00fc\u00b7he", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich schwimme! sagt der Fisch \u2013", "tokens": ["Ich", "schwim\u00b7me", "!", "sagt", "der", "Fisch", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Des Morgens in der Fr\u00fche.", "tokens": ["Des", "Mor\u00b7gens", "in", "der", "Fr\u00fc\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Zu meinem Blumentische", "tokens": ["Zu", "mei\u00b7nem", "Blu\u00b7men\u00b7ti\u00b7sche"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kommt her! ich lad' euch ein:", "tokens": ["Kommt", "her", "!", "ich", "lad'", "euch", "ein", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Seht meine goldnen Fische,", "tokens": ["Seht", "mei\u00b7ne", "gold\u00b7nen", "Fi\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Umgl\u00e4nzt vom Sonnenschein,", "tokens": ["Um\u00b7gl\u00e4nzt", "vom", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und wie in voller Frische", "tokens": ["Und", "wie", "in", "vol\u00b7ler", "Fri\u00b7sche"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bl\u00fch'n meine Bl\u00fcmelein", "tokens": ["Bl\u00fch'n", "mei\u00b7ne", "Bl\u00fc\u00b7me\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Auf meinem Blumentische!", "tokens": ["Auf", "mei\u00b7nem", "Blu\u00b7men\u00b7ti\u00b7sche", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}