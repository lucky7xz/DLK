{"textgrid.poem.43635": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Mein liebes Goldfischchen!", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, Goldfischchen, lieb Goldfischchen,", "tokens": ["Ach", ",", "Gold\u00b7fi\u00b7schchen", ",", "lieb", "Gold\u00b7fi\u00b7schchen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Spieltest gestern goldig roth", "tokens": ["Spiel\u00b7test", "ge\u00b7stern", "gol\u00b7dig", "roth"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADJD", "ADJD"], "meter": "+--++-+", "measure": "dactylic.init"}, "line.3": {"text": "Wie ein Sonnenstrahl im Wasser,", "tokens": ["Wie", "ein", "Son\u00b7nen\u00b7strahl", "im", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, und heute bist du todt.", "tokens": ["Ach", ",", "und", "heu\u00b7te", "bist", "du", "todt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KON", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Jeden Morgen war mein Erstes:", "tokens": ["Je\u00b7den", "Mor\u00b7gen", "war", "mein", "Ers\u00b7tes", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was doch wohl mein Goldfisch macht?", "tokens": ["Was", "doch", "wohl", "mein", "Gold\u00b7fisch", "macht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Abends, eh' ich ging zu Bette,", "tokens": ["A\u00b7bends", ",", "eh'", "ich", "ging", "zu", "Bet\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sagt' ich dir auch: gute Nacht!", "tokens": ["Sagt'", "ich", "dir", "auch", ":", "gu\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "$.", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn ich nach dem Fenster blicke,", "tokens": ["Wenn", "ich", "nach", "dem", "Fens\u00b7ter", "bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist anjetzt die Stelle leer;", "tokens": ["Ist", "an\u00b7jetzt", "die", "Stel\u00b7le", "leer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Weinen mu\u00df ich, denn ich sehe,", "tokens": ["Wei\u00b7nen", "mu\u00df", "ich", ",", "denn", "ich", "se\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$,", "KON", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, ich sehe dich nicht mehr.", "tokens": ["Ach", ",", "ich", "se\u00b7he", "dich", "nicht", "mehr", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach, Goldfischchen, lieb Goldfischchen,", "tokens": ["Ach", ",", "Gold\u00b7fi\u00b7schchen", ",", "lieb", "Gold\u00b7fi\u00b7schchen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nimmermehr verge\u00df ich dich!", "tokens": ["Nim\u00b7mer\u00b7mehr", "ver\u00b7ge\u00df", "ich", "dich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo ich nur ein Fischchen sehe,", "tokens": ["Wo", "ich", "nur", "ein", "Fischchen", "se\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Denk' ich gleich zuerst an dich.", "tokens": ["Denk'", "ich", "gleich", "zu\u00b7erst", "an", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}