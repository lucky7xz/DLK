{"textgrid.poem.55046": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Ein gleiches", "genre": "verse", "period": "N.A.", "pub_year": 1780, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber allen Gipfeln", "tokens": ["\u00dc\u00b7ber", "al\u00b7len", "Gip\u00b7feln"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ist Ruh,", "tokens": ["Ist", "Ruh", ","], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "In allen Wipfeln", "tokens": ["In", "al\u00b7len", "Wip\u00b7feln"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Sp\u00fcrest du", "tokens": ["Sp\u00fc\u00b7rest", "du"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Kaum einen Hauch;", "tokens": ["Kaum", "ei\u00b7nen", "Hauch", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Die V\u00f6gelein schweigen im Walde.", "tokens": ["Die", "V\u00f6\u00b7ge\u00b7lein", "schwei\u00b7gen", "im", "Wal\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Warte nur, balde", "tokens": ["War\u00b7te", "nur", ",", "bal\u00b7de"], "token_info": ["word", "word", "punct", "word"], "pos": ["NN", "ADV", "$,", "ADV"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Ruhest du auch.", "tokens": ["Ru\u00b7hest", "du", "auch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}