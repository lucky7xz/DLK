{"dta.poem.3712": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Zu Gaste gebeten .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Geh mit mir in die Heidelbeeren,               ", "tokens": ["Geh", "mit", "mir", "in", "die", "Hei\u00b7del\u00b7bee\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Heidelbeeren sind noch nit blo, (blau)", "tokens": ["Hei\u00b7del\u00b7bee\u00b7ren", "sind", "noch", "nit", "blo", ",", "(", "blau", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "PTKNEG", "ADV", "$,", "$(", "ADJD", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Geh mit mir ins Haberstoh,", "tokens": ["Geh", "mit", "mir", "ins", "Ha\u00b7ber\u00b7stoh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Haberstoh ist noch nit zeitig,", "tokens": ["Ha\u00b7ber\u00b7stoh", "ist", "noch", "nit", "zei\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Geh mit mir ins Besenreisig,", "tokens": ["Geh", "mit", "mir", "ins", "Be\u00b7sen\u00b7rei\u00b7sig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Besenreisig ist noch nit auf,", "tokens": ["Be\u00b7sen\u00b7rei\u00b7sig", "ist", "noch", "nit", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "PTKNEG", "PTKVZ", "$,"], "meter": "+-+-++-+", "measure": "unknown.measure.penta"}, "line.7": {"text": "Geh mit mir die Trepp hinauf,", "tokens": ["Geh", "mit", "mir", "die", "Trepp", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Trepplein ist verbrochen,", "tokens": ["Trep\u00b7plein", "ist", "ver\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Sind wir nauf gekrochen,", "tokens": ["Sind", "wir", "nauf", "ge\u00b7kro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Sind wir in dem K\u00e4mmerlein,", "tokens": ["Sind", "wir", "in", "dem", "K\u00e4m\u00b7mer\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Schenk ein Sch\u00f6pplein Wein ein.", "tokens": ["Schenk", "ein", "Sch\u00f6p\u00b7plein", "Wein", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}