{"textgrid.poem.35638": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von dem t\u00e4uschenden Wahn erwacht,", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von dem t\u00e4uschenden Wahn erwacht,", "tokens": ["Von", "dem", "t\u00e4u\u00b7schen\u00b7den", "Wahn", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Geb ich itzo des Nords Fittichen zu verwehn", "tokens": ["Geb", "ich", "it\u00b7zo", "des", "Nords", "Fit\u00b7ti\u00b7chen", "zu", "ver\u00b7wehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "NN", "PTKZU", "VVINF"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Die verschm\u00e4hete Liebe hin.", "tokens": ["Die", "ver\u00b7schm\u00e4\u00b7he\u00b7te", "Lie\u00b7be", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Die M\u00e4nade, vom Hauch Evans getrieben, rast", "tokens": ["Die", "M\u00e4\u00b7na\u00b7de", ",", "vom", "Hauch", "E\u00b7vans", "ge\u00b7trie\u00b7ben", ",", "rast"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "NE", "VVPP", "$,", "VVFIN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Nicht mit brausendem Thyrsus so,", "tokens": ["Nicht", "mit", "brau\u00b7sen\u00b7dem", "Thyr\u00b7sus", "so", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "ADV", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "So die Pythia nicht, wenn das Orakel ihr", "tokens": ["So", "die", "Py\u00b7thia", "nicht", ",", "wenn", "das", "O\u00b7ra\u00b7kel", "ihr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "PTKNEG", "$,", "KOUS", "ART", "NN", "PPOSAT"], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Im arbeitenden Busen kocht,", "tokens": ["Im", "ar\u00b7bei\u00b7ten\u00b7den", "Bu\u00b7sen", "kocht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Als der raset, auf den Amor den K\u00f6cher leert.", "tokens": ["Als", "der", "ra\u00b7set", ",", "auf", "den", "A\u00b7mor", "den", "K\u00f6\u00b7cher", "leert", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "VVFIN", "$,", "APPR", "ART", "NE", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Hat die Liebe der Grajer Wut", "tokens": ["Hat", "die", "Lie\u00b7be", "der", "Gra\u00b7jer", "Wut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "Nicht zehn Sommer entflammt? Hat sie nicht Ilions", "tokens": ["Nicht", "zehn", "Som\u00b7mer", "ent\u00b7flammt", "?", "Hat", "sie", "nicht", "I\u00b7li\u00b7ons"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "CARD", "NN", "VVPP", "$.", "VAFIN", "PPER", "PTKNEG", "NE"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Goldne T\u00fcrme gest\u00fcrzet, und", "tokens": ["Gold\u00b7ne", "T\u00fcr\u00b7me", "ge\u00b7st\u00fcr\u00b7zet", ",", "und"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADJA", "NN", "VVPP", "$,", "KON"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.12": {"text": "Des dardanischen Volks Heldengeschlecht erw\u00fcrgt? \u2013", "tokens": ["Des", "dar\u00b7da\u00b7ni\u00b7schen", "Volks", "Hel\u00b7den\u00b7ge\u00b7schlecht", "er\u00b7w\u00fcrgt", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ha! wie hat mich die Glut verzehrt!", "tokens": ["Ha", "!", "wie", "hat", "mich", "die", "Glut", "ver\u00b7zehrt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.14": {"text": "Ha! wie hat sie das Mark meines Gebeins verbrannt,", "tokens": ["Ha", "!", "wie", "hat", "sie", "das", "Mark", "mei\u00b7nes", "Ge\u00b7beins", "ver\u00b7brannt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VAFIN", "PPER", "ART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.15": {"text": "Und die schwindende Haut geschrumpft!", "tokens": ["Und", "die", "schwin\u00b7den\u00b7de", "Haut", "ge\u00b7schrumpft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.16": {"text": "Wie ward \u00f6fters mein Stolz tief in den Staub gebeugt,", "tokens": ["Wie", "ward", "\u00f6f\u00b7ters", "mein", "Stolz", "tief", "in", "den", "Staub", "ge\u00b7beugt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "PPOSAT", "NN", "ADJD", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.17": {"text": "Wenn ich meiner Gebieterin", "tokens": ["Wenn", "ich", "mei\u00b7ner", "Ge\u00b7bie\u00b7te\u00b7rin"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.18": {"text": "Mir verschlossene Th\u00fcr n\u00e4chtlich belagerte,", "tokens": ["Mir", "ver\u00b7schlos\u00b7se\u00b7ne", "Th\u00fcr", "n\u00e4cht\u00b7lich", "be\u00b7la\u00b7ger\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+--++--++-", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "Und nicht Regen, nicht schneidenden", "tokens": ["Und", "nicht", "Re\u00b7gen", ",", "nicht", "schnei\u00b7den\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PTKNEG", "NN", "$,", "PTKNEG", "VVINF"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.20": {"text": "Hagel achtete, noch st\u00fcrmender Winde Wut!", "tokens": ["Ha\u00b7gel", "ach\u00b7te\u00b7te", ",", "noch", "st\u00fcr\u00b7men\u00b7der", "Win\u00b7de", "Wut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "ADV", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.21": {"text": "Jetzt entz\u00fcckt mich kein M\u00e4dchen mehr,", "tokens": ["Jetzt", "ent\u00b7z\u00fcckt", "mich", "kein", "M\u00e4d\u00b7chen", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "ADV", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.22": {"text": "Jetzt kein t\u00e4ndelnder Kampf, noch ein ersiegter Ku\u00df;", "tokens": ["Jetzt", "kein", "t\u00e4n\u00b7deln\u00b7der", "Kampf", ",", "noch", "ein", "er\u00b7sieg\u00b7ter", "Ku\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$,", "ADV", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.23": {"text": "Jetzt entz\u00fcckt die catonische,", "tokens": ["Jetzt", "ent\u00b7z\u00fcckt", "die", "ca\u00b7to\u00b7ni\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Hohe Weisheit mich nur, welche der Liebe lacht,", "tokens": ["Ho\u00b7he", "Weis\u00b7heit", "mich", "nur", ",", "wel\u00b7che", "der", "Lie\u00b7be", "lacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "ADV", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.25": {"text": "Und, in Schl\u00fcssen verloren, mit", "tokens": ["Und", ",", "in", "Schl\u00fcs\u00b7sen", "ver\u00b7lo\u00b7ren", ",", "mit"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "$,", "APPR", "NN", "VVPP", "$,", "APPR"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.26": {"text": "Stolzgerunzelter Stirn lockende Nymphen sieht.", "tokens": ["Stolz\u00b7ge\u00b7run\u00b7zel\u00b7ter", "Stirn", "lo\u00b7cken\u00b7de", "Nym\u00b7phen", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.27": {"text": "Doch, ach Phyllis! was schielt mein Blick", "tokens": ["Doch", ",", "ach", "Phyl\u00b7lis", "!", "was", "schielt", "mein", "Blick"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "XY", "NE", "$.", "PWS", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Nach der blendenden Brust, wenn sie den Schleier hebt?", "tokens": ["Nach", "der", "blen\u00b7den\u00b7den", "Brust", ",", "wenn", "sie", "den", "Schlei\u00b7er", "hebt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.29": {"text": "Was verfolgt dich mein Fu\u00df, wenn du", "tokens": ["Was", "ver\u00b7folgt", "dich", "mein", "Fu\u00df", ",", "wenn", "du"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "KOUS", "PPER"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.30": {"text": "Unter Blumen entschl\u00e4fst, oder im finstern Hain,", "tokens": ["Un\u00b7ter", "Blu\u00b7men", "ent\u00b7schl\u00e4fst", ",", "o\u00b7der", "im", "fins\u00b7tern", "Hain", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "KON", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.31": {"text": "Oder, wenn du err\u00f6tend in", "tokens": ["O\u00b7der", ",", "wenn", "du", "er\u00b7r\u00f6\u00b7tend", "in"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "PPER", "VVPP", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "K\u00fchle Fluten den Reiz marmorner Glieder tauchst?", "tokens": ["K\u00fch\u00b7le", "Flu\u00b7ten", "den", "Reiz", "mar\u00b7mor\u00b7ner", "Glie\u00b7der", "tauchst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}}}}}