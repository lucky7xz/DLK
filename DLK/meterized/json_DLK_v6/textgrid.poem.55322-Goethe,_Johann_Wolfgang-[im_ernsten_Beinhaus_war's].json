{"textgrid.poem.55322": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "[im ernsten Beinhaus war's]", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im ernsten Beinhaus war's, wo ich beschaute,", "tokens": ["Im", "erns\u00b7ten", "Be\u00b7in\u00b7haus", "wa\u00b7r's", ",", "wo", "ich", "be\u00b7schau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie Sch\u00e4del Sch\u00e4deln angeordnet pa\u00dften;", "tokens": ["Wie", "Sch\u00e4\u00b7del", "Sch\u00e4\u00b7deln", "an\u00b7ge\u00b7ord\u00b7net", "pa\u00df\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die alte Zeit gedacht ich, die ergraute.", "tokens": ["Die", "al\u00b7te", "Zeit", "ge\u00b7dacht", "ich", ",", "die", "er\u00b7grau\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "PPER", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie stehn in Reih geklemmt, die sonst sich ha\u00dften,", "tokens": ["Sie", "stehn", "in", "Reih", "ge\u00b7klemmt", ",", "die", "sonst", "sich", "ha\u00df\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "VVPP", "$,", "PRELS", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und derbe Knochen, die sich t\u00f6dlich schlugen,", "tokens": ["Und", "der\u00b7be", "Kno\u00b7chen", ",", "die", "sich", "t\u00f6d\u00b7lich", "schlu\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "PRELS", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sie liegen kreuzweis, zahm allhier zu rasten.", "tokens": ["Sie", "lie\u00b7gen", "kreuz\u00b7weis", ",", "zahm", "all\u00b7hier", "zu", "ras\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "VVFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Entrenkte Schulterbl\u00e4tter! was sie trugen,", "tokens": ["E\u00b7ntrenk\u00b7te", "Schul\u00b7ter\u00b7bl\u00e4t\u00b7ter", "!", "was", "sie", "tru\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Fragt niemand mehr, und zierlich t\u00e4t'ge Glieder,", "tokens": ["Fragt", "nie\u00b7mand", "mehr", ",", "und", "zier\u00b7lich", "t\u00e4t'\u00b7ge", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "$,", "KON", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Die Hand, der Fu\u00df, zerstreut aus Lebensfugen.", "tokens": ["Die", "Hand", ",", "der", "Fu\u00df", ",", "zer\u00b7streut", "aus", "Le\u00b7bens\u00b7fu\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Ihr M\u00fcden also lagt vergebens nieder,", "tokens": ["Ihr", "M\u00fc\u00b7den", "al\u00b7so", "lagt", "ver\u00b7ge\u00b7bens", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Nicht Ruh im Grabe lie\u00df man euch, vertrieben", "tokens": ["Nicht", "Ruh", "im", "Gra\u00b7be", "lie\u00df", "man", "euch", ",", "ver\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PTKNEG", "NN", "APPRART", "NN", "VVFIN", "PIS", "PPER", "$,", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Seid ihr herauf zum lichten Tage wieder,", "tokens": ["Seid", "ihr", "her\u00b7auf", "zum", "lich\u00b7ten", "Ta\u00b7ge", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "ADV", "APPRART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und niemand kann die d\u00fcrre Schale lieben,", "tokens": ["Und", "nie\u00b7mand", "kann", "die", "d\u00fcr\u00b7re", "Scha\u00b7le", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Welch herrlich edlen Kern sie auch bewahrte.", "tokens": ["Welch", "herr\u00b7lich", "ed\u00b7len", "Kern", "sie", "auch", "be\u00b7wahr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "ADJA", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Doch mir Adepten war die Schrift geschrieben,", "tokens": ["Doch", "mir", "A\u00b7dep\u00b7ten", "war", "die", "Schrift", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Die heil'gen Sinn nicht jedem offenbarte,", "tokens": ["Die", "heil'\u00b7gen", "Sinn", "nicht", "je\u00b7dem", "of\u00b7fen\u00b7bar\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Als ich inmitten solcher starren Menge", "tokens": ["Als", "ich", "in\u00b7mit\u00b7ten", "sol\u00b7cher", "star\u00b7ren", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Unsch\u00e4tzbar herrlich ein Gebild gewahrte,", "tokens": ["Un\u00b7sch\u00e4tz\u00b7bar", "herr\u00b7lich", "ein", "Ge\u00b7bild", "ge\u00b7wahr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.19": {"text": "Da\u00df in des Raumes Moderk\u00e4lt und Enge", "tokens": ["Da\u00df", "in", "des", "Rau\u00b7mes", "Mo\u00b7der\u00b7k\u00e4lt", "und", "En\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Ich frei und w\u00e4rmef\u00fchlend mich erquickte,", "tokens": ["Ich", "frei", "und", "w\u00e4r\u00b7me\u00b7f\u00fch\u00b7lend", "mich", "er\u00b7quick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Als ob ein Lebensquell dem Tod entspr\u00e4nge.", "tokens": ["Als", "ob", "ein", "Le\u00b7bens\u00b7quell", "dem", "Tod", "ent\u00b7spr\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Wie mich geheimnisvoll die Form entz\u00fcckte!", "tokens": ["Wie", "mich", "ge\u00b7heim\u00b7nis\u00b7voll", "die", "Form", "ent\u00b7z\u00fcck\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Die gottgedachte Spur, die sich erhalten!", "tokens": ["Die", "gott\u00b7ge\u00b7dach\u00b7te", "Spur", ",", "die", "sich", "er\u00b7hal\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Ein Blick, der mich an jenes Meer entr\u00fcckte,", "tokens": ["Ein", "Blick", ",", "der", "mich", "an", "je\u00b7nes", "Meer", "ent\u00b7r\u00fcck\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Das flutend str\u00f6mt gesteigerte Gestalten.", "tokens": ["Das", "flu\u00b7tend", "str\u00f6mt", "ge\u00b7stei\u00b7ger\u00b7te", "Ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Geheim Gef\u00e4\u00df! Orakelspr\u00fcche spendend,", "tokens": ["Ge\u00b7heim", "Ge\u00b7f\u00e4\u00df", "!", "O\u00b7ra\u00b7kel\u00b7spr\u00fc\u00b7che", "spen\u00b7dend", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Wie bin ich wert, dich in der Hand zu halten,", "tokens": ["Wie", "bin", "ich", "wert", ",", "dich", "in", "der", "Hand", "zu", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "$,", "PRF", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Dich h\u00f6chsten Schatz aus Moder fromm entwendend", "tokens": ["Dich", "h\u00f6chs\u00b7ten", "Schatz", "aus", "Mo\u00b7der", "fromm", "ent\u00b7wen\u00b7dend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "APPR", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Und in die freie Luft, zu freiem Sinnen,", "tokens": ["Und", "in", "die", "frei\u00b7e", "Luft", ",", "zu", "frei\u00b7em", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Zum Sonnenlicht and\u00e4chtig hin mich wendend.", "tokens": ["Zum", "Son\u00b7nen\u00b7licht", "an\u00b7d\u00e4ch\u00b7tig", "hin", "mich", "wen\u00b7dend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was kann der Mensch im Leben mehr gewinnen,", "tokens": ["Was", "kann", "der", "Mensch", "im", "Le\u00b7ben", "mehr", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "APPRART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als da\u00df sich Gott-Natur ihm offenbare?", "tokens": ["Als", "da\u00df", "sich", "Got\u00b7tNa\u00b7tur", "ihm", "of\u00b7fen\u00b7ba\u00b7re", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PRF", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie sie das Feste l\u00e4\u00dft zu Geist verrinnen,", "tokens": ["Wie", "sie", "das", "Fes\u00b7te", "l\u00e4\u00dft", "zu", "Geist", "ver\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie sie das Geisterzeugte fest bewahre.", "tokens": ["Wie", "sie", "das", "Geis\u00b7ter\u00b7zeug\u00b7te", "fest", "be\u00b7wah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}