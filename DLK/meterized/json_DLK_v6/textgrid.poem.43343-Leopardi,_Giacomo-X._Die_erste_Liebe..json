{"textgrid.poem.43343": {"metadata": {"author": {"name": "Leopardi, Giacomo", "birth": "N.A.", "death": "N.A."}, "title": "X. Die erste Liebe.", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df den Tag, da ich zum ersten Mal", "tokens": ["Ich", "wei\u00df", "den", "Tag", ",", "da", "ich", "zum", "ers\u00b7ten", "Mal"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Kampf der Liebe stritt und zu mir sprach:", "tokens": ["Den", "Kampf", "der", "Lie\u00b7be", "stritt", "und", "zu", "mir", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "KON", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ist das die Liebe, weh, wie schafft sie Qual!", "tokens": ["Ist", "das", "die", "Lie\u00b7be", ",", "weh", ",", "wie", "schafft", "sie", "Qual", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "$,", "PTKVZ", "$,", "PWAV", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Am Boden haftete der Blick, doch ach,", "tokens": ["Am", "Bo\u00b7den", "haf\u00b7te\u00b7te", "der", "Blick", ",", "doch", "ach", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich sah nur Sie, die mit unschuld'gem Triebe", "tokens": ["Ich", "sah", "nur", "Sie", ",", "die", "mit", "un\u00b7schuld'\u00b7gem", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Zuerst sich Bahn zu diesem Herzen brach.", "tokens": ["Zu\u00b7erst", "sich", "Bahn", "zu", "die\u00b7sem", "Her\u00b7zen", "brach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "NN", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie schlimm mi\u00dfhandelt hast du mich, o Liebe!", "tokens": ["Wie", "schlimm", "mi\u00df\u00b7han\u00b7delt", "hast", "du", "mich", ",", "o", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVPP", "VAFIN", "PPER", "PRF", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Warum nur st\u00fcrzt uns diese s\u00fc\u00dfe Lust", "tokens": ["Wa\u00b7rum", "nur", "st\u00fcrzt", "uns", "die\u00b7se", "s\u00fc\u00b7\u00dfe", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In solcher Schmerzen sehnliches Getriebe!", "tokens": ["In", "sol\u00b7cher", "Schmer\u00b7zen", "sehn\u00b7li\u00b7ches", "Ge\u00b7trie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nicht sanft, nicht heiter ward ich mir bewu\u00dft", "tokens": ["Nicht", "sanft", ",", "nicht", "hei\u00b7ter", "ward", "ich", "mir", "be\u00b7wu\u00dft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "$,", "PTKNEG", "ADJD", "VAFIN", "PPER", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der neuen Macht. Sie kam mit Weh und Klagen", "tokens": ["Der", "neu\u00b7en", "Macht", ".", "Sie", "kam", "mit", "Weh", "und", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und schn\u00fcrte mir mit dunkler Angst die Brust.", "tokens": ["Und", "schn\u00fcr\u00b7te", "mir", "mit", "dunk\u00b7ler", "Angst", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Sprich, z\u00e4rtlich Herz, was machte dich verzagen,", "tokens": ["Sprich", ",", "z\u00e4rt\u00b7lich", "Herz", ",", "was", "mach\u00b7te", "dich", "ver\u00b7za\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ADJD", "NN", "$,", "PWS", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was bebtest du so tief vor dem Gedanken,", "tokens": ["Was", "beb\u00b7test", "du", "so", "tief", "vor", "dem", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der aller Wonnen Preis davongetragen?", "tokens": ["Der", "al\u00b7ler", "Won\u00b7nen", "Preis", "da\u00b7von\u00b7ge\u00b7tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Bei dem Gedanken, der sich ohne Wanken", "tokens": ["Bei", "dem", "Ge\u00b7dan\u00b7ken", ",", "der", "sich", "oh\u00b7ne", "Wan\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PRF", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dir Tags gesellt' und Nachts dir raunte zu", "tokens": ["Dir", "Tags", "ge\u00b7sellt'", "und", "Nachts", "dir", "raun\u00b7te", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVPP", "KON", "ADV", "PPER", "VVFIN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "S\u00fc\u00dfschmeichelnd, wenn in Schlaf die Fluren sanken?", "tokens": ["S\u00fc\u00df\u00b7schmei\u00b7chelnd", ",", "wenn", "in", "Schlaf", "die", "Flu\u00b7ren", "san\u00b7ken", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "APPR", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "In Unruh', Gl\u00fcck und Jammer st\u00fcrmtest du", "tokens": ["In", "Un\u00b7ruh'", ",", "Gl\u00fcck", "und", "Jam\u00b7mer", "st\u00fcrm\u00b7test", "du"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Lautpochend fort und fort an dein Gef\u00e4ngni\u00df", "tokens": ["Laut\u00b7po\u00b7chend", "fort", "und", "fort", "an", "dein", "Ge\u00b7f\u00e4ng\u00b7ni\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "KON", "PTKVZ", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und scheuchtest mir von meinem Pf\u00fchl die Ruh'.", "tokens": ["Und", "scheuch\u00b7test", "mir", "von", "mei\u00b7nem", "Pf\u00fchl", "die", "Ruh'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Und wenn ich, matt von gl\u00fchender Bedr\u00e4ngni\u00df,", "tokens": ["Und", "wenn", "ich", ",", "matt", "von", "gl\u00fc\u00b7hen\u00b7der", "Be\u00b7dr\u00e4ng\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "$,", "KOUI", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Augen schlo\u00df zum Schlummer, o wie bald", "tokens": ["Die", "Au\u00b7gen", "schlo\u00df", "zum", "Schlum\u00b7mer", ",", "o", "wie", "bald"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,", "FM", "KOKOM", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verst\u00f6rt' ihn, wie im Fieber, Traumesb\u00e4ngni\u00df!", "tokens": ["Ver\u00b7st\u00f6rt'", "ihn", ",", "wie", "im", "Fie\u00b7ber", ",", "Trau\u00b7mes\u00b7b\u00e4ng\u00b7ni\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "APPRART", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Wie leibhaft stand die reizende Gestalt", "tokens": ["Wie", "leib\u00b7haft", "stand", "die", "rei\u00b7zen\u00b7de", "Ge\u00b7stalt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.2": {"text": "Im Finstern da, und ob ich auch die Lider", "tokens": ["Im", "Fins\u00b7tern", "da", ",", "und", "ob", "ich", "auch", "die", "Li\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "KON", "KOUS", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zudr\u00fcckte, sie erblickt' ich tausendfalt.", "tokens": ["Zu\u00b7dr\u00fcck\u00b7te", ",", "sie", "er\u00b7blickt'", "ich", "tau\u00b7send\u00b7falt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Wie flo\u00df mit s\u00fc\u00dfem Grau'n durch meine Glieder", "tokens": ["Wie", "flo\u00df", "mit", "s\u00fc\u00b7\u00dfem", "Grau'n", "durch", "mei\u00b7ne", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verworrne Glut, wie wogten ohne Stocken", "tokens": ["Ver\u00b7worr\u00b7ne", "Glut", ",", "wie", "wog\u00b7ten", "oh\u00b7ne", "Sto\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PWAV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gedanken durch den Geist mir auf und nieder.", "tokens": ["Ge\u00b7dan\u00b7ken", "durch", "den", "Geist", "mir", "auf", "und", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PPER", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "So f\u00e4hrt ein Zephyr durch die dichten Locken", "tokens": ["So", "f\u00e4hrt", "ein", "Ze\u00b7phyr", "durch", "die", "dich\u00b7ten", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des alten Waldes, im Vor\u00fcberschweben", "tokens": ["Des", "al\u00b7ten", "Wal\u00b7des", ",", "im", "Vor\u00b7\u00fc\u00b7bersc\u00b7hwe\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihm lange, bange Klagen zu entlocken.", "tokens": ["Ihm", "lan\u00b7ge", ",", "ban\u00b7ge", "Kla\u00b7gen", "zu", "ent\u00b7lo\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADV", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Und da ich schweigend stand, wehrlos ergeben,", "tokens": ["Und", "da", "ich", "schwei\u00b7gend", "stand", ",", "wehr\u00b7los", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVFIN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was sagtest du, o Herz, als sie nun ging,", "tokens": ["Was", "sag\u00b7test", "du", ",", "o", "Herz", ",", "als", "sie", "nun", "ging", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "FM", "NN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Um die in tiefer Noth du solltest beben?", "tokens": ["Um", "die", "in", "tie\u00b7fer", "Noth", "du", "soll\u00b7test", "be\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "APPR", "ADJA", "NN", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Kaum, da\u00df ich v\u00f6llig an zu lodern fing,", "tokens": ["Kaum", ",", "da\u00df", "ich", "v\u00f6l\u00b7lig", "an", "zu", "lo\u00b7dern", "fing", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADJD", "PTKVZ", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So war des L\u00fcftchens linder Hauch entschwunden,", "tokens": ["So", "war", "des", "L\u00fcft\u00b7chens", "lin\u00b7der", "Hauch", "ent\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch das ich K\u00fchlung meiner Glut empfing.", "tokens": ["Durch", "das", "ich", "K\u00fch\u00b7lung", "mei\u00b7ner", "Glut", "emp\u00b7fing", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Wach lag ich noch in fr\u00fchen Morgenstunden,", "tokens": ["Wach", "lag", "ich", "noch", "in", "fr\u00fc\u00b7hen", "Mor\u00b7gen\u00b7stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da stampfend schon an unsres Hauses Thor", "tokens": ["Da", "stamp\u00b7fend", "schon", "an", "uns\u00b7res", "Hau\u00b7ses", "Thor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die R\u00e4uber meines Gl\u00fccks, die Rosse stunden.", "tokens": ["Die", "R\u00e4u\u00b7ber", "mei\u00b7nes", "Gl\u00fccks", ",", "die", "Ros\u00b7se", "stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.15": {"line.1": {"text": "Und ich, verzagt und stumm, ein bl\u00f6der Thor,", "tokens": ["Und", "ich", ",", "ver\u00b7zagt", "und", "stumm", ",", "ein", "bl\u00f6\u00b7der", "Thor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "VVPP", "KON", "ADJD", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hielt zum Balcon hin in den Finsternissen", "tokens": ["Hielt", "zum", "Bal\u00b7con", "hin", "in", "den", "Fins\u00b7ter\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-++-+-+-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Umsonst mein Aug' und mein begierig Ohr,", "tokens": ["Um\u00b7sonst", "mein", "Aug'", "und", "mein", "be\u00b7gie\u00b7rig", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "KON", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.16": {"line.1": {"text": "Ob ich noch einmal, eh' sie w\u00fcrd' entrissen,", "tokens": ["Ob", "ich", "noch", "ein\u00b7mal", ",", "eh'", "sie", "w\u00fcrd'", "ent\u00b7ris\u00b7sen", ","], 