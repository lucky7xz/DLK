,"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "PRELS", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie, die Vergifterin entschwundner Lust,", "tokens": ["Sie", ",", "die", "Ver\u00b7gif\u00b7te\u00b7rin", "ent\u00b7schwund\u00b7ner", "Lust", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.32": {"line.1": {"text": "Trieb ihren Dorn mir rastlos in das Blut", "tokens": ["Trieb", "ih\u00b7ren", "Dorn", "mir", "rast\u00b7los", "in", "das", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Im R\u00fcckgedenken; ob auch noch die Pein", "tokens": ["Im", "R\u00fcck\u00b7ge\u00b7den\u00b7ken", ";", "ob", "auch", "noch", "die", "Pein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$.", "KOUS", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Schuld nicht an mir nagt' in wilder Glut.", "tokens": ["Der", "Schuld", "nicht", "an", "mir", "nagt'", "in", "wil\u00b7der", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "APPR", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.33": {"line.1": {"text": "Euch, edle Seelen, dir, du Sonnenschein,", "tokens": ["Euch", ",", "ed\u00b7le", "See\u00b7len", ",", "dir", ",", "du", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "po