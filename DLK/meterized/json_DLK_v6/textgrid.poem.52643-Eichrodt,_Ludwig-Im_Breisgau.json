{"textgrid.poem.52643": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Im Breisgau", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welt der Berge!", "tokens": ["Welt", "der", "Ber\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Wie du wogst vor freudigen Augen!", "tokens": ["Wie", "du", "wogst", "vor", "freu\u00b7di\u00b7gen", "Au\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mein Gedank ist neu erhellt", "tokens": ["Mein", "Ge\u00b7dank", "ist", "neu", "er\u00b7hellt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Darf er also freigestellt", "tokens": ["Darf", "er", "al\u00b7so", "frei\u00b7ge\u00b7stellt"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In die blauen Gr\u00fcnde tauchen.", "tokens": ["In", "die", "blau\u00b7en", "Gr\u00fcn\u00b7de", "tau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Bergh\u00e4upter unwillk\u00fcrlich erscheinen", "tokens": ["Berg\u00b7h\u00e4up\u00b7ter", "un\u00b7will\u00b7k\u00fcr\u00b7lich", "er\u00b7schei\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJD", "VVINF"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Gleich lebendigen dunkeln Wesen,", "tokens": ["Gleich", "le\u00b7ben\u00b7di\u00b7gen", "dun\u00b7keln", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "ADJA", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ich mags an den finstern Stirnen lesen,", "tokens": ["Ich", "mags", "an", "den", "fins\u00b7tern", "Stir\u00b7nen", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie sie sich zaubergewaltig meinen", "tokens": ["Wie", "sie", "sich", "zau\u00b7ber\u00b7ge\u00b7wal\u00b7tig", "mei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "ADJD", "PPOSAT"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Bergh\u00e4upter riesengro\u00df", "tokens": ["Berg\u00b7h\u00e4up\u00b7ter", "rie\u00b7sen\u00b7gro\u00df"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Schauerlich her\u00fcbergr\u00fc\u00dfen", "tokens": ["Schau\u00b7er\u00b7lich", "her\u00b7\u00fc\u00b7ber\u00b7gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word"], "pos": ["ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Aus der Ferne unendlichem Schoos.", "tokens": ["Aus", "der", "Fer\u00b7ne", "un\u00b7end\u00b7li\u00b7chem", "Schoos", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Ich schaus wie die Zwergesr\u00fccken,", "tokens": ["Ich", "schaus", "wie", "die", "Zwer\u00b7ge\u00b7sr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KOKOM", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Die kleinen H\u00fcgel sich b\u00fccken,", "tokens": ["Die", "klei\u00b7nen", "H\u00fc\u00b7gel", "sich", "b\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Kr\u00fcmmen sich vor ihren F\u00fc\u00dfen.", "tokens": ["Kr\u00fcm\u00b7men", "sich", "vor", "ih\u00b7ren", "F\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ueber den Wald hin spielet das Himmelslicht,", "tokens": ["Ue\u00b7ber", "den", "Wald", "hin", "spie\u00b7let", "das", "Him\u00b7mels\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und die Tannenberge stehen", "tokens": ["Und", "die", "Tan\u00b7nen\u00b7ber\u00b7ge", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "In silbergrau blendendem Glanze,", "tokens": ["In", "sil\u00b7ber\u00b7grau", "blen\u00b7den\u00b7dem", "Glan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Liebliche L\u00fcfte wehen,", "tokens": ["Lieb\u00b7li\u00b7che", "L\u00fcf\u00b7te", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Kosen mit \u00fcppiger Pflanze,", "tokens": ["Ko\u00b7sen", "mit", "\u00fcp\u00b7pi\u00b7ger", "Pflan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Oder sie drehen", "tokens": ["O\u00b7der", "sie", "dre\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Sich hinunter im Wirbeltanze", "tokens": ["Sich", "hin\u00b7un\u00b7ter", "im", "Wir\u00b7bel\u00b7tan\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "ADV", "APPRART", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "In die aufg\u00e4hnenden Schluchten.", "tokens": ["In", "die", "auf\u00b7g\u00e4h\u00b7nen\u00b7den", "Schluch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Dort hausen die Br\u00fcder des Sturms", "tokens": ["Dort", "hau\u00b7sen", "die", "Br\u00fc\u00b7der", "des", "Sturms"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.10": {"text": "All die brausenden Stimmen", "tokens": ["All", "die", "brau\u00b7sen\u00b7den", "Stim\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.11": {"text": "Die von Abend und Morgen und Mitternacht", "tokens": ["Die", "von", "A\u00b7bend", "und", "Mor\u00b7gen", "und", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.12": {"text": "Rasend durchs Luftmeer schwimmen.", "tokens": ["Ra\u00b7send", "durchs", "Luft\u00b7meer", "schwim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.13": {"text": "Und mit sich rei\u00dfen sie", "tokens": ["Und", "mit", "sich", "rei\u00b7\u00dfen", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PRF", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Nebeld\u00fcnste,", "tokens": ["Ne\u00b7bel\u00b7d\u00fcns\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Die langsam den Tiefen entsteigen,", "tokens": ["Die", "lang\u00b7sam", "den", "Tie\u00b7fen", "ent\u00b7stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.16": {"text": "Th\u00fcrmen sie auf zu Wolken,", "tokens": ["Th\u00fcr\u00b7men", "sie", "auf", "zu", "Wol\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "APPR", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.17": {"text": "Da\u00df Donner erkracht", "tokens": ["Da\u00df", "Don\u00b7ner", "er\u00b7kracht"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "NN", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.18": {"text": "Und die starren W\u00e4lder sich beugen \u2013", "tokens": ["Und", "die", "star\u00b7ren", "W\u00e4l\u00b7der", "sich", "beu\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PRF", "VVINF", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.19": {"text": "Das ist der Winde Macht.", "tokens": ["Das", "ist", "der", "Win\u00b7de", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Im d\u00fcsteren Wirrwarr zucket der Schein,", "tokens": ["Im", "d\u00fcs\u00b7te\u00b7ren", "Wirr\u00b7warr", "zu\u00b7cket", "der", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wenn es g\u00e4hret und lange gegohren,", "tokens": ["Wenn", "es", "g\u00e4h\u00b7ret", "und", "lan\u00b7ge", "ge\u00b7goh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "ADV", "VVPP", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Der Blitzstrahl gl\u00fchet die L\u00fcfte rein \u2013", "tokens": ["Der", "Blitz\u00b7strahl", "gl\u00fc\u00b7het", "die", "L\u00fcf\u00b7te", "rein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJD", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da wird die Frische geboren.", "tokens": ["Da", "wird", "die", "Fri\u00b7sche", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Unendliche Bergwelt,", "tokens": ["Un\u00b7end\u00b7li\u00b7che", "Berg\u00b7welt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "In dir woget ein g\u00f6ttliches Schaffen,", "tokens": ["In", "dir", "wo\u00b7get", "ein", "g\u00f6tt\u00b7li\u00b7ches", "Schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Unsichtbar sichtbar", "tokens": ["Un\u00b7sicht\u00b7bar", "sicht\u00b7bar"], "token_info": ["word", "word"], "pos": ["ADJD", "ADJD"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Rauschend Leben", "tokens": ["Rau\u00b7schend", "Le\u00b7ben"], "token_info": ["word", "word"], "pos": ["ADJD", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Weben und Leben.", "tokens": ["We\u00b7ben", "und", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Wie frohaufsch\u00e4umend", "tokens": ["Wie", "fro\u00b7hauf\u00b7sch\u00e4u\u00b7mend"], "token_info": ["word", "word"], "pos": ["PWAV", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "K\u00fchn sich \u00fcbers Gekl\u00fcfte b\u00e4umend", "tokens": ["K\u00fchn", "sich", "\u00fc\u00b7bers", "Ge\u00b7kl\u00fcf\u00b7te", "b\u00e4u\u00b7mend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PRF", "APPRART", "NN", "VVPP"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Das sprudelnde Wasser zur Tiefe f\u00e4llt!", "tokens": ["Das", "spru\u00b7deln\u00b7de", "Was\u00b7ser", "zur", "Tie\u00b7fe", "f\u00e4llt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Durch Busch und Gestein", "tokens": ["Durch", "Busch", "und", "Ge\u00b7stein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Rieselt es in die Th\u00e4ler herein,", "tokens": ["Rie\u00b7selt", "es", "in", "die", "Th\u00e4\u00b7ler", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Wohin sich dr\u00e4nget sein Lauf", "tokens": ["Wo\u00b7hin", "sich", "dr\u00e4n\u00b7get", "sein", "Lauf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.12": {"text": "Springen lebendige Blumen auf.", "tokens": ["Sprin\u00b7gen", "le\u00b7ben\u00b7di\u00b7ge", "Blu\u00b7men", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.13": {"text": "\u2013 Und rings ein lustiges Gr\u00fcn!", "tokens": ["\u2013", "Und", "rings", "ein", "lus\u00b7ti\u00b7ges", "Gr\u00fcn", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.14": {"text": "O reiche Natur, o liebende Mutter \u2013", "tokens": ["O", "rei\u00b7che", "Na\u00b7tur", ",", "o", "lie\u00b7ben\u00b7de", "Mut\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "FM", "ADJA", "NN", "$("], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Da fliehn die erheiterten Wesen,", "tokens": ["Da", "fliehn", "die", "er\u00b7hei\u00b7ter\u00b7ten", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.16": {"text": "Auch deine Menschen hin!", "tokens": ["Auch", "dei\u00b7ne", "Men\u00b7schen", "hin", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}