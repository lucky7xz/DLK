{"dta.poem.20893": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df menschlicher Verstand k\u00f6nt\u2019 alle Sachen f\u00fcgen/", "tokens": ["Da\u00df", "menschli\u00b7cher", "Ver\u00b7stand", "k\u00f6nt'", "al\u00b7le", "Sa\u00b7chen", "f\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VMFIN", "PIAT", "NN", "VVINF", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der alles f\u00fcrgebracht/ ja selber aufgethan/", "tokens": ["Der", "al\u00b7les", "f\u00fcr\u00b7ge\u00b7bracht", "/", "ja", "sel\u00b7ber", "auf\u00b7ge\u00b7than", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$(", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den Abgrund der Vernunfft/ da alle Wunder liegen/", "tokens": ["Den", "Ab\u00b7grund", "der", "Ver\u00b7nunfft", "/", "da", "al\u00b7le", "Wun\u00b7der", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$(", "ADV", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und was das grosse Rund in sich begreiffen kan.", "tokens": ["Und", "was", "das", "gros\u00b7se", "Rund", "in", "sich", "be\u00b7greif\u00b7fen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJA", "NN", "APPR", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So lie\u00df ich mich alsbald durch diese Meinung binden/", "tokens": ["So", "lie\u00df", "ich", "mich", "als\u00b7bald", "durch", "die\u00b7se", "Mei\u00b7nung", "bin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und setzte sie getrost den andern allen f\u00fcr;", "tokens": ["Und", "setz\u00b7te", "sie", "ge\u00b7trost", "den", "an\u00b7dern", "al\u00b7len", "f\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ART", "ADJA", "PIAT", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich sprach: l\u00e4st solche Macht sich in dem Geiste finden/", "tokens": ["Ich", "sprach", ":", "l\u00e4st", "sol\u00b7che", "Macht", "sich", "in", "dem", "Geis\u00b7te", "fin\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "VVFIN", "PIAT", "NN", "PRF", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie solte sie sich nicht am ersten zeigen hier?", "tokens": ["Wie", "sol\u00b7te", "sie", "sich", "nicht", "am", "ers\u00b7ten", "zei\u00b7gen", "hier", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "PTKNEG", "APPRART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wenn wir den rechten Grund der Sachen wollen wissen/", "tokens": ["Wenn", "wir", "den", "rech\u00b7ten", "Grund", "der", "Sa\u00b7chen", "wol\u00b7len", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Warum di\u00df oder das zum sterben ist erkiest/", "tokens": ["Wa\u00b7rum", "di\u00df", "o\u00b7der", "das", "zum", "ster\u00b7ben", "ist", "er\u00b7kiest", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "KON", "ART", "APPRART", "VVINF", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Warum di\u00df fort gemust/ und jenes bleiben m\u00fcssen/", "tokens": ["Wa\u00b7rum", "di\u00df", "fort", "ge\u00b7must", "/", "und", "je\u00b7nes", "blei\u00b7ben", "m\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "PTKVZ", "VVPP", "$(", "KON", "PDS", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Weil unsers Geistes Zweck allein das gute ist/", "tokens": ["Weil", "un\u00b7sers", "Geis\u00b7tes", "Zweck", "al\u00b7lein", "das", "gu\u00b7te", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "ADV", "ART", "ADJA", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So mu\u00df man erstlich sehn wie alle Sachen stehen/", "tokens": ["So", "mu\u00df", "man", "erst\u00b7lich", "sehn", "wie", "al\u00b7le", "Sa\u00b7chen", "ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADJD", "VVINF", "KOKOM", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und wie es besser ist/ da\u00df di\u00df und jenes bleibt/", "tokens": ["Und", "wie", "es", "bes\u00b7ser", "ist", "/", "da\u00df", "di\u00df", "und", "je\u00b7nes", "bleibt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "VAFIN", "$(", "KOUS", "PDS", "KON", "PDS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Da\u00df dieses Leben kriegt/ und jenes mu\u00df vergehen/", "tokens": ["Da\u00df", "die\u00b7ses", "Le\u00b7ben", "kriegt", "/", "und", "je\u00b7nes", "mu\u00df", "ver\u00b7ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "VVFIN", "$(", "KON", "PDS", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und lerne/ wann man so zum Zweck der Sachen treibt.", "tokens": ["Und", "ler\u00b7ne", "/", "wann", "man", "so", "zum", "Zweck", "der", "Sa\u00b7chen", "treibt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "PWAV", "PIS", "ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}