{"textgrid.poem.44000": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Drey gelehrt- und treue Br\u00fcder", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drey gelehrt- und treue Br\u00fcder", "tokens": ["Drey", "ge\u00b7lehr\u00b7t", "und", "treu\u00b7e", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "TRUNC", "KON", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Hielten ein Collegium,", "tokens": ["Hiel\u00b7ten", "ein", "Col\u00b7le\u00b7gi\u00b7um", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Sungen frohe Jugendlieder,", "tokens": ["Sun\u00b7gen", "fro\u00b7he", "Ju\u00b7gend\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gaben Hand und Glas herum", "tokens": ["Ga\u00b7ben", "Hand", "und", "Glas", "he\u00b7rum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "KON", "NN", "APZR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und beklagten ohngefehr,", "tokens": ["Und", "be\u00b7klag\u00b7ten", "ohn\u00b7ge\u00b7fehr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie verg\u00e4nglich alles w\u00e4r.", "tokens": ["Wie", "ver\u00b7g\u00e4ng\u00b7lich", "al\u00b7les", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PIS", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was sonst auf den hohen Schulen", "tokens": ["Was", "sonst", "auf", "den", "ho\u00b7hen", "Schu\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Flei\u00df und M\u00fch und Ernst vers\u00fc\u00dft", "tokens": ["Flei\u00df", "und", "M\u00fch", "und", "Ernst", "ver\u00b7s\u00fc\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NE", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und bey Schwermen, Trunck und Buhlen", "tokens": ["Und", "bey", "Schwer\u00b7men", ",", "Trunck", "und", "Buh\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Freyer Geister Handwerck ist,", "tokens": ["Frey\u00b7er", "Geis\u00b7ter", "Hand\u00b7werck", "ist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00dcberlegten sie mit Gram,", "tokens": ["\u00dc\u00b7ber\u00b7leg\u00b7ten", "sie", "mit", "Gram", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Weil die Zeit nicht wieder kam.", "tokens": ["Weil", "die", "Zeit", "nicht", "wie\u00b7der", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Br\u00fcder, sagte darauf einer,", "tokens": ["Br\u00fc\u00b7der", ",", "sag\u00b7te", "da\u00b7rauf", "ei\u00b7ner", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PAV", "ART", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was verloren ist, sey hin!", "tokens": ["Was", "ver\u00b7lo\u00b7ren", "ist", ",", "sey", "hin", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVPP", "VAFIN", "$,", "VAFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist gleich jezt die Freyheit kleiner,", "tokens": ["Ist", "gleich", "jezt", "die", "Frey\u00b7heit", "klei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bleibt uns dennoch der Gewinn,", "tokens": ["Bleibt", "uns", "den\u00b7noch", "der", "Ge\u00b7winn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Da\u00df man ihr beliebt Confect", "tokens": ["Da\u00df", "man", "ihr", "be\u00b7liebt", "Con\u00b7fect"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch drey Jahre schon geschmeckt.", "tokens": ["Durch", "drey", "Jah\u00b7re", "schon", "ge\u00b7schmeckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Freylich bei\u00dft es in dem Herzen,", "tokens": ["Frey\u00b7lich", "bei\u00dft", "es", "in", "dem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df uns Zwang und P\u00f6bel qu\u00e4lt,", "tokens": ["Da\u00df", "uns", "Zwang", "und", "P\u00f6\u00b7bel", "qu\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df die Thoren mit uns scherzen", "tokens": ["Da\u00df", "die", "Tho\u00b7ren", "mit", "uns", "scher\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das Ungl\u00fcck Pfeile st\u00e4hlt,", "tokens": ["Und", "das", "Un\u00b7gl\u00fcck", "Pfei\u00b7le", "st\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja da\u00df unsre Treu und Flei\u00df", "tokens": ["Ja", "da\u00df", "uns\u00b7re", "Treu", "und", "Flei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "KOUS", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nirgends Brodt zu finden weis.", "tokens": ["Nir\u00b7gends", "Brodt", "zu", "fin\u00b7den", "weis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Aber ach, was will man machen?", "tokens": ["A\u00b7ber", "ach", ",", "was", "will", "man", "ma\u00b7chen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PWS", "VMFIN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das Verh\u00e4ngn\u00fc\u00df hat hier Schuld;", "tokens": ["Das", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "hat", "hier", "Schuld", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Last die Ignoranten lachen,", "tokens": ["Last", "die", "Ig\u00b7no\u00b7ran\u00b7ten", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schmiert die W\u00fcntsche mit Gedult", "tokens": ["Schmiert", "die", "W\u00fcnt\u00b7sche", "mit", "Ge\u00b7dult"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und erwarthet von der Zeit", "tokens": ["Und", "er\u00b7wart\u00b7het", "von", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wohlstand und Zufriedenheit.", "tokens": ["Wohl\u00b7stand", "und", "Zu\u00b7frie\u00b7den\u00b7heit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Unterdes thut der am besten,", "tokens": ["Un\u00b7ter\u00b7des", "thut", "der", "am", "bes\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "PTKA", "ADJD", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Wer zu Troz der tollen Welt", "tokens": ["Wer", "zu", "Troz", "der", "tol\u00b7len", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bey vergn\u00fcgt- und klugen G\u00e4sten", "tokens": ["Bey", "ver\u00b7gn\u00fcg\u00b7t", "und", "klu\u00b7gen", "G\u00e4s\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "TRUNC", "KON", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Lustig mit den Fingern schnellt", "tokens": ["Lus\u00b7tig", "mit", "den", "Fin\u00b7gern", "schnellt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und sich vor der lezten Nacht", "tokens": ["Und", "sich", "vor", "der", "lez\u00b7ten", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Selber gute Tage macht.", "tokens": ["Sel\u00b7ber", "gu\u00b7te", "Ta\u00b7ge", "macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Haben wir doch nichts zu hofen", "tokens": ["Ha\u00b7ben", "wir", "doch", "nichts", "zu", "ho\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "PIS", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als den Tod, das Vatertheil;", "tokens": ["Als", "den", "Tod", ",", "das", "Va\u00b7ter\u00b7theil", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unser Grab steht allzeit ofen", "tokens": ["Un\u00b7ser", "Grab", "steht", "all\u00b7zeit", "o\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und entrei\u00dft uns oft in Eil,", "tokens": ["Und", "ent\u00b7rei\u00dft", "uns", "oft", "in", "Eil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und hier sieht man, lernt's verstehn,", "tokens": ["Und", "hier", "sieht", "man", ",", "lernt's", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "$,", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Keine Stufe r\u00fcckw\u00e4rts gehn.", "tokens": ["Kei\u00b7ne", "Stu\u00b7fe", "r\u00fcck\u00b7w\u00e4rts", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Selten bleibt man stets beysammen,", "tokens": ["Sel\u00b7ten", "bleibt", "man", "stets", "bey\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und wir werden auch zerstreut.", "tokens": ["Und", "wir", "wer\u00b7den", "auch", "zer\u00b7streut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Niemand wird die Lust verdammen,", "tokens": ["Nie\u00b7mand", "wird", "die", "Lust", "ver\u00b7dam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die uns dieser Abend beuth;", "tokens": ["Die", "uns", "die\u00b7ser", "A\u00b7bend", "beuth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PDAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Singt und raucht und denckt daran,", "tokens": ["Singt", "und", "raucht", "und", "denckt", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "KON", "VVFIN", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Was uns Brieg wohl Guts gethan.", "tokens": ["Was", "uns", "Brieg", "wohl", "Guts", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NN", "ADV", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}