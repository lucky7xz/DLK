{"textgrid.poem.44521": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Zur goldnen Hochzeit", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Golden, silbern, eisern, ehern", "tokens": ["Gol\u00b7den", ",", "sil\u00b7bern", ",", "ei\u00b7sern", ",", "e\u00b7hern"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "$,", "KOUS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nennt die Alter man der Welt,", "tokens": ["Nennt", "die", "Al\u00b7ter", "man", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PIS", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zum mindern von dem h\u00f6hern", "tokens": ["Und", "zum", "min\u00b7dern", "von", "dem", "h\u00f6\u00b7hern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schreitet fort sie, wird erz\u00e4hlt.", "tokens": ["Schrei\u00b7tet", "fort", "sie", ",", "wird", "er\u00b7z\u00e4hlt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "PPER", "$,", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch der Mensch in unsern Tagen", "tokens": ["Doch", "der", "Mensch", "in", "un\u00b7sern", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieht die Alter sich verkehrt:", "tokens": ["Sieht", "die", "Al\u00b7ter", "sich", "ver\u00b7kehrt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jugend, die schon Sorgen plagen,", "tokens": ["Ju\u00b7gend", ",", "die", "schon", "Sor\u00b7gen", "pla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zeigt nur eisern ihren Wert.", "tokens": ["Zeigt", "nur", "ei\u00b7sern", "ih\u00b7ren", "Wert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Erzgewappnet geht das Leben,", "tokens": ["Erz\u00b7ge\u00b7wapp\u00b7net", "geht", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Selbst die Liebe wird zum Streit,", "tokens": ["Selbst", "die", "Lie\u00b7be", "wird", "zum", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dem stets erneuten Streben", "tokens": ["Und", "dem", "stets", "er\u00b7neu\u00b7ten", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Liegt der Ruhe Gl\u00fcck so weit.", "tokens": ["Liegt", "der", "Ru\u00b7he", "Gl\u00fcck", "so", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Erst nach durchgek\u00e4mpften Jahren", "tokens": ["Erst", "nach", "durch\u00b7ge\u00b7k\u00e4mpf\u00b7ten", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Lacht das Schicksal wieder hold,", "tokens": ["Lacht", "das", "Schick\u00b7sal", "wie\u00b7der", "hold", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit Silber in den Haaren", "tokens": ["Und", "mit", "Sil\u00b7ber", "in", "den", "Haa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird die Zeit, die Ehe \u2013 Gold.", "tokens": ["Wird", "die", "Zeit", ",", "die", "E\u00b7he", "\u2013", "Gold", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "ART", "NN", "$(", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}