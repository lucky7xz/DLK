{"dta.poem.21805": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "ViII.  \n  Amor/ der Wieder-t\u00e4uffer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Verzeih mir/ da\u00df von Rosilis/", "tokens": ["Ver\u00b7zeih", "mir", "/", "da\u00df", "von", "Ro\u00b7si\u00b7lis", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$(", "KOUS", "APPR", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und Mel\u2019 ich/ Buschgen/ hier was", "tokens": ["und", "Mel'", "ich", "/", "Buschgen", "/", "hier", "was"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "NN", "PPER", "$(", "NN", "$(", "ADV", "PWS"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "bleibt meine Treu auch dir gewi\u00df.", "tokens": ["bleibt", "mei\u00b7ne", "Treu", "auch", "dir", "ge\u00b7wi\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was hier von einer ist gedichtet/", "tokens": ["Was", "hier", "von", "ei\u00b7ner", "ist", "ge\u00b7dich\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "PIS", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "hab\u2019 ich auff drey au\u00df Schein gerichtet.", "tokens": ["hab'", "ich", "auff", "drey", "au\u00df", "Schein", "ge\u00b7rich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "CARD", "APPR", "NN", "VVPP", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Wenn dein verliebter Zukkermund", "tokens": ["Wenn", "dein", "ver\u00b7lieb\u00b7ter", "Zuk\u00b7ker\u00b7mund"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "mir die bes\u00fc\u00dften K\u00fcsse schenkte/", "tokens": ["mir", "die", "be\u00b7s\u00fc\u00df\u00b7ten", "K\u00fcs\u00b7se", "schenk\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "und mich mit solchem Labsal tr\u00e4nkte/", "tokens": ["und", "mich", "mit", "sol\u00b7chem", "Lab\u00b7sal", "tr\u00e4nk\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "der alle Krankheit macht gesund", "tokens": ["der", "al\u00b7le", "Krank\u00b7heit", "macht", "ge\u00b7sund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "der s\u00fcsse Honig meiner Liebe.", "tokens": ["der", "s\u00fcs\u00b7se", "Ho\u00b7nig", "mei\u00b7ner", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Betrachtet ich den roten Schein", "tokens": ["Be\u00b7trach\u00b7tet", "ich", "den", "ro\u00b7ten", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "in welchem deine Wangen bl\u00fchen/", "tokens": ["in", "wel\u00b7chem", "dei\u00b7ne", "Wan\u00b7gen", "bl\u00fc\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "wollt\u2019 ich es auff die Rosen ziehen/", "tokens": ["wollt'", "ich", "es", "auff", "die", "Ro\u00b7sen", "zie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "denn mustestu Rosille sein.", "tokens": ["denn", "mus\u00b7te\u00b7stu", "Ro\u00b7sil\u00b7le", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch wirstu wol mein Buschgen bleiben/", "tokens": ["Doch", "wirs\u00b7tu", "wol", "mein", "Buschgen", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "ich mag dich wie ich will beschreiben.", "tokens": ["ich", "mag", "dich", "wie", "ich", "will", "be\u00b7schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "PWAV", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "H\u00e4tt\u2019 ich dich Buschgen stets genannt/", "tokens": ["H\u00e4tt'", "ich", "dich", "Buschgen", "stets", "ge\u00b7nannt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "NN", "ADV", "VVPP", "$("], "meter": "+-++--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "der Leute spizziges Beginnen", "tokens": ["der", "Leu\u00b7te", "spiz\u00b7zi\u00b7ges", "Be\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "ist mir mehr als zuviel bekannt.", "tokens": ["ist", "mir", "mehr", "als", "zu\u00b7viel", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "KOUS", "PIS", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Nu deinen Nahmen ich bescheinet/", "tokens": ["Nu", "dei\u00b7nen", "Nah\u00b7men", "ich", "be\u00b7schei\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "wei\u00df mancher nicht/ wen ich gemeinet.", "tokens": ["wei\u00df", "man\u00b7cher", "nicht", "/", "wen", "ich", "ge\u00b7mei\u00b7net", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKNEG", "$(", "PWS", "PPER", "VVPP", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "So la\u00df mich nu die Rosilis", "tokens": ["So", "la\u00df", "mich", "nu", "die", "Ro\u00b7si\u00b7lis"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "die Mele gleicher massen loben:", "tokens": ["die", "Me\u00b7le", "glei\u00b7cher", "mas\u00b7sen", "lo\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du/ Buschgen/ wirst allein erhoben", "tokens": ["Du", "/", "Buschgen", "/", "wirst", "al\u00b7lein", "er\u00b7ho\u00b7ben"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$(", "NN", "$(", "VAFIN", "ADV", "VVPP"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "ob ich dich schon Dorinde hie\u00df\u2019", "tokens": ["ob", "ich", "dich", "schon", "Do\u00b7rin\u00b7de", "hie\u00df'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "ach", "tokens": ["ach"], "token_info": ["word"], "pos": ["XY"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "ich manches Lied auch lassen h\u00f6ren.", "tokens": ["ich", "man\u00b7ches", "Lied", "auch", "las\u00b7sen", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Gedenkstu nicht/ wie du mich auch", "tokens": ["Ge\u00b7denks\u00b7tu", "nicht", "/", "wie", "du", "mich", "auch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "$(", "PWAV", "PPER", "PRF", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "bald Oridor/ bald Karpas nennest/", "tokens": ["bald", "O\u00b7ri\u00b7dor", "/", "bald", "Kar\u00b7pas", "nen\u00b7nest", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "$(", "ADV", "NE", "VVFIN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "da du den Filidor nur kennest:", "tokens": ["da", "du", "den", "Fi\u00b7li\u00b7dor", "nur", "ken\u00b7nest", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich/ Schaz/ das ist auch mein Gebrauch/", "tokens": ["Sich", "/", "Schaz", "/", "das", "ist", "auch", "mein", "Ge\u00b7brauch", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "$(", "NN", "$(", "PDS", "VAFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "den ich zu erst von dir gesehen", "tokens": ["den", "ich", "zu", "erst", "von", "dir", "ge\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ADV", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "dem pfleg\u2019 ich k\u00fcnstlich nachzugehen.", "tokens": ["dem", "pfleg'", "ich", "k\u00fcnst\u00b7lich", "nach\u00b7zu\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ein Herze hab\u2019 ich nur allein/", "tokens": ["Ein", "Her\u00b7ze", "hab'", "ich", "nur", "al\u00b7lein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein einger Geist bewegt mein Leben/", "tokens": ["Ein", "ein\u00b7ger", "Geist", "be\u00b7wegt", "mein", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "die ich durch hundert-tausend nennen", "tokens": ["die", "ich", "durch", "hun\u00b7der\u00b7ttau\u00b7send", "nen\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRELS", "PPER", "APPR", "CARD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "f\u00fcr meine Seele wil bekennen.", "tokens": ["f\u00fcr", "mei\u00b7ne", "See\u00b7le", "wil", "be\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}