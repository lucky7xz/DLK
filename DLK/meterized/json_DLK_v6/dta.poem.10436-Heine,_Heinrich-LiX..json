{"dta.poem.10436": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "LiX.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Der Herbstwind r\u00fcttelt die B\u00e4ume,", "tokens": ["Der", "Herbst\u00b7wind", "r\u00fct\u00b7telt", "die", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Nacht ist feucht und kalt;", "tokens": ["Die", "Nacht", "ist", "feucht", "und", "kalt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Geh\u00fcllt im grauen Mantel,", "tokens": ["Ge\u00b7h\u00fcllt", "im", "grau\u00b7en", "Man\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Reite ich einsam im Wald!", "tokens": ["Rei\u00b7te", "ich", "ein\u00b7sam", "im", "Wald", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Und wie ich reite, so reiten", "tokens": ["Und", "wie", "ich", "rei\u00b7te", ",", "so", "rei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "ADV", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mir die Gedanken voraus;", "tokens": ["Mir", "die", "Ge\u00b7dan\u00b7ken", "vo\u00b7raus", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Sie tragen mich leicht und luftig", "tokens": ["Sie", "tra\u00b7gen", "mich", "leicht", "und", "luf\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "KON", "ADJD"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nach meiner Liebsten Haus.", "tokens": ["Nach", "mei\u00b7ner", "Liebs\u00b7ten", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Hunde bellen, die Diener", "tokens": ["Die", "Hun\u00b7de", "bel\u00b7len", ",", "die", "Die\u00b7ner"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Erscheinen mit Kerzengeflirr;", "tokens": ["Er\u00b7schei\u00b7nen", "mit", "Ker\u00b7zen\u00b7ge\u00b7flirr", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Die Wendeltreppe st\u00fcrm' ich", "tokens": ["Die", "Wen\u00b7del\u00b7trep\u00b7pe", "st\u00fcrm'", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hinauf mit Sporengeklirr.", "tokens": ["Hin\u00b7auf", "mit", "Spo\u00b7ren\u00b7ge\u00b7klirr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Im leuchtenden Teppichgemache,", "tokens": ["Im", "leuch\u00b7ten\u00b7den", "Tep\u00b7pich\u00b7ge\u00b7ma\u00b7che", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da ist es so duftig und warm,", "tokens": ["Da", "ist", "es", "so", "duf\u00b7tig", "und", "warm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Da harret meiner die Holde \u2014", "tokens": ["Da", "har\u00b7ret", "mei\u00b7ner", "die", "Hol\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ART", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ich fliege in ihren Arm.", "tokens": ["Ich", "flie\u00b7ge", "in", "ih\u00b7ren", "Arm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Es s\u00e4uselt der Wind in den Bl\u00e4ttern,", "tokens": ["Es", "s\u00e4u\u00b7selt", "der", "Wind", "in", "den", "Bl\u00e4t\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Es spricht der Eichenbaum:", "tokens": ["Es", "spricht", "der", "Ei\u00b7chen\u00b7baum", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was willst du, th\u00f6richter Reiter,", "tokens": ["Was", "willst", "du", ",", "th\u00f6\u00b7rich\u00b7ter", "Rei\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mit deinem th\u00f6richten Traum?", "tokens": ["Mit", "dei\u00b7nem", "th\u00f6\u00b7rich\u00b7ten", "Traum", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}