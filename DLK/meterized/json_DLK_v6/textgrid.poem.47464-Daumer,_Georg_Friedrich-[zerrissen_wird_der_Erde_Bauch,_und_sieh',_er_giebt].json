{"textgrid.poem.47464": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[zerrissen wird der Erde Bauch, und sieh', er giebt]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zerrissen wird der Erde Bauch, und sieh', er giebt", "tokens": ["Zer\u00b7ris\u00b7sen", "wird", "der", "Er\u00b7de", "Bauch", ",", "und", "sieh'", ",", "er", "giebt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN", "NN", "$,", "KON", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem, welcher ihn zerrissen hat, Goldsch\u00e4tze Preis;", "tokens": ["Dem", ",", "wel\u00b7cher", "ihn", "zer\u00b7ris\u00b7sen", "hat", ",", "Gold\u00b7sch\u00e4t\u00b7ze", "Preis", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "VVPP", "VAFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Steinw\u00fcrfe fliegen auf den Baum und er gew\u00e4hrt", "tokens": ["Stein\u00b7w\u00fcr\u00b7fe", "flie\u00b7gen", "auf", "den", "Baum", "und", "er", "ge\u00b7w\u00e4hrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "KON", "PPER", "VVPP"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Dem, welcher ihn geworfen hat, Fruchtlabungen;", "tokens": ["Dem", ",", "wel\u00b7cher", "ihn", "ge\u00b7wor\u00b7fen", "hat", ",", "Fruch\u00b7tla\u00b7bun\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "VVPP", "VAFIN", "$,", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Zerschlagen wird der Muschel Leib, sie aber schenkt", "tokens": ["Zer\u00b7schla\u00b7gen", "wird", "der", "Mu\u00b7schel", "Leib", ",", "sie", "a\u00b7ber", "schenkt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$,", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dem, welcher sie zerschlagen hat, ihr Perlenherz; \u2013", "tokens": ["Dem", ",", "wel\u00b7cher", "sie", "zer\u00b7schla\u00b7gen", "hat", ",", "ihr", "Per\u00b7len\u00b7herz", ";", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "VVPP", "VAFIN", "$,", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was willst du, Mensch, unedler als der Erde Bauch,", "tokens": ["Was", "willst", "du", ",", "Mensch", ",", "u\u00b7ned\u00b7ler", "als", "der", "Er\u00b7de", "Bauch", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "$,", "NN", "$,", "ADJD", "KOKOM", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Unedler, als der Baum und als die Muschel sein?", "tokens": ["U\u00b7ned\u00b7ler", ",", "als", "der", "Baum", "und", "als", "die", "Mu\u00b7schel", "sein", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "ART", "NN", "KON", "KOUS", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}