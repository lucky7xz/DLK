{"textgrid.poem.45651": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Abstimmung", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Habt ihr noch nie einen Preu\u00dfen gesehn?", "tokens": ["Habt", "ihr", "noch", "nie", "ei\u00b7nen", "Preu\u00b7\u00dfen", "ge\u00b7sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+---+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Betrachtet den schlagenden Finken,", "tokens": ["Be\u00b7trach\u00b7tet", "den", "schla\u00b7gen\u00b7den", "Fin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wenn andere rechts nach Ehre gehn,", "tokens": ["Wenn", "an\u00b7de\u00b7re", "rechts", "nach", "Eh\u00b7re", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sieht er den Vorteil zur Linken.", "tokens": ["Sieht", "er", "den", "Vor\u00b7teil", "zur", "Lin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}