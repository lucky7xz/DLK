{"textgrid.poem.53950": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Es ist", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist so viel unverbrauchte Z\u00e4rtlichkeit in Hotelzimmern,", "tokens": ["Es", "ist", "so", "viel", "un\u00b7ver\u00b7brauch\u00b7te", "Z\u00e4rt\u00b7lich\u00b7keit", "in", "Ho\u00b7tel\u00b7zim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "wo sie allein liegen:", "tokens": ["wo", "sie", "al\u00b7lein", "lie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "ein Mann, oder eine Frau, oder ein angebrochenes junges M\u00e4dchen \u2013", "tokens": ["ein", "Mann", ",", "o\u00b7der", "ei\u00b7ne", "Frau", ",", "o\u00b7der", "ein", "an\u00b7ge\u00b7bro\u00b7che\u00b7nes", "jun\u00b7ges", "M\u00e4d\u00b7chen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ART", "NN", "$,", "KON", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+--+-+-+-+-+--+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "in leiser L\u00e4cherlichkeit liegen wir allein.", "tokens": ["in", "lei\u00b7ser", "L\u00e4\u00b7cher\u00b7lich\u00b7keit", "lie\u00b7gen", "wir", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es ist eine Einsamkeit, umflossen", "tokens": ["Es", "ist", "ei\u00b7ne", "Ein\u00b7sam\u00b7keit", ",", "um\u00b7flos\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "VVINF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "von den Str\u00f6men des st\u00e4dtischen Gases,", "tokens": ["von", "den", "Str\u00f6\u00b7men", "des", "st\u00e4d\u00b7ti\u00b7schen", "Ga\u00b7ses", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.7": {"text": "des elektrischen Stromes, f\u00fcr alle gemacht,", "tokens": ["des", "e\u00b7lekt\u00b7ri\u00b7schen", "Stro\u00b7mes", ",", "f\u00fcr", "al\u00b7le", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PIS", "VVPP", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.8": {"text": "einer Zentralheizung, eines Zentralessens, einer Zentralzeitung . . .", "tokens": ["ei\u00b7ner", "Zent\u00b7ral\u00b7hei\u00b7zung", ",", "ei\u00b7nes", "Zent\u00b7ra\u00b7les\u00b7sens", ",", "ei\u00b7ner", "Zent\u00b7ral\u00b7zei\u00b7tung", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$.", "$.", "$."], "meter": "+-+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.9": {"text": "aber ein kleiner Fleck ist noch da,", "tokens": ["a\u00b7ber", "ein", "klei\u00b7ner", "Fleck", "ist", "noch", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VAFIN", "ADV", "ADV", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "auf dem sind wir allein.", "tokens": ["auf", "dem", "sind", "wir", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Jeder liegt in seiner Schublade.", "tokens": ["Je\u00b7der", "liegt", "in", "sei\u00b7ner", "Schub\u00b7la\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.12": {"text": "Die kleinen H\u00e4rchen auf den Oberarmen schwanken suchend im Luftzug,", "tokens": ["Die", "klei\u00b7nen", "H\u00e4r\u00b7chen", "auf", "den", "O\u00b7be\u00b7rar\u00b7men", "schwan\u00b7ken", "su\u00b7chend", "im", "Luft\u00b7zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+--+-", "measure": "iambic.octa.plus.relaxed"}, "line.13": {"text": "wie die Greifer der Meerespflanzen in str\u00f6mendem Wasser;", "tokens": ["wie", "die", "Grei\u00b7fer", "der", "Mee\u00b7res\u00b7pflan\u00b7zen", "in", "str\u00f6\u00b7men\u00b7dem", "Was\u00b7ser", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "die Haut langweilt sich.", "tokens": ["die", "Haut", "lang\u00b7weilt", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Wenn jetzt einer k\u00e4me und sagte: \u00bbBitte sehr! ich liege Ihnen zur Verf\u00fcgung!\u00ab", "tokens": ["Wenn", "jetzt", "ei\u00b7ner", "k\u00e4\u00b7me", "und", "sag\u00b7te", ":", "\u00bb", "Bit\u00b7te", "sehr", "!", "ich", "lie\u00b7ge", "Ih\u00b7nen", "zur", "Ver\u00b7f\u00fc\u00b7gung", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ADV", "PIS", "VVFIN", "KON", "VVFIN", "$.", "$(", "NN", "ADV", "$.", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$.", "$("], "meter": "+-+-+--+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.16": {"text": "wenn ich jetzt durch die Wand ginge zu meiner Nachbarin \u2013", "tokens": ["wenn", "ich", "jetzt", "durch", "die", "Wand", "gin\u00b7ge", "zu", "mei\u00b7ner", "Nach\u00b7ba\u00b7rin", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.17": {"text": "(\u00bbMan ist doch keine Hure! ich werfe mein Leben nicht in Hotels weg!\u00ab \u2013 Kusch.)", "tokens": ["(", "\u00bb", "Man", "ist", "doch", "kei\u00b7ne", "Hu\u00b7re", "!", "ich", "wer\u00b7fe", "mein", "Le\u00b7ben", "nicht", "in", "Ho\u00b7tels", "weg", "!", "\u00ab", "\u2013", "Kusch", ".", ")"], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "word", "punct", "punct"], "pos": ["$(", "$(", "PIS", "VAFIN", "ADV", "PIAT", "NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "PTKNEG", "APPR", "NN", "PTKVZ", "$.", "$(", "$(", "NN", "$.", "$("], "meter": "-+-+-+--+--+-+-+--+", "measure": "iambic.octa.plus.relaxed"}, "line.18": {"text": "\u2013 wenn jetzt eine dicke Dame k\u00e4me, mich im Bad zu massieren;", "tokens": ["\u2013", "wenn", "jetzt", "ei\u00b7ne", "di\u00b7cke", "Da\u00b7me", "k\u00e4\u00b7me", ",", "mich", "im", "Bad", "zu", "mas\u00b7sie\u00b7ren", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,", "PRF", "APPRART", "NE", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+--", "measure": "trochaic.octa.plus"}, "line.19": {"text": "ungen\u00fctzt ist die Nacht.", "tokens": ["un\u00b7ge\u00b7n\u00fctzt", "ist", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.20": {"text": "Dreivierteleins.", "tokens": ["Drei\u00b7vier\u00b7te\u00b7leins", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.21": {"text": "Es kocht in den R\u00f6hren des Badezimmers;", "tokens": ["Es", "kocht", "in", "den", "R\u00f6h\u00b7ren", "des", "Ba\u00b7de\u00b7zim\u00b7mers", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.22": {"text": "badet jemand noch so sp\u00e4t?", "tokens": ["ba\u00b7det", "je\u00b7mand", "noch", "so", "sp\u00e4t", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Neugierig sind wir auf fremde K\u00f6rper.", "tokens": ["Neu\u00b7gie\u00b7rig", "sind", "wir", "auf", "frem\u00b7de", "K\u00f6r\u00b7per", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.24": {"text": "Wie legen Sie abends das Hemd auf den Stuhl? Lieben Sie Fruchtsalz?", "tokens": ["Wie", "le\u00b7gen", "Sie", "a\u00b7bends", "das", "Hemd", "auf", "den", "Stuhl", "?", "Lie\u00b7ben", "Sie", "Frucht\u00b7salz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "ART", "NN", "$.", "NN", "PPER", "NN", "$."], "meter": "-+--+--+--++--+-", "measure": "amphibrach.tri.plus"}, "line.25": {"text": "Ziehen Sie Ihre Uhr morgens oder abends auf?", "tokens": ["Zie\u00b7hen", "Sie", "Ih\u00b7re", "Uhr", "mor\u00b7gens", "o\u00b7der", "a\u00b7bends", "auf", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ADV", "KON", "ADV", "PTKVZ", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.26": {"text": "Und in der Liebe?", "tokens": ["Und", "in", "der", "Lie\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.27": {"text": "Sind Sie gesund? Verzeihen Sie, ich habe solche Furcht vor Krankheiten \u2013", "tokens": ["Sind", "Sie", "ge\u00b7sund", "?", "Ver\u00b7zei\u00b7hen", "Sie", ",", "ich", "ha\u00b7be", "sol\u00b7che", "Furcht", "vor", "Krank\u00b7hei\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$.", "NN", "PPER", "$,", "PPER", "VAFIN", "PIAT", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-+--+-", "measure": "iambic.octa.plus.relaxed"}, "line.28": {"text": "das ist ein Teil meiner Tugend.", "tokens": ["das", "ist", "ein", "Teil", "mei\u00b7ner", "Tu\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.29": {"text": "I'm in love again \u2013", "tokens": ["I'm", "in", "lo\u00b7ve", "a\u00b7gain", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.30": {"text": "nein, das eigentlich nicht:", "tokens": ["nein", ",", "das", "ei\u00b7gent\u00b7lich", "nicht", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "ADV", "PTKNEG", "$."], "meter": "+----+", "measure": "dactylic.init"}, "line.31": {"text": "es sollte nur jemand da sein, an dem ich mich sp\u00fcren kann.", "tokens": ["es", "soll\u00b7te", "nur", "je\u00b7mand", "da", "sein", ",", "an", "dem", "ich", "mich", "sp\u00fc\u00b7ren", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIS", "ADV", "VAINF", "$,", "APPR", "PRELS", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.32": {"text": "Warum, 318 (mit Bad) liegen Sie so allein?", "tokens": ["Wa\u00b7rum", ",", "318", "(", "mit", "Bad", ")", "lie\u00b7gen", "Sie", "so", "al\u00b7lein", "?"], "token_info": ["word", "punct", "number", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "CARD", "$(", "APPR", "NE", "$(", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.33": {"text": "Denkbar w\u00e4re auch eine Hotelgeisha, die h\u00f6flich liebt,", "tokens": ["Denk\u00b7bar", "w\u00e4\u00b7re", "auch", "ei\u00b7ne", "Ho\u00b7tel\u00b7geis\u00b7ha", ",", "die", "h\u00f6f\u00b7lich", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-----+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "und die auf der Rechnung nur als kleiner, diskreter Kreis vermerkt ist \u2013", "tokens": ["und", "die", "auf", "der", "Rech\u00b7nung", "nur", "als", "klei\u00b7ner", ",", "dis\u00b7kre\u00b7ter", "Kreis", "ver\u00b7merkt", "ist", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "ART", "NN", "ADV", "KOUS", "ADJA", "$,", "ADJA", "NN", "VVPP", "VAFIN", "$("], "meter": "-+--+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.35": {"text": "aber sch\u00f6ner w\u00e4re ein Gast.", "tokens": ["a\u00b7ber", "sch\u00f6\u00b7ner", "w\u00e4\u00b7re", "ein", "Gast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.36": {"text": "Warum kommt nie ein Einsamer zu einer Einsamen?", "tokens": ["Wa\u00b7rum", "kommt", "nie", "ein", "Ein\u00b7sa\u00b7mer", "zu", "ei\u00b7ner", "Ein\u00b7sa\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.37": {"text": "Stolz kriechen wir in unser zust\u00e4ndiges Geh\u00e4us,", "tokens": ["Stolz", "krie\u00b7chen", "wir", "in", "un\u00b7ser", "zu\u00b7st\u00e4n\u00b7di\u00b7ges", "Ge\u00b7h\u00e4us", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.38": {"text": "hygienisch, unnahbar, vern\u00fcnftig,", "tokens": ["hy\u00b7gi\u00b7e\u00b7nisch", ",", "un\u00b7nah\u00b7bar", ",", "ver\u00b7n\u00fcnf\u00b7tig", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.39": {"text": "allein.", "tokens": ["al\u00b7lein", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+", "measure": "iambic.single"}, "line.40": {"text": "Knips das Licht an, sagt der Schlaflose zu sich selbst", "tokens": ["Knips", "das", "Licht", "an", ",", "sagt", "der", "Schlaf\u00b7lo\u00b7se", "zu", "sich", "selbst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "PTKVZ", "$,", "VVFIN", "ART", "NN", "APPR", "PRF", "ADV"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.41": {"text": "(er duzt sich, weil er sich schon so lange kennt) \u2013", "tokens": ["(", "er", "duzt", "sich", ",", "weil", "er", "sich", "schon", "so", "lan\u00b7ge", "kennt", ")", "\u2013"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PRF", "$,", "KOUS", "PPER", "PRF", "ADV", "ADV", "ADV", "VVFIN", "$(", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.42": {"text": "und lies noch ein bi\u00dfchen.", "tokens": ["und", "lies", "noch", "ein", "bi\u00df\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.43": {"text": "Du hast zu viel Pfirsich-Melba gegessen, daher solche Gedanken,", "tokens": ["Du", "hast", "zu", "viel", "Pfir\u00b7sich\u00b7Mel\u00b7ba", "ge\u00b7ges\u00b7sen", ",", "da\u00b7her", "sol\u00b7che", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN", "VVPP", "$,", "PAV", "PIAT", "NN", "$,"], "meter": "-+-+--+--+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.44": {"text": "Luftblasen auf dem Meer der innern Sekretion.", "tokens": ["