{"textgrid.poem.64266": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Dombaufest in K\u00f6ln", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wieder t\u00f6nt der Glocken Feierklang", "tokens": ["Und", "wie\u00b7der", "t\u00f6nt", "der", "Glo\u00b7cken", "Fei\u00b7er\u00b7klang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Durch preu\u00dfisch Land: er schwebet und er schwinget", "tokens": ["Durch", "preu\u00b7\u00dfisch", "Land", ":", "er", "schwe\u00b7bet", "und", "er", "schwin\u00b7get"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "NN", "$.", "PPER", "VVFIN", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Leis auf der Flut des alten Rheins dahin:", "tokens": ["Leis", "auf", "der", "Flut", "des", "al\u00b7ten", "Rheins", "da\u00b7hin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "ADJA", "NE", "PAV", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "Nicht Siegesl\u00e4uten ist's: ein heilig Werk", "tokens": ["Nicht", "Sie\u00b7ges\u00b7l\u00e4u\u00b7ten", "ist's", ":", "ein", "hei\u00b7lig", "Werk"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "NN", "VAFIN", "$.", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Verk\u00fcndet er dem heil'gen K\u00f6ln: den Dom,", "tokens": ["Ver\u00b7k\u00fcn\u00b7det", "er", "dem", "heil'\u00b7gen", "K\u00f6ln", ":", "den", "Dom", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Den gro\u00dfer Ahnen gro\u00dfer Sinn begonnen,", "tokens": ["Den", "gro\u00b7\u00dfer", "Ah\u00b7nen", "gro\u00b7\u00dfer", "Sinn", "be\u00b7gon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und den ein klein Geschlecht verzagend aufgab, \u2013", "tokens": ["Und", "den", "ein", "klein", "Ge\u00b7schlecht", "ver\u00b7za\u00b7gend", "auf\u00b7gab", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "ART", "ADJD", "NN", "VVPP", "VVFIN", "$,", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Ihn bauen w\u00fcrdigere Enkel aus!", "tokens": ["Ihn", "bau\u00b7en", "w\u00fcr\u00b7di\u00b7ge\u00b7re", "En\u00b7kel", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VAFIN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}