{"textgrid.poem.35070": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Berufung", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Greif zu, o Mensch, greif zu,", "tokens": ["Greif", "zu", ",", "o", "Mensch", ",", "greif", "zu", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "FM", "NN", "$,", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wenn dir der Himmel reicht die offne Hand,", "tokens": ["Wenn", "dir", "der", "Him\u00b7mel", "reicht", "die", "off\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sonst denke nicht, da\u00df du", "tokens": ["Sonst", "den\u00b7ke", "nicht", ",", "da\u00df", "du"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Einst seist im Buch des Gl\u00fcckes mit genannt.", "tokens": ["Einst", "seist", "im", "Buch", "des", "Gl\u00fc\u00b7ckes", "mit", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "ART", "NN", "APPR", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wer diesen Wink des Himmels nicht beachtet,", "tokens": ["Wer", "die\u00b7sen", "Wink", "des", "Him\u00b7mels", "nicht", "be\u00b7ach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der sieht auch nicht des Himmels Rathschlu\u00df ein", "tokens": ["Der", "sieht", "auch", "nicht", "des", "Him\u00b7mels", "Rath\u00b7schlu\u00df", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "PTKNEG", "ART", "NN", "NN", "ART"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und wird, wie er auch nach dem Gl\u00fccke trachtet,", "tokens": ["Und", "wird", ",", "wie", "er", "auch", "nach", "dem", "Gl\u00fc\u00b7cke", "trach\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "PWAV", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Doch ohne Gl\u00fcck, so lang er trachtet, sein.", "tokens": ["Doch", "oh\u00b7ne", "Gl\u00fcck", ",", "so", "lang", "er", "trach\u00b7tet", ",", "sein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "ADV", "ADJD", "PPER", "VVFIN", "$,", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Greif zu, o Volk, greif zu,", "tokens": ["Greif", "zu", ",", "o", "Volk", ",", "greif", "zu", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "FM", "NN", "$,", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wenn dir der Himmel reicht die offne Hand,", "tokens": ["Wenn", "dir", "der", "Him\u00b7mel", "reicht", "die", "off\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sonst denke nicht, da\u00df du", "tokens": ["Sonst", "den\u00b7ke", "nicht", ",", "da\u00df", "du"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Einst seist im Buch der V\u00f6lker mit genannt.", "tokens": ["Einst", "seist", "im", "Buch", "der", "V\u00f6l\u00b7ker", "mit", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "ART", "NN", "APPR", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenn diesen Fingerzeig du nicht beachtest,", "tokens": ["Wenn", "die\u00b7sen", "Fin\u00b7ger\u00b7zeig", "du", "nicht", "be\u00b7ach\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wirst du dem Tod, dem Untergang dich weihn", "tokens": ["Wirst", "du", "dem", "Tod", ",", "dem", "Un\u00b7ter\u00b7gang", "dich", "weihn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und, ob du auch nach Glanz und F\u00fchrung trachtest,", "tokens": ["Und", ",", "ob", "du", "auch", "nach", "Glanz", "und", "F\u00fch\u00b7rung", "trach\u00b7test", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Doch unter V\u00f6lkern nur ein V\u00f6lkchen sein.", "tokens": ["Doch", "un\u00b7ter", "V\u00f6l\u00b7kern", "nur", "ein", "V\u00f6lk\u00b7chen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADV", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Greif zu, o F\u00fcrst, greif zu,", "tokens": ["Greif", "zu", ",", "o", "F\u00fcrst", ",", "greif", "zu", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "FM", "NN", "$,", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wenn dir der Himmel reicht die offne Hand,", "tokens": ["Wenn", "dir", "der", "Him\u00b7mel", "reicht", "die", "off\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sonst denke nicht, da\u00df du", "tokens": ["Sonst", "den\u00b7ke", "nicht", ",", "da\u00df", "du"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Einst seist im Buch der F\u00fcrsten mit genannt.", "tokens": ["Einst", "seist", "im", "Buch", "der", "F\u00fcrs\u00b7ten", "mit", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "ART", "NN", "APPR", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Herrscher der des Himmels Stimme achtet,", "tokens": ["Ein", "Herr\u00b7scher", "der", "des", "Him\u00b7mels", "Stim\u00b7me", "ach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die ihn beruft, der V\u00f6lker Heil zu sein,", "tokens": ["Die", "ihn", "be\u00b7ruft", ",", "der", "V\u00f6l\u00b7ker", "Heil", "zu", "sein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "ART", "NN", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Bei dem stellt sich das Gl\u00fcck, nach dem er trachtet,", "tokens": ["Bei", "dem", "stellt", "sich", "das", "Gl\u00fcck", ",", "nach", "dem", "er", "trach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "PRF", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Ja ganz von selbst, als Himmelsgabe, ein.", "tokens": ["Ja", "ganz", "von", "selbst", ",", "als", "Him\u00b7mels\u00b7ga\u00b7be", ",", "ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PTKANT", "ADV", "APPR", "ADV", "$,", "KOUS", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}