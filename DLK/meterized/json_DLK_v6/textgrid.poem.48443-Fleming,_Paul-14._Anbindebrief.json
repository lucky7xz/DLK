{"textgrid.poem.48443": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "14. Anbindebrief", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "die nicht auf Sch\u00f6nheit mehr als auf die Tugend schauen,", "tokens": ["die", "nicht", "auf", "Sch\u00f6n\u00b7heit", "mehr", "als", "auf", "die", "Tu\u00b7gend", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPR", "NN", "PIAT", "KOKOM", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der Weiber besten Schmuck! Worauf di\u00df Binden zielt,", "tokens": ["der", "Wei\u00b7ber", "bes\u00b7ten", "Schmuck", "!", "Wo\u00b7rauf", "di\u00df", "Bin\u00b7den", "zielt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$.", "PAV", "PDS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "ist nichts als Ehrlichkeit. Wem Redlichkeit beliebt,", "tokens": ["ist", "nichts", "als", "Ehr\u00b7lich\u00b7keit", ".", "Wem", "Red\u00b7lich\u00b7keit", "be\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "KOKOM", "NN", "$.", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ist b\u00f6sen St\u00fccken feind. Was euch und mir geb\u00fchret,", "tokens": ["ist", "b\u00f6\u00b7sen", "St\u00fc\u00b7cken", "feind", ".", "Was", "euch", "und", "mir", "ge\u00b7b\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "NN", "$.", "PWS", "PPER", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "das wisset ihr und ich. Und da\u00df ihr denn versp\u00fcret,", "tokens": ["das", "wis\u00b7set", "ihr", "und", "ich", ".", "Und", "da\u00df", "ihr", "denn", "ver\u00b7sp\u00fc\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "KON", "PPER", "$.", "KON", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "da\u00df ich des auch bin froh, was euch der Himmel giebt,", "tokens": ["da\u00df", "ich", "des", "auch", "bin", "froh", ",", "was", "euch", "der", "Him\u00b7mel", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADV", "VAFIN", "ADJD", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "so nehmet, werte Frau, von der euch guten Hand", "tokens": ["so", "neh\u00b7met", ",", "wer\u00b7te", "Frau", ",", "von", "der", "euch", "gu\u00b7ten", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "NN", "$,", "APPR", "PRELS", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "di\u00df, obwol schlechte, hin! Der Gaben leichte Schalen", "tokens": ["di\u00df", ",", "ob\u00b7wol", "schlech\u00b7te", ",", "hin", "!", "Der", "Ga\u00b7ben", "leich\u00b7te", "Scha\u00b7len"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "$,", "KOUS", "ADJA", "$,", "PTKVZ", "$.", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.9": {"text": "wiegt auf mein schwerer Wundsch: da\u00df, weil der Sonnen Strahlen", "tokens": ["wiegt", "auf", "mein", "schwe\u00b7rer", "Wund\u00b7sch", ":", "da\u00df", ",", "weil", "der", "Son\u00b7nen", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$.", "KOUS", "$,", "KOUS", "ART", "NN", "NN"], "meter": "+--+-++-+-+-+-", "measure": "iambic.septa.invert"}, "line.10": {"text": "erw\u00e4rmen diese Welt, euch treff' kein \u00dcbelstand!", "tokens": ["er\u00b7w\u00e4r\u00b7men", "die\u00b7se", "Welt", ",", "euch", "treff'", "kein", "\u00dc\u00b7bel\u00b7stand", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "$,", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}