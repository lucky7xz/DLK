{"textgrid.poem.61806": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An der Bucht im Lotsenhause", "tokens": ["An", "der", "Bucht", "im", "Lot\u00b7sen\u00b7hau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hab' ich mich zur Ruh' gelegt,", "tokens": ["Hab'", "ich", "mich", "zur", "Ruh'", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo der nahen See Gebrause", "tokens": ["Wo", "der", "na\u00b7hen", "See", "Ge\u00b7brau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie Gesang ans Ohr mir schl\u00e4gt.", "tokens": ["Wie", "Ge\u00b7sang", "ans", "Ohr", "mir", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Bei dem Schall der Wellenlieder", "tokens": ["Bei", "dem", "Schall", "der", "Wel\u00b7len\u00b7lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wogt in eins, was fern und nah,", "tokens": ["Wogt", "in", "eins", ",", "was", "fern", "und", "nah", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIS", "$,", "PRELS", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mir tr\u00e4umt, ich f\u00fchre wieder", "tokens": ["Und", "mir", "tr\u00e4umt", ",", "ich", "f\u00fch\u00b7re", "wie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf der blauen Adria.", "tokens": ["Auf", "der", "blau\u00b7en", "Ad\u00b7ria", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NE", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Goldfruchtd\u00fcfte der Levante", "tokens": ["Gold\u00b7frucht\u00b7d\u00fcf\u00b7te", "der", "Le\u00b7van\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Flattern schon ins Schiff herein,", "tokens": ["Flat\u00b7tern", "schon", "ins", "Schiff", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schon aus Nebeln d\u00e4mmert Zante", "tokens": ["Schon", "aus", "Ne\u00b7beln", "d\u00e4m\u00b7mert", "Zan\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "VVFIN", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcbers Meer im Rosenschein.", "tokens": ["\u00dc\u00b7bers", "Meer", "im", "Ro\u00b7sen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und das Schiffsvolk summt und fl\u00f6tet,", "tokens": ["Und", "das", "Schiffs\u00b7volk", "summt", "und", "fl\u00f6\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und am Mast im Abendwehn", "tokens": ["Und", "am", "Mast", "im", "Ab\u00b7end\u00b7wehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "APPRART", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Seh' ich dich vom Strahl ger\u00f6tet,", "tokens": ["Seh'", "ich", "dich", "vom", "Strahl", "ge\u00b7r\u00f6\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schottlands schlanke Tochter, stehn.", "tokens": ["Schott\u00b7lands", "schlan\u00b7ke", "Toch\u00b7ter", ",", "stehn", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wohl umleuchtet weit im Bogen", "tokens": ["Wohl", "um\u00b7leuch\u00b7tet", "weit", "im", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Uns der Wogen himmlisch Blau,", "tokens": ["Uns", "der", "Wo\u00b7gen", "himm\u00b7lisch", "Blau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber blauer als die Wogen", "tokens": ["A\u00b7ber", "blau\u00b7er", "als", "die", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KOKOM", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gl\u00e4nzt dein Auge, sch\u00f6ne Frau.", "tokens": ["Gl\u00e4nzt", "dein", "Au\u00b7ge", ",", "sch\u00f6\u00b7ne", "Frau", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "L\u00e4chelnd mir im Silberbecher", "tokens": ["L\u00e4\u00b7chelnd", "mir", "im", "Sil\u00b7ber\u00b7be\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Reichst du Zyperns Traubenblut,", "tokens": ["Reichst", "du", "Zy\u00b7perns", "Trau\u00b7ben\u00b7blut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich trink', ein sel'ger Zecher,", "tokens": ["Und", "ich", "trink'", ",", "ein", "sel'\u00b7ger", "Ze\u00b7cher", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Wo dein s\u00fc\u00dfer Mund geruht.", "tokens": ["Wo", "dein", "s\u00fc\u00b7\u00dfer", "Mund", "ge\u00b7ruht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und umwallt vom Lockengolde,", "tokens": ["Und", "um\u00b7wallt", "vom", "Lo\u00b7cken\u00b7gol\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Drin der Seewind w\u00fchlt zum Scherz,", "tokens": ["Drin", "der", "See\u00b7wind", "w\u00fchlt", "zum", "Scherz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Scheinst du v\u00f6llig mir Isolde,", "tokens": ["Scheinst", "du", "v\u00f6l\u00b7lig", "mir", "I\u00b7sol\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPER", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und wie Tristans schwillt mein Herz.", "tokens": ["Und", "wie", "Tris\u00b7tans", "schwillt", "mein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NE", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "T\u00f6richt Herz, la\u00df ab zu schwellen!", "tokens": ["T\u00f6\u00b7richt", "Herz", ",", "la\u00df", "ab", "zu", "schwel\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVIMP", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Halt die rasche Glut zur\u00fcck!", "tokens": ["Halt", "die", "ra\u00b7sche", "Glut", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gaukelnd necken Wind und Wellen", "tokens": ["Gau\u00b7kelnd", "ne\u00b7cken", "Wind", "und", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dich mit l\u00e4ngst entschwundnem Gl\u00fcck.", "tokens": ["Dich", "mit", "l\u00e4ngst", "ent\u00b7schwund\u00b7nem", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}