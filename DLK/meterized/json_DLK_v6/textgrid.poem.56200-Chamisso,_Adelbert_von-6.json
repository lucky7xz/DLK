{"textgrid.poem.56200": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "6", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du mein Schmerz und meine Wonne,", "tokens": ["Du", "mein", "Schmerz", "und", "mei\u00b7ne", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meiner Blindheit andre Sonne,", "tokens": ["Mei\u00b7ner", "Blind\u00b7heit", "and\u00b7re", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Holde Stimme, bist verhallt.", "tokens": ["Hol\u00b7de", "Stim\u00b7me", ",", "bist", "ver\u00b7hallt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Nacht h\u00fcllt sich in Schweigen,", "tokens": ["Mei\u00b7ne", "Nacht", "h\u00fcllt", "sich", "in", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, so schaurig, ach, so eigen,", "tokens": ["Ach", ",", "so", "schau\u00b7rig", ",", "ach", ",", "so", "ei\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "ADJD", "$,", "ITJ", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alles \u00f6d und leer und kalt!", "tokens": ["Al\u00b7les", "\u00f6d", "und", "leer", "und", "kalt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Leise welken, mich entf\u00e4rben", "tokens": ["Lei\u00b7se", "wel\u00b7ken", ",", "mich", "ent\u00b7f\u00e4r\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVINF", "$,", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seht ihr Schwestern mich und sterben,", "tokens": ["Seht", "ihr", "Schwes\u00b7tern", "mich", "und", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ihr fragt und forscht und klagt:", "tokens": ["Und", "ihr", "fragt", "und", "forscht", "und", "klagt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00dft das Forschen, la\u00dft das Fragen,", "tokens": ["La\u00dft", "das", "For\u00b7schen", ",", "la\u00dft", "das", "Fra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$,", "VVIMP", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00dft das Klagen, seht mich tragen", "tokens": ["La\u00dft", "das", "Kla\u00b7gen", ",", "seht", "mich", "tra\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "$,", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Selbst mein Schicksal unverzagt.", "tokens": ["Selbst", "mein", "Schick\u00b7sal", "un\u00b7ver\u00b7zagt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hingeschwunden ist mein W\u00e4hnen,", "tokens": ["Hin\u00b7ge\u00b7schwun\u00b7den", "ist", "mein", "W\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ohne Tr\u00e4nen, ohne Sehnen", "tokens": ["Oh\u00b7ne", "Tr\u00e4\u00b7nen", ",", "oh\u00b7ne", "Seh\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "KOUI", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Welk ich meinem Grabe zu;", "tokens": ["Welk", "ich", "mei\u00b7nem", "Gra\u00b7be", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nichts dem Leben bin ich schuldig,", "tokens": ["Nichts", "dem", "Le\u00b7ben", "bin", "ich", "schul\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Stumm, geduldig, trag ich, duld ich,", "tokens": ["Stumm", ",", "ge\u00b7dul\u00b7dig", ",", "trag", "ich", ",", "duld", "ich", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "VVFIN", "PPER", "$,", "KOUS", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schon im Herzen Todesruh.", "tokens": ["Schon", "im", "Her\u00b7zen", "To\u00b7des\u00b7ruh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}