{"textgrid.poem.41855": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: H\u00e4tt' ich dich nie gesehen,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00e4tt' ich dich nie gesehen,", "tokens": ["H\u00e4tt'", "ich", "dich", "nie", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dann k\u00f6nnt' ich rasch dahin", "tokens": ["Dann", "k\u00f6nnt'", "ich", "rasch", "da\u00b7hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "PAV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch's heitre Leben gehen", "tokens": ["Durch's", "heit\u00b7re", "Le\u00b7ben", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Mit jugendlichem Sinn!", "tokens": ["Mit", "ju\u00b7gend\u00b7li\u00b7chem", "Sinn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und klagen w\u00fcrd' ich nimmer;", "tokens": ["Und", "kla\u00b7gen", "w\u00fcrd'", "ich", "nim\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O Lenz, wie ist so bald", "tokens": ["O", "Lenz", ",", "wie", "ist", "so", "bald"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PWAV", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Entflohn dein goldner Schimmer,", "tokens": ["Ent\u00b7flohn", "dein", "gold\u00b7ner", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und dein Gesang verhallt!", "tokens": ["Und", "dein", "Ge\u00b7sang", "ver\u00b7hallt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wo frische Rosen st\u00e4nden,", "tokens": ["Wo", "fri\u00b7sche", "Ro\u00b7sen", "st\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da f\u00e4nd' ich Dach und Strau\u00df,", "tokens": ["Da", "f\u00e4nd'", "ich", "Dach", "und", "Strau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn die Rosen schw\u00e4nden", "tokens": ["Und", "wenn", "die", "Ro\u00b7sen", "schw\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verlie\u00df' ich Schmuck und Haus.", "tokens": ["Ver\u00b7lie\u00df'", "ich", "Schmuck", "und", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wohl wechseln Licht und Farben,", "tokens": ["Wohl", "wech\u00b7seln", "Licht", "und", "Far\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Doch bleibt das Leben dein,", "tokens": ["Doch", "bleibt", "das", "Le\u00b7ben", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wo die Bl\u00fcthen starben,", "tokens": ["Und", "wo", "die", "Bl\u00fc\u00b7then", "star\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wird dich die Frucht erfreun.", "tokens": ["Wird", "dich", "die", "Frucht", "er\u00b7freun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Jetzt mu\u00df ich ewig weinen", "tokens": ["Jetzt", "mu\u00df", "ich", "e\u00b7wig", "wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um einen welken Kranz.", "tokens": ["Um", "ei\u00b7nen", "wel\u00b7ken", "Kranz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Frucht wird nie erscheinen,", "tokens": ["Die", "Frucht", "wird", "nie", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und ewig starb sein Glanz.", "tokens": ["Und", "e\u00b7wig", "starb", "sein", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Doch heg' ich wohl mit Freuden", "tokens": ["Doch", "heg'", "ich", "wohl", "mit", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Schmerz in stiller Brust;", "tokens": ["Den", "Schmerz", "in", "stil\u00b7ler", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und h\u00e4tt' ich mindre Leiden,", "tokens": ["Und", "h\u00e4tt'", "ich", "mind\u00b7re", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So h\u00e4tt' ich mindre Lust.", "tokens": ["So", "h\u00e4tt'", "ich", "mind\u00b7re", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wohl sinkt aus tr\u00fcben D\u00fcften", "tokens": ["Wohl", "sinkt", "aus", "tr\u00fc\u00b7ben", "D\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die D\u00e4mmrung \u00f6d' und grau;", "tokens": ["Die", "D\u00e4mm\u00b7rung", "\u00f6d'", "und", "grau", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch schwillt von s\u00fc\u00dfern D\u00fcften", "tokens": ["Doch", "schwillt", "von", "s\u00fc\u00b7\u00dfern", "D\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Bl\u00fcth' im n\u00e4cht'gen Thau.", "tokens": ["Die", "Bl\u00fcth'", "im", "n\u00e4cht'\u00b7gen", "Thau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wohl kehrt das V\u00f6glein nimmer,", "tokens": ["Wohl", "kehrt", "das", "V\u00f6\u00b7glein", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das einst sein Lied dir sang;", "tokens": ["Das", "einst", "sein", "Lied", "dir", "sang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch h\u00f6rt dein Herz noch immer", "tokens": ["Doch", "h\u00f6rt", "dein", "Herz", "noch", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den wunders\u00fc\u00dfen Klang.", "tokens": ["Den", "wun\u00b7der\u00b7s\u00fc\u00b7\u00dfen", "Klang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wer Sch\u00f6nes je empfangen,", "tokens": ["Wer", "Sch\u00f6\u00b7nes", "je", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem bleibt es ewig nah;", "tokens": ["Dem", "bleibt", "es", "e\u00b7wig", "nah", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch ewig mu\u00df verlangen,", "tokens": ["Doch", "e\u00b7wig", "mu\u00df", "ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wer nie das Sch\u00f6ne sah.", "tokens": ["Wer", "nie", "das", "Sch\u00f6\u00b7ne", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "H\u00e4tt' ich dich nie gesehen,", "tokens": ["H\u00e4tt'", "ich", "dich", "nie", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dann m\u00fc\u00dfte bald mein Herz", "tokens": ["Dann", "m\u00fc\u00df\u00b7te", "bald", "mein", "Herz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In Sehnsuchtsqual vergehen;", "tokens": ["In", "Sehn\u00b7suchts\u00b7qual", "ver\u00b7ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Jetzt lebt es durch den Schmerz.", "tokens": ["Jetzt", "lebt", "es", "durch", "den", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}