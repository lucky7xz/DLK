{"dta.poem.1123": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "9. Die Sch\u00f6ne Schielende.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Beneiderin des Gutts/ das dir doch selbst gebricht/ ", "tokens": ["Be\u00b7nei\u00b7de\u00b7rin", "des", "Gutts", "/", "das", "dir", "doch", "selbst", "ge\u00b7bricht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$(", "PRELS", "PPER", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was hat dein k\u00fchner Mund Dorinden vorzur\u00fccken?", "tokens": ["Was", "hat", "dein", "k\u00fch\u00b7ner", "Mund", "Do\u00b7rin\u00b7den", "vor\u00b7zu\u00b7r\u00fc\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du spottest/ da\u00df bey ihr der Augen doppelt Licht", "tokens": ["Du", "spot\u00b7test", "/", "da\u00df", "bey", "ihr", "der", "Au\u00b7gen", "dop\u00b7pelt", "Licht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "APPR", "PPOSAT", "ART", "NN", "ADJD", "NN"], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Mit falschen Strahlen nach der Seite pflegt zu blicken.", "tokens": ["Mit", "fal\u00b7schen", "Strah\u00b7len", "nach", "der", "Sei\u00b7te", "pflegt", "zu", "bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Warum erwehnest du der zarten Wangen nicht?", "tokens": ["Wa\u00b7rum", "er\u00b7weh\u00b7nest", "du", "der", "zar\u00b7ten", "Wan\u00b7gen", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der sch\u00f6nen Brust/ nach der wir tausend Seuffzer schicken?", "tokens": ["Der", "sch\u00f6\u00b7nen", "Brust", "/", "nach", "der", "wir", "tau\u00b7send", "Seuff\u00b7zer", "schi\u00b7cken", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "APPR", "PRELS", "PPER", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Soll dieser Fehl allein/ der dich ins Auge sticht/", "tokens": ["Soll", "die\u00b7ser", "Fehl", "al\u00b7lein", "/", "der", "dich", "ins", "Au\u00b7ge", "sticht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "NN", "ADV", "$(", "PRELS", "PRF", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Den wohlerworbnen Ruhm der Sch\u00f6nheit unterdr\u00fccken?", "tokens": ["Den", "woh\u00b7ler\u00b7worb\u00b7nen", "Ruhm", "der", "Sch\u00f6n\u00b7heit", "un\u00b7ter\u00b7dr\u00fc\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach/ solten wir zugleich das Feuer zweyer Sonnen", "tokens": ["Ach", "/", "sol\u00b7ten", "wir", "zu\u00b7gleich", "das", "Feu\u00b7er", "zwey\u00b7er", "Son\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$(", "VMFIN", "PPER", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Erdulden/ da den Mohr nur eine schw\u00e4rzt und brennt/", "tokens": ["E\u00b7rdul\u00b7den", "/", "da", "den", "Mohr", "nur", "ei\u00b7ne", "schw\u00e4rzt", "und", "brennt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADV", "ART", "NN", "ADV", "ART", "ADJD", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wir w\u00e4ren l\u00e4ngst/ wie Schnee und m\u00fcrbes Wachs zerron-", "tokens": ["Wir", "w\u00e4\u00b7ren", "l\u00e4ngst", "/", "wie", "Schnee", "und", "m\u00fcr\u00b7bes", "Wachs", "zer\u00b7ron"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "$(", "KOKOM", "NN", "KON", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "nen: ", "tokens": ["nen", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.13": {"text": "Wenn Phebus Westenwerts sein flammend Antlitz wendt/", "tokens": ["Wenn", "Phe\u00b7bus", "Wes\u00b7ten\u00b7werts", "sein", "flam\u00b7mend", "Ant\u00b7litz", "wendt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NE", "PPOSAT", "ADJD", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und gegen Morgen blinckt des Mondens Silber-Schein/", "tokens": ["Und", "ge\u00b7gen", "Mor\u00b7gen", "blinckt", "des", "Mon\u00b7dens", "Sil\u00b7ber\u00b7Schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Solln darum mangelhafft des Himmels Blicke seyn?", "tokens": ["Solln", "da\u00b7rum", "man\u00b7gel\u00b7hafft", "des", "Him\u00b7mels", "Bli\u00b7cke", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PAV", "VVFIN", "ART", "NN", "NN", "VAINF", "$."], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}}}}}