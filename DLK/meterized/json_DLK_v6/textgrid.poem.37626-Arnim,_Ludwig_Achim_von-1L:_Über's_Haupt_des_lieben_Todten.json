{"textgrid.poem.37626": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00dcber's Haupt des lieben Todten", "genre": "verse", "period": "N.A.", "pub_year": 1825, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber's Haupt des lieben Todten", "tokens": ["\u00dc\u00b7ber's", "Haupt", "des", "lie\u00b7ben", "Tod\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jagen freud'ge Siegesboten,", "tokens": ["Ja\u00b7gen", "freu\u00b7d'\u00b7ge", "Sie\u00b7ges\u00b7bo\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Seine Ahnung ist erf\u00fcllt!", "tokens": ["Sei\u00b7ne", "Ah\u00b7nung", "ist", "er\u00b7f\u00fcllt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mehr wird noch der Welt enth\u00fcllt,", "tokens": ["Mehr", "wird", "noch", "der", "Welt", "ent\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Denn die Welt verlangt noch mehr,", "tokens": ["Denn", "die", "Welt", "ver\u00b7langt", "noch", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Fr\u00e4gt nach neuer Neuigkeit,", "tokens": ["Fr\u00e4gt", "nach", "neu\u00b7er", "Neu\u00b7ig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ihr scheint morgen Kleinigkeit", "tokens": ["Ihr", "scheint", "mor\u00b7gen", "Klei\u00b7nig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Heut'ge That und heut'ge Lehr';", "tokens": ["Heut'\u00b7ge", "That", "und", "heut'\u00b7ge", "Lehr'", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Auf wohlan, die Zeit ist kommen", "tokens": ["Auf", "wo\u00b7hlan", ",", "die", "Zeit", "ist", "kom\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "$,", "ART", "NN", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und so kommt nun mit der Zeit", "tokens": ["Und", "so", "kommt", "nun", "mit", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Das, warum die Zeit ist kommen,", "tokens": ["Das", ",", "wa\u00b7rum", "die", "Zeit", "ist", "kom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWAV", "ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.12": {"text": "Aus dem Streit die Ewigkeit.", "tokens": ["Aus", "dem", "Streit", "die", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Einen schl\u00e4gt des Adlers Fl\u00fcgel,", "tokens": ["Ei\u00b7nen", "schl\u00e4gt", "des", "Ad\u00b7lers", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der sich selber schien ein Siegel,", "tokens": ["Der", "sich", "sel\u00b7ber", "schien", "ein", "Sie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein Schlu\u00df der ganzen Zeit,", "tokens": ["Und", "ein", "Schlu\u00df", "der", "gan\u00b7zen", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihn umschlingt der Zweifel heut,", "tokens": ["Ihn", "um\u00b7schlingt", "der", "Zwei\u00b7fel", "heut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihn erdr\u00fcckt der Kette Last,", "tokens": ["Ihn", "er\u00b7dr\u00fcckt", "der", "Ket\u00b7te", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die er mit gewandter Hand", "tokens": ["Die", "er", "mit", "ge\u00b7wand\u00b7ter", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hat gezogen \u00fcber's Land,", "tokens": ["Hat", "ge\u00b7zo\u00b7gen", "\u00fc\u00b7ber's", "Land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Frei sind die, die ihn geha\u00dft:", "tokens": ["Frei", "sind", "die", ",", "die", "ihn", "ge\u00b7ha\u00dft", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Auf wohlan, die Zeit wird kommen,", "tokens": ["Auf", "wo\u00b7hlan", ",", "die", "Zeit", "wird", "kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$,", "ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und so kommt denn mit der Zeit", "tokens": ["Und", "so", "kommt", "denn", "mit", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Das, warum die Zeit will kommen,", "tokens": ["Das", ",", "wa\u00b7rum", "die", "Zeit", "will", "kom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWAV", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Wenn wir alle sind bereit.", "tokens": ["Wenn", "wir", "al\u00b7le", "sind", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Schlecht Gehorchen, schlecht Regieren", "tokens": ["Schlecht", "Ge\u00b7hor\u00b7chen", ",", "schlecht", "Re\u00b7gie\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "$,", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird zu neuem Streite f\u00fchren!", "tokens": ["Wird", "zu", "neu\u00b7em", "Strei\u00b7te", "f\u00fch\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil ihr euch zu k\u00fchn verme\u00dft,", "tokens": ["Weil", "ihr", "euch", "zu", "k\u00fchn", "ver\u00b7me\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKA", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil die Einheit ihr verge\u00dft,", "tokens": ["Weil", "die", "Ein\u00b7heit", "ihr", "ver\u00b7ge\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sinket vieler Schwerter Schein,", "tokens": ["Sin\u00b7ket", "vie\u00b7ler", "Schwer\u00b7ter", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Eines gl\u00e4nzet himmelan,", "tokens": ["Ei\u00b7nes", "gl\u00e4n\u00b7zet", "him\u00b7me\u00b7lan", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der es f\u00fchrt, ", "tokens": ["Der", "es", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Wird des Friedens Herold sein:", "tokens": ["Wird", "des", "Frie\u00b7dens", "He\u00b7rold", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NE", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Auf wohlan, die Zeit wird kommen,", "tokens": ["Auf", "wo\u00b7hlan", ",", "die", "Zeit", "wird", "kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$,", "ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und so kommt dann mit der Zeit", "tokens": ["Und", "so", "kommt", "dann", "mit", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Das, warum die Zeit will kommen,", "tokens": ["Das", ",", "wa\u00b7rum", "die", "Zeit", "will", "kom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWAV", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Und aus Drei'n kommt Einigkeit.", "tokens": ["Und", "aus", "Drei'n", "kommt", "Ei\u00b7nig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Die aus sich die Welt berathen,", "tokens": ["Die", "aus", "sich", "die", "Welt", "be\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00f6rt der Held mit seinen Thaten,", "tokens": ["St\u00f6rt", "der", "Held", "mit", "sei\u00b7nen", "Tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zeiget jedem seinen Zorn,", "tokens": ["Zei\u00b7get", "je\u00b7dem", "sei\u00b7nen", "Zorn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn nur ihm geb\u00fchrt das Horn.", "tokens": ["Denn", "nur", "ihm", "ge\u00b7b\u00fchrt", "das", "Horn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Nach dem Tagwerk schl\u00e4ft der Held,", "tokens": ["Nach", "dem", "Tag\u00b7werk", "schl\u00e4ft", "der", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Selig, die sein Schlaf erschafft,", "tokens": ["Se\u00b7lig", ",", "die", "sein", "Schlaf", "er\u00b7schafft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Seiner Kinder frische Kraft", "tokens": ["Sei\u00b7ner", "Kin\u00b7der", "fri\u00b7sche", "Kraft"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Tanzt um ihn auf blut'gem Feld:", "tokens": ["Tanzt", "um", "ihn", "auf", "blut'\u00b7gem", "Feld", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Auf wohlan, die Zeit wird kommen,", "tokens": ["Auf", "wo\u00b7hlan", ",", "die", "Zeit", "wird", "kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$,", "ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und so kommt dann mit der Zeit,", "tokens": ["Und", "so", "kommt", "dann", "mit", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Das, warum die Zeit will kommen,", "tokens": ["Das", ",", "wa\u00b7rum", "die", "Zeit", "will", "kom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWAV", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Das, warum sie sich erneut.", "tokens": ["Das", ",", "wa\u00b7rum", "sie", "sich", "er\u00b7neut", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWAV", "PPER", "PRF", "ADJD", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.5": {"line.1": {"text": "Was der alte Mann getr\u00e4umet,", "tokens": ["Was", "der", "al\u00b7te", "Mann", "ge\u00b7tr\u00e4u\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem wilden Meere sch\u00e4umet,", "tokens": ["In", "dem", "wil\u00b7den", "Mee\u00b7re", "sch\u00e4u\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weht heran in Sturmes Nacht,", "tokens": ["Weht", "he\u00b7ran", "in", "Stur\u00b7mes", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Er h\u00e4lt schlafend gute Wacht.", "tokens": ["Er", "h\u00e4lt", "schla\u00b7fend", "gu\u00b7te", "Wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wie der Fischer aus dem Meer", "tokens": ["Wie", "der", "Fi\u00b7scher", "aus", "dem", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Fische zieht, die niemand sah,", "tokens": ["Fi\u00b7sche", "zieht", ",", "die", "nie\u00b7mand", "sah", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Also ist Erf\u00fcllung nah,", "tokens": ["Al\u00b7so", "ist", "Er\u00b7f\u00fcl\u00b7lung", "nah", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn die Ahnung schien so leer!", "tokens": ["Wenn", "die", "Ah\u00b7nung", "schien", "so", "leer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Auf wohlan, die Zeit ist kommen,", "tokens": ["Auf", "wo\u00b7hlan", ",", "die", "Zeit", "ist", "kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$,", "ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wo die alte Schlangenhaut", "tokens": ["Wo", "die", "al\u00b7te", "Schlan\u00b7gen\u00b7haut"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Dieser Welt ist abgenommen,", "tokens": ["Die\u00b7ser", "Welt", "ist", "ab\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Knieet nieder, schaut, vertraut.", "tokens": ["Kni\u00b7e\u00b7et", "nie\u00b7der", ",", "schaut", ",", "ver\u00b7traut", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "VVFIN", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}