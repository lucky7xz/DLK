{"dta.poem.9117": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kleine, sonnen\u00fcberstr\u00f6mte G\u00e4rten", "tokens": ["Klei\u00b7ne", ",", "son\u00b7nen\u00b7\u00fc\u00b7ber\u00b7str\u00f6m\u00b7te", "G\u00e4r\u00b7ten"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "mit bunten Lauben, K\u00fcrbissen und Schnittlauch.", "tokens": ["mit", "bun\u00b7ten", "Lau\u00b7ben", ",", "K\u00fcr\u00b7bis\u00b7sen", "und", "Schnitt\u00b7lauch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Noch blitzt der Thau.", "tokens": ["Noch", "blitzt", "der", "Thau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ueber den nahen H\u00e4userhorizont ragen Th\u00fcrme.", "tokens": ["Ue\u00b7ber", "den", "na\u00b7hen", "H\u00e4u\u00b7ser\u00b7ho\u00b7ri\u00b7zont", "ra\u00b7gen", "Th\u00fcr\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}}, "stanza.4": {"line.1": {"text": "Durch das monotone Ger\u00e4usch der Neubauten,", "tokens": ["Durch", "das", "mo\u00b7no\u00b7to\u00b7ne", "Ge\u00b7r\u00e4usch", "der", "Neu\u00b7bau\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "ab und zu,", "tokens": ["ab", "und", "zu", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "pfeifen Fabriken,", "tokens": ["pfei\u00b7fen", "Fab\u00b7ri\u00b7ken", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "schlagen Glocken an.", "tokens": ["schla\u00b7gen", "Glo\u00b7cken", "an", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Auf einer Hopfenstange sitzt ein Spatz.", "tokens": ["Auf", "ei\u00b7ner", "Hop\u00b7fen\u00b7stan\u00b7ge", "sitzt", "ein", "Spatz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ich stehe gegen einen alten Drahtzaun gelehnt", "tokens": ["Ich", "ste\u00b7he", "ge\u00b7gen", "ei\u00b7nen", "al\u00b7ten", "Draht\u00b7zaun", "ge\u00b7lehnt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "und sehe zu, wie \u00fcber einem Asternbeet", "tokens": ["und", "se\u00b7he", "zu", ",", "wie", "\u00fc\u00b7ber", "ei\u00b7nem", "As\u00b7tern\u00b7beet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "zwei Kohlweisslinge taumeln.", "tokens": ["zwei", "Kohl\u00b7weiss\u00b7lin\u00b7ge", "tau\u00b7meln", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}