{"textgrid.poem.57993": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "31. Ein jeder etwas hat", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein jeder etwas hat, worin er ist verliebet,", "tokens": ["Ein", "je\u00b7der", "et\u00b7was", "hat", ",", "wo\u00b7rin", "er", "ist", "ver\u00b7lie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PIS", "VAFIN", "$,", "PWAV", "PPER", "VAFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein jeder etwas hat, das ihm Gesellschaft giebet,", "tokens": ["Ein", "je\u00b7der", "et\u00b7was", "hat", ",", "das", "ihm", "Ge\u00b7sell\u00b7schaft", "gie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PIS", "VAFIN", "$,", "PRELS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein jeder etwas hat, das ihm verk\u00fcrzt die Zeit,", "tokens": ["Ein", "je\u00b7der", "et\u00b7was", "hat", ",", "das", "ihm", "ver\u00b7k\u00fcrzt", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PIS", "VAFIN", "$,", "PRELS", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein jeder etwas hat, worin er sich erfreut,", "tokens": ["Ein", "je\u00b7der", "et\u00b7was", "hat", ",", "wo\u00b7rin", "er", "sich", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PIS", "VAFIN", "$,", "PWAV", "PPER", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein jeder etwas hat, worauf er st\u00fctzt und trauet,", "tokens": ["Ein", "je\u00b7der", "et\u00b7was", "hat", ",", "wo\u00b7rauf", "er", "st\u00fctzt", "und", "trau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PIS", "VAFIN", "$,", "PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein jeder etwas hat, das er am liebsten schauet:", "tokens": ["Ein", "je\u00b7der", "et\u00b7was", "hat", ",", "das", "er", "am", "liebs\u00b7ten", "schau\u00b7et", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PIS", "VAFIN", "$,", "PRELS", "PPER", "PTKA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was w\u00e4hl' ich mir denn nun? O Jesu, du allein,", "tokens": ["Was", "w\u00e4hl'", "ich", "mir", "denn", "nun", "?", "O", "Je\u00b7su", ",", "du", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "ADV", "ADV", "$.", "NE", "NE", "$,", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Du sollst mein Schatz, mein Gut, mein Trost, mein Alles sein!", "tokens": ["Du", "sollst", "mein", "Schatz", ",", "mein", "Gut", ",", "mein", "Trost", ",", "mein", "Al\u00b7les", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "PIS", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}