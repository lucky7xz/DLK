{"textgrid.poem.26148": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Ein jeder hat vom Boden sich blind fortgesehnt", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Jahre haben alle Fliederb\u00e4ume am alten Hans gedehnt,", "tokens": ["Die", "Jah\u00b7re", "ha\u00b7ben", "al\u00b7le", "Flie\u00b7der\u00b7b\u00e4u\u00b7me", "am", "al\u00b7ten", "Hans", "ge\u00b7dehnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "APPRART", "ADJA", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Sie drehn sich mit verrenkten K\u00f6rpern fremd hinaus,", "tokens": ["Sie", "drehn", "sich", "mit", "ver\u00b7renk\u00b7ten", "K\u00f6r\u00b7pern", "fremd", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Garten sieht verkn\u00f6chert und vergeistert aus.", "tokens": ["Der", "Gar\u00b7ten", "sieht", "ver\u00b7kn\u00f6\u00b7chert", "und", "ver\u00b7geis\u00b7tert", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Manch kleiner Stamm, der fr\u00fcher sich an jeden Wind gelehnt,", "tokens": ["Manch", "klei\u00b7ner", "Stamm", ",", "der", "fr\u00fc\u00b7her", "sich", "an", "je\u00b7den", "Wind", "ge\u00b7lehnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "PRELS", "ADJD", "PRF", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Und dessen Gabel wir mit einem Griff gefa\u00dft,", "tokens": ["Und", "des\u00b7sen", "Ga\u00b7bel", "wir", "mit", "ei\u00b7nem", "Griff", "ge\u00b7fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verwuchs sich in die Leere ohne Rast.", "tokens": ["Ver\u00b7wuchs", "sich", "in", "die", "Lee\u00b7re", "oh\u00b7ne", "Rast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein jeder hat vom Boden sich blind fortgesehnt,", "tokens": ["Ein", "je\u00b7der", "hat", "vom", "Bo\u00b7den", "sich", "blind", "fort\u00b7ge\u00b7sehnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "APPRART", "NN", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die \u00c4ste sehn verst\u00fcrmt an uns vorbei,", "tokens": ["Die", "\u00c4s\u00b7te", "sehn", "ver\u00b7st\u00fcrmt", "an", "uns", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kaum ein paar Schattenreste fallen auf uns zwei.", "tokens": ["Kaum", "ein", "paar", "Schat\u00b7ten\u00b7res\u00b7te", "fal\u00b7len", "auf", "uns", "zwei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "PIAT", "NN", "VVFIN", "APPR", "PPER", "CARD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}