{"textgrid.poem.53744": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Dem Gehege der Z\u00e4hne", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor dem Richter stehn zwei Partein,", "tokens": ["Vor", "dem", "Rich\u00b7ter", "stehn", "zwei", "Par\u00b7tein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "CARD", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "die reden zu gleicher Zeit.", "tokens": ["die", "re\u00b7den", "zu", "glei\u00b7cher", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "PTKZU", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Man h\u00f6rt Frau Schnufke: \u00bbNa sowas!\u00ab schrein.", "tokens": ["Man", "h\u00f6rt", "Frau", "Schnuf\u00b7ke", ":", "\u00bb", "Na", "so\u00b7was", "!", "\u00ab", "schrein", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "NE", "$.", "$(", "ITJ", "ADV", "$.", "$(", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u00bbdas nehm ich glatt auf mein' Eid \u2013!\u00ab", "tokens": ["\u00bb", "das", "nehm", "ich", "glatt", "auf", "mein'", "Eid", "\u2013", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PDS", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$(", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Da sagt der Richter: \u00bbErz\u00e4hlen Sie mal:", "tokens": ["Da", "sagt", "der", "Rich\u00b7ter", ":", "\u00bb", "Er\u00b7z\u00e4h\u00b7len", "Sie", "mal", ":"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "$(", "NN", "PPER", "ADV", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wie war das am Sonntag mit Ihrem Skandal?\u00ab", "tokens": ["Wie", "war", "das", "am", "Sonn\u00b7tag", "mit", "Ih\u00b7rem", "Skan\u00b7dal", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VAFIN", "PDS", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Und Frau Schnufke erz\u00e4hlt unter Tr\u00e4nengewimmer;", "tokens": ["Und", "Frau", "Schnuf\u00b7ke", "er\u00b7z\u00e4hlt", "un\u00b7ter", "Tr\u00e4\u00b7nen\u00b7ge\u00b7wim\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NE", "VVFIN", "APPR", "NN", "$."], "meter": "-++--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "und aus allem, was sie berichtend klagt,", "tokens": ["und", "aus", "al\u00b7lem", ",", "was", "sie", "be\u00b7rich\u00b7tend", "klagt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "$,", "PRELS", "PPER", "VVPP", "VVFIN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.9": {"text": "h\u00f6rt der ganze Gerichtssaal nur immer:", "tokens": ["h\u00f6rt", "der", "gan\u00b7ze", "Ge\u00b7richts\u00b7saal", "nur", "im\u00b7mer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADV", "ADV", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "\u00bbund da hab ich gesagt . . . und da hat sie gesagt . . . und da hab ich gesagt . . . \u00ab", "tokens": ["\u00bb", "und", "da", "hab", "ich", "ge\u00b7sagt", ".", ".", ".", "und", "da", "hat", "sie", "ge\u00b7sagt", ".", ".", ".", "und", "da", "hab", "ich", "ge\u00b7sagt", ".", ".", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["$(", "KON", "ADV", "VAFIN", "PPER", "VVPP", "$.", "$.", "$.", "KON", "ADV", "VAFIN", "PPER", "VVPP", "$.", "$.", "$.", "KON", "ADV", "VAFIN", "PPER", "VVPP", "$.", "$.", "$.", "$("], "meter": "--+--+--+--+--+--+", "measure": "anapaest.tetra.plus"}}, "stanza.2": {"line.1": {"text": "Vor Deutschland stehen achtzehn Partein,", "tokens": ["Vor", "Deutschland", "ste\u00b7hen", "acht\u00b7zehn", "Par\u00b7tein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "CARD", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "die reden zu gleicher Zeit.", "tokens": ["die", "re\u00b7den", "zu", "glei\u00b7cher", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "PTKZU", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Man h\u00f6rt Herrn Jarres \u00bbRevanche!\u00ab schrein", "tokens": ["Man", "h\u00f6rt", "Herrn", "Jar\u00b7res", "\u00bb", "Re\u00b7van\u00b7che", "!", "\u00ab", "schrein"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct", "word"], "pos": ["PIS", "VVFIN", "NN", "NE", "$(", "NN", "$.", "$(", "ADJD"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "und Hergt nach der Kaiserzeit.", "tokens": ["und", "Hergt", "nach", "der", "Kai\u00b7ser\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Da sagt sich der Deutsche: \u00bbErz\u00e4hlt mir mal:", "tokens": ["Da", "sagt", "sich", "der", "Deut\u00b7sche", ":", "\u00bb", "Er\u00b7z\u00e4hlt", "mir", "mal", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$.", "$(", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Wie komm ich aus diesem Jammertal?", "tokens": ["Wie", "komm", "ich", "aus", "die\u00b7sem", "Jam\u00b7mer\u00b7tal", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Es wird doch t\u00e4glich schlimmer und schlimmer.", "tokens": ["Es", "wird", "doch", "t\u00e4g\u00b7lich", "schlim\u00b7mer", "und", "schlim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Wir sind isoliert. Bewuchert. Geplagt!\u00ab", "tokens": ["Wir", "sind", "i\u00b7so\u00b7liert", ".", "Be\u00b7wu\u00b7chert", ".", "Ge\u00b7plagt", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "VVPP", "$.", "VVPP", "$.", "$("], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.9": {"text": "Doch von den Politikern klingt es nur immer:", "tokens": ["Doch", "von", "den", "Po\u00b7li\u00b7ti\u00b7kern", "klingt", "es", "nur", "im\u00b7mer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u00bb . . . Und da hab ich gesagt . . . Und da hat er gesagt . . . Und da hat der gesagt . . . !\u00ab", "tokens": ["\u00bb", ".", ".", ".", "Und", "da", "hab", "ich", "ge\u00b7sagt", ".", ".", ".", "Und", "da", "hat", "er", "ge\u00b7sagt", ".", ".", ".", "Und", "da", "hat", "der", "ge\u00b7sagt", ".", ".", ".", "!", "\u00ab"], "token_info": ["punct", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$.", "$.", "$.", "KON", "ADV", "VAFIN", "PPER", "VVPP", "$.", "$.", "$.", "KON", "ADV", "VAFIN", "PPER", "VVPP", "$.", "$.", "$.", "KON", "ADV", "VAFIN", "ART", "VVPP", "$.", "$.", "$.", "$.", "$("], "meter": "--+--+--+--+--+--+", "measure": "anapaest.tetra.plus"}}, "stanza.3": {"line.1": {"text": "In den Landwehrkanal f\u00e4llt ein Mann hinein,", "tokens": ["In", "den", "Land\u00b7wehr\u00b7ka\u00b7nal", "f\u00e4llt", "ein", "Mann", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "der furchtbar um Hilfe schreit.", "tokens": ["der", "furcht\u00b7bar", "um", "Hil\u00b7fe", "schreit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Auf der Potsdamer Br\u00fccke stehn zwei Partein,", "tokens": ["Auf", "der", "Pots\u00b7da\u00b7mer", "Br\u00fc\u00b7cke", "stehn", "zwei", "Par\u00b7tein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "CARD", "NN", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "die reden zu gleicher Zeit.", "tokens": ["die", "re\u00b7den", "zu", "glei\u00b7cher", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "PTKZU", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sie zerfleischen sich fast in wildem Zwist:", "tokens": ["Sie", "zer\u00b7flei\u00b7schen", "sich", "fast", "in", "wil\u00b7dem", "Zwist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Ob dieser Mann ein Jude ist?", "tokens": ["Ob", "die\u00b7ser", "Mann", "ein", "Ju\u00b7de", "ist", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die einen sagen: \u00bbNich in die Hand!", "tokens": ["Die", "ei\u00b7nen", "sa\u00b7gen", ":", "\u00bb", "Nich", "in", "die", "Hand", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVINF", "$.", "$(", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Ich hab seine braven Eltern gekannt!\u00ab", "tokens": ["Ich", "hab", "sei\u00b7ne", "bra\u00b7ven", "El\u00b7tern", "ge\u00b7kannt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Die andern sagen: \u00bbSein Sie doch still!", "tokens": ["Die", "an\u00b7dern", "sa\u00b7gen", ":", "\u00bb", "Sein", "Sie", "doch", "still", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVINF", "$.", "$(", "PPOSAT", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Das h\u00f6rt man doch schon an seinem Gebr\u00fcll \u2013!\u00ab", "tokens": ["Das", "h\u00f6rt", "man", "doch", "schon", "an", "sei\u00b7nem", "Ge\u00b7br\u00fcll", "\u2013", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PDS", "VVFIN", "PIS", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$(", "$.", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Und da hab ich gesagt . . . und da hast du gesagt . . . und da hat er gesagt . . .", "tokens": ["Und", "da", "hab", "ich", "ge\u00b7sagt", ".", ".", ".", "und", "da", "hast", "du", "ge\u00b7sagt", ".", ".", ".", "und", "da", "hat", "er", "ge\u00b7sagt", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVPP", "$.", "$.", "$.", "KON", "ADV", "VAFIN", "PPER", "VVPP", "$.", "$.", "$.", "KON", "ADV", "VAFIN", "PPER", "VVPP", "$.", "$.", "$."], "meter": "--+--+--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.12": {"text": "Dann wurde die Debatte vertagt.", "tokens": ["Dann", "wur\u00b7de", "die", "De\u00b7bat\u00b7te", "ver\u00b7tagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Und dann sind alle ans Gitter geloffen.", "tokens": ["Und", "dann", "sind", "al\u00b7le", "ans", "Git\u00b7ter", "ge\u00b7lof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIS", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Mann war unterdessen versoffen.", "tokens": ["Der", "Mann", "war", "un\u00b7ter\u00b7des\u00b7sen", "ver\u00b7sof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}