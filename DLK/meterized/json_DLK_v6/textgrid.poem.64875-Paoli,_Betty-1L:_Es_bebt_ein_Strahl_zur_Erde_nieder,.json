{"textgrid.poem.64875": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es bebt ein Strahl zur Erde nieder,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es bebt ein Strahl zur Erde nieder,", "tokens": ["Es", "bebt", "ein", "Strahl", "zur", "Er\u00b7de", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vergoldet magisch Berg und Flur,", "tokens": ["Ver\u00b7gol\u00b7det", "ma\u00b7gisch", "Berg", "und", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und kehrt dann heim zur Sonne wieder,", "tokens": ["Und", "kehrt", "dann", "heim", "zur", "Son\u00b7ne", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKVZ", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und l\u00e4\u00dft auf Erden keine Spur.", "tokens": ["Und", "l\u00e4\u00dft", "auf", "Er\u00b7den", "kei\u00b7ne", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Aus Blumenkelchen str\u00f6men D\u00fcfte;", "tokens": ["Aus", "Blu\u00b7men\u00b7kel\u00b7chen", "str\u00f6\u00b7men", "D\u00fcf\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch schon nach kurzem Augenblick", "tokens": ["Doch", "schon", "nach", "kur\u00b7zem", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Entschweben sie, ein Spiel der L\u00fcfte,", "tokens": ["Ent\u00b7schwe\u00b7ben", "sie", ",", "ein", "Spiel", "der", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und lassen keine Spur zur\u00fcck.", "tokens": ["Und", "las\u00b7sen", "kei\u00b7ne", "Spur", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn Gluten den Demant durchhitzen,", "tokens": ["Wenn", "Glu\u00b7ten", "den", "De\u00b7mant", "durch\u00b7hit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verfl\u00fcchtigt schnell sich seine Spur,", "tokens": ["Ver\u00b7fl\u00fcch\u00b7tigt", "schnell", "sich", "sei\u00b7ne", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nimmer bleibt von seinen Blitzen", "tokens": ["Und", "nim\u00b7mer", "bleibt", "von", "sei\u00b7nen", "Blit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zur\u00fcck die todte Kohle nur.", "tokens": ["Zu\u00b7r\u00fcck", "die", "tod\u00b7te", "Koh\u00b7le", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O seid der stillen Weisheit Erben!", "tokens": ["O", "seid", "der", "stil\u00b7len", "Weis\u00b7heit", "Er\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn Lieb' aus eurem Busen schwand,", "tokens": ["Wenn", "Lieb'", "aus", "eu\u00b7rem", "Bu\u00b7sen", "schwand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So mag sie ganz und spurlos sterben,", "tokens": ["So", "mag", "sie", "ganz", "und", "spur\u00b7los", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Strahl und Duft und Diamant!", "tokens": ["Wie", "Strahl", "und", "Duft", "und", "Di\u00b7a\u00b7mant", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wohl nahm sie schon zu h\u00f6herm Reiche,", "tokens": ["Wohl", "nahm", "sie", "schon", "zu", "h\u00f6\u00b7herm", "Rei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKA", "ADJD", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Euch unbewu\u00dft, den k\u00fchnen Schwung;", "tokens": ["Euch", "un\u00b7be\u00b7wu\u00dft", ",", "den", "k\u00fch\u00b7nen", "Schwung", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zwingt nicht in's Sein zur\u00fcck die Leiche \u2013", "tokens": ["Zwingt", "nicht", "in's", "Sein", "zu\u00b7r\u00fcck", "die", "Lei\u00b7che", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPRART", "NN", "PTKVZ", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hinweg mit dir, Erinnerung!", "tokens": ["Hin\u00b7weg", "mit", "dir", ",", "E\u00b7rin\u00b7ne\u00b7rung", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$,", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}}}}