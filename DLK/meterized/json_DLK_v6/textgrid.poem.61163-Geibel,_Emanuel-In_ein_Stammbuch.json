{"textgrid.poem.61163": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "In ein Stammbuch", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn sich auf dieses Blatt dein Auge senkt,", "tokens": ["Wenn", "sich", "auf", "die\u00b7ses", "Blatt", "dein", "Au\u00b7ge", "senkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "APPR", "PDAT", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Betracht' es still, als w\u00e4r's mein Leichenstein;", "tokens": ["Be\u00b7tracht'", "es", "still", ",", "als", "w\u00e4r's", "mein", "Lei\u00b7chen\u00b7stein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "KOKOM", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und mild, wie man der Toten sonst gedenkt,", "tokens": ["Und", "mild", ",", "wie", "man", "der", "To\u00b7ten", "sonst", "ge\u00b7denkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "PIS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gedenke mein!", "tokens": ["Ge\u00b7den\u00b7ke", "mein", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPOSAT", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}