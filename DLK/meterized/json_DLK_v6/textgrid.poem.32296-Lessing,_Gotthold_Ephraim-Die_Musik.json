{"textgrid.poem.32296": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Die Musik", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Orpheus spielte; rings um ihn,", "tokens": ["Ein", "Or\u00b7pheus", "spiel\u00b7te", ";", "rings", "um", "ihn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$.", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit lauschendem Gedr\u00e4nge,", "tokens": ["Mit", "lau\u00b7schen\u00b7dem", "Ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Stand die erstaunte Menge,", "tokens": ["Stand", "die", "er\u00b7staun\u00b7te", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durchs Ohr die Wollust einzuziehn.", "tokens": ["Durchs", "Ohr", "die", "Wol\u00b7lust", "ein\u00b7zu\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Trinker kam von ungef\u00e4hr,", "tokens": ["Ein", "Trin\u00b7ker", "kam", "von", "un\u00b7ge\u00b7f\u00e4hr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und taumelte den Weg daher.", "tokens": ["Und", "tau\u00b7mel\u00b7te", "den", "Weg", "da\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Schnell fa\u00dft' er sich, blieb horchend stehn,", "tokens": ["Schnell", "fa\u00dft'", "er", "sich", ",", "blieb", "hor\u00b7chend", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "$,", "VVFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und ward entz\u00fcckt, und schriee: sch\u00f6n!", "tokens": ["Und", "ward", "ent\u00b7z\u00fcckt", ",", "und", "schri\u00b7ee", ":", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "$,", "KON", "VVFIN", "$.", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "So sch\u00f6n, als wenn bei meinem wackern Wirte", "tokens": ["So", "sch\u00f6n", ",", "als", "wenn", "bei", "mei\u00b7nem", "wa\u00b7ckern", "Wir\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KOKOM", "KOUS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Das helle Pa\u00dfglas klirrte!", "tokens": ["Das", "hel\u00b7le", "Pa\u00df\u00b7glas", "klirr\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}