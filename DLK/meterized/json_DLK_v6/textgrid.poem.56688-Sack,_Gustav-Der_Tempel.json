{"textgrid.poem.56688": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Der Tempel", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da sprang er fluchend aus dem Grab,", "tokens": ["Da", "sprang", "er", "flu\u00b7chend", "aus", "dem", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "in das er gr\u00fcbelnd sich verloren,", "tokens": ["in", "das", "er", "gr\u00fc\u00b7belnd", "sich", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "und suchte Markt und Stra\u00dfen ab,", "tokens": ["und", "such\u00b7te", "Markt", "und", "Stra\u00b7\u00dfen", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "um seine Unzucht auszuschmoren.", "tokens": ["um", "sei\u00b7ne", "Un\u00b7zucht", "aus\u00b7zu\u00b7schmo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Hei! wie die wackre Dirne brennt!", "tokens": ["Hei", "!", "wie", "die", "wack\u00b7re", "Dir\u00b7ne", "brennt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Doch in dem roten Lotterbette", "tokens": ["Doch", "in", "dem", "ro\u00b7ten", "Lot\u00b7ter\u00b7bet\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "steigt schon der Ekel hoch und rennt", "tokens": ["steigt", "schon", "der", "E\u00b7kel", "hoch", "und", "rennt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADJD", "KON", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "mit seinen Freuden um die Wette.", "tokens": ["mit", "sei\u00b7nen", "Freu\u00b7den", "um", "die", "Wet\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und doch \u2013 ein Mensch; und still umspannt", "tokens": ["Und", "doch", "\u2013", "ein", "Mensch", ";", "und", "still", "um\u00b7spannt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "$(", "ART", "NN", "$.", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "des Zweiseins wundersame Ruhe", "tokens": ["des", "Zwei\u00b7seins", "wun\u00b7der\u00b7sa\u00b7me", "Ru\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "wie eine hohe Tempelwand", "tokens": ["wie", "ei\u00b7ne", "ho\u00b7he", "Tem\u00b7pel\u00b7wand"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "die dampfumh\u00fcllte Unzuchtstruhe.", "tokens": ["die", "damp\u00b7fum\u00b7h\u00fcll\u00b7te", "Un\u00b7zuchts\u00b7tru\u00b7he", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}