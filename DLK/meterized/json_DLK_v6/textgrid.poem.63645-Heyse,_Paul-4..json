{"textgrid.poem.63645": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heut nacht kam das Gebet mir in den Sinn,", "tokens": ["Heut", "nacht", "kam", "das", "Ge\u00b7bet", "mir", "in", "den", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "ART", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit dem als Kind ich stets mich schlafen legte,", "tokens": ["Mit", "dem", "als", "Kind", "ich", "stets", "mich", "schla\u00b7fen", "leg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "KOUS", "NN", "PPER", "VVFIN", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wie die Lippe sich von selbst bewegte,", "tokens": ["Und", "wie", "die", "Lip\u00b7pe", "sich", "von", "selbst", "be\u00b7weg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PRF", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sagt' ich das \u00bbVaterunser\u00ab vor mich hin.", "tokens": ["Sagt'", "ich", "das", "\u00bb", "Va\u00b7ter\u00b7un\u00b7ser", "\u00ab", "vor", "mich", "hin", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "$(", "NN", "$(", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch weil ich l\u00e4ngst entw\u00f6hnt des Wahnes bin,", "tokens": ["Doch", "weil", "ich", "l\u00e4ngst", "ent\u00b7w\u00f6hnt", "des", "Wah\u00b7nes", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df v\u00e4terlich des Lebens Herr mich hegte,", "tokens": ["Da\u00df", "v\u00e4\u00b7ter\u00b7lich", "des", "Le\u00b7bens", "Herr", "mich", "heg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Geschah's, da\u00df der Gedank' in mir sich regte:", "tokens": ["Ge\u00b7scha\u00b7h's", ",", "da\u00df", "der", "Ge\u00b7dank'", "in", "mir", "sich", "reg\u00b7te", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "ART", "NN", "APPR", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Wie gut, da\u00df ich ein Kind des Todes bin!", "tokens": ["Wie", "gut", ",", "da\u00df", "ich", "ein", "Kind", "des", "To\u00b7des", "bin", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "KOUS", "PPER", "ART", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So betet' ich zu ihm: Gescheh' dein Wille! \u2013", "tokens": ["So", "be\u00b7tet'", "ich", "zu", "ihm", ":", "Ge\u00b7scheh'", "dein", "Wil\u00b7le", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "$.", "NN", "PPOSAT", "NN", "$.", "$("], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Gib mir mein t\u00e4glich Brot an Sorg' und M\u00fche! \u2013", "tokens": ["Gib", "mir", "mein", "t\u00e4g\u00b7lich", "Brot", "an", "Sor\u00b7g'", "und", "M\u00fc\u00b7he", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "ADJD", "NN", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "---+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Versuche ", "tokens": ["Ver\u00b7su\u00b7che"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Und lag in unaussprechlichem Gegr\u00fcbel,", "tokens": ["Und", "lag", "in", "un\u00b7aus\u00b7sprech\u00b7li\u00b7chem", "Ge\u00b7gr\u00fc\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bis ich aufd\u00e4mmern sah die erste Fr\u00fche,", "tokens": ["Bis", "ich", "auf\u00b7d\u00e4m\u00b7mern", "sah", "die", "ers\u00b7te", "Fr\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da schlo\u00df ich fromm: Erl\u00f6s' uns von dem \u00dcbel!", "tokens": ["Da", "schlo\u00df", "ich", "fromm", ":", "Er\u00b7l\u00f6s'", "uns", "von", "dem", "\u00dc\u00b7bel", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$.", "VVIMP", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}