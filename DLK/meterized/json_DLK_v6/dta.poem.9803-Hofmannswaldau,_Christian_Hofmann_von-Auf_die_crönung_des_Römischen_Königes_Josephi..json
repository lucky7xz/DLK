{"dta.poem.9803": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf die cr\u00f6nung des R\u00f6mischen K\u00f6niges  \n Josephi.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auff! auff bereite dich du gantzes rund der erden/", "tokens": ["Auff", "!", "auff", "be\u00b7rei\u00b7te", "dich", "du", "gant\u00b7zes", "rund", "der", "er\u00b7den", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "APPR", "VVFIN", "PPER", "PPER", "ADJA", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom auf- zum niedergang/ von s\u00fcd bi\u00df mitternacht/", "tokens": ["Vom", "auf", "zum", "nie\u00b7der\u00b7gang", "/", "von", "s\u00fcd", "bi\u00df", "mit\u00b7ter\u00b7nacht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "TRUNC", "APPRART", "NN", "$(", "APPR", "NE", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "La\u00df unsern K\u00f6nige/ la\u00df unsern Joseph werden/", "tokens": ["La\u00df", "un\u00b7sern", "K\u00f6\u00b7ni\u00b7ge", "/", "la\u00df", "un\u00b7sern", "Jo\u00b7se\u00b7ph", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "$(", "VVIMP", "PPOSAT", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Was ihn zwar gr\u00f6sser nicht/ doch ausgezierter macht.", "tokens": ["Was", "ihn", "zwar", "gr\u00f6s\u00b7ser", "nicht", "/", "doch", "aus\u00b7ge\u00b7zier\u00b7ter", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "PTKNEG", "$(", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Gib/ fernes Peru/ gold aus deinen reichen kl\u00fcfften/", "tokens": ["Gib", "/", "fer\u00b7nes", "Pe\u00b7ru", "/", "gold", "aus", "dei\u00b7nen", "rei\u00b7chen", "kl\u00fcff\u00b7ten", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$(", "ADJA", "NN", "$(", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die GOtt und gl\u00fcck geschenckt dem stamm von Oesterreich;", "tokens": ["Die", "Gott", "und", "gl\u00fcck", "ge\u00b7schenckt", "dem", "stamm", "von", "O\u00b7es\u00b7ter\u00b7reich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJD", "VVPP", "ART", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.7": {"text": "Daraus vor Josephs haupt die crone selbst zu stifften/", "tokens": ["Da\u00b7raus", "vor", "Jo\u00b7se\u00b7phs", "haupt", "die", "cro\u00b7ne", "selbst", "zu", "stiff\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "NE", "NN", "ART", "NN", "ADV", "PTKZU", "VVINF", "$("], "meter": "---+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Der auff der gantzen welt ist keiner crone gleich.", "tokens": ["Der", "auff", "der", "gant\u00b7zen", "welt", "ist", "kei\u00b7ner", "cro\u00b7ne", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VAFIN", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du fremdes Decan must ihm diamanten schicken/", "tokens": ["Du", "frem\u00b7des", "De\u00b7can", "must", "ihm", "di\u00b7a\u00b7man\u00b7ten", "schi\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VMFIN", "PPER", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "An deren werth nicht reicht/ Florentz/ dein wunder-stein/", "tokens": ["An", "de\u00b7ren", "werth", "nicht", "reicht", "/", "Flo\u00b7rentz", "/", "dein", "wun\u00b7der\u00b7stein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "ADJD", "PTKNEG", "VVFIN", "$(", "NE", "$(", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Damit der cronen gold so herrlich auszuschm\u00fccken/", "tokens": ["Da\u00b7mit", "der", "cro\u00b7nen", "gold", "so", "herr\u00b7lich", "aus\u00b7zu\u00b7schm\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df selbst der sonnen glantz nicht gl\u00e4ntzender kan seyn.", "tokens": ["Da\u00df", "selbst", "der", "son\u00b7nen", "glantz", "nicht", "gl\u00e4nt\u00b7zen\u00b7der", "kan", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "PTKNEG", "ADV", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Verbrandtes morgenland treib an die braune Mohren/", "tokens": ["Ver\u00b7brand\u00b7tes", "mor\u00b7gen\u00b7land", "treib", "an", "die", "brau\u00b7ne", "Moh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "La\u00df weisse perlen hol\u2019n aus deiner gr\u00fcnen fluth/", "tokens": ["La\u00df", "weis\u00b7se", "per\u00b7len", "hol'n", "aus", "dei\u00b7ner", "gr\u00fc\u00b7nen", "fluth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Zu dieses k\u00f6nigs schmuck hat die natur erkohren/", "tokens": ["Zu", "die\u00b7ses", "k\u00f6\u00b7nigs", "schmuck", "hat", "die", "na\u00b7tur", "er\u00b7koh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VAFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Was berg und wellen f\u00fchrn als ihr kostbarstes gut.", "tokens": ["Was", "berg", "und", "wel\u00b7len", "f\u00fchrn", "als", "ihr", "kost\u00b7bars\u00b7tes", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "VVINF", "VVFIN", "KOKOM", "PPOSAT", "ADJA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "La\u00df weites Mexico dein coxenil ersetzen/", "tokens": ["La\u00df", "wei\u00b7tes", "Me\u00b7xi\u00b7co", "dein", "co\u00b7xe\u00b7nil", "er\u00b7set\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NE", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wann Tyrus purpur nicht den mantel r\u00f6thet an;", "tokens": ["Wann", "Ty\u00b7rus", "pur\u00b7pur", "nicht", "den", "man\u00b7tel", "r\u00f6\u00b7thet", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ADJD", "PTKNEG", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Doch nein/ wir wollen ihn mit eignem blute netzen/", "tokens": ["Doch", "nein", "/", "wir", "wol\u00b7len", "ihn", "mit", "eig\u00b7nem", "blu\u00b7te", "net\u00b7zen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$(", "PPER", "VMFIN", "PPER", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Weil di\u00df die teutsche treu noch mehr beweisen kan.", "tokens": ["Weil", "di\u00df", "die", "teut\u00b7sche", "treu", "noch", "mehr", "be\u00b7wei\u00b7sen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ART", "ADJA", "ADJD", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ceilon beut helffenbein/ das rothe meer corallen/", "tokens": ["Cei\u00b7lon", "beut", "helf\u00b7fen\u00b7bein", "/", "das", "ro\u00b7the", "meer", "co\u00b7ral\u00b7len", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$(", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.22": {"text": "Den marmor Welschland an zu seines thrones stand;", "tokens": ["Den", "mar\u00b7mor", "Wel\u00b7schland", "an", "zu", "sei\u00b7nes", "thro\u00b7nes", "stand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "APPR", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Doch sind ihm mehr gerecht die Zemblischen crystallen/", "tokens": ["Doch", "sind", "ihm", "mehr", "ge\u00b7recht", "die", "Zem\u00b7bli\u00b7schen", "crys\u00b7tal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADJD", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die zwar nur ei\u00df/ doch ei\u00df das ewig wird genannt.", "tokens": ["Die", "zwar", "nur", "ei\u00df", "/", "doch", "ei\u00df", "das", "e\u00b7wig", "wird", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "$(", "ADV", "ADJD", "ART", "ADJD", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wolan dann/ wann er gl\u00e4ntzt in k\u00f6niglichem wesen/", "tokens": ["Wo\u00b7lan", "dann", "/", "wann", "er", "gl\u00e4ntzt", "in", "k\u00f6\u00b7nig\u00b7li\u00b7chem", "we\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "PWAV", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Das selbst nachs H\u00f6chsten wort was g\u00f6ttlichs in sich h\u00e4lt/", "tokens": ["Das", "selbst", "nachs", "H\u00f6chs\u00b7ten", "wort", "was", "g\u00f6tt\u00b7lichs", "in", "sich", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPRART", "ADJA", "NN", "PWS", "PIS", "APPR", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "So soll ihm unser hertz zum altar sein erlesen/", "tokens": ["So", "soll", "ihm", "un\u00b7ser", "hertz", "zum", "al\u00b7tar", "sein", "er\u00b7le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "APPRART", "NN", "PPOSAT", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Vor dem getreue brunst auff ihre knie f\u00e4llt.", "tokens": ["Vor", "dem", "ge\u00b7treu\u00b7e", "brunst", "auff", "ih\u00b7re", "knie", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.29": {"text": "Wir wollen vor sein heyl unl\u00f6schbar feuer weihen/", "tokens": ["Wir", "wol\u00b7len", "vor", "sein", "heyl", "un\u00b7l\u00f6schbar", "feu\u00b7er", "wei\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "ADJD", "ADJD", "VVINF", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Das sch\u00f6nre kohlen n\u00e4hrt als selbst der sternen licht/", "tokens": ["Das", "sch\u00f6n\u00b7re", "koh\u00b7len", "n\u00e4hrt", "als", "selbst", "der", "ster\u00b7nen", "licht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KOKOM", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "In dieses wollen wir so h\u00e4uffig weyrauch streuen/", "tokens": ["In", "die\u00b7ses", "wol\u00b7len", "wir", "so", "h\u00e4uf\u00b7fig", "wey\u00b7rauch", "streu\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VMFIN", "PPER", "ADV", "ADJD", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Da\u00df kaum der sonnen glantz durch seine wolcken bricht.", "tokens": ["Da\u00df", "kaum", "der", "son\u00b7nen", "glantz", "durch", "sei\u00b7ne", "wol\u00b7cken", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Du bist gl\u00fcckseeliges Arabien erkohrn/", "tokens": ["Du", "bist", "gl\u00fcck\u00b7see\u00b7li\u00b7ges", "A\u00b7ra\u00b7bi\u00b7en", "er\u00b7kohrn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "VVINF", "$("], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.34": {"text": "Da\u00df dieser grosse f\u00fcrst solch rauchwerck von dir nimt.", "tokens": ["Da\u00df", "die\u00b7ser", "gros\u00b7se", "f\u00fcrst", "solch", "rauch\u00b7werck", "von", "dir", "nimt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "ADV", "PIAT", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Denn weil das gl\u00fcck sich selbst zu seinem heyl verschworen/", "tokens": ["Denn", "weil", "das", "gl\u00fcck", "sich", "selbst", "zu", "sei\u00b7nem", "heyl", "ver\u00b7schwo\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PRF", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Mu\u00df auch was gl\u00fccklichs seyn das ihm zum opffer glimmt.", "tokens": ["Mu\u00df", "auch", "was", "gl\u00fcck\u00b7lichs", "seyn", "das", "ihm", "zum", "opf\u00b7fer", "glimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PWS", "PIS", "VAINF", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Wir w\u00fcnschen nur inde\u00df mit niedrigen geberden/", "tokens": ["Wir", "w\u00fcn\u00b7schen", "nur", "in\u00b7de\u00df", "mit", "nied\u00b7ri\u00b7gen", "ge\u00b7ber\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Da\u00df unser teutscher held sey seinen Teutschen hold/", "tokens": ["Da\u00df", "un\u00b7ser", "teut\u00b7scher", "held", "sey", "sei\u00b7nen", "Teut\u00b7schen", "hold", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "So liebet unser geist/ so ehren wir auff erden", "tokens": ["So", "lie\u00b7bet", "un\u00b7ser", "geist", "/", "so", "eh\u00b7ren", "wir", "auff", "er\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$(", "ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.40": {"text": "Nichts eifriger als ihn/ nechst GOtt und Leopold.", "tokens": ["Nichts", "eif\u00b7ri\u00b7ger", "als", "ihn", "/", "nechst", "Gott", "und", "Leo\u00b7pold", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "KOKOM", "PPER", "$(", "VVFIN", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}