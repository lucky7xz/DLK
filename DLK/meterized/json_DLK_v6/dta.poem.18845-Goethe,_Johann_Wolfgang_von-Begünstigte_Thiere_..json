{"dta.poem.18845": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Beg\u00fcnstigte Thiere .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Vier Thieren auch verheissen war", "tokens": ["Vier", "Thie\u00b7ren", "auch", "ver\u00b7heis\u00b7sen", "war"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "ADV", "VVPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In\u2019s Paradies zu kommen,", "tokens": ["In's", "Pa\u00b7ra\u00b7dies", "zu", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dort leben sie das ew\u2019ge Jahr", "tokens": ["Dort", "le\u00b7ben", "sie", "das", "ew'\u00b7ge", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Heiligen und Frommen.", "tokens": ["Mit", "Hei\u00b7li\u00b7gen", "und", "From\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Den Vortritt hier ein Esel hat,", "tokens": ["Den", "Vor\u00b7tritt", "hier", "ein", "E\u00b7sel", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er kommt mit muntern Schritten:", "tokens": ["Er", "kommt", "mit", "mun\u00b7tern", "Schrit\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn Jesus zur Propheten-Stadt", "tokens": ["Denn", "Je\u00b7sus", "zur", "Pro\u00b7phe\u00b7ten\u00b7Stadt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NE", "APPRART", "NN"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auf ihm ist eingeritten.", "tokens": ["Auf", "ihm", "ist", "ein\u00b7ge\u00b7rit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Halb sch\u00fcchtern kommt ein Wolf sodann,", "tokens": ["Halb", "sch\u00fcch\u00b7tern", "kommt", "ein", "Wolf", "so\u00b7dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VVFIN", "ART", "NE", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Mahomet befohlen:", "tokens": ["Dem", "Ma\u00b7ho\u00b7met", "be\u00b7foh\u00b7len", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Lass dieses Schaf dem armen Mann,", "tokens": ["Lass", "die\u00b7ses", "Schaf", "dem", "ar\u00b7men", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Reichen magst du\u2019s holen.", "tokens": ["Dem", "Rei\u00b7chen", "magst", "du's", "ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nun immer wedelnd, munter, brav,", "tokens": ["Nun", "im\u00b7mer", "we\u00b7delnd", ",", "mun\u00b7ter", ",", "brav", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit seinem Herrn, dem braven,", "tokens": ["Mit", "sei\u00b7nem", "Herrn", ",", "dem", "bra\u00b7ven", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das H\u00fcndlein das den Siebenschlaf", "tokens": ["Das", "H\u00fcnd\u00b7lein", "das", "den", "Sie\u00b7ben\u00b7schlaf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So treulich mitgeschlafen.", "tokens": ["So", "treu\u00b7lich", "mit\u00b7ge\u00b7schla\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Abuherrira\u2019s Katze hier", "tokens": ["Ab\u00b7u\u00b7herr\u00b7i\u00b7ra's", "Kat\u00b7ze", "hier"], "token_info": ["word", "word", "word"], "pos": ["NE", "NN", "ADV"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Knurrt um den Herrn und schmeichelt:", "tokens": ["Knurrt", "um", "den", "Herrn", "und", "schmei\u00b7chelt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn immer ist\u2019s ein heilig Thier", "tokens": ["Denn", "im\u00b7mer", "ist's", "ein", "hei\u00b7lig", "Thier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das der Prophet gestreichelt.", "tokens": ["Das", "der", "Pro\u00b7phet", "ge\u00b7strei\u00b7chelt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}