{"textgrid.poem.65898": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Das Drama", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verf\u00fchrerin, o Sonne, dein Gestrahle", "tokens": ["Ver\u00b7f\u00fch\u00b7re\u00b7rin", ",", "o", "Son\u00b7ne", ",", "dein", "Ge\u00b7strah\u00b7le"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "FM", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Beblendet Kore, k\u00fchn im Feld;", "tokens": ["Be\u00b7blen\u00b7det", "Ko\u00b7re", ",", "k\u00fchn", "im", "Feld", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$,", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An schw\u00fclem Tag, eine noch hei\u00dfre Welt", "tokens": ["An", "schw\u00fc\u00b7lem", "Tag", ",", "ei\u00b7ne", "noch", "hei\u00df\u00b7re", "Welt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ART", "ADV", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Umfa\u00dft das Kind vieltausendmale.", "tokens": ["Um\u00b7fa\u00dft", "das", "Kind", "viel\u00b7tau\u00b7send\u00b7ma\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Stern, der herzt: der Ku\u00df durch Blitz zu Tale", "tokens": ["Ein", "Stern", ",", "der", "herzt", ":", "der", "Ku\u00df", "durch", "Blitz", "zu", "Ta\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "VVPP", "$.", "ART", "NN", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hat Kore \u00fcbermannt \u2013 dem Gotte f\u00e4llt", "tokens": ["Hat", "Ko\u00b7re", "\u00fc\u00b7berm\u00b7annt", "\u2013", "dem", "Got\u00b7te", "f\u00e4llt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "NN", "VVPP", "$(", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Jungfrau in den Arm; sein Lachen gellt", "tokens": ["Die", "Jung\u00b7frau", "in", "den", "Arm", ";", "sein", "La\u00b7chen", "gellt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$.", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie Blut aufs Land, das rasch dem Blick zerfahle!", "tokens": ["Wie", "Blut", "aufs", "Land", ",", "das", "rasch", "dem", "Blick", "zer\u00b7fah\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "$,", "PRELS", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie ein Delphin am Strand, bleibt die Geraubte", "tokens": ["Wie", "ein", "Del\u00b7phin", "am", "Strand", ",", "bleibt", "die", "Ge\u00b7raub\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wach hingestreckt im Hadeswagen \u2013 ach!", "tokens": ["Wach", "hin\u00b7ge\u00b7streckt", "im", "Ha\u00b7des\u00b7wa\u00b7gen", "\u2013", "ach", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$(", "ITJ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie f\u00fchlt zu schwer die Glut im Gotteshaupte:", "tokens": ["Sie", "f\u00fchlt", "zu", "schwer", "die", "Glut", "im", "Got\u00b7tes\u00b7haup\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKA", "ADJD", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Noch ist f\u00fcr Liebeswucht das M\u00e4dchen schwach", "tokens": ["Noch", "ist", "f\u00fcr", "Lie\u00b7bes\u00b7wucht", "das", "M\u00e4d\u00b7chen", "schwach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und schreit \u2013 blickt auf. Der Rappen Brand entschnaubte,", "tokens": ["Und", "schreit", "\u2013", "blickt", "auf", ".", "Der", "Rap\u00b7pen", "Brand", "ent\u00b7schnaub\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$(", "VVFIN", "PTKVZ", "$.", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nun h\u00e4lt sie Hades' Brunst im Glastgemach.", "tokens": ["Nun", "h\u00e4lt", "sie", "Ha\u00b7des'", "Brunst", "im", "Glast\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NE", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}