{"dta.poem.21630": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Allvaters Anrufung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Der Du einst im Waldesrauschen               ", "tokens": ["Der", "Du", "einst", "im", "Wal\u00b7des\u00b7rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deinem Volke Dich genaht,", "tokens": ["Dei\u00b7nem", "Vol\u00b7ke", "Dich", "ge\u00b7naht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df sein Herz in br\u00fcnst\u2019gem Lauschen", "tokens": ["Da\u00df", "sein", "Herz", "in", "br\u00fcnst'\u00b7gem", "Lau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "sich entz\u00fcndete zur That,", "tokens": ["sich", "ent\u00b7z\u00fcn\u00b7de\u00b7te", "zur", "That", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der Du standst an Deutschlands Seite", "tokens": ["Der", "Du", "standst", "an", "Deutschlands", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "APPR", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "immerdar und allerorts,", "tokens": ["im\u00b7mer\u00b7dar", "und", "al\u00b7le\u00b7rorts", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Kraft-Verleiher warst im Streite,", "tokens": ["Kraft\u00b7Ver\u00b7lei\u00b7her", "warst", "im", "Strei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Spender tiefen Weisheits-Worts,", "tokens": ["Spen\u00b7der", "tie\u00b7fen", "Weis\u00b7heits\u00b7Worts", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Wir, von Deinem Blut geboren,", "tokens": ["Wir", ",", "von", "Dei\u00b7nem", "Blut", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Gott der Deutschen, nahen Dir,", "tokens": ["Gott", "der", "Deut\u00b7schen", ",", "na\u00b7hen", "Dir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJA", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Wir in fremdem Volk verloren,", "tokens": ["Wir", "in", "frem\u00b7dem", "Volk", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Dich Allvater, rufen wir,", "tokens": ["Dich", "All\u00b7va\u00b7ter", ",", "ru\u00b7fen", "wir", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Hast es manchesmal gesehen", "tokens": ["Hast", "es", "man\u00b7ches\u00b7mal", "ge\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "jenes Schauspiel voller Gram,", "tokens": ["je\u00b7nes", "Schau\u00b7spiel", "vol\u00b7ler", "Gram", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Sahst aus Deutschland Deutsche gehen,", "tokens": ["Sahst", "aus", "Deutschland", "Deut\u00b7sche", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "deren keiner wiederkam.", "tokens": ["de\u00b7ren", "kei\u00b7ner", "wie\u00b7der\u00b7kam", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Die in Angst vor fremden Sp\u00f6ttern", "tokens": ["Die", "in", "Angst", "vor", "frem\u00b7den", "Sp\u00f6t\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "sich des Vaterlands gesch\u00e4mt,", "tokens": ["sich", "des", "Va\u00b7ter\u00b7lands", "ge\u00b7sch\u00e4mt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Opfer brachten fremden G\u00f6ttern,", "tokens": ["Op\u00b7fer", "brach\u00b7ten", "frem\u00b7den", "G\u00f6t\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "sich mit fremdem Kleid verbr\u00e4mt;", "tokens": ["sich", "mit", "frem\u00b7dem", "Kleid", "ver\u00b7br\u00e4mt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "H\u00f6r\u2019 uns rufen, h\u00f6r\u2019 uns schw\u00f6ren,", "tokens": ["H\u00f6r'", "uns", "ru\u00b7fen", ",", "h\u00f6r'", "uns", "schw\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "wir sind treu und wir sind Dein,", "tokens": ["wir", "sind", "treu", "und", "wir", "sind", "Dein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "PPER", "VAFIN", "PPOSAT", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Unser Land soll uns geh\u00f6ren,", "tokens": ["Un\u00b7ser", "Land", "soll", "uns", "ge\u00b7h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "unsres Landes woll\u2019n wir sein!", "tokens": ["uns\u00b7res", "Lan\u00b7des", "woll'n", "wir", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Sieh, der Fremdling will\u2019s verhindern,", "tokens": ["Sieh", ",", "der", "Fremd\u00b7ling", "will's", "ver\u00b7hin\u00b7dern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "altes Recht er schreibt es neu \u2014", "tokens": ["al\u00b7tes", "Recht", "er", "schreibt", "es", "neu"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VVFIN", "PPER", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Vater bleibe Deinen Kindern,", "tokens": ["Va\u00b7ter", "blei\u00b7be", "Dei\u00b7nen", "Kin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Gott der Deutschen, bleib\u2019 uns treu!", "tokens": ["Gott", "der", "Deut\u00b7schen", ",", "bleib'", "uns", "treu", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Sch\u00fcttle Deine heil\u2019gen Locken,", "tokens": ["Sch\u00fctt\u00b7le", "Dei\u00b7ne", "heil'\u00b7gen", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "wecke die allm\u00e4cht\u2019ge Hand,", "tokens": ["we\u00b7cke", "die", "all\u00b7m\u00e4cht'\u00b7ge", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Da\u00df der Eindringling erschrocken", "tokens": ["Da\u00df", "der", "Ein\u00b7dring\u00b7ling", "er\u00b7schro\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.32": {"text": "weiche aus dem Deutschen Land,", "tokens": ["wei\u00b7che", "aus", "dem", "Deut\u00b7schen", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Da\u00df er zagen lerne, zittern", "tokens": ["Da\u00df", "er", "za\u00b7gen", "ler\u00b7ne", ",", "zit\u00b7tern"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "VVINF", "VVFIN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "vor urew\u2019ger Majest\u00e4t,", "tokens": ["vor", "u\u00b7re\u00b7w'\u00b7ger", "Ma\u00b7jes\u00b7t\u00e4t", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Wenn in heil\u2019gen Ungewittern", "tokens": ["Wenn", "in", "heil'\u00b7gen", "Un\u00b7ge\u00b7wit\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Deutsche Gottheit aufersteht,", "tokens": ["Deut\u00b7sche", "Got\u00b7theit", "auf\u00b7er\u00b7steht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Da\u00df das Herz uns muthig werde,", "tokens": ["Da\u00df", "das", "Herz", "uns", "mut\u00b7hig", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "stark in neuer Zuversicht:", "tokens": ["stark", "in", "neu\u00b7er", "Zu\u00b7ver\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Vater-Gott und Vater-Erde", "tokens": ["Va\u00b7ter\u00b7Gott", "und", "Va\u00b7ter\u00b7Er\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "raubt uns Macht der Menschen nicht.", "tokens": ["raubt", "uns", "Macht", "der", "Men\u00b7schen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}