{"dta.poem.6990": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Vereinigung und dem Unter-  \n scheid der Seelen und des C\u00f6rpers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein blinder Stoff, ein ungefehr Bewegen,", "tokens": ["Ein", "blin\u00b7der", "Stoff", ",", "ein", "un\u00b7ge\u00b7fehr", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die haben uns Gesetz und Tugenden gemacht?", "tokens": ["Die", "ha\u00b7ben", "uns", "Ge\u00b7setz", "und", "Tu\u00b7gen\u00b7den", "ge\u00b7macht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die k\u00f6nnen eine Lust zur Ehr\u2019 und Weisheit hegen?", "tokens": ["Die", "k\u00f6n\u00b7nen", "ei\u00b7ne", "Lust", "zur", "Ehr'", "und", "Weis\u00b7heit", "he\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "APPRART", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die haben ihnen selbst hervorgebracht", "tokens": ["Die", "ha\u00b7ben", "ih\u00b7nen", "selbst", "her\u00b7vor\u00b7ge\u00b7bracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Des Nachruhms Trieb, von ewigem Bestand?", "tokens": ["Des", "Nach\u00b7ruhms", "Trieb", ",", "von", "e\u00b7wi\u00b7gem", "Be\u00b7stand", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und haben das Gesetz des SCHOEPFFERS selbst erkannt?", "tokens": ["Und", "ha\u00b7ben", "das", "Ge\u00b7setz", "des", "ScHOE\u00b7PF\u00b7FERS", "selbst", "er\u00b7kannt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-++-+-+", "measure": "unknown.measure.septa"}}}}}