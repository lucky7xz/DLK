{"dta.poem.2420": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "An die Augen der sch\u00f6nen und hold-  \n seligen Karitillen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Gleich wie der Sternen Glantz am Firmament verg-", "tokens": ["Gleich", "wie", "der", "Ster\u00b7nen", "Glantz", "am", "Fir\u00b7ma\u00b7ment", "ver\u00b7g"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "NN", "APPRART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "het/", "tokens": ["het", "/"], "token_info": ["word", "punct"], "pos": ["VAFIN", "$("], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Wenn sich das grosse Licht der Sonnen wieder zeigt/", "tokens": ["Wenn", "sich", "das", "gros\u00b7se", "Licht", "der", "Son\u00b7nen", "wie\u00b7der", "zeigt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "ADJA", "NN", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gleich wie das Sternenbeer Dianen willig weicht/", "tokens": ["Gleich", "wie", "das", "Ster\u00b7nen\u00b7beer", "Di\u00b7a\u00b7nen", "wil\u00b7lig", "weicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "NE", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn sie mit vollem Schein\u2019 am heitren Himmel stehe", "tokens": ["Wenn", "sie", "mit", "vol\u00b7lem", "Schein'", "am", "hei\u00b7tren", "Him\u00b7mel", "ste\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So seyd ihr Augen auch die ihr euch freundlich drehet/", "tokens": ["So", "seyd", "ihr", "Au\u00b7gen", "auch", "die", "ihr", "euch", "freund\u00b7lich", "dre\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "ART", "PPER", "PRF", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Jm Antlitz meiner Zier/ vor euch der Mond sich neigt", "tokens": ["Jm", "Ant\u00b7litz", "mei\u00b7ner", "Zier", "/", "vor", "euch", "der", "Mond", "sich", "neigt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$(", "APPR", "PPER", "ART", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und euch des Glantzes Sieg gar willig \u00fcberreicht/", "tokens": ["Und", "euch", "des", "Glant\u00b7zes", "Sieg", "gar", "wil\u00b7lig", "\u00fc\u00b7berr\u00b7eicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "NN", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Auch F\u00f6bus sch\u00e4mt sich selbst am Himmel hoch erh\u00f6het.", "tokens": ["Auch", "F\u00f6\u00b7bus", "sch\u00e4mt", "sich", "selbst", "am", "Him\u00b7mel", "hoch", "er\u00b7h\u00f6\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "PRF", "ADV", "APPRART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Nichts seh\u2019 ich in der Welt das so die Hertzen bricht", "tokens": ["Nichts", "seh'", "ich", "in", "der", "Welt", "das", "so", "die", "Hert\u00b7zen", "bricht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Hertzliebste Karitill als deiner Augen Licht.", "tokens": ["Hertz\u00b7liebs\u00b7te", "Ka\u00b7ri\u00b7till", "als", "dei\u00b7ner", "Au\u00b7gen", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KOUS", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wie \u00fcberselig wird doch dieser seyn zunennen/", "tokens": ["Wie", "\u00fc\u00b7ber\u00b7se\u00b7lig", "wird", "doch", "die\u00b7ser", "seyn", "zu\u00b7nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der solchen klaren Schein vergn\u00fcgt geniessen kan/", "tokens": ["Der", "sol\u00b7chen", "kla\u00b7ren", "Schein", "ver\u00b7gn\u00fcgt", "ge\u00b7nies\u00b7sen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "VVPP", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und den sie wieder\u00fcm mit Liebe strahlen an!", "tokens": ["Und", "den", "sie", "wie\u00b7de\u00b7r\u00fcm", "mit", "Lie\u00b7be", "strah\u00b7len", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "ADV", "APPR", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Huff Himmel da\u00df sie mich vor solchen mag erkennen.", "tokens": ["Huff", "Him\u00b7mel", "da\u00df", "sie", "mich", "vor", "sol\u00b7chen", "mag", "er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KOUS", "PPER", "PRF", "APPR", "PIAT", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}