{"textgrid.poem.49523": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "Als Timoleon zu heyrathen gezwungen ward", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Eigensinn der Zeit zog mich und Henrietten", "tokens": ["Der", "Ei\u00b7gen\u00b7sinn", "der", "Zeit", "zog", "mich", "und", "Hen\u00b7riet\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "KON", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In Hymens Haus, trotz aller Gegenwehr.", "tokens": ["In", "Hy\u00b7mens", "Haus", ",", "trotz", "al\u00b7ler", "Ge\u00b7gen\u00b7wehr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O Liebe, bind uns da fein fest mit Blumenketten,", "tokens": ["O", "Lie\u00b7be", ",", "bind", "uns", "da", "fein", "fest", "mit", "Blu\u00b7men\u00b7ket\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VAFIN", "PPER", "ADV", "ADJD", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und h\u00fct' uns durch ein Fre\u00fcdenheer,", "tokens": ["Und", "h\u00fct'", "uns", "durch", "ein", "Fre\u00fc\u00b7den\u00b7heer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sonst werden wir uns bald ins Schlo\u00df der Freyheit retten,", "tokens": ["Sonst", "wer\u00b7den", "wir", "uns", "bald", "ins", "Schlo\u00df", "der", "Frey\u00b7heit", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ADV", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und dann bekomstu mich nicht mehr.", "tokens": ["Und", "dann", "be\u00b7koms\u00b7tu", "mich", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}