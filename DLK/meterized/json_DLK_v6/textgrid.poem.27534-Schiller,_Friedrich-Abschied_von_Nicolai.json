{"textgrid.poem.27534": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Abschied von Nicolai", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unersch\u00f6pflich wie deine Plattheit ist meine Satire,", "tokens": ["Un\u00b7er\u00b7sch\u00f6pf\u00b7lich", "wie", "dei\u00b7ne", "Platt\u00b7heit", "ist", "mei\u00b7ne", "Sa\u00b7ti\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-++-+--", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Doch f\u00fcr das laufende Jahr nimm mit dem Hundert vorlieb.", "tokens": ["Doch", "f\u00fcr", "das", "lau\u00b7fen\u00b7de", "Jahr", "nimm", "mit", "dem", "Hun\u00b7dert", "vor\u00b7lieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVIMP", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}}}}}