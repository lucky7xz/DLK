{"textgrid.poem.54347": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr so sch\u00f6n und gr\u00fcnen Auen,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr so sch\u00f6n und gr\u00fcnen Auen,", "tokens": ["Ihr", "so", "sch\u00f6n", "und", "gr\u00fc\u00b7nen", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meiner Augen Lustrevier,", "tokens": ["Mei\u00b7ner", "Au\u00b7gen", "Lus\u00b7tre\u00b7vier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Euch will ich mich anvertrauen,", "tokens": ["Euch", "will", "ich", "mich", "an\u00b7ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Brecht ihr Seufzer, brecht herf\u00fcr.", "tokens": ["Brecht", "ihr", "Seuf\u00b7zer", ",", "brecht", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ein geheim und still Verlangen,", "tokens": ["Ein", "ge\u00b7heim", "und", "still", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ein nur mir bewuster Schmerz", "tokens": ["Ein", "nur", "mir", "be\u00b7wus\u00b7ter", "Schmerz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PPER", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "H\u00e4lt die Brust schon l\u00e4ngst gefangen:", "tokens": ["H\u00e4lt", "die", "Brust", "schon", "l\u00e4ngst", "ge\u00b7fan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Thyrsis lebt, doch ohne Herz.", "tokens": ["Thyr\u00b7sis", "lebt", ",", "doch", "oh\u00b7ne", "Herz", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ist es m\u00f6glich, da\u00df ich lebe?", "tokens": ["Ist", "es", "m\u00f6g\u00b7lich", ",", "da\u00df", "ich", "le\u00b7be", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was n\u00e4hrt die gequ\u00e4lte Brust,", "tokens": ["Was", "n\u00e4hrt", "die", "ge\u00b7qu\u00e4l\u00b7te", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da\u00df ich nicht den Geist aufgebe?", "tokens": ["Da\u00df", "ich", "nicht", "den", "Geist", "auf\u00b7ge\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "+-+-++--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Ach! wo bleibt die erste Lust?", "tokens": ["Ach", "!", "wo", "bleibt", "die", "ers\u00b7te", "Lust", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Saget mir, verflogne Stunden,", "tokens": ["Sa\u00b7get", "mir", ",", "ver\u00b7flog\u00b7ne", "Stun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo ist mein Vergn\u00fcgen hin?", "tokens": ["Wo", "ist", "mein", "Ver\u00b7gn\u00fc\u00b7gen", "hin", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Itzo f\u00fchl ich erst die Wunden,", "tokens": ["It\u00b7zo", "f\u00fchl", "ich", "erst", "die", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da ich nicht mehr meine bin.", "tokens": ["Da", "ich", "nicht", "mehr", "mei\u00b7ne", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "PPOSAT", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ruh und Freyheit ist verlohren,", "tokens": ["Ruh", "und", "Frey\u00b7heit", "ist", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles, alles ist dahin!", "tokens": ["Al\u00b7les", ",", "al\u00b7les", "ist", "da\u00b7hin", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PIS", "VAFIN", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wozu bin ich auserkoren,", "tokens": ["Wo\u00b7zu", "bin", "ich", "au\u00b7ser\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach und Pein ist mein Gewinn.", "tokens": ["Ach", "und", "Pein", "ist", "mein", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KON", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Echo, h\u00f6r auf mein Klagen,", "tokens": ["E\u00b7cho", ",", "h\u00f6r", "auf", "mein", "Kla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Denn dein holder Wiederschall", "tokens": ["Denn", "dein", "hol\u00b7der", "Wie\u00b7der\u00b7schall"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Soll es in der Gegend sagen,", "tokens": ["Soll", "es", "in", "der", "Ge\u00b7gend", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Chloris Tod war Thyrsis Fall!", "tokens": ["Chlo\u00b7ris", "Tod", "war", "Thyr\u00b7sis", "Fall", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Fraget nicht, was ich beweine,", "tokens": ["Fra\u00b7get", "nicht", ",", "was", "ich", "be\u00b7wei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was mich in die Gegend ruft.", "tokens": ["Was", "mich", "in", "die", "Ge\u00b7gend", "ruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! die lieblichen Gebeine", "tokens": ["Ach", "!", "die", "lieb\u00b7li\u00b7chen", "Ge\u00b7bei\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ruhen hier in dieser Gruft.", "tokens": ["Ru\u00b7hen", "hier", "in", "die\u00b7ser", "Gruft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Meiner Chloris Angedenken", "tokens": ["Mei\u00b7ner", "Chlo\u00b7ris", "An\u00b7ge\u00b7den\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Spricht mir Geist und Leben ab.", "tokens": ["Spricht", "mir", "Geist", "und", "Le\u00b7ben", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "O! erbarmt dich nicht mein Kr\u00e4nken?", "tokens": ["O", "!", "er\u00b7barmt", "dich", "nicht", "mein", "Kr\u00e4n\u00b7ken", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nimm mich doch nur mit ins Grab.", "tokens": ["Nimm", "mich", "doch", "nur", "mit", "ins", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sammlet euch ihr matten Glieder,", "tokens": ["Samm\u00b7let", "euch", "ihr", "mat\u00b7ten", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du mein abgeh\u00e4rmter Leib,", "tokens": ["Du", "mein", "ab\u00b7ge\u00b7h\u00e4rm\u00b7ter", "Leib", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Du umarmst die Chloris wieder,", "tokens": ["Du", "um\u00b7armst", "die", "Chlo\u00b7ris", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NE", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das so tugendreiche Weib.", "tokens": ["Das", "so", "tu\u00b7gen\u00b7drei\u00b7che", "Weib", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihre kalt erstarrten H\u00e4nde", "tokens": ["Ih\u00b7re", "kalt", "er\u00b7starr\u00b7ten", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und der Klugheitsvolle Mund", "tokens": ["Und", "der", "Klug\u00b7heits\u00b7vol\u00b7le", "Mund"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Tr\u00f6sten mich bis an mein Ende.", "tokens": ["Tr\u00f6s\u00b7ten", "mich", "bis", "an", "mein", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nur der Tod trennt unsern Bund.", "tokens": ["Nur", "der", "Tod", "trennt", "un\u00b7sern", "Bund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wohl, ich warte mit Verlangen,", "tokens": ["Wohl", ",", "ich", "war\u00b7te", "mit", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis der Tod das Herze bricht.", "tokens": ["Bis", "der", "Tod", "das", "Her\u00b7ze", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meiner Sch\u00f6nen kalte Wangen", "tokens": ["Mei\u00b7ner", "Sch\u00f6\u00b7nen", "kal\u00b7te", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00fc\u00df ich, weil mir wohl geschicht.", "tokens": ["K\u00fc\u00df", "ich", ",", "weil", "mir", "wohl", "ge\u00b7schicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Oeffne dich, du Grabesh\u00f6le,", "tokens": ["Oeff\u00b7ne", "dich", ",", "du", "Gra\u00b7bes\u00b7h\u00f6\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ach! wie sanfte ruh ich hier,", "tokens": ["Ach", "!", "wie", "sanf\u00b7te", "ruh", "ich", "hier", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "ADJA", "NN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Neben meiner andern Seele,", "tokens": ["Ne\u00b7ben", "mei\u00b7ner", "an\u00b7dern", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Welt, ich tausche nicht mit dir.", "tokens": ["Welt", ",", "ich", "tau\u00b7sche", "nicht", "mit", "dir", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PTKNEG", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}