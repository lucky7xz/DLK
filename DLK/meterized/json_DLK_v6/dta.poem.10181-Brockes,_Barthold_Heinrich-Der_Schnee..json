{"dta.poem.10181": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Schnee.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich seh euch nimmer, ohn Vergn\u00fcgen,", "tokens": ["Ich", "seh", "euch", "nim\u00b7mer", ",", "ohn", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jhr rege weisse Flocken, fliegen,", "tokens": ["Ihr", "re\u00b7ge", "weis\u00b7se", "Flo\u00b7cken", ",", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Jhr sinckt, ihr steigt, ihr fallt, ihr spielt.", "tokens": ["Ihr", "sinckt", ",", "ihr", "steigt", ",", "ihr", "fallt", ",", "ihr", "spielt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer ist, wenn durch der Winde wehen", "tokens": ["Wer", "ist", ",", "wenn", "durch", "der", "Win\u00b7de", "we\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "$,", "KOUS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wir euch bald schnell, bald langsam sehen,", "tokens": ["Wir", "euch", "bald", "schnell", ",", "bald", "lang\u00b7sam", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "ADV", "ADJD", "$,", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Verworren durch einander gehen,", "tokens": ["Ver\u00b7wor\u00b7ren", "durch", "ein\u00b7an\u00b7der", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der nicht ein sanftes schauern f\u00fchlt?", "tokens": ["Der", "nicht", "ein", "sanf\u00b7tes", "schau\u00b7ern", "f\u00fchlt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ART", "ADJA", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zumahl wenn man den weissen Schimmer,", "tokens": ["Zu\u00b7mahl", "wenn", "man", "den", "weis\u00b7sen", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Aus einem wol gew\u00e4rmten Zimmer,", "tokens": ["Aus", "ei\u00b7nem", "wol", "ge\u00b7w\u00e4rm\u00b7ten", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Mit aufgemuntertem Gem\u00fcth,", "tokens": ["Mit", "auf\u00b7ge\u00b7mun\u00b7ter\u00b7tem", "Ge\u00b7m\u00fcth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Durch aufgethaute Scheiben sieht,", "tokens": ["Durch", "auf\u00b7ge\u00b7thau\u00b7te", "Schei\u00b7ben", "sieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und, frey von Unbequemlichkeit", "tokens": ["Und", ",", "frey", "von", "Un\u00b7be\u00b7quem\u00b7lich\u00b7keit"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Der frostig rauhen Winters-Zeit,", "tokens": ["Der", "fros\u00b7tig", "rau\u00b7hen", "Win\u00b7ter\u00b7sZeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Den Glantz, den uns der Schnee gebieret,", "tokens": ["Den", "Glantz", ",", "den", "uns", "der", "Schnee", "ge\u00b7bie\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Wenn er die Welt mit Silber zieret,", "tokens": ["Wenn", "er", "die", "Welt", "mit", "Sil\u00b7ber", "zie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Voll Lust ob GOTTES Werck, erblickt:", "tokens": ["Voll", "Lust", "ob", "GoT\u00b7TES", "Werck", ",", "er\u00b7blickt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "NN", "KOUS", "NE", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und man, wann Neiff die B\u00e4ume schm\u00fcckt,", "tokens": ["Und", "man", ",", "wann", "Neiff", "die", "B\u00e4u\u00b7me", "schm\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PWAV", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Wann Frost das Land mit Schollen dr\u00fcckt,", "tokens": ["Wann", "Frost", "das", "Land", "mit", "Schol\u00b7len", "dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Und mit Crystall die Fluth bebr\u00fcckt,", "tokens": ["Und", "mit", "Crys\u00b7tall", "die", "Fluth", "be\u00b7br\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Des Winters Lust, ohn\u2019 Unlust, sp\u00fchrt.", "tokens": ["Des", "Win\u00b7ters", "Lust", ",", "ohn'", "Un\u00b7lust", ",", "sp\u00fchrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "KOUI", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}