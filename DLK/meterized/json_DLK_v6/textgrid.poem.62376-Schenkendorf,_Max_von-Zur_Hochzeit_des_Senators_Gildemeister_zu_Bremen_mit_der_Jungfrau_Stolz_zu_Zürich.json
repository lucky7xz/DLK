{"textgrid.poem.62376": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Zur Hochzeit des Senators Gildemeister zu Bremen mit der Jungfrau Stolz zu Z\u00fcrich", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6n'res Bild wird nicht gefunden,", "tokens": ["Sch\u00f6n'\u00b7res", "Bild", "wird", "nicht", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "S\u00fc\u00dfe Ruhe, stilles Gl\u00fcck,", "tokens": ["S\u00fc\u00b7\u00dfe", "Ru\u00b7he", ",", "stil\u00b7les", "Gl\u00fcck", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und kein Wandrer mag erkunden", "tokens": ["Und", "kein", "Wand\u00b7rer", "mag", "er\u00b7kun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen hellern Himmelsblick,", "tokens": ["Ei\u00b7nen", "hel\u00b7lern", "Him\u00b7mels\u00b7blick", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Als der Kranz der gr\u00fcnen H\u00fcgel,", "tokens": ["Als", "der", "Kranz", "der", "gr\u00fc\u00b7nen", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die sich um die Limmat reihn,", "tokens": ["Die", "sich", "um", "die", "Lim\u00b7mat", "reihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Z\u00fcrchersee, in deinem Spiegel", "tokens": ["Z\u00fcr\u00b7ch\u00b7er\u00b7see", ",", "in", "dei\u00b7nem", "Spie\u00b7gel"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Deiner Alpen Rosenschein.", "tokens": ["Dei\u00b7ner", "Al\u00b7pen", "Ro\u00b7sen\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Mildes Schwimmen, s\u00fc\u00dfes Schweben,", "tokens": ["Mil\u00b7des", "Schwim\u00b7men", ",", "s\u00fc\u00b7\u00dfes", "Schwe\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stille wunderbare Fahrt,", "tokens": ["Stil\u00b7le", "wun\u00b7der\u00b7ba\u00b7re", "Fahrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles Lieben, alles Leben", "tokens": ["Al\u00b7les", "Lie\u00b7ben", ",", "al\u00b7les", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "ADJA", "$,", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat in dir sich offenbart.", "tokens": ["Hat", "in", "dir", "sich", "of\u00b7fen\u00b7bart", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "PRF", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Blick, du wirst hinabgezogen", "tokens": ["Blick", ",", "du", "wirst", "hin\u00b7ab\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In die tiefe, gr\u00fcne Flut,", "tokens": ["In", "die", "tie\u00b7fe", ",", "gr\u00fc\u00b7ne", "Flut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ein Bild entsteigt den Wogen,", "tokens": ["Und", "ein", "Bild", "ent\u00b7steigt", "den", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Freiheit, unser h\u00f6chstes Gut!", "tokens": ["Frei\u00b7heit", ",", "un\u00b7ser", "h\u00f6chs\u00b7tes", "Gut", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ueber Alpen, \u00fcber Seen,", "tokens": ["Ue\u00b7ber", "Al\u00b7pen", ",", "\u00fc\u00b7ber", "Seen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was ein Wandrer sucht und preist,", "tokens": ["Was", "ein", "Wand\u00b7rer", "sucht", "und", "preist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Waltet still und hehr dein Wehen,", "tokens": ["Wal\u00b7tet", "still", "und", "hehr", "dein", "We\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wunderbarer, ew'ger Geist!", "tokens": ["Wun\u00b7der\u00b7ba\u00b7rer", ",", "ew'\u00b7ger", "Geist", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kl\u00e4nge schweigen, Farben bleichen", "tokens": ["Kl\u00e4n\u00b7ge", "schwei\u00b7gen", ",", "Far\u00b7ben", "blei\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "$,", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Vor dem wunderbarsten Licht,", "tokens": ["Vor", "dem", "wun\u00b7der\u00b7bars\u00b7ten", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Alle Bilder m\u00fcssen weichen", "tokens": ["Al\u00b7le", "Bil\u00b7der", "m\u00fcs\u00b7sen", "wei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Vor dem Menschenangesicht.", "tokens": ["Vor", "dem", "Men\u00b7schen\u00b7an\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Frag' ihn drum, er hat's erfahren,", "tokens": ["Frag'", "ihn", "drum", ",", "er", "hat's", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PAV", "$,", "PPER", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jungfrau, der gereiste Mann,", "tokens": ["Jung\u00b7frau", ",", "der", "ge\u00b7reis\u00b7te", "Mann", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Als er in den Krieges-Jahren", "tokens": ["Als", "er", "in", "den", "Krie\u00b7ge\u00b7sJah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu des Vaters Wohnung kam.", "tokens": ["Zu", "des", "Va\u00b7ters", "Woh\u00b7nung", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zorn und Eifer in dem Herzen", "tokens": ["Zorn", "und", "Ei\u00b7fer", "in", "dem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fcr der deutschen Lande Gl\u00fcck,", "tokens": ["F\u00fcr", "der", "deut\u00b7schen", "Lan\u00b7de", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Fiel wie Schein von Altar-Kerzen", "tokens": ["Fiel", "wie", "Schein", "von", "Al\u00b7ta\u00b7rKer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Doch in ihn ", "tokens": ["Doch", "in", "ihn"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "PPER"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Deiner Heimat fern entnommen,", "tokens": ["Dei\u00b7ner", "Hei\u00b7mat", "fern", "ent\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bleibst du treu dem Weserthal;", "tokens": ["Bleibst", "du", "treu", "dem", "We\u00b7ser\u00b7thal", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus der Heimat mu\u00df er kommen,", "tokens": ["Aus", "der", "Hei\u00b7mat", "mu\u00df", "er", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welcher w\u00e4hlt mit schneller Wahl", "tokens": ["Wel\u00b7cher", "w\u00e4hlt", "mit", "schnel\u00b7ler", "Wahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAT", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wieder nach der Heimat nehmen", "tokens": ["Wie\u00b7der", "nach", "der", "Hei\u00b7mat", "neh\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Will er das geliebte Pfand \u2013", "tokens": ["Will", "er", "das", "ge\u00b7lieb\u00b7te", "Pfand", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Freies Z\u00fcrich, freies Bremen,", "tokens": ["Frei\u00b7es", "Z\u00fc\u00b7rich", ",", "frei\u00b7es", "Bre\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Preiset solch ein Liebesband!", "tokens": ["Prei\u00b7set", "solch", "ein", "Lie\u00b7bes\u00b7band", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wenn des Krieges blut'gen Saaten", "tokens": ["Wenn", "des", "Krie\u00b7ges", "blut'\u00b7gen", "Saa\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Solche Bl\u00fctenpracht entsprie\u00dft,", "tokens": ["Sol\u00b7che", "Bl\u00fc\u00b7ten\u00b7pracht", "ent\u00b7sprie\u00dft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn der Mann f\u00fcr Wort und Thaten", "tokens": ["Wenn", "der", "Mann", "f\u00fcr", "Wort", "und", "Tha\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Solchen holden Lohn genie\u00dft, \u2013", "tokens": ["Sol\u00b7chen", "hol\u00b7den", "Lohn", "ge\u00b7nie\u00dft", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Laut und fr\u00f6hlich soll man preisen", "tokens": ["Laut", "und", "fr\u00f6h\u00b7lich", "soll", "man", "prei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KON", "ADJD", "VMFIN", "PIS", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dann den schnellen Liebespfeil,", "tokens": ["Dann", "den", "schnel\u00b7len", "Lie\u00b7be\u00b7spfeil", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und in wohlbekannten Weisen", "tokens": ["Und", "in", "wohl\u00b7be\u00b7kann\u00b7ten", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sing' ich diesem Bunde Heil.", "tokens": ["Sing'", "ich", "die\u00b7sem", "Bun\u00b7de", "Heil", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PDAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}