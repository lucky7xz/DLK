{"dta.poem.11789": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "75.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Im Weg begegnen sich die Bien' und die Ameise,", "tokens": ["Im", "Weg", "be\u00b7geg\u00b7nen", "sich", "die", "Bi\u00b7en'", "und", "die", "A\u00b7mei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PRF", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die singend in der Luft, und die am Boden leise.", "tokens": ["Die", "sin\u00b7gend", "in", "der", "Luft", ",", "und", "die", "am", "Bo\u00b7den", "lei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "$,", "KON", "ART", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie haben keine Zeit einander zu begr\u00fc\u00dfen,", "tokens": ["Sie", "ha\u00b7ben", "kei\u00b7ne", "Zeit", "ein\u00b7an\u00b7der", "zu", "be\u00b7gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie treibt der rege Flei\u00df auf Fl\u00fcgeln fort und F\u00fc\u00dfen.", "tokens": ["Sie", "treibt", "der", "re\u00b7ge", "Flei\u00df", "auf", "Fl\u00fc\u00b7geln", "fort", "und", "F\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "PTKVZ", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Fort treibt sie reger Flei\u00df auf Fl\u00fcgeln und auf F\u00fc\u00dfen,", "tokens": ["Fort", "treibt", "sie", "re\u00b7ger", "Flei\u00df", "auf", "Fl\u00fc\u00b7geln", "und", "auf", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADJA", "NN", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu b\u00fc\u00dfen ihre Lust am bittern Werk und s\u00fc\u00dfen.", "tokens": ["Zu", "b\u00fc\u00b7\u00dfen", "ih\u00b7re", "Lust", "am", "bit\u00b7tern", "Werk", "und", "s\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Bien' am s\u00fc\u00dfen Werk, die Ameis' an dem bittern,", "tokens": ["Die", "Bi\u00b7en'", "am", "s\u00fc\u00b7\u00dfen", "Werk", ",", "die", "A\u00b7meis'", "an", "dem", "bit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,", "ART", "NN", "APPR", "ART", "ADJA", "$,"], "meter": "--+-+-+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Zu riechen Honigduft und Weihrauchkorn zu wittern.", "tokens": ["Zu", "rie\u00b7chen", "Ho\u00b7nig\u00b7duft", "und", "Weih\u00b7rauch\u00b7korn", "zu", "wit\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die Aems' am bittern Werk, die Bien' an ihrem s\u00fc\u00dfen,", "tokens": ["Die", "A\u00b7ems'", "am", "bit\u00b7tern", "Werk", ",", "die", "Bi\u00b7en'", "an", "ih\u00b7rem", "s\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,", "ART", "NN", "APPR", "PPOSAT", "ADJA", "$,"], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Arbeiten stets mit Lust, die Arbeitslust zu b\u00fc\u00dfen.", "tokens": ["Ar\u00b7bei\u00b7ten", "stets", "mit", "Lust", ",", "die", "Ar\u00b7beits\u00b7lust", "zu", "b\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und f\u00fcrchteten die Zeit zur Arbeit einzub\u00fc\u00dfen,", "tokens": ["Und", "f\u00fcrch\u00b7te\u00b7ten", "die", "Zeit", "zur", "Ar\u00b7beit", "ein\u00b7zu\u00b7b\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "N\u00e4hmen sie sich die Zeit einander zu begr\u00fc\u00dfen.", "tokens": ["N\u00e4h\u00b7men", "sie", "sich", "die", "Zeit", "ein\u00b7an\u00b7der", "zu", "be\u00b7gr\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.7": {"line.1": {"text": "Sie tummeln sich vorbei, und werden nicht gewahr,", "tokens": ["Sie", "tum\u00b7meln", "sich", "vor\u00b7bei", ",", "und", "wer\u00b7den", "nicht", "ge\u00b7wahr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "$,", "KON", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie gleich und ungleich sie zusammen sind ein Paar.", "tokens": ["Wie", "gleich", "und", "un\u00b7gleich", "sie", "zu\u00b7sam\u00b7men", "sind", "ein", "Paar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "KON", "ADJD", "PPER", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Die Imm' ist im Gesch\u00e4ft best\u00e4ndig immer kr\u00e4ftig,", "tokens": ["Die", "Imm'", "ist", "im", "Ge\u00b7sch\u00e4ft", "be\u00b7st\u00e4n\u00b7dig", "im\u00b7mer", "kr\u00e4f\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "ADJD", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Aems' in Aemsigkeit nach Kr\u00e4ften stets gesch\u00e4ftig.", "tokens": ["Die", "A\u00b7ems'", "in", "A\u00b7em\u00b7sig\u00b7keit", "nach", "Kr\u00e4f\u00b7ten", "stets", "ge\u00b7sch\u00e4f\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "APPR", "NN", "ADV", "ADJD", "$."], "meter": "-+---+-+-+-+-+-", "measure": "dactylic.init"}}, "stanza.9": {"line.1": {"text": "Den Vorrath schaffen sie nicht aus selbeignem Rath,", "tokens": ["Den", "Vor\u00b7rath", "schaf\u00b7fen", "sie", "nicht", "aus", "sel\u00b7beig\u00b7nem", "Rath", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie wirken f\u00fcr ein Volk, und leben einem Staat.", "tokens": ["Sie", "wir\u00b7ken", "f\u00fcr", "ein", "Volk", ",", "und", "le\u00b7ben", "ei\u00b7nem", "Staat", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Das Volk der Bienen w\u00e4hlt sich eine K\u00f6niginn,", "tokens": ["Das", "Volk", "der", "Bie\u00b7nen", "w\u00e4hlt", "sich", "ei\u00b7ne", "K\u00f6\u00b7ni\u00b7ginn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ameisen h\u00e4lt zusamm nur der gemeine Sinn.", "tokens": ["A\u00b7mei\u00b7sen", "h\u00e4lt", "zu\u00b7samm", "nur", "der", "ge\u00b7mei\u00b7ne", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Darum im Bienenschlo\u00df auch wohnen faule Dronen,", "tokens": ["Da\u00b7rum", "im", "Bie\u00b7nen\u00b7schlo\u00df", "auch", "woh\u00b7nen", "fau\u00b7le", "Dro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPRART", "NN", "ADV", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da im Ameisenhaus allein Arbeiter wohnen.", "tokens": ["Da", "im", "A\u00b7mei\u00b7sen\u00b7haus", "al\u00b7lein", "Ar\u00b7bei\u00b7ter", "woh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Darum die Bien' ihr Nest im Wipfel sucht gefl\u00fcgelt,", "tokens": ["Da\u00b7rum", "die", "Bi\u00b7en'", "ihr", "Nest", "im", "Wip\u00b7fel", "sucht", "ge\u00b7fl\u00fc\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und sich Ameisenbau vom Boden aufwerts h\u00fcgelt.", "tokens": ["Und", "sich", "A\u00b7mei\u00b7sen\u00b7bau", "vom", "Bo\u00b7den", "auf\u00b7werts", "h\u00fc\u00b7gelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "NE", "APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Im weiten Weg der Luft geht Bienenschwarm nicht irr,", "tokens": ["Im", "wei\u00b7ten", "Weg", "der", "Luft", "geht", "Bie\u00b7nen\u00b7schwarm", "nicht", "irr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVFIN", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch, Ameis', in der Kluft dein wimmelndes Gewirr.", "tokens": ["Noch", ",", "A\u00b7meis'", ",", "in", "der", "Kluft", "dein", "wim\u00b7meln\u00b7des", "Ge\u00b7wirr", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NE", "$,", "APPR", "ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}