{"textgrid.poem.62787": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Genius fuhr unerkannt", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Genius fuhr unerkannt", "tokens": ["Ein", "Ge\u00b7nius", "fuhr", "un\u00b7er\u00b7kannt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Im Marktschiff nach Paris. Kaum stieg er aus dem Nachen,", "tokens": ["Im", "Markt\u00b7schiff", "nach", "Pa\u00b7ris", ".", "Kaum", "stieg", "er", "aus", "dem", "Na\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NE", "$.", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So lief ihm alles nach, Sein attisches Gewand", "tokens": ["So", "lief", "ihm", "al\u00b7les", "nach", ",", "Sein", "at\u00b7ti\u00b7sches", "Ge\u00b7wand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Emp\u00f6rt der Damen Blick. Man rief mit lautem Lachen:", "tokens": ["Em\u00b7p\u00f6rt", "der", "Da\u00b7men", "Blick", ".", "Man", "rief", "mit", "lau\u00b7tem", "La\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$.", "PIS", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach Gott! mein Herr, wie drollicht seht ihr aus,", "tokens": ["Ach", "Gott", "!", "mein", "Herr", ",", "wie", "drol\u00b7licht", "seht", "ihr", "aus", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "PPOSAT", "NN", "$,", "PWAV", "ADJD", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ihr m\u00fcsset euch nach unsrer Mode kleiden.", "tokens": ["Ihr", "m\u00fcs\u00b7set", "euch", "nach", "uns\u00b7rer", "Mo\u00b7de", "klei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Gut, sprach er, weiset mir des besten Schneiders Haus.", "tokens": ["Gut", ",", "sprach", "er", ",", "wei\u00b7set", "mir", "des", "bes\u00b7ten", "Schnei\u00b7ders", "Haus", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ey was, ihr habt kein neues Kleid vonn\u00f6then;", "tokens": ["Ey", "was", ",", "ihr", "habt", "kein", "neu\u00b7es", "Kleid", "von\u00b7n\u00f6\u00b7then", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "$,", "PPER", "VAFIN", "PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Geht auf den Tr\u00f6delmarkt, da bietet man", "tokens": ["Geht", "auf", "den", "Tr\u00f6\u00b7del\u00b7markt", ",", "da", "bie\u00b7tet", "man"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "ADV", "VVFIN", "PIS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Den Reisenden vom Lord bis zum Poeten", "tokens": ["Den", "Rei\u00b7sen\u00b7den", "vom", "Lord", "bis", "zum", "Po\u00b7et\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Um guten Preis gemachte Kleider an.", "tokens": ["Um", "gu\u00b7ten", "Preis", "ge\u00b7mach\u00b7te", "Klei\u00b7der", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "\u00bbich gehe schon.\u00ab Mit schnellem Schritte", "tokens": ["\u00bb", "ich", "ge\u00b7he", "schon", ".", "\u00ab", "Mit", "schnel\u00b7lem", "Schrit\u00b7te"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "ADV", "$.", "$(", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Steigt er zum n\u00e4chsten Tr\u00f6dler hin.", "tokens": ["Steigt", "er", "zum", "n\u00e4chs\u00b7ten", "Tr\u00f6d\u00b7ler", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Der Franzmann \u00f6fnet ihm ein ganzes Magazin", "tokens": ["Der", "Franz\u00b7mann", "\u00f6f\u00b7net", "ihm", "ein", "gan\u00b7zes", "Ma\u00b7ga\u00b7zin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Voll R\u00f6cke nach dem neusten Schnitte.", "tokens": ["Voll", "R\u00f6\u00b7cke", "nach", "dem", "neus\u00b7ten", "Schnit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Vom Frieswams bis zum Galakleid.", "tokens": ["Vom", "Fries\u00b7wams", "bis", "zum", "Ga\u00b7la\u00b7kleid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Der arme Genius verdarb die Zeit", "tokens": ["Der", "ar\u00b7me", "Ge\u00b7nius", "ver\u00b7darb", "die", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Sie nacheinander anzupassen;", "tokens": ["Sie", "na\u00b7ch\u00b7ein\u00b7an\u00b7der", "an\u00b7zu\u00b7pas\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVIZU", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Zu reich, zu schlecht, zu eng, zu weit", "tokens": ["Zu", "reich", ",", "zu", "schlecht", ",", "zu", "eng", ",", "zu", "weit"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PTKA", "ADJD", "$,", "PTKA", "ADJD", "$,", "PTKA", "ADJD", "$,", "PTKA", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Zu hell, zu dunkel; kurz, ihm wollte keines lassen", "tokens": ["Zu", "hell", ",", "zu", "dun\u00b7kel", ";", "kurz", ",", "ihm", "woll\u00b7te", "kei\u00b7nes", "las\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "$,", "PTKA", "ADJD", "$.", "ADJD", "$,", "PPER", "VMFIN", "PIS", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "\u00bbme\u00dft mir ein neues an, ich berste vor Verdru\u00df.\u00ab", "tokens": ["\u00bb", "me\u00dft", "mir", "ein", "neu\u00b7es", "an", ",", "ich", "bers\u00b7te", "vor", "Ver\u00b7dru\u00df", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "ART", "ADJA", "PTKVZ", "$,", "PPER", "VVFIN", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ganz wohl, mein Herr, ihr habt es in zwey Tagen.", "tokens": ["Ganz", "wohl", ",", "mein", "Herr", ",", "ihr", "habt", "es", "in", "zwey", "Ta\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "PPER", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Was seh ich? Fl\u00fcgel! \u2013 Hum! ihr seyd ein Genius? \u2013", "tokens": ["Was", "seh", "ich", "?", "Fl\u00fc\u00b7gel", "!", "\u2013", "Hum", "!", "ihr", "seyd", "ein", "Ge\u00b7nius", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "NN", "$.", "$(", "NE", "$.", "PPER", "VAFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.24": {"text": "Pardon, ihr d\u00fcrft kein neues Kleid hier tragen.", "tokens": ["Par\u00b7don", ",", "ihr", "d\u00fcrft", "kein", "neu\u00b7es", "Kleid", "hier", "tra\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VMFIN", "PIAT", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}