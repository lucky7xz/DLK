{"textgrid.poem.30780": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "94.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil das Gold liegt in der Erde, gehn wir dr\u00fcber mit den F\u00fcssen;", "tokens": ["Weil", "das", "Gold", "liegt", "in", "der", "Er\u00b7de", ",", "gehn", "wir", "dr\u00fc\u00b7ber", "mit", "den", "F\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "PPER", "PAV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wann es rauff k\u00fcmt, k\u00fcmts, da\u00df selbsten wir ihm unten liegen m\u00fcssen.", "tokens": ["Wann", "es", "rauff", "k\u00fcmt", ",", "k\u00fcmts", ",", "da\u00df", "selbs\u00b7ten", "wir", "ihm", "un\u00b7ten", "lie\u00b7gen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVFIN", "$,", "VVFIN", "$,", "KOUS", "VVFIN", "PPER", "PPER", "ADV", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}