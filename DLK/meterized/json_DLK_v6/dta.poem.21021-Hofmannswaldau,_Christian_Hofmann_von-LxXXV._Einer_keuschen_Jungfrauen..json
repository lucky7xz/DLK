{"dta.poem.21021": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "LxXXV.  \n Einer keuschen Jungfrauen.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht rede hier zu frey/ entbl\u00f6sse dich auch nicht/", "tokens": ["Nicht", "re\u00b7de", "hier", "zu", "frey", "/", "ent\u00b7bl\u00f6s\u00b7se", "dich", "auch", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ADV", "PTKA", "ADJD", "$(", "VVFIN", "PPER", "ADV", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hier ruhet ", "tokens": ["Hier", "ru\u00b7het"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Den Leib so keine Brunst vermochte zu versehren/", "tokens": ["Den", "Leib", "so", "kei\u00b7ne", "Brunst", "ver\u00b7moch\u00b7te", "zu", "ver\u00b7seh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PIAT", "NN", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den solst du Reisemann auch bey den Todten ehren.", "tokens": ["Den", "solst", "du", "Rei\u00b7se\u00b7mann", "auch", "bey", "den", "Tod\u00b7ten", "eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}