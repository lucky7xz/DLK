{"textgrid.poem.56035": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "N\u00e4chtliche Fahrt", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Damals als wir mit den glatten Trabern", "tokens": ["Da\u00b7mals", "als", "wir", "mit", "den", "glat\u00b7ten", "Tra\u00b7bern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "(schwarzen, aus dem Orloff'schen Gest\u00fct) \u2013,", "tokens": ["(", "schwar\u00b7zen", ",", "aus", "dem", "Or\u00b7loff'\u00b7schen", "Ge\u00b7st\u00fct", ")", "\u2013", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "VVINF", "$,", "APPR", "ART", "ADJA", "NN", "$(", "$(", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "w\u00e4hrend hinter hohen Kandelabern", "tokens": ["w\u00e4h\u00b7rend", "hin\u00b7ter", "ho\u00b7hen", "Kan\u00b7de\u00b7la\u00b7bern"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Stadtnachtfronten lagen, angefr\u00fcht,", "tokens": ["Stadt\u00b7nacht\u00b7fron\u00b7ten", "la\u00b7gen", ",", "an\u00b7ge\u00b7fr\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "stumm und keiner Stunde mehr gem\u00e4\u00df \u2013,", "tokens": ["stumm", "und", "kei\u00b7ner", "Stun\u00b7de", "mehr", "ge\u00b7m\u00e4\u00df", "\u2013", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "KON", "PIAT", "NN", "ADV", "ADJD", "$(", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "fuhren, nein: vergingen oder flogen", "tokens": ["fuh\u00b7ren", ",", "nein", ":", "ver\u00b7gin\u00b7gen", "o\u00b7der", "flo\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "PTKANT", "$.", "VVPP", "KON", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "und um lastende Pal\u00e4ste bogen", "tokens": ["und", "um", "las\u00b7ten\u00b7de", "Pa\u00b7l\u00e4s\u00b7te", "bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "in das Wehn der Newa-Quais,", "tokens": ["in", "das", "Wehn", "der", "Ne\u00b7wa\u00b7Quais", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "hingerissen durch das wache Nachten,", "tokens": ["hin\u00b7ge\u00b7ris\u00b7sen", "durch", "das", "wa\u00b7che", "Nach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "das nicht Himmel und nicht Erde hat, \u2013", "tokens": ["das", "nicht", "Him\u00b7mel", "und", "nicht", "Er\u00b7de", "hat", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "PTKNEG", "NN", "KON", "PTKNEG", "NN", "VAFIN", "$,", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "als das Dr\u00e4ngende von unbewachten", "tokens": ["als", "das", "Dr\u00e4n\u00b7gen\u00b7de", "von", "un\u00b7be\u00b7wach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "G\u00e4rten g\u00e4rend aus dem Ljetnij-Ssad", "tokens": ["G\u00e4r\u00b7ten", "g\u00e4\u00b7rend", "aus", "dem", "Lj\u00b7et\u00b7ni\u00b7j\u00b7S\u00b7sad"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "aufstieg, w\u00e4hrend seine Steinfiguren", "tokens": ["auf\u00b7stieg", ",", "w\u00e4h\u00b7rend", "sei\u00b7ne", "Stein\u00b7fi\u00b7gu\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "schwindend mit ohnm\u00e4chtigen Konturen", "tokens": ["schwin\u00b7dend", "mit", "ohn\u00b7m\u00e4ch\u00b7ti\u00b7gen", "Kon\u00b7tu\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "hinter uns vergingen, wie wir fuhren \u2013:", "tokens": ["hin\u00b7ter", "uns", "ver\u00b7gin\u00b7gen", ",", "wie", "wir", "fuh\u00b7ren", "\u2013", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$(", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "damals h\u00f6rte diese Stadt", "tokens": ["da\u00b7mals", "h\u00f6r\u00b7te", "die\u00b7se", "Stadt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "auf zu sein. Auf einmal gab sie zu,", "tokens": ["auf", "zu", "sein", ".", "Auf", "ein\u00b7mal", "gab", "sie", "zu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PTKZU", "VAINF", "$.", "APPR", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "da\u00df sie niemals war, um nichts als Ruh", "tokens": ["da\u00df", "sie", "nie\u00b7mals", "war", ",", "um", "nichts", "als", "Ruh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "$,", "KOUI", "PIS", "KOKOM", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "flehend; wie ein Irrer, dem das Wirrn", "tokens": ["fle\u00b7hend", ";", "wie", "ein", "Ir\u00b7rer", ",", "dem", "das", "Wirrn"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "$.", "PWAV", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "pl\u00f6tzlich sich entwirrt, das ihn verriet,", "tokens": ["pl\u00f6tz\u00b7lich", "sich", "ent\u00b7wirrt", ",", "das", "ihn", "ver\u00b7riet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "VVPP", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "und der einen jahrelangen kranken", "tokens": ["und", "der", "ei\u00b7nen", "jah\u00b7re\u00b7lan\u00b7gen", "kran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "gar nicht zu verwandelnden Gedanken,", "tokens": ["gar", "nicht", "zu", "ver\u00b7wan\u00b7deln\u00b7den", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "PTKZU", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "den er nie mehr denken mu\u00df: Granit \u2013", "tokens": ["den", "er", "nie", "mehr", "den\u00b7ken", "mu\u00df", ":", "Gra\u00b7nit", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVINF", "VMFIN", "$.", "NE", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "aus dem leeren schwankenden Gehirn", "tokens": ["aus", "dem", "lee\u00b7ren", "schwan\u00b7ken\u00b7den", "Ge\u00b7hirn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "fallen f\u00fchlt, bis man ihn nicht mehr sieht.", "tokens": ["fal\u00b7len", "f\u00fchlt", ",", "bis", "man", "ihn", "nicht", "mehr", "sieht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVINF", "VVFIN", "$,", "KOUS", "PIS", "PPER", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}