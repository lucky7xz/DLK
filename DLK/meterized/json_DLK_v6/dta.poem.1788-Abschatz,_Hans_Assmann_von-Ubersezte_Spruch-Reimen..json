{"dta.poem.1788": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ubersezte Spruch-Reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Des sch\u00f6nen Leibes grade Pracht/", "tokens": ["Des", "sch\u00f6\u00b7nen", "Lei\u00b7bes", "gra\u00b7de", "Pracht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der schwartzen Angen lichte Nacht/", "tokens": ["Der", "schwart\u00b7zen", "An\u00b7gen", "lich\u00b7te", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der s\u00fcsse Blick/ der kluge Geist/", "tokens": ["Der", "s\u00fcs\u00b7se", "Blick", "/", "der", "klu\u00b7ge", "Geist", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der sich in Thun und Lassen weist/", "tokens": ["Der", "sich", "in", "Thun", "und", "Las\u00b7sen", "weist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Macht uns um Haubt und Hertze warm/", "tokens": ["Macht", "uns", "um", "Haubt", "und", "Hert\u00b7ze", "warm", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "KON", "VVFIN", "ADJD", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Um K\u00fcch und Beutel kalt und arm.", "tokens": ["Um", "K\u00fcch", "und", "Beu\u00b7tel", "kalt", "und", "arm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch find ich di\u00df/ so frag ich nicht/", "tokens": ["Doch", "find", "ich", "di\u00df", "/", "so", "frag", "ich", "nicht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PDS", "$(", "ADV", "VVFIN", "PPER", "PTKNEG", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ob sonsten Geld und Gutt gebricht.", "tokens": ["Ob", "sons\u00b7ten", "Geld", "und", "Gutt", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Was fehlt dem der Vergn\u00fcgen hat?", "tokens": ["Was", "fehlt", "dem", "der", "Ver\u00b7gn\u00fc\u00b7gen", "hat", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ein sch\u00f6nes Weib macht halber satt.", "tokens": ["Ein", "sch\u00f6\u00b7nes", "Weib", "macht", "hal\u00b7ber", "satt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPO", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}