{"dta.poem.7758": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "38.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Neid verzehrt sich selbst, sollt' er nichts andres k\u00f6nnen;", "tokens": ["Der", "Neid", "ver\u00b7zehrt", "sich", "selbst", ",", "sollt'", "er", "nichts", "and\u00b7res", "k\u00f6n\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "$,", "VMFIN", "PPER", "PIS", "PIS", "VMFIN", "$."], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Die rechte Misgunst ist, sich selbst nichts Gutes g\u00f6nnen.", "tokens": ["Die", "rech\u00b7te", "Mis\u00b7gunst", "ist", ",", "sich", "selbst", "nichts", "Gu\u00b7tes", "g\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "PRF", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Drei Neider sind in Streit, wer k\u00f6nn' am besten neiden,", "tokens": ["Drei", "Nei\u00b7der", "sind", "in", "Streit", ",", "wer", "k\u00f6nn'", "am", "bes\u00b7ten", "nei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "APPR", "NN", "$,", "PWS", "VMFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ihre Streitigkeit sollt' also sich entscheiden.", "tokens": ["Und", "ih\u00b7re", "Strei\u00b7tig\u00b7keit", "sollt'", "al\u00b7so", "sich", "ent\u00b7schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "ADV", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der eine sprach: Vernehmt, wie weit mein Neiden gehe:", "tokens": ["Der", "ei\u00b7ne", "sprach", ":", "Ver\u00b7nehmt", ",", "wie", "weit", "mein", "Nei\u00b7den", "ge\u00b7he", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$.", "VVFIN", "$,", "PWAV", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich g\u00f6nn' es keinem, da\u00df im Traum ihm Gut's geschehe.", "tokens": ["Ich", "g\u00f6nn'", "es", "kei\u00b7nem", ",", "da\u00df", "im", "Traum", "ihm", "Gut's", "ge\u00b7sche\u00b7he", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "$,", "KOUS", "APPRART", "NN", "PPER", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der andre sprach: Du bist noch gar zu schwach ein Ritter;", "tokens": ["Der", "and\u00b7re", "sprach", ":", "Du", "bist", "noch", "gar", "zu", "schwach", "ein", "Rit\u00b7ter", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "ADV", "PTKA", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich g\u00f6nn' es keinem, da\u00df ihm Gutes dank' ein Dritter.", "tokens": ["Ich", "g\u00f6nn'", "es", "kei\u00b7nem", ",", "da\u00df", "ihm", "Gu\u00b7tes", "dank'", "ein", "Drit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "$,", "KOUS", "PPER", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der dritte sprach: Ihr seid allbeide viel zu gut;", "tokens": ["Der", "drit\u00b7te", "sprach", ":", "Ihr", "seid", "all\u00b7bei\u00b7de", "viel", "zu", "gut", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$.", "PPER", "VAFIN", "PIS", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich g\u00f6nn' es keinem, da\u00df er selbst mir Gutes thut.", "tokens": ["Ich", "g\u00f6nn'", "es", "kei\u00b7nem", ",", "da\u00df", "er", "selbst", "mir", "Gu\u00b7tes", "thut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "$,", "KOUS", "PPER", "ADV", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}