{"textgrid.poem.28174": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "89.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Brot f\u00fcr diese Welt, das mag man t\u00e4glich essen;", "tokens": ["Das", "Brot", "f\u00fcr", "die\u00b7se", "Welt", ",", "das", "mag", "man", "t\u00e4g\u00b7lich", "es\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDAT", "NN", "$,", "PDS", "VMFIN", "PIS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Brot f\u00fcr jene Welt, das wil man bald vergessen.", "tokens": ["Das", "Brot", "f\u00fcr", "je\u00b7ne", "Welt", ",", "das", "wil", "man", "bald", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDAT", "NN", "$,", "PDS", "VMFIN", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Brot f\u00fcr jene Welt gibt Brot f\u00fcr diese Welt;", "tokens": ["Das", "Brot", "f\u00fcr", "je\u00b7ne", "Welt", "gibt", "Brot", "f\u00fcr", "die\u00b7se", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDAT", "NN", "VVFIN", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie da\u00df man dann nun Brot, als Gott, viel werther h\u00e4lt?", "tokens": ["Wie", "da\u00df", "man", "dann", "nun", "Brot", ",", "als", "Gott", ",", "viel", "wert\u00b7her", "h\u00e4lt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word",