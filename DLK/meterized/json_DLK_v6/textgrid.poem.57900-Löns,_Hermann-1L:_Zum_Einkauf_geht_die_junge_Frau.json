{"textgrid.poem.57900": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zum Einkauf geht die junge Frau", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum Einkauf geht die junge Frau", "tokens": ["Zum", "Ein\u00b7kauf", "geht", "die", "jun\u00b7ge", "Frau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Morgens in die Stadt,", "tokens": ["Des", "Mor\u00b7gens", "in", "die", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es lockt sie sehr das neue Wort:", "tokens": ["Es", "lockt", "sie", "sehr", "das", "neu\u00b7e", "Wort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Rabatt, Rabatt, Rabatt!", "tokens": ["Ra\u00b7batt", ",", "Ra\u00b7batt", ",", "Ra\u00b7batt", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Ein Kauffieber ganz f\u00fcrchterlich", "tokens": ["Ein", "Kauf\u00b7fie\u00b7ber", "ganz", "f\u00fcrch\u00b7ter\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADJD"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie auf dem Leibe hat,", "tokens": ["Sie", "auf", "dem", "Lei\u00b7be", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das kleine Buch verspricht ihr ja", "tokens": ["Das", "klei\u00b7ne", "Buch", "ver\u00b7spricht", "ihr", "ja"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Rabatt, Rabatt, Rabatt!", "tokens": ["Ra\u00b7batt", ",", "Ra\u00b7batt", ",", "Ra\u00b7batt", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Sie kauft bald hier, sie kauft bald da,", "tokens": ["Sie", "kauft", "bald", "hier", ",", "sie", "kauft", "bald", "da", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie kauft sich m\u00fcd und matt,", "tokens": ["Sie", "kauft", "sich", "m\u00fcd", "und", "matt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Backt sieben Seiten im Buche voll", "tokens": ["Backt", "sie\u00b7ben", "Sei\u00b7ten", "im", "Bu\u00b7che", "voll"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "CARD", "NN", "APPRART", "NN", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Rabatt, Rabatt, Rabatt!", "tokens": ["Ra\u00b7batt", ",", "Ra\u00b7batt", ",", "Ra\u00b7batt", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Das ganze Haushaltsgeld geht drauf;", "tokens": ["Das", "gan\u00b7ze", "Haus\u00b7halts\u00b7geld", "geht", "drauf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da f\u00e4llt ihr ein, sie hat", "tokens": ["Da", "f\u00e4llt", "ihr", "ein", ",", "sie", "hat"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kein Geld mehr f\u00fcr das Mittagsbrot \u2013", "tokens": ["Kein", "Geld", "mehr", "f\u00fcr", "das", "Mit\u00b7tags\u00b7brot", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das kommt von dem Rabatt!", "tokens": ["Das", "kommt", "von", "dem", "Ra\u00b7batt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}