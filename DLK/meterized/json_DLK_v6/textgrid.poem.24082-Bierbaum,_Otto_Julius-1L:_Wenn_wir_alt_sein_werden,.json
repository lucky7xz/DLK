{"textgrid.poem.24082": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn wir alt sein werden,", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn wir alt sein werden,", "tokens": ["Wenn", "wir", "alt", "sein", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAINF", "VAFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wenn der Ruhe D\u00e4mmerung", "tokens": ["Wenn", "der", "Ru\u00b7he", "D\u00e4m\u00b7me\u00b7rung"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Leis in immergleichem Atemzuge uns im Herzen haucht,", "tokens": ["Leis", "in", "im\u00b7mer\u00b7glei\u00b7chem", "A\u00b7tem\u00b7zu\u00b7ge", "uns", "im", "Her\u00b7zen", "haucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Wenn das Auge matt und milde blickt,", "tokens": ["Wenn", "das", "Au\u00b7ge", "matt", "und", "mil\u00b7de", "blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "K\u00e4ltre Farben sieht und flockigen Umri\u00df,", "tokens": ["K\u00e4lt\u00b7re", "Far\u00b7ben", "sieht", "und", "flo\u00b7cki\u00b7gen", "Um\u00b7ri\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "KON", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Wenn der H\u00e4nde Dr\u00fccke,", "tokens": ["Wenn", "der", "H\u00e4n\u00b7de", "Dr\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Altersfaltenweich,", "tokens": ["Al\u00b7ters\u00b7fal\u00b7ten\u00b7weich", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Immer abschiednehmender, zag sich f\u00fchlen,", "tokens": ["Im\u00b7mer", "ab\u00b7schied\u00b7neh\u00b7men\u00b7der", ",", "zag", "sich", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "VVFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.9": {"text": "Wenn das Hirn,", "tokens": ["Wenn", "das", "Hirn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "Von Erkenntnis starr, immer k\u00e4lter wird,", "tokens": ["Von", "Er\u00b7kennt\u00b7nis", "starr", ",", "im\u00b7mer", "k\u00e4l\u00b7ter", "wird", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,", "ADV", "ADJD", "VAFIN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Und der Hoffnung warmer Taubenfl\u00fcgelschlag", "tokens": ["Und", "der", "Hoff\u00b7nung", "war\u00b7mer", "Tau\u00b7ben\u00b7fl\u00fc\u00b7gel\u00b7schlag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.12": {"text": "Nicht mehr linde Gl\u00fccksgedankenwellen schl\u00e4gt,", "tokens": ["Nicht", "mehr", "lin\u00b7de", "Gl\u00fccks\u00b7ge\u00b7dan\u00b7ken\u00b7wel\u00b7len", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.13": {"text": "Wenn an Rosen-Statt", "tokens": ["Wenn", "an", "Ro\u00b7sen\u00b7Statt"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "APPR", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.14": {"text": "Herbstzeitlose bla\u00dft ...:", "tokens": ["Herbst\u00b7zeit\u00b7lo\u00b7se", "bla\u00dft", "...", ":"], "token_info": ["word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "$(", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.15": {"text": "Sonne, Sonne!", "tokens": ["Son\u00b7ne", ",", "Son\u00b7ne", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.16": {"text": "Du auch wirst mir dann verbleichen,", "tokens": ["Du", "auch", "wirst", "mir", "dann", "ver\u00b7blei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Die ich kindlich und anbetend liebe.", "tokens": ["Die", "ich", "kind\u00b7lich", "und", "an\u00b7be\u00b7tend", "lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.18": {"text": "Eine W\u00e4rme nur,", "tokens": ["Ei\u00b7ne", "W\u00e4r\u00b7me", "nur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.19": {"text": "Eine Liebe nur,", "tokens": ["Ei\u00b7ne", "Lie\u00b7be", "nur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.20": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.21": {"text": "Werd ich mir wahren:", "tokens": ["Werd", "ich", "mir", "wah\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.22": {"text": "Dich", "tokens": ["Dich"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.23": {"text": "Du Traumvergangene,", "tokens": ["Du", "Traum\u00b7ver\u00b7gan\u00b7ge\u00b7ne", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.24": {"text": "Heilige.", "tokens": ["Hei\u00b7li\u00b7ge", "."], "token_info": ["word", "punct"], "pos": ["ADJA", "$."], "meter": "+--", "measure": "dactylic.init"}}}}}