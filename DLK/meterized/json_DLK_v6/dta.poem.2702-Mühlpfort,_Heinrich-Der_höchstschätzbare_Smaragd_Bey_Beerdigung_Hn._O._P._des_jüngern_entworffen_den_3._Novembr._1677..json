{"dta.poem.2702": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der h\u00f6chstsch\u00e4tzbare Smaragd  \n  Bey Beerdigung Hn.   O. P.   des j\u00fcngern/  \n entworffen den 3. Novembr. 1677.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wje sol/ Wohl-Edle Frau/ sie nicht in Thr\u00e4nen schwim-\nmen? (gehn?", "tokens": ["Wie", "sol", "/", "Wohl\u00b7Ed\u00b7le", "Frau", "/", "sie", "nicht", "in", "Thr\u00e4\u00b7nen", "schwim", "men", "?", "(", "gehn", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["NE", "VMFIN", "$(", "ADJA", "NN", "$(", "PPER", "PTKNEG", "APPR", "NN", "TRUNC", "NN", "$.", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Was kan aus ihrem Mund itzt mehr als Seufzen", "tokens": ["Was", "kan", "aus", "ih\u00b7rem", "Mund", "itzt", "mehr", "als", "Seuf\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "APPR", "PPOSAT", "NN", "ADV", "PIAT", "KOKOM", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mag auch was schrecklichers des Himmels Schlu\u00df be-", "tokens": ["Mag", "auch", "was", "schreck\u00b7li\u00b7chers", "des", "Him\u00b7mels", "Schlu\u00df", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PWS", "ADV", "ART", "NN", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als da\u00df ihr liebster Sohn mu\u00df auf der Bahre stehn?", "tokens": ["Als", "da\u00df", "ihr", "liebs\u00b7ter", "Sohn", "mu\u00df", "auf", "der", "Bah\u00b7re", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPOSAT", "ADJA", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ist nicht ihr gantzer Schmuck von ihrem Haupt gerissen", "tokens": ["Ist", "nicht", "ihr", "gant\u00b7zer", "Schmuck", "von", "ih\u00b7rem", "Haupt", "ge\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Liebligkeit ", "tokens": ["Der", "Lieb\u00b7lig\u00b7keit"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Mu\u00df das/ was vor ihr Hertz/ anitzt der Sarg einschliessen?", "tokens": ["Mu\u00df", "das", "/", "was", "vor", "ihr", "Hertz", "/", "a\u00b7nitzt", "der", "Sarg", "ein\u00b7schlies\u00b7sen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "$(", "PWS", "APPR", "PPOSAT", "NN", "$(", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ist bleiche Traurigkeit der \u00fcbrige Gewin?", "tokens": ["Ist", "blei\u00b7che", "Trau\u00b7rig\u00b7keit", "der", "\u00fcb\u00b7ri\u00b7ge", "Ge\u00b7win", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach ja! der Jahre Trost und unvergleichlich Hoffen/", "tokens": ["Ach", "ja", "!", "der", "Jah\u00b7re", "Trost", "und", "un\u00b7ver\u00b7gleich\u00b7lich", "Hof\u00b7fen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$.", "ART", "NN", "NN", "KON", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Blume/ so zu Ruhm des Stammes aufgebl\u00fcht/", "tokens": ["Die", "Blu\u00b7me", "/", "so", "zu", "Ruhm", "des", "Stam\u00b7mes", "auf\u00b7ge\u00b7bl\u00fcht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "APPR", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Hat in dem ersten Lentz des Todes Strich betroffen/", "tokens": ["Hat", "in", "dem", "ers\u00b7ten", "Lentz", "des", "To\u00b7des", "Strich", "be\u00b7trof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df man sie welck und bla\u00df\u2019 in kaltem Sande sieht.", "tokens": ["Da\u00df", "man", "sie", "welck", "und", "bla\u00df'", "in", "kal\u00b7tem", "San\u00b7de", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "PTKVZ", "KON", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Nennt die Cornelia/ der Kern von R\u00f6mschen Frauen/", "tokens": ["Nennt", "die", "Cor\u00b7ne\u00b7lia", "/", "der", "Kern", "von", "R\u00f6m\u00b7schen", "Frau\u00b7en", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "$(", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.14": {"text": "Die Kinder Edler Art ihr Gut und Edle Stein/", "tokens": ["Die", "Kin\u00b7der", "Ed\u00b7ler", "Art", "ihr", "Gut", "und", "Ed\u00b7le", "Stein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PPOSAT", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und l\u00e4st vor aller Pracht die tapffre S\u00f6hne schauen/", "tokens": ["Und", "l\u00e4st", "vor", "al\u00b7ler", "Pracht", "die", "tapf\u00b7fre", "S\u00f6h\u00b7ne", "schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die ihrer Sch\u00e4tze Schatz und Reichthum solten seyn;", "tokens": ["Die", "ih\u00b7rer", "Sch\u00e4t\u00b7ze", "Schatz", "und", "Reicht\u00b7hum", "sol\u00b7ten", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "KON", "NN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "So war/ Wohl-Edle Frau/ ja ein ", "tokens": ["So", "war", "/", "Wohl\u00b7Ed\u00b7le", "Frau", "/", "ja", "ein"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "$(", "ADJA", "NN", "$(", "ADV", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Der allerliebste Sohn voll Witz und H\u00f6ffligkeit.", "tokens": ["Der", "al\u00b7ler\u00b7liebs\u00b7te", "Sohn", "voll", "Witz", "und", "H\u00f6ff\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wie jenes Gr\u00fcne kan die Augen nach sich reissen/", "tokens": ["Wie", "je\u00b7nes", "Gr\u00fc\u00b7ne", "kan", "die", "Au\u00b7gen", "nach", "sich", "reis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "VMFIN", "ART", "NN", "APPR", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So stand auch Anmuth hier ihm immer an der Seit.", "tokens": ["So", "stand", "auch", "An\u00b7muth", "hier", "ihm", "im\u00b7mer", "an", "der", "Seit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "ADV", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Entwirfft nicht der ", "tokens": ["Ent\u00b7wirfft", "nicht", "der"], "token_info": ["word", "word", "word"], "pos": ["NN", "PTKNEG", "ART"], "meter": "+-+-", "measure": "trochaic.di"}, "line.22": {"text": "Woran das Auge sich nicht sattergetzen kan?", "tokens": ["Wo\u00b7ran", "das", "Au\u00b7ge", "sich", "nicht", "sat\u00b7ter\u00b7get\u00b7zen", "kan", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wird seine Farbe nicht vor andern hochgepriesen/", "tokens": ["Wird", "sei\u00b7ne", "Far\u00b7be", "nicht", "vor", "an\u00b7dern", "hoch\u00b7ge\u00b7prie\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKNEG", "APPR", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Weil offt die nahe Lufft den gr\u00fcnen Blitz nimmt an?", "tokens": ["Weil", "offt", "die", "na\u00b7he", "Lufft", "den", "gr\u00fc\u00b7nen", "Blitz", "nimmt", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "So gr\u00fcnt und bl\u00fcht ihr Sohn die s\u00fcssest\u2019 Augen-Weide/", "tokens": ["So", "gr\u00fcnt", "und", "bl\u00fcht", "ihr", "Sohn", "die", "s\u00fcs\u00b7sest'", "Au\u00b7gen\u00b7Wei\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Des Hertzens einzig Trost/ der muntren Jugend Prei\u00df.", "tokens": ["Des", "Hert\u00b7zens", "ein\u00b7zig", "Trost", "/", "der", "mun\u00b7tren", "Ju\u00b7gend", "Prei\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$(", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Es schien sein Lebens-Garn gewebt von reiner Seide/", "tokens": ["Es", "schien", "sein", "Le\u00b7bens\u00b7Garn", "ge\u00b7webt", "von", "rei\u00b7ner", "Sei\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "VVPP", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Das Angesicht ein May und Freuden-Paradei\u00df.", "tokens": ["Das", "An\u00b7ge\u00b7sicht", "ein", "May", "und", "Freu\u00b7den\u00b7Pa\u00b7ra\u00b7dei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Es pflegt in den ", "tokens": ["Es", "pflegt", "in", "den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART"], "meter": "-+--", "measure": "dactylic.init"}, "line.30": {"text": "Hier pflantzte Tugenden der Lehrer Flei\u00df ihm ein/", "tokens": ["Hier", "pflantz\u00b7te", "Tu\u00b7gen\u00b7den", "der", "Leh\u00b7rer", "Flei\u00df", "ihm", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ART", "NN", "NN", "PPER", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Das Bildni\u00df/ das er solt in Seel und Hertzen haben/", "tokens": ["Das", "Bild\u00b7ni\u00df", "/", "das", "er", "solt", "in", "Seel", "und", "Hert\u00b7zen", "ha\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "VMFIN", "APPR", "NN", "KON", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "War/ da\u00df er m\u00f6chte gleich den Edlen Ahnen seyn:", "tokens": ["War", "/", "da\u00df", "er", "m\u00f6ch\u00b7te", "gleich", "den", "Ed\u00b7len", "Ah\u00b7nen", "seyn", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$(", "KOUS", "PPER", "VMFIN", "ADV", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Da\u00df angeerbter Glantz samt eigner Tugend-Flammen", "tokens": ["Da\u00df", "an\u00b7ge\u00b7erb\u00b7ter", "Glantz", "samt", "eig\u00b7ner", "Tu\u00b7gen\u00b7dFlam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Steckt\u2019 ein verduppelt Licht der sp\u00e4ten Nach-Welt auf;", "tokens": ["Steckt'", "ein", "ver\u00b7dup\u00b7pelt", "Licht", "der", "sp\u00e4\u00b7ten", "Nach\u00b7Welt", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "VVPP", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und br\u00e4chte Stand und Witz/ Kunst und Vernunfft zusammen/", "tokens": ["Und", "br\u00e4ch\u00b7te", "Stand", "und", "Witz", "/", "Kunst", "und", "Ver\u00b7nunfft", "zu\u00b7sam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "$(", "NN", "KON", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "W\u00e4r\u2019 als ein Hercules in seinem Tugend-Lauff.", "tokens": ["W\u00e4r'", "als", "ein", "Her\u00b7cu\u00b7les", "in", "sei\u00b7nem", "Tu\u00b7gen\u00b7dLauff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Der liebliche ", "tokens": ["Der", "lieb\u00b7li\u00b7che"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+--", "measure": "dactylic.init"}, "line.38": {"text": "Und weil er kostbar ist von Greiffen wol verwacht;", "tokens": ["Und", "weil", "er", "kost\u00b7bar", "ist", "von", "Greif\u00b7fen", "wol", "ver\u00b7wacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VAFIN", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Sie hat Wohl-Edle Frau noch M\u00fch\u2019 noch Flei\u00df verlohren/", "tokens": ["Sie", "hat", "Wohl\u00b7Ed\u00b7le", "Frau", "noch", "M\u00fch'", "noch", "Flei\u00df", "ver\u00b7loh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "ADV", "NN", "ADV", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "An nichts so auf der Welt/ als an den Sohn gedacht.", "tokens": ["An", "nichts", "so", "auf", "der", "Welt", "/", "als", "an", "den", "Sohn", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "APPR", "ART", "NN", "$(", "KOKOM", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Vorhin war der Smaragd dem Gott Mercur geweyhet", "tokens": ["Vor\u00b7hin", "war", "der", "Sma\u00b7ragd", "dem", "Gott", "Mer\u00b7cur", "ge\u00b7wey\u00b7het"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "NE", "VVFIN"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.42": {"text": "Als der Beredsamkeit und holde Sitten schenckt:", "tokens": ["Als", "der", "Be\u00b7red\u00b7sam\u00b7keit", "und", "hol\u00b7de", "Sit\u00b7ten", "schenckt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.43": {"text": "Wie hat nicht dieser Sohn der Mutter Hertz erfreuet", "tokens": ["Wie", "hat", "nicht", "die\u00b7ser", "Sohn", "der", "Mut\u00b7ter", "Hertz", "er\u00b7freu\u00b7et"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PTKNEG", "PDAT", "NN", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Wenn er den edlen Sinn den Musen zugelenckt?", "tokens": ["Wenn", "er", "den", "ed\u00b7len", "Sinn", "den", "Mu\u00b7sen", "zu\u00b7ge\u00b7lenckt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Wor\u00fcber sie mehr Lust und Liebligkeit empfunden/", "tokens": ["Wo\u00b7r\u00fc\u00b7ber", "sie", "mehr", "Lust", "und", "Lieb\u00b7lig\u00b7keit", "emp\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Als wenn durch den Smaragd der Nero Fechter schaut.", "tokens": ["Als", "wenn", "durch", "den", "Sma\u00b7ragd", "der", "Ne\u00b7ro", "Fech\u00b7ter", "schaut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "APPR", "ART", "NN", "ART", "NE", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.47": {"text": "Und als durch Spiegel siht die aufgeschlitzten Wunden/", "tokens": ["Und", "als", "durch", "Spie\u00b7gel", "siht", "die", "auf\u00b7ge\u00b7schlitz\u00b7ten", "Wun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Aus denen frisches Blut den Purpur-Regen thaut.", "tokens": ["Aus", "de\u00b7nen", "fri\u00b7sches", "Blut", "den", "Pur\u00b7pur\u00b7Re\u00b7gen", "thaut", "."], "token_info": ["word", "word",