{"dta.poem.9087": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hinter bl\u00fchenden Apfelbaumzweigen", "tokens": ["Hin\u00b7ter", "bl\u00fc\u00b7hen\u00b7den", "Ap\u00b7fel\u00b7baum\u00b7zwei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "steigt der Mond auf.", "tokens": ["steigt", "der", "Mond", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Zarte Ranken,", "tokens": ["Zar\u00b7te", "Ran\u00b7ken", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "blasse Schatten", "tokens": ["blas\u00b7se", "Schat\u00b7ten"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "zackt sein Schimmer in den Kies.", "tokens": ["zackt", "sein", "Schim\u00b7mer", "in", "den", "Kies", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Lautlos fliegt ein Falter.", "tokens": ["Laut\u00b7los", "fliegt", "ein", "Fal\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Ich strecke mich selig ins silberne Gras", "tokens": ["Ich", "stre\u00b7cke", "mich", "se\u00b7lig", "ins", "sil\u00b7ber\u00b7ne", "Gras"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "und liege da", "tokens": ["und", "lie\u00b7ge", "da"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "das Herz im Himmel!", "tokens": ["das", "Herz", "im", "Him\u00b7mel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}