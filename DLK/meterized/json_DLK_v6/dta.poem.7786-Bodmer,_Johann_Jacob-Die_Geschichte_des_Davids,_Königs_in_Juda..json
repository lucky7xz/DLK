{"dta.poem.7786": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Die Geschichte des Davids,  \n K\u00f6nigs in Juda.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198429", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In solcher meiner Qual, in Ada ihrem W\u00fcten,", "tokens": ["In", "sol\u00b7cher", "mei\u00b7ner", "Qual", ",", "in", "A\u00b7da", "ih\u00b7rem", "W\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$,", "APPR", "NE", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In Beroa Gefecht, die ihre Ehr zu h\u00fcten,", "tokens": ["In", "Be\u00b7roa", "Ge\u00b7fecht", ",", "die", "ih\u00b7re", "Ehr", "zu", "h\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,", "PRELS", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "That mehr als menschlich war, kommt da mein Bruder hin,", "tokens": ["That", "mehr", "als", "menschlich", "war", ",", "kommt", "da", "mein", "Bru\u00b7der", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "KOKOM", "ADJD", "VAFIN", "$,", "VVFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Mit zw\u00f6lfen seiner Freund, denck nur, wie froh ich bin.", "tokens": ["Mit", "zw\u00f6l\u00b7fen", "sei\u00b7ner", "Freund", ",", "denck", "nur", ",", "wie", "froh", "ich", "bin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPOSAT", "NN", "$,", "VVIMP", "ADV", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Beroa macht er lo\u00df, und Ada ihr Entsetzen", "tokens": ["Be\u00b7roa", "macht", "er", "lo\u00df", ",", "und", "A\u00b7da", "ihr", "Ent\u00b7set\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "NE", "PPOSAT", "NN"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "War unsrer Freude gleich, sie wollt sich widersetzen,", "tokens": ["War", "uns\u00b7rer", "Freu\u00b7de", "gleich", ",", "sie", "wollt", "sich", "wi\u00b7der\u00b7set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "$,", "PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mit ihrem Volck, das da von Joels Dienern war,", "tokens": ["Mit", "ih\u00b7rem", "Volck", ",", "das", "da", "von", "Joels", "Die\u00b7nern", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "ADV", "APPR", "NE", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Indeme Gera auch sich findet bey uns dar,", "tokens": ["In\u00b7de\u00b7me", "Ge\u00b7ra", "auch", "sich", "fin\u00b7det", "bey", "uns", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "PRF", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Er und Melchisua, die hatten uns verlohren,", "tokens": ["Er", "und", "Melc\u00b7hi\u00b7sua", ",", "die", "hat\u00b7ten", "uns", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "NE", "$,", "PRELS", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.10": {"text": "Und folgten nach der Spur, da sie wie neu gebohren", "tokens": ["Und", "folg\u00b7ten", "nach", "der", "Spur", ",", "da", "sie", "wie", "neu", "ge\u00b7boh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "KOKOM", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Uns funden beyderseits. Die Ada ward gebracht", "tokens": ["Uns", "fun\u00b7den", "bey\u00b7der\u00b7seits", ".", "Die", "A\u00b7da", "ward", "ge\u00b7bracht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$.", "ART", "NE", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "In Haft, wir aber beyd hingegen frey gemacht.", "tokens": ["In", "Haft", ",", "wir", "a\u00b7ber", "beyd", "hin\u00b7ge\u00b7gen", "frey", "ge\u00b7macht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PPER", "ADV", "PIS", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Des Joels Knecht, die da verrathen ihren Herren,", "tokens": ["Des", "Joels", "Knecht", ",", "die", "da", "ver\u00b7ra\u00b7then", "ih\u00b7ren", "Her\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "$,", "PRELS", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.14": {"text": "Setzt man gleich zu der Red, da was man wollt begehren,", "tokens": ["Setzt", "man", "gleich", "zu", "der", "Red", ",", "da", "was", "man", "wollt", "be\u00b7geh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "APPR", "ART", "NN", "$,", "KOUS", "PWS", "PIS", "VMFIN", "VVINF", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "Uns ward von ihnen kund, auch sagte Ada an", "tokens": ["Uns", "ward", "von", "ih\u00b7nen", "kund", ",", "auch", "sag\u00b7te", "A\u00b7da", "an"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "NE", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Was schier niemand von uns f\u00fcr Schreken glauben kan;", "tokens": ["Was", "schier", "nie\u00b7mand", "von", "uns", "f\u00fcr", "Schre\u00b7ken", "glau\u00b7ben", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PIS", "APPR", "PPER", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wie nemlich Joel h\u00e4tt sie mit Gewalt gesch\u00e4ndet,", "tokens": ["Wie", "nem\u00b7lich", "Joel", "h\u00e4tt", "sie", "mit", "Ge\u00b7walt", "ge\u00b7sch\u00e4n\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NE", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.18": {"text": "Als sie zu Bersaba, und ihr viel zugewendet", "tokens": ["Als", "sie", "zu", "Ber\u00b7sa\u00b7ba", ",", "und", "ihr", "viel", "zu\u00b7ge\u00b7wen\u00b7det"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NE", "$,", "KON", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Von G\u00fctern, da\u00df sie sollt ja halten reinen Mund,", "tokens": ["Von", "G\u00fc\u00b7tern", ",", "da\u00df", "sie", "sollt", "ja", "hal\u00b7ten", "rei\u00b7nen", "Mund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUS", "PPER", "VMFIN", "ADV", "VVINF", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wie Gera nun von ihr zu melden was begunt;", "tokens": ["Wie", "Ge\u00b7ra", "nun", "von", "ihr", "zu", "mel\u00b7den", "was", "be\u00b7gunt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "PWS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wiewohl mit Adriel er f\u00e4lschlich sie betrogen,", "tokens": ["Wie\u00b7wohl", "mit", "Ad\u00b7riel", "er", "f\u00e4lschlich", "sie", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NE", "PPER", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "H\u00e4tt sie sich diese That schwer zu Gem\u00fcth gezogen,", "tokens": ["H\u00e4tt", "sie", "sich", "die\u00b7se", "That", "schwer", "zu", "Ge\u00b7m\u00fcth", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PDAT", "NN", "ADJD", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Doch w\u00e4r von Joel ihr ein guter Muth gemacht,", "tokens": ["Doch", "w\u00e4r", "von", "Joel", "ihr", "ein", "gu\u00b7ter", "Muth", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NE", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Nach dessen Rath und Schlu\u00df sie alles so vollbracht.", "tokens": ["Nach", "des\u00b7sen", "Rath", "und", "Schlu\u00df", "sie", "al\u00b7les", "so", "voll\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "KON", "NN", "PPER", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Dem Adriel zur Plag und Joel zu Gefallen", "tokens": ["Dem", "Ad\u00b7riel", "zur", "Plag", "und", "Joel", "zu", "Ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "KON", "NE", "APPR", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "H\u00e4tt sie die\u00df falsch Ger\u00fccht von ihnen lassen schallen;", "tokens": ["H\u00e4tt", "sie", "die\u00df", "falsch", "Ge\u00b7r\u00fccht", "von", "ih\u00b7nen", "las\u00b7sen", "schal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "ADJD", "NN", "APPR", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und weil sie Joel drauf aus ihrer Haft befreyt,", "tokens": ["Und", "weil", "sie", "Joel", "drauf", "aus", "ih\u00b7rer", "Haft", "be\u00b7freyt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NE", "PAV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "Und heimlich sie hieher in dieses Haus begleit,", "tokens": ["Und", "heim\u00b7lich", "sie", "hie\u00b7her", "in", "die\u00b7ses", "Haus", "be\u00b7gleit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "PAV", "APPR", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Allda er ihrer meynt recht wieder zu geniessen,", "tokens": ["All\u00b7da", "er", "ih\u00b7rer", "meynt", "recht", "wie\u00b7der", "zu", "ge\u00b7nies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "VVFIN", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "H\u00e4tt sie ihm ihren Schlu\u00df f\u00fcrhero lassen wissen,", "tokens": ["H\u00e4tt", "sie", "ihm", "ih\u00b7ren", "Schlu\u00df", "f\u00fcr\u00b7he\u00b7ro", "las\u00b7sen", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PPOSAT", "NN", "ADV", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Da\u00df wann sie Beroa in solchen Spott gebracht,", "tokens": ["Da\u00df", "wann", "sie", "Be\u00b7roa", "in", "sol\u00b7chen", "Spott", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "PPER", "NE", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.32": {"text": "Als wie ihr Vater sie zu Schanden h\u00e4tt gemacht,", "tokens": ["Als", "wie", "ihr", "Va\u00b7ter", "sie", "zu", "Schan\u00b7den", "h\u00e4tt", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "PPOSAT", "NN", "PPER", "APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wollt sie gantz williglich dann seines Willens leben,", "tokens": ["Wollt", "sie", "gantz", "wil\u00b7lig\u00b7lich", "dann", "sei\u00b7nes", "Wil\u00b7lens", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Daher man die\u00df gethan, was ihr m\u00f6cht Ruhe geben.", "tokens": ["Da\u00b7her", "man", "die\u00df", "ge\u00b7than", ",", "was", "ihr", "m\u00f6cht", "Ru\u00b7he", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIS", "PDS", "VVPP", "$,", "PWS", "PPER", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Hierauf der Gera sagt, er h\u00e4tt sie einst geschaut", "tokens": ["Hier\u00b7auf", "der", "Ge\u00b7ra", "sagt", ",", "er", "h\u00e4tt", "sie", "einst", "ge\u00b7schaut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "In ihrer geilen Lust, und als er es getraut", "tokens": ["In", "ih\u00b7rer", "gei\u00b7len", "Lust", ",", "und", "als", "er", "es", "ge\u00b7traut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "KON", "KOUS", "PPER", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Dem Richter kund zu thun, der doch selbst Schuld gewesen,", "tokens": ["Dem", "Rich\u00b7ter", "kund", "zu", "thun", ",", "der", "doch", "selbst", "Schuld", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "PTKZU", "VVINF", "$,", "PRELS", "ADV", "ADV", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "H\u00e4tt er den Adrtel zu dieser That erlesen,", "tokens": ["H\u00e4tt", "er", "den", "Adr\u00b7tel", "zu", "die\u00b7ser", "That", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.39": {"text": "Und selber ihm bekannt, da\u00df er es gar wohl w\u00fcst,", "tokens": ["Und", "sel\u00b7ber", "ihm", "be\u00b7kannt", ",", "da\u00df", "er", "es", "gar", "wohl", "w\u00fcst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "ADJD", "$,", "KOUS", "PPER", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Darauf er diese Klag auf sein Gehei\u00df gem\u00fcst", "tokens": ["Da\u00b7rauf", "er", "die\u00b7se", "Klag", "auf", "sein", "Ge\u00b7hei\u00df", "ge\u00b7m\u00fcst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "PDAT", "NN", "APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Anmelden \u00f6ffentlich, das er nun wollt bereuen,", "tokens": ["An\u00b7mel\u00b7den", "\u00f6f\u00b7fent\u00b7lich", ",", "das", "er", "nun", "wollt", "be\u00b7reu\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PRELS", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "+--+---+-+-+-", "measure": "dactylic.di.plus"}, "line.42": {"text": "Weil Joel ihn verf\u00fchrt. Wie ich mich that erfreuen,", "tokens": ["Weil", "Joel", "ihn", "ver\u00b7f\u00fchrt", ".", "Wie", "ich", "mich", "that", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPER", "VVPP", "$.", "PWAV", "PPER", "PRF", "VVFIN", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.43": {"text": "Da\u00df Adriel ohn Schuld, ist zu erzehlen nicht.", "tokens": ["Da\u00df", "Ad\u00b7riel", "ohn", "Schuld", ",", "ist", "zu", "er\u00b7zeh\u00b7len", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "$,", "VAFIN", "PTKZU", "VVINF", "PTKNEG", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}}}}