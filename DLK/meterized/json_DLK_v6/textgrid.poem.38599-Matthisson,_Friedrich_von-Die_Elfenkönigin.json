{"textgrid.poem.38599": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die Elfenk\u00f6nigin", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was unterm Monde gleicht", "tokens": ["Was", "un\u00b7term", "Mon\u00b7de", "gleicht"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPRART", "NE", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Uns Elfen flink und leicht?", "tokens": ["Uns", "El\u00b7fen", "flink", "und", "leicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir spiegeln uns im Thau", "tokens": ["Wir", "spie\u00b7geln", "uns", "im", "Thau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der sternenhellen Au,", "tokens": ["Der", "ster\u00b7nen\u00b7hel\u00b7len", "Au", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wir tanzen auf des Baches Moos',", "tokens": ["Wir", "tan\u00b7zen", "auf", "des", "Ba\u00b7ches", "Moos'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wir wiegen uns am Fr\u00fchlingsspro\u00df,", "tokens": ["Wir", "wie\u00b7gen", "uns", "am", "Fr\u00fch\u00b7lings\u00b7spro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und ruhn in weicher Blumen Schoo\u00df!", "tokens": ["Und", "ruhn", "in", "wei\u00b7cher", "Blu\u00b7men", "Schoo\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihr Elfen, auf den H\u00f6h'n!", "tokens": ["Ihr", "El\u00b7fen", ",", "auf", "den", "H\u00f6h'n", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Elfen, an den See'n,", "tokens": ["Ihr", "El\u00b7fen", ",", "an", "den", "See'n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zum thaubeperlten Gr\u00fcn", "tokens": ["Zum", "thau\u00b7be\u00b7perl\u00b7ten", "Gr\u00fcn"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Folgt eurer K\u00f6nigin!", "tokens": ["Folgt", "eu\u00b7rer", "K\u00f6\u00b7ni\u00b7gin", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Im grauen Mettenf\u00e4dleinkranz,", "tokens": ["Im", "grau\u00b7en", "Met\u00b7ten\u00b7f\u00e4d\u00b7lein\u00b7kranz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Umflimmert von des Gl\u00fchwurms Glanz,", "tokens": ["Um\u00b7flim\u00b7mert", "von", "des", "Gl\u00fch\u00b7wurms", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Herbei! herbei! zum Mondscheintanz!", "tokens": ["Her\u00b7bei", "!", "her\u00b7bei", "!", "zum", "Mond\u00b7schein\u00b7tanz", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKVZ", "$.", "PTKVZ", "$.", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein Schleier, wei\u00df und fein,", "tokens": ["Ein", "Schlei\u00b7er", ",", "wei\u00df", "und", "fein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Gebleicht im Sternenschein", "tokens": ["Ge\u00b7bleicht", "im", "Ster\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf k\u00fchler Todtengruft,", "tokens": ["Auf", "k\u00fch\u00b7ler", "Tod\u00b7ten\u00b7gruft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Umwall' euch leicht wie Duft!", "tokens": ["Um\u00b7wall'", "euch", "leicht", "wie", "Duft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Durch Moos und Schilf, durch Korn und Hain,", "tokens": ["Durch", "Moos", "und", "Schilf", ",", "durch", "Korn", "und", "Hain", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bergauf, thalab, waldaus, feldein,", "tokens": ["Ber\u00b7gauf", ",", "tha\u00b7lab", ",", "wald\u00b7aus", ",", "feld\u00b7ein", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "PWAV", "$,", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Herbei! herbei! zum Ringelreihn!", "tokens": ["Her\u00b7bei", "!", "her\u00b7bei", "!", "zum", "Rin\u00b7gel\u00b7reihn", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKVZ", "$.", "PTKVZ", "$.", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Beim Sommermondscheinball,", "tokens": ["Beim", "Som\u00b7mer\u00b7mond\u00b7schein\u00b7ball", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Am Quell im Erlenthal,", "tokens": ["Am", "Quell", "im", "Er\u00b7len\u00b7thal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Umschleiert unser Chor", "tokens": ["Um\u00b7schlei\u00b7ert", "un\u00b7ser", "Chor"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein weisser Nebelflor;", "tokens": ["Ein", "weis\u00b7ser", "Ne\u00b7bel\u00b7flor", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wir kreisen schnell, wir schweben leicht,", "tokens": ["Wir", "krei\u00b7sen", "schnell", ",", "wir", "schwe\u00b7ben", "leicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein finstres Gnomenheer entsteigt", "tokens": ["Ein", "finst\u00b7res", "Gno\u00b7me\u00b7nheer", "ent\u00b7steigt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dem Erdenschoo\u00df und harft und geigt!", "tokens": ["Dem", "Er\u00b7den\u00b7schoo\u00df", "und", "harft", "und", "geigt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Das Mark vom Schmetterling", "tokens": ["Das", "Mark", "vom", "Schmet\u00b7ter\u00b7ling"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Den eine Jungfrau fieng,", "tokens": ["Den", "ei\u00b7ne", "Jung\u00b7frau", "fi\u00b7eng", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das Hirn der Nachtigall", "tokens": ["Das", "Hirn", "der", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Bereiten wir zum Mahl,", "tokens": ["Be\u00b7rei\u00b7ten", "wir", "zum", "Mahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und schl\u00fcrfen, unter Rundgesang", "tokens": ["Und", "schl\u00fcr\u00b7fen", ",", "un\u00b7ter", "Rund\u00b7ge\u00b7sang"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$,", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Fl\u00f6tenton und Harfenklang,", "tokens": ["Und", "Fl\u00f6\u00b7ten\u00b7ton", "und", "Har\u00b7fen\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Aus Blumenkelchen G\u00f6ttertrank!", "tokens": ["Aus", "Blu\u00b7men\u00b7kel\u00b7chen", "G\u00f6t\u00b7ter\u00b7trank", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Herbei! herbei! zum Tanz", "tokens": ["Her\u00b7bei", "!", "her\u00b7bei", "!", "zum", "Tanz"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["PTKVZ", "$.", "PTKVZ", "$.", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Im Mettenf\u00e4dleinkranz!", "tokens": ["Im", "Met\u00b7ten\u00b7f\u00e4d\u00b7lein\u00b7kranz", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schnell rollt der Elfen Kreis", "tokens": ["Schnell", "rollt", "der", "El\u00b7fen", "Kreis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Im zirkelrunden Gleis!", "tokens": ["Im", "zir\u00b7kel\u00b7run\u00b7den", "Gleis", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wo ist ein Fu\u00df der nimmer glitt?", "tokens": ["Wo", "ist", "ein", "Fu\u00df", "der", "nim\u00b7mer", "glitt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ART", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wir Elfen fliehn mit Zephyrschritt,", "tokens": ["Wir", "El\u00b7fen", "fliehn", "mit", "Ze\u00b7phyr\u00b7schritt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kein Gr\u00e4schen beuget unser Tritt!", "tokens": ["Kein", "Gr\u00e4sc\u00b7hen", "beu\u00b7get", "un\u00b7ser", "Tritt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}