{"textgrid.poem.36700": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Genzano", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6stlichen Wein, du findest ihn hier, und junonische Frauen,", "tokens": ["K\u00f6st\u00b7li\u00b7chen", "Wein", ",", "du", "fin\u00b7dest", "ihn", "hier", ",", "und", "ju\u00b7no\u00b7ni\u00b7sche", "Frau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,", "KON", "ADJA", "NN", "$,"], "meter": "+--+-+--+-+-+-+-", "measure": "iambic.septa.invert"}, "line.2": {"text": "Artemis floh, doch sie lie\u00df uns ihre Nymphen zur\u00fcck.", "tokens": ["Ar\u00b7te\u00b7mis", "floh", ",", "doch", "sie", "lie\u00df", "uns", "ih\u00b7re", "Nym\u00b7phen", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "KON", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Gl\u00fccklicher Wanderer, wenn am Blumenfeste der G\u00f6ttin", "tokens": ["Gl\u00fcck\u00b7li\u00b7cher", "Wan\u00b7de\u00b7rer", ",", "wenn", "am", "Blu\u00b7men\u00b7fes\u00b7te", "der", "G\u00f6t\u00b7tin"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "KOUS", "APPRART", "NN", "ART", "NN"], "meter": "+--+-++-+-+--+-", "measure": "iambic.septa.invert"}, "line.4": {"text": "Venus auch dir einen Kranz duftiger Rosen bescheert.", "tokens": ["Ve\u00b7nus", "auch", "dir", "ei\u00b7nen", "Kranz", "duf\u00b7ti\u00b7ger", "Ro\u00b7sen", "be\u00b7scheert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPER", "ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+--+--+", "measure": "amphibrach.penta.plus"}}}}}