{"dta.poem.2600": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Trauer-Gedancken \u00fcber dem Absterben/  \n  Fr. H. W. g. S. den 11. Jan.  1664.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer so ein grosses Leid und so ergrimmte Schmertzen", "tokens": ["Wer", "so", "ein", "gros\u00b7ses", "Leid", "und", "so", "er\u00b7grimm\u00b7te", "Schmert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ART", "ADJA", "NN", "KON", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit s\u00fcssen Worten nur zu \u00fcberzuckern denckt/", "tokens": ["Mit", "s\u00fcs\u00b7sen", "Wor\u00b7ten", "nur", "zu", "\u00fc\u00b7ber\u00b7zu\u00b7ckern", "denckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der wei\u00df noch nicht wie tieff sie schneiden in die Hertzen", "tokens": ["Der", "wei\u00df", "noch", "nicht", "wie", "tieff", "sie", "schnei\u00b7den", "in", "die", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "PTKNEG", "KOKOM", "ADJD", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ist ein Cynicus/ den Stein und Stahl umb-", "tokens": ["Und", "ist", "ein", "Cy\u00b7ni\u00b7cus", "/", "den", "Stein", "und", "Stahl", "um\u00b7b"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NE", "$(", "ART", "NN", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Brust mu\u00df Rhodope/ das Blut ein Kiesel werden/", "tokens": ["Die", "Brust", "mu\u00df", "Rho\u00b7do\u00b7pe", "/", "das", "Blut", "ein", "Kie\u00b7sel", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "NE", "$(", "ART", "NN", "ART", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Eh man empfindlich-seyn au\u00df unsrem Leben nimmt/", "tokens": ["Eh", "man", "emp\u00b7find\u00b7lich\u00b7seyn", "au\u00df", "uns\u00b7rem", "Le\u00b7ben", "nimmt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Mensch sey wie er will/ er tr\u00e4gt was mehr als Erden/", "tokens": ["Der", "Mensch", "sey", "wie", "er", "will", "/", "er", "tr\u00e4gt", "was", "mehr", "als", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "PPER", "VMFIN", "$(", "PPER", "VVFIN", "PWS", "PIS", "KOKOM", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Geist ist zart und f\u00fchlts so bald ein Wetter k\u00f6mmt.", "tokens": ["Der", "Geist", "ist", "zart", "und", "f\u00fchlts", "so", "bald", "ein", "Wet\u00b7ter", "k\u00f6mmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "VVFIN", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Gantz unerschrocken stehn wenn schwartze Donner krachen/", "tokens": ["Gantz", "un\u00b7er\u00b7schro\u00b7cken", "stehn", "wenn", "schwart\u00b7ze", "Don\u00b7ner", "kra\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "KOUS", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Kommt einem harten Fels nicht einem Weisen zu;", "tokens": ["Kommt", "ei\u00b7nem", "har\u00b7ten", "Fels", "nicht", "ei\u00b7nem", "Wei\u00b7sen", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKNEG", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wer in dem Hafen liegt/ der kan der Winde lachen/", "tokens": ["Wer", "in", "dem", "Ha\u00b7fen", "liegt", "/", "der", "kan", "der", "Win\u00b7de", "la\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$(", "ART", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Nicht aber der da irrt in Fluthen sonder Ruh.", "tokens": ["Nicht", "a\u00b7ber", "der", "da", "irrt", "in", "Flut\u00b7hen", "son\u00b7der", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "ADV", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Hochwerthster/ da\u00df wir sehn jetzt Thr\u00e4n\u2019 auf Thr\u00e4nen st\u00fcrtzen/", "tokens": ["Hoch\u00b7werths\u00b7ter", "/", "da\u00df", "wir", "sehn", "jetzt", "Thr\u00e4n'", "auf", "Thr\u00e4\u00b7nen", "st\u00fcrt\u00b7zen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PPER", "VVFIN", "ADV", "NN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df unersch\u00f6pfftes Weh die Seel\u2019 in St\u00fccke bricht/", "tokens": ["Da\u00df", "un\u00b7er\u00b7sch\u00f6pff\u00b7tes", "Weh", "die", "Seel'", "in", "St\u00fc\u00b7cke", "bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und denckt der Jahre Rest bey solcher Angst zu k\u00fcrtzen/", "tokens": ["Und", "denckt", "der", "Jah\u00b7re", "Rest", "bey", "sol\u00b7cher", "Angst", "zu", "k\u00fcrt\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "APPR", "PIAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Trifft ein mit der Vernunfft und ist zu wundern nicht.", "tokens": ["Trifft", "ein", "mit", "der", "Ver\u00b7nunfft", "und", "ist", "zu", "wun\u00b7dern", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "APPR", "ART", "NN", "KON", "VAFIN", "PTKZU", "VVINF", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Di\u00df was man hertzlich liebt/ mu\u00df man nur sehnlich klagen/", "tokens": ["Di\u00df", "was", "man", "hertz\u00b7lich", "liebt", "/", "mu\u00df", "man", "nur", "sehn\u00b7lich", "kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRELS", "PIS", "ADJD", "VVFIN", "$(", "VMFIN", "PIS", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Je gr\u00f6sser der Verlust/ je herber ist das Leid/", "tokens": ["Je", "gr\u00f6s\u00b7ser", "der", "Ver\u00b7lust", "/", "je", "her\u00b7ber", "ist", "das", "Leid", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$(", "ADV", "ADV", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Geringe Schmertzen sind gro\u00dfm\u00fctig zu ertragen/", "tokens": ["Ge\u00b7rin\u00b7ge", "Schmert\u00b7zen", "sind", "gro\u00df\u00b7m\u00fc\u00b7tig", "zu", "er\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Den Sto\u00df so ihn jetzt trifft heilt nicht die lange Zeit.", "tokens": ["Den", "Sto\u00df", "so", "ihn", "jetzt", "trifft", "heilt", "nicht", "die", "lan\u00b7ge", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "ADV", "VVFIN", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Gold/ G\u00fcter zu verliern/ das Tockenwerck des Gl\u00fcckes/", "tokens": ["Gold", "/", "G\u00fc\u00b7ter", "zu", "ver\u00b7li\u00b7ern", "/", "das", "To\u00b7cken\u00b7werck", "des", "Gl\u00fc\u00b7ckes", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "PTKZU", "VVINF", "$(", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Und selbst zerfallen sehn der Ehren schimmernd Gla\u00df/", "tokens": ["Und", "selbst", "zer\u00b7fal\u00b7len", "sehn", "der", "Eh\u00b7ren", "schim\u00b7mernd", "Gla\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "VVFIN", "ART", "NN", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Kr\u00e4nckt. Doch wenn weg geraubt vom Hauptschlu\u00df des Geschickes/", "tokens": ["Kr\u00e4nckt", ".", "Doch", "wenn", "weg", "ge\u00b7raubt", "vom", "Haupt\u00b7schlu\u00df", "des", "Ge\u00b7schi\u00b7ckes", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "KON", "KOUS", "ADV", "VVPP", "APPRART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die Seele so man liebt/ denn wird das Aug\u2019 erstna\u00df.", "tokens": ["Die", "See\u00b7le", "so", "man", "liebt", "/", "denn", "wird", "das", "Aug'", "erst\u00b7na\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PIS", "VVFIN", "$(", "KON", "VAFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und soll ich weiter noch gefirnster Reden brauchen?", "tokens": ["Und", "soll", "ich", "wei\u00b7ter", "noch", "ge\u00b7firns\u00b7ter", "Re\u00b7den", "brau\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Nein; seines Hauses Sonn\u2019 und Morgen-R\u00f6th\u2019 ist hin/", "tokens": ["Nein", ";", "sei\u00b7nes", "Hau\u00b7ses", "Sonn'", "und", "Mor\u00b7gen\u00b7R\u00f6\u00b7th'", "ist", "hin", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPOSAT", "NN", "NN", "KON", "NN", "VAFIN", "ADV", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Wo Anmuth vor gespielt/ da m\u00fcssen Lichter rauchen/", "tokens": ["Wo", "An\u00b7muth", "vor", "ge\u00b7spielt", "/", "da", "m\u00fcs\u00b7sen", "Lich\u00b7ter", "rau\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "VVPP", "$(", "ADV", "VMFIN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Die vor der schwartzen Bahr das Trauren an heist gl\u00fchn.", "tokens": ["Die", "vor", "der", "schwart\u00b7zen", "Bahr", "das", "Trau\u00b7ren", "an", "heist", "gl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "ART", "NN", "APPR", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wer leugnet/ da\u00df ihm nicht Cometen jetzt geschienen?", "tokens": ["Wer", "leug\u00b7net", "/", "da\u00df", "ihm", "nicht", "Co\u00b7me\u00b7ten", "jetzt", "ge\u00b7schie\u00b7nen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$(", "KOUS", "PPER", "PTKNEG", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Da gleich der Wunder Stern die sch\u00fcchtre Welt erschreckt;", "tokens": ["Da", "gleich", "der", "Wun\u00b7der", "Stern", "die", "sch\u00fcch\u00b7tre", "Welt", "er\u00b7schreckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Kan nicht der Jammer-Blick zu einer Warnung dienen/", "tokens": ["Kan", "nicht", "der", "Jam\u00b7mer\u00b7Blick", "zu", "ei\u00b7ner", "War\u00b7nung", "die\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Was k\u00fcnfftig \u00fcber uns des H\u00f6chsten Zorn erweckt.", "tokens": ["Was", "k\u00fcnff\u00b7tig", "\u00fc\u00b7ber", "uns", "des", "H\u00f6chs\u00b7ten", "Zorn", "er\u00b7weckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "APPR", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Und freylich ist sein Leid gedoppelt zu erwegen/", "tokens": ["Und", "frey\u00b7lich", "ist", "sein", "Leid", "ge\u00b7dop\u00b7pelt", "zu", "er\u00b7we\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Was einfach ist das dr\u00fcckt/ geh\u00e4uffte Last doch mehr.", "tokens": ["Was", "ein\u00b7fach", "ist", "das", "dr\u00fcckt", "/", "ge\u00b7h\u00e4uff\u00b7te", "Last", "doch", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "PDS", "VVFIN", "$(", "ADJA", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Die Mutter mit der Frucht zugleich zu Grabe legen/", "tokens": ["Die", "Mut\u00b7ter", "mit", "der", "Frucht", "zu\u00b7gleich", "zu", "Gra\u00b7be", "le\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ADV", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Prest bittre Thr\u00e4nen au\u00df/ und schmertzt mehr allzu sehr.", "tokens": ["Prest", "bitt\u00b7re", "Thr\u00e4\u00b7nen", "au\u00df", "/", "und", "schmertzt", "mehr", "all\u00b7zu", "sehr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PTKVZ", "$(", "KON", "VVFIN", "ADV", "PTKA", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "De\u00df Lebens Eingang ist durchgehend allgemeine/", "tokens": ["De\u00df", "Le\u00b7bens", "Ein\u00b7gang", "ist", "durch\u00b7ge\u00b7hend", "all\u00b7ge\u00b7mei\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJD", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und tausend Arten sind wodurch der Tod uns f\u00e4llt.", "tokens": ["Und", "tau\u00b7send", "Ar\u00b7ten", "sind", "wo\u00b7durch", "der", "Tod", "uns", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "VAFIN", "PWAV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Die Mutter wird ein Grab/ die Scho\u00df zum Leichen-Steine/", "tokens": ["Die", "Mut\u00b7ter", "wird", "ein", "Grab", "/", "die", "Scho\u00df", "zum", "Lei\u00b7chen\u00b7Stei\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$(", "ART", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexa