{"textgrid.poem.54523": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer leugnets? sie sind voll/ und wissen selber nicht", "tokens": ["Wer", "leug\u00b7nets", "?", "sie", "sind", "voll", "/", "und", "wis\u00b7sen", "sel\u00b7ber", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$.", "PPER", "VAFIN", "ADJD", "$(", "KON", "VVFIN", "ADV", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie ihnen sey geschehn. Der vorhin schier verzaget", "tokens": ["Wie", "ih\u00b7nen", "sey", "ge\u00b7schehn", ".", "Der", "vor\u00b7hin", "schier", "ver\u00b7za\u00b7get"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VAFIN", "VVPP", "$.", "ART", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vor einer blossen Magd/ hat sich ins Volck gewaget/", "tokens": ["Vor", "ei\u00b7ner", "blos\u00b7sen", "Magd", "/", "hat", "sich", "ins", "Volck", "ge\u00b7wa\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$(", "VAFIN", "PRF", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Darunter redend er drey tau\u00dfend Hertzen bricht.", "tokens": ["Da\u00b7run\u00b7ter", "re\u00b7dend", "er", "drey", "tau\u00b7\u00dfend", "Hert\u00b7zen", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVPP", "PPER", "CARD", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der keine Schrifft gelernt versteht was ", "tokens": ["Der", "kei\u00b7ne", "Schrifft", "ge\u00b7lernt", "ver\u00b7steht", "was"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVPP", "VVFIN", "PWS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Erkl\u00e4rt was David l\u00e4ngst von Jesu vor gesaget;", "tokens": ["Er\u00b7kl\u00e4rt", "was", "Da\u00b7vid", "l\u00e4ngst", "von", "Je\u00b7su", "vor", "ge\u00b7sa\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PWS", "NE", "ADV", "APPR", "NE", "APPR", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Zw\u00f6lffe freuen sich indem man sie verklaget/", "tokens": ["Die", "Zw\u00f6lf\u00b7fe", "freu\u00b7en", "sich", "in\u00b7dem", "man", "sie", "ver\u00b7kla\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "KOUS", "PIS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und \u00fcber sie die Schul hegt blutig Hals-Gericht.", "tokens": ["Und", "\u00fc\u00b7ber", "sie", "die", "Schul", "hegt", "blu\u00b7tig", "Hals\u00b7Ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ART", "NN", "VVFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie ringen nach dem Tod und hassen Stand und Leben/", "tokens": ["Sie", "rin\u00b7gen", "nach", "dem", "Tod", "und", "has\u00b7sen", "Stand", "und", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KON", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sie sch\u00e4tzen Schmach f\u00fcr Ehr und Kercker f\u00fcr Pall\u00e4st/", "tokens": ["Sie", "sch\u00e4t\u00b7zen", "Schmach", "f\u00fcr", "Ehr", "und", "Ker\u00b7cker", "f\u00fcr", "Pal\u00b7l\u00e4st", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN", "KON", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sie achten Gold f\u00fcr Koth und St\u00fcrtzen f\u00fcr Erheben;", "tokens": ["Sie", "ach\u00b7ten", "Gold", "f\u00fcr", "Koth", "und", "St\u00fcrt\u00b7zen", "f\u00fcr", "Er\u00b7he\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "NN", "KON", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Sind B\u00fcrger anderwerts/ im Vaterlande G\u00e4st/", "tokens": ["Sind", "B\u00fcr\u00b7ger", "an\u00b7der\u00b7werts", "/", "im", "Va\u00b7ter\u00b7lan\u00b7de", "G\u00e4st", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "$(", "APPRART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und sollen/ ", "tokens": ["Und", "sol\u00b7len", "/"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VMFIN", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Wo aber keltert man so s\u00fc\u00df und starcken Wein.", "tokens": ["Wo", "a\u00b7ber", "kel\u00b7tert", "man", "so", "s\u00fc\u00df", "und", "star\u00b7cken", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PIS", "ADV", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}