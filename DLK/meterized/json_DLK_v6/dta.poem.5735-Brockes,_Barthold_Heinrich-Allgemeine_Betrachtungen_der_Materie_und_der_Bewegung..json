{"dta.poem.5735": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Allgemeine Betrachtungen der Materie  \n und der Bewegung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man hatte wohl gefast, da\u00df die Materie", "tokens": ["Man", "hat\u00b7te", "wohl", "ge\u00b7fast", ",", "da\u00df", "die", "Ma\u00b7te\u00b7rie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VAFIN", "ADV", "VVPP", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein allgemeiner Grund zu allen C\u00f6rpern sey,", "tokens": ["Ein", "all\u00b7ge\u00b7mei\u00b7ner", "Grund", "zu", "al\u00b7len", "C\u00f6r\u00b7pern", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn man in tieffem Sinn sie nennt: Die ", "tokens": ["Wenn", "man", "in", "tief\u00b7fem", "Sinn", "sie", "nennt", ":", "Die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PIS", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$.", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus ungeformter Erden", "tokens": ["Aus", "un\u00b7ge\u00b7form\u00b7ter", "Er\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Geschickt zu allerley,", "tokens": ["Ge\u00b7schickt", "zu", "al\u00b7ler\u00b7ley", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PIAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und, ob sie gleich noch nichts, doch allerley zu werden.", "tokens": ["Und", ",", "ob", "sie", "gleich", "noch", "nichts", ",", "doch", "al\u00b7ler\u00b7ley", "zu", "wer\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV", "ADV", "PIS", "$,", "ADV", "PIS", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So wie ein roher Thon, der mancherley Gestallten", "tokens": ["So", "wie", "ein", "ro\u00b7her", "Thon", ",", "der", "man\u00b7cher\u00b7ley", "Ge\u00b7stall\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NN", "$,", "PRELS", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Durch eines K\u00fcnstlers Hand von gleicher Weis\u2019 erhalten.", "tokens": ["Durch", "ei\u00b7nes", "K\u00fcnst\u00b7lers", "Hand", "von", "glei\u00b7cher", "Weis'", "er\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der edlen Dichter Witz hatt\u2019 allzu wohl gefasst:", "tokens": ["Der", "ed\u00b7len", "Dich\u00b7ter", "Witz", "hatt'", "all\u00b7zu", "wohl", "ge\u00b7fasst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VAFIN", "PTKA", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Es sey der Stoff, so lang\u2019 er in der Rast", "tokens": ["Es", "sey", "der", "Stoff", ",", "so", "lang'", "er", "in", "der", "Rast"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ADV", "ADV", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und unbeweget w\u00e4r,", "tokens": ["Und", "un\u00b7be\u00b7we\u00b7get", "w\u00e4r", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Ein\u2019 Ursach sonder Folg\u2019, ein Grund, von Nutzen leer,", "tokens": ["Ein'", "Ur\u00b7sach", "son\u00b7der", "Fol\u00b7g'", ",", "ein", "Grund", ",", "von", "Nut\u00b7zen", "leer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,", "ART", "NN", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Und da\u00df Bewegung blos die Ordnung, Zier und Leben", "tokens": ["Und", "da\u00df", "Be\u00b7we\u00b7gung", "blos", "die", "Ord\u00b7nung", ",", "Zier", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "NN", "ADV", "ART", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Jhm m\u00fcsse geben.", "tokens": ["Jhm", "m\u00fcs\u00b7se", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Sie sungen: Da\u00df die dunckle Nacht", "tokens": ["Sie", "sun\u00b7gen", ":", "Da\u00df", "die", "dunck\u00b7le", "Nacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Der Liebe Mutter sey, des Ursprungs aller G\u00f6tter,", "tokens": ["Der", "Lie\u00b7be", "Mut\u00b7ter", "sey", ",", "des", "Ur\u00b7sprungs", "al\u00b7ler", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "$,", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die alle Ding\u2019 hervorgebracht,", "tokens": ["Die", "al\u00b7le", "Ding'", "her\u00b7vor\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Und allenthalben strahl\u2019: Da\u00df die aus jener stamme.", "tokens": ["Und", "al\u00b7len\u00b7thal\u00b7ben", "strahl'", ":", "Da\u00df", "die", "aus", "je\u00b7ner", "stam\u00b7me", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$.", "KOUS", "ART", "APPR", "PDAT", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Sie sagten: Da\u00df derselben s\u00fcsse Flamme", "tokens": ["Sie", "sag\u00b7ten", ":", "Da\u00df", "der\u00b7sel\u00b7ben", "s\u00fcs\u00b7se", "Flam\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "KOUS", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Voll Fruchtbarkeit,", "tokens": ["Voll", "Frucht\u00b7bar\u00b7keit", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.21": {"text": "Aus Chaos gantz verworrner Dunckelheit,", "tokens": ["Aus", "Chaos", "gantz", "ver\u00b7worr\u00b7ner", "Dun\u00b7ckel\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Die Welt befreyt;", "tokens": ["Die", "Welt", "be\u00b7freyt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.23": {"text": "Und, durch ein gl\u00fcckliches Gesetz, die Kette", "tokens": ["Und", ",", "durch", "ein", "gl\u00fcck\u00b7li\u00b7ches", "Ge\u00b7setz", ",", "die", "Ket\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "APPR", "ART", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Der Elementen wohl verbunden h\u00e4tte;", "tokens": ["Der", "E\u00b7le\u00b7men\u00b7ten", "wohl", "ver\u00b7bun\u00b7den", "h\u00e4t\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "VAFIN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.25": {"text": "Da\u00df ihrer Fackel Strahl das Licht h\u00e4tt\u2019 angesteckt.", "tokens": ["Da\u00df", "ih\u00b7rer", "Fa\u00b7ckel", "Strahl", "das", "Licht", "h\u00e4tt'", "an\u00b7ge\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Da\u00df die Bewegung erst durch ihre Hand erweckt:", "tokens": ["Da\u00df", "die", "Be\u00b7we\u00b7gung", "erst", "durch", "ih\u00b7re", "Hand", "er\u00b7weckt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wodurch der Stoff erst aus einander gieng;", "tokens": ["Wo\u00b7durch", "der", "Stoff", "erst", "aus", "ein\u00b7an\u00b7der", "gieng", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Auch da\u00df durch sie ein jedes Ding", "tokens": ["Auch", "da\u00df", "durch", "sie", "ein", "je\u00b7des", "Ding"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "APPR", "PPER", "ART", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Sein\u2019 Ordnung und sein Maa\u00df empfing.", "tokens": ["Sein'", "Ord\u00b7nung", "und", "sein", "Maa\u00df", "emp\u00b7fing", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}