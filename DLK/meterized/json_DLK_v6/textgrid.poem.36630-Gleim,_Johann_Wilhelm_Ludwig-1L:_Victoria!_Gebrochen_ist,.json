{"textgrid.poem.36630": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Victoria! Gebrochen ist,", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Victoria! Gebrochen ist,", "tokens": ["Vic\u00b7to\u00b7ria", "!", "Ge\u00b7bro\u00b7chen", "ist", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Burg, in welche sich", "tokens": ["Die", "Burg", ",", "in", "wel\u00b7che", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit l\u00e4ngst gewohnter Schlangenlist,", "tokens": ["Mit", "l\u00e4ngst", "ge\u00b7wohn\u00b7ter", "Schlan\u00b7gen\u00b7list", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der K\u00f6nigsm\u00f6rder schlich!", "tokens": ["Der", "K\u00f6\u00b7nigs\u00b7m\u00f6r\u00b7der", "schlich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die l\u00e4ngstgewohnte Schlangenlist,", "tokens": ["Die", "l\u00e4ngst\u00b7ge\u00b7wohn\u00b7te", "Schlan\u00b7gen\u00b7list", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Dolch, der Meuchelmord,", "tokens": ["Der", "Dolch", ",", "der", "Meu\u00b7chel\u00b7mord", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der d\u00fcrre Palmbaum, der Klubist,", "tokens": ["Der", "d\u00fcr\u00b7re", "Palm\u00b7baum", ",", "der", "Klu\u00b7bist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die rothe Kapp' ist fort!", "tokens": ["Die", "ro\u00b7the", "Kapp'", "ist", "fort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und der so leicht Verf\u00fchrte hinkt,", "tokens": ["Und", "der", "so", "leicht", "Ver\u00b7f\u00fchr\u00b7te", "hinkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Spielzeug hinterher!", "tokens": ["Dem", "Spiel\u00b7zeug", "hin\u00b7ter\u00b7her", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mitleidig sehn wir ihn, er singt,", "tokens": ["Mit\u00b7lei\u00b7dig", "sehn", "wir", "ihn", ",", "er", "singt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Freudenlied nicht mehr!", "tokens": ["Sein", "Freu\u00b7den\u00b7lied", "nicht", "mehr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er singt ein Lied", "tokens": ["Er", "singt", "ein", "Lied"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "In Menschenseelen bringt,", "tokens": ["In", "Men\u00b7schen\u00b7see\u00b7len", "bringt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und trinkt das theure Menschenblut,", "tokens": ["Und", "trinkt", "das", "theu\u00b7re", "Men\u00b7schen\u00b7blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das sein Verf\u00fchrer trinkt!", "tokens": ["Das", "sein", "Ver\u00b7f\u00fch\u00b7rer", "trinkt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Verschweigs, Gesang! die Menschlichkeit", "tokens": ["Ver\u00b7schweigs", ",", "Ge\u00b7sang", "!", "die", "Menschlich\u00b7keit"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Entsetzt sich, wenn sie's h\u00f6rt!", "tokens": ["Ent\u00b7setzt", "sich", ",", "wenn", "sie's", "h\u00f6rt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gesetzes Feindschaft, Stolz und Neid,", "tokens": ["Ge\u00b7set\u00b7zes", "Feind\u00b7schaft", ",", "Stolz", "und", "Neid", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hat unser Mainz zerst\u00f6rt!", "tokens": ["Hat", "un\u00b7ser", "Mainz", "zer\u00b7st\u00f6rt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Zerrissen achtete sie sich", "tokens": ["Zer\u00b7ris\u00b7sen", "ach\u00b7te\u00b7te", "sie", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von eines Tigers Klaun,", "tokens": ["Von", "ei\u00b7nes", "Ti\u00b7gers", "Klaun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die gute Stadt! Wir wollen dich", "tokens": ["Die", "gu\u00b7te", "Stadt", "!", "Wir", "wol\u00b7len", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VMFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du gute! wieder baun!", "tokens": ["Du", "gu\u00b7te", "!", "wie\u00b7der", "baun", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$.", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wenn wieder deutsche Redlichkeit", "tokens": ["Wenn", "wie\u00b7der", "deut\u00b7sche", "Red\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In deinen H\u00e4usern wohnt,", "tokens": ["In", "dei\u00b7nen", "H\u00e4u\u00b7sern", "wohnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wieder nun Gerechtigkeit,", "tokens": ["Und", "wie\u00b7der", "nun", "Ge\u00b7rech\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf deinem Throne thront!", "tokens": ["Auf", "dei\u00b7nem", "Thro\u00b7ne", "thront", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wir fochten! von des Tigers Klaun", "tokens": ["Wir", "foch\u00b7ten", "!", "von", "des", "Ti\u00b7gers", "Klaun"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dich, Schwester! zu befreyn!", "tokens": ["Dich", ",", "Schwes\u00b7ter", "!", "zu", "be\u00b7freyn", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$.", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir wollten all' in sie zu haun,", "tokens": ["Wir", "woll\u00b7ten", "all'", "in", "sie", "zu", "haun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Erste jeder seyn!", "tokens": ["Der", "Ers\u00b7te", "je\u00b7der", "seyn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PIS", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Sing's, du Gesang! der ganzen Welt:", "tokens": ["Sing's", ",", "du", "Ge\u00b7sang", "!", "der", "gan\u00b7zen", "Welt", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "\u00bbder Deutsche ", "tokens": ["\u00bb", "der", "Deut\u00b7sche"], "token_info": ["punct", "word", "word"], "pos": ["$(", "ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "F\u00fcrs Vaterland, ist nur ein Held", "tokens": ["F\u00fcrs", "Va\u00b7ter\u00b7land", ",", "ist", "nur", "ein", "Held"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "$,", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als Mensch, und Patriot!\u00ab", "tokens": ["Als", "Mensch", ",", "und", "Pat\u00b7ri\u00b7ot", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KOUS", "NN", "$,", "KON", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ein edler J\u00fcngling war ein Mann!", "tokens": ["Ein", "ed\u00b7ler", "J\u00fcng\u00b7ling", "war", "ein", "Mann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er flog in unsern Krieg,", "tokens": ["Er", "flog", "in", "un\u00b7sern", "Krieg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "War unsern M\u00e4nnern oft voran,", "tokens": ["War", "un\u00b7sern", "M\u00e4n\u00b7nern", "oft", "vo\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dacht' immer hohen Sieg!", "tokens": ["Dacht'", "im\u00b7mer", "ho\u00b7hen", "Sieg", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Wir haben's all' Ihm angesehn,", "tokens": ["Wir", "ha\u00b7ben's", "all'", "Ihm", "an\u00b7ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie wohl ihm, in Gefahr,", "tokens": ["Wie", "wohl", "ihm", ",", "in", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis in den Tod mit uns zu gehn,", "tokens": ["Bis", "in", "den", "Tod", "mit", "uns", "zu", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am heissen Tage war", "tokens": ["Am", "heis\u00b7sen", "Ta\u00b7ge", "war"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Sein Auge gl\u00fchte hohen Muth,", "tokens": ["Sein", "Au\u00b7ge", "gl\u00fch\u00b7te", "ho\u00b7hen", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gefahr war ihm, wie Scherz!", "tokens": ["Ge\u00b7fahr", "war", "ihm", ",", "wie", "Scherz", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "PWAV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer anders Heldenthaten thut,", "tokens": ["Wer", "an\u00b7ders", "Hel\u00b7den\u00b7tha\u00b7ten", "thut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem ist nicht wohl ums Herz!", "tokens": ["Dem", "ist", "nicht", "wohl", "ums", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Still aber! Br\u00fcder! ins Gewehr!", "tokens": ["Still", "a\u00b7ber", "!", "Br\u00fc\u00b7der", "!", "ins", "Ge\u00b7wehr", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$.", "NN", "$.", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie lebt! die Schlangenlist!", "tokens": ["Sie", "lebt", "!", "die", "Schlan\u00b7gen\u00b7list", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gesang h\u00f6r' auf! Und singe mehr,", "tokens": ["Ge\u00b7sang", "h\u00f6r'", "auf", "!", "Und", "sin\u00b7ge", "mehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$.", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn sie bezwungen ist!", "tokens": ["Wenn", "sie", "be\u00b7zwun\u00b7gen", "ist", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}