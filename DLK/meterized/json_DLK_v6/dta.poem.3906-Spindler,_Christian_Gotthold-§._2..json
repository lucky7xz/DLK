{"dta.poem.3906": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "\u00a7. 2.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn man Erlaubni\u00df hat, zu ihr ins Hau\u00df zu", "tokens": ["Wenn", "man", "Er\u00b7laub\u00b7ni\u00df", "hat", ",", "zu", "ihr", "ins", "Hau\u00df", "zu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "NN", "VAFIN", "$,", "APPR", "PPOSAT", "APPRART", "NN", "PTKZU"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "kommen,", "tokens": ["kom\u00b7men", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Und wird von seinem Schatz gantz freundlich aufge-", "tokens": ["Und", "wird", "von", "sei\u00b7nem", "Schatz", "gantz", "freund\u00b7lich", "auf\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "PPOSAT", "NN", "ADV", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "nommen,", "tokens": ["nom\u00b7men", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Da\u00df sie die Gegenwart recht lieb und g\u00fcnstig nen\u0303t,", "tokens": ["Da\u00df", "sie", "die", "Ge\u00b7gen\u00b7wart", "recht", "lieb", "und", "g\u00fcns\u00b7tig", "ne\u00f1t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So machet er daf\u00fcr ein artigs Compliment.", "tokens": ["So", "ma\u00b7chet", "er", "da\u00b7f\u00fcr", "ein", "ar\u00b7tigs", "Com\u00b7pli\u00b7ment", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}