{"textgrid.poem.25113": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Am Grabe Friedrichs des Zweiten", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus Palermos Bl\u00fctenf\u00fclle, die mit Duft den Sinn bet\u00e4ubt,", "tokens": ["Aus", "Pa\u00b7ler\u00b7mos", "Bl\u00fc\u00b7ten\u00b7f\u00fcl\u00b7le", ",", "die", "mit", "Duft", "den", "Sinn", "be\u00b7t\u00e4ubt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,", "PRELS", "APPR", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Aus dem Strahlenglanz, der blendend \u00fcber Meer und G\u00e4rten st\u00e4ubt,", "tokens": ["Aus", "dem", "Strah\u00b7len\u00b7glanz", ",", "der", "blen\u00b7dend", "\u00fc\u00b7ber", "Meer", "und", "G\u00e4r\u00b7ten", "st\u00e4ubt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADJD", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "In die Gr\u00e4berhalle fl\u00fccht' ich, fern dem l\u00e4rmerf\u00fcllten Tag,", "tokens": ["In", "die", "Gr\u00e4\u00b7ber\u00b7hal\u00b7le", "fl\u00fccht'", "ich", ",", "fern", "dem", "l\u00e4r\u00b7mer\u00b7f\u00fcll\u00b7ten", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "$,", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Dir den Totenkranz zu winden um den dunklen Sarkophag,", "tokens": ["Dir", "den", "To\u00b7ten\u00b7kranz", "zu", "win\u00b7den", "um", "den", "dunk\u00b7len", "Sar\u00b7ko\u00b7phag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKZU", "VVINF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "M\u00e4cht'ger, der um ein Jahrtausend deiner Zeit du schrittst voran,", "tokens": ["M\u00e4cht'\u00b7ger", ",", "der", "um", "ein", "Jahr\u00b7tau\u00b7send", "dei\u00b7ner", "Zeit", "du", "schrittst", "vo\u00b7ran", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "ART", "NN", "PPOSAT", "NN", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Dessen Riesennamen bebend nur der Deutsche stammeln kann!", "tokens": ["Des\u00b7sen", "Rie\u00b7sen\u00b7na\u00b7men", "be\u00b7bend", "nur", "der", "Deut\u00b7sche", "stam\u00b7meln", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJD", "ADV", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "La\u00df in deiner heil'gen Stille, wo du, alles Wandels bar,", "tokens": ["La\u00df", "in", "dei\u00b7ner", "heil'\u00b7gen", "Stil\u00b7le", ",", "wo", "du", ",", "al\u00b7les", "Wan\u00b7dels", "bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PPOSAT", "ADJA", "NN", "$,", "PWAV", "PPER", "$,", "PIAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Nicht den Tag und nicht die Nacht kennst, nicht das Ist und nicht das War,", "tokens": ["Nicht", "den", "Tag", "und", "nicht", "die", "Nacht", "kennst", ",", "nicht", "das", "Ist", "und", "nicht", "das", "War", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "KON", "PTKNEG", "ART", "NN", "VVFIN", "$,", "PTKNEG", "PDS", "VAFIN", "KON", "PTKNEG", "PDS", "VAFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "La\u00df mich denken, wie von Deutschlands Kaiserthrone schicksalsvoll", "tokens": ["La\u00df", "mich", "den\u00b7ken", ",", "wie", "von", "Deutschlands", "Kai\u00b7ser\u00b7thro\u00b7ne", "schick\u00b7sals\u00b7voll"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "PWAV", "APPR", "NE", "NE", "ADJD"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Einst gebietend durch die L\u00e4nder deines Wortes Donner scholl;", "tokens": ["Einst", "ge\u00b7bie\u00b7tend", "durch", "die", "L\u00e4n\u00b7der", "dei\u00b7nes", "Wor\u00b7tes", "Don\u00b7ner", "scholl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "ART", "NN", "PPOSAT", "NN", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Denken, wie vom Nord- zum S\u00fcdmeer durch dein unerme\u00dfnes Reich", "tokens": ["Den\u00b7ken", ",", "wie", "vom", "Nord", "zum", "S\u00fcd\u00b7meer", "durch", "dein", "un\u00b7er\u00b7me\u00df\u00b7nes", "Reich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "APPRART", "TRUNC", "APPRART", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Du den Adler Ruhm, den k\u00fchnen, einem Edelfalken gleich,", "tokens": ["Du", "den", "Ad\u00b7ler", "Ruhm", ",", "den", "k\u00fch\u00b7nen", ",", "ei\u00b7nem", "E\u00b7del\u00b7fal\u00b7ken", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NN", "$,", "ART", "ADJA", "$,", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Auf der starken Faust getragen und gespornt von Flug zu Flug,", "tokens": ["Auf", "der", "star\u00b7ken", "Faust", "ge\u00b7tra\u00b7gen", "und", "ge\u00b7spornt", "von", "Flug", "zu", "Flug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "KON", "VVPP", "APPR", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Bis die Schwinge, alles wagend, ihn in Sonnenferne trug!", "tokens": ["Bis", "die", "Schwin\u00b7ge", ",", "al\u00b7les", "wa\u00b7gend", ",", "ihn", "in", "Son\u00b7nen\u00b7fer\u00b7ne", "trug", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PIS", "ADJD", "$,", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.15": {"text": "Um dich her mit Schild und Lanze, als ein eisenfester Wall,", "tokens": ["Um", "dich", "her", "mit", "Schild", "und", "Lan\u00b7ze", ",", "als", "ein", "ei\u00b7sen\u00b7fes\u00b7ter", "Wall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "APZR", "APPR", "NN", "KON", "NN", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.16": {"text": "Reihten sich die Erdenf\u00fcrsten, jeder deines Throns Vasall,", "tokens": ["Reih\u00b7ten", "sich", "die", "Er\u00b7den\u00b7f\u00fcrs\u00b7ten", ",", "je\u00b7der", "dei\u00b7nes", "Throns", "Va\u00b7sall", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,", "PIAT", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+--", "measure": "unknown.measure.septa"}, "line.17": {"text": "Und, das Werk der Nacht zerst\u00f6rend, f\u00fcr des Priesters Bannfluch taub,", "tokens": ["Und", ",", "das", "Werk", "der", "Nacht", "zer\u00b7st\u00f6\u00b7rend", ",", "f\u00fcr", "des", "Pries\u00b7ters", "Bann\u00b7fluch", "taub", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ART", "NN", "ART", "NN", "VVPP", "$,", "APPR", "ART", "NN", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.18": {"text": "Tratst du, die ihn dreifach kr\u00f6nte, die Tiara in den Staub,", "tokens": ["Tratst", "du", ",", "die", "ihn", "drei\u00b7fach", "kr\u00f6n\u00b7te", ",", "die", "Ti\u00b7a\u00b7ra", "in", "den", "Staub", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "PPER", "PAV", "VVFIN", "$,", "PRELS", "NE", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.19": {"text": "W\u00e4hrend an dein eh'rnes Deutschland du das sonn'ge Morgenland", "tokens": ["W\u00e4h\u00b7rend", "an", "dein", "eh'r\u00b7nes", "Deutschland", "du", "das", "sonn'\u00b7ge", "Mor\u00b7gen\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "ADJA", "NN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.20": {"text": "Und des S\u00fcdens heitre K\u00fcsten bandest mit gewalt'ger Hand. \u2013", "tokens": ["Und", "des", "S\u00fc\u00b7dens", "heit\u00b7re", "K\u00fcs\u00b7ten", "ban\u00b7dest", "mit", "ge\u00b7walt'\u00b7ger", "Hand", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.21": {"text": "Aber weh! die hehren Bilder, wer verh\u00fcllt sie meinem Blick?", "tokens": ["A\u00b7ber", "weh", "!", "die", "heh\u00b7ren", "Bil\u00b7der", ",", "wer", "ver\u00b7h\u00fcllt", "sie", "mei\u00b7nem", "Blick", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "ART", "ADJA", "NN", "$,", "PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.22": {"text": "Neuen, immer neuen Wechsel bringt das rollende Geschick,", "tokens": ["Neu\u00b7en", ",", "im\u00b7mer", "neu\u00b7en", "Wech\u00b7sel", "bringt", "das", "rol\u00b7len\u00b7de", "Ge\u00b7schick", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.23": {"text": "Und durch siebenhundert Jahre seh' ich wie im Traumgesicht", "tokens": ["Und", "durch", "sie\u00b7ben\u00b7hun\u00b7dert", "Jah\u00b7re", "seh'", "ich", "wie", "im", "Traum\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "CARD", "NN", "VVFIN", "PPER", "KOKOM", "APPRART", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.24": {"text": "Finstrer stets den Himmel kreisen mit erloschnem Sternenlicht,", "tokens": ["Finst\u00b7rer", "stets", "den", "Him\u00b7mel", "krei\u00b7sen", "mit", "er\u00b7lo\u00b7schnem", "Ster\u00b7nen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.25": {"text": "Seh' dein Reich in Tr\u00fcmmer sinken, da\u00df, zerbr\u00f6ckelt und zernagt,", "tokens": ["Seh'", "dein", "Reich", "in", "Tr\u00fcm\u00b7mer", "sin\u00b7ken", ",", "da\u00df", ",", "zer\u00b7br\u00f6\u00b7ckelt", "und", "zer\u00b7nagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$,", "KOUS", "$,", "VVPP", "KON", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.26": {"text": "Selten noch ein halbgebrochner Pfeiler aus dem Schutte ragt;", "tokens": ["Sel\u00b7ten", "noch", "ein", "halb\u00b7ge\u00b7broch\u00b7ner", "Pfei\u00b7ler", "aus", "dem", "Schut\u00b7te", "ragt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.27": {"text": "Weithin geht durch seine Zinnen, seinen Wall der Ri\u00df hindurch,", "tokens": ["Weit\u00b7hin", "geht", "durch", "sei\u00b7ne", "Zin\u00b7nen", ",", "sei\u00b7nen", "Wall", "der", "Ri\u00df", "hin\u00b7durch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "ART", "NN", "PAV", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.28": {"text": "Und am Boden liegt die starke, liegt die heil'ge V\u00f6lkerburg.", "tokens": ["Und", "am", "Bo\u00b7den", "liegt", "die", "star\u00b7ke", ",", "liegt", "die", "heil'\u00b7ge", "V\u00f6l\u00b7ker\u00b7burg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ART", "ADJA", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.29": {"text": "Trauernd \u00fcber deinem Lande hat der Genius sich verh\u00fcllt;", "tokens": ["Trau\u00b7ernd", "\u00fc\u00b7ber", "dei\u00b7nem", "Lan\u00b7de", "hat", "der", "Ge\u00b7nius", "sich", "ver\u00b7h\u00fcllt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "VAFIN", "ART", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+-+--+--+", "measure": "trochaic.septa.relaxed"}, "line.30": {"text": "Von den eignen S\u00f6hnen wurde seiner Schande Ma\u00df erf\u00fcllt;", "tokens": ["Von", "den", "eig\u00b7nen", "S\u00f6h\u00b7nen", "wur\u00b7de", "sei\u00b7ner", "Schan\u00b7de", "Ma\u00df", "er\u00b7f\u00fcllt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.31": {"text": "Seine Lenker in Verblendung denken nicht der Zeit, die war,", "tokens": ["Sei\u00b7ne", "Len\u00b7ker", "in", "Ver\u00b7blen\u00b7dung", "den\u00b7ken", "nicht", "der", "Zeit", ",", "die", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVFIN", "PTKNEG", "ART", "NN", "$,", "PRELS", "VAFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.32": {"text": "Als sich herrschend \u00fcber alle schwang der doppelh\u00e4upt'ge Aar,", "tokens": ["Als", "sich", "herr\u00b7schend", "\u00fc\u00b7ber", "al\u00b7le", "schwang", "der", "dop\u00b7pel\u00b7h\u00e4upt'\u00b7ge", "Aar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "VVPP", "APPR", "PIS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.33": {"text": "Nicht sein Volk, da\u00df ihm der Kaiser, was dem Schiffer der Pilot;", "tokens": ["Nicht", "sein", "Volk", ",", "da\u00df", "ihm", "der", "Kai\u00b7ser", ",", "was", "dem", "Schif\u00b7fer", "der", "Pi\u00b7lot", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "$,", "KOUS", "PPER", "ART", "NN", "$,", "PRELS", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.34": {"text": "Ohne ihn auf st\u00fcrm'schem Meere sinkt es selbst im lecken Boot.", "tokens": ["Oh\u00b7ne", "ihn", "auf", "st\u00fcr\u00b7m'\u00b7schem", "Mee\u00b7re", "sinkt", "es", "selbst", "im", "le\u00b7cken", "Boot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ADJA", "NN", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+-+-+", "measure": "iambic.octa.plus.invert"}, "line.35": {"text": "Nun verzagend stehn sie alle, da der Boden kracht und wankt;", "tokens": ["Nun", "ver\u00b7za\u00b7gend", "stehn", "sie", "al\u00b7le", ",", "da", "der", "Bo\u00b7den", "kracht", "und", "wankt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VVFIN", "PPER", "PIS", "$,", "KOUS", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.36": {"text": "Wilder tobt um sie die Woge, und der Kompa\u00df tr\u00fcgt und schwankt;", "tokens": ["Wil\u00b7der", "t