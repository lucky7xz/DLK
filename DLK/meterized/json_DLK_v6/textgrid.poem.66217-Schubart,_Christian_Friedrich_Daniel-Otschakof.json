{"textgrid.poem.66217": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Otschakof", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Otschakof fiel! \u2013 Da liegt die gewaltige Feste!", "tokens": ["Ot\u00b7scha\u00b7kof", "fiel", "!", "\u2013", "Da", "liegt", "die", "ge\u00b7wal\u00b7ti\u00b7ge", "Fes\u00b7te", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "$(", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "---+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wir trotzten schon lange dem felsigten Neste;", "tokens": ["Wir", "trotz\u00b7ten", "schon", "lan\u00b7ge", "dem", "fel\u00b7sig\u00b7ten", "Nes\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Nun liegt es zertr\u00fcmmert! \u2013 Singt, Russen, ins Spiel", "tokens": ["Nun", "liegt", "es", "zer\u00b7tr\u00fcm\u00b7mert", "!", "\u2013", "Singt", ",", "Rus\u00b7sen", ",", "ins", "Spiel"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "$.", "$(", "VVFIN", "$,", "NE", "$,", "APPRART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Der Trommel und Pfeifen! \u2013 Otschakof fiel.", "tokens": ["Der", "Trom\u00b7mel", "und", "Pfei\u00b7fen", "!", "\u2013", "Ot\u00b7scha\u00b7kof", "fiel", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$.", "$(", "NE", "VVFIN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Zwar haben die Feinde sich tapfer gehalten;", "tokens": ["Zwar", "ha\u00b7ben", "die", "Fein\u00b7de", "sich", "tap\u00b7fer", "ge\u00b7hal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PRF", "ADJD", "VVPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Denn Blitze zu schleudern und Speere zu spalten", "tokens": ["Denn", "Blit\u00b7ze", "zu", "schleu\u00b7dern", "und", "Spee\u00b7re", "zu", "spal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "PTKZU", "VVINF", "KON", "NN", "PTKZU", "VVINF"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ist ihren gewaltigen Armen ein Spiel.", "tokens": ["Ist", "ih\u00b7ren", "ge\u00b7wal\u00b7ti\u00b7gen", "Ar\u00b7men", "ein", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Doch sind sie Besiegte! \u2013 Denn Otschakof fiel.", "tokens": ["Doch", "sind", "sie", "Be\u00b7sieg\u00b7te", "!", "\u2013", "Denn", "Ot\u00b7scha\u00b7kof", "fiel", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVFIN", "$.", "$(", "KON", "NE", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Was kann auf der Erde uns Russen besiegen?", "tokens": ["Was", "kann", "auf", "der", "Er\u00b7de", "uns", "Rus\u00b7sen", "be\u00b7sie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "APPR", "ART", "NN", "PPER", "NN", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wir stehen wie Berge in schrecklichen Kriegen.", "tokens": ["Wir", "ste\u00b7hen", "wie", "Ber\u00b7ge", "in", "schreck\u00b7li\u00b7chen", "Krie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wir achten nicht Hitze, nicht St\u00fcrme, nicht Frost,", "tokens": ["Wir", "ach\u00b7ten", "nicht", "Hit\u00b7ze", ",", "nicht", "St\u00fcr\u00b7me", ",", "nicht", "Frost", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN", "$,", "PTKNEG", "NN", "$,", "PTKNEG", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Thun m\u00e4nnliche Thaten bei k\u00e4rglicher Kost.", "tokens": ["Thun", "m\u00e4nn\u00b7li\u00b7che", "Tha\u00b7ten", "bei", "k\u00e4rg\u00b7li\u00b7cher", "Kost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "++--+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Wir hatten uns H\u00f6hlen ins Schneefeld gescharret,", "tokens": ["Wir", "hat\u00b7ten", "uns", "H\u00f6h\u00b7len", "ins", "Schnee\u00b7feld", "ge\u00b7schar\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Da dachten die Feinde: Sie liegen erstarret.", "tokens": ["Da", "dach\u00b7ten", "die", "Fein\u00b7de", ":", "Sie", "lie\u00b7gen", "er\u00b7star\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "VVFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wir aber durch z\u00f6gernde Tapferkeit hei\u00df", "tokens": ["Wir", "a\u00b7ber", "durch", "z\u00f6\u00b7gern\u00b7de", "Tap\u00b7fer\u00b7keit", "hei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "ADJA", "NN", "ADJD"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Verlie\u00dfen die H\u00f6hlen mit Haaren von Eis.", "tokens": ["Ver\u00b7lie\u00b7\u00dfen", "die", "H\u00f6h\u00b7len", "mit", "Haa\u00b7ren", "von", "Eis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Wir packten die Feste mit rasendem Grimme,", "tokens": ["Wir", "pack\u00b7ten", "die", "Fes\u00b7te", "mit", "ra\u00b7sen\u00b7dem", "Grim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Gef\u00fchrt von Potemkins gebietender Stimme.", "tokens": ["Ge\u00b7f\u00fchrt", "von", "Po\u00b7tem\u00b7kins", "ge\u00b7bie\u00b7ten\u00b7der", "Stim\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wir stiegen die Schanzen im Sturme hinan,", "tokens": ["Wir", "stie\u00b7gen", "die", "Schan\u00b7zen", "im", "Stur\u00b7me", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}}, "stanza.6": {"line.1": {"text": "Da st\u00fcrzten die Mauern und w\u00fcthig wie B\u00e4ren", "tokens": ["Da", "st\u00fcrz\u00b7ten", "die", "Mau\u00b7ern", "und", "w\u00fct\u00b7hig", "wie", "B\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "ADJD", "KOKOM", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Bestiegen wir alle mit blanken Gewehren", "tokens": ["Be\u00b7stie\u00b7gen", "wir", "al\u00b7le", "mit", "blan\u00b7ken", "Ge\u00b7weh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PIS", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die H\u00fcgel von Schutte \u2013 bald f\u00fcllte die Wuth", "tokens": ["Die", "H\u00fc\u00b7gel", "von", "Schut\u00b7te", "\u2013", "bald", "f\u00fcll\u00b7te", "die", "Wuth"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "$(", "ADV", "VVFIN", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die Stra\u00dfen der Feste mit dampfendem Blut.", "tokens": ["Die", "Stra\u00b7\u00dfen", "der", "Fes\u00b7te", "mit", "damp\u00b7fen\u00b7dem", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "Da lagen die T\u00fcrken gefallen vom Schwerte,", "tokens": ["Da", "la\u00b7gen", "die", "T\u00fcr\u00b7ken", "ge\u00b7fal\u00b7len", "vom", "Schwer\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP", "APPRART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Verzweiflung noch zerrte die bleiche Geberde,", "tokens": ["Ver\u00b7zwei\u00b7flung", "noch", "zerr\u00b7te", "die", "blei\u00b7che", "Ge\u00b7ber\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Sie fielen und starben mit r\u00f6chelndem Weh", "tokens": ["Sie", "fie\u00b7len", "und", "star\u00b7ben", "mit", "r\u00f6\u00b7cheln\u00b7dem", "Weh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und ballten im Kampfe den blutigen Schnee.", "tokens": ["Und", "ball\u00b7ten", "im", "Kamp\u00b7fe", "den", "blu\u00b7ti\u00b7gen", "Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.8": {"line.1": {"text": "Zwar ist auch im Streite ", "tokens": ["Zwar", "ist", "auch", "im", "Strei\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ein tapferer Krieger, ein Vater uns allen,", "tokens": ["Ein", "tap\u00b7fe\u00b7rer", "Krie\u00b7ger", ",", "ein", "Va\u00b7ter", "uns", "al\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "PPER", "PIS", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Doch droben im Himmel empf\u00e4ngt er den Lohn,", "tokens": ["Doch", "dro\u00b7ben", "im", "Him\u00b7mel", "emp\u00b7f\u00e4ngt", "er", "den", "Lohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und Peter der Gro\u00dfe umarmt ihn als Sohn.", "tokens": ["Und", "Pe\u00b7ter", "der", "Gro\u00b7\u00dfe", "um\u00b7armt", "ihn", "als", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ART", "ADJA", "VVFIN", "PPER", "KOUS", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.9": {"line.1": {"text": "Viktoria, Russen ihr M\u00e4nner von Eisen!", "tokens": ["Vik\u00b7to\u00b7ria", ",", "Rus\u00b7sen", "ihr", "M\u00e4n\u00b7ner", "von", "Ei\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Es lebe die m\u00e4chtige F\u00fcrstin der Reu\u00dfen!", "tokens": ["Es", "le\u00b7be", "die", "m\u00e4ch\u00b7ti\u00b7ge", "F\u00fcrs\u00b7tin", "der", "Reu\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Es lebe ", "tokens": ["Es", "le\u00b7be"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Da\u00df Stambul, die Stolze, wie Otschakof f\u00e4llt!", "tokens": ["Da\u00df", "Stam\u00b7bul", ",", "die", "Stol\u00b7ze", ",", "wie", "Ot\u00b7scha\u00b7kof", "f\u00e4llt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "ART", "ADJA", "$,", "PWAV", "NE", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}