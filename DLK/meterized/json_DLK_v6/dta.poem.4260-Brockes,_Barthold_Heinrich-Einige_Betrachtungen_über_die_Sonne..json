{"dta.poem.4260": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Einige  \n  Betrachtungen \u00fcber die Sonne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ohimmlisches, von allen dem, was irdisch, unterschied-", "tokens": ["O\u00b7himm\u00b7li\u00b7sches", ",", "von", "al\u00b7len", "dem", ",", "was", "ir\u00b7disch", ",", "un\u00b7ter\u00b7schie\u00b7d"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "$,", "APPR", "PIAT", "PDS", "$,", "PRELS", "ADJD", "$,", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "nes Wesen!", "tokens": ["nes", "We\u00b7sen", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "O Sonne! dich hab\u2019 ich, von neuem, zu einem Vorwurf", "tokens": ["O", "Son\u00b7ne", "!", "dich", "hab'", "ich", ",", "von", "neu\u00b7em", ",", "zu", "ei\u00b7nem", "Vor\u00b7wurf"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$.", "PPER", "VAFIN", "PPER", "$,", "APPR", "ADJA", "$,", "APPR", "ART", "NN"], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}, "line.4": {"text": "auserlesen,", "tokens": ["au\u00b7ser\u00b7le\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Um dich mit meiner Seelen Augen, voll Lust und Andacht,", "tokens": ["Um", "dich", "mit", "mei\u00b7ner", "See\u00b7len", "Au\u00b7gen", ",", "voll", "Lust", "und", "An\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "APPR", "PPOSAT", "NN", "NN", "$,", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "anzuseh'n,", "tokens": ["an\u00b7zu\u00b7seh'n", ","], "token_info": ["word", "punct"], "pos": ["VVIZU", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Und mit zwar k\u00fchn- doch wohlgemeynten Ges\u00e4ngen in dir", "tokens": ["Und", "mit", "zwar", "k\u00fchn", "doch", "wohl\u00b7ge\u00b7meyn\u00b7ten", "Ge\u00b7s\u00e4n\u00b7gen", "in", "dir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADV", "TRUNC", "KON", "ADJA", "NN", "APPR", "PPER"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "zu erh\u00f6h'n", "tokens": ["zu", "er\u00b7h\u00f6h'n"], "token_info": ["word", "word"], "pos": ["PTKZU", "VVINF"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Die Quell\u2019, aus Der du, so wie alles, doch auf ganz andre", "tokens": ["Die", "Quell'", ",", "aus", "Der", "du", ",", "so", "wie", "al\u00b7les", ",", "doch", "auf", "ganz", "and\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NE", "$,", "ADV", "KOKOM", "PIS", "$,", "ADV", "APPR", "ADV", "ADJA"], "meter": "-++-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Weise, stammest,", "tokens": ["Wei\u00b7se", ",", "stam\u00b7mest", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Den Born, aus welchem du so herrlich, zum Heyl der Crea-", "tokens": ["Den", "Born", ",", "aus", "wel\u00b7chem", "du", "so", "herr\u00b7lich", ",", "zum", "Heyl", "der", "Crea"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NE", "$,", "APPR", "PRELS", "PPER", "ADV", "ADJD", "$,", "APPRART", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "turen, flammest.", "tokens": ["tu\u00b7ren", ",", "flam\u00b7mest", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "O herrlichs Licht, voll Fruchtbarkeit, voll Sch\u00f6nheit, An-", "tokens": ["O", "herr\u00b7lichs", "Licht", ",", "voll", "Frucht\u00b7bar\u00b7keit", ",", "voll", "Sch\u00f6n\u00b7heit", ",", "An"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "ADJA", "NN", "$,", "ADJD", "NN", "$,", "ADJD", "NN", "$,", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "muht, Nutz und Segen,", "tokens": ["muht", ",", "Nutz", "und", "Se\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ohn\u2019 dessen \u00fcberird\u2019schen Glanz, und unersch\u00f6pfter Strah-", "tokens": ["Ohn'", "des\u00b7sen", "\u00fc\u00b7ber\u00b7ird'\u00b7schen", "Glanz", ",", "und", "un\u00b7er\u00b7sch\u00f6pf\u00b7ter", "Strah"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PRELAT", "ADJA", "NN", "$,", "KON", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "len Pracht,", "tokens": ["len", "Pracht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Die Wesen aller Dinge fast in ein\u2019 unwesentliche Nacht", "tokens": ["Die", "We\u00b7sen", "al\u00b7ler", "Din\u00b7ge", "fast", "in", "ein'", "un\u00b7we\u00b7sent\u00b7li\u00b7che", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "NN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+--+--+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Verwirret und versunken l\u00e4gen,", "tokens": ["Ver\u00b7wir\u00b7ret", "und", "ver\u00b7sun\u00b7ken", "l\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ohn\u2019 dessen Licht, ohn\u2019 dessen Schein", "tokens": ["Ohn'", "des\u00b7sen", "Licht", ",", "ohn'", "des\u00b7sen", "Schein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "$,", "APPR", "PRELAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wir nichts von aller Dinge Seyn", "tokens": ["Wir", "nichts", "von", "al\u00b7ler", "Din\u00b7ge", "Seyn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PIS", "APPR", "PIAT", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Zu fassen, noch zu seh\u2019n verm\u00f6gen.", "tokens": ["Zu", "fas\u00b7sen", ",", "noch", "zu", "seh'n", "ver\u00b7m\u00f6\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wir werden dich, mit allem Recht,", "tokens": ["Wir", "wer\u00b7den", "dich", ",", "mit", "al\u00b7lem", "Recht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "APPR", "PIS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Vom Sch\u00f6pfer einen Ausflu\u00df nennen,", "tokens": ["Vom", "Sch\u00f6p\u00b7fer", "ei\u00b7nen", "Aus\u00b7flu\u00df", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Und, da du nicht nur Licht und Schein, zu unsrer Anmuht,", "tokens": ["Und", ",", "da", "du", "nicht", "nur", "Licht", "und", "Schein", ",", "zu", "uns\u00b7rer", "An\u00b7muht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "NN", "KON", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "uns gew\u00e4hrest,", "tokens": ["uns", "ge\u00b7w\u00e4h\u00b7rest", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.14": {"text": "Nein, da du uns zugleich erh\u00e4ltst, erw\u00e4rmst, belebest und", "tokens": ["Nein", ",", "da", "du", "uns", "zu\u00b7gleich", "er\u00b7h\u00e4ltst", ",", "er\u00b7w\u00e4rmst", ",", "be\u00b7le\u00b7best", "und"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON"], "meter": "-+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.15": {"text": "ern\u00e4hrest,", "tokens": ["er\u00b7n\u00e4h\u00b7rest", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Dich f\u00fcr des grossen Sch\u00f6pfers Werkzeug, ja seinen Schat-", "tokens": ["Dich", "f\u00fcr", "des", "gros\u00b7sen", "Sch\u00f6p\u00b7fers", "Werk\u00b7zeug", ",", "ja", "sei\u00b7nen", "Schat"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "NN", "$,", "ADV", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "ten, halten k\u00f6nnen.", "tokens": ["ten", ",", "hal\u00b7ten", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["FM", "$,", "VVINF", "VMINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wer allen Nutzen fassen wollte von deinen milden Segens-", "tokens": ["Wer", "al\u00b7len", "Nut\u00b7zen", "fas\u00b7sen", "woll\u00b7te", "von", "dei\u00b7nen", "mil\u00b7den", "Se\u00b7gens"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PIAT", "NN", "VVINF", "VMFIN", "APPR", "PPOSAT", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Fl\u00fcssen,", "tokens": ["Fl\u00fcs\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "W\u00fcrd\u2019 aller deiner Strahlen Zahl im Sommer-Mittag", "tokens": ["W\u00fcrd'", "al\u00b7ler", "dei\u00b7ner", "Strah\u00b7len", "Zahl", "im", "Som\u00b7mer\u00b7Mit\u00b7tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "PPOSAT", "NN", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "z\u00e4hlen m\u00fcssen.", "tokens": ["z\u00e4h\u00b7len", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Zumahl sich, unter so viel Kr\u00e4ften, noch eine sondre Kraft", "tokens": ["Zu\u00b7mahl", "sich", ",", "un\u00b7ter", "so", "viel", "Kr\u00e4f\u00b7ten", ",", "noch", "ei\u00b7ne", "sond\u00b7re", "Kraft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "$,", "APPR", "ADV", "PIAT", "NN", "$,", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "befindet,", "tokens": ["be\u00b7fin\u00b7det", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Durch welche, durch ein streng- geheimes, und unaufh\u00f6r-", "tokens": ["Durch", "wel\u00b7che", ",", "durch", "ein", "streng", "ge\u00b7hei\u00b7mes", ",", "und", "un\u00b7auf\u00b7h\u00f6r"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PRELS", "$,", "APPR", "ART", "TRUNC", "ADJA", "$,", "KON", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "lichs Ansichziehn,", "tokens": ["lichs", "An\u00b7sich\u00b7ziehn", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Sich der Planeten Meng\u2019 und Gr\u00f6sse in ihren weiten Kreisen", "tokens": ["Sich", "der", "Pla\u00b7ne\u00b7ten", "Meng'", "und", "Gr\u00f6s\u00b7se", "in", "ih\u00b7ren", "wei\u00b7ten", "Krei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ART", "NN", "NE", "KON", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "--+--+-+--+-+-+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "bindet,", "tokens": ["bin\u00b7det", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "So, da\u00df sie, blo\u00df durch dich regieret, in einer steten Ordnung", "tokens": ["So", ",", "da\u00df", "sie", ",", "blo\u00df", "durch", "dich", "re\u00b7gie\u00b7ret", ",", "in", "ei\u00b7ner", "ste\u00b7ten", "Ord\u00b7nung"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "$,", "ADV", "APPR", "PPER", "VVFIN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.14": {"text": "fliehn", "tokens": ["fliehn"], "token_info": ["word"], "pos": ["VVINF"], "meter": "-", "measure": "single.down"}, "line.15": {"text": "Vom weit entlegenen Saturn bis zum kaum sichtbaren", "tokens": ["Vom", "weit", "ent\u00b7le\u00b7ge\u00b7nen", "Sa\u00b7turn", "bis", "zum", "kaum", "sicht\u00b7ba\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJD", "ADJA", "NN", "APPR", "APPRART", "ADV", "VVINF"], "meter": "-+--+-+-+-++-+", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "Mercur.", "tokens": ["Mer\u00b7cur", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "--", "measure": "unknown.measure.zero"}, "line.17": {"text": "Du bildest, f\u00e4rbest und versch\u00f6nerst die ganze sichtbare", "tokens": ["Du", "bil\u00b7dest", ",", "f\u00e4r\u00b7best", "und", "ver\u00b7sch\u00f6\u00b7nerst", "die", "gan\u00b7ze", "sicht\u00b7ba\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "ADJA"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.18": {"text": "Natur,", "tokens": ["Na\u00b7tur", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.19": {"text": "Du schenkst dem Himmel den Sapphir, der sch\u00f6nen Morgen-", "tokens": ["Du", "schenkst", "dem", "Him\u00b7mel", "den", "Sap\u00b7phir", ",", "der", "sch\u00f6\u00b7nen", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "TRUNC"], "meter": "-+-+--++-+-+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "R\u00f6ht Opalen,", "tokens": ["R\u00f6ht", "O\u00b7pa\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.21": {"text": "Wie auch des bunten Regen-Bogens, sind alle Kinder deiner", "tokens": ["Wie", "auch", "des", "bun\u00b7ten", "Re\u00b7gen\u00b7Bo\u00b7gens", ",", "sind", "al\u00b7le", "Kin\u00b7der", "dei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "$,", "VAFIN", "PIAT", "NN", "PPOSAT"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.22": {"text": "Strahlen.", "tokens": ["Strah\u00b7len", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.23": {"text": "Bey so viel wunderreichen Kr\u00e4ften, bey so viel Segens-Ei-", "tokens": ["Bey", "so", "viel", "wun\u00b7der\u00b7rei\u00b7chen", "Kr\u00e4f\u00b7ten", ",", "bey", "so", "viel", "Se\u00b7gens\u00b7Ei"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "PIAT", "ADJA", "NN", "$,", "APPR", "ADV", "PIAT", "NN"], "meter": "+--+-+-+-+--+-+", "measure": "iambic.septa.invert"}, "line.24": {"text": "genschaften,", "tokens": ["gen\u00b7schaf\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.25": {"text": "Die all\u2019 an deines hellen Lichts Bewundrungs- wehrten", "tokens": ["Die", "all'", "an", "dei\u00b7nes", "hel\u00b7len", "Lichts", "Be\u00b7wun\u00b7drungs", "wehr\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "APPR", "PPOSAT", "ADJA", "NN", "TRUNC", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ausflu\u00df haften,", "tokens": ["Aus\