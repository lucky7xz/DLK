{"textgrid.poem.39969": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "10. Wie Herodes die Kindlein in Bethlehem ermorden lie\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu Bethlehem am selben Tage,", "tokens": ["Zu", "Beth\u00b7le\u00b7hem", "am", "sel\u00b7ben", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da Joseph mit dem Kind geflohen,", "tokens": ["Da", "Jo\u00b7se\u00b7ph", "mit", "dem", "Kind", "ge\u00b7flo\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Erhebet sich Geheul und Klage,", "tokens": ["Er\u00b7he\u00b7bet", "sich", "Ge\u00b7heul", "und", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da jammern Frauen, W\u00fcrger drohen", "tokens": ["Da", "jam\u00b7mern", "Frau\u00b7en", ",", "W\u00fcr\u00b7ger", "dro\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJA", "NN", "$,", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und gehn, wie Tiger in der W\u00fcsten", "tokens": ["Und", "gehn", ",", "wie", "Ti\u00b7ger", "in", "der", "W\u00fcs\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "$,", "PWAV", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auf Raub mit m\u00f6rderischem Mute;", "tokens": ["Auf", "Raub", "mit", "m\u00f6r\u00b7de\u00b7ri\u00b7schem", "Mu\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da mischet an der Mutter Br\u00fcsten", "tokens": ["Da", "mi\u00b7schet", "an", "der", "Mut\u00b7ter", "Br\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Milch sich mit des S\u00e4uglings Blute.", "tokens": ["Die", "Milch", "sich", "mit", "des", "S\u00e4ug\u00b7lings", "Blu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das kleinste Kind wird nicht verschonet,", "tokens": ["Das", "kleins\u00b7te", "Kind", "wird", "nicht", "ver\u00b7scho\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mu\u00df, eh' es l\u00e4cheln kann, verderben,", "tokens": ["Mu\u00df", ",", "eh'", "es", "l\u00e4\u00b7cheln", "kann", ",", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "$,", "KOUS", "PPER", "VVFIN", "VMFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und, dem schon Lust im Auge wohnet,", "tokens": ["Und", ",", "dem", "schon", "Lust", "im", "Au\u00b7ge", "woh\u00b7net", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "ADV", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht Gnade kann sein Blick erwerben.", "tokens": ["Nicht", "Gna\u00b7de", "kann", "sein", "Blick", "er\u00b7wer\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dort ruft mit halbgel\u00f6ster Zunge", "tokens": ["Dort", "ruft", "mit", "halb\u00b7ge\u00b7l\u00f6s\u00b7ter", "Zun\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein anderes dem Vater lallend;", "tokens": ["Ein", "an\u00b7de\u00b7res", "dem", "Va\u00b7ter", "lal\u00b7lend", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da kommt ein Schwert mit raschem Schwunge", "tokens": ["Da", "kommt", "ein", "Schwert", "mit", "ra\u00b7schem", "Schwun\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ihm in sein stammelnd Bitten fallend.", "tokens": ["Ihm", "in", "sein", "stam\u00b7melnd", "Bit\u00b7ten", "fal\u00b7lend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und nieder schaut von dem Gebirge", "tokens": ["Und", "nie\u00b7der", "schaut", "von", "dem", "Ge\u00b7bir\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKVZ", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf die mit Blut getr\u00e4nkten Matten,", "tokens": ["Auf", "die", "mit", "Blut", "ge\u00b7tr\u00e4nk\u00b7ten", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In das Geschrei, in das Gew\u00fcrge,", "tokens": ["In", "das", "Ge\u00b7schrei", ",", "in", "das", "Ge\u00b7w\u00fcr\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Der Stammfrau jammervoller Schatten.", "tokens": ["Der", "Stamm\u00b7frau", "jam\u00b7mer\u00b7vol\u00b7ler", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Eins um das Andre sieht sie fassen,", "tokens": ["Eins", "um", "das", "And\u00b7re", "sieht", "sie", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "PIS", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie weint, sie ruft, sie kann's nicht hindern,", "tokens": ["Sie", "weint", ",", "sie", "ruft", ",", "sie", "kann's", "nicht", "hin\u00b7dern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Rahel will sich nicht tr\u00f6sten lassen,", "tokens": ["Ra\u00b7hel", "will", "sich", "nicht", "tr\u00f6s\u00b7ten", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PRF", "PTKNEG", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Denn es ist aus mit ihren Kindern.", "tokens": ["Denn", "es", "ist", "aus", "mit", "ih\u00b7ren", "Kin\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch des Herodes Henkersknechte,", "tokens": ["Doch", "des", "He\u00b7ro\u00b7des", "Hen\u00b7kers\u00b7knech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So sicher sie auch sp\u00e4hn und schlagen,", "tokens": ["So", "si\u00b7cher", "sie", "auch", "sp\u00e4hn", "und", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie treffen nimmermehr das rechte:", "tokens": ["Sie", "tref\u00b7fen", "nim\u00b7mer\u00b7mehr", "das", "rech\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das wandert s\u00e4nftiglich getragen. \u2013", "tokens": ["Das", "wan\u00b7dert", "s\u00e4nf\u00b7tig\u00b7lich", "ge\u00b7tra\u00b7gen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In seinen Traum am Mutterherzen", "tokens": ["In", "sei\u00b7nen", "Traum", "am", "Mut\u00b7ter\u00b7her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Verirrt sich nicht der M\u00f6rder Toben;", "tokens": ["Ver\u00b7irrt", "sich", "nicht", "der", "M\u00f6r\u00b7der", "To\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Es ist ein Kind, zu andern Schmerzen", "tokens": ["Es", "ist", "ein", "Kind", ",", "zu", "an\u00b7dern", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und andrem Sterben aufgehoben.", "tokens": ["Und", "an\u00b7drem", "Ster\u00b7ben", "auf\u00b7ge\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ein Mann, wird er das Land durchwandeln,", "tokens": ["Ein", "Mann", ",", "wird", "er", "das", "Land", "durch\u00b7wan\u00b7deln", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Zeichen thun und g\u00f6ttlich lehren,", "tokens": ["Und", "Zei\u00b7chen", "thun", "und", "g\u00f6tt\u00b7lich", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit seinem Wort, mit seinem Handeln", "tokens": ["Mit", "sei\u00b7nem", "Wort", ",", "mit", "sei\u00b7nem", "Han\u00b7deln"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Himmelreiche viel bekehren,", "tokens": ["Zum", "Him\u00b7mel\u00b7rei\u00b7che", "viel", "be\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zu einem Reich, vor dem kein K\u00f6nig", "tokens": ["Zu", "ei\u00b7nem", "Reich", ",", "vor", "dem", "kein", "K\u00f6\u00b7nig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Thron mit Morde braucht zu wahren,", "tokens": ["Den", "Thron", "mit", "Mor\u00b7de", "braucht", "zu", "wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zu einem Reich, dem unterth\u00e4nig", "tokens": ["Zu", "ei\u00b7nem", "Reich", ",", "dem", "un\u00b7ter\u00b7th\u00e4\u00b7nig"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nur Seelen sind und Engelscharen.", "tokens": ["Nur", "See\u00b7len", "sind", "und", "En\u00b7gel\u00b7scha\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Er aber, dieses Reiches Gr\u00fcnder,", "tokens": ["Er", "a\u00b7ber", ",", "die\u00b7ses", "Rei\u00b7ches", "Gr\u00fcn\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er wandelt nicht den Weg zum Throne,", "tokens": ["Er", "wan\u00b7delt", "nicht", "den", "Weg", "zum", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er geht den Weg verdammter S\u00fcnder,", "tokens": ["Er", "geht", "den", "Weg", "ver\u00b7damm\u00b7ter", "S\u00fcn\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von Dornen tr\u00e4gt er eine Krone.", "tokens": ["Von", "Dor\u00b7nen", "tr\u00e4gt", "er", "ei\u00b7ne", "Kro\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er wird am Kreuz den Fluch der Erde,", "tokens": ["Er", "wird", "am", "Kreuz", "den", "Fluch", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Welt erl\u00f6send, g\u00f6ttlich b\u00fc\u00dfen;", "tokens": ["Die", "Welt", "er\u00b7l\u00f6\u00b7send", ",", "g\u00f6tt\u00b7lich", "b\u00fc\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Den Geist durchbohrt von einem Schwerte", "tokens": ["Den", "Geist", "durch\u00b7bohrt", "von", "ei\u00b7nem", "Schwer\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Steht ", "tokens": ["Steht"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-", "measure": "single.down"}}}}}