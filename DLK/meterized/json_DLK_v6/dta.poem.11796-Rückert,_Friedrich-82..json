{"dta.poem.11796": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "82.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Stets l\u00f6blich ist es, sich mit andern zu vergleichen,", "tokens": ["Stets", "l\u00f6b\u00b7lich", "ist", "es", ",", "sich", "mit", "an\u00b7dern", "zu", "ver\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "$,", "PRF", "APPR", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mag es zum Vortheil, mags zum Nachtheil dir gereichen.", "tokens": ["Mag", "es", "zum", "Vor\u00b7theil", ",", "mags", "zum", "Nacht\u00b7heil", "dir", "ge\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "$,", "VMFIN", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wo du den Vorzug hast, nie tracht' ihn zu verlieren;", "tokens": ["Wo", "du", "den", "Vor\u00b7zug", "hast", ",", "nie", "tracht'", "ihn", "zu", "ver\u00b7lie\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sieh was dir noch fehlt, um dich damit zu zieren.", "tokens": ["Und", "sieh", "was", "dir", "noch", "fehlt", ",", "um", "dich", "da\u00b7mit", "zu", "zie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PWS", "PPER", "ADV", "VVFIN", "$,", "KOUI", "PRF", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch wie du deinen hast, hat seinen Vorzug jeder;", "tokens": ["Doch", "wie", "du", "dei\u00b7nen", "hast", ",", "hat", "sei\u00b7nen", "Vor\u00b7zug", "je\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PPOSAT", "VAFIN", "$,", "VAFIN", "PPOSAT", "NN", "PIS", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit eigner schm\u00fccke dich, und nicht mit fremder Feder.", "tokens": ["Mit", "eig\u00b7ner", "schm\u00fc\u00b7cke", "dich", ",", "und", "nicht", "mit", "frem\u00b7der", "Fe\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVFIN", "PPER", "$,", "KON", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}