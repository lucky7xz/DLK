{"textgrid.poem.62776": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Geh, Betty, schlie\u00df die Halle zu", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geh, Betty, schlie\u00df die Halle zu", "tokens": ["Geh", ",", "Bet\u00b7ty", ",", "schlie\u00df", "die", "Hal\u00b7le", "zu"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "NE", "$,", "VVFIN", "ART", "NN", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und gieb die Harfe mir;", "tokens": ["Und", "gieb", "die", "Har\u00b7fe", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von einem Fr\u00e4ulein, sch\u00f6n wie Du", "tokens": ["Von", "ei\u00b7nem", "Fr\u00e4u\u00b7lein", ",", "sch\u00f6n", "wie", "Du"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "KOKOM", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sing ich ein Liedchen Dir.", "tokens": ["Sing", "ich", "ein", "Lied\u00b7chen", "Dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der gro\u00dfe Carl, ein deutscher Held,", "tokens": ["Der", "gro\u00b7\u00dfe", "Carl", ",", "ein", "deut\u00b7scher", "Held", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Fr\u00e4uleins Vater war;", "tokens": ["Des", "Fr\u00e4u\u00b7leins", "Va\u00b7ter", "war", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Sachsen schlug er aus dem Feld", "tokens": ["Die", "Sach\u00b7sen", "schlug", "er", "aus", "dem", "Feld"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und manche Maurenschaar.", "tokens": ["Und", "man\u00b7che", "Mau\u00b7ren\u00b7schaar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch Emma war so furchtbar nicht,", "tokens": ["Doch", "Em\u00b7ma", "war", "so", "furcht\u00b7bar", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "ADV", "ADJD", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mild, heiter, minnereich;", "tokens": ["Mild", ",", "hei\u00b7ter", ",", "min\u00b7ne\u00b7reich", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Rosenbeet war ihr Gesicht,", "tokens": ["Ein", "Ro\u00b7sen\u00b7beet", "war", "ihr", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Aug dem Himmel gleich.", "tokens": ["Ihr", "Aug", "dem", "Him\u00b7mel", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die schlaue Mutter hielt sie hart;", "tokens": ["Die", "schlau\u00b7e", "Mut\u00b7ter", "hielt", "sie", "hart", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Ritter kam ihr nah,", "tokens": ["Kein", "Rit\u00b7ter", "kam", "ihr", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis auf den Junker Eginhard,", "tokens": ["Bis", "auf", "den", "Jun\u00b7ker", "E\u00b7gin\u00b7hard", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Schreiber des Papa.", "tokens": ["Den", "Schrei\u00b7ber", "des", "Pa\u00b7pa", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ein h\u00fcbscher Mann aus altem Stamm,", "tokens": ["Ein", "h\u00fcb\u00b7scher", "Mann", "aus", "al\u00b7tem", "Stamm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Pechschwarz von Aug und Haar,", "tokens": ["Pech\u00b7schwarz", "von", "Aug", "und", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Flink wie ein Hirsch, sanft wie ein Lamm,", "tokens": ["Flink", "wie", "ein", "Hirsch", ",", "sanft", "wie", "ein", "Lamm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und keck wie Roland war.", "tokens": ["Und", "keck", "wie", "Ro\u00b7land", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "NE", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Den ganzen Winter gab er ihr", "tokens": ["Den", "gan\u00b7zen", "Win\u00b7ter", "gab", "er", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Schreiben Unterricht;", "tokens": ["Im", "Schrei\u00b7ben", "Un\u00b7ter\u00b7richt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Allein sie sah nicht aufs Papier,", "tokens": ["Al\u00b7lein", "sie", "sah", "nicht", "aufs", "Pa\u00b7pier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur stets ihm ins Gesicht.", "tokens": ["Nur", "stets", "ihm", "ins", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ein weiches Herz f\u00fchrt M\u00e4dchen weit", "tokens": ["Ein", "wei\u00b7ches", "Herz", "f\u00fchrt", "M\u00e4d\u00b7chen", "weit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im siebenzehnten Jahr.", "tokens": ["Im", "sie\u00b7ben\u00b7zehn\u00b7ten", "Jahr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Herr Eginhard in kurzer Zeit", "tokens": ["Herr", "E\u00b7gin\u00b7hard", "in", "kur\u00b7zer", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NE", "APPR", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Der Hahn im Korbe war.", "tokens": ["Der", "Hahn", "im", "Kor\u00b7be", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Einst hatte Carl das Zipperlein", "tokens": ["Einst", "hat\u00b7te", "Carl", "das", "Zip\u00b7per\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NE", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und zog mit seinem Weib,", "tokens": ["Und", "zog", "mit", "sei\u00b7nem", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der sch\u00f6nen Hildegard, allein", "tokens": ["Der", "sch\u00f6\u00b7nen", "Hil\u00b7de\u00b7gard", ",", "al\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NE", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Schach zum Zeitvertreib.", "tokens": ["Im", "Schach", "zum", "Zeit\u00b7ver\u00b7treib", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Im Vorsaal bebt des Schreibers Knie", "tokens": ["Im", "Vor\u00b7saal", "bebt", "des", "Schrei\u00b7bers", "Knie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor Nachtfrost. Immer wach", "tokens": ["Vor", "Nacht\u00b7frost", ".", "Im\u00b7mer", "wach"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$.", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fchrt Satan ihn, man wei\u00df nicht wie", "tokens": ["F\u00fchrt", "Sa\u00b7tan", "ihn", ",", "man", "wei\u00df", "nicht", "wie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "PPER", "$,", "PIS", "VVFIN", "PTKNEG", "KOKOM"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Emmas Schlafgemach.", "tokens": ["In", "Em\u00b7mas", "Schlaf\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Lag sie zu Bett? Die Chronika", "tokens": ["Lag", "sie", "zu", "Bett", "?", "Die", "Chro\u00b7ni\u00b7ka"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$.", "ART", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sagt nichts davon. Genug,", "tokens": ["Sagt", "nichts", "da\u00b7von", ".", "Ge\u00b7nug", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PIS", "PAV", "$.", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der arme Junker w\u00e4rmt sich da,", "tokens": ["Der", "ar\u00b7me", "Jun\u00b7ker", "w\u00e4rmt", "sich", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis Glocke zw\u00f6lfe schlug.", "tokens": ["Bis", "Glo\u00b7cke", "zw\u00f6l\u00b7fe", "schlug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "CARD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Die Mette schallt. Mit einem Ku\u00df", "tokens": ["Die", "Met\u00b7te", "schallt", ".", "Mit", "ei\u00b7nem", "Ku\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Entwich er. Doch, o weh!", "tokens": ["Ent\u00b7wich", "er", ".", "Doch", ",", "o", "weh", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "PPER", "$.", "KON", "$,", "FM", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Hof, durch den er waten mu\u00df,", "tokens": ["Im", "Hof", ",", "durch", "den", "er", "wa\u00b7ten", "mu\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "PRELS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Lag nun ein tiefer Schnee.", "tokens": ["Lag", "nun", "ein", "tie\u00b7fer", "Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Was seh ich, schrie er, gro\u00dfer Gott!", "tokens": ["Was", "seh", "ich", ",", "schrie", "er", ",", "gro\u00b7\u00dfer", "Gott", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "L\u00e4\u00dft sich mein Fu\u00dftritt sehn,", "tokens": ["L\u00e4\u00dft", "sich", "mein", "Fu\u00df\u00b7tritt", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So sterb ich heut auf dem Schaffot,", "tokens": ["So", "sterb", "ich", "heut", "auf", "dem", "Schaf\u00b7fot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Du mu\u00dft ins Kloster gehn.", "tokens": ["Du", "mu\u00dft", "ins", "Klos\u00b7ter", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Stumm, wie die Schmerzensmutter, lief", "tokens": ["Stumm", ",", "wie", "die", "Schmer\u00b7zens\u00b7mut\u00b7ter", ",", "lief"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Fr\u00e4ulein durchs Gemach;", "tokens": ["Das", "Fr\u00e4u\u00b7lein", "durchs", "Ge\u00b7mach", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf einmal stand sie still und rief:", "tokens": ["Auf", "ein\u00b7mal", "stand", "sie", "still", "und", "rief", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur mir, Geliebter, nach.", "tokens": ["Nur", "mir", ",", "Ge\u00b7lieb\u00b7ter", ",", "nach", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "PPER", "$,", "NN", "$,", "APPR", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Auf ihren Schultern tr\u00e4gt sie ihn,", "tokens": ["Auf", "ih\u00b7ren", "Schul\u00b7tern", "tr\u00e4gt", "sie", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beym klaren Mondenschein,", "tokens": ["Beym", "kla\u00b7ren", "Mon\u00b7den\u00b7schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch den beschneyten Schlo\u00dfhof hin,", "tokens": ["Durch", "den", "be\u00b7schney\u00b7ten", "Schlo\u00df\u00b7hof", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis in sein K\u00e4mmerlein.", "tokens": ["Bis", "in", "sein", "K\u00e4m\u00b7mer\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Doch ach, ihr Heilgen alle, steht", "tokens": ["Doch", "ach", ",", "ihr", "Heil\u00b7gen", "al\u00b7le", ",", "steht"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "XY", "$,", "PPOSAT", "NN", "PIS", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem armen Paare bey!", "tokens": ["Dem", "ar\u00b7men", "Paa\u00b7re", "bey", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Carl sieht aus seinem Kabinet", "tokens": ["Carl", "sieht", "aus", "sei\u00b7nem", "Ka\u00b7bi\u00b7net"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die seltne Reuterey.", "tokens": ["Die", "selt\u00b7ne", "Reu\u00b7te\u00b7rey", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Voll Wuth griff er nach seinem Schwerdt,", "tokens": ["Voll", "Wuth", "griff", "er", "nach", "sei\u00b7nem", "Schwerdt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Scho\u00df wie ein Pfeil heran:", "tokens": ["Scho\u00df", "wie", "ein", "Pfeil", "he\u00b7ran", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Sterbt beyde, rief er \u2013 Nein, bekehrt", "tokens": ["Sterbt", "bey\u00b7de", ",", "rief", "er", "\u2013", "Nein", ",", "be\u00b7kehrt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["VVFIN", "PIS", "$,", "VVFIN", "PPER", "$(", "PTKANT", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Euch erst! \u2013 Holla, Caplan!", "tokens": ["Euch", "erst", "!", "\u2013", "Hol\u00b7la", ",", "Cap\u00b7lan", "!"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "$.", "$(", "NE", "$,", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.17": {"line.1": {"text": "Der Priester h\u00f6rts; mit schwerem Kopf,", "tokens": ["Der", "Pries\u00b7ter", "h\u00f6rts", ";", "mit", "schwe\u00b7rem", "Kopf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VV