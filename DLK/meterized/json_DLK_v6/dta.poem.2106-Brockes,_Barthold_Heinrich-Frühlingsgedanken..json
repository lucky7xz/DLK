{"dta.poem.2106": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingsgedanken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn wir den Blick nun wieder abwerts drehn,", "tokens": ["Wenn", "wir", "den", "Blick", "nun", "wie\u00b7der", "ab\u00b7werts", "drehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verstummt man fast f\u00fcr Lust. Man sieht die sammtne", "tokens": ["Ver\u00b7stummt", "man", "fast", "f\u00fcr", "Lust", ".", "Man", "sieht", "die", "sammt\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADV", "APPR", "NN", "$.", "PIS", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Decken", "tokens": ["De\u00b7cken"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Der Wiesen, und ihr vor blo\u00df gr\u00fcnes Gr\u00fcn,", "tokens": ["Der", "Wie\u00b7sen", ",", "und", "ihr", "vor", "blo\u00df", "gr\u00fc\u00b7nes", "Gr\u00fcn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "PPER", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Um mehr Vergn\u00fcgen uns noch zu erwecken,", "tokens": ["Um", "mehr", "Ver\u00b7gn\u00fc\u00b7gen", "uns", "noch", "zu", "er\u00b7we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "NN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit gelben Blumen \u00fcberziehn.", "tokens": ["Mit", "gel\u00b7ben", "Blu\u00b7men", "\u00fc\u00b7ber\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kaum sieht man es so sch\u00f6n verg\u00fcldet,", "tokens": ["Kaum", "sieht", "man", "es", "so", "sch\u00f6n", "ver\u00b7g\u00fcl\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Kaum stralt ihr g\u00fcldner Glanz uns ins Gesicht;", "tokens": ["Kaum", "stralt", "ihr", "g\u00fcld\u00b7ner", "Glanz", "uns", "ins", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "So ziehet unsern Blick ein silber Licht,", "tokens": ["So", "zie\u00b7het", "un\u00b7sern", "Blick", "ein", "sil\u00b7ber", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Das sich inzwischen", "tokens": ["Das", "sich", "in\u00b7zwi\u00b7schen"], "token_info": ["word", "word", "word"], "pos": ["PDS", "PRF", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "In aufgebrochner Bl\u00fct auf B\u00e4um- und B\u00fcschen", "tokens": ["In", "auf\u00b7ge\u00b7broch\u00b7ner", "Bl\u00fct", "auf", "B\u00e4um", "und", "B\u00fc\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Von Fingern der Natur gebildet,", "tokens": ["Von", "Fin\u00b7gern", "der", "Na\u00b7tur", "ge\u00b7bil\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Mit neuer Lust von neuem in die H\u00f6h.", "tokens": ["Mit", "neu\u00b7er", "Lust", "von", "neu\u00b7em", "in", "die", "H\u00f6h", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Mein Auge stutzt, da in so kurzer Zeit", "tokens": ["Mein", "Au\u00b7ge", "stutzt", ",", "da", "in", "so", "kur\u00b7zer", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "KOUS", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Ich unsre Welt in solcher Herrlichkeit", "tokens": ["Ich", "uns\u00b7re", "Welt", "in", "sol\u00b7cher", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Verg\u00fcldet und versilbert seh,", "tokens": ["Ver\u00b7g\u00fcl\u00b7det", "und", "ver\u00b7sil\u00b7bert", "seh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Wobey von Gras und Laub das fast schmaragdne Gr\u00fcn", "tokens": ["Wo\u00b7bey", "von", "Gras", "und", "Laub", "das", "fast", "schma\u00b7ragd\u00b7ne", "Gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "KON", "NN", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Noch desto lieblicher zu gl\u00e4nzen schien.", "tokens": ["Noch", "des\u00b7to", "lieb\u00b7li\u00b7cher", "zu", "gl\u00e4n\u00b7zen", "schien", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}