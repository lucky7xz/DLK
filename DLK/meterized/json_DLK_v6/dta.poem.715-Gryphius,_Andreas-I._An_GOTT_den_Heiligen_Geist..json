{"dta.poem.715": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "I.  An GOTT den Heiligen Geist.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Dfewer wahrer Lieb! O Brunn der guten ", "tokens": ["Dfe\u00b7wer", "wah\u00b7rer", "Lieb", "!", "O", "Brunn", "der", "gu\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$.", "NE", "NE", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "gaben!", "tokens": ["ga\u00b7ben", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "O ", "tokens": ["O"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "ligkeit!", "tokens": ["lig\u00b7keit", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "O dreymal grosser Gott", "tokens": ["O", "drey\u00b7mal", "gros\u00b7ser", "Gott"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADV", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "leid", "tokens": ["leid"], "token_info": ["word"], "pos": ["ADJD"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Vertreibt! O keusche Taub! O furcht der", "tokens": ["Ver\u00b7treibt", "!", "O", "keu\u00b7sche", "Taub", "!", "O", "furcht", "der"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$.", "NE", "ADJA", "NN", "$.", "NE", "NN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "H\u00f6llen Raben!", "tokens": ["H\u00f6l\u00b7len", "Ra\u00b7ben", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Die/ ehr das w\u00fcste Meer/ mit Bergen rings vmbgraben/", "tokens": ["Die", "/", "ehr", "das", "w\u00fcs\u00b7te", "Meer", "/", "mit", "Ber\u00b7gen", "rings", "vmb\u00b7gra\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "NN", "ART", "ADJA", "NN", "$(", "APPR", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ehr Luft vnd Erden ward/ ehr das gestirnte kleid", "tokens": ["Ehr", "Luft", "vnd", "Er\u00b7den", "ward", "/", "ehr", "das", "ge\u00b7stirn\u00b7te", "kleid"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "KON", "NN", "VAFIN", "$(", "NN", "ART", "ADJA", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dem Himmel angelegt/ vor ewigkeit vnd zeitt/", "tokens": ["Dem", "Him\u00b7mel", "an\u00b7ge\u00b7legt", "/", "vor", "e\u00b7wig\u00b7keit", "vnd", "zeitt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$(", "APPR", "NN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die zwey die gantz dir gleich/ von fich gelassen haben.", "tokens": ["Die", "zwey", "die", "gantz", "dir", "gleich", "/", "von", "fich", "ge\u00b7las\u00b7sen", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "ART", "ADV", "PPER", "ADV", "$(", "APPR", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}