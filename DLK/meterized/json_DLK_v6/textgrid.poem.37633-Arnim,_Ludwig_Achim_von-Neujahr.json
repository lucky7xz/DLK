{"textgrid.poem.37633": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Neujahr", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Altes Jahr, du ruhst in Frieden,", "tokens": ["Al\u00b7tes", "Jahr", ",", "du", "ruhst", "in", "Frie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine Augen sind geschlossen;", "tokens": ["Dei\u00b7ne", "Au\u00b7gen", "sind", "ge\u00b7schlos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bist von uns so still geschieden", "tokens": ["Bist", "von", "uns", "so", "still", "ge\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPER", "ADV", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hin zu himmlischen Genossen,", "tokens": ["Hin", "zu", "himm\u00b7li\u00b7schen", "Ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die neuen Jahre kommen,", "tokens": ["Und", "die", "neu\u00b7en", "Jah\u00b7re", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Werden auch wie du vergehen,", "tokens": ["Wer\u00b7den", "auch", "wie", "du", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "KOKOM", "PPER", "VVINF", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Bis wir alle aufgenommen", "tokens": ["Bis", "wir", "al\u00b7le", "auf\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIS", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Uns im letzten wiedersehen.", "tokens": ["Uns", "im", "letz\u00b7ten", "wie\u00b7der\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wenn dies letzte angefangen,", "tokens": ["Wenn", "dies", "letz\u00b7te", "an\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ADJA", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Deutet sich dies Neujahrgr\u00fc\u00dfen,", "tokens": ["Deu\u00b7tet", "sich", "dies", "Neu\u00b7jahr\u00b7gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PDS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Denn erkannt ist dies Verlangen,", "tokens": ["Denn", "er\u00b7kannt", "ist", "dies", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "PDS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Nach dem Wiedersehn und K\u00fcssen.", "tokens": ["Nach", "dem", "Wie\u00b7der\u00b7sehn", "und", "K\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}