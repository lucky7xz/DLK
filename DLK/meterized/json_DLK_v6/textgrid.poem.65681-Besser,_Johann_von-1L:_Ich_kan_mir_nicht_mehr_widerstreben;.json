{"textgrid.poem.65681": {"metadata": {"author": {"name": "Besser, Johann von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich kan mir nicht mehr widerstreben;", "genre": "verse", "period": "N.A.", "pub_year": 1691, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kan mir nicht mehr widerstreben;", "tokens": ["Ich", "kan", "mir", "nicht", "mehr", "wi\u00b7der\u00b7stre\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die sch\u00f6nheit fl\u00f6\u00dft mir das gel\u00fcsten ein.", "tokens": ["Die", "sch\u00f6n\u00b7heit", "fl\u00f6\u00dft", "mir", "das", "ge\u00b7l\u00fcs\u00b7ten", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im Paradie\u00df kan keiner leben /", "tokens": ["Im", "Pa\u00b7ra\u00b7die\u00df", "kan", "kei\u00b7ner", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ohne fall und fehl-tritt seyn.", "tokens": ["Und", "oh\u00b7ne", "fall", "und", "fehl\u00b7tritt", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein Edens-platz / mein kind Caliste /", "tokens": ["Dein", "E\u00b7dens\u00b7platz", "/", "mein", "kind", "Ca\u00b7lis\u00b7te", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "NN", "NE", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zieht meine hand", "tokens": ["Zieht", "mei\u00b7ne", "hand"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Auff deinen krey\u00df der rundten br\u00fcste /", "tokens": ["Auff", "dei\u00b7nen", "krey\u00df", "der", "rund\u00b7ten", "br\u00fcs\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und meinen leib in dein gelobtes land.", "tokens": ["Und", "mei\u00b7nen", "leib", "in", "dein", "ge\u00b7lob\u00b7tes", "land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der lentz pflegt uns in herbst zu leiten;", "tokens": ["Der", "lentz", "pflegt", "uns", "in", "herbst", "zu", "lei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das jahr l\u00e4st uns nach blumen fr\u00fcchte sehn:", "tokens": ["Das", "jahr", "l\u00e4st", "uns", "nach", "blu\u00b7men", "fr\u00fcch\u00b7te", "sehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ADJA", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "la\u00df mich doch auch nach deinen zeiten", "tokens": ["la\u00df", "mich", "doch", "auch", "nach", "dei\u00b7nen", "zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In deinen anmuths-garten gehn.", "tokens": ["In", "dei\u00b7nen", "an\u00b7muths\u00b7gar\u00b7ten", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein fr\u00fchling ist ein ku\u00df gewesen /", "tokens": ["Mein", "fr\u00fch\u00b7ling", "ist", "ein", "ku\u00df", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VAFIN", "ART", "NN", "VAPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "La\u00df aus der schoo\u00df", "tokens": ["La\u00df", "aus", "der", "schoo\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "ART", "VVFIN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Mich endlich reiffe fr\u00fcchte lesen /", "tokens": ["Mich", "end\u00b7lich", "reif\u00b7fe", "fr\u00fcch\u00b7te", "le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wie in dem stand der unschuld nackt und blo\u00df.", "tokens": ["Wie", "in", "dem", "stand", "der", "un\u00b7schuld", "nackt", "und", "blo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "VVFIN", "ART", "NN", "ADJD", "KON", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du kanst den leib mir nicht verschliessen/", "tokens": ["Du", "kanst", "den", "leib", "mir", "nicht", "ver\u00b7schlies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PPER", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von welchem du mir schon das hertz entdeckt.", "tokens": ["Von", "wel\u00b7chem", "du", "mir", "schon", "das", "hertz", "ent\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "La\u00df unsern geist zusammen fliessen /", "tokens": ["La\u00df", "un\u00b7sern", "geist", "zu\u00b7sam\u00b7men", "flies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil doch kein ku\u00df ihm selber schmeckt.", "tokens": ["Weil", "doch", "kein", "ku\u00df", "ihm", "sel\u00b7ber", "schmeckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vergrabe mich in helffenbeine /", "tokens": ["Ver\u00b7gra\u00b7be", "mich", "in", "helf\u00b7fen\u00b7bei\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Voll fleisch und blut;", "tokens": ["Voll", "fleisch", "und", "blut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Denn werd ich gleich darinn zum steine /", "tokens": ["Denn", "werd", "ich", "gleich", "da\u00b7rinn", "zum", "stei\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PAV", "APPRART", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So wei\u00df ich doch/ da\u00df es mir sanffte thut.", "tokens": ["So", "wei\u00df", "ich", "doch", "/", "da\u00df", "es", "mir", "sanff\u00b7te", "thut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$(", "KOUS", "PPER", "PPER", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Er\u00f6ffne mir das thor zum lande /", "tokens": ["Er\u00b7\u00f6ff\u00b7ne", "mir", "das", "thor", "zum", "lan\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo zucker rinnt / und wollust tafel h\u00e4lt;", "tokens": ["Wo", "zu\u00b7cker", "rinnt", "/", "und", "wol\u00b7lust", "ta\u00b7fel", "h\u00e4lt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "$(", "KON", "VMFIN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "La\u00df meinen kahn am engen strande", "tokens": ["La\u00df", "mei\u00b7nen", "kahn", "am", "en\u00b7gen", "stran\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In deine neu-erfundne welt.", "tokens": ["In", "dei\u00b7ne", "neu\u00b7er\u00b7fund\u00b7ne", "welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du darffst dich nicht / Caliste / sch\u00e4men;", "tokens": ["Du", "darffst", "dich", "nicht", "/", "Ca\u00b7lis\u00b7te", "/", "sch\u00e4\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$(", "NE", "$(", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das feigen-blat /", "tokens": ["Das", "fei\u00b7gen\u00b7blat", "/"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Das Eva f\u00fcr sich muste nehmen /", "tokens": ["Das", "E\u00b7va", "f\u00fcr", "sich", "mus\u00b7te", "neh\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zeigt und verdeckt nicht unsre lagerstatt.", "tokens": ["Zeigt", "und", "ver\u00b7deckt", "nicht", "uns\u00b7re", "la\u00b7ger\u00b7statt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Bestraffe mich mit keinem tadel /", "tokens": ["Be\u00b7straf\u00b7fe", "mich", "mit", "kei\u00b7nem", "ta\u00b7del", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df deinen schoo\u00df mein hertze lieb gewinnt;", "tokens": ["Da\u00df", "dei\u00b7nen", "schoo\u00df", "mein", "hert\u00b7ze", "lieb", "ge\u00b7winnt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "VVFIN", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn der magnet forscht mit der nadel /", "tokens": ["Denn", "der", "mag\u00b7net", "forscht", "mit", "der", "na\u00b7del", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Bi\u00df er den mittel-punct ergr\u00fcndt.", "tokens": ["Bi\u00df", "er", "den", "mit\u00b7tel\u00b7punct", "er\u00b7gr\u00fcndt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein sch\u00e4fgen weidt in thal und auen /", "tokens": ["Ein", "sch\u00e4f\u00b7gen", "weidt", "in", "thal", "und", "au\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "NE", "KON", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wo schatten ist;", "tokens": ["Wo", "schat\u00b7ten", "ist", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Mein hertze will das deine schauen;", "tokens": ["Mein", "hert\u00b7ze", "will", "das", "dei\u00b7ne", "schau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ART", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Drum such ich es / da / wo du offen bist.", "tokens": ["Drum", "such", "ich", "es", "/", "da", "/", "wo", "du", "of\u00b7fen", "bist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "$(", "KOUS", "$(", "PWAV", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}