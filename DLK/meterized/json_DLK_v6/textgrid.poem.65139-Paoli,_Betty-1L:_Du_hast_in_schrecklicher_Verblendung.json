{"textgrid.poem.65139": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du hast in schrecklicher Verblendung", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast in schrecklicher Verblendung", "tokens": ["Du", "hast", "in", "schreck\u00b7li\u00b7cher", "Ver\u00b7blen\u00b7dung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Umstrickt uns mit so schwerem Bann,", "tokens": ["Um\u00b7strickt", "uns", "mit", "so", "schwe\u00b7rem", "Bann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df keine milde Schicksalswendung,", "tokens": ["Da\u00df", "kei\u00b7ne", "mil\u00b7de", "Schick\u00b7sals\u00b7wen\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df nur der Tod ihn l\u00f6sen kann!", "tokens": ["Da\u00df", "nur", "der", "Tod", "ihn", "l\u00f6\u00b7sen", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von einem finstern Geist getrieben,", "tokens": ["Von", "ei\u00b7nem", "fins\u00b7tern", "Geist", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hast du die F\u00e4den so verwirrt,", "tokens": ["Hast", "du", "die", "F\u00e4\u00b7den", "so", "ver\u00b7wirrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df dir mein Leben und mein Lieben", "tokens": ["Da\u00df", "dir", "mein", "Le\u00b7ben", "und", "mein", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "KON", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zum Unheil und zum Fluche wird!", "tokens": ["Zum", "Un\u00b7heil", "und", "zum", "Flu\u00b7che", "wird", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ob ich dir darum grolle, z\u00fcrne?", "tokens": ["Ob", "ich", "dir", "da\u00b7rum", "grol\u00b7le", ",", "z\u00fcr\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PAV", "ADJA", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nein, du Geliebter! ewig nein!", "tokens": ["Nein", ",", "du", "Ge\u00b7lieb\u00b7ter", "!", "e\u00b7wig", "nein", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "NN", "$.", "ADJD", "PTKANT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seh' ich denn nicht auf deiner Stirne", "tokens": ["Seh'", "ich", "denn", "nicht", "auf", "dei\u00b7ner", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das dunkle Flammenmal der Pein?", "tokens": ["Das", "dunk\u00b7le", "Flam\u00b7men\u00b7mal", "der", "Pein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vom Schmerz bew\u00e4ltigt deiner Seele,", "tokens": ["Vom", "Schmerz", "be\u00b7w\u00e4l\u00b7tigt", "dei\u00b7ner", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie h\u00e4tt' ich meines Leides Acht?", "tokens": ["Wie", "h\u00e4tt'", "ich", "mei\u00b7nes", "Lei\u00b7des", "Acht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPOSAT", "NN", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wie ged\u00e4cht ich noch der Fehle,", "tokens": ["Und", "wie", "ge\u00b7d\u00e4cht", "ich", "noch", "der", "Feh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVPP", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die solchen Jammer dir gebracht? \u2013", "tokens": ["Die", "sol\u00b7chen", "Jam\u00b7mer", "dir", "ge\u00b7bracht", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PIAT", "NN", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Rettungsbr\u00fccke dir zu schlagen,", "tokens": ["Die", "Ret\u00b7tungs\u00b7br\u00fc\u00b7cke", "dir", "zu", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O letzte, tiefste Seligkeit!", "tokens": ["O", "letz\u00b7te", ",", "tiefs\u00b7te", "Se\u00b7lig\u00b7keit", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch nicht durch Dulden und Entsagen,", "tokens": ["Doch", "nicht", "durch", "Dul\u00b7den", "und", "Ent\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vers\u00f6hnt sich dieser herbe Streit!", "tokens": ["Ver\u00b7s\u00f6hnt", "sich", "die\u00b7ser", "her\u00b7be", "Streit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich kann nur noch in meinem Blute", "tokens": ["Ich", "kann", "nur", "noch", "in", "mei\u00b7nem", "Blu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Dienste deines Gl\u00fccks mich weih'n,", "tokens": ["Dem", "Diens\u00b7te", "dei\u00b7nes", "Gl\u00fccks", "mich", "weih'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und, s\u00fchnend, mit gehob'nem Muthe,", "tokens": ["Und", ",", "s\u00fch\u00b7nend", ",", "mit", "ge\u00b7hob'\u00b7nem", "Mu\u00b7the", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVPP", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Priester und das Opfer sein!", "tokens": ["Der", "Pries\u00b7ter", "und", "das", "Op\u00b7fer", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}