{"textgrid.poem.40051": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Vorwurf, der so hoch, als f\u00fcrchterlich,", "genre": "verse", "period": "N.A.", "pub_year": 1713, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Vorwurf, der so hoch, als f\u00fcrchterlich,", "tokens": ["O", "Vor\u00b7wurf", ",", "der", "so", "hoch", ",", "als", "f\u00fcrch\u00b7ter\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "ADV", "ADJD", "$,", "KOUS", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "De\u00df Unerme\u00dflichkeit das Hertz mit Schrecken r\u00fchret,", "tokens": ["De\u00df", "Un\u00b7er\u00b7me\u00df\u00b7lich\u00b7keit", "das", "Hertz", "mit", "Schre\u00b7cken", "r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O Abgrund, den kein Mensch begreift, in welchem sich", "tokens": ["O", "Ab\u00b7grund", ",", "den", "kein", "Mensch", "be\u00b7greift", ",", "in", "wel\u00b7chem", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,", "APPR", "PRELS", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein gantz verwirrter Geist verlieret;", "tokens": ["Mein", "gantz", "ver\u00b7wirr\u00b7ter", "Geist", "ver\u00b7lie\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mit welchen Farben mahl' ich dich!", "tokens": ["Mit", "wel\u00b7chen", "Far\u00b7ben", "mahl'", "ich", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du tiefes Meer der Zeiten, die vergehen;", "tokens": ["Du", "tie\u00b7fes", "Meer", "der", "Zei\u00b7ten", ",", "die", "ver\u00b7ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ART", "NN", "$,", "PRELS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus dir kommt jedes Jahr, das wieder in dich f\u00e4llt,", "tokens": ["Aus", "dir", "kommt", "je\u00b7des", "Jahr", ",", "das", "wie\u00b7der", "in", "dich", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PIAT", "NN", "$,", "PRELS", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du k\u00fcnftigs Grab von unsrer Welt,", "tokens": ["Du", "k\u00fcnf\u00b7tigs", "Grab", "von", "uns\u00b7rer", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du Quell, woraus dereinst die k\u00fcnft'gen Welt' entstehen!", "tokens": ["Du", "Quell", ",", "wo\u00b7raus", "de\u00b7reinst", "die", "k\u00fcnft'\u00b7gen", "Welt'", "ent\u00b7ste\u00b7hen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PWAV", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Entstehn, sich enden, sterben, leben,", "tokens": ["Ent\u00b7stehn", ",", "sich", "en\u00b7den", ",", "ster\u00b7ben", ",", "le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "PRF", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verweilen, folgen, Aufschub geben,", "tokens": ["Ver\u00b7wei\u00b7len", ",", "fol\u00b7gen", ",", "Auf\u00b7schub", "ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVINF", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sind W\u00f6rter, die bey dir nichts heissen und nichts seyn,", "tokens": ["Sind", "W\u00f6r\u00b7ter", ",", "die", "bey", "dir", "nichts", "heis\u00b7sen", "und", "nichts", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "PRELS", "APPR", "PPER", "PIS", "VVINF", "KON", "PIS", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Folge der Natur, die Zeiten, so verschwunden,", "tokens": ["Die", "Fol\u00b7ge", "der", "Na\u00b7tur", ",", "die", "Zei\u00b7ten", ",", "so", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Versencken, samt den k\u00fcnft'gen Stunden,", "tokens": ["Ver\u00b7sen\u00b7cken", ",", "samt", "den", "k\u00fcnft'\u00b7gen", "Stun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr kurtzes Seyn in dich, als einen Punct, hinein.", "tokens": ["Ihr", "kurt\u00b7zes", "Seyn", "in", "dich", ",", "als", "ei\u00b7nen", "Punct", ",", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PPER", "$,", "KOUS", "ART", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ihr Stunden, Tag', ihr Wochen, Monden, Jahr',", "tokens": ["Ihr", "Stun\u00b7den", ",", "Tag'", ",", "ihr", "Wo\u00b7chen", ",", "Mon\u00b7den", ",", "Jahr'", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "PPOSAT", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Fort, h\u00e4uft euch auf einander auf!", "tokens": ["Fort", ",", "h\u00e4uft", "euch", "auf", "ein\u00b7an\u00b7der", "auf", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Eilt, fliegt, erf\u00fcllet euren Lauf!", "tokens": ["Eilt", ",", "fliegt", ",", "er\u00b7f\u00fcl\u00b7let", "eu\u00b7ren", "Lauf", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erschreckt uns durch die Zahl der ungez\u00e4hlten Schaar.", "tokens": ["Er\u00b7schreckt", "uns", "durch", "die", "Zahl", "der", "un\u00b7ge\u00b7z\u00e4hl\u00b7ten", "Schaar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Welch ein gewaltigs Heer! Vergebens sucht das Dencken", "tokens": ["Welch", "ein", "ge\u00b7wal\u00b7tigs", "Heer", "!", "Ver\u00b7ge\u00b7bens", "sucht", "das", "Den\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "ART", "ADJA", "NN", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der tiefen Algebra darein sich zu versencken.", "tokens": ["Der", "tie\u00b7fen", "Al\u00b7ge\u00b7bra", "da\u00b7rein", "sich", "zu", "ver\u00b7sen\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PAV", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Allein, was seyd ihr doch bey der Unendlichkeit,", "tokens": ["Al\u00b7lein", ",", "was", "seyd", "ihr", "doch", "bey", "der", "Un\u00b7end\u00b7lich\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Aus welcher ihr gebohren seyd?", "tokens": ["Aus", "wel\u00b7cher", "ihr", "ge\u00b7boh\u00b7ren", "seyd", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr seyd nicht einst geschickt, sie anzufangen.", "tokens": ["Ihr", "seyd", "nicht", "einst", "ge\u00b7schickt", ",", "sie", "an\u00b7zu\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "VVPP", "$,", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Die Thaten, wovon itzt so viele melden,", "tokens": ["Die", "Tha\u00b7ten", ",", "wo\u00b7von", "itzt", "so", "vie\u00b7le", "mel\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ADV", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der edlen Geister Frucht, versincken samt den Helden", "tokens": ["Der", "ed\u00b7len", "Geis\u00b7ter", "Frucht", ",", "ver\u00b7sin\u00b7cken", "samt", "den", "Hel\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In eine finstre Nacht.", "tokens": ["In", "ei\u00b7ne", "finst\u00b7re", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Viel tausend herrliche, vortreffliche Gedancken", "tokens": ["Viel", "tau\u00b7send", "herr\u00b7li\u00b7che", ",", "vor\u00b7treff\u00b7li\u00b7che", "Ge\u00b7dan\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "CARD", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Versencken sich mit dem, der sie gedacht,", "tokens": ["Ver\u00b7sen\u00b7cken", "sich", "mit", "dem", ",", "der", "sie", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In seines Sarges enge Schrancken.", "tokens": ["In", "sei\u00b7nes", "Sar\u00b7ges", "en\u00b7ge", "Schran\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Unverg\u00e4nglichkeit, mit welcher ihre Seelen", "tokens": ["Die", "Un\u00b7ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", ",", "mit", "wel\u00b7cher", "ih\u00b7re", "See\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sich, voll von eitlem Stoltz, verm\u00e4hlen,", "tokens": ["Sich", ",", "voll", "von", "eit\u00b7lem", "Stoltz", ",", "ver\u00b7m\u00e4h\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PRF", "$,", "ADJD", "APPR", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ist bey der Ewigkeit ein kleines B\u00e4chlein nur,", "tokens": ["Ist", "bey", "der", "E\u00b7wig\u00b7keit", "ein", "klei\u00b7nes", "B\u00e4ch\u00b7lein", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Von dessen Kriechen man im Grase kaum die Spur", "tokens": ["Von", "des\u00b7sen", "Krie\u00b7chen", "man", "im", "Gra\u00b7se", "kaum", "die", "Spur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "PIS", "APPRART", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Gewahr wird, und das sich im Ocean verlieret,", "tokens": ["Ge\u00b7wahr", "wird", ",", "und", "das", "sich", "im", "O\u00b7cean", "ver\u00b7lie\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "KON", "PRELS", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.12": {"text": "Wohin sein Lauf es f\u00fchret.", "tokens": ["Wo\u00b7hin", "sein", "Lauf", "es", "f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ihr festen Ehren-Mahl', ihr stoltzen Mausoleen,", "tokens": ["Ihr", "fes\u00b7ten", "Eh\u00b7ren\u00b7Mahl'", ",", "ihr", "stolt\u00b7zen", "Mau\u00b7so\u00b7leen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Umsonst sucht euer Grund von Ertz und Marmor-Stein", "tokens": ["Um\u00b7sonst", "sucht", "eu\u00b7er", "Grund", "von", "Ertz", "und", "Mar\u00b7mor\u00b7Stein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bey V\u00f6lckern, die annoch von uns entfernet seyn,", "tokens": ["Bey", "V\u00f6l\u00b7ckern", ",", "die", "an\u00b7noch", "von", "uns", "ent\u00b7fer\u00b7net", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADV", "APPR", "PPER", "VVFIN", "VAINF", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Den Ruhm, nein mehr den Hochmuths-Tand,", "tokens": ["Den", "Ruhm", ",", "nein", "mehr", "den", "Hoch\u00b7muths\u00b7Tand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKANT", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von Rom und Griechen-Land", "tokens": ["Von", "Rom", "und", "Grie\u00b7chen\u00b7Land"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Zu \u00fcbertragen, zu erh\u00f6hen;", "tokens": ["Zu", "\u00fc\u00b7ber\u00b7tra\u00b7gen", ",", "zu", "er\u00b7h\u00f6\u00b7hen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ihr werdet alle schnell, dem Schatten gleich, vergehen.", "tokens": ["Ihr", "wer\u00b7det", "al\u00b7le", "schnell", ",", "dem", "Schat\u00b7ten", "gleich", ",", "ver\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "ADJD", "$,", "ART", "NN", "ADV", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Ewigkeit in ihrer d\u00fcstern Nacht,", "tokens": ["Die", "E\u00b7wig\u00b7keit", "in", "ih\u00b7rer", "d\u00fcs\u00b7tern", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "In welcher sie aus tausend Dingen,", "tokens": ["In", "wel\u00b7cher", "sie", "aus", "tau\u00b7send", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die allbereit dahin sind und vergingen,", "tokens": ["Die", "all\u00b7be\u00b7reit", "da\u00b7hin", "sind", "und", "ver\u00b7gin\u00b7gen", ","], "token_info": 