{"dta.poem.15620": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "53.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Welt ist immer ganz, die du in Theile brachtest;", "tokens": ["Die", "Welt", "ist", "im\u00b7mer", "ganz", ",", "die", "du", "in", "Thei\u00b7le", "brach\u00b7test", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Ganzes wird der Theil, den du f\u00fcr sich betrachtest:", "tokens": ["Ein", "Gan\u00b7zes", "wird", "der", "Theil", ",", "den", "du", "f\u00fcr", "sich", "be\u00b7trach\u00b7test", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie einen Blumenstraus aus einem Kranz heraus", "tokens": ["Wie", "ei\u00b7nen", "Blu\u00b7men\u00b7straus", "aus", "ei\u00b7nem", "Kranz", "he\u00b7raus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du nehmen kannst und dann ein Bl\u00fcmchen aus dem Straus;", "tokens": ["Du", "neh\u00b7men", "kannst", "und", "dann", "ein", "Bl\u00fcm\u00b7chen", "aus", "dem", "Straus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "KON", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und alle Blumen kannst in Str\u00e4u\u00dfe wieder f\u00fcgen,", "tokens": ["Und", "al\u00b7le", "Blu\u00b7men", "kannst", "in", "Str\u00e4u\u00b7\u00dfe", "wie\u00b7der", "f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VMFIN", "APPR", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und immer neu den Kranz erschaffen zum Vergn\u00fcgen.", "tokens": ["Und", "im\u00b7mer", "neu", "den", "Kranz", "er\u00b7schaf\u00b7fen", "zum", "Ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ART", "NN", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wirst mit einander du Un\u00e4hnlichstes verbinden,", "tokens": ["Wirst", "mit", "ein\u00b7an\u00b7der", "du", "Un\u00b7\u00e4hn\u00b7lichs\u00b7tes", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PRF", "NE", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird sich die Aehnlichkeit von selbst dazwischen finden.", "tokens": ["Wird", "sich", "die", "A\u00b7ehn\u00b7lich\u00b7keit", "von", "selbst", "da\u00b7zwi\u00b7schen", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ART", "NN", "APPR", "ADV", "PAV", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}, "stanza.5": {"line.1": {"text": "Von jedem Dinge geht zu jedem eine Br\u00fccke,", "tokens": ["Von", "je\u00b7dem", "Din\u00b7ge", "geht", "zu", "je\u00b7dem", "ei\u00b7ne", "Br\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "APPR", "PIS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und augenblicklich f\u00fcllt Einbildungskraft die L\u00fccke.", "tokens": ["Und", "au\u00b7gen\u00b7blick\u00b7lich", "f\u00fcllt", "Ein\u00b7bil\u00b7dungs\u00b7kraft", "die", "L\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Doch das Gef\u00fchl, womit du sie auf dich beziehst,", "tokens": ["Doch", "das", "Ge\u00b7f\u00fchl", ",", "wo\u00b7mit", "du", "sie", "auf", "dich", "be\u00b7ziehst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PWAV", "PPER", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Macht da\u00df du sch\u00f6n um dich die Welt geordnet siehst.", "tokens": ["Macht", "da\u00df", "du", "sch\u00f6n", "um", "dich", "die", "Welt", "ge\u00b7ord\u00b7net", "siehst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "ADJD", "APPR", "PRF", "ART", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}