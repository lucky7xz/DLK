{"dta.poem.20612": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Der Knabe  \n im Erdbeerschlag .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1803", "urn": "urn:nbn:de:kobv:b4-200905192133", "language": ["de:0.99"], "booktitle": "[Hebel, Johann Peter]: Allemannische Gedichte. Karlsruhe, 1803."}, "poem": {"stanza.1": {"line.1": {"text": "E B\u00fcebli lauft, es goht in Wald               ", "tokens": ["E", "B\u00fceb\u00b7li", "lauft", ",", "es", "goht", "in", "Wald"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "am Sunntig Nomittag;", "tokens": ["am", "Sunn\u00b7tig", "No\u00b7mit\u00b7tag", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "es chunnt in d\u2019 H\u00fcrst und findet bald", "tokens": ["es", "chunnt", "in", "d'", "H\u00fcrst", "und", "fin\u00b7det", "bald"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "APPR", "NE", "NN", "KON", "VVFIN", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Erberi Schlag an Schlag;", "tokens": ["Er\u00b7be\u00b7ri", "Schlag", "an", "Schlag", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "NN", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.5": {"text": "es g\u00fcnnt und i\u00dft si halber z\u2019 tod,", "tokens": ["es", "g\u00fcnnt", "und", "i\u00dft", "si", "hal\u00b7ber", "z'", "tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "und denkt: \u201eDas isch mi Obebrod.\u201c", "tokens": ["und", "denkt", ":", "\u201e", "Das", "isch", "mi", "O\u00b7be\u00b7brod", ".", "\u201c"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "ART", "FM", "FM", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und wie nes i\u00dft, se ruuschts im Laub;", "tokens": ["Und", "wie", "nes", "i\u00dft", ",", "se", "ruuschts", "im", "Laub", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "VVFIN", "$,", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "es chunnt e sch\u00f6ne Chnab.", "tokens": ["es", "chunnt", "e", "sch\u00f6\u00b7ne", "Chnab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er het e Rock, wie Silberstaub,", "tokens": ["Er", "het", "e", "Rock", ",", "wie", "Sil\u00b7ber\u00b7staub", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "NN", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und treit e goldige Stab;", "tokens": ["und", "treit", "e", "gol\u00b7di\u00b7ge", "Stab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NE", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "er gl\u00e4nzt wie d\u2019 Sunn am Schwitzer-Schnee;", "tokens": ["er", "gl\u00e4nzt", "wie", "d'", "Sunn", "am", "Schwit\u00b7zer\u00b7Schnee", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NE", "NE", "APPRART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "si lebelang hets n\u00fct so gseh.", "tokens": ["si", "le\u00b7be\u00b7lang", "hets", "n\u00fct", "so", "gseh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "Druf redt der Chnab mi B\u00fcebli a:", "tokens": ["Druf", "redt", "der", "Chnab", "mi", "B\u00fceb\u00b7li", "a", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "FM", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201ewas i\u00dfisch, i halts mit?\u201c \u2014", "tokens": ["\u201e", "was", "i\u00b7\u00dfisch", ",", "i", "halts", "mit", "?", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PWS", "ADJD", "$,", "NE", "VVFIN", "PTKVZ", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201ehe, n\u00fct,\u201c seit \u2019s B\u00fcebli, luegt en a,", "tokens": ["\u201e", "he", ",", "n\u00fct", ",", "\u201c", "seit", "'s", "B\u00fceb\u00b7li", ",", "luegt", "en", "a", ","], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "$,", "VVFIN", "$,", "$(", "APPR", "PPER", "NE", "$,", "VVFIN", "FM", "FM", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "und l\u00fcpft si Ch\u00e4ppli nit.", "tokens": ["und", "l\u00fcpft", "si", "Ch\u00e4pp\u00b7li", "nit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "NE", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Druf seit der Chnab: \u201eHe, i\u00dfisch n\u00fct,", "tokens": ["Druf", "seit", "der", "Chnab", ":", "\u201e", "He", ",", "i\u00b7\u00dfisch", "n\u00fct", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$.", "$(", "ITJ", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u201edu grobe Burst, se battet \u2019s n\u00fct!\u201c", "tokens": ["\u201e", "du", "gro\u00b7be", "Burst", ",", "se", "bat\u00b7tet", "'s", "n\u00fct", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Verschwunden isch mi Chnab, unds st\u00f6hn", "tokens": ["Ver\u00b7schwun\u00b7den", "isch", "mi", "Chnab", ",", "unds", "st\u00f6hn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "KON", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "die n\u00f6chste H\u00fcrst im Duft;", "tokens": ["die", "n\u00f6chs\u00b7te", "H\u00fcrst", "im", "Duft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "drus fliegt en Engeli wundersch\u00f6n", "tokens": ["drus", "fliegt", "en", "En\u00b7ge\u00b7li", "wun\u00b7der\u00b7sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "NE", "NE", "NE"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "uf in die blaue Luft,", "tokens": ["uf", "in", "die", "blau\u00b7e", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "und \u2019s B\u00fcebli stoht, und luegt em no,", "tokens": ["und", "'s", "B\u00fceb\u00b7li", "stoht", ",", "und", "luegt", "em", "no", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NE", "VVFIN", "$,", "KON", "VVFIN", "FM", "FM", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "und chrazt im Hoor, und lauft dervo.", "tokens": ["und", "chrazt", "im", "Hoor", ",", "und", "lauft", "der\u00b7vo", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,", "KON", "VVFIN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und sieder isch kei Sege meh", "tokens": ["Und", "sie\u00b7der", "isch", "kei", "Se\u00b7ge", "meh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJD", "PIAT", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "im Beeri-Esse gsi.", "tokens": ["im", "Bee\u00b7ri\u00b7Es\u00b7se", "gsi", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "J ha mi lebtig n\u00fct so gseh,", "tokens": ["J", "ha", "mi", "leb\u00b7tig", "n\u00fct", "so", "gseh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "ADJD", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "sie bschie\u00dfen ebe nie.", "tokens": ["sie", "bschie\u00b7\u00dfen", "e\u00b7be", "nie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "I\u00df hampflevoll, so viel de witt,", "tokens": ["I\u00df", "hampf\u00b7le\u00b7voll", ",", "so", "viel", "de", "witt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "ADV", "ADV", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "sie stille der di Hunger nit!", "tokens": ["sie", "stil\u00b7le", "der", "di", "Hun\u00b7ger", "nit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NE", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Was gibi der f\u00fcr Lehre dri?", "tokens": ["Was", "gi\u00b7bi", "der", "f\u00fcr", "Leh\u00b7re", "dri", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was seisch derzu? Me mue\u00df", "tokens": ["Was", "seisch", "der\u00b7zu", "?", "Me", "mu\u00b7e\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "ADJD", "PAV", "$.", "NE", "NE"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "vor fremde L\u00fcte fr\u00fcndli si", "tokens": ["vor", "frem\u00b7de", "L\u00fc\u00b7te", "fr\u00fcnd\u00b7li", "si"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "XY", "XY"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "mit Wort und Red und Grue\u00df,", "tokens": ["mit", "Wort", "und", "Red", "und", "Grue\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "und \u2019s Ch\u00e4ppli l\u00fcpfe z\u2019 rechter Zit", "tokens": ["und", "'s", "Ch\u00e4pp\u00b7li", "l\u00fcp\u00b7fe", "z'", "rech\u00b7ter", "Zit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "NE", "VVFIN", "NE", "ADJA", "NN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "sust het me Schimpf und chunnt nit wit.", "tokens": ["sust", "het", "me", "Schimpf", "und", "chunnt", "nit", "wit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VAFIN", "ADJA", "NN", "KON", "ADJD", "PTKNEG", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}