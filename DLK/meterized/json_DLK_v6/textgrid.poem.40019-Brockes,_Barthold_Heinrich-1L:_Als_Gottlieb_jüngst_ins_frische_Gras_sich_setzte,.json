{"textgrid.poem.40019": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als Gottlieb j\u00fcngst ins frische Gras sich setzte,", "genre": "verse", "period": "N.A.", "pub_year": 1713, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Gottlieb j\u00fcngst ins frische Gras sich setzte,", "tokens": ["Als", "Gott\u00b7lieb", "j\u00fcngst", "ins", "fri\u00b7sche", "Gras", "sich", "setz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "APPRART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An einem reinen Bach, und sahe, wie die Fluth,", "tokens": ["An", "ei\u00b7nem", "rei\u00b7nen", "Bach", ",", "und", "sa\u00b7he", ",", "wie", "die", "Fluth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KON", "VVFIN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bestrahlet durch der Sonnen Gluht,", "tokens": ["Be\u00b7strah\u00b7let", "durch", "der", "Son\u00b7nen", "Gluht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch Schilf und Bluhmen lieblich rann;", "tokens": ["Durch", "Schilf", "und", "Bluh\u00b7men", "lieb\u00b7lich", "rann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bezeugt' er, wie ihn die\u00df recht inniglich ergetzte,", "tokens": ["Be\u00b7zeugt'", "er", ",", "wie", "ihn", "die\u00df", "recht", "in\u00b7nig\u00b7lich", "er\u00b7getz\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPER", "PDS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und priese seine Lust Chrysandern an,", "tokens": ["Und", "prie\u00b7se", "sei\u00b7ne", "Lust", "Chry\u00b7san\u00b7dern", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der, wegen einiger Processen,", "tokens": ["Der", ",", "we\u00b7gen", "ei\u00b7ni\u00b7ger", "Pro\u00b7ces\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ihn zu besuchen kommen war.", "tokens": ["Ihn", "zu", "be\u00b7su\u00b7chen", "kom\u00b7men", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wer kann die Herrlichkeit, sprach er, genug ermessen,", "tokens": ["Wer", "kann", "die", "Herr\u00b7lich\u00b7keit", ",", "sprach", "er", ",", "ge\u00b7nug", "er\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "$,", "VVFIN", "PPER", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Die die Natur so wunderbar", "tokens": ["Die", "die", "Na\u00b7tur", "so", "wun\u00b7der\u00b7bar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "An allen Orten uns vor Augen leget?", "tokens": ["An", "al\u00b7len", "Or\u00b7ten", "uns", "vor", "Au\u00b7gen", "le\u00b7get", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Mein Auge siehet sich nicht satt, wenn es erweget", "tokens": ["Mein", "Au\u00b7ge", "sie\u00b7het", "sich", "nicht", "satt", ",", "wenn", "es", "er\u00b7we\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "PTKNEG", "ADJD", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "Den dick-belaubten Wald, den bunt-bebl\u00fchmten Klee,", "tokens": ["Den", "dick\u00b7be\u00b7laub\u00b7ten", "Wald", ",", "den", "bunt\u00b7be\u00b7bl\u00fchm\u00b7ten", "Klee", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die helle Reinigkeit der gl\u00e4ntzenden Krystallen,", "tokens": ["Die", "hel\u00b7le", "Rei\u00b7nig\u00b7keit", "der", "gl\u00e4nt\u00b7zen\u00b7den", "Krys\u00b7tal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Woran den gantzen Tag ich mich nicht m\u00fcde seh'.", "tokens": ["Wo\u00b7ran", "den", "gant\u00b7zen", "Tag", "ich", "mich", "nicht", "m\u00fc\u00b7de", "seh'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PPER", "PRF", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das w\u00fcrde mir unm\u00f6glich fallen,", "tokens": ["Das", "w\u00fcr\u00b7de", "mir", "un\u00b7m\u00f6g\u00b7lich", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Fiel ihm Chrysander ein. Was seh' ich mir daran?", "tokens": ["Fiel", "ihm", "Chry\u00b7san\u00b7der", "ein", ".", "Was", "seh'", "ich", "mir", "da\u00b7ran", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "PTKVZ", "$.", "PWS", "VVFIN", "PPER", "PRF", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Au' ist bunt, der Wald ist gr\u00fcn, der Bach ist klar.", "tokens": ["Die", "Au'", "ist", "bunt", ",", "der", "Wald", "ist", "gr\u00fcn", ",", "der", "Bach", "ist", "klar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Recht sch\u00f6n ist alles, das ist wahr:", "tokens": ["Recht", "sch\u00f6n", "ist", "al\u00b7les", ",", "das", "ist", "wahr", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VAFIN", "PIS", "$,", "PDS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weil ich die\u00df aber schnell beschauen kann;", "tokens": ["Weil", "ich", "die\u00df", "a\u00b7ber", "schnell", "be\u00b7schau\u00b7en", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Warum soll ich die Zeit, worin ich was verdienen", "tokens": ["Wa\u00b7rum", "soll", "ich", "die", "Zeit", ",", "wo\u00b7rin", "ich", "was", "ver\u00b7die\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "ART", "NN", "$,", "PWAV", "PPER", "PIS", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und Geld erwerben mag, hier, wie ein Frosch im Gr\u00fcnen,", "tokens": ["Und", "Geld", "er\u00b7wer\u00b7ben", "mag", ",", "hier", ",", "wie", "ein", "Frosch", "im", "Gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "VMFIN", "$,", "ADV", "$,", "PWAV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Im faulen M\u00fc\u00dfiggang, verderben?", "tokens": ["Im", "fau\u00b7len", "M\u00fc\u00b7\u00dfig\u00b7gang", ",", "ver\u00b7der\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Sollt' ich nichts anders thun, ich wollte lieber sterben,", "tokens": ["Sollt'", "ich", "nichts", "an\u00b7ders", "thun", ",", "ich", "woll\u00b7te", "lie\u00b7ber", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIS", "ADV", "VVINF", "$,", "PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Als hier so m\u00fc\u00dfig seyn.", "tokens": ["Als", "hier", "so", "m\u00fc\u00b7\u00dfig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dem Gottlieb kamen zwar die Thr\u00e4nen in die Augen;", "tokens": ["Dem", "Gott\u00b7lieb", "ka\u00b7men", "zwar", "die", "Thr\u00e4\u00b7nen", "in", "die", "Au\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Allein er sagte nichts. Ihm war bewust,", "tokens": ["Al\u00b7lein", "er", "sag\u00b7te", "nichts", ".", "Ihm", "war", "be\u00b7wust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PIS", "$.", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df nichts, als Geitz, Chrysanders Brust", "tokens": ["Da\u00df", "nichts", ",", "als", "Geitz", ",", "Chry\u00b7san\u00b7ders", "Brust"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PIS", "$,", "KOUS", "NN", "$,", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit gelber Sucht erf\u00fcllt, da\u00df folglich alle Lehren,", "tokens": ["Mit", "gel\u00b7ber", "Sucht", "er\u00b7f\u00fcllt", ",", "da\u00df", "folg\u00b7lich", "al\u00b7le", "Leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,", "KOUS", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ihn aus dem Labyrinth auf rechten Weg zu kehren,", "tokens": ["Ihn", "aus", "dem", "La\u00b7by\u00b7rinth", "auf", "rech\u00b7ten", "Weg", "zu", "keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nur gantz vergeblich sind, und nichts zu wircken taugen:", "tokens": ["Nur", "gantz", "ver\u00b7geb\u00b7lich", "sind", ",", "und", "nichts", "zu", "wir\u00b7cken", "tau\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VAFIN", "$,", "KON", "PIS", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Weswegen er von andern Dingen sprach,", "tokens": ["Wes\u00b7we\u00b7gen", "er", "von", "an\u00b7dern", "Din\u00b7gen", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ihm einig' H\u00f6flichkeit erwies',", "tokens": ["Ihm", "ei\u00b7nig'", "H\u00f6f\u00b7lich\u00b7keit", "er\u00b7wies'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und, ohn' ihn gar zu sehr", "tokens": ["Und", ",", "ohn'", "ihn", "gar", "zu", "sehr"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUI", "PPER", "ADV", "PTKA", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Zu n\u00f6thigen, ihn von sich lie\u00df.", "tokens": ["Zu", "n\u00f6\u00b7thi\u00b7gen", ",", "ihn", "von", "sich", "lie\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PPER", "APPR", "PRF", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Kaum war er fort, so dachte Gottlieb nach,", "tokens": ["Kaum", "war", "er", "fort", ",", "so", "dach\u00b7te", "Gott\u00b7lieb", "nach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was doch die Ursach sey, da\u00df aller Farben Schein,", "tokens": ["Was", "doch", "die", "Ur\u00b7sach", "sey", ",", "da\u00df", "al\u00b7ler", "Far\u00b7ben", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VAFIN", "$,", "KOUS", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df aller Bildung Pracht, der Menschen Hertz nicht r\u00fchret;", "tokens": ["Da\u00df", "al\u00b7ler", "Bil\u00b7dung", "Pracht", ",", "der", "Men\u00b7schen", "Hertz", "nicht", "r\u00fch\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "NN", "$,", "ART", "NN", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df keiner fast daran was recht beh\u00e4glichs sp\u00fcret;", "tokens": ["Da\u00df", "kei\u00b7ner", "fast", "da\u00b7ran", "was", "recht", "be\u00b7h\u00e4g\u00b7lichs", "sp\u00fc\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "PAV", "PWS", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df sie fast jedermann", "tokens": ["Da\u00df", "sie", "fast", "je\u00b7der\u00b7mann"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PIS"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Vor Augen zwar, doch nicht im Hertzen, liegen,", "tokens": ["Vor", "Au\u00b7gen", "zwar", ",", "doch", "nicht", "im", "Hert\u00b7zen", ",", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "ADV", "$,", "ADV", "PTKNEG", "APPRART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da jeder sich so schnell daran,", "tokens": ["Da", "je\u00b7der", "sich", "so", "schnell", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "ADV", "ADJD", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit einem blinden Blick, vergn\u00fcgen,", "tokens": ["Mit", "ei\u00b7nem", "blin\u00b7den", "Blick", ",", "ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und so geschwinde s\u00e4tt'gen kann;", "tokens": ["Und", "so", "ge\u00b7schwin\u00b7de", "s\u00e4tt'\u00b7gen", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Da\u00df keiner sie mit Lust betrachtet,", "tokens": ["Da\u00df", "kei\u00b7ner", "sie", "mit", "Lust", "be\u00b7trach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Da\u00df keiner sie des Anblicks w\u00fcrdig achtet,", "tokens": ["Da\u00df", "kei\u00b7ner", "sie", "des", "An\u00b7blicks", "w\u00fcr\u00b7dig", "ach\u00b7tet", ","], "token_info": ["word",