{"textgrid.poem.24703": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbwas rinnen dir die Tr\u00e4nen,", "genre": "verse", "period": "N.A.", "pub_year": 1893, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwas rinnen dir die Tr\u00e4nen,", "tokens": ["\u00bb", "was", "rin\u00b7nen", "dir", "die", "Tr\u00e4\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Tr\u00e4nen stumm und hei\u00df,", "tokens": ["Die", "Tr\u00e4\u00b7nen", "stumm", "und", "hei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch deine feinen Finger,", "tokens": ["Durch", "dei\u00b7ne", "fei\u00b7nen", "Fin\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Finger fein und wei\u00df?\u00ab", "tokens": ["Die", "Fin\u00b7ger", "fein", "und", "wei\u00df", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mein Schleier ist zerrissen", "tokens": ["Mein", "Schlei\u00b7er", "ist", "zer\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und wehet doch kein Wind", "tokens": ["Und", "we\u00b7het", "doch", "kein", "Wind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und bin doch nirgends gangen", "tokens": ["Und", "bin", "doch", "nir\u00b7gends", "gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Niemals, wo Dornen sind ...", "tokens": ["Nie\u00b7mals", ",", "wo", "Dor\u00b7nen", "sind", "..."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "NN", "VAFIN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Die Glocken haben heute", "tokens": ["Die", "Glo\u00b7cken", "ha\u00b7ben", "heu\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So sonderbaren Klang,", "tokens": ["So", "son\u00b7der\u00b7ba\u00b7ren", "Klang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gott wei\u00df, warum ich weine,", "tokens": ["Gott", "wei\u00df", ",", "wa\u00b7rum", "ich", "wei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir ist zum Sterben bang.", "tokens": ["Mir", "ist", "zum", "Ster\u00b7ben", "bang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}