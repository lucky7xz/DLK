{"textgrid.poem.63096": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Die Geburt", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Vorhang l\u00e4\u00dft nur mattes Licht herein,", "tokens": ["Der", "Vor\u00b7hang", "l\u00e4\u00dft", "nur", "mat\u00b7tes", "Licht", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie windet sich auf tr\u00e4nennassen Kissen,", "tokens": ["Sie", "win\u00b7det", "sich", "auf", "tr\u00e4\u00b7nen\u00b7nas\u00b7sen", "Kis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie hat die Z\u00e4hne in den Daumen fest gebissen,", "tokens": ["Sie", "hat", "die", "Z\u00e4h\u00b7ne", "in", "den", "Dau\u00b7men", "fest", "ge\u00b7bis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df blau er schwillt. Sie h\u00e4lt es nicht... mu\u00df schrein,", "tokens": ["Da\u00df", "blau", "er", "schwillt", ".", "Sie", "h\u00e4lt", "es", "nicht", "...", "mu\u00df", "schrein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$(", "VMFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es rast heraus, es bricht sich an den W\u00e4nden", "tokens": ["Es", "rast", "he\u00b7raus", ",", "es", "bricht", "sich", "an", "den", "W\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der grause Ton und klopft mit f\u00fcrchterlichen H\u00e4nden..", "tokens": ["Der", "grau\u00b7se", "Ton", "und", "klopft", "mit", "f\u00fcrch\u00b7ter\u00b7li\u00b7chen", "H\u00e4n\u00b7den", ".."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da schl\u00e4gt hoch \u00fcber aller Wipfel Glut die Flamme...", "tokens": ["Da", "schl\u00e4gt", "hoch", "\u00fc\u00b7ber", "al\u00b7ler", "Wip\u00b7fel", "Glut", "die", "Flam\u00b7me", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "APPR", "PIAT", "NN", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein rosig, klumpig Etwas tr\u00e4gt die Amme.", "tokens": ["Ein", "ro\u00b7sig", ",", "klum\u00b7pig", "Et\u00b7was", "tr\u00e4gt", "die", "Am\u00b7me", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "ADJD", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der Sanit\u00e4tsrat hat den \u00c4rmel aufgekrempelt,", "tokens": ["Der", "Sa\u00b7ni\u00b7t\u00e4ts\u00b7rat", "hat", "den", "\u00c4r\u00b7mel", "auf\u00b7ge\u00b7krem\u00b7pelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Indem er diesen roten Fleck zu einem Knaben stempelt.", "tokens": ["In\u00b7dem", "er", "die\u00b7sen", "ro\u00b7ten", "Fleck", "zu", "ei\u00b7nem", "Kna\u00b7ben", "stem\u00b7pelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.11": {"text": "Dem Vater perlt der kalte Schwei\u00df.", "tokens": ["Dem", "Va\u00b7ter", "perlt", "der", "kal\u00b7te", "Schwei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Die Mutter aber l\u00e4chelt, und sie wei\u00df,", "tokens": ["Die", "Mut\u00b7ter", "a\u00b7ber", "l\u00e4\u00b7chelt", ",", "und", "sie", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Es singt mit Harfen und mit Fl\u00f6ten ihren Ohren:", "tokens": ["Es", "singt", "mit", "Har\u00b7fen", "und", "mit", "Fl\u00f6\u00b7ten", "ih\u00b7ren", "Oh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ich habe einen ", "tokens": ["Ich", "ha\u00b7be", "ei\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "ART"], "meter": "-+-+-", "measure": "iambic.di"}}}}}