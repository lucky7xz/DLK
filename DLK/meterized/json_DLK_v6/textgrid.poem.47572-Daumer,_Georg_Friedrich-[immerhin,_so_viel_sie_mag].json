{"textgrid.poem.47572": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[immerhin, so viel sie mag]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Immerhin, so viel sie mag", "tokens": ["Im\u00b7mer\u00b7hin", ",", "so", "viel", "sie", "mag"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "ADV", "PPER", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wider dich zum Streit", "tokens": ["Wi\u00b7der", "dich", "zum", "Streit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPRART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "R\u00fcste sich zelotische", "tokens": ["R\u00fcs\u00b7te", "sich", "ze\u00b7lo\u00b7ti\u00b7sche"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PRF", "ADJA"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "P\u00f6belhaftigkeit!", "tokens": ["P\u00f6\u00b7bel\u00b7haf\u00b7tig\u00b7keit", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Hege keine Furcht, Hafis!", "tokens": ["He\u00b7ge", "kei\u00b7ne", "Furcht", ",", "Ha\u00b7fis", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,", "NE", "$."], "meter": "+-+-++-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Gn\u00e4dig ohne Ma\u00df", "tokens": ["Gn\u00e4\u00b7dig", "oh\u00b7ne", "Ma\u00df"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPR", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "H\u00e4lt dir Allah seinen Schirm,", "tokens": ["H\u00e4lt", "dir", "Al\u00b7lah", "sei\u00b7nen", "Schirm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Seinen Lohn bereit.", "tokens": ["Sei\u00b7nen", "Lohn", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Selsebile spendet er,", "tokens": ["Sel\u00b7se\u00b7bi\u00b7le", "spen\u00b7det", "er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Welche deinem Mund", "tokens": ["Wel\u00b7che", "dei\u00b7nem", "Mund"], "token_info": ["word", "word", "word"], "pos": ["PWAT", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Seine hohe Sonne kocht", "tokens": ["Sei\u00b7ne", "ho\u00b7he", "Son\u00b7ne", "kocht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In der hei\u00dfen Zeit.", "tokens": ["In", "der", "hei\u00b7\u00dfen", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Manchen ungefl\u00fcgelten", "tokens": ["Man\u00b7chen", "un\u00b7ge\u00b7fl\u00fc\u00b7gel\u00b7ten"], "token_info": ["word", "word"], "pos": ["PIAT", "ADJA"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Engel sendet er,", "tokens": ["En\u00b7gel", "sen\u00b7det", "er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Dir zu scheuchen wunders\u00fc\u00df", "tokens": ["Dir", "zu", "scheu\u00b7chen", "wun\u00b7der\u00b7s\u00fc\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jede Bitterkeit.", "tokens": ["Je\u00b7de", "Bit\u00b7ter\u00b7keit", "."], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Nicht genug; ein Ueberschu\u00df", "tokens": ["Nicht", "ge\u00b7nug", ";", "ein", "Ue\u00b7ber\u00b7schu\u00df"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "ADV", "$.", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Seiner Gnaden ist,", "tokens": ["Sei\u00b7ner", "Gna\u00b7den", "ist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df er auch des Dichterrangs", "tokens": ["Da\u00df", "er", "auch", "des", "Dich\u00b7ter\u00b7rangs"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00f6ne Krone leiht.", "tokens": ["Sch\u00f6\u00b7ne", "Kro\u00b7ne", "leiht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Sina lernt und Griechenland", "tokens": ["Si\u00b7na", "lernt", "und", "Grie\u00b7chen\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "KON", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine Melodei,", "tokens": ["Dei\u00b7ne", "Me\u00b7lo\u00b7dei", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Neidet deinem Liede schon", "tokens": ["Nei\u00b7det", "dei\u00b7nem", "Lie\u00b7de", "schon"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die Unsterblichkeit.", "tokens": ["Die", "U\u00b7nsterb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Eine Wallfahrtst\u00e4tte wird", "tokens": ["Ei\u00b7ne", "Wall\u00b7fahrt\u00b7st\u00e4t\u00b7te", "wird"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Deiner Gruft dereinst,", "tokens": ["Dei\u00b7ner", "Gruft", "de\u00b7reinst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Lockerem Gesindel stets", "tokens": ["Lo\u00b7cke\u00b7rem", "Ge\u00b7sin\u00b7del", "stets"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Heilig und geweiht.", "tokens": ["Hei\u00b7lig", "und", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "KON", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Ja du steckst, so wie du todt,", "tokens": ["Ja", "du", "steckst", ",", "so", "wie", "du", "todt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "$,", "ADV", "KOKOM", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dein verkl\u00e4rtes Haupt", "tokens": ["Dein", "ver\u00b7kl\u00e4r\u00b7tes", "Haupt"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Mitten aus der Sonne Pracht", "tokens": ["Mit\u00b7ten", "aus", "der", "Son\u00b7ne", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In die Zeitlichkeit.", "tokens": ["In", "die", "Zeit\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}