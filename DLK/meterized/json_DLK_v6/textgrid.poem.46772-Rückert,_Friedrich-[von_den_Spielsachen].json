{"textgrid.poem.46772": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[von den Spielsachen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von den Spielsachen", "tokens": ["Von", "den", "Spiel\u00b7sa\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.2": {"text": "Zu Weihnachten,", "tokens": ["Zu", "Weih\u00b7nach\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.3": {"text": "Von den vielfachen,", "tokens": ["Von", "den", "viel\u00b7fa\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,"], "meter": "++-+-", "measure": "iambic.di"}, "line.4": {"text": "Die sie brachten,", "tokens": ["Die", "sie", "brach\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Will ich ausw\u00e4hlen", "tokens": ["Will", "ich", "aus\u00b7w\u00e4h\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Sch\u00f6nste, beste,", "tokens": ["Sch\u00f6ns\u00b7te", ",", "bes\u00b7te", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Nebenaus z\u00e4hlen", "tokens": ["Ne\u00b7be\u00b7naus", "z\u00e4h\u00b7len"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Von dem Reste,", "tokens": ["Von", "dem", "Res\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "--+-", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Einen Straus stehlen", "tokens": ["Ei\u00b7nen", "Straus", "steh\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Von dem Feste,", "tokens": ["Von", "dem", "Fes\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.3": {"text": "Ihn im Haus hehlen", "tokens": ["Ihn", "im", "Haus", "heh\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPRART", "NN", "VVINF"], "meter": "+-++-", "measure": "unknown.measure.tri"}, "line.4": {"text": "F\u00fcr zwei G\u00e4ste.", "tokens": ["F\u00fcr", "zwei", "G\u00e4s\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Nicht die Spielknaben", "tokens": ["Nicht", "die", "Spiel\u00b7kna\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["PTKNEG", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Werden's missen,", "tokens": ["Wer\u00b7den's", "mis\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Die zuviel haben,", "tokens": ["Die", "zu\u00b7viel", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Um's zu wissen.", "tokens": ["Um's", "zu", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PTKZU", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Doch ein Spielpl\u00e4tzchen", "tokens": ["Doch", "ein", "Spiel\u00b7pl\u00e4tz\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Will ich gr\u00fcnden,", "tokens": ["Will", "ich", "gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Dort die Vielsch\u00e4tzchen", "tokens": ["Dort", "die", "Viel\u00b7sch\u00e4tz\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "++-+-", "measure": "iambic.di"}, "line.4": {"text": "Zierlich r\u00fcnden.", "tokens": ["Zier\u00b7lich", "r\u00fcn\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Den zwei Spielr\u00e4tzchen", "tokens": ["Den", "zwei", "Spiel\u00b7r\u00e4tz\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ART", "CARD", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Will ich's machen,", "tokens": ["Will", "ich's", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Wenn die Spielk\u00e4tzchen", "tokens": ["Wenn", "die", "Spiel\u00b7k\u00e4tz\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mir erwachen.", "tokens": ["Mir", "er\u00b7wa\u00b7chen", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "Wenn die Spielm\u00e4tzchen", "tokens": ["Wenn", "die", "Spiel\u00b7m\u00e4tz\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "NN"], "meter": "+-++-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Zu mir kommen,", "tokens": ["Zu", "mir", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Soll das Spielpl\u00e4tzchen", "tokens": ["Soll", "das", "Spiel\u00b7pl\u00e4tz\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "ART", "NN"], "meter": "+-++-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Ihnen frommen.", "tokens": ["Ih\u00b7nen", "from\u00b7men", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.8": {"line.1": {"text": "Ja das Spielsch\u00e4tzchen", "tokens": ["Ja", "das", "Spiel\u00b7sch\u00e4tz\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["PTKANT", "ART", "NN"], "meter": "+-++-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Soll sie locken,", "tokens": ["Soll", "sie", "lo\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Da\u00df die Spielfr\u00e4tzchen", "tokens": ["Da\u00df", "die", "Spiel\u00b7fr\u00e4tz\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "NN"], "meter": "+-++-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Mir nicht stocken;", "tokens": ["Mir", "nicht", "sto\u00b7cken", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.9": {"line.1": {"text": "Da\u00df im Sternreigen,", "tokens": ["Da\u00df", "im", "Stern\u00b7rei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "$,"], "meter": "+-++-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Dort mit Schimmer", "tokens": ["Dort", "mit", "Schim\u00b7mer"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Sie mir gern steigen", "tokens": ["Sie", "mir", "gern", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "PPER", "ADV", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Her ins Zimmer,", "tokens": ["Her", "ins", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.10": {"line.1": {"text": "Und ein Nachtst\u00fcndechen", "tokens": ["Und", "ein", "Nacht\u00b7st\u00fcn\u00b7de\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Hier vertreiben,", "tokens": ["Hier", "ver\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Bellst du, Wachth\u00fcndchen?", "tokens": ["Bellst", "du", ",", "Wacht\u00b7h\u00fcnd\u00b7chen", "?"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "La\u00df es bleiben!", "tokens": ["La\u00df", "es", "blei\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.11": {"line.1": {"text": "Sollst nicht voreilig", "tokens": ["Sollst", "nicht", "vor\u00b7ei\u00b7lig"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PTKNEG", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Sie verjagen,", "tokens": ["Sie", "ver\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Bis empor heilig", "tokens": ["Bis", "em\u00b7por", "hei\u00b7lig"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "ADJD"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Sie sich tragen,", "tokens": ["Sie", "sich", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PRF", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.12": {"line.1": {"text": "Wann die Fr\u00fchsonne", "tokens": ["Wann", "die", "Fr\u00fch\u00b7son\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "+-++-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Her wird dringen", "tokens": ["Her", "wird", "drin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "VAFIN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Und mit Spr\u00fchwonne", "tokens": ["Und", "mit", "Spr\u00fch\u00b7won\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Sie beschwingen.", "tokens": ["Sie", "be\u00b7schwin\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}