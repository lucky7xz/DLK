{"textgrid.poem.35459": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Das war ein lust'ges Feiern", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das war ein lust'ges Feiern \u2013", "tokens": ["Das", "war", "ein", "lust'\u00b7ges", "Fei\u00b7ern", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Schw\u00e4rmen bei Nacht und bei Tag!", "tokens": ["Ein", "Schw\u00e4r\u00b7men", "bei", "Nacht", "und", "bei", "Tag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Nun liegt's auf mir so felsenhart \u2013", "tokens": ["Nun", "liegt's", "auf", "mir", "so", "fel\u00b7sen\u00b7hart", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jach sind mir Freud und Lust erstarrt:", "tokens": ["Jach", "sind", "mir", "Freud", "und", "Lust", "er\u00b7starrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "NN", "KON", "NN", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Nun liegt's auf mir so bleiern", "tokens": ["Nun", "liegt's", "auf", "mir", "so", "blei\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nach all dem lust'gen Feiern \u2013", "tokens": ["Nach", "all", "dem", "lust'\u00b7gen", "Fei\u00b7ern", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dem Schw\u00e4rmen bei Nacht und bei Tag! ...", "tokens": ["Dem", "Schw\u00e4r\u00b7men", "bei", "Nacht", "und", "bei", "Tag", "!", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "APPR", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Das war ein tolles Zechen \u2013", "tokens": ["Das", "war", "ein", "tol\u00b7les", "Ze\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir wurden's schier nicht satt! ...", "tokens": ["Wir", "wur\u00b7den's", "schier", "nicht", "satt", "!", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Jach starb mir da der bl\u00fch'nde Scherz \u2013", "tokens": ["Jach", "starb", "mir", "da", "der", "bl\u00fch'n\u00b7de", "Scherz", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nun liegt's auf mir wie schweres Erz:", "tokens": ["Nun", "liegt's", "auf", "mir", "wie", "schwe\u00b7res", "Erz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als wollte das Hirn mir zerbrechen", "tokens": ["Als", "woll\u00b7te", "das", "Hirn", "mir", "zer\u00b7bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "ART", "NN", "PPER", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Nach all dem lust'gen Zechen \u2013", "tokens": ["Nach", "all", "dem", "lust'\u00b7gen", "Ze\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dem Schw\u00e4rmen bei Nacht und bei Tag! ...", "tokens": ["Dem", "Schw\u00e4r\u00b7men", "bei", "Nacht", "und", "bei", "Tag", "!", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "APPR", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Das war ein keck Erfassen", "tokens": ["Das", "war", "ein", "keck", "Er\u00b7fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Lebens in jauchzender Lust! ...", "tokens": ["Des", "Le\u00b7bens", "in", "jauch\u00b7zen\u00b7der", "Lust", "!", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Nun liegt's mir vor Augen so tot und so fahl \u2013", "tokens": ["Nun", "liegt's", "mir", "vor", "Au\u00b7gen", "so", "tot", "und", "so", "fahl", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "ADV", "ADJD", "KON", "ADV", "ADJD", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Nun liegt mir die Welt so namenlos schal: \u2013", "tokens": ["Nun", "liegt", "mir", "die", "Welt", "so", "na\u00b7men\u00b7los", "schal", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "ADJD", "ADJD", "$.", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Als sollte ich sie nun ", "tokens": ["Als", "soll\u00b7te", "ich", "sie", "nun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "PPER", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "So ward mir nach all dem Erfassen", "tokens": ["So", "ward", "mir", "nach", "all", "dem", "Er\u00b7fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PIAT", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Des Lebens in jauchzender Lust! ...", "tokens": ["Des", "Le\u00b7bens", "in", "jauch\u00b7zen\u00b7der", "Lust", "!", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}