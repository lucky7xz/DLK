{"textgrid.poem.58367": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "405. Was ist dir dran gelegen?", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was drau\u00dfen ist und mag geschehen,", "tokens": ["Was", "drau\u00b7\u00dfen", "ist", "und", "mag", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "KON", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du wie ein Fremdling mu\u00dft ansehen;", "tokens": ["Du", "wie", "ein", "Fremd\u00b7ling", "mu\u00dft", "an\u00b7se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOKOM", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bleib unber\u00fchrt und ungest\u00f6rt", "tokens": ["Bleib", "un\u00b7be\u00b7r\u00fchrt", "und", "un\u00b7ge\u00b7st\u00f6rt"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bei deinem Freund, in 'n Geist gekehrt!", "tokens": ["Bei", "dei\u00b7nem", "Freund", ",", "in", "'n", "Geist", "ge\u00b7kehrt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}