{"textgrid.poem.67081": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Hymne an die G\u00f6ttin der Harmonie", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Froh, als k\u00f6nnt ich Sch\u00f6pfungen begl\u00fccken,", "tokens": ["Froh", ",", "als", "k\u00f6nnt", "ich", "Sch\u00f6p\u00b7fun\u00b7gen", "be\u00b7gl\u00fc\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "VVFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "K\u00fchn, als huldigten die Geister mir,", "tokens": ["K\u00fchn", ",", "als", "hul\u00b7dig\u00b7ten", "die", "Geis\u00b7ter", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "NN", "ART", "NN", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nahet, in dein Heiligtum zu blicken,", "tokens": ["Na\u00b7het", ",", "in", "dein", "Hei\u00b7lig\u00b7tum", "zu", "bli\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Hocherhabne! meine Liebe dir;", "tokens": ["Ho\u00b7cher\u00b7hab\u00b7ne", "!", "mei\u00b7ne", "Lie\u00b7be", "dir", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Schon ergl\u00fcht der wonnetrunkne Seher", "tokens": ["Schon", "er\u00b7gl\u00fcht", "der", "won\u00b7ne\u00b7trunk\u00b7ne", "Se\u00b7her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Von den Ahndungen der Herrlichkeit,", "tokens": ["Von", "den", "Ahn\u00b7dun\u00b7gen", "der", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Ha, und deinem G\u00f6tterscho\u00dfe n\u00e4her", "tokens": ["Ha", ",", "und", "dei\u00b7nem", "G\u00f6t\u00b7ter\u00b7scho\u00b7\u00dfe", "n\u00e4\u00b7her"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KON", "PPOSAT", "NN", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "H\u00f6hnt des Siegers Fahne Grab und Zeit.", "tokens": ["H\u00f6hnt", "des", "Sie\u00b7gers", "Fah\u00b7ne", "Grab", "und", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Tausendf\u00e4ltig, wie der G\u00f6tter Wille,", "tokens": ["Tau\u00b7send\u00b7f\u00e4l\u00b7tig", ",", "wie", "der", "G\u00f6t\u00b7ter", "Wil\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Weht Begeisterung den S\u00e4nger an,", "tokens": ["Weht", "Be\u00b7geis\u00b7te\u00b7rung", "den", "S\u00e4n\u00b7ger", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Unersch\u00f6pflich ist der Sch\u00f6nheit F\u00fclle,", "tokens": ["Un\u00b7er\u00b7sch\u00f6pf\u00b7lich", "ist", "der", "Sch\u00f6n\u00b7heit", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Grenzenlos der Hoheit Ozean.", "tokens": ["Gren\u00b7zen\u00b7los", "der", "Ho\u00b7heit", "O\u00b7ze\u00b7an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Doch vor allem hab ich dich erkoren,", "tokens": ["Doch", "vor", "al\u00b7lem", "hab", "ich", "dich", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "VAFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Bebend, als ich ferne dich ersah,", "tokens": ["Be\u00b7bend", ",", "als", "ich", "fer\u00b7ne", "dich", "er\u00b7sah", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Bebend hab ich Liebe dir geschworen,", "tokens": ["Be\u00b7bend", "hab", "ich", "Lie\u00b7be", "dir", "ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "K\u00f6nigin der Welt! Urania.", "tokens": ["K\u00f6\u00b7ni\u00b7gin", "der", "Welt", "!", "U\u00b7ra\u00b7nia", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "NE", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Was der Geister stolzestes Verlangen", "tokens": ["Was", "der", "Geis\u00b7ter", "stol\u00b7zes\u00b7tes", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In den Tiefen und den H\u00f6hn erzielt,", "tokens": ["In", "den", "Tie\u00b7fen", "und", "den", "H\u00f6hn", "er\u00b7zielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Hab ich allzumal in dir empfangen,", "tokens": ["Hab", "ich", "all\u00b7zu\u00b7mal", "in", "dir", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sint dich ahndend meine Seele f\u00fchlt.", "tokens": ["Sint", "dich", "ahn\u00b7dend", "mei\u00b7ne", "See\u00b7le", "f\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Dir entsprossen Myriaden Leben,", "tokens": ["Dir", "ent\u00b7spros\u00b7sen", "My\u00b7ria\u00b7den", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Als die Strahlen deines Angesichts,", "tokens": ["Als", "die", "Strah\u00b7len", "dei\u00b7nes", "An\u00b7ge\u00b7sichts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Wendest du dein Angesicht, so beben", "tokens": ["Wen\u00b7dest", "du", "dein", "An\u00b7ge\u00b7sicht", ",", "so", "be\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Und vergehn sie, und die Welt ist Nichts.", "tokens": ["Und", "ver\u00b7gehn", "sie", ",", "und", "die", "Welt", "ist", "Nichts", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KON", "ART", "NN", "VAFIN", "PIS", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Thronend auf des alten Chaos Wogen,", "tokens": ["Thro\u00b7nend", "auf", "des", "al\u00b7ten", "Chaos", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Majest\u00e4tisch l\u00e4chelnd winktest du,", "tokens": ["Ma\u00b7jes\u00b7t\u00e4\u00b7tisch", "l\u00e4\u00b7chelnd", "wink\u00b7test", "du", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und die wilden Elemente flogen", "tokens": ["Und", "die", "wil\u00b7den", "E\u00b7le\u00b7men\u00b7te", "flo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Liebend sich auf deine Winke zu.", "tokens": ["Lie\u00b7bend", "sich", "auf", "dei\u00b7ne", "Win\u00b7ke", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Froh der seligen Verm\u00e4hlungsstunde", "tokens": ["Froh", "der", "se\u00b7li\u00b7gen", "Ver\u00b7m\u00e4h\u00b7lungs\u00b7stun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Schlangen Wesen nun um Wesen sich,", "tokens": ["Schlan\u00b7gen", "We\u00b7sen", "nun", "um", "We\u00b7sen", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "APPR", "NN", "PRF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "In den Himmeln, auf dem Erdenrunde", "tokens": ["In", "den", "Him\u00b7meln", ",", "auf", "dem", "Er\u00b7den\u00b7run\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Sahst du, Meisterin! im Bilde dich. \u2013", "tokens": ["Sahst", "du", ",", "Meis\u00b7te\u00b7rin", "!", "im", "Bil\u00b7de", "dich", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$.", "APPRART", "NN", "PPER", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Ausgegossen ist des Lebens Schale,", "tokens": ["Aus\u00b7ge\u00b7gos\u00b7sen", "ist", "des", "Le\u00b7bens", "Scha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "B\u00e4chlein, Sonnen treten in die Bahn,", "tokens": ["B\u00e4ch\u00b7lein", ",", "Son\u00b7nen", "tre\u00b7ten", "in", "die", "Bahn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Liebetrunken schmiegen junge Tale", "tokens": ["Lie\u00b7be\u00b7trun\u00b7ken", "schmie\u00b7gen", "jun\u00b7ge", "Ta\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sich den liebetrunknen H\u00fcgeln an:", "tokens": ["Sich", "den", "lie\u00b7be\u00b7trunk\u00b7nen", "H\u00fc\u00b7geln", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Sch\u00f6n und stolz wie G\u00f6tters\u00f6hne hangen", "tokens": ["Sch\u00f6n", "und", "stolz", "wie", "G\u00f6t\u00b7ter\u00b7s\u00f6h\u00b7ne", "han\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "KOKOM", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Felsen an der m\u00fctterlichen Brust,", "tokens": ["Fel\u00b7sen", "an", "der", "m\u00fct\u00b7ter\u00b7li\u00b7chen", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Von der Meere wildem Arm umfangen,", "tokens": ["Von", "der", "Mee\u00b7re", "wil\u00b7dem", "Arm", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Bebt das Land in niegef\u00fchlter Lust.", "tokens": ["Bebt", "das", "Land", "in", "nie\u00b7ge\u00b7f\u00fchl\u00b7ter", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Warm und leise wehen nun die L\u00fcfte,", "tokens": ["Warm", "und", "lei\u00b7se", "we\u00b7hen", "nun", "die", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Liebend sinkt der holde Lenz ins Tal:", "tokens": ["Lie\u00b7bend", "sinkt", "der", "hol\u00b7de", "Lenz", "ins", "Tal", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Haine sprossen an dem Felsgekl\u00fcfte,", "tokens": ["Hai\u00b7ne", "spros\u00b7sen", "an", "dem", "Fels\u00b7ge\u00b7kl\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Gras und Blumen zeugt der junge Strahl.", "tokens": ["Gras", "und", "Blu\u00b7men", "zeugt", "der", "jun\u00b7ge", "Strahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Siehe, siehe, vom emp\u00f6rten Meere,", "tokens": ["Sie\u00b7he", ",", "sie\u00b7he", ",", "vom", "em\u00b7p\u00f6r\u00b7ten", "Mee\u00b7re", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "VVIMP", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Von den H\u00fcgeln, von der Tale Scho\u00df,", "tokens": ["Von", "den", "H\u00fc\u00b7geln", ",", "von", "der", "Ta\u00b7le", "Scho\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Winden sich die ungez\u00e4hlten Heere", "tokens": ["Win\u00b7den", "sich", "die", "un\u00b7ge\u00b7z\u00e4hl\u00b7ten", "Hee\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Freudetaumelnder Gesch\u00f6pfe los.", "tokens": ["Freu\u00b7de\u00b7tau\u00b7meln\u00b7der", "Ge\u00b7sch\u00f6p\u00b7fe", "los", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Aus den Hainen wallt ins Lenzgefilde", "tokens": ["Aus", "den", "Hai\u00b7nen", "wallt", "ins", "Lenz\u00b7ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Himmlischsch\u00f6n der G\u00f6ttin Sohn hervor,", "tokens": ["Himm\u00b7lischsch\u00f6n", "der", "G\u00f6t\u00b7tin", "Sohn", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den zum k\u00f6niglichen Ebenbilde", "tokens": ["Den", "zum", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "E\u00b7ben\u00b7bil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sie im Anbeginne sich erkor:", "tokens": ["Sie", "im", "An\u00b7be\u00b7gin\u00b7ne", "sich", "er\u00b7kor", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Sanftbegr\u00fc\u00dft von Paradiesesd\u00fcften", "tokens": ["Sanft\u00b7be\u00b7gr\u00fc\u00dft", "vo