{"dta.poem.22809": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "115. Die Nachreu kombt zu sp\u00e4t.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Da GOtt auf Erden gieng/ ward Er fast nicht geacht:", "tokens": ["Da", "Gott", "auf", "Er\u00b7den", "gieng", "/", "ward", "Er", "fast", "nicht", "ge\u00b7acht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "VVFIN", "$(", "VAFIN", "PPER", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nu Er im Himmel ist beklagt Jhn jedermann", "tokens": ["Nu", "Er", "im", "Him\u00b7mel", "ist", "be\u00b7klagt", "Jhn", "je\u00b7der\u00b7mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "APPRART", "NN", "VAFIN", "VVFIN", "PPER", "PIS"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df Jhm nicht gr\u00f6sser Ehr ist worden angethan.", "tokens": ["Da\u00df", "Jhm", "nicht", "gr\u00f6s\u00b7ser", "Ehr", "ist", "wor\u00b7den", "an\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJA", "NN", "VAFIN", "VAPP", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So th\u00f6richk ist die Welt/ da\u00df sie\u2019s nicht vor bedacht!", "tokens": ["So", "th\u00f6\u00b7richk", "ist", "die", "Welt", "/", "da\u00df", "sie's", "nicht", "vor", "be\u00b7dacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"],