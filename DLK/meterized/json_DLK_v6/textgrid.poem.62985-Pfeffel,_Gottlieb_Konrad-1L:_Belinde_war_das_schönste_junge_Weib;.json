{"textgrid.poem.62985": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Belinde war das sch\u00f6nste junge Weib;", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Belinde war das sch\u00f6nste junge Weib;", "tokens": ["Be\u00b7lin\u00b7de", "war", "das", "sch\u00f6ns\u00b7te", "jun\u00b7ge", "Weib", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch kein vollkommner Glied fand sich an ihrem Leib,", "tokens": ["Doch", "kein", "voll\u00b7komm\u00b7ner", "Glied", "fand", "sich", "an", "ih\u00b7rem", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Als ihre Zunge. Welche Plage", "tokens": ["Als", "ih\u00b7re", "Zun\u00b7ge", ".", "Wel\u00b7che", "Pla\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "$.", "PWAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr ihren guten Mann! Doch war ihr Cleon lieb,", "tokens": ["F\u00fcr", "ih\u00b7ren", "gu\u00b7ten", "Mann", "!", "Doch", "war", "ihr", "Cle\u00b7on", "lieb", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "KON", "VAFIN", "PPER", "NE", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So gern er auch im Trinkgelage", "tokens": ["So", "gern", "er", "auch", "im", "Trink\u00b7ge\u00b7la\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Grillen finstern Schwarm vertrieb.", "tokens": ["Der", "Gril\u00b7len", "fins\u00b7tern", "Schwarm", "ver\u00b7trieb", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Um unverhofft ihn zu erfreuen,", "tokens": ["Um", "un\u00b7ver\u00b7hofft", "ihn", "zu", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Lie\u00df sie sich insgeheim von einer Meisterhand", "tokens": ["Lie\u00df", "sie", "sich", "ins\u00b7ge\u00b7heim", "von", "ei\u00b7ner", "Meis\u00b7ter\u00b7hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "In Lebensgr\u00f6\u00dfe conterfeyen", "tokens": ["In", "Le\u00b7bens\u00b7gr\u00f6\u00b7\u00dfe", "con\u00b7ter\u00b7fe\u00b7yen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und hieng das Bildni\u00df an die Wand.", "tokens": ["Und", "hieng", "das", "Bild\u00b7ni\u00df", "an", "die", "Wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Nun kam der Mann nach Haus, und zwar vom Zechen;", "tokens": ["Nun", "kam", "der", "Mann", "nach", "Haus", ",", "und", "zwar", "vom", "Ze\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$,", "KON", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Er sah das Bild \u2013 \u2013 Sie ists vom Hute bis zum Schuh,", "tokens": ["Er", "sah", "das", "Bild", "\u2013", "\u2013", "Sie", "ists", "vom", "Hu\u00b7te", "bis", "zum", "Schuh", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "$(", "PPER", "VAFIN", "APPRART", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Rief er, und hielt, aus Furcht sie m\u00f6chte sprechen,", "tokens": ["Rief", "er", ",", "und", "hielt", ",", "aus", "Furcht", "sie", "m\u00f6ch\u00b7te", "spre\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "VVFIN", "$,", "APPR", "NN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Sich schnell die beyden Ohren zu.", "tokens": ["Sich", "schnell", "die", "bey\u00b7den", "Oh\u00b7ren", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ART", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}