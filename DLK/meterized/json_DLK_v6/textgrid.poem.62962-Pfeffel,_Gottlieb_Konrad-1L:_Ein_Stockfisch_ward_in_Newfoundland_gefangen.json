{"textgrid.poem.62962": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Stockfisch ward in Newfoundland gefangen", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Stockfisch ward in Newfoundland gefangen", "tokens": ["Ein", "Stock\u00b7fisch", "ward", "in", "New\u00b7fo\u00b7und\u00b7land", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "NE", "VVPP"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und sprach mit \u00e4ngstlichem Verlangen", "tokens": ["Und", "sprach", "mit", "\u00e4ngst\u00b7li\u00b7chem", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zum rohen Schiffer: H\u00f6re, Mann!", "tokens": ["Zum", "ro\u00b7hen", "Schif\u00b7fer", ":", "H\u00f6\u00b7re", ",", "Mann", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "NE", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was hast du mit mir vor? \u2013 Ey nun, fieng dieser an,", "tokens": ["Was", "hast", "du", "mit", "mir", "vor", "?", "\u2013", "Ey", "nun", ",", "fi\u00b7eng", "die\u00b7ser", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "PPER", "PTKVZ", "$.", "$(", "NN", "ADV", "$,", "VVFIN", "PDS", "PTKVZ", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Das kann ich dir ja leicht vertrauen;", "tokens": ["Das", "kann", "ich", "dir", "ja", "leicht", "ver\u00b7trau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zuerst wird dir der Kopf vom Rumpf gehauen,", "tokens": ["Zu\u00b7erst", "wird", "dir", "der", "Kopf", "vom", "Rumpf", "ge\u00b7hau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Dann wirst du in die Welt gesandt", "tokens": ["Dann", "wirst", "du", "in", "die", "Welt", "ge\u00b7sandt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und \u2013 Himmel! \u00e4chzt der Arrestant,", "tokens": ["Und", "\u2013", "Him\u00b7mel", "!", "\u00e4chzt", "der", "Ar\u00b7res\u00b7tant", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$(", "NN", "$.", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Als tr\u00e4f ihn schon des Britten Eisen,", "tokens": ["Als", "tr\u00e4f", "ihn", "schon", "des", "Brit\u00b7ten", "Ei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Im tiefsten Elegienton:", "tokens": ["Im", "tiefs\u00b7ten", "E\u00b7le\u00b7gien\u00b7ton", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Was! ohne Kopf? \u2013 Nun ja, versetzt der Schiffspatron,", "tokens": ["Was", "!", "oh\u00b7ne", "Kopf", "?", "\u2013", "Nun", "ja", ",", "ver\u00b7setzt", "der", "Schiffs\u00b7pat\u00b7ron", ","], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "$.", "APPR", "NN", "$.", "$(", "ADV", "ADV", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Es ist die neuste Art zu reisen.", "tokens": ["Es", "ist", "die", "neus\u00b7te", "Art", "zu", "rei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}