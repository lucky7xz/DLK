{"textgrid.poem.50581": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "46. Jugend hindert nicht zur Befoderung", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Erk\u00fchnte sich einmahl zu bitten", "tokens": ["Er\u00b7k\u00fchn\u00b7te", "sich", "ein\u00b7mahl", "zu", "bit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADV", "PTKZU", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Vom Bischoff' eine ", "tokens": ["Vom", "Bi\u00b7schoff'", "ei\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ich kenn' eur ", "tokens": ["Ich", "kenn'", "eur"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Und nichtes hindert mich, euch was ihr w\u00fcnscht, zu geben,", "tokens": ["Und", "nich\u00b7tes", "hin\u00b7dert", "mich", ",", "euch", "was", "ihr", "w\u00fcnscht", ",", "zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "$,", "PPER", "PWS", "PPER", "VVFIN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sagt' er, ", "tokens": ["Sagt'", "er", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Der J\u00fcngling sprach: ", "tokens": ["Der", "J\u00fcng\u00b7ling", "sprach", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}