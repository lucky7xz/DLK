{"textgrid.poem.25874": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[als ich gl\u00fccklich war]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich gl\u00fccklich war,", "tokens": ["Als", "ich", "gl\u00fcck\u00b7lich", "war", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Konnte ich nicht singen,", "tokens": ["Konn\u00b7te", "ich", "nicht", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Schm\u00fcckte tagelang", "tokens": ["Schm\u00fcck\u00b7te", "ta\u00b7ge\u00b7lang"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Haar und Hals und Brust", "tokens": ["Haar", "und", "Hals", "und", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Mit Korallenrot und mit goldnen Ringen.", "tokens": ["Mit", "Ko\u00b7ral\u00b7len\u00b7rot", "und", "mit", "gold\u00b7nen", "Rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "ADJA", "NN", "$."], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Nun ich arm, arm bin,", "tokens": ["Nun", "ich", "arm", ",", "arm", "bin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADJD", "$,", "ADJD", "VAFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Sing ich tausend Lieder,", "tokens": ["Sing", "ich", "tau\u00b7send", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "CARD", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Schlinge rote T\u00f6ne", "tokens": ["Schlin\u00b7ge", "ro\u00b7te", "T\u00f6\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "\u00dcber Haar, Hals, Glieder,", "tokens": ["\u00dc\u00b7ber", "Haar", ",", "Hals", ",", "Glie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Keiner soll es sehen,", "tokens": ["Kei\u00b7ner", "soll", "es", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Da\u00df ich gl\u00fccknackt bin.", "tokens": ["Da\u00df", "ich", "gl\u00fcc\u00b7knackt", "bin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}