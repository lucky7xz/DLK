{"textgrid.poem.57706": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Freundschaft", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als du mir gabst die Bruderhand,", "tokens": ["Als", "du", "mir", "gabst", "die", "Bru\u00b7der\u00b7hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da war es gr\u00fcner Mai;", "tokens": ["Da", "war", "es", "gr\u00fc\u00b7ner", "Mai", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Wind pfeift auf dem Stoppelland,", "tokens": ["Der", "Wind", "pfeift", "auf", "dem", "Stop\u00b7pel\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Freundschaft ist vorbei.", "tokens": ["Die", "Freund\u00b7schaft", "ist", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Bei Finkenschlag und gelbem Wein", "tokens": ["Bei", "Fin\u00b7ken\u00b7schlag", "und", "gel\u00b7bem", "Wein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Trankst du mir Schmollis zu;", "tokens": ["Trankst", "du", "mir", "Schmol\u00b7lis", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "NE", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbwir wollen treue Br\u00fcder sein,", "tokens": ["\u00bb", "wir", "wol\u00b7len", "treu\u00b7e", "Br\u00fc\u00b7der", "sein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Hand auf Du und Du.\u00ab", "tokens": ["Die", "Hand", "auf", "Du", "und", "Du", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "KON", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Jetzt heult der Wind sein Herbstgedicht,", "tokens": ["Jetzt", "heult", "der", "Wind", "sein", "Herbst\u00b7ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "D\u00fcrr Laub tanzt auf dem Sand;", "tokens": ["D\u00fcrr", "Laub", "tanzt", "auf", "dem", "Sand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du siehst mir ruhig ins Gesicht", "tokens": ["Du", "siehst", "mir", "ru\u00b7hig", "ins", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hebst zum Schu\u00df die Hand.", "tokens": ["Und", "hebst", "zum", "Schu\u00df", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}