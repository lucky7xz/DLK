{"dta.poem.19961": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Gespr\u00e4ch .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Widrig ist mir f\u00fcrwahr, was sch\u00f6n t\u00f6nt, ohne zu n\u00fctzen.", "tokens": ["Wid\u00b7rig", "ist", "mir", "f\u00fcr\u00b7wahr", ",", "was", "sch\u00f6n", "t\u00f6nt", ",", "oh\u00b7ne", "zu", "n\u00fct\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "$,", "PRELS", "ADJD", "VVFIN", "$,", "KOUI", "PTKZU", "VVINF", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Triebe des Hirten Gesang nur eine M\u00fchle des Thals!", "tokens": ["Trie\u00b7be", "des", "Hir\u00b7ten", "Ge\u00b7sang", "nur", "ei\u00b7ne", "M\u00fch\u00b7le", "des", "Thals", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Widrig ist mir f\u00fcrwahr der Wind, den die Orgel vergeudet,", "tokens": ["Wid\u00b7rig", "ist", "mir", "f\u00fcr\u00b7wahr", "der", "Wind", ",", "den", "die", "Or\u00b7gel", "ver\u00b7geu\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Wenn, aus der Pfeife gejagt, er nicht Getraide noch", "tokens": ["Wenn", ",", "aus", "der", "Pfei\u00b7fe", "ge\u00b7jagt", ",", "er", "nicht", "Ge\u00b7trai\u00b7de", "noch"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "APPR", "ART", "NN", "VVPP", "$,", "PPER", "PTKNEG", "NN", "ADV"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "st\u00e4ubt.", "tokens": ["st\u00e4ubt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}}, "stanza.3": {"line.1": {"text": "Widrig ist mir f\u00fcrwahr der Abendglocken Gel\u00e4ute,", "tokens": ["Wid\u00b7rig", "ist", "mir", "f\u00fcr\u00b7wahr", "der", "A\u00b7bend\u00b7glo\u00b7cken", "Ge\u00b7l\u00e4u\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Treibt es nicht drohend Gew\u00f6lk' \u00fcber dem Acker mir weg.", "tokens": ["Treibt", "es", "nicht", "dro\u00b7hend", "Ge\u00b7w\u00f6l\u00b7k'", "\u00fc\u00b7ber", "dem", "A\u00b7cker", "mir", "weg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "VVPP", "NN", "APPR", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+--+-+--+--+", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Widrig ist mir f\u00fcrwahr jedwedes Bildni\u00df von Marmor,", "tokens": ["Wid\u00b7rig", "ist", "mir", "f\u00fcr\u00b7wahr", "jed\u00b7we\u00b7des", "Bild\u00b7ni\u00df", "von", "Mar\u00b7mor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "PIAT", "NN", "APPR", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Spendet nicht Wasser sein Mund, tr\u00e4gt es nicht st\u00fctzend", "tokens": ["Spen\u00b7det", "nicht", "Was\u00b7ser", "sein", "Mund", ",", "tr\u00e4gt", "es", "nicht", "st\u00fct\u00b7zend"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "NN", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "ADJD"], "meter": "+--+--++--+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "ein Haus.", "tokens": ["ein", "Haus", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.5": {"line.1": {"text": "Immer am widrigsten bleibt der Schein des Monds und", "tokens": ["Im\u00b7mer", "am", "wid\u00b7rigs\u00b7ten", "bleibt", "der", "Schein", "des", "Monds", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "VVFIN", "ART", "NN", "ART", "NN", "KON"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "der Sterne,", "tokens": ["der", "Ster\u00b7ne", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Nicht ein K\u00f6rnlein, bei Gott! weckt ihr unpraktischer", "tokens": ["Nicht", "ein", "K\u00f6rn\u00b7lein", ",", "bei", "Gott", "!", "weckt", "ihr", "un\u00b7prak\u00b7ti\u00b7scher"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "$,", "APPR", "NN", "$.", "VVFIN", "PPOSAT", "ADJA"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Stral.", "tokens": ["Stral", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}}}}}