{"textgrid.poem.60510": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Scherz nach einer zuf\u00e4llig aufgeschlagenen Bibelstelle", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ahimaaz, der Sohn des Zadok, sprach,", "tokens": ["A\u00b7hi\u00b7maaz", ",", "der", "Sohn", "des", "Za\u00b7dok", ",", "sprach", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Sprach wiederholten Males zu Joab:", "tokens": ["Sprach", "wie\u00b7der\u00b7hol\u00b7ten", "Ma\u00b7les", "zu", "Joab", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "APPR", "NE", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "\u00bbwie, wenn ich liefe auch dem Chusi nach,", "tokens": ["\u00bb", "wie", ",", "wenn", "ich", "lie\u00b7fe", "auch", "dem", "Chu\u00b7si", "nach", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "$,", "KOUS", "PPER", "VVFIN", "ADV", "ART", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Schnell hinter seiner Ferse Staub im Trab?\u00ab", "tokens": ["Schnell", "hin\u00b7ter", "sei\u00b7ner", "Fer\u00b7se", "Staub", "im", "Trab", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da sprach Joab: \u00bbGemach, mein Sohn, gemach!", "tokens": ["Da", "sprach", "Joab", ":", "\u00bb", "Ge\u00b7mach", ",", "mein", "Sohn", ",", "ge\u00b7mach", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$.", "$(", "ADV", "$,", "PPOSAT", "NN", "$,", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bleib, gib dem Winkel deinen Wanderstab,", "tokens": ["Bleib", ",", "gib", "dem", "Win\u00b7kel", "dei\u00b7nen", "Wan\u00b7der\u00b7stab", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "La\u00df deine Botschaft unter meinem Dach,", "tokens": ["La\u00df", "dei\u00b7ne", "Bot\u00b7schaft", "un\u00b7ter", "mei\u00b7nem", "Dach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der K\u00f6nig kauft sie dir mit Pr\u00fcgeln ab!\u00ab", "tokens": ["Der", "K\u00f6\u00b7nig", "kauft", "sie", "dir", "mit", "Pr\u00fc\u00b7geln", "ab", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "APPR", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch jener spricht: \u00bbWie, wenn ich dennoch laufe,", "tokens": ["Doch", "je\u00b7ner", "spricht", ":", "\u00bb", "Wie", ",", "wenn", "ich", "den\u00b7noch", "lau\u00b7fe", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "$.", "$(", "PWAV", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und bald zur\u00fcck den Chusi spring und schnaufe?\u00ab", "tokens": ["Und", "bald", "zu\u00b7r\u00fcck", "den", "Chu\u00b7si", "spring", "und", "schnau\u00b7fe", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "NE", "VVFIN", "KON", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da sprach Joab: \u00bbSo laufe doch mein Sohn!\u00ab", "tokens": ["Da", "sprach", "Joab", ":", "\u00bb", "So", "lau\u00b7fe", "doch", "mein", "Sohn", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "NE", "$.", "$(", "ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$.", "$("], "meter": "--+-+-+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Und also lief stracks fort Ahimaaz", "tokens": ["Und", "al\u00b7so", "lief", "stracks", "fort", "A\u00b7hi\u00b7maaz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADV", "PTKVZ", "NN"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und springt dem Chusi vor im schnellen Satz,", "tokens": ["Und", "springt", "dem", "Chu\u00b7si", "vor", "im", "schnel\u00b7len", "Satz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und Chusi kommt um seinen Botenlohn.", "tokens": ["Und", "Chu\u00b7si", "kommt", "um", "sei\u00b7nen", "Bo\u00b7ten\u00b7lohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}