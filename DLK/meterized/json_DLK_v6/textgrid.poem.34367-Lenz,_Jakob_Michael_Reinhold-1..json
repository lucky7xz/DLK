{"textgrid.poem.34367": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Woher, Herr Seelen-Archiater,", "tokens": ["Wo\u00b7her", ",", "Herr", "See\u00b7len\u00b7A\u00b7rch\u00b7i\u00b7a\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PWAV", "$,", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Geistlich-Armen Procurater,", "tokens": ["Der", "Geist\u00b7lich\u00b7Ar\u00b7men", "Pro\u00b7cu\u00b7ra\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Verse wahre Pia-Mater,", "tokens": ["Der", "Ver\u00b7se", "wah\u00b7re", "Pia\u00b7Ma\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Der Versemacher Prior-Pater,", "tokens": ["Der", "Ver\u00b7se\u00b7ma\u00b7cher", "Pri\u00b7o\u00b7rPa\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Von guten Sch\u00e4deln stets der Frater,", "tokens": ["Von", "gu\u00b7ten", "Sch\u00e4\u00b7deln", "stets", "der", "Fra\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von allen Schwachen stets der Vater,", "tokens": ["Von", "al\u00b7len", "Schwa\u00b7chen", "stets", "der", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Von allen Starken der Calfater \u2013", "tokens": ["Von", "al\u00b7len", "Star\u00b7ken", "der", "Cal\u00b7fa\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Kurzum, mein lieber Herr Lavater,", "tokens": ["Kur\u00b7zum", ",", "mein", "lie\u00b7ber", "Herr", "La\u00b7va\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Des Herr Gotts Nuntius a Later! \u2013", "tokens": ["Des", "Herr", "Gotts", "Nun\u00b7ti\u00b7us", "a", "La\u00b7ter", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NE", "NE", "NE", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sag Er, wo nehm ich einen Stater?", "tokens": ["Sag", "Er", ",", "wo", "nehm", "ich", "ei\u00b7nen", "Sta\u00b7ter", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PWAV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}