{"textgrid.poem.42877": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Ich tanzte mit ihr", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Reiter die Steppe durchjagen \u2013", "tokens": ["Als", "Rei\u00b7ter", "die", "Step\u00b7pe", "durch\u00b7ja\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Wandern in Schritten, ersungen aus gleichem Gef\u00fchl,", "tokens": ["Wan\u00b7dern", "in", "Schrit\u00b7ten", ",", "er\u00b7sun\u00b7gen", "aus", "glei\u00b7chem", "Ge\u00b7f\u00fchl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.3": {"text": "Oder mit K\u00fchnheit gespannt den Wagen", "tokens": ["O\u00b7der", "mit", "K\u00fchn\u00b7heit", "ge\u00b7spannt", "den", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVPP", "ART", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Lenkend durch Gefahren und Stra\u00dfengew\u00fchl \u2013", "tokens": ["Len\u00b7kend", "durch", "Ge\u00b7fah\u00b7ren", "und", "Stra\u00b7\u00dfen\u00b7ge\u00b7w\u00fchl", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$("], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Mit der Schaukel hinauf und hernieder,", "tokens": ["Mit", "der", "Schau\u00b7kel", "hin\u00b7auf", "und", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Treibend im Boote \u00fcber die Wellen gewiegt,", "tokens": ["Trei\u00b7bend", "im", "Boo\u00b7te", "\u00fc\u00b7ber", "die", "Wel\u00b7len", "ge\u00b7wiegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Mit dem Schlitten zu Tal. Und dann wieder", "tokens": ["Mit", "dem", "Schlit\u00b7ten", "zu", "Tal", ".", "Und", "dann", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$.", "KON", "ADV", "ADV"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Auf, wie die M\u00f6we dem Winde entgegenfliegt.", "tokens": ["Auf", ",", "wie", "die", "M\u00f6\u00b7we", "dem", "Win\u00b7de", "ent\u00b7ge\u00b7gen\u00b7fliegt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PWAV", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.9": {"text": "Und das alles allzumal", "tokens": ["Und", "das", "al\u00b7les", "all\u00b7zu\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "PIS", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Genossen wir tanzend im Saal.", "tokens": ["Ge\u00b7nos\u00b7sen", "wir", "tan\u00b7zend", "im", "Saal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.11": {"text": "In uns kreiste das Blut und der Wein,", "tokens": ["In", "uns", "kreis\u00b7te", "das", "Blut", "und", "der", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.12": {"text": "Um uns ein Fest mit W\u00e4nden und H\u00e4nden,", "tokens": ["Um", "uns", "ein", "Fest", "mit", "W\u00e4n\u00b7den", "und", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Gesichtern, Lichtern und Gegenst\u00e4nden.", "tokens": ["Ge\u00b7sich\u00b7tern", ",", "Lich\u00b7tern", "und", "Ge\u00b7gen\u00b7st\u00e4n\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Wir standen in dem Ringelreihn", "tokens": ["Wir", "stan\u00b7den", "in", "dem", "Rin\u00b7gel\u00b7reihn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Eigentlich ganz allein,", "tokens": ["Ei\u00b7gent\u00b7lich", "ganz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Ein Mensch aus zwein.", "tokens": ["Ein", "Mensch", "aus", "zwein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}