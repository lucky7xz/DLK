{"textgrid.poem.34020": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Ein Bild", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Morgenwinde sah ich Blumen wanken", "tokens": ["Im", "Mor\u00b7gen\u00b7win\u00b7de", "sah", "ich", "Blu\u00b7men", "wan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sah, wie sie den Thau der gold'nen Fr\u00fche,", "tokens": ["Und", "sah", ",", "wie", "sie", "den", "Thau", "der", "gold'\u00b7nen", "Fr\u00fc\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df jede voller dufte, tiefer gl\u00fche,", "tokens": ["Da\u00df", "je\u00b7de", "vol\u00b7ler", "duf\u00b7te", ",", "tie\u00b7fer", "gl\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "ADJA", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit hei\u00dfem Mund begierig in sich tranken.", "tokens": ["Mit", "hei\u00b7\u00dfem", "Mund", "be\u00b7gie\u00b7rig", "in", "sich", "tran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ges\u00e4ttigt sah ich bald die meisten schwanken,", "tokens": ["Ge\u00b7s\u00e4t\u00b7tigt", "sah", "ich", "bald", "die", "meis\u00b7ten", "schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "ADV", "ART", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als glaubten sie, da\u00df keine nun verbl\u00fche,", "tokens": ["Als", "glaub\u00b7ten", "sie", ",", "da\u00df", "kei\u00b7ne", "nun", "ver\u00b7bl\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "$,", "KOUS", "PIAT", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Rosen tranken fort mit s\u00fc\u00dfer M\u00fche,", "tokens": ["Die", "Ro\u00b7sen", "tran\u00b7ken", "fort", "mit", "s\u00fc\u00b7\u00dfer", "M\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bis ihre Kelche fast zur Erde sanken.", "tokens": ["Bis", "ih\u00b7re", "Kel\u00b7che", "fast", "zur", "Er\u00b7de", "san\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die andern wiegten sich in Luftgef\u00fchlen,", "tokens": ["Die", "an\u00b7dern", "wieg\u00b7ten", "sich", "in", "Luft\u00b7ge\u00b7f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie wollten eben lauten Spott erheben,", "tokens": ["Sie", "woll\u00b7ten", "e\u00b7ben", "lau\u00b7ten", "Spott", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da scho\u00df die Sonne ihre Flammen-Pfeile.", "tokens": ["Da", "scho\u00df", "die", "Son\u00b7ne", "ih\u00b7re", "Flam\u00b7men\u00b7Pfei\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Rosen l\u00f6schten sie im Thau, dem k\u00fchlen,", "tokens": ["Die", "Ro\u00b7sen", "l\u00f6schten", "sie", "im", "Thau", ",", "dem", "k\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Doch jenen drangen sie in Mark und Leben,", "tokens": ["Doch", "je\u00b7nen", "dran\u00b7gen", "sie", "in", "Mark", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Man sah sie hingewelkt nach kurzer Weile.", "tokens": ["Man", "sah", "sie", "hin\u00b7ge\u00b7welkt", "nach", "kur\u00b7zer", "Wei\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}