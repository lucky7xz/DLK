{"dta.poem.7541": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "40.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer einen Fehltritt that, verzeih ihm, lieber Mann!", "tokens": ["Wer", "ei\u00b7nen", "Fehl\u00b7tritt", "that", ",", "ver\u00b7zeih", "ihm", ",", "lie\u00b7ber", "Mann", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "VVIMP", "PPER", "$,", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bedenk, auch einen Fu\u00df hast du der straucheln kann.", "tokens": ["Be\u00b7denk", ",", "auch", "ei\u00b7nen", "Fu\u00df", "hast", "du", "der", "strau\u00b7cheln", "kann", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ART", "NN", "VAFIN", "PPER", "ART", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Heil dem, der Demuth lernt nicht durch Dem\u00fcthigungen,", "tokens": ["Heil", "dem", ",", "der", "De\u00b7muth", "lernt", "nicht", "durch", "De\u00b7m\u00fct\u00b7hi\u00b7gun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "$,", "ART", "NN", "VVFIN", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der ohne da\u00df die Welt ihn zwang, sich hat bezwungen.", "tokens": ["Der", "oh\u00b7ne", "da\u00df", "die", "Welt", "ihn", "zwang", ",", "sich", "hat", "be\u00b7zwun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,", "PRF", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Den Niedern bl\u00e4ht Besitz, und Armuth macht ihn zahm,", "tokens": ["Den", "Nie\u00b7dern", "bl\u00e4ht", "Be\u00b7sitz", ",", "und", "Ar\u00b7muth", "macht", "ihn", "zahm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$,", "KON", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Edeln macht sie stolz, und Reichthum demuthsam.", "tokens": ["Den", "E\u00b7deln", "macht", "sie", "stolz", ",", "und", "Reicht\u00b7hum", "de\u00b7muth\u00b7sam", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$,", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Ein schlechtes Schauspiel ists, wenn hoch die Niedern steigen,", "tokens": ["Ein", "schlech\u00b7tes", "Schau\u00b7spiel", "ists", ",", "wenn", "hoch", "die", "Nie\u00b7dern", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "KOUS", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ein erb\u00e4rmliches, wenn sie zum Fall sich neigen.", "tokens": ["Und", "ein", "er\u00b7b\u00e4rm\u00b7li\u00b7ches", ",", "wenn", "sie", "zum", "Fall", "sich", "nei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$,", "KOUS", "PPER", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wer ohne sein Verdienst gestiegen ist, erh\u00e4lt", "tokens": ["Wer", "oh\u00b7ne", "sein", "Ver\u00b7dienst", "ge\u00b7stie\u00b7gen", "ist", ",", "er\u00b7h\u00e4lt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "APPR", "PPOSAT", "NN", "VVPP", "VAFIN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durchs Steigen Achtung nicht, noch Mitleid wenn er f\u00e4llt.", "tokens": ["Durchs", "Stei\u00b7gen", "Ach\u00b7tung", "nicht", ",", "noch", "Mit\u00b7leid", "wenn", "er", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "PTKNEG", "$,", "ADV", "NN", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der oberste der Pl\u00e4tz' ist schwankender als alle,", "tokens": ["Der", "o\u00b7bers\u00b7te", "der", "Pl\u00e4tz'", "ist", "schwan\u00b7ken\u00b7der", "als", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "NN", "VAFIN", "ADJD", "KOKOM", "PIS", "$,"], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Und jeder strebt hinauf, nur da\u00df herab er falle.", "tokens": ["Und", "je\u00b7der", "strebt", "hin\u00b7auf", ",", "nur", "da\u00df", "her\u00b7ab", "er", "fal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PTKVZ", "$,", "ADV", "KOUS", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wer seine Stellung kennt und dazu seine Kraft,", "tokens": ["Wer", "sei\u00b7ne", "Stel\u00b7lung", "kennt", "und", "da\u00b7zu", "sei\u00b7ne", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "KON", "PAV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und beiden wirkt gem\u00e4\u00df, der wirkt untadelhaft.", "tokens": ["Und", "bei\u00b7den", "wirkt", "ge\u00b7m\u00e4\u00df", ",", "der", "wirkt", "un\u00b7ta\u00b7del\u00b7haft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADJD", "$,", "PRELS", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Zum Selbstgef\u00e4ll'gen sprich: Ich m\u00f6chte lieber Allen,", "tokens": ["Zum", "Selbst\u00b7ge\u00b7f\u00e4ll'\u00b7gen", "sprich", ":", "Ich", "m\u00f6ch\u00b7te", "lie\u00b7ber", "Al\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$.", "PPER", "VMFIN", "ADV", "NE", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie du dir selbst, als mir, wie ihnen du, gefallen.", "tokens": ["Wie", "du", "dir", "selbst", ",", "als", "mir", ",", "wie", "ih\u00b7nen", "du", ",", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "$,", "KOUS", "PPER", "$,", "PWAV", "PPER", "PPER", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Die Demuth ehre du, und zu der Demuth Ehren", "tokens": ["Die", "De\u00b7muth", "eh\u00b7re", "du", ",", "und", "zu", "der", "De\u00b7muth", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sei gegen Stolze stolz, um Demuth sie zu lehren.", "tokens": ["Sei", "ge\u00b7gen", "Stol\u00b7ze", "stolz", ",", "um", "De\u00b7muth", "sie", "zu", "leh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "ADJD", "$,", "KOUI", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+---+-", "measure": "iambic.penta.invert"}}}}}