{"textgrid.poem.50678": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Der bestechliche Richter", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Strafe scheut er nicht, wenn er sich l\u00e4\u00dft bestechen;", "tokens": ["Die", "Stra\u00b7fe", "scheut", "er", "nicht", ",", "wenn", "er", "sich", "l\u00e4\u00dft", "be\u00b7ste\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "PRF", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn er zahlt die, wird er entdeckt, ", "tokens": ["Denn", "er", "zahlt", "die", ",", "wird", "er", "ent\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "$,", "VAFIN", "PPER", "VVPP", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}}}}