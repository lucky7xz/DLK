{"textgrid.poem.50709": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Der Trompeter des Prinzen Moriz", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war dem ", "tokens": ["Es", "war", "dem"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Da\u00df Moriz mit dem Heer sich zu verschanzen trachte.", "tokens": ["Da\u00df", "Mo\u00b7riz", "mit", "dem", "Heer", "sich", "zu", "ver\u00b7schan\u00b7zen", "trach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "ART", "NN", "PRF", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als ein Trompeter nun ihm wurde zugesandt,", "tokens": ["Als", "ein", "Trom\u00b7pe\u00b7ter", "nun", "ihm", "wur\u00b7de", "zu\u00b7ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der ein Gewerb' vom Prinzen brachte,", "tokens": ["Der", "ein", "Ge\u00b7werb'", "vom", "Prin\u00b7zen", "brach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So fragt' er ihn: \u00bbWie kommt's, da\u00df euer junger Held", "tokens": ["So", "fragt'", "er", "ihn", ":", "\u00bb", "Wie", "kommt's", ",", "da\u00df", "eu\u00b7er", "jun\u00b7ger", "Held"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$.", "$(", "PWAV", "VVFIN", "$,", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So bl\u00f6d' und furchtsam sich in der Verschanzung h\u00e4lt?\u00ab \u2013", "tokens": ["So", "bl\u00f6d'", "und", "furcht\u00b7sam", "sich", "in", "der", "Ver\u00b7schan\u00b7zung", "h\u00e4lt", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VVFIN", "KON", "ADJD", "PRF", "APPR", "ART", "NN", "VVFIN", "$.", "$(", "$("], "meter": "-+-+--++-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "\u00bbes m\u00f6cht'\u00ab, antwortet' er, sich neigend bis zur Erden,", "tokens": ["\u00bb", "es", "m\u00f6cht'", "\u00ab", ",", "ant\u00b7wor\u00b7tet'", "er", ",", "sich", "nei\u00b7gend", "bis", "zur", "Er\u00b7den", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "$(", "$,", "VVFIN", "PPER", "$,", "PRF", "ADJD", "APPR", "APPRART", "NN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "\u00bbmein Herr auch gern, wie Ihr, ein ", "tokens": ["\u00bb", "mein", "Herr", "auch", "gern", ",", "wie", "Ihr", ",", "ein"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["$(", "PPOSAT", "NN", "ADV", "ADV", "$,", "PWAV", "PPER", "$,", "ART"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}}}}}