{"textgrid.poem.53427": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "[ach frommer Gott, wo sol ich hin]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach frommer Gott, wo sol ich hin", "tokens": ["Ach", "from\u00b7mer", "Gott", ",", "wo", "sol", "ich", "hin"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "ADJA", "NN", "$,", "PWAV", "VMFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit meinem hochbetr\u00fcbten Sinn", "tokens": ["Mit", "mei\u00b7nem", "hoch\u00b7be\u00b7tr\u00fcb\u00b7ten", "Sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und tieffen SeelenSchaden,", "tokens": ["Und", "tief\u00b7fen", "See\u00b7len", "Scha\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein kranckes Hertz, ist wie mit Ertz", "tokens": ["Mein", "kran\u00b7ckes", "Hertz", ",", "ist", "wie", "mit", "Ertz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VAFIN", "KOKOM", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und Steinen \u00fcberladen.", "tokens": ["Und", "Stei\u00b7nen", "\u00fc\u00b7berl\u00b7a\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie klagt mich mein Gewissen an,", "tokens": ["Wie", "klagt", "mich", "mein", "Ge\u00b7wis\u00b7sen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es thut mich grausam in den Bann,", "tokens": ["Es", "thut", "mich", "grau\u00b7sam", "in", "den", "Bann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich mu\u00df mich selbst verjagen,", "tokens": ["Ich", "mu\u00df", "mich", "selbst", "ver\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und seinen Mord an allem Ort", "tokens": ["Und", "sei\u00b7nen", "Mord", "an", "al\u00b7lem", "Ort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PIS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In meinem Busen tragen.", "tokens": ["In", "mei\u00b7nem", "Bu\u00b7sen", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Gleich wie ein Wild durch schnelle Flucht", "tokens": ["Gleich", "wie", "ein", "Wild", "durch", "schnel\u00b7le", "Flucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Pfeilen zu entgehen sucht,", "tokens": ["Den", "Pfei\u00b7len", "zu", "ent\u00b7ge\u00b7hen", "sucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die schon sein Hertz empfinden,", "tokens": ["Die", "schon", "sein", "Hertz", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So eyl auch ich, und trage mich", "tokens": ["So", "eyl", "auch", "ich", ",", "und", "tra\u00b7ge", "mich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "NN", "ADV", "PPER", "$,", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit meinen H\u00f6llen-Hunden.", "tokens": ["Mit", "mei\u00b7nen", "H\u00f6l\u00b7len\u00b7Hun\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wer hilfft in diesen N\u00f6then mir?", "tokens": ["Wer", "hilfft", "in", "die\u00b7sen", "N\u00f6\u00b7then", "mir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PDAT", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herr, mein Verlangen steht nach dir,", "tokens": ["Herr", ",", "mein", "Ver\u00b7lan\u00b7gen", "steht", "nach", "dir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich stell auff dich Vertrauen", "tokens": ["Ich", "stell", "auff", "dich", "Ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "APPR", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und Hoffnung, Gott, la\u00df keinen Spott", "tokens": ["Und", "Hoff\u00b7nung", ",", "Gott", ",", "la\u00df", "kei\u00b7nen", "Spott"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$,", "NN", "$,", "VVIMP", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bey deiner Furcht mich schauen.", "tokens": ["Bey", "dei\u00b7ner", "Furcht", "mich", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Dann keiner, der gedultig dein", "tokens": ["Dann", "kei\u00b7ner", ",", "der", "ge\u00b7dul\u00b7tig", "dein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PIS", "$,", "PRELS", "ADJD", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kan harren, wird in Schanden seyn,", "tokens": ["Kan", "har\u00b7ren", ",", "wird", "in", "Schan\u00b7den", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "VAFIN", "APPR", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df den zu Schanden werden,", "tokens": ["La\u00df", "den", "zu", "Schan\u00b7den", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "APPR", "NN", "VAINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Der deiner Macht ver\u00e4chtlich lacht,", "tokens": ["Der", "dei\u00b7ner", "Macht", "ver\u00b7\u00e4cht\u00b7lich", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und traut der schn\u00f6den Erden.", "tokens": ["Und", "traut", "der", "schn\u00f6\u00b7den", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Gedenck an die Barmhertzigkeit,", "tokens": ["Ge\u00b7denck", "an", "die", "Barm\u00b7hert\u00b7zig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die du erwiesen allezeit", "tokens": ["Die", "du", "er\u00b7wie\u00b7sen", "al\u00b7le\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seyt da\u00df die Welt gestanden,", "tokens": ["Seyt", "da\u00df", "die", "Welt", "ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gedencke nicht an dein Gericht", "tokens": ["Ge\u00b7den\u00b7cke", "nicht", "an", "dein", "Ge\u00b7richt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und meiner Jugend Schanden.", "tokens": ["Und", "mei\u00b7ner", "Ju\u00b7gend", "Schan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Sieh meine Thorheit \u00fcberhin", "tokens": ["Sieh", "mei\u00b7ne", "Thor\u00b7heit", "\u00fc\u00b7berh\u00b7in"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach deiner gro\u00dfen Langmuht Sinn,", "tokens": ["Nach", "dei\u00b7ner", "gro\u00b7\u00dfen", "Lang\u00b7muht", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df doch mein Hertz sich stillen,", "tokens": ["La\u00df", "doch", "mein", "Hertz", "sich", "stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPOSAT", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gedencke mein in Lieb allein", "tokens": ["Ge\u00b7den\u00b7cke", "mein", "in", "Lieb", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "APPR", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Umb deiner G\u00fcte willen.", "tokens": ["Umb", "dei\u00b7ner", "G\u00fc\u00b7te", "wil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Gib deinem grossen Namen stat,", "tokens": ["Gib", "dei\u00b7nem", "gros\u00b7sen", "Na\u00b7men", "stat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sey gn\u00e4dig meiner Missethat,", "tokens": ["Sey", "gn\u00e4\u00b7dig", "mei\u00b7ner", "Mis\u00b7se\u00b7that", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die ich dir nicht verh\u00e4le,", "tokens": ["Die", "ich", "dir", "nicht", "ver\u00b7h\u00e4\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist gleich kein Ziel, und ihr so viel,", "tokens": ["Ist", "gleich", "kein", "Ziel", ",", "und", "ihr", "so", "viel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "$,", "KON", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df ich sie gar nicht zehle.", "tokens": ["Da\u00df", "ich", "sie", "gar", "nicht", "zeh\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Mach mich von meinem Kummer lo\u00df,", "tokens": ["Mach", "mich", "von", "mei\u00b7nem", "Kum\u00b7mer", "lo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann meines Hertzens-Angst ist gro\u00df,", "tokens": ["Dann", "mei\u00b7nes", "Hert\u00b7zens\u00b7Angst", "ist", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Entf\u00fchr mich meinen N\u00f6then,", "tokens": ["Ent\u00b7f\u00fchr", "mich", "mei\u00b7nen", "N\u00f6\u00b7then", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schau gn\u00e4dig her auff mein Beschwer,", "tokens": ["Schau", "gn\u00e4\u00b7dig", "her", "auff", "mein", "Be\u00b7schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Es dr\u00e4uet mich zu t\u00f6dten.", "tokens": ["Es", "dr\u00e4u\u00b7et", "mich", "zu", "t\u00f6d\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Vergib, O Vater, aus Gedult", "tokens": ["Ver\u00b7gib", ",", "O", "Va\u00b7ter", ",", "aus", "Ge\u00b7dult"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "$,", "NE", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir alle meine S\u00fcnden-Schuld,", "tokens": ["Mir", "al\u00b7le", "mei\u00b7ne", "S\u00fcn\u00b7den\u00b7Schuld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df meine Seele leben,", "tokens": ["La\u00df", "mei\u00b7ne", "See\u00b7le", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Errette sie, damit ich nie", "tokens": ["Er\u00b7ret\u00b7te", "sie", ",", "da\u00b7mit", "ich", "nie"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In Schanden m\u00f6ge schweben.", "tokens": ["In", "Schan\u00b7den", "m\u00f6\u00b7ge", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Dann sieh, ich trau allein auff dich,", "tokens": ["Dann", "sieh", ",", "ich", "trau", "al\u00b7lein", "auff", "dich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Schlecht und Recht beh\u00fcte mich,", "tokens": ["Durch", "Schlecht", "und", "Recht", "be\u00b7h\u00fc\u00b7te", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gott woll au\u00df allem B\u00f6sen", "tokens": ["Gott", "woll", "au\u00df", "al\u00b7lem", "B\u00f6\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "PIS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein arme Seel und Israel", "tokens": ["Mein", "ar\u00b7me", "Seel", "und", "Is\u00b7rael"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NE"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Au\u00df aller Noth erl\u00f6sen.", "tokens": ["Au\u00df", "al\u00b7ler", "Noth", "er\u00b7l\u00f6\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}