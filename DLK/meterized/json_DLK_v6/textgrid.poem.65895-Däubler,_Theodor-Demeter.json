{"textgrid.poem.65895": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Demeter", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verehrte Demeter, im Gl\u00fcck der Felder,", "tokens": ["Ver\u00b7ehr\u00b7te", "De\u00b7me\u00b7ter", ",", "im", "Gl\u00fcck", "der", "Fel\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Beherzte Hirtin unter bangen Seelen,", "tokens": ["Be\u00b7herz\u00b7te", "Hir\u00b7tin", "un\u00b7ter", "ban\u00b7gen", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer mag das Blut in deine Hut empfehlen?", "tokens": ["Wer", "mag", "das", "Blut", "in", "dei\u00b7ne", "Hut", "emp\u00b7feh\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du thronst gewertig vor dem Alp der W\u00e4lder,", "tokens": ["Du", "thronst", "ge\u00b7wer\u00b7tig", "vor", "dem", "Alp", "der", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Vertraut sind dir der Br\u00e4utlichkeit Vermelder;", "tokens": ["Ver\u00b7traut", "sind", "dir", "der", "Br\u00e4ut\u00b7lich\u00b7keit", "Ver\u00b7mel\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bejubelt aus Millionen Lerchenkehlen,", "tokens": ["Be\u00b7ju\u00b7belt", "aus", "Mil\u00b7lion\u00b7en", "Ler\u00b7chen\u00b7keh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Beschirmst du P\u00e4rchen, die aus Narrheit fehlen,", "tokens": ["Be\u00b7schirmst", "du", "P\u00e4r\u00b7chen", ",", "die", "aus", "Nar\u00b7rheit", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Besorgst, zu V\u00e4tern l\u00e4chelnd, Hochzeitsgelder.", "tokens": ["Be\u00b7sorgst", ",", "zu", "V\u00e4\u00b7tern", "l\u00e4\u00b7chelnd", ",", "Hoch\u00b7zeits\u00b7gel\u00b7der", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "NN", "ADJD", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Um meine Mitgift werb ich durch Erfahrung:", "tokens": ["Um", "mei\u00b7ne", "Mit\u00b7gift", "werb", "ich", "durch", "Er\u00b7fah\u00b7rung", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gelebte Sonnigkeit sei Angebinde", "tokens": ["Ge\u00b7leb\u00b7te", "Son\u00b7nig\u00b7keit", "sei", "An\u00b7ge\u00b7bin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VAFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von dir dereinst, Verwalterin der Nahrung,", "tokens": ["Von", "dir", "de\u00b7reinst", ",", "Ver\u00b7wal\u00b7te\u00b7rin", "der", "Nah\u00b7rung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wenn ich den Pfad zu deiner Tochter finde;", "tokens": ["Wenn", "ich", "den", "Pfad", "zu", "dei\u00b7ner", "Toch\u00b7ter", "fin\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Bock verwandt, bleibt meinem Leib Behaarung,", "tokens": ["Dem", "Bock", "ver\u00b7wandt", ",", "bleibt", "mei\u00b7nem", "Leib", "Be\u00b7haa\u00b7rung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVPP", "$,", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Begreif, da\u00df ich voll Leid von dannen \u2013 schwinde!", "tokens": ["Be\u00b7greif", ",", "da\u00df", "ich", "voll", "Leid", "von", "dan\u00b7nen", "\u2013", "schwin\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADJD", "NN", "APPR", "ADV", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}