{"textgrid.poem.35938": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ade, ihr Blumen! m\u00fc\u00dft nun all' verderben,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ade, ihr Blumen! m\u00fc\u00dft nun all' verderben,", "tokens": ["A\u00b7de", ",", "ihr", "Blu\u00b7men", "!", "m\u00fc\u00dft", "nun", "all'", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$.", "VMFIN", "ADV", "PIS", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Der Rose Pracht ist lange schon vergl\u00fcht;", "tokens": ["Der", "Ro\u00b7se", "Pracht", "ist", "lan\u00b7ge", "schon", "ver\u00b7gl\u00fcht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bald neigte dann die Lilie sich zum Sterben,", "tokens": ["Bald", "neig\u00b7te", "dann", "die", "Li\u00b7lie", "sich", "zum", "Ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann sind die duft'gen Nelken auch verbl\u00fcht.", "tokens": ["Dann", "sind", "die", "duft'\u00b7gen", "Nel\u00b7ken", "auch", "ver\u00b7bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie sind so wenig Blumen noch zu sehen", "tokens": ["Wie", "sind", "so", "we\u00b7nig", "Blu\u00b7men", "noch", "zu", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ADV", "PIAT", "NN", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von all dem bunten, fr\u00f6hlichen Gew\u00fchl,", "tokens": ["Von", "all", "dem", "bun\u00b7ten", ",", "fr\u00f6h\u00b7li\u00b7chen", "Ge\u00b7w\u00fchl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und die auch m\u00fcssen alle bald vergehen:", "tokens": ["Und", "die", "auch", "m\u00fcs\u00b7sen", "al\u00b7le", "bald", "ver\u00b7ge\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "VMFIN", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es weht vom Abend her so herbstlich k\u00fchl.", "tokens": ["Es", "weht", "vom", "A\u00b7bend", "her", "so", "herbst\u00b7lich", "k\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APZR", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die blasse Sonne will noch einmal gr\u00fc\u00dfen,", "tokens": ["Die", "blas\u00b7se", "Son\u00b7ne", "will", "noch", "ein\u00b7mal", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie kann vor Wehmuth nicht, sie h\u00fcllt sich ein.", "tokens": ["Sie", "kann", "vor", "Weh\u00b7muth", "nicht", ",", "sie", "h\u00fcllt", "sich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "PTKNEG", "$,", "PPER", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "D\u00fcrft' ich wie sie das m\u00fcde Auge schlie\u00dfen!", "tokens": ["D\u00fcrft'", "ich", "wie", "sie", "das", "m\u00fc\u00b7de", "Au\u00b7ge", "schlie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PWAV", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie mu\u00df doch Sterben gar so selig sein.", "tokens": ["Wie", "mu\u00df", "doch", "Ster\u00b7ben", "gar", "so", "se\u00b7lig", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADV", "NN", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wir irren, tr\u00e4umen, suchen viel hienieden,", "tokens": ["Wir", "ir\u00b7ren", ",", "tr\u00e4u\u00b7men", ",", "su\u00b7chen", "viel", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und eh' wir's ahnen, kommt das Abendroth,", "tokens": ["Und", "eh'", "wir's", "ah\u00b7nen", ",", "kommt", "das", "A\u00b7ben\u00b7droth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "VVINF", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann l\u00e4cheln wir und folgen ihm in Frieden,", "tokens": ["Dann", "l\u00e4\u00b7cheln", "wir", "und", "fol\u00b7gen", "ihm", "in", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem Heimath-Boten, ja, dem ernsten Tod. \u2013", "tokens": ["Dem", "Hei\u00b7math\u00b7Bo\u00b7ten", ",", "ja", ",", "dem", "erns\u00b7ten", "Tod", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "PTKANT", "$,", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Du, treuer Bruder, in der blauen Ferne,", "tokens": ["Du", ",", "treu\u00b7er", "Bru\u00b7der", ",", "in", "der", "blau\u00b7en", "Fer\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist Dir wie mir so herbstlich und so weh?", "tokens": ["Ist", "Dir", "wie", "mir", "so", "herbst\u00b7lich", "und", "so", "weh", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PWAV", "PPER", "ADV", "ADJD", "KON", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie denk' ich Dein in stiller Wehmuth gerne,", "tokens": ["Wie", "denk'", "ich", "Dein", "in", "stil\u00b7ler", "Weh\u00b7muth", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "APPR", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du reife Seele, denke mein! Ade! \u2013", "tokens": ["Du", "rei\u00b7fe", "See\u00b7le", ",", "den\u00b7ke", "mein", "!", "A\u00b7de", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "VVFIN", "PPOSAT", "$.", "NN", "$.", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}