{"textgrid.poem.33306": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "An Mlle. Jaquet, als Sara im Holl\u00e4nder", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn man der guten Sara Lehren,", "tokens": ["Wenn", "man", "der", "gu\u00b7ten", "Sa\u00b7ra", "Leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So wie der Dichter sie entworfen, liest,", "tokens": ["So", "wie", "der", "Dich\u00b7ter", "sie", "ent\u00b7wor\u00b7fen", ",", "liest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "PPER", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So scheint's, da\u00df Lernach, welchen sie bekehren,", "tokens": ["So", "scheint's", ",", "da\u00df", "Ler\u00b7nach", ",", "wel\u00b7chen", "sie", "be\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "NN", "$,", "PWAT", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein klein Theaterwunder ist:", "tokens": ["Ein", "klein", "The\u00b7a\u00b7ter\u00b7wun\u00b7der", "ist", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch wie wir sie aus deinem Munde h\u00f6rten,", "tokens": ["Doch", "wie", "wir", "sie", "aus", "dei\u00b7nem", "Mun\u00b7de", "h\u00f6r\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "W\u00e4r's noch ein gr\u00f6sseres, \u2013 wenn sie ihn nicht bekehrten.", "tokens": ["W\u00e4r's", "noch", "ein", "gr\u00f6s\u00b7se\u00b7res", ",", "\u2013", "wenn", "sie", "ihn", "nicht", "be\u00b7kehr\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "$,", "$(", "KOUS", "PPER", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}