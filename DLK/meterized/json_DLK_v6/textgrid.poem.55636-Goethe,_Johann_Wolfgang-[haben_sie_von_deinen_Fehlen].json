{"textgrid.poem.55636": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "[haben sie von deinen Fehlen]", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Haben sie von deinen Fehlen", "tokens": ["Ha\u00b7ben", "sie", "von", "dei\u00b7nen", "Feh\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Immer viel erz\u00e4hlt", "tokens": ["Im\u00b7mer", "viel", "er\u00b7z\u00e4hlt"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und, f\u00fcr wahr sie zu erz\u00e4hlen,", "tokens": ["Und", ",", "f\u00fcr", "wahr", "sie", "zu", "er\u00b7z\u00e4h\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ADJD", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vielfach sich gequ\u00e4lt.", "tokens": ["Viel\u00b7fach", "sich", "ge\u00b7qu\u00e4lt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PRF", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "H\u00e4tten sie von deinem Guten", "tokens": ["H\u00e4t\u00b7ten", "sie", "von", "dei\u00b7nem", "Gu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Freundlich dir erz\u00e4hlt,", "tokens": ["Freund\u00b7lich", "dir", "er\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Mit verst\u00e4ndig treuen Winken.", "tokens": ["Mit", "ver\u00b7st\u00e4n\u00b7dig", "treu\u00b7en", "Win\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie man Be\u00dfres w\u00e4hlt:", "tokens": ["Wie", "man", "Be\u00df\u00b7res", "w\u00e4hlt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "O gewi\u00df! das Allerbeste", "tokens": ["O", "ge\u00b7wi\u00df", "!", "das", "Al\u00b7ler\u00b7bes\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ITJ", "ADV", "$.", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Blieb ihm nicht verhehlt,", "tokens": ["Blieb", "ihm", "nicht", "ver\u00b7hehlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Das f\u00fcrwahr nur wenig G\u00e4ste", "tokens": ["Das", "f\u00fcr\u00b7wahr", "nur", "we\u00b7nig", "G\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ADV", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "In der Klause z\u00e4hlt,", "tokens": ["In", "der", "Klau\u00b7se", "z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.13": {"text": "Nun als Sch\u00fcler mich, zu kommen", "tokens": ["Nun", "als", "Sch\u00fc\u00b7ler", "mich", ",", "zu", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "KOUS", "NN", "PPER", "$,", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Endlich auserw\u00e4hlt,", "tokens": ["End\u00b7lich", "au\u00b7ser\u00b7w\u00e4hlt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.15": {"text": "Und mich lehrt der Bu\u00dfe Frommen,", "tokens": ["Und", "mich", "lehrt", "der", "Bu\u00b7\u00dfe", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Wenn der Mensch gefehlt.", "tokens": ["Wenn", "der", "Mensch", "ge\u00b7fehlt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}