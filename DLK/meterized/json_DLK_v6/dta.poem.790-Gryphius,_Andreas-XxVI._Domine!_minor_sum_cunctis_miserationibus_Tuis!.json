{"dta.poem.790": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxVI.  \n Domine! minor sum cunctis miserationibus  \n Tuis!", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00df alles trawren seyn/ H\u00f6r auff mein Hertz zu klagen. ", "tokens": ["La\u00df", "al\u00b7les", "traw\u00b7ren", "seyn", "/", "H\u00f6r", "auff", "mein", "Hertz", "zu", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIS", "VVFIN", "VAINF", "$(", "NE", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil dir der h\u00f6chste GOtt/ mehr als du je begehrt.", "tokens": ["Weil", "dir", "der", "h\u00f6chs\u00b7te", "Gott", "/", "mehr", "als", "du", "je", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$(", "ADV", "KOUS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mehr als zu w\u00fcndschen stund mit reicher Hand gewehrt.", "tokens": ["Mehr", "als", "zu", "w\u00fcnd\u00b7schen", "stund", "mit", "rei\u00b7cher", "Hand", "ge\u00b7wehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "PTKZU", "VVINF", "VVFIN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jtzt lach ich aller Noth! jtzt bricht die Lust mein zagen!", "tokens": ["Jtzt", "lach", "ich", "al\u00b7ler", "Noth", "!", "jtzt", "bricht", "die", "Lust", "mein", "za\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Angst/ da schier das Fleisch durch \u00fcber h\u00e4uffte plagen.", "tokens": ["Die", "Angst", "/", "da", "schier", "das", "Fleisch", "durch", "\u00fc\u00b7ber", "h\u00e4uff\u00b7te", "pla\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "ADJD", "ART", "NN", "APPR", "APPR", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den m\u00fcden Geist ausstie\u00df/ die wehmutt ist verkehrt/", "tokens": ["Den", "m\u00fc\u00b7den", "Geist", "aus\u00b7stie\u00df", "/", "die", "weh\u00b7mutt", "ist", "ver\u00b7kehrt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$(", "ART", "ADJD", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In frey vnd fr\u00f6lich seyn/ was jemal mich beschwehrt", "tokens": ["In", "frey", "vnd", "fr\u00f6\u00b7lich", "seyn", "/", "was", "je\u00b7mal", "mich", "be\u00b7schwehrt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "KON", "ADJD", "VAINF", "$(", "PWS", "ADV", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist als ein tr\u00fcber dunst vom winde weg getragen.", "tokens": ["Ist", "als", "ein", "tr\u00fc\u00b7ber", "dunst", "vom", "win\u00b7de", "weg", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "ADJA", "NN", "APPRART", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wie soll ich h\u00f6chster Herr! wie soll ich schwaches Kind/", "tokens": ["Wie", "soll", "ich", "h\u00f6chs\u00b7ter", "Herr", "!", "wie", "soll", "ich", "schwa\u00b7ches", "Kind", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADJA", "NN", "$.", "PWAV", "VMFIN", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erzehlen deine werck/ die nicht zu zehlen sind/", "tokens": ["Er\u00b7zeh\u00b7len", "dei\u00b7ne", "werck", "/", "die", "nicht", "zu", "zeh\u00b7len", "sind", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$(", "PRELS", "PTKNEG", "PTKZU", "VVINF", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer ist es doch/ den du der Gnade werth geschetzet/", "tokens": ["Wer", "ist", "es", "doch", "/", "den", "du", "der", "Gna\u00b7de", "werth", "ge\u00b7schet\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "$(", "ART", "PPER", "ART", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nicht dieser Thr\u00e4nen bach/ nicht dieser Seufftzen wind", "tokens": ["Nicht", "die\u00b7ser", "Thr\u00e4\u00b7nen", "bach", "/", "nicht", "die\u00b7ser", "Seufft\u00b7zen", "wind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "PDAT", "NN", "VVFIN", "$(", "PTKNEG", "PDAT", "NN", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht mein Gebett verdient/ die wollust die ich find.", "tokens": ["Nicht", "mein", "Ge\u00b7bett", "ver\u00b7dient", "/", "die", "wol\u00b7lust", "die", "ich", "find", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "VVPP", "$(", "ART", "NN", "ART", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du der du alles bist/ hast mich/ der nichts/ ergetzet.", "tokens": ["Du", "der", "du", "al\u00b7les", "bist", "/", "hast", "mich", "/", "der", "nichts", "/", "er\u00b7get\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ART", "PPER", "PIS", "VAFIN", "$(", "VAFIN", "PPER", "$(", "ART", "PIS", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}