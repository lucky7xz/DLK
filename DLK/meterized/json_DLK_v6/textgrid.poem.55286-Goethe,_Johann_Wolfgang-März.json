{"textgrid.poem.55286": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "M\u00e4rz", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein Schnee gefallen,", "tokens": ["Es", "ist", "ein", "Schnee", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Denn es ist noch nicht Zeit,", "tokens": ["Denn", "es", "ist", "noch", "nicht", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "PTKNEG", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Da\u00df von den Bl\u00fcmlein allen,", "tokens": ["Da\u00df", "von", "den", "Bl\u00fcm\u00b7lein", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PIAT", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df von den Bl\u00fcmlein allen", "tokens": ["Da\u00df", "von", "den", "Bl\u00fcm\u00b7lein", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wir werden hoch erfreut.", "tokens": ["Wir", "wer\u00b7den", "hoch", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Sonnenblick betr\u00fcget", "tokens": ["Der", "Son\u00b7nen\u00b7blick", "be\u00b7tr\u00fc\u00b7get"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit mildem, falschem Schein,", "tokens": ["Mit", "mil\u00b7dem", ",", "fal\u00b7schem", "Schein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Schwalbe selber l\u00fcget,", "tokens": ["Die", "Schwal\u00b7be", "sel\u00b7ber", "l\u00fc\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Schwalbe selber l\u00fcget,", "tokens": ["Die", "Schwal\u00b7be", "sel\u00b7ber", "l\u00fc\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Warum? Sie kommt allein!", "tokens": ["Wa\u00b7rum", "?", "Sie", "kommt", "al\u00b7lein", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sollt ich mich einzeln freuen,", "tokens": ["Sollt", "ich", "mich", "ein\u00b7zeln", "freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn auch der Fr\u00fchling nah?", "tokens": ["Wenn", "auch", "der", "Fr\u00fch\u00b7ling", "nah", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch kommen wir zu zweien,", "tokens": ["Doch", "kom\u00b7men", "wir", "zu", "zwei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch kommen wir zu zweien,", "tokens": ["Doch", "kom\u00b7men", "wir", "zu", "zwei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gleich ist der Sommer da.", "tokens": ["Gleich", "ist", "der", "Som\u00b7mer", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}