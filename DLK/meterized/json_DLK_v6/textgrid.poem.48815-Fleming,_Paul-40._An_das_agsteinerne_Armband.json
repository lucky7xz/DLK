{"textgrid.poem.48815": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "40. An das agsteinerne Armband", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der, so gelesen hat am Strande seiner ", "tokens": ["Der", ",", "so", "ge\u00b7le\u00b7sen", "hat", "am", "Stran\u00b7de", "sei\u00b7ner"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "ADV", "VVPP", "VAFIN", "APPRART", "NN", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "den Ursprung deiner Zier, du kostbarlicher Sand,", "tokens": ["den", "Ur\u00b7sprung", "dei\u00b7ner", "Zier", ",", "du", "kost\u00b7bar\u00b7li\u00b7cher", "Sand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "und der auch, welcher hat die Kost an dich gewandt,", "tokens": ["und", "der", "auch", ",", "wel\u00b7cher", "hat", "die", "Kost", "an", "dich", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "$,", "PWAT", "VAFIN", "ART", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "da\u00df du ein edler Schmuck der Arme soltest hei\u00dfen,", "tokens": ["da\u00df", "du", "ein", "ed\u00b7ler", "Schmuck", "der", "Ar\u00b7me", "sol\u00b7test", "hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "m\u00fc\u00df' allzeit gl\u00fcckhaft sein! Auch ich will mich beflei\u00dfen,", "tokens": ["m\u00fc\u00df'", "all\u00b7zeit", "gl\u00fcck\u00b7haft", "sein", "!", "Auch", "ich", "will", "mich", "be\u00b7flei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADJD", "VAINF", "$.", "ADV", "PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "den ungelobten ", "tokens": ["den", "un\u00b7ge\u00b7lob\u00b7ten"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "zu r\u00fchmen \u00fcberhoch und hier durch diese Hand,", "tokens": ["zu", "r\u00fch\u00b7men", "\u00fc\u00b7ber\u00b7hoch", "und", "hier", "durch", "die\u00b7se", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADV", "KON", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "die etwas Ewigs kan, der Sterblichkeit entrei\u00dfen.", "tokens": ["die", "et\u00b7was", "E\u00b7wigs", "kan", ",", "der", "Sterb\u00b7lich\u00b7keit", "ent\u00b7rei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "VMFIN", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Bist du ein Tannenharz, durchs Meersalz und Apollen", "tokens": ["Bist", "du", "ein", "Tan\u00b7nen\u00b7harz", ",", "durchs", "Meer\u00b7salz", "und", "A\u00b7pol\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "APPRART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "in die Gestalt gebracht, das viel beweisen wollen,", "tokens": ["in", "die", "Ge\u00b7stalt", "ge\u00b7bracht", ",", "das", "viel", "be\u00b7wei\u00b7sen", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,", "PRELS", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "mehr aber gl\u00e4uben nicht, ich la\u00df' es itzt darbei.", "tokens": ["mehr", "a\u00b7ber", "gl\u00e4u\u00b7ben", "nicht", ",", "ich", "la\u00df'", "es", "itzt", "dar\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PTKNEG", "$,", "PPER", "VVFIN", "PPER", "ADV", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Du seist, aus was du seist, so mu\u00df doch, wenn wir fragen,", "tokens": ["Du", "seist", ",", "aus", "was", "du", "seist", ",", "so", "mu\u00df", "doch", ",", "wenn", "wir", "fra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "APPR", "PRELS", "PPER", "VAFIN", "$,", "ADV", "VMFIN", "ADV", "$,", "KOUS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Guinee und Peru auch wider Wollen sagen,", "tokens": ["Gui\u00b7nee", "und", "Pe\u00b7ru", "auch", "wi\u00b7der", "Wol\u00b7len", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "----+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "da\u00df dein Schein g\u00fcldner noch als ihres Goldes sei.", "tokens": ["da\u00df", "dein", "Schein", "g\u00fcld\u00b7ner", "noch", "als", "ih\u00b7res", "Gol\u00b7des", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "ADV", "KOKOM", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+++-+-+-+-+", "measure": "unknown.measure.septa"}}}}}