{"dta.poem.11280": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf einer jungfer spr\u00fcchwort:  \n Ich habe nichts.  \n M. S. R.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du sprichst: Ich habe nichts! so hab ich was zu lachen;", "tokens": ["Du", "sprichst", ":", "Ich", "ha\u00b7be", "nichts", "!", "so", "hab", "ich", "was", "zu", "la\u00b7chen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VAFIN", "PIS", "$.", "ADV", "VAFIN", "PPER", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein kind! wir k\u00f6nnen schon aus diesem nichts was", "tokens": ["Mein", "kind", "!", "wir", "k\u00f6n\u00b7nen", "schon", "aus", "die\u00b7sem", "nichts", "was"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PPER", "VMFIN", "ADV", "APPR", "PDAT", "PIS", "PWS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "machen.", "tokens": ["ma\u00b7chen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Ists keine grosse nicht, so ists die kleine welt:", "tokens": ["Ists", "kei\u00b7ne", "gros\u00b7se", "nicht", ",", "so", "ists", "die", "klei\u00b7ne", "welt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ADJA", "PTKNEG", "$,", "ADV", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "La\u00df mich nur sch\u00f6pffer seyn, so lang es mir gef\u00e4llt.", "tokens": ["La\u00df", "mich", "nur", "sch\u00f6pf\u00b7fer", "seyn", ",", "so", "lang", "es", "mir", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADJD", "VAINF", "$,", "ADV", "ADJD", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}