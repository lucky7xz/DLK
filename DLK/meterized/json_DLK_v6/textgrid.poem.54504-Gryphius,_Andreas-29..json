{"textgrid.poem.54504": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "29.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer in de\u00df H\u00f6chsten Thron dem Schlo\u00df der Herrligkeit", "tokens": ["Wer", "in", "de\u00df", "H\u00f6chs\u00b7ten", "Thron", "dem", "Schlo\u00df", "der", "Herr\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wil \u00fcber alle gehn: mu\u00df hier der kleinste heissen.", "tokens": ["Wil", "\u00fc\u00b7ber", "al\u00b7le", "gehn", ":", "mu\u00df", "hier", "der", "kleins\u00b7te", "heis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PIS", "VVINF", "$.", "VMFIN", "ADV", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die sich der Demuth nicht mit gantzem Ernst befleissen:", "tokens": ["Die", "sich", "der", "De\u00b7muth", "nicht", "mit", "gant\u00b7zem", "Ernst", "be\u00b7fleis\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ART", "NN", "PTKNEG", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vert\u00e4uffen ihre Seel' in tausendfaches Leid.", "tokens": ["Ver\u00b7t\u00e4uf\u00b7fen", "ih\u00b7re", "Seel'", "in", "tau\u00b7send\u00b7fa\u00b7ches", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn schon der rotte Drach/ samt seinen Schaaren streit/", "tokens": ["Wenn", "schon", "der", "rot\u00b7te", "Drach", "/", "samt", "sei\u00b7nen", "Schaa\u00b7ren", "streit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "$(", "APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vnd wieder Gott sich lehnt: mu\u00df seine Krafft zureissen.", "tokens": ["Vnd", "wie\u00b7der", "Gott", "sich", "lehnt", ":", "mu\u00df", "sei\u00b7ne", "Krafft", "zu\u00b7reis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "PRF", "VVFIN", "$.", "VMFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Man schaut was an ihm hing/ mit von dem Himmel schmeissen/", "tokens": ["Man", "schaut", "was", "an", "ihm", "hing", "/", "mit", "von", "dem", "Him\u00b7mel", "schmeis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PIS", "APPR", "PPER", "VVFIN", "$(", "APPR", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "In Abgrund aller Angst/ auff ewig stette Zeit.", "tokens": ["In", "Ab\u00b7grund", "al\u00b7ler", "Angst", "/", "auff", "e\u00b7wig", "stet\u00b7te", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIAT", "NN", "$(", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Drumb mache dich nicht gro\u00df/ schau da\u00df du keinem schadest/", "tokens": ["Drumb", "ma\u00b7che", "dich", "nicht", "gro\u00df", "/", "schau", "da\u00df", "du", "kei\u00b7nem", "scha\u00b7dest", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKNEG", "ADJD", "$(", "ADJD", "KOUS", "PPER", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Vnd durch dein Aergern\u00fc\u00df dir frembde schuld auffladest:", "tokens": ["Vnd", "durch", "dein", "A\u00b7er\u00b7ger\u00b7n\u00fc\u00df", "dir", "fremb\u00b7de", "schuld", "auf\u00b7fla\u00b7dest", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "So wird der Engel-Hauff' vmb deine Demuth seyn", "tokens": ["So", "wird", "der", "En\u00b7gel\u00b7Hauff'", "vmb", "dei\u00b7ne", "De\u00b7muth", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VAINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der Engel die dir hier in Demuth Dinst erweisen/", "tokens": ["Der", "En\u00b7gel", "die", "dir", "hier", "in", "De\u00b7muth", "Dinst", "er\u00b7wei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "ADV", "APPR", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mensch so du wilt ins Haus der reinen Geister reisen/", "tokens": ["Mensch", "so", "du", "wilt", "ins", "Haus", "der", "rei\u00b7nen", "Geis\u00b7ter", "rei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "VMFIN", "APPRART", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.14": {"text": "So nim auch Demuth an/ vnd sey hier Engelrein.", "tokens": ["So", "nim", "auch", "De\u00b7muth", "an", "/", "vnd", "sey", "hier", "En\u00b7gel\u00b7rein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "NN", "PTKVZ", "$(", "KON", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}