{"textgrid.poem.65737": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "11", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So schnell du abbl\u00fchst, sprossest du heran", "tokens": ["So", "schnell", "du", "ab\u00b7bl\u00fchst", ",", "spros\u00b7sest", "du", "he\u00b7ran"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus dem, was dir entging, in deinen Zweigen,", "tokens": ["Aus", "dem", ",", "was", "dir", "ent\u00b7ging", ",", "in", "dei\u00b7nen", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PWS", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und was du jugendlich an Blut vertan,", "tokens": ["Und", "was", "du", "ju\u00b7gend\u00b7lich", "an", "Blut", "ver\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADJD", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das nennst du, wenn die Jugend schwand, dein eigen.", "tokens": ["Das", "nennst", "du", ",", "wenn", "die", "Ju\u00b7gend", "schwand", ",", "dein", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "VVFIN", "$,", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Hierin lebt Weisheit, Sch\u00f6nheit, Nachwuchs fort;", "tokens": ["Hie\u00b7rin", "lebt", "Weis\u00b7heit", ",", "Sch\u00f6n\u00b7heit", ",", "Nach\u00b7wuchs", "fort", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "NN", "$,", "NN", "$,", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sonst, Torheit, Alter, eisiges Gerinnen.", "tokens": ["Sonst", ",", "Tor\u00b7heit", ",", "Al\u00b7ter", ",", "ei\u00b7si\u00b7ges", "Ge\u00b7rin\u00b7nen", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "D\u00e4cht' alles so, die Zeit w\u00e4r l\u00e4ngst verdorrt,", "tokens": ["D\u00e4cht'", "al\u00b7les", "so", ",", "die", "Zeit", "w\u00e4r", "l\u00e4ngst", "ver\u00b7dorrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "$,", "ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "In sechzig Jahren diese Welt von hinnen.", "tokens": ["In", "sech\u00b7zig", "Jah\u00b7ren", "die\u00b7se", "Welt", "von", "hin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "PDAT", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "La\u00df sterben unfruchtbar, die anmutleer,", "tokens": ["La\u00df", "ster\u00b7ben", "un\u00b7frucht\u00b7bar", ",", "die", "an\u00b7mut\u00b7leer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADJD", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Rauh von Natur und w\u00fcst nicht zur Vermehrung taugen;", "tokens": ["Rauh", "von", "Na\u00b7tur", "und", "w\u00fcst", "nicht", "zur", "Ver\u00b7meh\u00b7rung", "tau\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "VVFIN", "PTKNEG", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sieh ihre Bestbegabten; dir ward mehr;", "tokens": ["Sieh", "ih\u00b7re", "Best\u00b7be\u00b7gab\u00b7ten", ";", "dir", "ward", "mehr", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "So reiche Gabe sollst du reichlich brauchen!", "tokens": ["So", "rei\u00b7che", "Ga\u00b7be", "sollst", "du", "reich\u00b7lich", "brau\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VMFIN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Natur schnitt ihren Stempel ", "tokens": ["Na\u00b7tur", "schnitt", "ih\u00b7ren", "Stem\u00b7pel"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "La\u00df ihn nicht untergehen, pr\u00e4g' ihn nach.", "tokens": ["La\u00df", "ihn", "nicht", "un\u00b7ter\u00b7ge\u00b7hen", ",", "pr\u00e4g'", "ihn", "nach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "VVINF", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}