{"textgrid.poem.37121": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen Hexametristen", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des niedern Fluges Feind, des armen Reimes Ha\u00dfer", "tokens": ["Des", "nie\u00b7dern", "Flu\u00b7ges", "Feind", ",", "des", "ar\u00b7men", "Rei\u00b7mes", "Ha\u00b7\u00dfer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Fliegt dunkel schw\u00fclstig in die H\u00f6h", "tokens": ["Fliegt", "dun\u00b7kel", "schw\u00fcls\u00b7tig", "in", "die", "H\u00f6h"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Lied. \u2013 Es schimmert wie der Schnee,", "tokens": ["Sein", "Lied", ".", "\u2013", "Es", "schim\u00b7mert", "wie", "der", "Schnee", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "$(", "PPER", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch l\u00f6se beide auf, was bleibet \u00fcbrig? \u2013 Wa\u00dfer.", "tokens": ["Doch", "l\u00f6\u00b7se", "bei\u00b7de", "auf", ",", "was", "blei\u00b7bet", "\u00fcb\u00b7rig", "?", "\u2013", "Wa\u00b7\u00dfer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PTKVZ", "$,", "PWS", "VVFIN", "PTKVZ", "$.", "$(", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}