{"textgrid.poem.33934": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Proteus", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was oben und unten in F\u00fclle und Kraft", "tokens": ["Was", "o\u00b7ben", "und", "un\u00b7ten", "in", "F\u00fcl\u00b7le", "und", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "KON", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die ewige Mutter erschuf und erschafft,", "tokens": ["Die", "e\u00b7wi\u00b7ge", "Mut\u00b7ter", "er\u00b7schuf", "und", "er\u00b7schafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Sie hat es in Formen, in steife, geh\u00fcllt,", "tokens": ["Sie", "hat", "es", "in", "For\u00b7men", ",", "in", "stei\u00b7fe", ",", "ge\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "NN", "$,", "APPR", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "In starrende Normen das Leben gef\u00fcllt.", "tokens": ["In", "star\u00b7ren\u00b7de", "Nor\u00b7men", "das", "Le\u00b7ben", "ge\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Und wie's in den Formen auch brauset und zischt,", "tokens": ["Und", "wie's", "in", "den", "For\u00b7men", "auch", "brau\u00b7set", "und", "zischt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "So bleibt es doch immer mit Erde gemischt,", "tokens": ["So", "bleibt", "es", "doch", "im\u00b7mer", "mit", "Er\u00b7de", "ge\u00b7mischt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Nie kann sich's entrei\u00dfen der dumpfen Gewalt,", "tokens": ["Nie", "kann", "sich's", "ent\u00b7rei\u00b7\u00dfen", "der", "dum\u00b7pfen", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Da wird es so tr\u00fcbe, da wird es so kalt.", "tokens": ["Da", "wird", "es", "so", "tr\u00fc\u00b7be", ",", "da", "wird", "es", "so", "kalt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJA", "$,", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Doch mich hat sie nimmer gebannt in den Ring,", "tokens": ["Doch", "mich", "hat", "sie", "nim\u00b7mer", "ge\u00b7bannt", "in", "den", "Ring", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Mit welchem sie grausam die Wesen umfing,", "tokens": ["Mit", "wel\u00b7chem", "sie", "grau\u00b7sam", "die", "We\u00b7sen", "um\u00b7fing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ich steige hinunter, ich steige empor", "tokens": ["Ich", "stei\u00b7ge", "hin\u00b7un\u00b7ter", ",", "ich", "stei\u00b7ge", "em\u00b7por"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PTKVZ"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Nach eig'nem Behagen im wirbelnden Chor.", "tokens": ["Nach", "eig'\u00b7nem", "Be\u00b7ha\u00b7gen", "im", "wir\u00b7beln\u00b7den", "Chor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Ich schl\u00fcrfe begierig aus jeglichem Sein", "tokens": ["Ich", "schl\u00fcr\u00b7fe", "be\u00b7gie\u00b7rig", "aus", "jeg\u00b7li\u00b7chem", "Sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PIAT", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Mit tiefem Entz\u00fccken den Honig hinein,", "tokens": ["Mit", "tie\u00b7fem", "Ent\u00b7z\u00fc\u00b7cken", "den", "Ho\u00b7nig", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "An keines gebunden, mu\u00df jedes mir schnell", "tokens": ["An", "kei\u00b7nes", "ge\u00b7bun\u00b7den", ",", "mu\u00df", "je\u00b7des", "mir", "schnell"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "VVPP", "$,", "VMFIN", "PIAT", "PPER", "ADJD"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die Pforten entriegeln zum innersten Quell.", "tokens": ["Die", "Pfor\u00b7ten", "ent\u00b7rie\u00b7geln", "zum", "in\u00b7ners\u00b7ten", "Quell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Ich bin's, der die Welle des Lebens bewegt,", "tokens": ["Ich", "bin's", ",", "der", "die", "Wel\u00b7le", "des", "Le\u00b7bens", "be\u00b7wegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Der ihre gewaltigste Str\u00f6mung erregt,", "tokens": ["Der", "ih\u00b7re", "ge\u00b7wal\u00b7tigs\u00b7te", "Str\u00f6\u00b7mung", "er\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und dann, was sie innerlich eigen besitzt,", "tokens": ["Und", "dann", ",", "was", "sie", "in\u00b7ner\u00b7lich", "ei\u00b7gen", "be\u00b7sitzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PRELS", "PPER", "ADJD", "ADJD", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Enteilend, in's d\u00fcrstende Weltall verspritzt.", "tokens": ["Ent\u00b7ei\u00b7lend", ",", "in's", "d\u00fcrs\u00b7ten\u00b7de", "Wel\u00b7tall", "ver\u00b7spritzt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "Ha! oben in Wolken in bl\u00e4ulichem Glanz", "tokens": ["Ha", "!", "o\u00b7ben", "in", "Wol\u00b7ken", "in", "bl\u00e4u\u00b7li\u00b7chem", "Glanz"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ADV", "APPR", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Mit brausenden St\u00fcrmen der schwindelnde Tanz!", "tokens": ["Mit", "brau\u00b7sen\u00b7den", "St\u00fcr\u00b7men", "der", "schwin\u00b7deln\u00b7de", "Tanz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Als Blitz, dies Verflammen im n\u00e4chtlichen Blau!", "tokens": ["Als", "Blitz", ",", "dies", "Ver\u00b7flam\u00b7men", "im", "n\u00e4cht\u00b7li\u00b7chen", "Blau", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PDS", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Als Regen, dies Tr\u00e4nken der durstigen Au!", "tokens": ["Als", "Re\u00b7gen", ",", "dies", "Tr\u00e4n\u00b7ken", "der", "durs\u00b7ti\u00b7gen", "Au", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PDS", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "Das stille Verschlie\u00dfen, das liebliche Ruh'n!", "tokens": ["Das", "stil\u00b7le", "Ver\u00b7schlie\u00b7\u00dfen", ",", "das", "lieb\u00b7li\u00b7che", "Ruh'n", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und wenn ich entsteige der thauigen Gruft,", "tokens": ["Und", "wenn", "ich", "ent\u00b7stei\u00b7ge", "der", "thau\u00b7i\u00b7gen", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Umstr\u00f6mt mich, entbunden, der gl\u00fchendste Duft!", "tokens": ["Um\u00b7str\u00f6mt", "mich", ",", "ent\u00b7bun\u00b7den", ",", "der", "gl\u00fc\u00b7hends\u00b7te", "Duft", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVPP", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "O seliges Wohnen in Nachtigallbrust!", "tokens": ["O", "se\u00b7li\u00b7ges", "Woh\u00b7nen", "in", "Nach\u00b7ti\u00b7gall\u00b7brust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+---+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "O s\u00fc\u00dfes Zerrinnen in heimlichster Lust!", "tokens": ["O", "s\u00fc\u00b7\u00dfes", "Zer\u00b7rin\u00b7nen", "in", "heim\u00b7lichs\u00b7ter", "Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ich hauch' ihr die Liebe in's klopfende Herz,", "tokens": ["Ich", "hauch'", "ihr", "die", "Lie\u00b7be", "in's", "klop\u00b7fen\u00b7de", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Dann scheid' ich, da singt sie in ewigem Schmerz.", "tokens": ["Dann", "scheid'", "ich", ",", "da", "singt", "sie", "in", "e\u00b7wi\u00b7gem", "Schmerz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.9": {"line.1": {"text": "In Seelen der Menschen hinein und hinaus!", "tokens": ["In", "See\u00b7len", "der", "Men\u00b7schen", "hin\u00b7ein", "und", "hin\u00b7aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Sie m\u00f6gten mich fesseln, o neckischer Strau\u00df!", "tokens": ["Sie", "m\u00f6g\u00b7ten", "mich", "fes\u00b7seln", ",", "o", "ne\u00b7cki\u00b7scher", "Strau\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "$,", "FM", "ADJA", "NE", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die fromme des Dichters nur ist's, die mich h\u00e4lt,", "tokens": ["Die", "from\u00b7me", "des", "Dich\u00b7ters", "nur", "ist's", ",", "die", "mich", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "NN", "ADV", "VAFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ihr geb' ich ein volles Empfinden der Welt.", "tokens": ["Ihr", "geb'", "ich", "ein", "vol\u00b7les", "Emp\u00b7fin\u00b7den", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}