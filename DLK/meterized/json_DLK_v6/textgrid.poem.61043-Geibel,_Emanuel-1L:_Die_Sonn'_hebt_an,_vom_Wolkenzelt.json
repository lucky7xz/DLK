{"textgrid.poem.61043": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Sonn' hebt an, vom Wolkenzelt", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sonn' hebt an, vom Wolkenzelt", "tokens": ["Die", "Sonn'", "hebt", "an", ",", "vom", "Wol\u00b7ken\u00b7zelt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verstohlnen Glanz zu schie\u00dfen;", "tokens": ["Ver\u00b7stohl\u00b7nen", "Glanz", "zu", "schie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da gibt es rings in Wald und Feld", "tokens": ["Da", "gibt", "es", "rings", "in", "Wald", "und", "Feld"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Rauschen, Rieseln, Flie\u00dfen.", "tokens": ["Ein", "Rau\u00b7schen", ",", "Rie\u00b7seln", ",", "Flie\u00b7\u00dfen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das Eis zergeht, der Schnee zerrinnt,", "tokens": ["Das", "Eis", "zer\u00b7geht", ",", "der", "Schnee", "zer\u00b7rinnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann gr\u00fcnt es \u00fcber ein Weilchen,", "tokens": ["Dann", "gr\u00fcnt", "es", "\u00fc\u00b7ber", "ein", "Weil\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und leise singt der laue Wind:", "tokens": ["Und", "lei\u00b7se", "singt", "der", "lau\u00b7e", "Wind", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wacht auf, wacht auf, ihr Veilchen!", "tokens": ["Wacht", "auf", ",", "wacht", "auf", ",", "ihr", "Veil\u00b7chen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O lindes S\u00e4useln tief im Tal!", "tokens": ["O", "lin\u00b7des", "S\u00e4u\u00b7seln", "tief", "im", "Tal", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O erster Duft des M\u00e4rzen!", "tokens": ["O", "ers\u00b7ter", "Duft", "des", "M\u00e4r\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nun bl\u00fcht und klingt die Welt zumal,", "tokens": ["Nun", "bl\u00fcht", "und", "klingt", "die", "Welt", "zu\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nun klingt's auch mir im Herzen.", "tokens": ["Nun", "klingt's", "auch", "mir", "im", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und wie die L\u00fcfte wundervoll", "tokens": ["Und", "wie", "die", "L\u00fcf\u00b7te", "wun\u00b7der\u00b7voll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich blau und blauer dehnen -", "tokens": ["Sich", "blau", "und", "blau\u00b7er", "deh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADJD", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich wei\u00df nicht, was das werden soll,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "was", "das", "wer\u00b7den", "soll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PDS", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was will dies Ringen und Sehnen?", "tokens": ["Was", "will", "dies", "Rin\u00b7gen", "und", "Seh\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PDS", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Mir wird die Brust so weit, so weit,", "tokens": ["Mir", "wird", "die", "Brust", "so", "weit", ",", "so", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als ob's drin bl\u00fcht' und triebe -", "tokens": ["Als", "ob's", "drin", "bl\u00fcht'", "und", "trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "ADV", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kommst du noch einmal, Jugendzeit?", "tokens": ["Kommst", "du", "noch", "ein\u00b7mal", ",", "Ju\u00b7gend\u00b7zeit", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kommst du noch einmal, Liebe?", "tokens": ["Kommst", "du", "noch", "ein\u00b7mal", ",", "Lie\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}