{"textgrid.poem.41133": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dort unten in der M\u00fchle", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dort unten in der M\u00fchle", "tokens": ["Dort", "un\u00b7ten", "in", "der", "M\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sa\u00df ich in s\u00fc\u00dfer Ruh'", "tokens": ["Sa\u00df", "ich", "in", "s\u00fc\u00b7\u00dfer", "Ruh'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sah dem R\u00e4derspiele", "tokens": ["Und", "sah", "dem", "R\u00e4\u00b7der\u00b7spie\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und sah den Wassern zu.", "tokens": ["Und", "sah", "den", "Was\u00b7sern", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sah zu der blanken S\u00e4ge,", "tokens": ["Sah", "zu", "der", "blan\u00b7ken", "S\u00e4\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es war mir wie ein Traum,", "tokens": ["Es", "war", "mir", "wie", "ein", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die bahnte lange Wege", "tokens": ["Die", "bahn\u00b7te", "lan\u00b7ge", "We\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In einen Tannenbaum.", "tokens": ["In", "ei\u00b7nen", "Tan\u00b7nen\u00b7baum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Tanne war wie lebend,", "tokens": ["Die", "Tan\u00b7ne", "war", "wie", "le\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Trauermelodie", "tokens": ["In", "Trau\u00b7er\u00b7me\u00b7lo\u00b7die"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch alle Fasern bebend", "tokens": ["Durch", "al\u00b7le", "Fa\u00b7sern", "be\u00b7bend"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sang diese Worte sie:", "tokens": ["Sang", "die\u00b7se", "Wor\u00b7te", "sie", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Du kehrst zur rechten Stunde,", "tokens": ["Du", "kehrst", "zur", "rech\u00b7ten", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O Wanderer, hier ein,", "tokens": ["O", "Wan\u00b7de\u00b7rer", ",", "hier", "ein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du bist's, f\u00fcr den die Wunde", "tokens": ["Du", "bist's", ",", "f\u00fcr", "den", "die", "Wun\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NE", "$,", "APPR", "ART", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir dringt ins Herz hinein!", "tokens": ["Mir", "dringt", "ins", "Herz", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du bist's, f\u00fcr den wird werden,", "tokens": ["Du", "bist's", ",", "f\u00fcr", "den", "wird", "wer\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "$,", "APPR", "ART", "VAFIN", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn kurz gewandert du,", "tokens": ["Wenn", "kurz", "ge\u00b7wan\u00b7dert", "du", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VVPP", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dies Holz im Scho\u00df der Erden", "tokens": ["Dies", "Holz", "im", "Scho\u00df", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Schrein zur langen Ruh'.", "tokens": ["Ein", "Schrein", "zur", "lan\u00b7gen", "Ruh'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Vier Bretter sah ich fallen,", "tokens": ["Vier", "Bret\u00b7ter", "sah", "ich", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir ward's ums Herze schwer,", "tokens": ["Mir", "ward's", "ums", "Her\u00b7ze", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein W\u00f6rtlein wollt' ich lallen,", "tokens": ["Ein", "W\u00f6rt\u00b7lein", "wollt'", "ich", "lal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da ging das Rad nicht mehr.", "tokens": ["Da", "ging", "das", "Rad", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}