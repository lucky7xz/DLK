{"dta.poem.12987": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "* *", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was suchest du, o mensch! mit der vernunfft zu prangen?", "tokens": ["Was", "su\u00b7chest", "du", ",", "o", "mensch", "!", "mit", "der", "ver\u00b7nunfft", "zu", "pran\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "FM", "FM", "$.", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich wei\u00df, man nennet sie das allerbeste guth.", "tokens": ["Ich", "wei\u00df", ",", "man", "nen\u00b7net", "sie", "das", "al\u00b7ler\u00b7bes\u00b7te", "guth", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PIS", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich aber schaue nicht, was sie uns gutes thut.", "tokens": ["Ich", "a\u00b7ber", "schau\u00b7e", "nicht", ",", "was", "sie", "uns", "gu\u00b7tes", "thut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PTKNEG", "$,", "PRELS", "PPER", "PPER", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn wir, als kinder, noch an mutter-br\u00fcsten hangen,", "tokens": ["Wenn", "wir", ",", "als", "kin\u00b7der", ",", "noch", "an", "mut\u00b7ter\u00b7br\u00fcs\u00b7ten", "han\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "NN", "$,", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So ist sie selbst ein kind. Kommt denn die j\u00fcnglingschafft,", "tokens": ["So", "ist", "sie", "selbst", "ein", "kind", ".", "Kommt", "denn", "die", "j\u00fcng\u00b7ling\u00b7schafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "$.", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So tantzet die vernunfft nach schn\u00f6der l\u00fcste willen.", "tokens": ["So", "tant\u00b7zet", "die", "ver\u00b7nunfft", "nach", "schn\u00f6\u00b7der", "l\u00fcs\u00b7te", "wil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Jm alter plaget sie den kopff mit sorg und grillen,", "tokens": ["Jm", "al\u00b7ter", "pla\u00b7get", "sie", "den", "kopff", "mit", "sorg", "und", "gril\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und endlich wird sie noch vor uns hinweggerafft.", "tokens": ["Und", "end\u00b7lich", "wird", "sie", "noch", "vor", "uns", "hin\u00b7weg\u00b7ge\u00b7rafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}