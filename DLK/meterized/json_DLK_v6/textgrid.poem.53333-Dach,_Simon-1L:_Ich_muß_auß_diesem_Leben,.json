{"textgrid.poem.53333": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich mu\u00df au\u00df diesem Leben,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich mu\u00df au\u00df diesem Leben,", "tokens": ["Ich", "mu\u00df", "au\u00df", "die\u00b7sem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Di\u00df ist Gesetz vnd Pflicht,", "tokens": ["Di\u00df", "ist", "Ge\u00b7setz", "vnd", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich mag gleich wiederstreben,", "tokens": ["Ich", "mag", "gleich", "wie\u00b7der\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mag wollen oder nicht,", "tokens": ["Mag", "wol\u00b7len", "o\u00b7der", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VMFIN", "KON", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Drumb nim mich, Jesu, doch davon", "tokens": ["Drumb", "nim", "mich", ",", "Je\u00b7su", ",", "doch", "da\u00b7von"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "$,", "NE", "$,", "ADV", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Fried als deinen Simeon.", "tokens": ["In", "Fried", "als", "dei\u00b7nen", "Si\u00b7me\u00b7on", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KOKOM", "PPOSAT", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Auch ich hab', Herr, gesehen", "tokens": ["Auch", "ich", "hab'", ",", "Herr", ",", "ge\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "PPER", "VAFIN", "$,", "NN", "$,", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dich aller Menschen Heil,", "tokens": ["Dich", "al\u00b7ler", "Men\u00b7schen", "Heil", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Rettung, so geschehen", "tokens": ["Die", "Ret\u00b7tung", ",", "so", "ge\u00b7sche\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch dich, ist nur mein Theil,", "tokens": ["Durch", "dich", ",", "ist", "nur", "mein", "Theil", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.5": {"text": "Ich trag auff meiner Glaubens-Hand", "tokens": ["Ich", "trag", "auff", "mei\u00b7ner", "Glau\u00b7bens\u00b7Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dich meiner Seelen h\u00f6chstes pfand.", "tokens": ["Dich", "mei\u00b7ner", "See\u00b7len", "h\u00f6chs\u00b7tes", "pfand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du bist mein Weg-Bereiter,", "tokens": ["Du", "bist", "mein", "Weg\u00b7Be\u00b7rei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Durchzug, meine Bahn,", "tokens": ["Mein", "Durch\u00b7zug", ",", "mei\u00b7ne", "Bahn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Himmels Th\u00fcr vnd Leiter,", "tokens": ["Des", "Him\u00b7mels", "Th\u00fcr", "vnd", "Lei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den du mir auffgethan,", "tokens": ["Den", "du", "mir", "auff\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der durchbruch wird mir nun nicht schwer,", "tokens": ["Der", "durch\u00b7bruch", "wird", "mir", "nun", "nicht", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weil du, Gott, durchbrichst f\u00fcr mir her.", "tokens": ["Weil", "du", ",", "Gott", ",", "durch\u00b7brichst", "f\u00fcr", "mir", "her", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "NN", "$,", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Jetzt sitzest du zur Rechten", "tokens": ["Jetzt", "sit\u00b7zest", "du", "zur", "Rech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Gottes-Krafft gestellt,", "tokens": ["Der", "Got\u00b7tes\u00b7Krafft", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd hast in deinen N\u00e4chten", "tokens": ["Vnd", "hast", "in", "dei\u00b7nen", "N\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "S\u00fcnd', Helle, Tod vnd Welt,", "tokens": ["S\u00fcnd'", ",", "Hel\u00b7le", ",", "Tod", "vnd", "Welt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was Himmel, Lufft vnd Welt erh\u00f6ht", "tokens": ["Was", "Him\u00b7mel", ",", "Lufft", "vnd", "Welt", "er\u00b7h\u00f6ht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "NN", "$,", "NN", "KON", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dient deiner hohen Majest\u00e4t.", "tokens": ["Dient", "dei\u00b7ner", "ho\u00b7hen", "Ma\u00b7jes\u00b7t\u00e4t", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dir wird stets lob gesungen", "tokens": ["Dir", "wird", "stets", "lob", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von aller Engel-Schaar,", "tokens": ["Von", "al\u00b7ler", "En\u00b7gel\u00b7Schaar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es r\u00fchmen dich die Zungen", "tokens": ["Es", "r\u00fch\u00b7men", "dich", "die", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der V\u00e4ter immerdar,", "tokens": ["Der", "V\u00e4\u00b7ter", "im\u00b7mer\u00b7dar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vmb dich wohnt Ehre, Danck vnd Prei\u00df", "tokens": ["Vmb", "dich", "wohnt", "Eh\u00b7re", ",", "Danck", "vnd", "Prei\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUI", "PPER", "VVFIN", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd Frewde die kein Ende wei\u00df.", "tokens": ["Vnd", "Frew\u00b7de", "die", "kein", "En\u00b7de", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "La\u00df mich dahin gelangen,", "tokens": ["La\u00df", "mich", "da\u00b7hin", "ge\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PAV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mach mich von allem frey", "tokens": ["Mach", "mich", "von", "al\u00b7lem", "frey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "PIS", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was mich hie h\u00e4lt gefangen,", "tokens": ["Was", "mich", "hie", "h\u00e4lt", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auff da\u00df ich bey dir sey", "tokens": ["Auff", "da\u00df", "ich", "bey", "dir", "sey"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "APPR", "PPER", "VAFIN"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.5": {"text": "Vnd lobe dich, in dir erfrewt,", "tokens": ["Vnd", "lo\u00b7be", "dich", ",", "in", "dir", "er\u00b7frewt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In alle ewig' Ewigkeit.", "tokens": ["In", "al\u00b7le", "e\u00b7wig'", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}