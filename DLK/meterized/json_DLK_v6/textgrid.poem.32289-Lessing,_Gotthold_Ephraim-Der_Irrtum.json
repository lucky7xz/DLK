{"textgrid.poem.32289": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Der Irrtum", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Hund im Arm, mit blo\u00dfen Br\u00fcsten,", "tokens": ["Den", "Hund", "im", "Arm", ",", "mit", "blo\u00b7\u00dfen", "Br\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sah Lotte frech herab.", "tokens": ["Sah", "Lot\u00b7te", "frech", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADJD", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie mancher lie\u00df sichs nicht gel\u00fcsten,", "tokens": ["Wie", "man\u00b7cher", "lie\u00df", "sichs", "nicht", "ge\u00b7l\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "PIS", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df er ihr Blicke gab.", "tokens": ["Da\u00df", "er", "ihr", "Bli\u00b7cke", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich kam gedankenvoll gegangen", "tokens": ["Ich", "kam", "ge\u00b7dan\u00b7ken\u00b7voll", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und sahe steif heran.", "tokens": ["Und", "sa\u00b7he", "steif", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ha! denkt sie, der ist auch gefangen,", "tokens": ["Ha", "!", "denkt", "sie", ",", "der", "ist", "auch", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "$,", "PRELS", "VAFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und lacht mich schalkhaft an.", "tokens": ["Und", "lacht", "mich", "schalk\u00b7haft", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Allein, gesagt zur guten Stunde,", "tokens": ["Al\u00b7lein", ",", "ge\u00b7sagt", "zur", "gu\u00b7ten", "Stun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Jungfer irrt sich hier.", "tokens": ["Die", "Jung\u00b7fer", "irrt", "sich", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich sah nach ihrem bunten Hunde:", "tokens": ["Ich", "sah", "nach", "ih\u00b7rem", "bun\u00b7ten", "Hun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es ist ein artig Tier.", "tokens": ["Es", "ist", "ein", "ar\u00b7tig", "Tier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}