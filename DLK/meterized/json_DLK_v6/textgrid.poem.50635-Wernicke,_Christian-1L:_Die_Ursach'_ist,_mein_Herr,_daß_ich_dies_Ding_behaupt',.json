{"textgrid.poem.50635": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Ursach' ist, mein Herr, da\u00df ich dies Ding behaupt',", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Ursach' ist, mein Herr, da\u00df ich dies Ding behaupt',", "tokens": ["Die", "Ur\u00b7sach'", "ist", ",", "mein", "Herr", ",", "da\u00df", "ich", "dies", "Ding", "be\u00b7haupt'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PDS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dieweil die \u2013 \u2013 \u2013 \u00bbO, das wird von Niemand nicht geglaubt!\u00ab \u2013", "tokens": ["Die\u00b7weil", "die", "\u2013", "\u2013", "\u2013", "\u00bb", "O", ",", "das", "wird", "von", "Nie\u00b7mand", "nicht", "ge\u00b7glaubt", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "punct", "punct", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KOUS", "ART", "$(", "$(", "$(", "$(", "ITJ", "$,", "PDS", "VAFIN", "APPR", "PIS", "PTKNEG", "VVPP", "$.", "$(", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie? Eh' er mich geh\u00f6rt? Und darauf hebt er an", "tokens": ["Wie", "?", "Eh'", "er", "mich", "ge\u00b7h\u00f6rt", "?", "Und", "da\u00b7rauf", "hebt", "er", "an"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "KOUS", "PPER", "PRF", "VVFIN", "$.", "KON", "PAV", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und widerlegt ein Ding, davon ich nie getr\u00e4umt,", "tokens": ["Und", "wi\u00b7der\u00b7legt", "ein", "Ding", ",", "da\u00b7von", "ich", "nie", "ge\u00b7tr\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PAV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und das so wenig er, wie ich, verstehen kann.", "tokens": ["Und", "das", "so", "we\u00b7nig", "er", ",", "wie", "ich", ",", "ver\u00b7ste\u00b7hen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "PIS", "PPER", "$,", "PWAV", "PPER", "$,", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Er redet, da\u00df der Mund ihm sch\u00e4umt;", "tokens": ["Er", "re\u00b7det", ",", "da\u00df", "der", "Mund", "ihm", "sch\u00e4umt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein ungeduldig Wort, das l\u00e4uft die andern \u00fcber", "tokens": ["Ein", "un\u00b7ge\u00b7dul\u00b7dig", "Wort", ",", "das", "l\u00e4uft", "die", "an\u00b7dern", "\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "$,", "PDS", "VVFIN", "ART", "ADJA", "APPR"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und setzet den Verstand fast in ein hitzig Fieber.", "tokens": ["Und", "set\u00b7zet", "den", "Ver\u00b7stand", "fast", "in", "ein", "hit\u00b7zig", "Fie\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich segne mich und merke wohl,", "tokens": ["Ich", "seg\u00b7ne", "mich", "und", "mer\u00b7ke", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Da\u00df ich der Thorheit sehr verd\u00e4chtig mich gemacht,", "tokens": ["Da\u00df", "ich", "der", "Thor\u00b7heit", "sehr", "ver\u00b7d\u00e4ch\u00b7tig", "mich", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Weil ich vermessentlich gedacht,", "tokens": ["Weil", "ich", "ver\u00b7mes\u00b7sent\u00b7lich", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}