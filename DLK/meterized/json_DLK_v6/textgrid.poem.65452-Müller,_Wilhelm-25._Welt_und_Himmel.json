{"textgrid.poem.65452": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "25. Welt und Himmel", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem Meer mu\u00df ich die Welt vergleichen, der Himmel giebt als Strand sich kund.", "tokens": ["Dem", "Meer", "mu\u00df", "ich", "die", "Welt", "ver\u00b7glei\u00b7chen", ",", "der", "Him\u00b7mel", "giebt", "als", "Strand", "sich", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,", "ART", "NN", "VVFIN", "KOUS", "NN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Dort schwimmt die leere Muschel oben, die Perlenschnecke liegt im Grund.", "tokens": ["Dort", "schwimmt", "die", "lee\u00b7re", "Mu\u00b7schel", "o\u00b7ben", ",", "die", "Per\u00b7len\u00b7schne\u00b7cke", "liegt", "im", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADV", "$,", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Doch wenn des Todes Woge beide zusammen an das Ufer tr\u00e4gt,", "tokens": ["Doch", "wenn", "des", "To\u00b7des", "Wo\u00b7ge", "bei\u00b7de", "zu\u00b7sam\u00b7men", "an", "das", "U\u00b7fer", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "PIS", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Wird jene schm\u00e4hlich weggeworfen, die andr' als Kleinod eingelegt.", "tokens": ["Wird", "je\u00b7ne", "schm\u00e4h\u00b7lich", "weg\u00b7ge\u00b7wor\u00b7fen", ",", "die", "andr'", "als", "Klei\u00b7nod", "ein\u00b7ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADJD", "VVPP", "$,", "PRELS", "PIS", "KOKOM", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}}}}