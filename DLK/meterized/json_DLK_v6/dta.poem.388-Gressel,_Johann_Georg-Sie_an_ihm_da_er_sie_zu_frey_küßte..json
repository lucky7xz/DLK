{"dta.poem.388": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Sie an ihm da er sie zu frey k\u00fc\u00dfte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht zu dreiste denn die Br\u00fcste sind der Keuschheit noch", "tokens": ["Nicht", "zu", "dreis\u00b7te", "denn", "die", "Br\u00fcs\u00b7te", "sind", "der", "Keuschheit", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "PTKZU", "VVFIN", "ADV", "ART", "NN", "VAFIN", "ART", "NN", "ADV"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "geweyht/", "tokens": ["ge\u00b7weyht", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Diese z\u00fcrnet wenn ich euch lasse diese Gegend k\u00fcssen", "tokens": ["Die\u00b7se", "z\u00fcr\u00b7net", "wenn", "ich", "euch", "las\u00b7se", "die\u00b7se", "Ge\u00b7gend", "k\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KOUS", "PPER", "PPER", "VVFIN", "PDAT", "NN", "VVINF"], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.4": {"text": "Sie bewahret diese Gegend mit der gr\u00f6\u00dften Emsigkeit/", "tokens": ["Sie", "be\u00b7wah\u00b7ret", "die\u00b7se", "Ge\u00b7gend", "mit", "der", "gr\u00f6\u00df\u00b7ten", "Em\u00b7sig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Und will keinen fremden Gast in den zarten Garten wissen.", "tokens": ["Und", "will", "kei\u00b7nen", "frem\u00b7den", "Gast", "in", "den", "zar\u00b7ten", "Gar\u00b7ten", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIAT", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}