{"dta.poem.19406": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "139.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Mann zu werden, ist des Kindes Stolz; ein Mann", "tokens": ["Ein", "Mann", "zu", "wer\u00b7den", ",", "ist", "des", "Kin\u00b7des", "Stolz", ";", "ein", "Mann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VAINF", "$,", "VAFIN", "ART", "NN", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bedauert wol, da\u00df er kein Kind mehr werden kann.", "tokens": ["Be\u00b7dau\u00b7ert", "wol", ",", "da\u00df", "er", "kein", "Kind", "mehr", "wer\u00b7den", "kann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "PPER", "PIAT", "NN", "ADV", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wollt' er ein Kind seyn, um sich kindisch zu geberden?", "tokens": ["Wollt'", "er", "ein", "Kind", "seyn", ",", "um", "sich", "kin\u00b7disch", "zu", "ge\u00b7ber\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VAINF", "$,", "KOUI", "PRF", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O nein, als Kind m\u00f6cht' er zu anderm Manne werden.", "tokens": ["O", "nein", ",", "als", "Kind", "m\u00f6cht'", "er", "zu", "an\u00b7derm", "Man\u00b7ne", "wer\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$,", "KOUS", "NN", "VMFIN", "PPER", "APPR", "PIS", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein Vater ist begl\u00fcckt, da\u00df er ein andrer Mann,", "tokens": ["Ein", "Va\u00b7ter", "ist", "be\u00b7gl\u00fcckt", ",", "da\u00df", "er", "ein", "an\u00b7drer", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "KOUS", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als er geworden ist, im Kinde werden kann.", "tokens": ["Als", "er", "ge\u00b7wor\u00b7den", "ist", ",", "im", "Kin\u00b7de", "wer\u00b7den", "kann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAPP", "VAFIN", "$,", "APPRART", "NN", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Mit aller Einsicht, die Erfahrung ihm verliehn,", "tokens": ["Mit", "al\u00b7ler", "Ein\u00b7sicht", ",", "die", "Er\u00b7fah\u00b7rung", "ihm", "ver\u00b7liehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Streb' er sich selbst im Kind zum Manne zu erziehn.", "tokens": ["Streb'", "er", "sich", "selbst", "im", "Kind", "zum", "Man\u00b7ne", "zu", "er\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "APPRART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}