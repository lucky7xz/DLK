{"dta.poem.120": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An  Amarianen  um ein  Affections- Band.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Schatz/ mein Engel-Kind mein eintziges Vergn\u00fcgen", "tokens": ["Mein", "Schatz", "/", "mein", "En\u00b7gel\u00b7Kind", "mein", "eint\u00b7zi\u00b7ges", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erblicke deinen Knecht zu deinen F\u00fcssen liegen/", "tokens": ["Er\u00b7bli\u00b7cke", "dei\u00b7nen", "Knecht", "zu", "dei\u00b7nen", "F\u00fcs\u00b7sen", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der da von deiner Hand ein kleines Band begehrt/", "tokens": ["Der", "da", "von", "dei\u00b7ner", "Hand", "ein", "klei\u00b7nes", "Band", "be\u00b7gehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das alle Furcht vertreibt/ und seine Hoffnung nehrt.", "tokens": ["Das", "al\u00b7le", "Furcht", "ver\u00b7treibt", "/", "und", "sei\u00b7ne", "Hoff\u00b7nung", "nehrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$(", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Hoffnung die mich sonst aus leeren Schaalen spei\u00dfte/", "tokens": ["Die", "Hoff\u00b7nung", "die", "mich", "sonst", "aus", "lee\u00b7ren", "Schaa\u00b7len", "spei\u00df\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ob gleich ein holder Blick nach deinen Knecht verrei\u00dfte/", "tokens": ["Ob", "gleich", "ein", "hol\u00b7der", "Blick", "nach", "dei\u00b7nen", "Knecht", "ver\u00b7rei\u00df\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Weil du den Gnaden-Bund mit mir nicht auffgericht/", "tokens": ["Weil", "du", "den", "Gna\u00b7den\u00b7Bund", "mit", "mir", "nicht", "auff\u00b7ge\u00b7richt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPER", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Noch durch ein Zeichen mich zu deinem Dienst verpflicht.", "tokens": ["Noch", "durch", "ein", "Zei\u00b7chen", "mich", "zu", "dei\u00b7nem", "Dienst", "ver\u00b7pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wie dort der Regen-Bog dem Noah Zeugni\u00df gabe", "tokens": ["Wie", "dort", "der", "Re\u00b7gen\u00b7Bog", "dem", "Noah", "Zeug\u00b7ni\u00df", "ga\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "ART", "NE", "NN", "VVFIN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Da\u00df seine Gnaden-Zeit den gr\u00f6\u00dften Wachsthum habe/", "tokens": ["Da\u00df", "sei\u00b7ne", "Gna\u00b7den\u00b7Zeit", "den", "gr\u00f6\u00df\u00b7ten", "Wach\u00b7sthum", "ha\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So mag ein Band bey mir das Gnaden-Zeichen seyn", "tokens": ["So", "mag", "ein", "Band", "bey", "mir", "das", "Gna\u00b7den\u00b7Zei\u00b7chen", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "APPR", "PPER", "ART", "NN", "VAINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df nach vergagner Nacht der heitre Sonnen-Schein", "tokens": ["Da\u00df", "nach", "ver\u00b7gag\u00b7ner", "Nacht", "der", "heit\u00b7re", "Son\u00b7nen\u00b7Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mit dem Erquickungs-Strahl nach deinen Knechte zielet/", "tokens": ["Mit", "dem", "Er\u00b7qui\u00b7ckungs\u00b7Strahl", "nach", "dei\u00b7nen", "Knech\u00b7te", "zie\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wodurch er neue Krafft in seiner Seelen f\u00fchlet.", "tokens": ["Wo\u00b7durch", "er", "neu\u00b7e", "Krafft", "in", "sei\u00b7ner", "See\u00b7len", "f\u00fch\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Kein s\u00fcsser Morgen-Thau so sehr das Feld erquickt", "tokens": ["Kein", "s\u00fcs\u00b7ser", "Mor\u00b7gen\u00b7\u00b7T\u00b7hau", "so", "sehr", "das", "Feld", "er\u00b7quickt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "ADV", "ADV", "ART", "NN", "VVPP"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Als mich ein Gnaden-Band von deiner Hand begl\u00fcckt.", "tokens": ["Als", "mich", "ein", "Gna\u00b7den\u00b7Band", "von", "dei\u00b7ner", "Hand", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Kein Balsam kan so bald das matte Hertz beleben", "tokens": ["Kein", "Bal\u00b7sam", "kan", "so", "bald", "das", "mat\u00b7te", "Hertz", "be\u00b7le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VMFIN", "ADV", "ADV", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Als mir dein B\u00e4ndgen wird ein neues Leben geben.", "tokens": ["Als", "mir", "dein", "B\u00e4nd\u00b7gen", "wird", "ein", "neu\u00b7es", "Le\u00b7ben", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ach neige doch dein Ohr zu deines Knechtes Flehn/", "tokens": ["Ach", "nei\u00b7ge", "doch", "dein", "Ohr", "zu", "dei\u00b7nes", "Knech\u00b7tes", "Flehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "La\u00df ihn nicht unbeschenckt von deinen F\u00fcssen gehn.", "tokens": ["La\u00df", "ihn", "nicht", "un\u00b7be\u00b7schenckt", "von", "dei\u00b7nen", "F\u00fcs\u00b7sen", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Verkn\u00fcpffe durch ein Band dein Wollen und mein Hertze/", "tokens": ["Ver\u00b7kn\u00fcpf\u00b7fe", "durch", "ein", "Band", "dein", "Wol\u00b7len", "und", "mein", "Hert\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PPOSAT", "NN", "KON", "PPOSAT", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So stillt sich meine Furcht/ so leget sich mein Schmertze/", "tokens": ["So", "stillt", "sich", "mei\u00b7ne", "Furcht", "/", "so", "le\u00b7get", "sich", "mein", "Schmert\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "$(", "ADV", "VVFIN", "PRF", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So liebe ich vergn\u00fcgt/ so sage ich mit recht/", "tokens": ["So", "lie\u00b7be", "ich", "ver\u00b7gn\u00fcgt", "/", "so", "sa\u00b7ge", "ich", "mit", "recht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "$(", "ADV", "VVFIN", "PPER", "APPR", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ich heisse Lebens lang ", "tokens": ["Ich", "heis\u00b7se", "Le\u00b7bens", "lang"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}