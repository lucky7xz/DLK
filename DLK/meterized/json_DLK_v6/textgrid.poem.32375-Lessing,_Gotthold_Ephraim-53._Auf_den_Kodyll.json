{"textgrid.poem.32375": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "53. Auf den Kodyll", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der kindische Kodyll wird keiner Steigrung satt,", "tokens": ["Der", "kin\u00b7di\u00b7sche", "Ko\u00b7dyll", "wird", "kei\u00b7ner", "Stei\u00b7grung", "satt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "L\u00e4\u00dft keinen Kr\u00e4mer laufen,", "tokens": ["L\u00e4\u00dft", "kei\u00b7nen", "Kr\u00e4\u00b7mer", "lau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kauft alles, was er sieht: um alles, was er hat,", "tokens": ["Kauft", "al\u00b7les", ",", "was", "er", "sieht", ":", "um", "al\u00b7les", ",", "was", "er", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PWS", "PPER", "VVFIN", "$.", "APPR", "PIS", "$,", "PWS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bald wieder zu verkaufen.", "tokens": ["Bald", "wie\u00b7der", "zu", "ver\u00b7kau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}