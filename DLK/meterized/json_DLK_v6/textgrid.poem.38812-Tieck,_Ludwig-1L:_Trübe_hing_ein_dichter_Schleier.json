{"textgrid.poem.38812": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Tr\u00fcbe hing ein dichter Schleier", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tr\u00fcbe hing ein dichter Schleier", "tokens": ["Tr\u00fc\u00b7be", "hing", "ein", "dich\u00b7ter", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber Busch und Wald daher.", "tokens": ["Ue\u00b7ber", "Busch", "und", "Wald", "da\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sagt, wo ist die Fr\u00fchlingsfeier?", "tokens": ["Sagt", ",", "wo", "ist", "die", "Fr\u00fch\u00b7lings\u00b7fei\u00b7er", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist der Wald an T\u00f6nen leer?", "tokens": ["Ist", "der", "Wald", "an", "T\u00f6\u00b7nen", "leer", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "R\u00fchrt kein Wind sich in den Zweigen,", "tokens": ["R\u00fchrt", "kein", "Wind", "sich", "in", "den", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Treibt die Wolken \u00fcber's Feld? \u2013", "tokens": ["Treibt", "die", "Wol\u00b7ken", "\u00fc\u00b7ber's", "Feld", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dumpfes, \u00f6des, todtes Schweigen,", "tokens": ["Dum\u00b7pfes", ",", "\u00f6\u00b7des", ",", "tod\u00b7tes", "Schwei\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die Natur gefangen h\u00e4lt. \u2013", "tokens": ["Die", "Na\u00b7tur", "ge\u00b7fan\u00b7gen", "h\u00e4lt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und mir ward im Busen bange,", "tokens": ["Und", "mir", "ward", "im", "Bu\u00b7sen", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPRART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn kein Stimmlein sprach mich an,", "tokens": ["Denn", "kein", "Stimm\u00b7lein", "sprach", "mich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seufzte tief und harrte lange,", "tokens": ["Seufz\u00b7te", "tief", "und", "harr\u00b7te", "lan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Klagte: Sonne, komm heran!", "tokens": ["Klag\u00b7te", ":", "Son\u00b7ne", ",", "komm", "he\u00b7ran", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$.", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber dichter ward der Schatten,", "tokens": ["A\u00b7ber", "dich\u00b7ter", "ward", "der", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wolken hingen tiefer ab,", "tokens": ["Wol\u00b7ken", "hin\u00b7gen", "tie\u00b7fer", "ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dunkler schw\u00e4rzten sich die Matten,", "tokens": ["Dunk\u00b7ler", "schw\u00e4rz\u00b7ten", "sich", "die", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles Feld ein enges Grab.", "tokens": ["Al\u00b7les", "Feld", "ein", "en\u00b7ges", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Durch den Nebel warf ich Blicke", "tokens": ["Durch", "den", "Ne\u00b7bel", "warf", "ich", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie man in die Ferne schaut,", "tokens": ["Wie", "man", "in", "die", "Fer\u00b7ne", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle kamen mir zur\u00fccke,", "tokens": ["Al\u00b7le", "ka\u00b7men", "mir", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Finsterni\u00df war vorgebaut.", "tokens": ["Fins\u00b7ter\u00b7ni\u00df", "war", "vor\u00b7ge\u00b7baut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Da warf ich mich weinend nieder,", "tokens": ["Da", "warf", "ich", "mich", "wei\u00b7nend", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "PTKVZ", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "W\u00fcnscht' im Unmuth todt zu seyn:", "tokens": ["W\u00fcnscht'", "im", "Un\u00b7muth", "todt", "zu", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ADJD", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Todt sind alle Lerchenlieder,", "tokens": ["Todt", "sind", "al\u00b7le", "Ler\u00b7chen\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Abgestorben Sonnenschein. \u2013", "tokens": ["Ab\u00b7ge\u00b7stor\u00b7ben", "Son\u00b7nen\u00b7schein", ".", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Warum soll denn ich noch leben", "tokens": ["Wa\u00b7rum", "soll", "denn", "ich", "noch", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "KON", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der w\u00fcsten Dunkelheit,", "tokens": ["In", "der", "w\u00fcs\u00b7ten", "Dun\u00b7kel\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hier wo Schrecken um mich weben,", "tokens": ["Hier", "wo", "Schre\u00b7cken", "um", "mich", "we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In mir selber Angst und Leid? \u2013", "tokens": ["In", "mir", "sel\u00b7ber", "Angst", "und", "Leid", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "ADV", "NN", "KON", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Pl\u00f6tzlich war's, wie wenn an Saiten,", "tokens": ["Pl\u00f6tz\u00b7lich", "wa\u00b7r's", ",", "wie", "wenn", "an", "Sai\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "KOKOM", "KOUS", "APPR", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Abendwind vor\u00fcberschwebt", "tokens": ["Ab\u00b7end\u00b7wind", "vor\u00b7\u00fc\u00b7bersc\u00b7hwebt"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und in Harfent\u00f6nen webt,", "tokens": ["Und", "in", "Har\u00b7fen\u00b7t\u00f6\u00b7nen", "webt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueber Blumen hinzuschreiten,", "tokens": ["Ue\u00b7ber", "Blu\u00b7men", "hin\u00b7zu\u00b7schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "An der fernsten fernsten Gr\u00e4nze", "tokens": ["An", "der", "ferns\u00b7ten", "ferns\u00b7ten", "Gr\u00e4n\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Theilte sich die dunkle Nacht,", "tokens": ["Theil\u00b7te", "sich", "die", "dunk\u00b7le", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein Sonnenblick voll Pracht", "tokens": ["Und", "ein", "Son\u00b7nen\u00b7blick", "voll", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wand sich durch die Nebelkr\u00e4nze.", "tokens": ["Wand", "sich", "durch", "die", "Ne\u00b7bel\u00b7kr\u00e4n\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Als ich kaum zu athmen wagte,", "tokens": ["Als", "ich", "kaum", "zu", "ath\u00b7men", "wag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Scho\u00df der Strahl, ein goldner Pfeil,", "tokens": ["Scho\u00df", "der", "Strahl", ",", "ein", "gold\u00b7ner", "Pfeil", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schnell in gl\u00fchendrother Eil", "tokens": ["Schnell", "in", "gl\u00fc\u00b7hen\u00b7dro\u00b7ther", "Eil"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hin zum Orte, wo ich klagte.", "tokens": ["Hin", "zum", "Or\u00b7te", ",", "wo", "ich", "klag\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Schreckenfroh sah ich den Schein,", "tokens": ["Schre\u00b7cken\u00b7froh", "sah", "ich", "den", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Kriegte Muth zu neuem Leben:", "tokens": ["Krieg\u00b7te", "Muth", "zu", "neu\u00b7em", "Le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sollte das der Fr\u00fchling seyn?", "tokens": ["Soll\u00b7te", "das", "der", "Fr\u00fch\u00b7ling", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "ART", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00f6nnt' es doch wohl Freuden geben?", "tokens": ["K\u00f6nnt'", "es", "doch", "wohl", "Freu\u00b7den", "ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Da ergl\u00fchten schon die Wogen,", "tokens": ["Da", "er\u00b7gl\u00fch\u00b7ten", "schon", "die", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Funkeln ging auf gr\u00fcner Flur,", "tokens": ["Fun\u00b7keln", "ging", "auf", "gr\u00fc\u00b7ner", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Morgenroth sprang k\u00fchn in Bogen,", "tokens": ["Mor\u00b7gen\u00b7roth", "sprang", "k\u00fchn", "in", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gl\u00e4nzend, taumelnd die Natur.", "tokens": ["Gl\u00e4n\u00b7zend", ",", "tau\u00b7melnd", "die", "Na\u00b7tur", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Und die Waldung blieb nicht tr\u00e4ge,", "tokens": ["Und", "die", "Wal\u00b7dung", "blieb", "nicht", "tr\u00e4\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle V\u00f6gel sprangen auf,", "tokens": ["Al\u00b7le", "V\u00f6\u00b7gel", "spran\u00b7gen", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jubelten durch das Geh\u00e4ge,", "tokens": ["Ju\u00b7bel\u00b7ten", "durch", "das", "Ge\u00b7h\u00e4\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Jagten sich im muntern Lauf. \u2013", "tokens": ["Jag\u00b7ten", "sich", "im", "mun\u00b7tern", "Lauf", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "In des Jauchzens Lust verloren", "tokens": ["In", "des", "Jauch\u00b7zens", "Lust", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dacht ich nicht an Sterben mehr,", "tokens": ["Dacht", "ich", "nicht", "an", "Ster\u00b7ben", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchlte mich nun neugeboren", "tokens": ["F\u00fchl\u00b7te", "mich", "nun", "neu\u00b7ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem goldnen Freudenmeer.", "tokens": ["In", "dem", "gold\u00b7nen", "Freu\u00b7den\u00b7meer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Ach! sie ist mir endlich nahe,", "tokens": ["Ach", "!", "sie", "ist", "mir", "end\u00b7lich", "na\u00b7he", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach der meine Sehnsucht rang,", "tokens": ["Nach", "der", "mei\u00b7ne", "Sehn\u00b7sucht"