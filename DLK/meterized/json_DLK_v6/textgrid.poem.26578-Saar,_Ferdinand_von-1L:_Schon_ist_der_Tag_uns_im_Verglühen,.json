{"textgrid.poem.26578": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schon ist der Tag uns im Vergl\u00fchen,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon ist der Tag uns im Vergl\u00fchen,", "tokens": ["Schon", "ist", "der", "Tag", "uns", "im", "Ver\u00b7gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In letzter Sch\u00f6nheit prangt dein Leib;", "tokens": ["In", "letz\u00b7ter", "Sch\u00f6n\u00b7heit", "prangt", "dein", "Leib", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Herzen allerletztes Bl\u00fchen", "tokens": ["Der", "Her\u00b7zen", "al\u00b7ler\u00b7letz\u00b7tes", "Bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist unser Gl\u00fcck, geliebtes Weib!", "tokens": ["Ist", "un\u00b7ser", "Gl\u00fcck", ",", "ge\u00b7lieb\u00b7tes", "Weib", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "D'rum la\u00df \u2013 o la\u00df die Zeit uns frommen,", "tokens": ["D'\u00b7rum", "la\u00df", "\u2013", "o", "la\u00df", "die", "Zeit", "uns", "from\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "$(", "FM", "FM", "ART", "NN", "PPER", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und keine Stunde sei vers\u00e4umt;", "tokens": ["Und", "kei\u00b7ne", "Stun\u00b7de", "sei", "ver\u00b7s\u00e4umt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Wonne sei die Nacht durchglommen,", "tokens": ["Von", "Won\u00b7ne", "sei", "die", "Nacht", "durch\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und dann der Morgen hold vertr\u00e4umt.", "tokens": ["Und", "dann", "der", "Mor\u00b7gen", "hold", "ver\u00b7tr\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und jede Freude dieses Lebens", "tokens": ["Und", "je\u00b7de", "Freu\u00b7de", "die\u00b7ses", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Soll noch durch uns're Seele geh'n;", "tokens": ["Soll", "noch", "durch", "un\u00b7s'\u00b7re", "See\u00b7le", "geh'n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Wir wollen sie, entz\u00fcckten Bebens,", "tokens": ["Wir", "wol\u00b7len", "sie", ",", "ent\u00b7z\u00fcck\u00b7ten", "Be\u00b7bens", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch ganz genie\u00dfen und versteh'n.", "tokens": ["Noch", "ganz", "ge\u00b7nie\u00b7\u00dfen", "und", "ver\u00b7steh'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "ADV", "VVINF", "KON", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mag auch der Himmel leise nachten,", "tokens": ["Mag", "auch", "der", "Him\u00b7mel", "lei\u00b7se", "nach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und h\u00e4lt er seinen Blitz gez\u00fcckt \u2013", "tokens": ["Und", "h\u00e4lt", "er", "sei\u00b7nen", "Blitz", "ge\u00b7z\u00fcckt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir wollen nimmer es beachten,", "tokens": ["Wir", "wol\u00b7len", "nim\u00b7mer", "es", "be\u00b7ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie nahe das Verh\u00e4ngni\u00df r\u00fcckt.", "tokens": ["Wie", "na\u00b7he", "das", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "r\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und sterben la\u00dft uns lebenstrunken,", "tokens": ["Und", "ster\u00b7ben", "la\u00dft", "uns", "le\u00b7bens\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist der Vernichtung Stunde da,", "tokens": ["Ist", "der", "Ver\u00b7nich\u00b7tung", "Stun\u00b7de", "da", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "ADV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Wie einst im Tode hingesunken", "tokens": ["Wie", "einst", "im", "To\u00b7de", "hin\u00b7ge\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Antonius und Cleopatra!", "tokens": ["An\u00b7to\u00b7ni\u00b7us", "und", "Cleo\u00b7pat\u00b7ra", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}