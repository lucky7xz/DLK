{"textgrid.poem.62521": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Der Feigenbaum", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nimm, G\u00e4rtner, diesen Feigenbaum", "tokens": ["Nimm", ",", "G\u00e4rt\u00b7ner", ",", "die\u00b7sen", "Fei\u00b7gen\u00b7baum"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$,", "NN", "$,", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wirf ihn aus dem Garten,", "tokens": ["Und", "wirf", "ihn", "aus", "dem", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich g\u00f6nn' ihm l\u00e4nger keinen Raum", "tokens": ["Ich", "g\u00f6nn'", "ihm", "l\u00e4n\u00b7ger", "kei\u00b7nen", "Raum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mag nicht l\u00e4nger warten.", "tokens": ["Und", "mag", "nicht", "l\u00e4n\u00b7ger", "war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich komme schon so manches Jahr", "tokens": ["Ich", "kom\u00b7me", "schon", "so", "man\u00b7ches", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und keine Frucht wird offenbar.", "tokens": ["Und", "kei\u00b7ne", "Frucht", "wird", "of\u00b7fen\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der G\u00e4rtner spricht mit weiser Huld:", "tokens": ["Der", "G\u00e4rt\u00b7ner", "spricht", "mit", "wei\u00b7ser", "Huld", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df noch ein Jahr vergehen,", "tokens": ["La\u00df", "noch", "ein", "Jahr", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Herr, diesmal trage noch Geduld", "tokens": ["Herr", ",", "dies\u00b7mal", "tra\u00b7ge", "noch", "Ge\u00b7duld"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VVFIN", "ADV", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und la\u00df ihn ferner stehen;", "tokens": ["Und", "la\u00df", "ihn", "fer\u00b7ner", "ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich will ihn pflegen still und treu,", "tokens": ["Ich", "will", "ihn", "pfle\u00b7gen", "still", "und", "treu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "PTKVZ", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ob endlich uns die Frucht erfreu'.", "tokens": ["Ob", "end\u00b7lich", "uns", "die", "Frucht", "er\u00b7freu'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du Sohn, der ewig uns vertritt", "tokens": ["Du", "Sohn", ",", "der", "e\u00b7wig", "uns", "ver\u00b7tritt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit br\u00fcnstigen Gebeten,", "tokens": ["Mit", "br\u00fcns\u00b7ti\u00b7gen", "Ge\u00b7be\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der an dem Oelberg f\u00fcr uns litt,", "tokens": ["Der", "an", "dem", "O\u00b7el\u00b7berg", "f\u00fcr", "uns", "litt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sich martern lie\u00df und t\u00f6dten,", "tokens": ["Sich", "mar\u00b7tern", "lie\u00df", "und", "t\u00f6d\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wir h\u00f6ren dein Gebet und Wort,", "tokens": ["Wir", "h\u00f6\u00b7ren", "dein", "Ge\u00b7bet", "und", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O Hoherpriester, bete fort!", "tokens": ["O", "Ho\u00b7her\u00b7pries\u00b7ter", ",", "be\u00b7te", "fort", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du milder G\u00e4rtner, Jesus Christ,", "tokens": ["Du", "mil\u00b7der", "G\u00e4rt\u00b7ner", ",", "Je\u00b7sus", "Christ", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du wirst uns nicht vers\u00e4umen,", "tokens": ["Du", "wirst", "uns", "nicht", "ver\u00b7s\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und, weil du so geduldig bist,", "tokens": ["Und", ",", "weil", "du", "so", "ge\u00b7dul\u00b7dig", "bist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kann mancher Zweig noch keimen;", "tokens": ["Kann", "man\u00b7cher", "Zweig", "noch", "kei\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Garten, welcher dich erfreut,", "tokens": ["Der", "Gar\u00b7ten", ",", "wel\u00b7cher", "dich", "er\u00b7freut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O Heiland, ist die Christenheit.", "tokens": ["O", "Hei\u00b7land", ",", "ist", "die", "Chris\u00b7ten\u00b7heit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Zorn des Richters, welcher droht,", "tokens": ["Der", "Zorn", "des", "Rich\u00b7ters", ",", "wel\u00b7cher", "droht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er mag uns zwar erschrecken,", "tokens": ["Er", "mag", "uns", "zwar", "er\u00b7schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch wissen wir, vom ew'gen Tod", "tokens": ["Doch", "wis\u00b7sen", "wir", ",", "vom", "ew'\u00b7gen", "Tod"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kann uns dein Wort erwecken.", "tokens": ["Kann", "uns", "dein", "Wort", "er\u00b7we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du ladest ein, treibst Keinen fort", "tokens": ["Du", "la\u00b7dest", "ein", ",", "treibst", "Kei\u00b7nen", "fort"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "VVFIN", "PIAT", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und g\u00f6nnest Jedem Zeit und Ort.", "tokens": ["Und", "g\u00f6n\u00b7nest", "Je\u00b7dem", "Zeit", "und", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "La\u00df Keinen von uns unbesucht,", "tokens": ["La\u00df", "Kei\u00b7nen", "von", "uns", "un\u00b7be\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "APPR", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir f\u00fchlen schon dein Wehen,", "tokens": ["Wir", "f\u00fch\u00b7len", "schon", "dein", "We\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df uns in Bl\u00fcten und in Frucht,", "tokens": ["La\u00df", "uns", "in", "Bl\u00fc\u00b7ten", "und", "in", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wann du wirst kommen, stehen.", "tokens": ["Wann", "du", "wirst", "kom\u00b7men", ",", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Viel Zweiglein, jung und frisch und gr\u00fcn,", "tokens": ["Viel", "Zwei\u00b7glein", ",", "jung", "und", "frisch", "und", "gr\u00fcn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ADJD", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vergelten, G\u00e4rtner, dein Bem\u00fchn!", "tokens": ["Ver\u00b7gel\u00b7ten", ",", "G\u00e4rt\u00b7ner", ",", "dein", "Be\u00b7m\u00fchn", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}