{"textgrid.poem.30389": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Solln allererst die Schnecken", "tokens": ["Solln", "al\u00b7le\u00b7rerst", "die", "Schne\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Hurtigkeit erwecken,", "tokens": ["Die", "Hur\u00b7tig\u00b7keit", "er\u00b7we\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So mustu harren lange;", "tokens": ["So", "mus\u00b7tu", "har\u00b7ren", "lan\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VVINF", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie w\u00fcrcken nach dem Gange.", "tokens": ["Sie", "w\u00fcr\u00b7cken", "nach", "dem", "Gan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}