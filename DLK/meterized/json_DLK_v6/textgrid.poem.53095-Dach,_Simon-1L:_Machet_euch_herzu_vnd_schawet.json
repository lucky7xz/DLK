{"textgrid.poem.53095": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Machet euch herzu vnd schawet", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Machet euch herzu vnd schawet", "tokens": ["Ma\u00b7chet", "euch", "her\u00b7zu", "vnd", "scha\u00b7wet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "KON", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Auff den Schmuck der sch\u00f6nen Braut,", "tokens": ["Auff", "den", "Schmuck", "der", "sch\u00f6\u00b7nen", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber so nicht als jhr pflegt,", "tokens": ["A\u00b7ber", "so", "nicht", "als", "jhr", "pflegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Was sie f\u00fcr Geschmeide tr\u00e4gt,", "tokens": ["Was", "sie", "f\u00fcr", "Ge\u00b7schmei\u00b7de", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was Schamarg, Rock, Koller, Drat", "tokens": ["Was", "Scha\u00b7marg", ",", "Rock", ",", "Kol\u00b7ler", ",", "Drat"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["PWS", "NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.6": {"text": "F\u00fcr Geschick vnd M\u00e4ngel hat;", "tokens": ["F\u00fcr", "Ge\u00b7schick", "vnd", "M\u00e4n\u00b7gel", "hat", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Solches ist nur Kakel-Werck", "tokens": ["Sol\u00b7ches", "ist", "nur", "Ka\u00b7kel\u00b7\u00b7Werck"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "ADV", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Vnd ein Vppigkeit-Gemerck.", "tokens": ["Vnd", "ein", "Vp\u00b7pig\u00b7keit\u00b7Ge\u00b7merck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Nein! es sehe wer sie kan", "tokens": ["Nein", "!", "es", "se\u00b7he", "wer", "sie", "kan"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "PPER", "VVFIN", "PWS", "PPER", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Mit Verstandes-Augen an,", "tokens": ["Mit", "Ver\u00b7stan\u00b7des\u00b7Au\u00b7gen", "an", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Hab auff Ihrer Tugend Pracht,", "tokens": ["Hab", "auff", "Ih\u00b7rer", "Tu\u00b7gend", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Demuth, Zucht vnd Vnschuldt acht,", "tokens": ["De\u00b7muth", ",", "Zucht", "vnd", "Vn\u00b7schuldt", "acht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "CARD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Ich wil wetten, er gesteht,", "tokens": ["Ich", "wil", "wet\u00b7ten", ",", "er", "ge\u00b7steht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "PPER", "VVFIN", "$,"], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}, "line.14": {"text": "Da\u00df sie gantz ", "tokens": ["Da\u00df", "sie", "gantz"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ADV"], "meter": "+-+", "measure": "trochaic.di"}}}}}