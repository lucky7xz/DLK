{"dta.poem.3202": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "53.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ja such' in deines Volks Ruhmtempel nur zu prangen,", "tokens": ["Ja", "such'", "in", "dei\u00b7nes", "Volks", "Ruhm\u00b7tem\u00b7pel", "nur", "zu", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "ADV", "PTKZU", "VVFIN", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wo lebend nicht hinein, im Tod doch, zu gelangen.", "tokens": ["Wo", "le\u00b7bend", "nicht", "hin\u00b7ein", ",", "im", "Tod", "doch", ",", "zu", "ge\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PTKNEG", "PTKVZ", "$,", "APPRART", "NN", "ADV", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da\u00df, wann viel Namen, die nun klingen, sind verklungen,", "tokens": ["Da\u00df", ",", "wann", "viel", "Na\u00b7men", ",", "die", "nun", "klin\u00b7gen", ",", "sind", "ver\u00b7klun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "PIAT", "NN", "$,", "PRELS", "ADV", "VVINF", "$,", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch deiner sei genannt von sp\u00e4ter Enkel Zungen.", "tokens": ["Noch", "dei\u00b7ner", "sei", "ge\u00b7nannt", "von", "sp\u00e4\u00b7ter", "En\u00b7kel", "Zun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "VAFIN", "VVPP", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nicht wecken wird dich das, noch st\u00f6ren deine Ruh,", "tokens": ["Nicht", "we\u00b7cken", "wird", "dich", "das", ",", "noch", "st\u00f6\u00b7ren", "dei\u00b7ne", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VAFIN", "PPER", "PDS", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch Trost und Lohn dir seyn: Noch ruhend wirkest du.", "tokens": ["Doch", "Trost", "und", "Lohn", "dir", "seyn", ":", "Noch", "ru\u00b7hend", "wir\u00b7kest", "du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "PPER", "VAINF", "$.", "ADV", "VVPP", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Im Tode wirkst du, was du nicht gekonnt im Leben,", "tokens": ["Im", "To\u00b7de", "wirkst", "du", ",", "was", "du", "nicht", "ge\u00b7konnt", "im", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,", "PWS", "PPER", "PTKNEG", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu sittigen die Welt und Herzen zu erheben.", "tokens": ["Zu", "sit\u00b7ti\u00b7gen", "die", "Welt", "und", "Her\u00b7zen", "zu", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}