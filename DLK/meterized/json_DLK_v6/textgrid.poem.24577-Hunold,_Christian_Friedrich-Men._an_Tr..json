{"textgrid.poem.24577": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Men. an Tr.", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du redest/wie man will/und schickst dich in die Welt/", "tokens": ["Du", "re\u00b7dest", "/", "wie", "man", "will", "/", "und", "schickst", "dich", "in", "die", "Welt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWAV", "PIS", "VMFIN", "$(", "KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn sie die Tugenden vor was beliebtes h\u00e4lt/", "tokens": ["Wenn", "sie", "die", "Tu\u00b7gen\u00b7den", "vor", "was", "be\u00b7lieb\u00b7tes", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PRELS", "ADJA", "VVFIN", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So sprichst du gleichfals ja; lobt sie die Lustbarkeiten/", "tokens": ["So", "sprichst", "du", "gleich\u00b7fals", "ja", ";", "lobt", "sie", "die", "Lust\u00b7bar\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$.", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "So steht mein T - - ihr am ersten an der Seiten.", "tokens": ["So", "steht", "mein", "T", "ihr", "am", "ers\u00b7ten", "an", "der", "Sei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NE", "$(", "$(", "PPER", "APPRART", "ADJA", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Inzwischen liebe nur die Tugend oder nicht/", "tokens": ["I\u00b7nzwi\u00b7schen", "lie\u00b7be", "nur", "die", "Tu\u00b7gend", "o\u00b7der", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "KON", "PTKNEG", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.6": {"text": "Ich liebe dich gleichwohl und gebe dir Bericht/", "tokens": ["Ich", "lie\u00b7be", "dich", "gleich\u00b7wohl", "und", "ge\u00b7be", "dir", "Be\u00b7richt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "PPER", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df meine Poesie nun wird zur Presse kommen/", "tokens": ["Da\u00df", "mei\u00b7ne", "Poe\u00b7sie", "nun", "wird", "zur", "Pres\u00b7se", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VAFIN", "APPRART", "NN", "VVINF", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Sonst hatt ich gleich zu dir den Weg erfreut genommen.", "tokens": ["Sonst", "hatt", "ich", "gleich", "zu", "dir", "den", "Weg", "er\u00b7freut", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPER", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Vieleicht am Abend wird dein treuer Freund dich sehn/", "tokens": ["Vie\u00b7leicht", "am", "A\u00b7bend", "wird", "dein", "treu\u00b7er", "Freund", "dich", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wo nicht/so kan es auch ein ander mahl geschehn.", "tokens": ["Wo", "nicht", "/", "so", "kan", "es", "auch", "ein", "an\u00b7der", "mahl", "ge\u00b7schehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$(", "ADV", "VMFIN", "PPER", "ADV", "ART", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}