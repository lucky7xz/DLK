{"textgrid.poem.37309": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der alte Narr", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein K\u00fcnstler auf dem hohen Seil,", "tokens": ["Ein", "K\u00fcnst\u00b7ler", "auf", "dem", "ho\u00b7hen", "Seil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der alt geworden mittlerweil,", "tokens": ["Der", "alt", "ge\u00b7wor\u00b7den", "mitt\u00b7ler\u00b7weil", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAPP", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Stieg eines Tages vom Ger\u00fcst", "tokens": ["Stieg", "ei\u00b7nes", "Ta\u00b7ges", "vom", "Ge\u00b7r\u00fcst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sprach: Nun will ich unten bleiben", "tokens": ["Und", "sprach", ":", "Nun", "will", "ich", "un\u00b7ten", "blei\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "ADV", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und nur noch Hausgymnastik treiben,", "tokens": ["Und", "nur", "noch", "Haus\u00b7gym\u00b7nas\u00b7tik", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was zur Verdauung n\u00f6tig ist.", "tokens": ["Was", "zur", "Ver\u00b7dau\u00b7ung", "n\u00f6\u00b7tig", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da riefen alle: Oh, wie schad!", "tokens": ["Da", "rie\u00b7fen", "al\u00b7le", ":", "Oh", ",", "wie", "schad", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$.", "ITJ", "$,", "PWAV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Meister scheint doch allnachgrad", "tokens": ["Der", "Meis\u00b7ter", "scheint", "doch", "all\u00b7nach\u00b7grad"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu schwach und steif zum Seilbesteigen!", "tokens": ["Zu", "schwach", "und", "steif", "zum", "Seil\u00b7be\u00b7stei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "KON", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ha! denkt er, dieses wird sich zeigen!", "tokens": ["Ha", "!", "denkt", "er", ",", "die\u00b7ses", "wird", "sich", "zei\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "$,", "PDS", "VAFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und richtig, eh der Markt geschlossen,", "tokens": ["Und", "rich\u00b7tig", ",", "eh", "der", "Markt", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Treibt er aufs neu die alten Possen", "tokens": ["Treibt", "er", "aufs", "neu", "die", "al\u00b7ten", "Pos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hoch in der Luft und zwar mit Gl\u00fcck,", "tokens": ["Hoch", "in", "der", "Luft", "und", "zwar", "mit", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "KON", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bis auf ein kleines Mi\u00dfgeschick.", "tokens": ["Bis", "auf", "ein", "klei\u00b7nes", "Mi\u00df\u00b7ge\u00b7schick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er fiel herab in gro\u00dfer Eile", "tokens": ["Er", "fiel", "her\u00b7ab", "in", "gro\u00b7\u00dfer", "Ei\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und knickte sich die Wirbels\u00e4ule.", "tokens": ["Und", "knick\u00b7te", "sich", "die", "Wir\u00b7bel\u00b7s\u00e4u\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der alte Narr! Jetzt bleibt er krumm!", "tokens": ["Der", "al\u00b7te", "Narr", "!", "Jetzt", "bleibt", "er", "krumm", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So \u00e4u\u00dfert sich das Publikum.", "tokens": ["So", "\u00e4u\u00b7\u00dfert", "sich", "das", "Pub\u00b7li\u00b7kum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}