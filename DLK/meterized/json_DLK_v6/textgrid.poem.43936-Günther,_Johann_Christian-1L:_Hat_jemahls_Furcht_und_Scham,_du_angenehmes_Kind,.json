{"textgrid.poem.43936": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hat jemahls Furcht und Scham, du angenehmes Kind,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hat jemahls Furcht und Scham, du angenehmes Kind,", "tokens": ["Hat", "je\u00b7mahls", "Furcht", "und", "Scham", ",", "du", "an\u00b7ge\u00b7neh\u00b7mes", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "KON", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem wenig an Verstand und Sch\u00f6nheit \u00e4hnlich sind,", "tokens": ["Dem", "we\u00b7nig", "an", "Ver\u00b7stand", "und", "Sch\u00f6n\u00b7heit", "\u00e4hn\u00b7lich", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "NN", "KON", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den angesezten Kiel mir in der Hand verr\u00fccket,", "tokens": ["Den", "an\u00b7ge\u00b7sez\u00b7ten", "Kiel", "mir", "in", "der", "Hand", "ver\u00b7r\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So ist es warlich wohl auf diesen Tag geschehn,", "tokens": ["So", "ist", "es", "war\u00b7lich", "wohl", "auf", "die\u00b7sen", "Tag", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da meine Grobheit es um deine Gunst versehn", "tokens": ["Da", "mei\u00b7ne", "Grob\u00b7heit", "es", "um", "dei\u00b7ne", "Gunst", "ver\u00b7sehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und meine Demuth sich vor deinem Eifer b\u00fccket.", "tokens": ["Und", "mei\u00b7ne", "De\u00b7muth", "sich", "vor", "dei\u00b7nem", "Ei\u00b7fer", "b\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich f\u00fchle deinen Zorn, der als ein scharfes Schwerd", "tokens": ["Ich", "f\u00fch\u00b7le", "dei\u00b7nen", "Zorn", ",", "der", "als", "ein", "schar\u00b7fes", "Schwerd"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PRELS", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In meine Seele dringt und durch das Herze f\u00e4hrt,", "tokens": ["In", "mei\u00b7ne", "See\u00b7le", "dringt", "und", "durch", "das", "Her\u00b7ze", "f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein jeder Blick von dir verweist mir das Verbrechen;", "tokens": ["Ein", "je\u00b7der", "Blick", "von", "dir", "ver\u00b7weist", "mir", "das", "Ver\u00b7bre\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mich deucht, ich sehe schon, wie heftig, wie erhizt", "tokens": ["Mich", "deucht", ",", "ich", "se\u00b7he", "schon", ",", "wie", "hef\u00b7tig", ",", "wie", "er\u00b7hizt"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "$,", "PWAV", "ADJD", "$,", "PWAV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Augen Wetterstrahl auf meine Scheitel blizt,", "tokens": ["Der", "Au\u00b7gen", "Wet\u00b7ter\u00b7strahl", "auf", "mei\u00b7ne", "Schei\u00b7tel", "blizt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mich deucht, ich h\u00f6re dich schon zu dir selber sprechen:", "tokens": ["Mich", "deucht", ",", "ich", "h\u00f6\u00b7re", "dich", "schon", "zu", "dir", "sel\u00b7ber", "spre\u00b7chen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "APPR", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ist dies der sch\u00f6ne Mensch, der sich so heilig stellt", "tokens": ["Ist", "dies", "der", "sch\u00f6\u00b7ne", "Mensch", ",", "der", "sich", "so", "hei\u00b7lig", "stellt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PDS", "ART", "ADJA", "NN", "$,", "PRELS", "PRF", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und der kein Wa\u00dfer tr\u00fcbt, bis er ins Wa\u00dfer f\u00e4llt?", "tokens": ["Und", "der", "kein", "Wa\u00b7\u00dfer", "tr\u00fcbt", ",", "bis", "er", "ins", "Wa\u00b7\u00dfer", "f\u00e4llt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIAT", "NN", "VVFIN", "$,", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das, warlich, h\u00e4tt ich mir von ihm nicht tr\u00e4umen la\u00dfen,", "tokens": ["Das", ",", "war\u00b7lich", ",", "h\u00e4tt", "ich", "mir", "von", "ihm", "nicht", "tr\u00e4u\u00b7men", "la\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "ADV", "$,", "VAFIN", "PPER", "PRF", "APPR", "PPER", "PTKNEG", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das h\u00e4tt ich auch in ihm mit Spie\u00dfen nicht gesucht.", "tokens": ["Das", "h\u00e4tt", "ich", "auch", "in", "ihm", "mit", "Spie\u00b7\u00dfen", "nicht", "ge\u00b7sucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "APPR", "PPER", "APPR", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "O gro\u00dfer Aberwiz, o Junggesellenzucht!", "tokens": ["O", "gro\u00b7\u00dfer", "A\u00b7ber\u00b7wiz", ",", "o", "Jung\u00b7ge\u00b7sel\u00b7len\u00b7zucht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ach, m\u00f6chte doch ein Strick ihn bey der Gurgel fa\u00dfen!", "tokens": ["Ach", ",", "m\u00f6ch\u00b7te", "doch", "ein", "Strick", "ihn", "bey", "der", "Gur\u00b7gel", "fa\u00b7\u00dfen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VMFIN", "ADV", "ART", "NN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ach, keusche Marilis, dein Eifer ist gerecht,", "tokens": ["Ach", ",", "keu\u00b7sche", "Ma\u00b7ri\u00b7lis", ",", "dein", "Ei\u00b7fer", "ist", "ge\u00b7recht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "NE", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Strafe noch zu klein, und dein gefallner Knecht", "tokens": ["Die", "Stra\u00b7fe", "noch", "zu", "klein", ",", "und", "dein", "ge\u00b7fall\u00b7ner", "Knecht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "PTKA", "ADJD", "$,", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nicht w\u00fcrdig, nur ein Wort vor dich mehr aufzusezen.", "tokens": ["Nicht", "w\u00fcr\u00b7dig", ",", "nur", "ein", "Wort", "vor", "dich", "mehr", "auf\u00b7zu\u00b7se\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "ADV", "ART", "NN", "APPR", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ruf alle Hencker auf, sprich mir das Leben ab,", "tokens": ["Ruf", "al\u00b7le", "Hen\u00b7cker", "auf", ",", "sprich", "mir", "das", "Le\u00b7ben", "ab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "PTKVZ", "$,", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Sto\u00df den zerfleischten Leib in ein beschimpftes Grab,", "tokens": ["Sto\u00df", "den", "zer\u00b7fleischten", "Leib", "in", "ein", "be\u00b7schimpf\u00b7tes", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Auch dieses m\u00fcst ich noch vor eine Gnade sch\u00e4zen.", "tokens": ["Auch", "die\u00b7ses", "m\u00fcst", "ich", "noch", "vor", "ei\u00b7ne", "Gna\u00b7de", "sch\u00e4\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VMFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Allein was n\u00fczt dir wohl die schlechte Hand voll Blut", "tokens": ["Al\u00b7lein", "was", "n\u00fczt", "dir", "wohl", "die", "schlech\u00b7te", "Hand", "voll", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PWS", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des S\u00fcnders, der in Staub und Asche Bu\u00dfe thut?", "tokens": ["Des", "S\u00fcn\u00b7ders", ",", "der", "in", "Staub", "und", "A\u00b7sche", "Bu\u00b7\u00dfe", "thut", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zum Creuze kriech ich jezt wie gestern in das Bette.", "tokens": ["Zum", "Creu\u00b7ze", "kriech", "ich", "jezt", "wie", "ge\u00b7stern", "in", "das", "Bet\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "KOKOM", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Erwege meine Reu, schau meine Thr\u00e4nen an", "tokens": ["Er\u00b7we\u00b7ge", "mei\u00b7ne", "Reu", ",", "schau", "mei\u00b7ne", "Thr\u00e4\u00b7nen", "an"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NE", "$,", "ADJD", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und glaube, da\u00df kein Mensch so ernstlich weinen kan,", "tokens": ["Und", "glau\u00b7be", ",", "da\u00df", "kein", "Mensch", "so", "ernst\u00b7lich", "wei\u00b7nen", "kan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PIAT", "NN", "ADV", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn er wie Petrus gleich auch Gott verleugnet h\u00e4tte.", "tokens": ["Wenn", "er", "wie", "Pet\u00b7rus", "gleich", "auch", "Gott", "ver\u00b7leug\u00b7net", "h\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOKOM", "FM", "ADV", "ADV", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ich rede fast zuviel; jedoch der herbe Schmerz", "tokens": ["Ich", "re\u00b7de", "fast", "zu\u00b7viel", ";", "je\u00b7doch", "der", "her\u00b7be", "Schmerz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "$.", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Beraubt mich der Vernunft, und mein beklemmtes Herz", "tokens": ["Be\u00b7raubt", "mich", "der", "Ver\u00b7nunft", ",", "und", "mein", "be\u00b7klemm\u00b7tes", "Herz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hat in der Brust nicht Raum, weil es der Kummer schw\u00e4ngert.", "tokens": ["Hat", "in", "der", "Brust", "nicht", "Raum", ",", "weil", "es", "der", "Kum\u00b7mer", "schw\u00e4n\u00b7gert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "PTKNEG", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Erbarme dich, wo noch Erbarmung \u00fcbrig ist;", "tokens": ["Er\u00b7bar\u00b7me", "dich", ",", "wo", "noch", "Er\u00b7bar\u00b7mung", "\u00fcb\u00b7rig", "ist", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PWAV", "ADV", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du weist es ohnedem, der ist kein guter Christ,", "tokens": ["Du", "weist", "es", "oh\u00b7ne\u00b7dem", ",", "der", "ist", "kein", "gu\u00b7ter", "Christ", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PAV", "$,", "PRELS", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der, wenn er helfen kan, des Nechsten Pein verl\u00e4ngert.", "tokens": ["Der", ",", "wenn", "er", "hel\u00b7fen", "kan", ",", "des", "Nechs\u00b7ten", "Pein", "ver\u00b7l\u00e4n\u00b7gert", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Es stirbt kein Mensch so jung, den nicht ein Fall gedenckt;", "tokens": ["Es", "stirbt", "kein", "Mensch", "so", "jung", ",", "den", "nicht", "ein", "Fall", "ge\u00b7denckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ADV", "ADJD", "$,", "PRELS", "PTKNEG", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kein Weiser ist so klug, den nicht ein Irrthum kr\u00e4nckt;", "tokens": ["Kein", "Wei\u00b7ser", "ist", "so", "klug", ",", "den", "nicht", "ein", "Irr\u00b7thum", "kr\u00e4nckt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ADJD", "$,", "PRELS", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den Salomon besch\u00e4mt die Wei\u00dfheit seiner B\u00fccher.", "tokens": ["Den", "Sa\u00b7lo\u00b7mon", "be\u00b7sch\u00e4mt", "die", "Wei\u00df\u00b7heit", "sei\u00b7ner", "B\u00fc\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie leichtlich wird doch nicht die Jugend \u00fcbereilt;", "tokens": ["Wie", "leicht\u00b7lich", "wird", "doch", "nicht", "die", "Ju\u00b7gend", "\u00fc\u00b7be\u00b7reilt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "PTKNEG", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wer weis, wer heute noch mit mir die Strafe theilt;", "tokens": ["Wer", "weis", ",", "wer", "heu\u00b7te", "noch", "mit", "mir", "die", "Stra\u00b7fe", "theilt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKVZ", "$,", "PWS", "ADV", "ADV", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es lebt kein Sterblicher vor Tod und Fehlern sicher.", "tokens": ["Es", "lebt", "kein", "Sterb\u00b7li\u00b7cher", "vor", "Tod", "und", "Feh\u00b7lern", "si\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Vergieb, vergi\u00df und nimm vor das, was ich ver\u00fcbt,", "tokens": ["Ver\u00b7gieb", ",", "ver\u00b7gi\u00df", "und", "nimm", "vor", "das", ",", "was", "ich", "ver\u00b7\u00fcbt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "VVIMP", "KON", "VVIMP", "APPR", "PDS", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dies Blat, so meine Faust dir zitternd \u00fcbergiebt,", "tokens": ["Dies", "Blat", ",", "so", "mei\u00b7ne", "Faust", "dir", "zit\u00b7ternd", "\u00fc\u00b7berg\u00b7iebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "$,", "ADV", "PPOSAT", "NN", "PPER", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und schencke meiner Haut vor dieses Mahl die Strafe;", "tokens": ["Und", "schen\u00b7cke", "mei\u00b7ner", "Haut", "vor", "die\u00b7ses", "Mahl", "die", "Stra\u00b7fe", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "PDAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Entrei\u00df mir deinen Zorn, der mich wie Feuer schmerzt.", "tokens": ["Ent\u00b7rei\u00df", "mir", "dei\u00b7nen", "Zorn", ",", "der", "mich", "wie", "Feu\u00b7er", "schmerzt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,", "PRELS", "PPER", "KOKOM", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Schweig, doch wo jemand fragt, ob G\u00fcnther dich geherzt,", "tokens": ["Schweig", ",", "doch", "wo", "je\u00b7mand", "fragt", ",", "ob", "G\u00fcn\u00b7ther", "dich", "ge\u00b7herzt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "PWAV", "PIS", "VVFIN", "$,", "KOUS", "NE", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So gieb zur Antwort: Ja, er that es nur im Schlafe.", "tokens": ["So", "gieb", "zur", "Ant\u00b7wort", ":", "Ja", ",", "er", "that", "es", "nur", "im", "Schla\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "APPRART", "NN", "$.", "PTKANT", "$,", "PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}