{"textgrid.poem.37159": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "Der Galgen", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Kaufmann, welchen Lug und Trug", "tokens": ["Ein", "Kauf\u00b7mann", ",", "wel\u00b7chen", "Lug", "und", "Trug"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und gutes Gl\u00fcck zum Ritter schlug,", "tokens": ["Und", "gu\u00b7tes", "Gl\u00fcck", "zum", "Rit\u00b7ter", "schlug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lie\u00df einen Galgen j\u00fcngst auf seinen G\u00fctern bauen", "tokens": ["Lie\u00df", "ei\u00b7nen", "Gal\u00b7gen", "j\u00fcngst", "auf", "sei\u00b7nen", "G\u00fc\u00b7tern", "bau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und stand mit stolzem Blick, der Arbeit zuzuschauen.", "tokens": ["Und", "stand", "mit", "stol\u00b7zem", "Blick", ",", "der", "Ar\u00b7beit", "zu\u00b7zu\u00b7schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u00bbo welche Sudelei!\u00ab rief er dem Zimmermann.", "tokens": ["\u00bb", "o", "wel\u00b7che", "Su\u00b7de\u00b7lei", "!", "\u00ab", "rief", "er", "dem", "Zim\u00b7mer\u00b7mann", "."], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "PWAT", "NN", "$.", "$(", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u00bbwie lange denkt Ihr wol, da\u00df dieses halten kann?\u00ab", "tokens": ["\u00bb", "wie", "lan\u00b7ge", "denkt", "Ihr", "wol", ",", "da\u00df", "die\u00b7ses", "hal\u00b7ten", "kann", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PDS", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie lange, gn\u00e4diger Herr? versetzte der geschwinder;", "tokens": ["Wie", "lan\u00b7ge", ",", "gn\u00e4\u00b7di\u00b7ger", "Herr", "?", "ver\u00b7setz\u00b7te", "der", "ge\u00b7schwin\u00b7der", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "ADJA", "NN", "$.", "VVFIN", "ART", "ADJA", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Der Galgen h\u00e4lt gewi\u00df f\u00fcr Sie und Ihre Kinder.", "tokens": ["Der", "Gal\u00b7gen", "h\u00e4lt", "ge\u00b7wi\u00df", "f\u00fcr", "Sie", "und", "Ih\u00b7re", "Kin\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}