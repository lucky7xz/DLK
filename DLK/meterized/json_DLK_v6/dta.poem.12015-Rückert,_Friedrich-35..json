{"dta.poem.12015": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "35.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kein Irrthum hinter dem nicht eine Wahrheit steht,", "tokens": ["Kein", "Irr\u00b7thum", "hin\u00b7ter", "dem", "nicht", "ei\u00b7ne", "Wahr\u00b7heit", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kein Schatten der nicht aus von einem Lichte geht.", "tokens": ["Kein", "Schat\u00b7ten", "der", "nicht", "aus", "von", "ei\u00b7nem", "Lich\u00b7te", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "PTKNEG", "APPR", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und wie der Schatten selbst dich wird zum Lichte leiten,", "tokens": ["Und", "wie", "der", "Schat\u00b7ten", "selbst", "dich", "wird", "zum", "Lich\u00b7te", "lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "PPER", "VAFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So auf des Irrthums Spur magst du zur Wahrheit schreiten.", "tokens": ["So", "auf", "des", "Irr\u00b7thums", "Spur", "magst", "du", "zur", "Wahr\u00b7heit", "schrei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "VMFIN", "PPER", "APPRART", "N