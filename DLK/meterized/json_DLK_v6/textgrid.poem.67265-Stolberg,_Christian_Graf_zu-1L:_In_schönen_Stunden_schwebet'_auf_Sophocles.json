{"textgrid.poem.67265": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: In sch\u00f6nen Stunden schwebet' auf Sophocles", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In sch\u00f6nen Stunden schwebet' auf Sophocles", "tokens": ["In", "sch\u00f6\u00b7nen", "Stun\u00b7den", "schwe\u00b7bet'", "auf", "So\u00b7pho\u00b7cles"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "APPR", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Cothurn auch ich einst; Doch es sind sch\u00f6nere,", "tokens": ["Cot\u00b7hurn", "auch", "ich", "einst", ";", "Doch", "es", "sind", "sch\u00f6\u00b7ne\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPER", "ADV", "$.", "KON", "PPER", "VAFIN", "ADJA", "$,"], "meter": "+---+-+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Wenn uns, von Worten unentweihte,", "tokens": ["Wenn", "uns", ",", "von", "Wor\u00b7ten", "un\u00b7ent\u00b7weih\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stille Begeistrung der Mus' ums\u00e4uselt.", "tokens": ["Stil\u00b7le", "Be\u00b7geis\u00b7trung", "der", "Mus'", "um\u00b7s\u00e4u\u00b7selt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "NN", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Wohl sind die sch\u00f6nen, wohl sind die sch\u00f6neren", "tokens": ["Wohl", "sind", "die", "sch\u00f6\u00b7nen", ",", "wohl", "sind", "die", "sch\u00f6\u00b7ne\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "$,", "ADV", "VAFIN", "ART", "ADJA"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mir werth. Doch was sind sch\u00f6ne, was sch\u00f6nere,", "tokens": ["Mir", "werth", ".", "Doch", "was", "sind", "sch\u00f6\u00b7ne", ",", "was", "sch\u00f6\u00b7ne\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$.", "KON", "PWS", "VAFIN", "ADJA", "$,", "PWS", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Gewogen mit der Stunden sch\u00f6nsten? \u2013", "tokens": ["Ge\u00b7wo\u00b7gen", "mit", "der", "Stun\u00b7den", "sch\u00f6ns\u00b7ten", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Welche? \u2013 Du fragst, und wer kennt, wie Du, sie?", "tokens": ["Wel\u00b7che", "?", "\u2013", "Du", "fragst", ",", "und", "wer", "kennt", ",", "wie", "Du", ",", "sie", "?"], "token_info": ["word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAT", "$.", "$(", "PPER", "VVFIN", "$,", "KON", "PWS", "VVFIN", "$,", "PWAV", "PPER", "$,", "PPER", "$."], "meter": "+--+-++-+-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Wenn Lieb' an heil'gem Strahle das Herz entflammt,", "tokens": ["Wenn", "Lieb'", "an", "heil'\u00b7gem", "Strah\u00b7le", "das", "Herz", "ent\u00b7flammt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wenn sich auf Ahnungsfittigen schwingt der Geist", "tokens": ["Wenn", "sich", "auf", "Ah\u00b7nungs\u00b7fit\u00b7ti\u00b7gen", "schwingt", "der", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPR", "NN", "VVFIN", "ART", "NN"], "meter": "---+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Empor \u2013 denn, ach! die Seinen flossen", "tokens": ["Em\u00b7por", "\u2013", "denn", ",", "ach", "!", "die", "Sei\u00b7nen", "flos\u00b7sen"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$(", "KON", "$,", "ITJ", "$.", "ART", "PPOSS", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nieder!", "tokens": ["Nie\u00b7der", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.4": {"line.1": {"text": "In solchen Stunden schlag' ich die Bundeshand", "tokens": ["In", "sol\u00b7chen", "Stun\u00b7den", "schlag'", "ich", "die", "Bun\u00b7des\u00b7hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein in die Rechten meiner Vertrautesten;", "tokens": ["Ein", "in", "die", "Rech\u00b7ten", "mei\u00b7ner", "Ver\u00b7trau\u00b7tes\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+--+-+--+--", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Dann dr\u00fcck' ich meine Lippen, ewig", "tokens": ["Dann", "dr\u00fcck", "ich", "mei\u00b7ne", "Lip\u00b7pen", ",", "e\u00b7wig"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Theure Sophia! auf Deine Wange.", "tokens": ["Theu\u00b7re", "So\u00b7phia", "!", "auf", "Dei\u00b7ne", "Wan\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}}}}