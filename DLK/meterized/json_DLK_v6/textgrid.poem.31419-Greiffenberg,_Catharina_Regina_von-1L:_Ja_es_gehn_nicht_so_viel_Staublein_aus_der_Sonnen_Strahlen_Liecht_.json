{"textgrid.poem.31419": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ja es gehn nicht so viel Staublein aus der Sonnen Strahlen Liecht /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja es gehn nicht so viel Staublein aus der Sonnen Strahlen Liecht /", "tokens": ["Ja", "es", "gehn", "nicht", "so", "viel", "Stau\u00b7blein", "aus", "der", "Son\u00b7nen", "Strah\u00b7len", "Liecht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "PTKNEG", "ADV", "PIAT", "NN", "APPR", "ART", "NN", "NN", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "als aus Gottes Wundern / Trost und bewehrte H\u00fclffesschl\u00fc\u00dfe.", "tokens": ["als", "aus", "Got\u00b7tes", "Wun\u00b7dern", "/", "Trost", "und", "be\u00b7wehr\u00b7te", "H\u00fclf\u00b7fes\u00b7schl\u00fc\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "NN", "$(", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "wann ich durch Vertrauens Krafft seine Gnaden mir eingie\u00dfe /", "tokens": ["wann", "ich", "durch", "Ver\u00b7trau\u00b7ens", "Krafft", "sei\u00b7ne", "Gna\u00b7den", "mir", "ein\u00b7gie\u00b7\u00dfe", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "NN", "PPOSAT", "NN", "PPER", "VVFIN", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "schadt das aussen Vngl\u00fccks St\u00fcrmen / meiner Sinnen-Ruhe nicht.", "tokens": ["schadt", "das", "aus\u00b7sen", "Vn\u00b7gl\u00fccks", "St\u00fcr\u00b7men", "/", "mei\u00b7ner", "Sin\u00b7nen\u00b7Ru\u00b7he", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NN", "$(", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "durch die tr\u00fcben Tr\u00fcbsal wolken / blickt dein Gnaden-Angesicht.", "tokens": ["durch", "die", "tr\u00fc\u00b7ben", "Tr\u00fcb\u00b7sal", "wol\u00b7ken", "/", "blickt", "dein", "Gna\u00b7den\u00b7An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$(", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "deiner Gnaden milder Thau / ist in saurer Angst sehr s\u00fcsse:", "tokens": ["dei\u00b7ner", "Gna\u00b7den", "mil\u00b7der", "Thau", "/", "ist", "in", "sau\u00b7rer", "Angst", "sehr", "s\u00fcs\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$(", "VAFIN", "APPR", "ADJA", "NN", "ADV", "ADJA", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "thu mich / wie der Perlen-Schneck / auf / damit ich ihn geniesse.", "tokens": ["thu", "mich", "/", "wie", "der", "Per\u00b7len\u00b7Schneck", "/", "auf", "/", "da\u00b7mit", "ich", "ihn", "ge\u00b7nies\u00b7se", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "KOKOM", "ART", "NN", "$(", "APPR", "$(", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "meines Geists begeiste Nadel nach dir / ihrem Stern / sich richt.", "tokens": ["mei\u00b7nes", "Geists", "be\u00b7geis\u00b7te", "Na\u00b7del", "nach", "dir", "/", "ih\u00b7rem", "Stern", "/", "sich", "richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "APPR", "PPER", "$(", "PPOSAT", "NN", "$(", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Ach mein Gast / es ist wol leicht / leider! leicht ihm vorgenommen:", "tokens": ["Ach", "mein", "Gast", "/", "es", "ist", "wol", "leicht", "/", "lei\u00b7der", "!", "leicht", "ihm", "vor\u00b7ge\u00b7nom\u00b7men", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "NN", "$(", "PPER", "VAFIN", "ADV", "ADJD", "$(", "ADV", "$.", "ADJD", "PPER", "VVPP", "$."], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "aber ach! die matte That / kan so hart zum Ziel hinkommen.", "tokens": ["a\u00b7ber", "ach", "!", "die", "mat\u00b7te", "That", "/", "kan", "so", "hart", "zum", "Ziel", "hin\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "ART", "ADJA", "NN", "$(", "VMFIN", "ADV", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "Mein Gott / der mit reichen Willen / ihm zu dienen mich begabt /", "tokens": ["Mein", "Gott", "/", "der", "mit", "rei\u00b7chen", "Wil\u00b7len", "/", "ihm", "zu", "die\u00b7nen", "mich", "be\u00b7gabt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ART", "APPR", "ADJA", "NN", "$(", "PPER", "PTKZU", "VVINF", "PPER", "ADJD", "$("], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "wird vor lauter werk es rechnen / was ich guts im Sinn gehabt.", "tokens": ["wird", "vor", "lau\u00b7ter", "werk", "es", "rech\u00b7nen", "/", "was", "ich", "guts", "im", "Sinn", "ge\u00b7habt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PIAT", "NN", "PPER", "VVINF", "$(", "PWS", "PPER", "NN", "APPRART", "NN", "VAPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Er wird meiner Schwachheit auch nicht nur verauch Kr\u00e4ffte geben /", "tokens": ["Er", "wird", "mei\u00b7ner", "Schwach\u00b7heit", "auch", "nicht", "nur", "ver\u00b7auch", "Kr\u00e4ff\u00b7te", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADV", "PTKNEG", "ADV", "ADJD", "NN", "VVINF", "$("], "meter": "-+--+---+--+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "da\u00df sie / alle Noht besiegt / wird in Ihm / in mir auch leben!", "tokens": ["da\u00df", "sie", "/", "al\u00b7le", "Noht", "be\u00b7siegt", "/", "wird", "in", "Ihm", "/", "in", "mir", "auch", "le\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "PIAT", "NN", "VVPP", "$(", "VAFIN", "APPR", "PPER", "$(", "APPR", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}