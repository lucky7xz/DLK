{"textgrid.poem.67736": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Beim heil'gen Feu'r, das Jovah in mich go\u00df,", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Beim heil'gen Feu'r, das Jovah in mich go\u00df,", "tokens": ["Beim", "heil'\u00b7gen", "Feu'r", ",", "das", "Jo\u00b7vah", "in", "mich", "go\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Beim Flammenmeer, aus dem es flo\u00df,", "tokens": ["Beim", "Flam\u00b7men\u00b7meer", ",", "aus", "dem", "es", "flo\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bei dieses Hauptes g\u00f6ttlichstem Gedanken,", "tokens": ["Bei", "die\u00b7ses", "Haup\u00b7tes", "g\u00f6tt\u00b7lichs\u00b7tem", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den selbst mein Engel mir als Lobschrift schrieb,", "tokens": ["Den", "selbst", "mein", "En\u00b7gel", "mir", "als", "Lob\u00b7schrift", "schrieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "PPER", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und seinem edelsten Gedanken,", "tokens": ["Und", "sei\u00b7nem", "e\u00b7dels\u00b7ten", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.6": {"text": "Der auch dem Busenfreund ein R\u00e4thsel blieb,", "tokens": ["Der", "auch", "dem", "Bu\u00b7sen\u00b7freund", "ein", "R\u00e4th\u00b7sel", "blieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Bei meines heil'gen Altars heil'gem Gold,", "tokens": ["Bei", "mei\u00b7nes", "heil'\u00b7gen", "Al\u00b7tars", "heil'\u00b7gem", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und dem er seinen Weihrauch zollt,", "tokens": ["Und", "dem", "er", "sei\u00b7nen", "Weih\u00b7rauch", "zollt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Bei mir, der in mir gilt,", "tokens": ["Bei", "mir", ",", "der", "in", "mir", "gilt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Schw\u00f6r' ich. Du, h\u00f6r es, der auf Feuerthronen", "tokens": ["Schw\u00f6r'", "ich", ".", "Du", ",", "h\u00f6r", "es", ",", "der", "auf", "Feu\u00b7er\u00b7thro\u00b7nen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "PPER", "$,", "VVFIN", "PPER", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Entschlu\u00df und That mit Gottes Wage wiegt,", "tokens": ["Ent\u00b7schlu\u00df", "und", "That", "mit", "Got\u00b7tes", "Wa\u00b7ge", "wiegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Ihr Tausend, die (ich feire) um mich wohnen,", "tokens": ["Ihr", "Tau\u00b7send", ",", "die", "(", "ich", "fei\u00b7re", ")", "um", "mich", "woh\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "$(", "PPER", "VVFIN", "$(", "KOUI", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Die Ihr vor meinem Wort auf Eurem Antlitz liegt,", "tokens": ["Die", "Ihr", "vor", "mei\u00b7nem", "Wort", "auf", "Eu\u00b7rem", "Ant\u00b7litz", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Du Geist, der meines Herzens Tiefen r\u00fcgt,", "tokens": ["Du", "Geist", ",", "der", "mei\u00b7nes", "Her\u00b7zens", "Tie\u00b7fen", "r\u00fcgt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "H\u00f6r's, Richter in mir, dessen Donnerstimme", "tokens": ["H\u00f6r's", ",", "Rich\u00b7ter", "in", "mir", ",", "des\u00b7sen", "Don\u00b7ner\u00b7stim\u00b7me"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "NN", "APPR", "PPER", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Wie Sturm von Ocean aufbr\u00fcllt,", "tokens": ["Wie", "Sturm", "von", "O\u00b7cean", "auf\u00b7br\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NE", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.17": {"text": "Dann, in Obadonnons Wolken schwarz verh\u00fcllt,", "tokens": ["Dann", ",", "in", "O\u00b7ba\u00b7don\u00b7nons", "Wol\u00b7ken", "schwarz", "ver\u00b7h\u00fcllt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.18": {"text": "Zum Himmel donnert, zum entschlafnen Grimme,", "tokens": ["Zum", "Him\u00b7mel", "don\u00b7nert", ",", "zum", "ent\u00b7schlaf\u00b7nen", "Grim\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Der meinem Haupt vielleicht schon sieben Schalen f\u00fcllt,", "tokens": ["Der", "mei\u00b7nem", "Haupt", "viel\u00b7leicht", "schon", "sie\u00b7ben", "Scha\u00b7len", "f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "ADV", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Du, eine Stimme, die in Nachtmelancholien", "tokens": ["Du", ",", "ei\u00b7ne", "Stim\u00b7me", ",", "die", "in", "Nacht\u00b7me\u00b7lan\u00b7cho\u00b7li\u00b7en"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Gespensterfurchtbar Rechnung mit mir h\u00e4lt,", "tokens": ["Ge\u00b7spens\u00b7ter\u00b7furcht\u00b7bar", "Rech\u00b7nung", "mit", "mir", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Mir meines Leichnams, ach! ermord'te Phantasien,", "tokens": ["Mir", "mei\u00b7nes", "Leich\u00b7nams", ",", "ach", "!", "er\u00b7mord'\u00b7te", "Phan\u00b7ta\u00b7si\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,", "ITJ", "$.", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Auch Eltern, Freund und meine Lasterwelt,", "tokens": ["Auch", "El\u00b7tern", ",", "Freund", "und", "mei\u00b7ne", "Las\u00b7ter\u00b7welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Den schwarzen Unmensch, mich, mir hinstellt;", "tokens": ["Den", "schwar\u00b7zen", "Un\u00b7mensch", ",", "mich", ",", "mir", "hin\u00b7stellt", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "$,", "PPER", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "Ach, Bild! \u2013 und schweigt! \u2013 O h\u00f6r's, allm\u00e4cht'ge Stimme,", "tokens": ["Ach", ",", "Bild", "!", "\u2013", "und", "schweigt", "!", "\u2013", "O", "h\u00f6r's", ",", "all\u00b7m\u00e4cht'\u00b7ge", "Stim\u00b7me", ","], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$.", "$(", "KON", "VVFIN", "$.", "$(", "NE", "NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Die mir, ich seh' und beb', gewi\u00df auch diese Zeugenwelt,", "tokens": ["Die", "mir", ",", "ich", "seh'", "und", "beb'", ",", "ge\u00b7wi\u00df", "auch", "die\u00b7se", "Zeu\u00b7gen\u00b7welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "PPER", "VVFIN", "KON", "VVFIN", "$,", "ADV", "ADV", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.27": {"text": "Gewi\u00df, gewi\u00df vor Augen stellt!", "tokens": ["Ge\u00b7wi\u00df", ",", "ge\u00b7wi\u00df", "vor", "Au\u00b7gen", "stellt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Ich beb'! der Richter, Engel, ich! \u2013 Ich h\u00f6re! \u2013", "tokens": ["Ich", "beb'", "!", "der", "Rich\u00b7ter", ",", "En\u00b7gel", ",", "ich", "!", "\u2013", "Ich", "h\u00f6\u00b7re", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "$,", "NE", "$,", "PPER", "$.", "$(", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Es donnert \u2013 um mich rauscht's wie Heere;", "tokens": ["Es", "don\u00b7nert", "\u2013", "um", "mich", "rauscht's", "wie", "Hee\u00b7re", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUI", "PPER", "VVFIN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "In mir wie still! Gott? Engel, ich, ich h\u00f6re!", "tokens": ["In", "mir", "wie", "still", "!", "Gott", "?", "En\u00b7gel", ",", "ich", ",", "ich", "h\u00f6\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "KOKOM", "ADJD", "$.", "NN", "$.", "NE", "$,", "PPER", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Erbebe, Herz, und schw\u00f6re!", "tokens": ["Er\u00b7be\u00b7be", ",", "Herz", ",", "und", "schw\u00f6\u00b7re", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "KON", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}