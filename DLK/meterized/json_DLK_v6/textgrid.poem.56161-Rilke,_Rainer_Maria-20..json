{"textgrid.poem.56161": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwischen den Sternen, wie weit; und doch, um wievieles noch weiter,", "tokens": ["Zwi\u00b7schen", "den", "Ster\u00b7nen", ",", "wie", "weit", ";", "und", "doch", ",", "um", "wie\u00b7vie\u00b7les", "noch", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "ADJD", "$.", "KON", "ADV", "$,", "KOUI", "ADV", "ADV", "ADV", "$,"], "meter": "+--+--+-+-++--+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "was man am Hiesigen lernt.", "tokens": ["was", "man", "am", "Hie\u00b7si\u00b7gen", "lernt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "APPRART", "NN", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Einer, zum Beispiel, ein Kind... und ein N\u00e4chster, ein Zweiter \u2013,", "tokens": ["Ei\u00b7ner", ",", "zum", "Bei\u00b7spiel", ",", "ein", "Kind", "...", "und", "ein", "N\u00e4chs\u00b7ter", ",", "ein", "Zwei\u00b7ter", "\u2013", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PIS", "$,", "APPRART", "NN", "$,", "ART", "NN", "$(", "KON", "ART", "NN", "$,", "ART", "NN", "$(", "$,"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.4": {"text": "o wie unfa\u00dflich entfernt.", "tokens": ["o", "wie", "un\u00b7fa\u00df\u00b7lich", "ent\u00b7fernt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM", "KOKOM", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schicksal, es mi\u00dft uns vielleicht mit des Seienden Spanne,", "tokens": ["Schick\u00b7sal", ",", "es", "mi\u00dft", "uns", "viel\u00b7leicht", "mit", "des", "Sei\u00b7en\u00b7den", "Span\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+---+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "da\u00df es uns fremd erscheint;", "tokens": ["da\u00df", "es", "uns", "fremd", "er\u00b7scheint", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "denk, wieviel Spannen allein vom M\u00e4dchen zum Manne,", "tokens": ["denk", ",", "wie\u00b7viel", "Span\u00b7nen", "al\u00b7lein", "vom", "M\u00e4d\u00b7chen", "zum", "Man\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAT", "NN", "ADV", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "wenn es ihn meidet und meint.", "tokens": ["wenn", "es", "ihn", "mei\u00b7det", "und", "meint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Alles ist weit \u2013, und nirgends schlie\u00dft sich der Kreis.", "tokens": ["Al\u00b7les", "ist", "weit", "\u2013", ",", "und", "nir\u00b7gends", "schlie\u00dft", "sich", "der", "Kreis", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADJD", "$(", "$,", "KON", "ADV", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Sieh in der Sch\u00fcssel, auf heiter bereitetem Tische,", "tokens": ["Sieh", "in", "der", "Sch\u00fcs\u00b7sel", ",", "auf", "hei\u00b7ter", "be\u00b7rei\u00b7te\u00b7tem", "Ti\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.3": {"text": "seltsam der Fische Gesicht.", "tokens": ["selt\u00b7sam", "der", "Fi\u00b7sche", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Fische sind stumm..., meinte man einmal. Wer wei\u00df?", "tokens": ["Fi\u00b7sche", "sind", "stumm", "...", ",", "mein\u00b7te", "man", "ein\u00b7mal", ".", "Wer", "wei\u00df", "?"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "$(", "$,", "VVFIN", "PIS", "ADV", "$.", "PWS", "VVFIN", "$."], "meter": "+--++-+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Aber ist nicht am Ende ein Ort, wo man das, was der Fische", "tokens": ["A\u00b7ber", "ist", "nicht", "am", "En\u00b7de", "ein", "Ort", ",", "wo", "man", "das", ",", "was", "der", "Fi\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VAFIN", "PTKNEG", "APPRART", "NN", "ART", "NN", "$,", "PWAV", "PIS", "PDS", "$,", "PRELS", "ART", "NN"], "meter": "+--+-+--+--++-+-", "measure": "iambic.septa.invert"}, "line.3": {"text": "Sprache w\u00e4re, ", "tokens": ["Spra\u00b7che", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}}}}}