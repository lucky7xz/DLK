{"textgrid.poem.43436": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die ganze deutsche Litteratur", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die ganze deutsche Litteratur", "tokens": ["Die", "gan\u00b7ze", "deut\u00b7sche", "Lit\u00b7te\u00b7ra\u00b7tur"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ist leider f\u00fcr Gelehrte nur.", "tokens": ["Ist", "lei\u00b7der", "f\u00fcr", "Ge\u00b7lehr\u00b7te", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gelehrte haben sie gemacht,", "tokens": ["Ge\u00b7lehr\u00b7te", "ha\u00b7ben", "sie", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nie dabei ans Volk gedacht.", "tokens": ["Und", "nie", "da\u00b7bei", "ans", "Volk", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PAV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was n\u00fctzet Wissenschaft und Kunst?", "tokens": ["Was", "n\u00fct\u00b7zet", "Wis\u00b7sen\u00b7schaft", "und", "Kunst", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das ist ja eitel Schein und Dunst,", "tokens": ["Das", "ist", "ja", "ei\u00b7tel", "Schein", "und", "Dunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn beides nicht zum Volke dringt,", "tokens": ["Wenn", "bei\u00b7des", "nicht", "zum", "Vol\u00b7ke", "dringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr all' und jeden Fr\u00fcchte bringt.", "tokens": ["F\u00fcr", "all'", "und", "je\u00b7den", "Fr\u00fcch\u00b7te", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was n\u00fctzt dem Volke der Poet,", "tokens": ["Was", "n\u00fctzt", "dem", "Vol\u00b7ke", "der", "Po\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn's Volk sein Singen nicht versteht?", "tokens": ["Wenn's", "Volk", "sein", "Sin\u00b7gen", "nicht", "ver\u00b7steht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ins Herz des Volkes drang noch nie", "tokens": ["Ins", "Herz", "des", "Vol\u00b7kes", "drang", "noch", "nie"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gelehrter Herren Poesie.", "tokens": ["Ge\u00b7lehr\u00b7ter", "Her\u00b7ren", "Poe\u00b7sie", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "La\u00dft euern Wissensqualm und Dunst,", "tokens": ["La\u00dft", "eu\u00b7ern", "Wis\u00b7sens\u00b7qualm", "und", "Dunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und \u00fcbet reine deutsche Kunst!", "tokens": ["Und", "\u00fc\u00b7bet", "rei\u00b7ne", "deut\u00b7sche", "Kunst", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Werft allen Plunder \u00fcber Bord,", "tokens": ["Werft", "al\u00b7len", "Plun\u00b7der", "\u00fc\u00b7ber", "Bord", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Singt ein verst\u00e4ndlich deutsches Wort!", "tokens": ["Singt", "ein", "ver\u00b7st\u00e4nd\u00b7lich", "deut\u00b7sches", "Wort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}