{"dta.poem.21761": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.57", "af:0.42"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Jn dehm der Mars die Paukken", "tokens": ["In", "dehm", "der", "Mars", "die", "Pauk\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "schl\u00e4get", "tokens": ["schl\u00e4\u00b7get"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "und die Kartaune t\u00f6hnt den groben Lei-", "tokens": ["und", "die", "Kar\u00b7tau\u00b7ne", "t\u00f6hnt", "den", "gro\u00b7ben", "Lei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "chenklang", "tokens": ["chen\u00b7klang"], "token_info": ["word"], "pos": ["NE"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "wirstu mit Liebes-Gluht beweget", "tokens": ["wirs\u00b7tu", "mit", "Lie\u00b7bes\u00b7Gluht", "be\u00b7we\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "und spielest fr\u00f6lich her den schmeichelnden", "tokens": ["und", "spie\u00b7lest", "fr\u00f6\u00b7lich", "her", "den", "schmei\u00b7cheln\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Gesang", "tokens": ["Ge\u00b7sang"], "token_info": ["word"], "pos": ["NN"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "den die Liebinne singet", "tokens": ["den", "die", "Lie\u00b7bin\u00b7ne", "sin\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "VVFIN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "wenn sie dem Adon ein St\u00e4ndchen", "tokens": ["wenn", "sie", "dem", "A\u00b7don", "ein", "St\u00e4nd\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "bringet.", "tokens": ["brin\u00b7get", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Wenn die Gequetschten sterben kla-", "tokens": ["Wenn", "die", "Ge\u00b7quetschten", "ster\u00b7ben", "kla"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVINF", "TRUNC"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "gen", "tokens": ["gen"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.13": {"text": "so legestu es auff die Liebes-seuffzer", "tokens": ["so", "le\u00b7ge\u00b7stu", "es", "auff", "die", "Lie\u00b7bes\u00b7seuff\u00b7zer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "au\u00df", "tokens": ["au\u00df"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.15": {"text": "sichstu ein Werk zusammen tragen", "tokens": ["sichs\u00b7tu", "ein", "Werk", "zu\u00b7sam\u00b7men", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "so meinestu es sey Rosillen Herzen-", "tokens": ["so", "mei\u00b7nes\u00b7tu", "es", "sey", "Ro\u00b7sil\u00b7len", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VAFIN", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Hau\u00df", "tokens": ["Hau\u00df"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.18": {"text": "das du durch Sturm und Siegen", "tokens": ["das", "du", "durch", "Sturm", "und", "Sie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "offt mit Tapferkeit hast \u00fcberstiegen.", "tokens": ["offt", "mit", "Tap\u00b7fer\u00b7keit", "hast", "\u00fc\u00b7bers\u00b7tie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Der kleine Sch\u00fczz hat dich besessen", "tokens": ["Der", "klei\u00b7ne", "Sch\u00fczz", "hat", "dich", "be\u00b7ses\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "er macht dich taub und blind im mitten", "tokens": ["er", "macht", "dich", "taub", "und", "blind", "im", "mit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "APPRART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "der Gefahr", "tokens": ["der", "Ge\u00b7fahr"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.23": {"text": "wie k\u00f6ntstu sonst so sein vermessen", "tokens": ["wie", "k\u00f6nts\u00b7tu", "sonst", "so", "sein", "ver\u00b7mes\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "ADV", "ADV", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "zu schreiben von der Lieb\u2019 in der verdoll-", "tokens": ["zu", "schrei\u00b7ben", "von", "der", "Lieb'", "in", "der", "ver\u00b7doll"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "ten 'Schaar", "tokens": ["ten", "'schaar"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.26": {"text": "wo Barbarey und Schrekken", "tokens": ["wo", "Bar\u00b7ba\u00b7rey", "und", "Schrek\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "und der nahe Tod lebt aller Ekken.", "tokens": ["und", "der", "na\u00b7he", "Tod", "lebt", "al\u00b7ler", "Ek\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.28": {"text": "Es ist die freche Lieb\u2019 alleine", "tokens": ["Es", "ist", "die", "fre\u00b7che", "Lieb'", "al\u00b7lei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "die Blizz und Pulver trozzt und auf die", "tokens": ["die", "Blizz", "und", "Pul\u00b7ver", "trozzt", "und", "auf", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "KON", "APPR", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Kugeln lacht", "tokens": ["Ku\u00b7geln", "lacht"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.31": {"text": "sie spottet grober M\u00f6rser-steine", "tokens": ["sie", "spot\u00b7tet", "gro\u00b7ber", "M\u00f6r\u00b7ser\u00b7stei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "und h\u00fctet frisch des Tohrs/ wenn die Pe-", "tokens": ["und", "h\u00fc\u00b7tet", "frisch", "des", "Tohrs", "/", "wenn", "die", "Pe"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "$(", "KOUS", "ART", "TRUNC"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.33": {"text": "tarde kracht/", "tokens": ["tar\u00b7de", "kracht", "/"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.34": {"text": "sie kan mit Freuden singen", "tokens": ["sie", "kan", "mit", "Freu\u00b7den", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "wenn Schekkan und Sebel m\u00f6rdlich", "tokens": ["wenn", "Schek\u00b7kan", "und", "Se\u00b7bel", "m\u00f6rd\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "KON", "NN", "ADJD"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.36": {"text": "klingen.", "tokens": ["klin\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.37": {"text": "Der Amor schenkt nu nicht mehr", "tokens": ["Der", "A\u00b7mor", "schenkt", "nu", "nicht", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "ADV", "PTKNEG", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.38": {"text": "Myrten", "tokens": ["Myr\u00b7ten"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.39": {"text": "di\u00df ist ein schlechtes Tuhn um so viel", "tokens": ["di\u00df", "ist", "ein", "schlech\u00b7tes", "Tuhn", "um", "so", "viel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "APPR", "ADV", "ADV"], "meter": "---+-+--+", "measure": "iambic.tri.chol"}, "line.40": {"text": "Kunst und Flei\u00df", "tokens": ["Kunst", "und", "Flei\u00df"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.41": {"text": "mit Lorbeer wil er den umg\u00fcrten", "tokens": ["mit", "Lor\u00b7beer", "wil", "er", "den", "um\u00b7g\u00fcr\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VMFIN", "PPER", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "der seines Nahmens Ruhm der Welt zu", "tokens": ["der", "sei\u00b7nes", "Nah\u00b7mens", "Ruhm", "der", "Welt", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "NN", "ART", "NN", "APPR"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "melden wei\u00df", "tokens": ["mel\u00b7den", "wei\u00df"], "token_info": ["word", "word"], "pos": ["VVINF", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.44": {"text": "den hastu l\u00e4ngst verdienet/", "tokens": ["den", "has\u00b7tu", "l\u00e4ngst", "ver\u00b7die\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADV", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.45": {"text": "auch/ eh dir Rosille war vers\u00fchnet.", "tokens": ["auch", "/", "eh", "dir", "Ro\u00b7sil\u00b7le", "war", "ver\u00b7s\u00fch\u00b7net", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOUS", "PPER", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}