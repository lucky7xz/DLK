{"dta.poem.2803": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein nachgesprochenes Gebet kann etwa n\u00fctzen,", "tokens": ["Ein", "nach\u00b7ge\u00b7spro\u00b7che\u00b7nes", "Ge\u00b7bet", "kann", "et\u00b7wa", "n\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als Zaubersegen, dich mit Wunderkraft besch\u00fctzen.", "tokens": ["Als", "Zau\u00b7ber\u00b7se\u00b7gen", ",", "dich", "mit", "Wun\u00b7der\u00b7kraft", "be\u00b7sch\u00fct\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein nachgebetet Wort der Lehre n\u00fctzet nicht,", "tokens": ["Ein", "nach\u00b7ge\u00b7be\u00b7tet", "Wort", "der", "Leh\u00b7re", "n\u00fct\u00b7zet", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ART", "NN", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn in dir selbst den vorgesprochnen nichts entspricht.", "tokens": ["Wenn", "in", "dir", "selbst", "den", "vor\u00b7ge\u00b7sproch\u00b7nen", "nichts", "ent\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "ADV", "ART", "ADJA", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der eingepflanzte Stab mag wohl die Pflanze tragen,", "tokens": ["Der", "ein\u00b7ge\u00b7pflanz\u00b7te", "Stab", "mag", "wohl", "die", "Pflan\u00b7ze", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Pflanze doch mu\u00df, um zu wurzeln, Wurzel schlagen.", "tokens": ["Die", "Pflan\u00b7ze", "doch", "mu\u00df", ",", "um", "zu", "wur\u00b7zeln", ",", "Wur\u00b7zel", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VMFIN", "$,", "KOUI", "PTKZU", "VVINF", "$,", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}