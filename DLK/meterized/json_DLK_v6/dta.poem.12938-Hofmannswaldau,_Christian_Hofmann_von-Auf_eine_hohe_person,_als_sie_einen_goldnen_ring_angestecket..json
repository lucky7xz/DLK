{"dta.poem.12938": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf eine hohe person, als sie einen  \n goldnen ring angestecket.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was macht der goldne ring in deiner netten hand?", "tokens": ["Was", "macht", "der", "gold\u00b7ne", "ring", "in", "dei\u00b7ner", "net\u00b7ten", "hand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er ist zu schlecht den finger zu umschlie\u00dfen,", "tokens": ["Er", "ist", "zu", "schlecht", "den", "fin\u00b7ger", "zu", "um\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "ART", "ADJA", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den lieb und sch\u00f6nheit selbst begierig sind zu k\u00fcssen;", "tokens": ["Den", "lieb", "und", "sch\u00f6n\u00b7heit", "selbst", "be\u00b7gie\u00b7rig", "sind", "zu", "k\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "NN", "ADV", "ADJD", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch ist mir auch kein sch\u00f6nerer bekant.", "tokens": ["Doch", "ist", "mir", "auch", "kein", "sch\u00f6\u00b7ne\u00b7rer", "be\u00b7kant", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PIAT", "ADJA", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es wollte denn des himmels reiner crei\u00df", "tokens": ["Es", "woll\u00b7te", "denn", "des", "him\u00b7mels", "rei\u00b7ner", "crei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sich in dergleichen enge fassen,", "tokens": ["Sich", "in", "derg\u00b7lei\u00b7chen", "en\u00b7ge", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PIS", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und vor den diamant durch wunder-vollen flei\u00df,", "tokens": ["Und", "vor", "den", "di\u00b7a\u00b7mant", "durch", "wun\u00b7der\u00b7vol\u00b7len", "flei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der sonne mittel-punct darein versetzen lassen.", "tokens": ["Der", "son\u00b7ne", "mit\u00b7tel\u00b7punct", "da\u00b7rein", "ver\u00b7set\u00b7zen", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}