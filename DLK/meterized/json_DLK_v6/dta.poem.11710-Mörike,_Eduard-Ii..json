{"dta.poem.11710": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Ii.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Aufgeschm\u00fcckt ist der Freudensaal.", "tokens": ["Auf\u00b7ge\u00b7schm\u00fcckt", "ist", "der", "Freu\u00b7den\u00b7saal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Lichterhell, bunt, in laulicher Sommernacht", "tokens": ["Lich\u00b7ter\u00b7hell", ",", "bunt", ",", "in", "lau\u00b7li\u00b7cher", "Som\u00b7mer\u00b7nacht"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "$,", "APPR", "ADJA", "NN"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Stehet das offene Gartengezelte;", "tokens": ["Ste\u00b7het", "das", "of\u00b7fe\u00b7ne", "Gar\u00b7ten\u00b7ge\u00b7zel\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "S\u00e4ulengleich steigen,", "tokens": ["S\u00e4u\u00b7len\u00b7gleich", "stei\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Reichlich durchwirket mit Laubwerk,", "tokens": ["Reich\u00b7lich", "durch\u00b7wir\u00b7ket", "mit", "Laub\u00b7werk", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Die stolzen Leiber", "tokens": ["Die", "stol\u00b7zen", "Lei\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Sechs gez\u00e4hmter, riesiger Schlangen,", "tokens": ["Sechs", "ge\u00b7z\u00e4hm\u00b7ter", ",", "rie\u00b7si\u00b7ger", "Schlan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Tragend und st\u00fctzend das", "tokens": ["Tra\u00b7gend", "und", "st\u00fct\u00b7zend", "das"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "ART"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.9": {"text": "Leichtgegitterte Dach.", "tokens": ["Leicht\u00b7ge\u00b7git\u00b7ter\u00b7te", "Dach", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Aber die Braut noch wartet bescheiden", "tokens": ["A\u00b7ber", "die", "Braut", "noch", "war\u00b7tet", "be\u00b7schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "VVINF"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "In dem K\u00e4mmerlein ihres Hauses;", "tokens": ["In", "dem", "K\u00e4m\u00b7mer\u00b7lein", "ih\u00b7res", "Hau\u00b7ses", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "--+-++-+-", "measure": "anapaest.init"}, "line.3": {"text": "Endlich bewegt sich der Zug der Hochzeit,", "tokens": ["End\u00b7lich", "be\u00b7wegt", "sich", "der", "Zug", "der", "Hoch\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Fackeln tragend,", "tokens": ["Fa\u00b7ckeln", "tra\u00b7gend", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Feierlich stumm.", "tokens": ["Fei\u00b7er\u00b7lich", "stumm", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Und in der Mitte,", "tokens": ["Und", "in", "der", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,"], "meter": "++-+-", "measure": "iambic.di"}, "line.7": {"text": "Mich an der rechten Hand,", "tokens": ["Mich", "an", "der", "rech\u00b7ten", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Schwarzgekleidet geht einfach die Braut,", "tokens": ["Schwarz\u00b7ge\u00b7klei\u00b7det", "geht", "ein\u00b7fach", "die", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Sch\u00f6ngefaltet ein Scharlachtuch", "tokens": ["Sch\u00f6n\u00b7ge\u00b7fal\u00b7tet", "ein", "Schar\u00b7lach\u00b7tuch"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.10": {"text": "Liegt um den zierlichen Kopf geschlagen;", "tokens": ["Liegt", "um", "den", "zier\u00b7li\u00b7chen", "Kopf", "ge\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "L\u00e4chelnd geht sie dahin;", "tokens": ["L\u00e4\u00b7chelnd", "geht", "sie", "da\u00b7hin", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PAV", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.12": {"text": "Das Mahl schon duftet.", "tokens": ["Das", "Mahl", "schon", "duf\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Sp\u00e4ter im L\u00e4rmen des Fests", "tokens": ["Sp\u00e4\u00b7ter", "im", "L\u00e4r\u00b7men", "des", "Fests"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Stahlen wir seitw\u00e4rts uns Beide", "tokens": ["Stah\u00b7len", "wir", "seit\u00b7w\u00e4rts", "uns", "Bei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "PIS"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Weg, nach den Schatten des Gartens wandelnd,", "tokens": ["Weg", ",", "nach", "den", "Schat\u00b7ten", "des", "Gar\u00b7tens", "wan\u00b7delnd", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Wo im Geb\u00fcsche die Rosen brannten,", "tokens": ["Wo", "im", "Ge\u00b7b\u00fc\u00b7sche", "die", "Ro\u00b7sen", "brann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wo der Mondstrahl um Lilien zuckte,", "tokens": ["Wo", "der", "Mond\u00b7strahl", "um", "Li\u00b7li\u00b7en", "zuck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Wo die B\u00e4ume vom Nachtthau troffen.", "tokens": ["Wo", "die", "B\u00e4u\u00b7me", "vom", "Nachtt\u00b7hau", "trof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Und nun strich sie mir, stillestehend,", "tokens": ["Und", "nun", "strich", "sie", "mir", ",", "stil\u00b7les\u00b7te\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "$,", "ADJD", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Seltsamen Blicks mit dem Finger die Schl\u00e4fe,", "tokens": ["Selt\u00b7sa\u00b7men", "Blicks", "mit", "dem", "Fin\u00b7ger", "die", "Schl\u00e4\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "J\u00e4hlings versank ich in tiefen Schlummer,", "tokens": ["J\u00e4h\u00b7lings", "ver\u00b7sank", "ich", "in", "tie\u00b7fen", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Aber gest\u00e4rkt vom Wunderschlafe", "tokens": ["A\u00b7ber", "ge\u00b7st\u00e4rkt", "vom", "Wun\u00b7der\u00b7schla\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVPP", "APPRART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Bin ich erwacht zu gl\u00fcckseligen Tagen,", "tokens": ["Bin", "ich", "er\u00b7wacht", "zu", "gl\u00fcck\u00b7se\u00b7li\u00b7gen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "F\u00fchrte die seltsame Braut in mein Haus ein.", "tokens": ["F\u00fchr\u00b7te", "die", "selt\u00b7sa\u00b7me", "Braut", "in", "mein", "Haus", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}}}}