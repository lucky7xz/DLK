{"dta.poem.2518": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf der Durchl. F\u00fcrstin und Fr.  \n  Frauen Eleonoren Charlotten/ gebohrner  \n und verm\u00e4hlter Hertzogin zu W\u00fcrtemberg/ Teck und  \n  Chastillon  auch in Schlesien zur Oel\u00df/ rc. erschienenen  \n Geburts-Tag 1680. im Namen Jhrer Hoch-  \n F\u00fcrstl: Durchl. Durchlauchtigsten  \n Gemahl.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So sag\u2019 ich di\u00df geschicht/ ", "tokens": ["So", "sag'", "ich", "di\u00df", "ge\u00b7schicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDS", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Du Wunder dieser Welt/ du Pallas unsrer Zeit/", "tokens": ["Du", "Wun\u00b7der", "die\u00b7ser", "Welt", "/", "du", "Pal\u00b7las", "uns\u00b7rer", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PDAT", "NN", "$(", "PPER", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du Perle meiner Lust/ Bild der Vollkommenheit/", "tokens": ["Du", "Per\u00b7le", "mei\u00b7ner", "Lust", "/", "Bild", "der", "Voll\u00b7kom\u00b7men\u00b7heit", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPOSAT", "NN", "$(", "NN", "ART", "NN", "$("], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Du vierdte Gratie und mehr als andre Flore", "tokens": ["Du", "vierd\u00b7te", "Gra\u00b7tie", "und", "mehr", "als", "and\u00b7re", "Flo\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "KON", "ADV", "KOUS", "ADJA", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Nur eintzig dir zum Ruhm/ weil deine G\u00f6ttlichkeit", "tokens": ["Nur", "eint\u00b7zig", "dir", "zum", "Ruhm", "/", "weil", "dei\u00b7ne", "G\u00f6tt\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "APPRART", "NN", "$(", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So wol der Erden Rund/ als Sternen-Krei\u00df erfreut.", "tokens": ["So", "wol", "der", "Er\u00b7den", "Rund", "/", "als", "Ster\u00b7nen\u00b7Krei\u00df", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ADJD", "$(", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dein erster Ursprung ist ja von dem Himmel kommen/", "tokens": ["Dein", "ers\u00b7ter", "Ur\u00b7sprung", "ist", "ja", "von", "dem", "Him\u00b7mel", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dein Himmel-hoher Geist hengt gleicher Regung nach.", "tokens": ["Dein", "Him\u00b7mel\u00b7ho\u00b7her", "Geist", "hengt", "glei\u00b7cher", "Re\u00b7gung", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Erlauchte Seelen ziehn in kein gemeines Dach/", "tokens": ["Er\u00b7lauch\u00b7te", "See\u00b7len", "ziehn", "in", "kein", "ge\u00b7mei\u00b7nes", "Dach", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es wird der beste Zeug zu ihrem Hau\u00df genommen.", "tokens": ["Es", "wird", "der", "bes\u00b7te", "Zeug", "zu", "ih\u00b7rem", "Hau\u00df", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Sterne stimmten bald durch ihren Einflu\u00df bey/", "tokens": ["Die", "Ster\u00b7ne", "stimm\u00b7ten", "bald", "durch", "ih\u00b7ren", "Ein\u00b7flu\u00df", "bey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df die ", "tokens": ["Da\u00df", "die"], "token_info": ["word", "word"], "pos": ["KOUS", "ART"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Charlotte liebste Seel' mein einig Hertz und Leben", "tokens": ["Char\u00b7lot\u00b7te", "liebs\u00b7te", "Seel'", "mein", "ei\u00b7nig", "Hertz", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "PPOSAT", "ADJD", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein hoher F\u00fcrsten Stamm/ den Auff- und Untergang", "tokens": ["Dein", "ho\u00b7her", "F\u00fcrs\u00b7ten", "Stamm", "/", "den", "Auf\u00b7f", "und", "Un\u00b7ter\u00b7gang"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "$(", "ART", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "In tieffer Demuth ehrt/ und den der Nach-Welt Klang", "tokens": ["In", "tief\u00b7fer", "De\u00b7muth", "ehrt", "/", "und", "den", "der", "Nach\u00b7Welt", "Klang"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$(", "KON", "ART", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bey S\u00fcden und bey Nord nach W\u00fcrden wird erheben", "tokens": ["Bey", "S\u00fc\u00b7den", "und", "bey", "Nord", "nach", "W\u00fcr\u00b7den", "wird", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NE", "APPR", "NN", "VAFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "H\u00fcllt\u2019 in der Wiege zwar dich schon in Purpur ein;", "tokens": ["H\u00fcllt'", "in", "der", "Wie\u00b7ge", "zwar", "dich", "schon", "in", "Pur\u00b7pur", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADV", "PPER", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch muste Tugend da die treuest\u2019 Amme seyn.", "tokens": ["Doch", "mus\u00b7te", "Tu\u00b7gend", "da", "die", "treu\u00b7est'", "Am\u00b7me", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Du Himmel-werthes Pfand zu Nutz der Welt gelihen/", "tokens": ["Du", "Him\u00b7mel\u00b7wert\u00b7hes", "Pfand", "zu", "Nutz", "der", "Welt", "ge\u00b7li\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Minerva legte dich beh\u00e4glich an die Brust/", "tokens": ["Mi\u00b7ner\u00b7va", "leg\u00b7te", "dich", "be\u00b7h\u00e4g\u00b7lich", "an", "die", "Brust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "ADJD", "APPR", "ART", "NN", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Die keusche Musen-Schaar sah\u2019 ihre Hertzens Lust", "tokens": ["Die", "keu\u00b7sche", "Mu\u00b7sen\u00b7Schaar", "sah'", "ih\u00b7re", "Hert\u00b7zens", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als ein neu Helikon umb dich fing an zubl\u00fchen;", "tokens": ["Als", "ein", "neu", "He\u00b7li\u00b7kon", "umb", "dich", "fing", "an", "zu\u00b7bl\u00fc\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "NN", "APPR", "PPER", "VVFIN", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Sinnen Trefligkeit/ der himmlische Verstand", "tokens": ["Der", "Sin\u00b7nen", "Tre\u00b7flig\u00b7keit", "/", "der", "himm\u00b7li\u00b7sche", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Bezeugte da\u00df dein Geist nur G\u00f6ttern sey verwand.", "tokens": ["Be\u00b7zeug\u00b7te", "da\u00df", "dein", "Geist", "nur", "G\u00f6t\u00b7tern", "sey", "ver\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "PPOSAT", "NN", "ADV", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Apollo wich vom Thron in Gold-beflammten Haaren", "tokens": ["A\u00b7pol\u00b7lo", "wich", "vom", "Thron", "in", "Gold\u00b7be\u00b7flamm\u00b7ten", "Haa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und hieng sein Lauten Spiel an einen Lorber-Baum\u2019/", "tokens": ["Und", "hieng", "sein", "Lau\u00b7ten", "Spiel", "an", "ei\u00b7nen", "Lor\u00b7ber\u00b7Baum'", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er rief/ ihr Clarien/ macht dieser ", "tokens": ["Er", "rief", "/", "ihr", "Cla\u00b7ri\u00b7en", "/", "macht", "die\u00b7ser"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "PPOSAT", "NN", "$(", "VVFIN", "PDAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie wird als ", "tokens": ["Sie", "wird", "als"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "KOKOM"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Sie ist es/ die euch schenckt aufs neue Seel\u2019 und Geist", "tokens": ["Sie", "ist", "es", "/", "die", "euch", "schenckt", "aufs", "neu\u00b7e", "Seel'", "und", "Geist"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "$(", "PRELS", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und unter ihrem Schirm vergn\u00fcget wohnen heist/", "tokens": ["Und", "un\u00b7ter", "ih\u00b7rem", "Schirm", "ver\u00b7gn\u00fc\u00b7get", "woh\u00b7nen", "heist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "VVINF", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der Au\u00dfgang kr\u00f6nt die That. ", "tokens": ["Der", "Au\u00df\u00b7gang", "kr\u00f6nt", "die", "That", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Den Krei\u00df der Wissenschafft/ den Circkel dieser Welt/", "tokens": ["Den", "Krei\u00df", "der", "Wis\u00b7sen\u00b7schafft", "/", "den", "Cir\u00b7ckel", "die\u00b7ser", "Welt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$(", "ART", "NN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nichts ist so tief versteckt/ nichts ist so hoch gestellt/", "tokens": ["Nichts", "ist", "so", "tief", "ver\u00b7steckt", "/", "nichts", "ist", "so", "hoch", "ge\u00b7stellt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "ADJD", "VVPP", "$(", "PIS", "VAFIN", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nichts wird manch kluger Kopff in der Erfindung zeigen/", "tokens": ["Nichts", "wird", "manch", "klu\u00b7ger", "Kopff", "in", "der", "Er\u00b7fin\u00b7dung", "zei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PIAT", "ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das ihre F\u00e4higkeit nicht zu entscheiden wei\u00df/", "tokens": ["Das", "ih\u00b7re", "F\u00e4\u00b7hig\u00b7keit", "nicht", "zu", "ent\u00b7schei\u00b7den", "wei\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und Wercke voller Witz belohnt mit Danck und Prei\u00df.", "tokens": ["Und", "Wer\u00b7cke", "vol\u00b7ler", "Witz", "be\u00b7lohnt", "mit", "Danck", "und", "Prei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJA", "NN", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Was aber z\u00fcnd ich doch ", "tokens": ["Was", "a\u00b7ber", "z\u00fcnd", "ich", "doch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Bey einem hellen Tag der Sonnen Fackeln an?", "tokens": ["Bey", "ei\u00b7nem", "hel\u00b7len", "Tag", "der", "Son\u00b7nen", "Fa\u00b7ckeln", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach Leit-Stern meiner Ruh/ mit dem ists nicht gethan", "tokens": ["Ach", "Leit\u00b7S\u00b7tern", "mei\u00b7ner", "Ruh", "/", "mit", "dem", "ists", "nicht", "ge\u00b7than"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "PPOSAT", "NN", "$(", "APPR", "ART", "VAFIN", "PTKNEG", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was gleich Apollo singt mit seinem gantzen Chore.", "tokens": ["Was", "gleich", "A\u00b7pol\u00b7lo", "singt", "mit", "sei\u00b7nem", "gant\u00b7zen", "Cho\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NE", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nein; viel ein st\u00e4rcker Zug und W\u00fcrckung aus der H\u00f6h", "tokens": ["Nein", ";", "viel", "ein", "st\u00e4r\u00b7cker", "Zug", "und", "W\u00fcr\u00b7ckung", "aus", "der", "H\u00f6h"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "ADV", "ART", "ADJA", "NN", "KON", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Erfordert da\u00df ich heut dein Freuden-Fest begeh?", "tokens": ["Er\u00b7for\u00b7dert", "da\u00df", "ich", "heut", "dein", "Freu\u00b7den\u00b7Fest", "be\u00b7geh", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Es wil die Liebe selbst mir Hand und Feder leiten/", "tokens": ["Es", "wil", "die", "Lie\u00b7be", "selbst", "mir", "Hand", "und", "Fe\u00b7der", "lei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "PPER", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und die Eintr\u00e4chtigkeit hat ein Altar geba