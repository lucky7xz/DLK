{"textgrid.poem.50717": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es d\u00e4mmert, es d\u00e4mmert den See herab,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es d\u00e4mmert, es d\u00e4mmert den See herab,", "tokens": ["Es", "d\u00e4m\u00b7mert", ",", "es", "d\u00e4m\u00b7mert", "den", "See", "her\u00b7ab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die Wasser sind gar so dunkel;", "tokens": ["Die", "Was\u00b7ser", "sind", "gar", "so", "dun\u00b7kel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch wann der Blitz \u00fcber den Bergen strahlt,", "tokens": ["Doch", "wann", "der", "Blitz", "\u00fc\u00b7ber", "den", "Ber\u00b7gen", "strahlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Was ist das f\u00fcr ein Gefunkel!", "tokens": ["Was", "ist", "das", "f\u00fcr", "ein", "Ge\u00b7fun\u00b7kel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Dann tun dem Schiffer die Augen weh,", "tokens": ["Dann", "tun", "dem", "Schif\u00b7fer", "die", "Au\u00b7gen", "weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er sputet sich \u00e4ngstlich zu Lande,", "tokens": ["Er", "spu\u00b7tet", "sich", "\u00e4ngst\u00b7lich", "zu", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wo gaffend der Feierabend steht", "tokens": ["Wo", "gaf\u00b7fend", "der", "Fei\u00b7er\u00b7a\u00b7bend", "steht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVPP", "ART", "NN", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Am fahlerleuchteten Strande.", "tokens": ["Am", "fah\u00b7ler\u00b7leuch\u00b7te\u00b7ten", "Stran\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Die Leute freuen und f\u00fcrchten sich", "tokens": ["Die", "Leu\u00b7te", "freu\u00b7en", "und", "f\u00fcrch\u00b7ten", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "KON", "VVFIN", "PRF"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und w\u00fcnschen ein gutes Ende", "tokens": ["Und", "w\u00fcn\u00b7schen", "ein", "gu\u00b7tes", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und da\u00df der Herr sein Schlo\u00dfengericht", "tokens": ["Und", "da\u00df", "der", "Herr", "sein", "Schlo\u00b7\u00dfen\u00b7ge\u00b7richt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Nicht in den Krautgarten sende!", "tokens": ["Nicht", "in", "den", "Kr\u00b7aut\u00b7gar\u00b7ten", "sen\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Jetzt zischt der Strahl in die laue Flut", "tokens": ["Jetzt", "zischt", "der", "Strahl", "in", "die", "lau\u00b7e", "Flut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie eine feurige Kette,", "tokens": ["Wie", "ei\u00b7ne", "feu\u00b7ri\u00b7ge", "Ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der dumme Haufen ergreift die Flucht", "tokens": ["Der", "dum\u00b7me", "Hau\u00b7fen", "er\u00b7greift", "die", "Flucht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und kriecht erschrocken zu Bette!", "tokens": ["Und", "kriecht", "er\u00b7schro\u00b7cken", "zu", "Bet\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Wann Gott einen guten Gedanken hat,", "tokens": ["Wann", "Gott", "ei\u00b7nen", "gu\u00b7ten", "Ge\u00b7dan\u00b7ken", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Dann sagt man: Es wetterleuchtet!", "tokens": ["Dann", "sagt", "man", ":", "Es", "wet\u00b7ter\u00b7leuch\u00b7tet", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$.", "PPER", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Gib acht, du Gesindel, da\u00df nicht einmal", "tokens": ["Gib", "acht", ",", "du", "Ge\u00b7sin\u00b7del", ",", "da\u00df", "nicht", "ein\u00b7mal"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "CARD", "$,", "PPER", "NN", "$,", "KOUS", "PTKNEG", "ADV"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "In deine Wirtschaft er leuchtet!", "tokens": ["In", "dei\u00b7ne", "Wirt\u00b7schaft", "er", "leuch\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}