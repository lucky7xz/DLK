{"textgrid.poem.27776": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "91.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Streich vom m ein Strichlein aus,", "tokens": ["Streich", "vom", "m", "ein", "Strich\u00b7lein", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dann wird dir ein n drau\u00df;", "tokens": ["Dann", "wird", "dir", "ein", "n", "drau\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Thu' das n f\u00fcr das a,", "tokens": ["Thu'", "das", "n", "f\u00fcr", "das", "a", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "XY", "APPR", "ART", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Also steht f\u00fcr Augen da,", "tokens": ["Al\u00b7so", "steht", "f\u00fcr", "Au\u00b7gen", "da", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie da den die schn\u00f6de Welt,", "tokens": ["Wie", "da", "den", "die", "schn\u00f6\u00b7de", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ART", "ADJA", "NN", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.6": {"text": "Welcher arm ist, hat und h\u00e4lt.", "tokens": ["Wel\u00b7cher", "arm", "ist", ",", "hat", "und", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAT", "ADJD", "VAFIN", "$,", "VAFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}