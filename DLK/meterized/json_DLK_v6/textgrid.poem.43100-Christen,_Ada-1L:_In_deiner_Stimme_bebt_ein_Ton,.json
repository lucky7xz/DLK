{"textgrid.poem.43100": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: In deiner Stimme bebt ein Ton,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In deiner Stimme bebt ein Ton,", "tokens": ["In", "dei\u00b7ner", "Stim\u00b7me", "bebt", "ein", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Alles \u00fcberklingt,", "tokens": ["Der", "Al\u00b7les", "\u00fc\u00b7berk\u00b7lingt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und der mir wie ein schneidend Weh", "tokens": ["Und", "der", "mir", "wie", "ein", "schnei\u00b7dend", "Weh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PPER", "KOKOM", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum tiefsten Herzen dringt.", "tokens": ["Zum", "tiefs\u00b7ten", "Her\u00b7zen", "dringt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie ri\u00df doch dieser eine Ton", "tokens": ["Wie", "ri\u00df", "doch", "die\u00b7ser", "ei\u00b7ne", "Ton"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADV", "PDAT", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir auf die alten Wunden;", "tokens": ["Mir", "auf", "die", "al\u00b7ten", "Wun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O da\u00df ich nimmer ihn geh\u00f6rt", "tokens": ["O", "da\u00df", "ich", "nim\u00b7mer", "ihn", "ge\u00b7h\u00f6rt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "PPER", "ADV", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nimmer dich gefunden!", "tokens": ["Und", "nim\u00b7mer", "dich", "ge\u00b7fun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O la\u00df das Heut' vergessen sein", "tokens": ["O", "la\u00df", "das", "Heut'", "ver\u00b7ges\u00b7sen", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "VVPP", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und mich sammt meinen Scherzen;", "tokens": ["Und", "mich", "sammt", "mei\u00b7nen", "Scher\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es sind ja doch die Schreie nur", "tokens": ["Es", "sind", "ja", "doch", "die", "Schrei\u00b7e", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der unheilbaren Schmerzen!", "tokens": ["Der", "un\u00b7heil\u00b7ba\u00b7ren", "Schmer\u00b7zen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}