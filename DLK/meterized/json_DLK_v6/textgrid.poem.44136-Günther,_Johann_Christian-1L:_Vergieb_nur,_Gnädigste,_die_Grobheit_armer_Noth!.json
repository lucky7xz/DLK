{"textgrid.poem.44136": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vergieb nur, Gn\u00e4digste, die Grobheit armer Noth!", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vergieb nur, Gn\u00e4digste, die Grobheit armer Noth!", "tokens": ["Ver\u00b7gieb", "nur", ",", "Gn\u00e4\u00b7digs\u00b7te", ",", "die", "Grob\u00b7heit", "ar\u00b7mer", "Noth", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "ADJA", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da mir das Eu\u00dferste von Schw\u00e4ch und Mangel droht", "tokens": ["Da", "mir", "das", "Eu\u00b7\u00dfers\u00b7te", "von", "Schw\u00e4ch", "und", "Man\u00b7gel", "droht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und unbekandtes Volck hier weder sch\u00fczt noch heilet", "tokens": ["Und", "un\u00b7be\u00b7kand\u00b7tes", "Volck", "hier", "we\u00b7der", "sch\u00fczt", "noch", "hei\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "ADV", "KON", "VVFIN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Noch anderswo ein Freund der Sehnsucht Rath ertheilet,", "tokens": ["Noch", "an\u00b7ders\u00b7wo", "ein", "Freund", "der", "Sehn\u00b7sucht", "Rath", "er\u00b7thei\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So flieh ich noch zu dir mit Ehrfurcht und Vertraun.", "tokens": ["So", "flieh", "ich", "noch", "zu", "dir", "mit", "Ehr\u00b7furcht", "und", "Ver\u00b7traun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich kan mein Leipzig nicht halb lahm und d\u00fcrftig schaun", "tokens": ["Ich", "kan", "mein", "Leip\u00b7zig", "nicht", "halb", "lahm", "und", "d\u00fcrf\u00b7tig", "schaun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NE", "PTKNEG", "ADJD", "PTKVZ", "KON", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und warth allhier auf Trost und lieg in fremden H\u00e4nden;", "tokens": ["Und", "warth", "all\u00b7hier", "auf", "Trost", "und", "lieg", "in", "frem\u00b7den", "H\u00e4n\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Misgunst wird mich zwar nach Wuntsch und L\u00fcgen sch\u00e4nden.", "tokens": ["Die", "Mis\u00b7gunst", "wird", "mich", "zwar", "nach", "Wunt\u00b7sch", "und", "L\u00fc\u00b7gen", "sch\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "So geht's, wer einmahl liegt, den dr\u00fcckt wohl jede Luft.", "tokens": ["So", "geht's", ",", "wer", "ein\u00b7mahl", "liegt", ",", "den", "dr\u00fcckt", "wohl", "je\u00b7de", "Luft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWS", "ADV", "VVFIN", "$,", "ART", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Jedoch wenn hier mein Reim um etwas Gnade ruft,", "tokens": ["Je\u00b7doch", "wenn", "hier", "mein", "Reim", "um", "et\u00b7was", "Gna\u00b7de", "ruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ADV", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So s\u00fcndigt er wohl nicht; wenn ich auch nichts verdiene,", "tokens": ["So", "s\u00fcn\u00b7digt", "er", "wohl", "nicht", ";", "wenn", "ich", "auch", "nichts", "ver\u00b7die\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "$.", "KOUS", "PPER", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Verdient mein Ungl\u00fcck doch, da\u00df be\u00dfre Hofnung gr\u00fcne.", "tokens": ["Ver\u00b7di\u00b7ent", "mein", "Un\u00b7gl\u00fcck", "doch", ",", "da\u00df", "be\u00df\u00b7re", "Hof\u00b7nung", "gr\u00fc\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "$,", "KOUS", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-++-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.13": {"text": "Versichre nur vor mich den gn\u00e4digsten Gemahl,", "tokens": ["Ver\u00b7sich\u00b7re", "nur", "vor", "mich", "den", "gn\u00e4\u00b7digs\u00b7ten", "Ge\u00b7mahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.14": {"text": "Mein Wuntsch sey fort und f\u00fcr, nach \u00fcberstandner Qual", "tokens": ["Mein", "Wunt\u00b7sch", "sey", "fort", "und", "f\u00fcr", ",", "nach", "\u00fc\u00b7bers\u00b7tand\u00b7ner", "Qual"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKVZ", "KON", "APPR", "$,", "APPR", "ADJA", "NN"], "meter": "--+-+-+-+-+-+", "measure": "anapaest.init"}, "line.15": {"text": "Ihm, wie ich schuldig bin, vor so viel Gnad und G\u00fcte", "tokens": ["Ihm", ",", "wie", "ich", "schul\u00b7dig", "bin", ",", "vor", "so", "viel", "Gnad", "und", "G\u00fc\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PWAV", "PPER", "ADJD", "VAFIN", "$,", "APPR", "ADV", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+--++-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Nur einen Dienst zu thun. Ein gro\u00dfmuthsvoll Gem\u00fcthe", "tokens": ["Nur", "ei\u00b7nen", "Dienst", "zu", "thun", ".", "Ein", "gro\u00df\u00b7muths\u00b7voll", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "PTKZU", "VVINF", "$.", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Entschuldigt dann und wann, wenn Jugend was versieht,", "tokens": ["Ent\u00b7schul\u00b7digt", "dann", "und", "wann", ",", "wenn", "Ju\u00b7gend", "was", "ver\u00b7sieht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "PWAV", "$,", "KOUS", "NN", "PWS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die Jugend, die sich noch erst um Erfahrung m\u00fcht.", "tokens": ["Die", "Ju\u00b7gend", ",", "die", "sich", "noch", "erst", "um", "Er\u00b7fah\u00b7rung", "m\u00fcht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADV", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ach, w\u00fcrd ich jezt gest\u00fczt! Ich k\u00f6nt in andern L\u00e4ndern", "tokens": ["Ach", ",", "w\u00fcrd", "ich", "jezt", "ge\u00b7st\u00fczt", "!", "Ich", "k\u00f6nt", "in", "an\u00b7dern", "L\u00e4n\u00b7dern"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VAFIN", "PPER", "ADV", "VVPP", "$.", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Vielleicht in kurzer Zeit so Feind als Gl\u00fccksstern \u00e4ndern.", "tokens": ["Viel\u00b7leicht", "in", "kur\u00b7zer", "Zeit", "so", "Feind", "als", "Gl\u00fccks\u00b7stern", "\u00e4n\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "ADV", "NN", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ich kan nichts mehr als flehn, du aber gn\u00e4dig seyn.", "tokens": ["Ich", "kan", "nichts", "mehr", "als", "flehn", ",", "du", "a\u00b7ber", "gn\u00e4\u00b7dig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "PIS", "KOKOM", "VVINF", "$,", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die Marterwoch ist da, nur leider trift es ein;", "tokens": ["Die", "Mar\u00b7ter\u00b7woch", "ist", "da", ",", "nur", "lei\u00b7der", "trift", "es", "ein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "ADV", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ach, wenn sich doch nur bald ein Tag der Ostern f\u00e4nde,", "tokens": ["Ach", ",", "wenn", "sich", "doch", "nur", "bald", "ein", "Tag", "der", "Os\u00b7tern", "f\u00e4n\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PRF", "ADV", "ADV", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "An dem zum wenigsten die Hofnung auferst\u00e4nde!", "tokens": ["An", "dem", "zum", "we\u00b7nigs\u00b7ten", "die", "Hof\u00b7nung", "auf\u00b7er\u00b7st\u00e4n\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPRART", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}}}}}