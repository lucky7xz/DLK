{"textgrid.poem.41039": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "T\u00fcbinger Burschenlied", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O T\u00fcbingen! du teure Stadt!", "tokens": ["O", "T\u00fc\u00b7bin\u00b7gen", "!", "du", "teu\u00b7re", "Stadt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bin deiner Weisheit voll und satt!", "tokens": ["Bin", "dei\u00b7ner", "Weis\u00b7heit", "voll", "und", "satt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ade! ihr alten Mauern!", "tokens": ["A\u00b7de", "!", "ihr", "al\u00b7ten", "Mau\u00b7ern", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Aus ist es mit dem Trauern!", "tokens": ["Aus", "ist", "es", "mit", "dem", "Trau\u00b7ern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und aus wohl mit dem blanken Geld,", "tokens": ["Und", "aus", "wohl", "mit", "dem", "blan\u00b7ken", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Doch in der weiten, freien Welt", "tokens": ["Doch", "in", "der", "wei\u00b7ten", ",", "frei\u00b7en", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lebt stets der Bursche munter.", "tokens": ["Lebt", "stets", "der", "Bur\u00b7sche", "mun\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Juchhei! ins Tal hinunter!", "tokens": ["Juch\u00b7hei", "!", "ins", "Tal", "hin\u00b7un\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Neckar rauscht, die Sonn' nicht steht,", "tokens": ["Der", "Ne\u00b7ckar", "rauscht", ",", "die", "Sonn'", "nicht", "steht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Wind von Wolk' zu Wolke weht", "tokens": ["Der", "Wind", "von", "Wol\u00b7k'", "zu", "Wol\u00b7ke", "weht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NE", "APPR", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und Storch und Reiher fliegen,", "tokens": ["Und", "Storch", "und", "Rei\u00b7her", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Juchhei! in langen Z\u00fcgen.", "tokens": ["Juch\u00b7hei", "!", "in", "lan\u00b7gen", "Z\u00fc\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O Erde! wie bist neu du mir!", "tokens": ["O", "Er\u00b7de", "!", "wie", "bist", "neu", "du", "mir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWAV", "VAFIN", "ADJD", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Herz! wie regt es sich in dir", "tokens": ["O", "Herz", "!", "wie", "regt", "es", "sich", "in", "dir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "PWAV", "VVFIN", "PPER", "PRF", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Jauchzen und mit Singen,", "tokens": ["Mit", "Jauch\u00b7zen", "und", "mit", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df m\u00f6cht' die Brust zerspringen.", "tokens": ["Da\u00df", "m\u00f6cht'", "die", "Brust", "zer\u00b7sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Fahr aus, du Staub, der in mich kam,", "tokens": ["Fahr", "aus", ",", "du", "Staub", ",", "der", "in", "mich", "kam", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPER", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schulweisheit und du B\u00fccherkram,", "tokens": ["Schul\u00b7weis\u00b7heit", "und", "du", "B\u00fc\u00b7cher\u00b7kram", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PPER", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In alle Winde fliehe,", "tokens": ["In", "al\u00b7le", "Win\u00b7de", "flie\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df die Natur einziehe!", "tokens": ["Da\u00df", "die", "Na\u00b7tur", "ein\u00b7zie\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Herz! \u00f6ffne dich nur weit, nur weit,", "tokens": ["Herz", "!", "\u00f6ff\u00b7ne", "dich", "nur", "weit", ",", "nur", "weit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sieh, all die gr\u00fcne Herrlichkeit", "tokens": ["Sieh", ",", "all", "die", "gr\u00fc\u00b7ne", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PIAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mu\u00df in dir Raum jetzt finden.", "tokens": ["Mu\u00df", "in", "dir", "Raum", "jetzt", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPER", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ade! ihr Herrn dahinten.", "tokens": ["A\u00b7de", "!", "ihr", "Herrn", "da\u00b7hin\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}