{"dta.poem.23439": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Herbstlied eines Chinesen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir sind nicht reif!", "tokens": ["Wir", "sind", "nicht", "reif", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Das ist das Lied, das sie gesungen haben", "tokens": ["Das", "ist", "das", "Lied", ",", "das", "sie", "ge\u00b7sun\u00b7gen", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "VAINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Jahrhunderte lang uns armen Waisenknaben,", "tokens": ["Jahr\u00b7hun\u00b7der\u00b7te", "lang", "uns", "ar\u00b7men", "Wai\u00b7sen\u00b7kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Womit sie uns noch immer beschwichten,", "tokens": ["Wo\u00b7mit", "sie", "uns", "noch", "im\u00b7mer", "be\u00b7schwich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Des Volkes Hoffen immer vernichten,", "tokens": ["Des", "Vol\u00b7kes", "Hof\u00b7fen", "im\u00b7mer", "ver\u00b7nich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Den Sinn der Bessern immer beth\u00f6ren", "tokens": ["Den", "Sinn", "der", "Bes\u00b7sern", "im\u00b7mer", "be\u00b7th\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "ADV", "VVINF"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und unsre Zukunft immer zerst\u00f6ren.", "tokens": ["Und", "uns\u00b7re", "Zu\u00b7kunft", "im\u00b7mer", "zer\u00b7st\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Wir sind nicht reif?", "tokens": ["Wir", "sind", "nicht", "reif", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Reif sind wir immer, reif zum Gl\u00fcck auf Erden,", "tokens": ["Reif", "sind", "wir", "im\u00b7mer", ",", "reif", "zum", "Gl\u00fcck", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "$,", "ADJD", "APPRART", "NN", "APPR", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Wir sollen gl\u00fccklicher und besser werden.", "tokens": ["Wir", "sol\u00b7len", "gl\u00fcck\u00b7li\u00b7cher", "und", "bes\u00b7ser", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Reif sind wir, unsre Leiden zu klagen,", "tokens": ["Reif", "sind", "wir", ",", "uns\u00b7re", "Lei\u00b7den", "zu", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Reif sind wir, unsre W\u00fcnsche zu sagen,", "tokens": ["Reif", "sind", "wir", ",", "uns\u00b7re", "W\u00fcn\u00b7sche", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Reif sind wir, euch nicht mehr zu ertragen,", "tokens": ["Reif", "sind", "wir", ",", "euch", "nicht", "mehr", "zu", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "PPER", "PTKNEG", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Reif, f\u00fcr die Freiheit Alles zu wagen.", "tokens": ["Reif", ",", "f\u00fcr", "die", "Frei\u00b7heit", "Al\u00b7les", "zu", "wa\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "PIS", "PTKZU", "VVINF", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}}}}