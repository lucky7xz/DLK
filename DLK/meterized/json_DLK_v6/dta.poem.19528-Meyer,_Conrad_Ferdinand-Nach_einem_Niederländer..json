{"dta.poem.19528": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Nach einem Niederl\u00e4nder.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Der Meister malt ein kleines zartes Bild,", "tokens": ["Der", "Meis\u00b7ter", "malt", "ein", "klei\u00b7nes", "zar\u00b7tes", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zur\u00fcckgelehnt, beschaut er's liebevoll.", "tokens": ["Zu\u00b7r\u00fcck\u00b7ge\u00b7lehnt", ",", "be\u00b7schaut", "er's", "lie\u00b7be\u00b7voll", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es pocht. \u201eHerein.\u201c Ein vl\u00e4mischer Junker ist's", "tokens": ["Es", "pocht", ".", "\u201e", "Her\u00b7ein", ".", "\u201c", "Ein", "vl\u00e4\u00b7mi\u00b7scher", "Jun\u00b7ker", "ist's"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "$(", "PTKVZ", "$.", "$(", "ART", "ADJA", "NN", "NE"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Mit einer drallen, aufgedonnerten Dirn,", "tokens": ["Mit", "ei\u00b7ner", "dral\u00b7len", ",", "auf\u00b7ge\u00b7don\u00b7ner\u00b7ten", "Dirn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Der vor Gesundheit fast die Wange birst.", "tokens": ["Der", "vor", "Ge\u00b7sund\u00b7heit", "fast", "die", "Wan\u00b7ge", "birst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ADV", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sie rauscht von Seide, flimmert von Geschmeid.", "tokens": ["Sie", "rauscht", "von", "Sei\u00b7de", ",", "flim\u00b7mert", "von", "Ge\u00b7schmeid", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "\u201ewir haben's eilig, lieber Meister. Wi\u00dft,", "tokens": ["\u201e", "wir", "ha\u00b7ben's", "ei\u00b7lig", ",", "lie\u00b7ber", "Meis\u00b7ter", ".", "Wi\u00dft", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADJD", "$,", "ADV", "NE", "$.", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ein wackrer Schelm stiehlt mir das T\u00f6chterlein.", "tokens": ["Ein", "wack\u00b7rer", "Schelm", "stiehlt", "mir", "das", "T\u00f6ch\u00b7ter\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Morgen ist Hochzeit. Malet mir mein Kind!\u201c", "tokens": ["Mor\u00b7gen", "ist", "Hoch\u00b7zeit", ".", "Ma\u00b7let", "mir", "mein", "Kind", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VAFIN", "NN", "$.", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.10": {"text": "\u201ezur Stunde, Herr! Nur noch den Pinselstrich!\u201c", "tokens": ["\u201e", "zur", "Stun\u00b7de", ",", "Herr", "!", "Nur", "noch", "den", "Pin\u00b7sel\u00b7strich", "!", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPRART", "NN", "$,", "NN", "$.", "ADV", "ADV", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Sie treten lustig vor die Staffelei:", "tokens": ["Sie", "tre\u00b7ten", "lus\u00b7tig", "vor", "die", "Staf\u00b7fe\u00b7lei", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Auf einem blanken Kissen schlummernd liegt", "tokens": ["Auf", "ei\u00b7nem", "blan\u00b7ken", "Kis\u00b7sen", "schlum\u00b7mernd", "liegt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Ein feiner M\u00e4dchenkopf. Der Meister setzt", "tokens": ["Ein", "fei\u00b7ner", "M\u00e4d\u00b7chen\u00b7kopf", ".", "Der", "Meis\u00b7ter", "setzt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Des Blumenkranzes tiefste Knospe noch", "tokens": ["Des", "Blu\u00b7men\u00b7kran\u00b7zes", "tiefs\u00b7te", "Knos\u00b7pe", "noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Auf die verblichne Stirn mit leichter Hand.", "tokens": ["Auf", "die", "ver\u00b7blich\u00b7ne", "Stirn", "mit", "leich\u00b7ter", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "\u2014 \u201eNach der Natur?\u201c \u2014 \u201eNach der Natur. Mein Kind.", "tokens": ["\u201e", "Nach", "der", "Na\u00b7tur", "?", "\u201c", "\u201e", "Nach", "der", "Na\u00b7tur", ".", "Mein", "Kind", "."], "token_info": ["punct", "punct", "word", "word", "word", "punct", "punct", "punct", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "$(", "APPR", "ART", "NN", "$.", "$(", "$(", "$(", "APPR", "ART", "NN", "$.", "PPOSAT", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.17": {"text": "Gestern beerdigt. Herr, ich bin zu Dienst.\u201c", "tokens": ["Ge\u00b7stern", "be\u00b7er\u00b7digt", ".", "Herr", ",", "ich", "bin", "zu", "Dienst", ".", "\u201c"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVPP", "$.", "NN", "$,", "PPER", "VAFIN", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}