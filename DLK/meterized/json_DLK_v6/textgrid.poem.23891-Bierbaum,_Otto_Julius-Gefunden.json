{"textgrid.poem.23891": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Gefunden", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Laue Sommernacht; am Himmel", "tokens": ["Lau\u00b7e", "Som\u00b7mer\u00b7nacht", ";", "am", "Him\u00b7mel"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$.", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stand kein Stern; im weiten Walde", "tokens": ["Stand", "kein", "Stern", ";", "im", "wei\u00b7ten", "Wal\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "$.", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Suchten wir uns tief im Dunkel,", "tokens": ["Such\u00b7ten", "wir", "uns", "tief", "im", "Dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und wir fanden uns.", "tokens": ["Und", "wir", "fan\u00b7den", "uns", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Fanden uns im weiten Walde", "tokens": ["Fan\u00b7den", "uns", "im", "wei\u00b7ten", "Wal\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Nacht, der sternenlosen,", "tokens": ["In", "der", "Nacht", ",", "der", "ster\u00b7nen\u00b7lo\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hielten staunend uns im Arme", "tokens": ["Hiel\u00b7ten", "stau\u00b7nend", "uns", "im", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der dunklen Nacht.", "tokens": ["In", "der", "dunk\u00b7len", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "War nicht unser ganzes Leben", "tokens": ["War", "nicht", "un\u00b7ser", "gan\u00b7zes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So ein Tappen, so ein Suchen?", "tokens": ["So", "ein", "Tap\u00b7pen", ",", "so", "ein", "Su\u00b7chen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da: In seine Finsternisse,", "tokens": ["Da", ":", "In", "sei\u00b7ne", "Fins\u00b7ter\u00b7nis\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Liebe, fiel Dein Licht.", "tokens": ["Lie\u00b7be", ",", "fiel", "Dein", "Licht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}