{"textgrid.poem.55590": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Ach, wie bist du mir ...", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, wie bist du mir,", "tokens": ["Ach", ",", "wie", "bist", "du", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VAFIN", "PPER", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Wie bin ich dir geblieben!", "tokens": ["Wie", "bin", "ich", "dir", "ge\u00b7blie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nein, an der Wahrheit", "tokens": ["Nein", ",", "an", "der", "Wahr\u00b7heit"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Verzweifl' ich nicht mehr.", "tokens": ["Ver\u00b7zweifl'", "ich", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Ach, wenn du da bist,", "tokens": ["Ach", ",", "wenn", "du", "da", "bist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "ADV", "VAFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "F\u00fchl ich, ich soll dich nicht lieben,", "tokens": ["F\u00fchl", "ich", ",", "ich", "soll", "dich", "nicht", "lie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach, wenn du fern bist,", "tokens": ["Ach", ",", "wenn", "du", "fern", "bist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "F\u00fchl ich, ich lieb dich so sehr.", "tokens": ["F\u00fchl", "ich", ",", "ich", "lieb", "dich", "so", "sehr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "ADJD", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}