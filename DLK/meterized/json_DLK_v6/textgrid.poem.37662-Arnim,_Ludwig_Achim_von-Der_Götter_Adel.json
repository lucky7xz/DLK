{"textgrid.poem.37662": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der G\u00f6tter Adel", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Still bewahr es in Gedanken", "tokens": ["Still", "be\u00b7wahr", "es", "in", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses tief geheime Wort,", "tokens": ["Die\u00b7ses", "tief", "ge\u00b7hei\u00b7me", "Wort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur im Herzen ist der Ort,", "tokens": ["Nur", "im", "Her\u00b7zen", "ist", "der", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo der Adel tritt in Schranken,", "tokens": ["Wo", "der", "A\u00b7del", "tritt", "in", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo die silbernen Drommeten", "tokens": ["Wo", "die", "sil\u00b7ber\u00b7nen", "Drom\u00b7me\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Helllaut rufen aus den N\u00f6then.", "tokens": ["Hell\u00b7laut", "ru\u00b7fen", "aus", "den", "N\u00f6\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihr Gedanken seid die Ahnen,", "tokens": ["Ihr", "Ge\u00b7dan\u00b7ken", "seid", "die", "Ah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn des Zweifels Kampf beginnt,", "tokens": ["Wenn", "des", "Zwei\u00b7fels", "Kampf", "be\u00b7ginnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie aus Stein die Quelle rinnt,", "tokens": ["Wie", "aus", "Stein", "die", "Quel\u00b7le", "rinnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Frischend ihre Geister mahnen,", "tokens": ["Fri\u00b7schend", "ih\u00b7re", "Geis\u00b7ter", "mah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der Gedanken trockne Bl\u00e4tter", "tokens": ["Der", "Ge\u00b7dan\u00b7ken", "trock\u00b7ne", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wehen frisch im Fr\u00fchlingswetter.", "tokens": ["We\u00b7hen", "frisch", "im", "Fr\u00fch\u00b7lings\u00b7wet\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nicht vom Leichtsinn abgerissen,", "tokens": ["Nicht", "vom", "Leicht\u00b7sinn", "ab\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht zum Spiele hingestreut,", "tokens": ["Nicht", "zum", "Spie\u00b7le", "hin\u00b7ge\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimmer es dem Baum gereut,", "tokens": ["Nim\u00b7mer", "es", "dem", "Baum", "ge\u00b7reut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df er nun so viel mu\u00df missen,", "tokens": ["Da\u00df", "er", "nun", "so", "viel", "mu\u00df", "mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Er im Wind sich freudig sch\u00fcttert.", "tokens": ["Er", "im", "Wind", "sich", "freu\u00b7dig", "sch\u00fct\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "PRF", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df ein Kranz am Halme zittert.", "tokens": ["Da\u00df", "ein", "Kranz", "am", "Hal\u00b7me", "zit\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wer vermag es zu vernichten,", "tokens": ["Wer", "ver\u00b7mag", "es", "zu", "ver\u00b7nich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was ein Herz ganz eingef\u00fchlt,", "tokens": ["Was", "ein", "Herz", "ganz", "ein\u00b7ge\u00b7f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Messet doch wohin es zielt", "tokens": ["Mes\u00b7set", "doch", "wo\u00b7hin", "es", "zielt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KON", "PWAV", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieses Nordlicht, hell in Schichten,", "tokens": ["Die\u00b7ses", "Nord\u00b7licht", ",", "hell", "in", "Schich\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur was h\u00e4\u00dflich zeigt sich schnelle,", "tokens": ["Nur", "was", "h\u00e4\u00df\u00b7lich", "zeigt", "sich", "schnel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "ADJD", "VVFIN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Spottend zeigt's die Sonn' recht helle.", "tokens": ["Spot\u00b7tend", "zeigt's", "die", "Sonn'", "recht", "hel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ADJD", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Geister sind in jedem Hause,", "tokens": ["Geis\u00b7ter", "sind", "in", "je\u00b7dem", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wecken aus dem Schlaf den Muth,", "tokens": ["We\u00b7cken", "aus", "dem", "Schlaf", "den", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Also rinnt das edle Blut,", "tokens": ["Al\u00b7so", "rinnt", "das", "ed\u00b7le", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Geistig wie der Wein beim Schmause,", "tokens": ["Geis\u00b7tig", "wie", "der", "Wein", "beim", "Schmau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was gesprochen da zusammen,", "tokens": ["Was", "ge\u00b7spro\u00b7chen", "da", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVPP", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nimmer kann's von einem stammen.", "tokens": ["Nim\u00b7mer", "kann's", "von", "ei\u00b7nem", "stam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPR", "ART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Immer mit dem gr\u00f6\u00dften Maa\u00dfe", "tokens": ["Im\u00b7mer", "mit", "dem", "gr\u00f6\u00df\u00b7ten", "Maa\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mi\u00dft des Hauses Geist das Kind,", "tokens": ["Mi\u00dft", "des", "Hau\u00b7ses", "Geist", "das", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das Kind sich dehnt geschwind,", "tokens": ["Und", "das", "Kind", "sich", "dehnt", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PRF", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Will sich zeigen von der Rasse;", "tokens": ["Will", "sich", "zei\u00b7gen", "von", "der", "Ras\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was ihm G\u00f6ttliches bescheeret,", "tokens": ["Was", "ihm", "G\u00f6tt\u00b7li\u00b7ches", "be\u00b7schee\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zeigt sich h\u00f6her, sichrer w\u00e4hret.", "tokens": ["Zeigt", "sich", "h\u00f6\u00b7her", ",", "sich\u00b7rer", "w\u00e4h\u00b7ret", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "$,", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Nicht die Geister zu vertreiben", "tokens": ["Nicht", "die", "Geis\u00b7ter", "zu", "ver\u00b7trei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stand des Volkes Geist einst auf,", "tokens": ["Stand", "des", "Vol\u00b7kes", "Geist", "einst", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nein da\u00df jedem freier Lauf,", "tokens": ["Nein", "da\u00df", "je\u00b7dem", "frei\u00b7er", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "KOUS", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jedem Haus ein Geist soll bleiben,", "tokens": ["Je\u00b7dem", "Haus", "ein", "Geist", "soll", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nein, da\u00df adlich all' auf Erden,", "tokens": ["Nein", ",", "da\u00df", "ad\u00b7lich", "all'", "auf", "Er\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "ADV", "PIS", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mu\u00df der Adel M\u00e4rtrer werden.", "tokens": ["Mu\u00df", "der", "A\u00b7del", "M\u00e4r\u00b7trer", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "NN", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}