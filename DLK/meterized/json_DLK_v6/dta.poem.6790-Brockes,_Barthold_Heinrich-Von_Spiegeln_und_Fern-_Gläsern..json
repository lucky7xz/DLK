{"dta.poem.6790": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von Spiegeln und Fern-  \n Gl\u00e4sern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Doch lasset uns noch \u00fcberlegen,", "tokens": ["Doch", "las\u00b7set", "uns", "noch", "\u00fc\u00b7berl\u00b7e\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Augen, voll von fremden Feuchtigkeiten,", "tokens": ["Wie", "Au\u00b7gen", ",", "voll", "von", "frem\u00b7den", "Feuch\u00b7tig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Seele manchesmal verleiten.", "tokens": ["Die", "See\u00b7le", "man\u00b7ches\u00b7mal", "ver\u00b7lei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie sich in einer tr\u00fcben Lufft,", "tokens": ["Wie", "sich", "in", "ei\u00b7ner", "tr\u00fc\u00b7ben", "Lufft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Beschwert mit einem dicken Dufft,", "tokens": ["Be\u00b7schwert", "mit", "ei\u00b7nem", "di\u00b7cken", "Dufft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Figuren bilden, durch ver\u00e4ndertes Bewegen.", "tokens": ["Fi\u00b7gu\u00b7ren", "bil\u00b7den", ",", "durch", "ver\u00b7\u00e4n\u00b7der\u00b7tes", "Be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Man mu\u00df mit Ernst besehn, durch welche Mittel-Dinge,", "tokens": ["Man", "mu\u00df", "mit", "Ernst", "be\u00b7sehn", ",", "durch", "wel\u00b7che", "Mit\u00b7tel\u00b7Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "NE", "VVINF", "$,", "APPR", "PWAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dergleichen Vorwurff sich in unser Auge dringe.", "tokens": ["Derg\u00b7lei\u00b7chen", "Vor\u00b7wurff", "sich", "in", "un\u00b7ser", "Au\u00b7ge", "drin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wir m\u00fcssen die verschiedne Fl\u00e4chen", "tokens": ["Wir", "m\u00fcs\u00b7sen", "die", "ver\u00b7schied\u00b7ne", "Fl\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Von unterschiednen Gl\u00e4sern sehn;", "tokens": ["Von", "un\u00b7ter\u00b7schied\u00b7nen", "Gl\u00e4\u00b7sern", "sehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "So findet man, wenn sich die Strahlen brechen,", "tokens": ["So", "fin\u00b7det", "man", ",", "wenn", "sich", "die", "Strah\u00b7len", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "KOUS", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Da\u00df hell und klar,", "tokens": ["Da\u00df", "hell", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Auf einen Spiegel-Glas entstehn", "tokens": ["Auf", "ei\u00b7nen", "Spie\u00b7gel\u00b7Glas", "ent\u00b7stehn"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Gem\u00e4hlde, welche bald nicht wahr sind, und bald wahr.", "tokens": ["Ge\u00b7m\u00e4hl\u00b7de", ",", "wel\u00b7che", "bald", "nicht", "wahr", "sind", ",", "und", "bald", "wahr", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "PTKNEG", "ADJD", "VAFIN", "$,", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}