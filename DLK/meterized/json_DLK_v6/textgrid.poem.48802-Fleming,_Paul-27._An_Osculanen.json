{"textgrid.poem.48802": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "27. An Osculanen", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was, ", "tokens": ["Was", ","], "token_info": ["word", "punct"], "pos": ["PWS", "$,"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "den ich doch von dir stahl mit deinem halben Willen,", "tokens": ["den", "ich", "doch", "von", "dir", "stahl", "mit", "dei\u00b7nem", "hal\u00b7ben", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "als du dich hattest umverwandt nach ", "tokens": ["als", "du", "dich", "hat\u00b7test", "um\u00b7ver\u00b7wandt", "nach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "VVPP", "APPR"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "hat dir derselbe Ku\u00df erwecket den Verdru\u00df,", "tokens": ["hat", "dir", "der\u00b7sel\u00b7be", "Ku\u00df", "er\u00b7we\u00b7cket", "den", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PDAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "da\u00df du mir auch nicht dankst auf meinen Ehrengru\u00df?", "tokens": ["da\u00df", "du", "mir", "auch", "nicht", "dankst", "auf", "mei\u00b7nen", "Eh\u00b7ren\u00b7gru\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du kehrst dein Antlitz weg, tust unter dem verh\u00fcllen,", "tokens": ["Du", "kehrst", "dein", "Ant\u00b7litz", "weg", ",", "tust", "un\u00b7ter", "dem", "ver\u00b7h\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,", "VVFIN", "APPR", "ART", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "als sehest du mich nicht die Ehre dir erf\u00fcllen.", "tokens": ["als", "se\u00b7hest", "du", "mich", "nicht", "die", "Eh\u00b7re", "dir", "er\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "PRF", "PTKNEG", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denk, ", "tokens": ["Denk", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}}, "stanza.3": {"line.1": {"text": "Es sei, ich habe Schuld. Doch tust du mir zu viel,", "tokens": ["Es", "sei", ",", "ich", "ha\u00b7be", "Schuld", ".", "Doch", "tust", "du", "mir", "zu", "viel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VAFIN", "NN", "$.", "KON", "VVFIN", "PPER", "PPER", "PTKA", "PIS", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "da\u00df dein erweckter Zorn sich ganz nicht legen will.", "tokens": ["da\u00df", "dein", "er\u00b7weck\u00b7ter", "Zorn", "sich", "ganz", "nicht", "le\u00b7gen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PRF", "ADV", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach, Sch\u00f6ne, schone doch so hart mich anzutasten.", "tokens": ["Ach", ",", "Sch\u00f6\u00b7ne", ",", "scho\u00b7ne", "doch", "so", "hart", "mich", "an\u00b7zu\u00b7tas\u00b7ten", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "VVFIN", "ADV", "ADV", "ADJD", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Hat dich mein Mund erz\u00fcrnt, so straf' ihn nach Geb\u00fcr',", "tokens": ["Hat", "dich", "mein", "Mund", "er\u00b7z\u00fcrnt", ",", "so", "straf'", "ihn", "nach", "Ge\u00b7b\u00fcr'", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$,", "ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und la\u00df ihn f\u00fcr den Geiz, so lang' es recht ist, fasten.", "tokens": ["und", "la\u00df", "ihn", "f\u00fcr", "den", "Geiz", ",", "so", "lang'", "es", "recht", "ist", ",", "fas\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "APPR", "ART", "NN", "$,", "ADV", "ADV", "PPER", "ADJD", "VAFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "G\u00f6nn' aber unterde\u00df doch nur ein Auge mir!", "tokens": ["G\u00f6nn'", "a\u00b7ber", "un\u00b7ter\u00b7de\u00df", "doch", "nur", "ein", "Au\u00b7ge", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "ADV", "ADV", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}