{"dta.poem.10500": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "LvII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Habe mich mit Liebesreden", "tokens": ["Ha\u00b7be", "mich", "mit", "Lie\u00b7bes\u00b7re\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PRF", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Festgelogen an dein Herz,", "tokens": ["Fest\u00b7ge\u00b7lo\u00b7gen", "an", "dein", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und, verstrickt in eignen F\u00e4den,", "tokens": ["Und", ",", "ver\u00b7strickt", "in", "eig\u00b7nen", "F\u00e4\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird zum Ernste mir mein Scherz.", "tokens": ["Wird", "zum", "Erns\u00b7te", "mir", "mein", "Scherz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn du dich, mit vollem Rechte,", "tokens": ["Wenn", "du", "dich", ",", "mit", "vol\u00b7lem", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Scherzend nun von mir entfernst,", "tokens": ["Scher\u00b7zend", "nun", "von", "mir", "ent\u00b7fernst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nah'n sich mir die H\u00f6llenm\u00e4chte,", "tokens": ["Nah'n", "sich", "mir", "die", "H\u00f6l\u00b7len\u00b7m\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich schie\u00df' mich todt im Ernst.", "tokens": ["Und", "ich", "schie\u00df'", "mich", "todt", "im", "Ernst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}