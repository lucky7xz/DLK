{"textgrid.poem.48406": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "2. Elegie an das traurige Hartenstein", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "War es denn noch nicht gnug, da\u00df Mamers seine Plagen,", "tokens": ["War", "es", "denn", "noch", "nicht", "gnug", ",", "da\u00df", "Ma\u00b7mers", "sei\u00b7ne", "Pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "PTKNEG", "ADV", "$,", "KOUS", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "du liebes ", "tokens": ["du", "lie\u00b7bes"], "token_info": ["word", "word"], "pos": ["PPER", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "der, wie man sagen tut, bei Nachten und bei Tagen", "tokens": ["der", ",", "wie", "man", "sa\u00b7gen", "tut", ",", "bei", "Nach\u00b7ten", "und", "bei", "Ta\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "PWAV", "PIS", "VVINF", "VVFIN", "$,", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "mit seiner Grausamkeit dir l\u00e4sset wenig Ruh'?", "tokens": ["mit", "sei\u00b7ner", "Grau\u00b7sam\u00b7keit", "dir", "l\u00e4s\u00b7set", "we\u00b7nig", "Ruh'", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es muste noch Fortun sich besser an dir r\u00e4chen,", "tokens": ["Es", "mus\u00b7te", "noch", "For\u00b7tun", "sich", "bes\u00b7ser", "an", "dir", "r\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "NE", "PRF", "ADJD", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "wiewol ohn' deine Schuld, und f\u00fchren \u00fcber dich", "tokens": ["wie\u00b7wol", "ohn'", "dei\u00b7ne", "Schuld", ",", "und", "f\u00fch\u00b7ren", "\u00fc\u00b7ber", "dich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "$,", "KON", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Den, welcher grimmer ist denn jenes Hauen, Stechen,", "tokens": ["Den", ",", "wel\u00b7cher", "grim\u00b7mer", "ist", "denn", "je\u00b7nes", "Hau\u00b7en", ",", "Ste\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ADJD", "VAFIN", "ADV", "PDAT", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "den Tod, den rauhen Tod. Mars l\u00e4sset weisen sich,", "tokens": ["den", "Tod", ",", "den", "rau\u00b7hen", "Tod", ".", "Mars", "l\u00e4s\u00b7set", "wei\u00b7sen", "sich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$.", "NN", "VVFIN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "wann man ihm, was er will, ohn Wegerung erleget,", "tokens": ["wann", "man", "ihm", ",", "was", "er", "will", ",", "ohn", "We\u00b7ge\u00b7rung", "er\u00b7le\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "$,", "PWS", "PPER", "VMFIN", "$,", "KOUI", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "und gibt ihm seinen Sold: so bistu nicht, o Tod!", "tokens": ["und", "gibt", "ihm", "sei\u00b7nen", "Sold", ":", "so", "bis\u00b7tu", "nicht", ",", "o", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "ADV", "ADV", "PTKNEG", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dich weder Geld, noch Gold, noch Ranzion beweget,", "tokens": ["Dich", "we\u00b7der", "Geld", ",", "noch", "Gold", ",", "noch", "Ran\u00b7zi\u00b7on", "be\u00b7we\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "NN", "$,", "ADV", "NN", "$,", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "sie w\u00e4re noch so gro\u00df, f\u00fcr Eines Sterbensnot.", "tokens": ["sie", "w\u00e4\u00b7re", "noch", "so", "gro\u00df", ",", "f\u00fcr", "Ei\u00b7nes", "Ster\u00b7bens\u00b7not", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mars \u00e4ndert seinen Rat, \u2013 oft gibt er Gnad' umb Bitten,", "tokens": ["Mars", "\u00e4n\u00b7dert", "sei\u00b7nen", "Rat", ",", "\u2013", "oft", "gibt", "er", "Gnad'", "umb", "Bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,", "$(", "ADV", "VVFIN", "PPER", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "auch mitten in dem Zorn. O Tod, so bistu nicht!", "tokens": ["auch", "mit\u00b7ten", "in", "dem", "Zorn", ".", "O", "Tod", ",", "so", "bis\u00b7tu", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$.", "NE", "NN", "$,", "ADV", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Du \u00e4nderst keinen Rat, du bleibst bei deinen Sitten;", "tokens": ["Du", "\u00e4n\u00b7derst", "kei\u00b7nen", "Rat", ",", "du", "bleibst", "bei", "dei\u00b7nen", "Sit\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "erz\u00fcrnestu dich denn, da hilfet keine Pflicht.", "tokens": ["er\u00b7z\u00fcr\u00b7ne\u00b7stu", "dich", "denn", ",", "da", "hil\u00b7fet", "kei\u00b7ne", "Pflicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Mars, ob er gleich will sein der st\u00e4rkste Gott der Erden", "tokens": ["Mars", ",", "ob", "er", "gleich", "will", "sein", "der", "st\u00e4rks\u00b7te", "Gott", "der", "Er\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "PPER", "ADV", "VMFIN", "PPOSAT", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "und solcher nur allein, oft werden ihrer mehr;", "tokens": ["und", "sol\u00b7cher", "nur", "al\u00b7lein", ",", "oft", "wer\u00b7den", "ih\u00b7rer", "mehr", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADV", "ADV", "$,", "ADV", "VAFIN", "PPOSAT", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "der steckt ihn in den Sack, der jenes Herr kan werden:", "tokens": ["der", "steckt", "ihn", "in", "den", "Sack", ",", "der", "je\u00b7nes", "Herr", "kan", "wer\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PRELS", "PDAT", "NN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "o Tod, o starker Tod! wes ist, wes ist das Heer,", "tokens": ["o", "Tod", ",", "o", "star\u00b7ker", "Tod", "!", "wes", "ist", ",", "wes", "ist", "das", "Heer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM", "NN", "$,", "FM", "ADJA", "NN", "$.", "PIS", "VAFIN", "$,", "PWS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "wer ist, wer ist der Herr, der dich m\u00f6g' \u00fcberwinden?", "tokens": ["wer", "ist", ",", "wer", "ist", "der", "Herr", ",", "der", "dich", "m\u00f6g'", "\u00fc\u00b7berw\u00b7in\u00b7den", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PWS", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und w\u00e4r' er noch so stark, so bistu st\u00e4rker noch;", "tokens": ["Und", "w\u00e4r'", "er", "noch", "so", "stark", ",", "so", "bis\u00b7tu", "st\u00e4r\u00b7ker", "noch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,", "ADV", "ADV", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "und wolten Tausent dich und aber Tausent binden,", "tokens": ["und", "wol\u00b7ten", "Tau\u00b7sent", "dich", "und", "a\u00b7ber", "Tau\u00b7sent", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NN", "PPER", "KON", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "du bindest alle sie, sie zwingstu unters Joch.", "tokens": ["du", "bin\u00b7dest", "al\u00b7le", "sie", ",", "sie", "zwings\u00b7tu", "un\u00b7ters", "Joch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PPER", "$,", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Nun kom und frag' ich dich von dieser beider Wesen,", "tokens": ["Nun", "kom", "und", "frag'", "ich", "dich", "von", "die\u00b7ser", "bei\u00b7der", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVIMP", "PPER", "PRF", "APPR", "PDAT", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "o traurigs ", "tokens": ["o", "trau\u00b7rigs"], "token_info": ["word", "word"], "pos": ["FM", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.27": {"text": "wann du aus Mars und Tod den Einen solst erlesen,", "tokens": ["wann", "du", "aus", "Mars", "und", "Tod", "den", "Ei\u00b7nen", "solst", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "KON", "NN", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "wen n\u00e4mbstu dieser beid'? O streckstu deine Hand", "tokens": ["wen", "n\u00e4mbs\u00b7tu", "die\u00b7ser", "beid'", "?", "O", "strecks\u00b7tu", "dei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "PDAT", "PTKVZ", "$.", "NE", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "zu Mars? Ja freilich wol. Er war gar leicht zu w\u00e4hlen", "tokens": ["zu", "Mars", "?", "Ja", "frei\u00b7lich", "wol", ".", "Er", "war", "gar", "leicht", "zu", "w\u00e4h\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$.", "PTKANT", "ADV", "ADV", "$.", "PPER", "VAFIN", "ADV", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "f\u00fcr jenem, welcher ist ein steter Menschenfra\u00df;", "tokens": ["f\u00fcr", "je\u00b7nem", ",", "wel\u00b7cher", "ist", "ein", "ste\u00b7ter", "Men\u00b7schen\u00b7fra\u00df", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "$,", "PWAT", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "hingegen dieser fromm, er h\u00f6rt noch auf zu qu\u00e4len,", "tokens": ["hin\u00b7ge\u00b7gen", "die\u00b7ser", "fromm", ",", "er", "h\u00f6rt", "noch", "auf", "zu", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "ADJD", "$,", "PPER", "VVFIN", "ADV", "APPR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "da jener garausmacht und w\u00fcrgt ohn Unterla\u00df.", "tokens": ["da", "je\u00b7ner", "ga\u00b7raus\u00b7macht", "und", "w\u00fcrgt", "ohn", "Un\u00b7ter\u00b7la\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Es ist nicht ohne zwar, da\u00df mancher oft begehret", "tokens": ["Es", "ist", "nicht", "oh\u00b7ne", "zwar", ",", "da\u00df", "man\u00b7cher", "oft", "be\u00b7ge\u00b7hret"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "APPR", "ADV", "$,", "KOUS", "PIS", "ADV", "VVFIN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.34": {"text": "zu sein viel lieber tot, als wenn ihn Mamers zwingt,", "tokens": ["zu", "sein", "viel", "lie\u00b7ber", "tot", ",", "als", "wenn", "ihn", "Ma\u00b7mers", "zwingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VAINF", "ADV", "ADV", "ADJD", "$,", "KOKOM", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "weil dieses Tyrannei endlos gar gerne w\u00e4hret,", "tokens": ["weil", "die\u00b7ses", "Ty\u00b7ran\u00b7nei", "end\u00b7los", "gar", "ger\u00b7ne", "w\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "hingegen jener ihn zum guten Ende bringt.", "tokens": ["hin\u00b7ge\u00b7gen", "je\u00b7ner", "ihn", "zum", "gu\u00b7ten", "En\u00b7de", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.37": {"text": "Di\u00df aber, wei\u00df ich wol, dir w\u00fcrde nicht gefallen,", "tokens": ["Di\u00df", "a\u00b7ber", ",", "wei\u00df", "ich", "wol", ",", "dir", "w\u00fcr\u00b7de", "nicht", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "$,", "VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "von Liebe, die du tr\u00e4gst zu deiner Obrigkeit;", "tokens": ["von", "Lie\u00b7be", ",", "die", "du", "tr\u00e4gst", "zu", "dei\u00b7ner", "Ob\u00b7rig\u00b7keit", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Mars w\u00fcte noch so sehr, hing\u00e4bstu was euch allen,", "tokens": ["Mars", "w\u00fc\u00b7te", "noch", "so", "sehr", ",", "hin\u00b7g\u00e4bs\u00b7tu", "was", "euch", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADV", "ADV", "$,", "ITJ", "PWS", "PPER", "PIS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "wenn du dein und ihr Leid k\u00f6ntst wenden dieser Zeit.", "tokens": ["wenn", "du", "dein", "und", "ihr", "Leid", "k\u00f6ntst", "wen\u00b7den", "die\u00b7ser", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "KON", "PPOSAT", "NN", "VMFIN", "PIS", "PDAT", 