{"dta.poem.5449": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Sinnlicher GOttes-Dienst.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich meines Geistes Kr\u00e4fte", "tokens": ["Wenn", "ich", "mei\u00b7nes", "Geis\u00b7tes", "Kr\u00e4f\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf der Creaturen Pracht,", "tokens": ["Auf", "der", "Crea\u00b7tu\u00b7ren", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch die Sinnen, gleichsam hefte,", "tokens": ["Durch", "die", "Sin\u00b7nen", ",", "gleich\u00b7sam", "hef\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ehr\u2019 ich den, der sie gemacht;", "tokens": ["Ehr'", "ich", "den", ",", "der", "sie", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und es scheint, als wann die Ehre,", "tokens": ["Und", "es", "scheint", ",", "als", "wann", "die", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die aus der Betrachtung qvillt,", "tokens": ["Die", "aus", "der", "Be\u00b7trach\u00b7tung", "qvillt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Da sie mich mit Lust erf\u00fcllt,", "tokens": ["Da", "sie", "mich", "mit", "Lust", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Dir, o HERR! gef\u00e4llig w\u00e4re.", "tokens": ["Dir", ",", "o", "HeRR", "!", "ge\u00b7f\u00e4l\u00b7lig", "w\u00e4\u00b7re", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "FM", "NN", "$.", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Denn jemehr ich sie besehe,", "tokens": ["Denn", "je\u00b7mehr", "ich", "sie", "be\u00b7se\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und in ihnen dich erh\u00f6he,", "tokens": ["Und", "in", "ih\u00b7nen", "dich", "er\u00b7h\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Je empfindlicher versp\u00fchre", "tokens": ["Je", "emp\u00b7find\u00b7li\u00b7cher", "ver\u00b7sp\u00fch\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Und befind\u2019 ich, da\u00df die Seele,", "tokens": ["Und", "be\u00b7find'", "ich", ",", "da\u00df", "die", "See\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "$,", "KOUS", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "In der gantz durchdrungnen Brust,", "tokens": ["In", "der", "gantz", "durch\u00b7drung\u00b7nen", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.15": {"text": "Da\u00df sie mit besondrer Lust", "tokens": ["Da\u00df", "sie", "mit", "be\u00b7sond\u00b7rer", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "S\u00fcsser Liebe sich verm\u00e4hle,", "tokens": ["S\u00fcs\u00b7ser", "Lie\u00b7be", "sich", "ver\u00b7m\u00e4h\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Die, von Andacht angeflammt,", "tokens": ["Die", ",", "von", "An\u00b7dacht", "an\u00b7ge\u00b7flammt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Aus der Wercke Sch\u00f6nheit stammt.", "tokens": ["Aus", "der", "Wer\u00b7cke", "Sch\u00f6n\u00b7heit", "stammt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Anmuth, die so ungemein,", "tokens": ["An\u00b7muth", ",", "die", "so", "un\u00b7ge\u00b7mein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Scheint ein Gnaden-Lohn zu seyn,", "tokens": ["Scheint", "ein", "Gna\u00b7den\u00b7Lohn", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKZU", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Den GOtt in sein Werck gesencket,", "tokens": ["Den", "Gott", "in", "sein", "Werck", "ge\u00b7sen\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.22": {"text": "Und den, wenn man sein gedencket,", "tokens": ["Und", "den", ",", "wenn", "man", "sein", "ge\u00b7den\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "KOUS", "PIS", "VAINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Und sich wol zu sehn bestrebet,", "tokens": ["Und", "sich", "wol", "zu", "sehn", "be\u00b7stre\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Man aus seiner Huld erhebet.", "tokens": ["Man", "aus", "sei\u00b7ner", "Huld", "er\u00b7he\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "In den Creaturen steckt,", "tokens": ["In", "den", "Crea\u00b7tu\u00b7ren", "steckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.26": {"text": "Wenn man sie, dem HErrn zum Preise,", "tokens": ["Wenn", "man", "sie", ",", "dem", "Herrn", "zum", "Prei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "H\u00f6ret, riecht, sieht, f\u00fchlt und schmeckt,", "tokens": ["H\u00f6\u00b7ret", ",", "riecht", ",", "sieht", ",", "f\u00fchlt", "und", "schmeckt", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Eine rechte Seelen-Speise.", "tokens": ["Ei\u00b7ne", "rech\u00b7te", "See\u00b7len\u00b7Spei\u00b7se", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}