{"textgrid.poem.42509": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: An einem breiten Wege", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An einem breiten Wege", "tokens": ["An", "ei\u00b7nem", "brei\u00b7ten", "We\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Stand eine Statue;", "tokens": ["Stand", "ei\u00b7ne", "Sta\u00b7tue", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+---", "measure": "dactylic.init"}, "line.3": {"text": "Das Volk ging dran vor\u00fcber", "tokens": ["Das", "Volk", "ging", "dran", "vor\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PAV", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Sommer und im Schnee.", "tokens": ["Im", "Som\u00b7mer", "und", "im", "Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es hing ein sch\u00f6nes M\u00e4dchen", "tokens": ["Es", "hing", "ein", "sch\u00f6\u00b7nes", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An steilem Kreuze da,", "tokens": ["An", "stei\u00b7lem", "Kreu\u00b7ze", "da", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Sie lie\u00df die Stirne sinken,", "tokens": ["Sie", "lie\u00df", "die", "Stir\u00b7ne", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Misericordia.", "tokens": ["Mi\u00b7se\u00b7ri\u00b7cor\u00b7dia", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+---+", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Und blickt unendlich traurig;", "tokens": ["Und", "blickt", "un\u00b7end\u00b7lich", "trau\u00b7rig", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es lag der Erde Leid", "tokens": ["Es", "lag", "der", "Er\u00b7de", "Leid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf ihrem Antlitz nieder,", "tokens": ["Auf", "ih\u00b7rem", "Ant\u00b7litz", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da lag es ohne Neid.", "tokens": ["Da", "lag", "es", "oh\u00b7ne", "Neid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie tr\u00e4gt die F\u00fcrstenkrone,", "tokens": ["Sie", "tr\u00e4gt", "die", "F\u00fcrs\u00b7ten\u00b7kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein pr\u00e4chtiges Gewand;", "tokens": ["Ein", "pr\u00e4ch\u00b7ti\u00b7ges", "Ge\u00b7wand", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Steinen und mit Ringen", "tokens": ["Mit", "Stei\u00b7nen", "und", "mit", "Rin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist ihr geschm\u00fcckt die Hand.", "tokens": ["Ist", "ihr", "ge\u00b7schm\u00fcckt", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Zu ihren F\u00fc\u00dfen stellt sich", "tokens": ["Zu", "ih\u00b7ren", "F\u00fc\u00b7\u00dfen", "stellt", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein junger Fant und kniet,", "tokens": ["Ein", "jun\u00b7ger", "Fant", "und", "kniet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und spielt auf seiner Geige", "tokens": ["Und", "spielt", "auf", "sei\u00b7ner", "Gei\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein letztes Abschiedslied.", "tokens": ["Ein", "letz\u00b7tes", "Ab\u00b7schieds\u00b7lied", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sie warf ihm hin zum Danke", "tokens": ["Sie", "warf", "ihm", "hin", "zum", "Dan\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den einen goldnen Schuh;", "tokens": ["Den", "ei\u00b7nen", "gold\u00b7nen", "Schuh", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann stockt ihr Leben wieder,", "tokens": ["Dann", "stockt", "ihr", "Le\u00b7ben", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie schlo\u00df die Augen zu.", "tokens": ["Sie", "schlo\u00df", "die", "Au\u00b7gen", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Das Volk geht dran vor\u00fcber,", "tokens": ["Das", "Volk", "geht", "dran", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Empfindet Ruck und Ri\u00df,", "tokens": ["Emp\u00b7fin\u00b7det", "Ruck", "und", "Ri\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und spricht halblaut und zitternd:", "tokens": ["Und", "spricht", "hal\u00b7blaut", "und", "zit\u00b7ternd", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die heilige K\u00fcmmernis.", "tokens": ["Die", "hei\u00b7li\u00b7ge", "K\u00fcm\u00b7mer\u00b7nis", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}