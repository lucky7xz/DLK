{"textgrid.poem.48935": {"metadata": {"author": {"name": "Arnold, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "31.", "genre": "verse", "period": "N.A.", "pub_year": 1690, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du unumschr\u00e4nckter Geist/ du freyes Wesen/", "tokens": ["Du", "un\u00b7um\u00b7schr\u00e4nck\u00b7ter", "Geist", "/", "du", "frey\u00b7es", "We\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Darnach sich alle Welt bequemen mu\u00df.", "tokens": ["Dar\u00b7nach", "sich", "al\u00b7le", "Welt", "be\u00b7que\u00b7men", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PRF", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich hab von deinem Rath genug gelesen/", "tokens": ["Ich", "hab", "von", "dei\u00b7nem", "Rath", "ge\u00b7nug", "ge\u00b7le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und bin in Demuth an der Wei\u00dfheit Schlu\u00df.", "tokens": ["Und", "bin", "in", "De\u00b7muth", "an", "der", "Wei\u00df\u00b7heit", "Schlu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ach da\u00df ich ihm m\u00f6cht gantz gleichf\u00f6rmig werden/", "tokens": ["Ach", "da\u00df", "ich", "ihm", "m\u00f6cht", "gantz", "gleich\u00b7f\u00f6r\u00b7mig", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "PPER", "PPER", "VMFIN", "ADV", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie solt ich nicht dir selbst so \u00e4hnlich seyn!", "tokens": ["Wie", "solt", "ich", "nicht", "dir", "selbst", "so", "\u00e4hn\u00b7lich", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PTKNEG", "PPER", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Himmel w\u00e4r in mir schon auff der Erden/", "tokens": ["Der", "Him\u00b7mel", "w\u00e4r", "in", "mir", "schon", "auff", "der", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mein Wille w\u00e4re mehr als Englisch rein.", "tokens": ["Mein", "Wil\u00b7le", "w\u00e4\u00b7re", "mehr", "als", "Eng\u00b7lisch", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "KOUS", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "O Wille/ der du nur mein Wohl verlangest/", "tokens": ["O", "Wil\u00b7le", "/", "der", "du", "nur", "mein", "Wohl", "ver\u00b7lan\u00b7gest", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "PRELS", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "La\u00df diesen Schlu\u00df alsbald gehn in die That.", "tokens": ["La\u00df", "die\u00b7sen", "Schlu\u00df", "als\u00b7bald", "gehn", "in", "die", "That", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PDAT", "NN", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Ich wei\u00df/ wie du mit einer Seelen prangest/", "tokens": ["Ich", "wei\u00df", "/", "wie", "du", "mit", "ei\u00b7ner", "See\u00b7len", "pran\u00b7gest", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWAV", "PPER", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Die sich nur \u00fcberl\u00e4st dem treuen Rath.", "tokens": ["Die", "sich", "nur", "\u00fc\u00b7berl\u00b7\u00e4st", "dem", "treu\u00b7en", "Rath", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "La\u00df meinen Vorsatz nicht so fl\u00fcchtig bleiben/", "tokens": ["La\u00df", "mei\u00b7nen", "Vor\u00b7satz", "nicht", "so", "fl\u00fcch\u00b7tig", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PTKNEG", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Bereite/ st\u00e4rck/ erhalt/ was du gethan:", "tokens": ["Be\u00b7rei\u00b7te", "/", "st\u00e4rck", "/", "er\u00b7halt", "/", "was", "du", "ge\u00b7than", ":"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADJD", "$(", "VVFIN", "$(", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Du wollst mich lieber in die Enge treiben/", "tokens": ["Du", "wollst", "mich", "lie\u00b7ber", "in", "die", "En\u00b7ge", "trei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Wenn ich au\u00dftreten wollt von deiner Bahn.", "tokens": ["Wenn", "ich", "au\u00df\u00b7tre\u00b7ten", "wollt", "von", "dei\u00b7ner", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Dein Wille sey mein Weg/ die Regel/ und das Ziel/", "tokens": ["Dein", "Wil\u00b7le", "sey", "mein", "Weg", "/", "die", "Re\u00b7gel", "/", "und", "das", "Ziel", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$(", "ART", "NN", "$(", "KON", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Kurtz: Alles sey er mir! Ich will ja nicht zu viel.", "tokens": ["Kurtz", ":", "Al\u00b7les", "sey", "er", "mir", "!", "Ich", "will", "ja", "nicht", "zu", "viel", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "PIS", "VAFIN", "PPER", "PPER", "$.", "PPER", "VMFIN", "ADV", "PTKNEG", "PTKA", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}