{"textgrid.poem.25528": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: So ist denn endlich alles still?", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So ist denn endlich alles still?", "tokens": ["So", "ist", "denn", "end\u00b7lich", "al\u00b7les", "still", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann ist es Zeit, das Sch\u00fctt der Thr\u00e4nen aufzuziehen;", "tokens": ["Dann", "ist", "es", "Zeit", ",", "das", "Sch\u00fctt", "der", "Thr\u00e4\u00b7nen", "auf\u00b7zu\u00b7zie\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "ART", "NN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und du, mein Herz! das immer l\u00e4rmen will,", "tokens": ["Und", "du", ",", "mein", "Herz", "!", "das", "im\u00b7mer", "l\u00e4r\u00b7men", "will", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPOSAT", "NN", "$.", "ART", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So l\u00e4rme nun, da\u00df deine Seufzer fliehen!", "tokens": ["So", "l\u00e4r\u00b7me", "nun", ",", "da\u00df", "dei\u00b7ne", "Seuf\u00b7zer", "flie\u00b7hen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "KOUS", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Denn alles ruhet. \u2013 ", "tokens": ["Denn", "al\u00b7les", "ru\u00b7het", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KON", "PIS", "VVFIN", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Den Odem schnarchend schon herauf aus tiefer Lunge;", "tokens": ["Den", "O\u00b7dem", "schnar\u00b7chend", "schon", "her\u00b7auf", "aus", "tie\u00b7fer", "Lun\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADV", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Spadille, der allein mich weinen sieht,", "tokens": ["Spa\u00b7dil\u00b7le", ",", "der", "al\u00b7lein", "mich", "wei\u00b7nen", "sieht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Hat, zum Verrath' von Nanten, keine Zunge.", "tokens": ["Hat", ",", "zum", "Ver\u00b7ra\u00b7th'", "von", "Nan\u00b7ten", ",", "kei\u00b7ne", "Zun\u00b7ge", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "APPRART", "NN", "APPR", "NE", "$,", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.9": {"text": "Auch meine gute Mutter, kann", "tokens": ["Auch", "mei\u00b7ne", "gu\u00b7te", "Mut\u00b7ter", ",", "kann"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$,", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die\u00df Pochen meiner Brust nicht aus dem Schlummer wecken;", "tokens": ["Die\u00df", "Po\u00b7chen", "mei\u00b7ner", "Brust", "nicht", "aus", "dem", "Schlum\u00b7mer", "we\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "PPOSAT", "NN", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Doch w\u00fc\u00dfte sie \u2013 ach! M\u00e4dchen! ach! wie dann?", "tokens": ["Doch", "w\u00fc\u00df\u00b7te", "sie", "\u2013", "ach", "!", "M\u00e4d\u00b7chen", "!", "ach", "!", "wie", "dann", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$(", "ITJ", "$.", "NN", "$.", "XY", "$.", "PWAV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Sie t\u00f6dtete der Gram, und dich, der Schrecken!", "tokens": ["Sie", "t\u00f6d\u00b7te\u00b7te", "der", "Gram", ",", "und", "dich", ",", "der", "Schre\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und doch, mein Herz! erlaubst du mir", "tokens": ["Und", "doch", ",", "mein", "Herz", "!", "er\u00b7laubst", "du", "mir"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Den wunderbaren Mann zu sehen und zu k\u00fcssen?", "tokens": ["Den", "wun\u00b7der\u00b7ba\u00b7ren", "Mann", "zu", "se\u00b7hen", "und", "zu", "k\u00fcs\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ha! welch ein Ku\u00df? Den k\u00f6nnen trotzig wir", "tokens": ["Ha", "!", "welch", "ein", "Ku\u00df", "?", "Den", "k\u00f6n\u00b7nen", "trot\u00b7zig", "wir"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PWAT", "ART", "NN", "$.", "NE", "VMFIN", "ADJD", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Der Welt gestehn, kann selbst der Himmel wissen.", "tokens": ["Der", "Welt", "ge\u00b7stehn", ",", "kann", "selbst", "der", "Him\u00b7mel", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Schlaf, theure Mutter, schlafe nur,", "tokens": ["Schlaf", ",", "theu\u00b7re", "Mut\u00b7ter", ",", "schla\u00b7fe", "nur", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Und tr\u00e4ume nicht einmal, da\u00df Nante sich verloren;", "tokens": ["Und", "tr\u00e4u\u00b7me", "nicht", "ein\u00b7mal", ",", "da\u00df", "Nan\u00b7te", "sich", "ver\u00b7lo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "$,", "KOUS", "NE", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Denn Amarant, der oft mir Liebe schwur,", "tokens": ["Denn", "A\u00b7ma\u00b7rant", ",", "der", "oft", "mir", "Lie\u00b7be", "schwur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "PRELS", "ADV", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Hat \u00f6ftrer noch auf meine Ruh' geschworen.", "tokens": ["Hat", "\u00f6ft\u00b7rer", "noch", "auf", "mei\u00b7ne", "Ruh'", "ge\u00b7schwo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Herz! das f\u00fcr den nur Liebe f\u00fchlt,", "tokens": ["Herz", "!", "das", "f\u00fcr", "den", "nur", "Lie\u00b7be", "f\u00fchlt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "APPR", "ART", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.22": {"text": "Den Mensch und Engel liebt, wer kann dich schuldig sprechen?", "tokens": ["Den", "Mensch", "und", "En\u00b7gel", "liebt", ",", "wer", "kann", "dich", "schul\u00b7dig", "spre\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NE", "VVFIN", "$,", "PWS", "VMFIN", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und liebt' ich ihn, wenn ich ihn f\u00e4hig hielt',", "tokens": ["Und", "liebt'", "ich", "ihn", ",", "wenn", "ich", "ihn", "f\u00e4\u00b7hig", "hielt'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "$,", "KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Er k\u00f6nne dich und seine Schw\u00fcre brechen?", "tokens": ["Er", "k\u00f6n\u00b7ne", "dich", "und", "sei\u00b7ne", "Schw\u00fc\u00b7re", "bre\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Ein unbegrenzt Vertrauen legt", "tokens": ["Ein", "un\u00b7be\u00b7grenzt", "Ver\u00b7trau\u00b7en", "legt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Den Arm auf seinen Arm, die Hand in seine H\u00e4nde. \u2013", "tokens": ["Den", "Arm", "auf", "sei\u00b7nen", "Arm", ",", "die", "Hand", "in", "sei\u00b7ne", "H\u00e4n\u00b7de", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,", "ART", "NN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Doch stille! horch! Die Klosterglocke schl\u00e4gt! \u2013", "tokens": ["Doch", "stil\u00b7le", "!", "horch", "!", "Die", "Klos\u00b7ter\u00b7glo\u00b7cke", "schl\u00e4gt", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$.", "VVIMP", "$.", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Drei Viertel? ach! \u2013 Zeit! nimmst du nie ein Ende?", "tokens": ["Drei", "Vier\u00b7tel", "?", "ach", "!", "\u2013", "Zeit", "!", "nimmst", "du", "nie", "ein", "En\u00b7de", "?"], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$.", "XY", "$.", "$(", "NN", "$.", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Ha! wie mein Odem zitternd schnaubt!", "tokens": ["Ha", "!", "wie", "mein", "O\u00b7dem", "zit\u00b7ternd", "schnaubt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOKOM", "PPOSAT", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Vom Wirbel bis zum Zeh' brenn' ich in Feuerflammen!", "tokens": ["Vom", "Wir\u00b7bel", "bis", "zum", "Zeh'", "brenn'", "ich", "in", "Feu\u00b7er\u00b7flam\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "APPRART", "NN", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Hu! Fieberfrost! wie sch\u00fcttelst du mein Haupt!", "tokens": ["Hu", "!", "Fie\u00b7berf\u00b7rost", "!", "wie", "sch\u00fct\u00b7telst", "du", "mein", "Haupt", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NN", "$.", "PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Ihr Kniee! fallt ihr unter mir zusammen?", "tokens": ["Ihr", "Kni\u00b7ee", "!", "fallt", "ihr", "un\u00b7ter", "mir", "zu\u00b7sam\u00b7men", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Auf keiner Stelle hab' ich Rast!", "tokens": ["Auf", "kei\u00b7ner", "Stel\u00b7le", "hab'", "ich", "Rast", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "O! da\u00df er doch nur nicht so \u00fcberlange bliebe!", "tokens": ["O", "!", "da\u00df", "er", "doch", "nur", "nicht", "so", "\u00fc\u00b7berl\u00b7an\u00b7ge", "blie\u00b7be", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "KOUS", "PPER", "ADV", "ADV", "PTKNEG", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und dennoch ist die\u00df Zittern, glaub' ich fast,", "tokens": ["Und", "den\u00b7noch", "ist", "die\u00df", "Zit\u00b7tern", ",", "glaub'", "ich", "fast", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PDS", "NN", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Nicht eben Furcht; was sollt' es seyn als Liebe?", "tokens": ["Nicht", "e\u00b7ben", "Furcht", ";", "was", "sollt'", "es", "seyn", "als", "Lie\u00b7be", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "NN", "$.", "PWS", "VMFIN", "PPER", "VAINF", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Wie dreht sie mich im Kreis herum,", "tokens": ["Wie", "dreht", "sie", "mich", "im", "Kreis", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Da\u00df alle meine Kr\u00e4ft' und Sinne mich verlassen!", "tokens": ["Da\u00df", "al\u00b7le", "mei\u00b7ne", "Kr\u00e4ft'", "und", "Sin\u00b7ne", "mich", "ver\u00b7las\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "KON", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Komm, Amarant! ich st\u00fcrze schwindlich um!", "tokens": ["Komm", ",", "A\u00b7ma\u00b7rant", "!", "ich", "st\u00fcr\u00b7ze", "schwind\u00b7lich", "um", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NE", "$.", "PPER", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Komm! la\u00df mich nur an deiner Brust erblassen!", "tokens": ["Komm", "!", "la\u00df", "mich", "nur", "an", "dei\u00b7ner", "Brust", "er\u00b7blas\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VVIMP", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Doch kann er treu dem Schwure seyn,", "tokens": ["Doch", "kann", "er", "treu", "dem", "Schwu\u00b7re", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Wenn ich mich so berauscht ihm vor die Augen stelle?", "tokens": ["Wenn", "ich", "mich", "so", "be\u00b7rauscht", "ihm", "vor", "die", "Au\u00b7gen", "stel\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Drum krieche nur du kleiner Dacht hinein,", "tokens": ["Drum", "krie\u00b7che", "nur", "du", "klei\u00b7ner", "Dacht", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "PPER", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Mein L\u00e4mpchen brennt f\u00fcrwahr sonst gar zu helle.", "tokens": ["Mein", "L\u00e4mp\u00b7chen", "brennt", "f\u00fcr\u00b7wahr", "sonst", "gar", "zu", "hel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", 