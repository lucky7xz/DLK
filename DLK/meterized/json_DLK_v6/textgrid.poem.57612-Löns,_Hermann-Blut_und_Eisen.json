{"textgrid.poem.57612": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Blut und Eisen", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, Blut und Eisen,", "tokens": ["Ja", ",", "Blut", "und", "Ei\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die werden beweisen", "tokens": ["Die", "wer\u00b7den", "be\u00b7wei\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VAFIN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Der neuen Zeit ihr altes Recht,", "tokens": ["Der", "neu\u00b7en", "Zeit", "ihr", "al\u00b7tes", "Recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es wird zerschlagen", "tokens": ["Es", "wird", "zer\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "In kurzen Tagen", "tokens": ["In", "kur\u00b7zen", "Ta\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Die Kerkerwand der m\u00fcde Knecht.", "tokens": ["Die", "Ker\u00b7ker\u00b7wand", "der", "m\u00fc\u00b7de", "Knecht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In starken Armen", "tokens": ["In", "star\u00b7ken", "Ar\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Pocht ohne Erbarmen", "tokens": ["Pocht", "oh\u00b7ne", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Der rote Saft voll Lebensmut,", "tokens": ["Der", "ro\u00b7te", "Saft", "voll", "Le\u00b7bens\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er wird ersetzen", "tokens": ["Er", "wird", "er\u00b7set\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "An allen Pl\u00e4tzen", "tokens": ["An", "al\u00b7len", "Pl\u00e4t\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Das abgestandne, blaue Blut.", "tokens": ["Das", "ab\u00b7ge\u00b7stand\u00b7ne", ",", "blau\u00b7e", "Blut", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ambo\u00df und Hammer,", "tokens": ["Am\u00b7bo\u00df", "und", "Ham\u00b7mer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Der alte Jammer", "tokens": ["Der", "al\u00b7te", "Jam\u00b7mer"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wird kurz und b\u00fcndig abgeschafft,", "tokens": ["Wird", "kurz", "und", "b\u00fcn\u00b7dig", "ab\u00b7ge\u00b7schafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der breite R\u00fccken", "tokens": ["Der", "brei\u00b7te", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Verlernt das B\u00fccken,", "tokens": ["Ver\u00b7lernt", "das", "B\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Die Schwielenfaust merkt ihre Kraft.", "tokens": ["Die", "Schwie\u00b7len\u00b7faust", "merkt", "ih\u00b7re", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Das Schlachtschwert werde", "tokens": ["Das", "Schlacht\u00b7schwert", "wer\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VAFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Ein Pflug f\u00fcr die Erde,", "tokens": ["Ein", "Pflug", "f\u00fcr", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Kein ekles R\u00fcstzeug f\u00fcr den Mord,", "tokens": ["Kein", "ek\u00b7les", "R\u00fcst\u00b7zeug", "f\u00fcr", "den", "Mord", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es wird der Krieger", "tokens": ["Es", "wird", "der", "Krie\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Ein Geistessieger", "tokens": ["Ein", "Geis\u00b7tes\u00b7sie\u00b7ger"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Und l\u00e4chelt \u00fcber den alten Sport.", "tokens": ["Und", "l\u00e4\u00b7chelt", "\u00fc\u00b7ber", "den", "al\u00b7ten", "Sport", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Ein Liebesverlangen", "tokens": ["Ein", "Lie\u00b7bes\u00b7ver\u00b7lan\u00b7gen"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wird hei\u00df umfangen", "tokens": ["Wird", "hei\u00df", "um\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADJD", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Der feindlichen V\u00f6lker geisthellen Sohn,", "tokens": ["Der", "feind\u00b7li\u00b7chen", "V\u00f6l\u00b7ker", "geis\u00b7thel\u00b7len", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit offenen Armen", "tokens": ["Mit", "of\u00b7fe\u00b7nen", "Ar\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Wird sich umarmen", "tokens": ["Wird", "sich", "um\u00b7ar\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PRF", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Die ungeborne Generation.", "tokens": ["Die", "un\u00b7ge\u00b7bor\u00b7ne", "Ge\u00b7ne\u00b7ra\u00b7ti\u00b7on", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}