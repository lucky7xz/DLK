{"textgrid.poem.55848": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Pont du Carrousel", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der blinde Mann, der auf der Br\u00fccke steht,", "tokens": ["Der", "blin\u00b7de", "Mann", ",", "der", "auf", "der", "Br\u00fc\u00b7cke", "steht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "grau wie ein Markstein namenloser Reiche,", "tokens": ["grau", "wie", "ein", "Mark\u00b7stein", "na\u00b7men\u00b7lo\u00b7ser", "Rei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "APPR", "NE", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "er ist vielleicht das Ding, das immer gleiche,", "tokens": ["er", "ist", "viel\u00b7leicht", "das", "Ding", ",", "das", "im\u00b7mer", "glei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$,", "PRELS", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "um das von fern die Sternenstunde geht,", "tokens": ["um", "das", "von", "fern", "die", "Ster\u00b7nen\u00b7stun\u00b7de", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "und der Gestirne stiller Mittelpunkt.", "tokens": ["und", "der", "Ge\u00b7stir\u00b7ne", "stil\u00b7ler", "Mit\u00b7tel\u00b7punkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Denn alles um ihn irrt und rinnt und prunkt.", "tokens": ["Denn", "al\u00b7les", "um", "ihn", "irrt", "und", "rinnt", "und", "prunkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Er ist der unbewegliche Gerechte,", "tokens": ["Er", "ist", "der", "un\u00b7be\u00b7weg\u00b7li\u00b7che", "Ge\u00b7rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "in viele wirre Wege hingestellt;", "tokens": ["in", "vie\u00b7le", "wir\u00b7re", "We\u00b7ge", "hin\u00b7ge\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "der dunkle Eingang in die Unterwelt", "tokens": ["der", "dunk\u00b7le", "Ein\u00b7gang", "in", "die", "Un\u00b7ter\u00b7welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "bei einem oberfl\u00e4chlichen Geschlechte.", "tokens": ["bei", "ei\u00b7nem", "o\u00b7berf\u00b7l\u00e4ch\u00b7li\u00b7chen", "Ge\u00b7schlech\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}