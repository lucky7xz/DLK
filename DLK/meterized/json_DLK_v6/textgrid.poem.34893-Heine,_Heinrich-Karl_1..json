{"textgrid.poem.34893": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Karl 1.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Wald, in der K\u00f6hlerh\u00fctte, sitzt", "tokens": ["Im", "Wald", ",", "in", "der", "K\u00f6h\u00b7ler\u00b7h\u00fct\u00b7te", ",", "sitzt"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "$,", "APPR", "ART", "NN", "$,", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Tr\u00fcbsinnig allein der K\u00f6nig;", "tokens": ["Tr\u00fcb\u00b7sin\u00b7nig", "al\u00b7lein", "der", "K\u00f6\u00b7nig", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Er sitzt an der Wiege des K\u00f6hlerkinds", "tokens": ["Er", "sitzt", "an", "der", "Wie\u00b7ge", "des", "K\u00f6h\u00b7ler\u00b7kinds"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und wiegt und singt eint\u00f6nig:", "tokens": ["Und", "wiegt", "und", "singt", "ein\u00b7t\u00f6\u00b7nig", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbeiapopeia, was raschelt im Stroh?", "tokens": ["\u00bb", "ei\u00b7a\u00b7po\u00b7pei\u00b7a", ",", "was", "ra\u00b7schelt", "im", "Stroh", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "$,", "PWS", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es bl\u00f6ken im Stalle die Schafe \u2013", "tokens": ["Es", "bl\u00f6\u00b7ken", "im", "Stal\u00b7le", "die", "Scha\u00b7fe", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Du tr\u00e4gst das Zeichen an der Stirn", "tokens": ["Du", "tr\u00e4gst", "das", "Zei\u00b7chen", "an", "der", "Stirn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und l\u00e4chelst so furchtbar im Schlafe.", "tokens": ["Und", "l\u00e4\u00b7chelst", "so", "furcht\u00b7bar", "im", "Schla\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Eiapopeia, das K\u00e4tzchen ist tot \u2013", "tokens": ["Ei\u00b7a\u00b7po\u00b7pei\u00b7a", ",", "das", "K\u00e4tz\u00b7chen", "ist", "tot", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VAFIN", "ADJD", "$("], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Du tr\u00e4gst auf der Stirne das Zeichen \u2013", "tokens": ["Du", "tr\u00e4gst", "auf", "der", "Stir\u00b7ne", "das", "Zei\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Du wirst ein Mann und schwingst das Beil,", "tokens": ["Du", "wirst", "ein", "Mann", "und", "schwingst", "das", "Beil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schon zittern im Walde die Eichen.", "tokens": ["Schon", "zit\u00b7tern", "im", "Wal\u00b7de", "die", "Ei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Der alte K\u00f6hlerglaube verschwand,", "tokens": ["Der", "al\u00b7te", "K\u00f6h\u00b7ler\u00b7glau\u00b7be", "ver\u00b7schwand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Es glauben die K\u00f6hlerkinder \u2013", "tokens": ["Es", "glau\u00b7ben", "die", "K\u00f6h\u00b7ler\u00b7kin\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Eiapopeia \u2013 nicht mehr an Gott,", "tokens": ["Ei\u00b7a\u00b7po\u00b7pei\u00b7a", "\u2013", "nicht", "mehr", "an", "Gott", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PTKNEG", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und an den K\u00f6nig noch minder.", "tokens": ["Und", "an", "den", "K\u00f6\u00b7nig", "noch", "min\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Das K\u00e4tzchen ist tot, die M\u00e4uschen sind froh \u2013", "tokens": ["Das", "K\u00e4tz\u00b7chen", "ist", "tot", ",", "die", "M\u00e4u\u00b7schen", "sind", "froh", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wir m\u00fcssen zuschanden werden \u2013", "tokens": ["Wir", "m\u00fcs\u00b7sen", "zu\u00b7schan\u00b7den", "wer\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVPP", "VAINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Eiapopeia \u2013 im Himmel der Gott", "tokens": ["Ei\u00b7a\u00b7po\u00b7pei\u00b7a", "\u2013", "im", "Him\u00b7mel", "der", "Gott"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$(", "APPRART", "NN", "ART", "NN"], "meter": "+---+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und ich, der K\u00f6nig auf Erden.", "tokens": ["Und", "ich", ",", "der", "K\u00f6\u00b7nig", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Mein Mut erlischt, mein Herz ist krank,", "tokens": ["Mein", "Mut", "er\u00b7lischt", ",", "mein", "Herz", "ist", "krank", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und t\u00e4glich wird es kr\u00e4nker \u2013", "tokens": ["Und", "t\u00e4g\u00b7lich", "wird", "es", "kr\u00e4n\u00b7ker", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Eiapopeia \u2013 du K\u00f6hlerkind,", "tokens": ["Ei\u00b7a\u00b7po\u00b7pei\u00b7a", "\u2013", "du", "K\u00f6h\u00b7ler\u00b7kind", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$(", "PPER", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ich wei\u00df es, du bist mein Henker.", "tokens": ["Ich", "wei\u00df", "es", ",", "du", "bist", "mein", "Hen\u00b7ker", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Mein Todesgesang ist dein Wiegenlied \u2013", "tokens": ["Mein", "To\u00b7des\u00b7ge\u00b7sang", "ist", "dein", "Wie\u00b7gen\u00b7lied", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Eiapopeia \u2013 die greisen", "tokens": ["Ei\u00b7a\u00b7po\u00b7pei\u00b7a", "\u2013", "die", "grei\u00b7sen"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$(", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Haarlocken schneidest du ab zuvor \u2013", "tokens": ["Haar\u00b7lo\u00b7cken", "schnei\u00b7dest", "du", "ab", "zu\u00b7vor", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "ADV", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Im Nacken klirrt mir das Eisen.", "tokens": ["Im", "Na\u00b7cken", "klirrt", "mir", "das", "Ei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Eiapopeia, was raschelt im Stroh?", "tokens": ["Ei\u00b7a\u00b7po\u00b7pei\u00b7a", ",", "was", "ra\u00b7schelt", "im", "Stroh", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "VVFIN", "APPRART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Du hast das Reich erworben,", "tokens": ["Du", "hast", "das", "Reich", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und schl\u00e4gst mir das Haupt vom Rumpf herab \u2013", "tokens": ["Und", "schl\u00e4gst", "mir", "das", "Haupt", "vom", "Rumpf", "her\u00b7ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "ADV", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das K\u00e4tzchen ist gestorben.", "tokens": ["Das", "K\u00e4tz\u00b7chen", "ist", "ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Eiapopeia, was raschelt im Stroh?", "tokens": ["Ei\u00b7a\u00b7po\u00b7pei\u00b7a", ",", "was", "ra\u00b7schelt", "im", "Stroh", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "VVFIN", "APPRART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Es bl\u00f6ken im Stalle die Schafe.", "tokens": ["Es", "bl\u00f6\u00b7ken", "im", "Stal\u00b7le", "die", "Scha\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Das K\u00e4tzchen ist tot, die M\u00e4uschen sind froh \u2013", "tokens": ["Das", "K\u00e4tz\u00b7chen", "ist", "tot", ",", "die", "M\u00e4u\u00b7schen", "sind", "froh", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Schlafe, mein Henkerchen, schlafe!\u00ab", "tokens": ["Schla\u00b7fe", ",", "mein", "Hen\u00b7ker\u00b7chen", ",", "schla\u00b7fe", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$.", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}