{"textgrid.poem.44079": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Endlich wird die Hofnung, endlich", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Endlich wird die Hofnung, endlich", "tokens": ["End\u00b7lich", "wird", "die", "Hof\u00b7nung", ",", "end\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einmahl an dem Ziele stehn.", "tokens": ["Ein\u00b7mahl", "an", "dem", "Zie\u00b7le", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist die Welt jezt unerkenntlich,", "tokens": ["Ist", "die", "Welt", "jezt", "un\u00b7er\u00b7kennt\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kan sie doch wohl in sich gehn,", "tokens": ["Kan", "sie", "doch", "wohl", "in", "sich", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "APPR", "PRF", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn die Zeit die Unschuld rettet", "tokens": ["Wenn", "die", "Zeit", "die", "Un\u00b7schuld", "ret\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und dem Haupte weicher bettet,", "tokens": ["Und", "dem", "Haup\u00b7te", "wei\u00b7cher", "bet\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Dem der Neid durch manche Nacht", "tokens": ["Dem", "der", "Neid", "durch", "man\u00b7che", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Schlaf und Feder schwer gemacht.", "tokens": ["Schlaf", "und", "Fe\u00b7der", "schwer", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Gewi\u00dfheit deiner Freude,", "tokens": ["Die", "Ge\u00b7wi\u00df\u00b7heit", "dei\u00b7ner", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebstes Endlich, st\u00e4rckt den Geist,", "tokens": ["Liebs\u00b7tes", "End\u00b7lich", ",", "st\u00e4rckt", "den", "Geist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn die Ungedult im Leide", "tokens": ["Wenn", "die", "Un\u00b7ge\u00b7dult", "im", "Lei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Muth und Ancker niederrei\u00dft.", "tokens": ["Muth", "und", "An\u00b7cker", "nie\u00b7der\u00b7rei\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Seh ich jezt erz\u00fcrnte Sterne,", "tokens": ["Seh", "ich", "jezt", "er\u00b7z\u00fcrn\u00b7te", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seh ich dennoch in der Ferne", "tokens": ["Seh", "ich", "den\u00b7noch", "in", "der", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ein versprochnes Canaan", "tokens": ["Ein", "ver\u00b7sproch\u00b7nes", "Ca\u00b7naan"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Von den Ungl\u00fccksbergen an.", "tokens": ["Von", "den", "Un\u00b7gl\u00fccks\u00b7ber\u00b7gen", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wurden ehmahls Jacobs Kinder", "tokens": ["Wur\u00b7den", "eh\u00b7mahls", "Ja\u00b7cobs", "Kin\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von Egyptens Last gepre\u00dft,", "tokens": ["Von", "E\u00b7gyp\u00b7tens", "Last", "ge\u00b7pre\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "War die Marter doch gelinder,", "tokens": ["War", "die", "Mar\u00b7ter", "doch", "ge\u00b7lin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als die mir das Auge n\u00e4\u00dft:", "tokens": ["Als", "die", "mir", "das", "Au\u00b7ge", "n\u00e4\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jene kam vom wilden Feinde,", "tokens": ["Je\u00b7ne", "kam", "vom", "wil\u00b7den", "Fein\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Meine kommt vom nechsten Freunde;", "tokens": ["Mei\u00b7ne", "kommt", "vom", "nechs\u00b7ten", "Freun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach, dergleichen herbe Pein", "tokens": ["Ach", ",", "derg\u00b7lei\u00b7chen", "her\u00b7be", "Pein"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "PIS", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Dringt zu tief und schmerzlich ein.", "tokens": ["Dringt", "zu", "tief", "und", "schmerz\u00b7lich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKA", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich verbei\u00df es doch so lange,", "tokens": ["Ich", "ver\u00b7bei\u00df", "es", "doch", "so", "lan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als mir immer m\u00f6glich ist.", "tokens": ["Als", "mir", "im\u00b7mer", "m\u00f6g\u00b7lich", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Judas spinnt zu seinem Strange,", "tokens": ["Ju\u00b7das", "spinnt", "zu", "sei\u00b7nem", "Stran\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn er falsch und freundlich k\u00fcst;", "tokens": ["Wenn", "er", "falsch", "und", "freund\u00b7lich", "k\u00fcst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Joab mu\u00df mit grauen Haaren", "tokens": ["Joab", "mu\u00df", "mit", "grau\u00b7en", "Haa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "APPR", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Endlich doch noch blutig fahren,", "tokens": ["End\u00b7lich", "doch", "noch", "blu\u00b7tig", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und der Fall des Simei", "tokens": ["Und", "der", "Fall", "des", "Si\u00b7mei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.8": {"text": "R\u00e4cht die Majest\u00e4t noch fr\u00fch.", "tokens": ["R\u00e4cht", "die", "Ma\u00b7jes\u00b7t\u00e4t", "noch", "fr\u00fch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Schminckt, ihr Heuchler, eure T\u00fccke", "tokens": ["Schminckt", ",", "ihr", "Heuch\u00b7ler", ",", "eu\u00b7re", "T\u00fc\u00b7cke"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch so heilig, als ihr wollt,", "tokens": ["Noch", "so", "hei\u00b7lig", ",", "als", "ihr", "wollt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$,", "KOUS", "PPER", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Breitet Blumen auf die Stricke,", "tokens": ["Brei\u00b7tet", "Blu\u00b7men", "auf", "die", "Stri\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schenckt nur Gift und Tod in Gold,", "tokens": ["Schenckt", "nur", "Gift", "und", "Tod", "in", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dies mein Herz bleibt unaufh\u00f6rlich", "tokens": ["Dies", "mein", "Herz", "bleibt", "un\u00b7auf\u00b7h\u00f6r\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "PPOSAT", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Allzeit so getrost als ehrlich;", "tokens": ["All\u00b7zeit", "so", "ge\u00b7trost", "als", "ehr\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "KOKOM", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Eure Bo\u00dfheit und mein Heil", "tokens": ["Eu\u00b7re", "Bo\u00df\u00b7heit", "und", "mein", "Heil"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Findet endlich doch sein Theil.", "tokens": ["Fin\u00b7det", "end\u00b7lich", "doch", "sein", "Theil", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}