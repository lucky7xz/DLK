{"textgrid.poem.43463": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fr\u00fchling hat sich wieder", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fchling hat sich wieder", "tokens": ["Fr\u00fch\u00b7ling", "hat", "sich", "wie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PRF", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Bei uns eingestellt,", "tokens": ["Bei", "uns", "ein\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Bringet Freud' hernieder", "tokens": ["Brin\u00b7get", "Freud'", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "NN", "PTKVZ"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "In die tr\u00fcbe Welt.", "tokens": ["In", "die", "tr\u00fc\u00b7be", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Und er kommt vergebens", "tokens": ["Und", "er", "kommt", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Nirgend, nirgendwo:", "tokens": ["Nir\u00b7gend", ",", "nir\u00b7gend\u00b7wo", ":"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Freut euch eures Lebens!", "tokens": ["Freut", "euch", "eu\u00b7res", "Le\u00b7bens", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Werdet alle froh!", "tokens": ["Wer\u00b7det", "al\u00b7le", "froh", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Auch die kleinste M\u00fccke", "tokens": ["Auch", "die", "kleins\u00b7te", "M\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Tanzt im Sonnenschein,", "tokens": ["Tanzt", "im", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Kann im Fr\u00fchlingsgl\u00fccke", "tokens": ["Kann", "im", "Fr\u00fch\u00b7lings\u00b7gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Froh und lustig sein.", "tokens": ["Froh", "und", "lus\u00b7tig", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Alle die da trauern", "tokens": ["Al\u00b7le", "die", "da", "trau\u00b7ern"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "ART", "ADV", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Hat er froh gemacht,", "tokens": ["Hat", "er", "froh", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Nur der armen Bauern", "tokens": ["Nur", "der", "ar\u00b7men", "Bau\u00b7ern"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Hat er nicht gedacht.", "tokens": ["Hat", "er", "nicht", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Denn was bringt er heuer", "tokens": ["Denn", "was", "bringt", "er", "heu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Uns doch f\u00fcr ein Gl\u00fcck?", "tokens": ["Uns", "doch", "f\u00fcr", "ein", "Gl\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Nichts als Frohn' und Steuer", "tokens": ["Nichts", "als", "Frohn'", "und", "Steu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "KOKOM", "NE", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Bringt er uns zur\u00fcck.", "tokens": ["Bringt", "er", "uns", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}