{"textgrid.poem.53132": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ma\u00df Stimme h\u00f6r ich klingen,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ma\u00df Stimme h\u00f6r ich klingen,", "tokens": ["Ma\u00df", "Stim\u00b7me", "h\u00f6r", "ich", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O S\u00fcssigkeit! Wer kan", "tokens": ["O", "S\u00fcs\u00b7sig\u00b7keit", "!", "Wer", "kan"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$.", "PWS", "VMFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So sch\u00f6n vnd lieblich singen,", "tokens": ["So", "sch\u00f6n", "vnd", "lieb\u00b7lich", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Al\u00df Orpheus nie gethan?", "tokens": ["Al\u00df", "Or\u00b7pheus", "nie", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Lebt vnsre Sappho wieder?", "tokens": ["Lebt", "vns\u00b7re", "Sap\u00b7pho", "wie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Thalia? oder sind", "tokens": ["Tha\u00b7lia", "?", "o\u00b7der", "sind"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$.", "KON", "VAFIN"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.7": {"text": "Es etwa solche Lieder,", "tokens": ["Es", "et\u00b7wa", "sol\u00b7che", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die deine Kunst beginnt?", "tokens": ["Die", "dei\u00b7ne", "Kunst", "be\u00b7ginnt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Verkriecht euch, meine Seiten,", "tokens": ["Ver\u00b7kriecht", "euch", ",", "mei\u00b7ne", "Sei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dein Werck, o Nachtigall,", "tokens": ["Dein", "Werck", ",", "o", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erreicht noch nicht bey weiten", "tokens": ["Er\u00b7reicht", "noch", "nicht", "bey", "wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "PTKNEG", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den angenehmen Schall.", "tokens": ["Den", "an\u00b7ge\u00b7neh\u00b7men", "Schall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Kan ich recht Vrtheil f\u00e4llen,", "tokens": ["Kan", "ich", "recht", "Vrtheil", "f\u00e4l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "So dringet vngefehr", "tokens": ["So", "drin\u00b7get", "vn\u00b7ge\u00b7fehr"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Au\u00df himlischen Capellen", "tokens": ["Au\u00df", "him\u00b7li\u00b7schen", "Ca\u00b7pel\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die\u00df sch\u00f6ne Stimchen her.", "tokens": ["Die\u00df", "sch\u00f6\u00b7ne", "Stim\u00b7chen", "her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Bist du es, Euphrosyne?", "tokens": ["Bist", "du", "es", ",", "Eu\u00b7phro\u00b7sy\u00b7ne", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "$,", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dein Mund, du edle Zier,", "tokens": ["Dein", "Mund", ",", "du", "ed\u00b7le", "Zier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist eine zarte Biene", "tokens": ["Ist", "ei\u00b7ne", "zar\u00b7te", "Bie\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd ziehet vn\u00df nach dir,", "tokens": ["Vnd", "zie\u00b7het", "vn\u00df", "nach", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wei\u00df vnsern Sinn zu z\u00e4hmen,", "tokens": ["Wei\u00df", "vn\u00b7sern", "Sinn", "zu", "z\u00e4h\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wen deine S\u00fcssigkeit", "tokens": ["Wen", "dei\u00b7ne", "S\u00fcs\u00b7sig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["PWS", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nicht kan gefangen nehmen,", "tokens": ["Nicht", "kan", "ge\u00b7fan\u00b7gen", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der ist ein treuges Scheidt.", "tokens": ["Der", "ist", "ein", "treu\u00b7ges", "Scheidt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Heb, sch\u00f6nste, an zu d\u00f6hnen", "tokens": ["Heb", ",", "sch\u00f6ns\u00b7te", ",", "an", "zu", "d\u00f6h\u00b7nen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "$,", "APPR", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo vmb des Meeres Strand,", "tokens": ["Wo", "vmb", "des", "Mee\u00b7res", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es werden die Sirehnen", "tokens": ["Es", "wer\u00b7den", "die", "Si\u00b7reh\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verst\u00fcrtzt in dich entbrandt,", "tokens": ["Ver\u00b7st\u00fcrtzt", "in", "dich", "ent\u00b7brandt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Winde Sturm vnd sausen", "tokens": ["Der", "Win\u00b7de", "Sturm", "vnd", "sau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Geht dir Gehorsam ein,", "tokens": ["Geht", "dir", "Ge\u00b7hor\u00b7sam", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Neptun wirdt nicht mehr brausen", "tokens": ["Nep\u00b7tun", "wirdt", "nicht", "mehr", "brau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PTKNEG", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vnd dein gefangner seyn.", "tokens": ["Vnd", "dein", "ge\u00b7fang\u00b7ner", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wa\u00df soll ich davon sagen,", "tokens": ["Wa\u00df", "soll", "ich", "da\u00b7von", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PAV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df du mein Sch\u00e4fferlied", "tokens": ["Da\u00df", "du", "mein", "Sch\u00e4f\u00b7fer\u00b7lied"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Au\u00df freyem Wolbehagen", "tokens": ["Au\u00df", "frey\u00b7em", "Wol\u00b7be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu singen bist bem\u00fcht?", "tokens": ["Zu", "sin\u00b7gen", "bist", "be\u00b7m\u00fcht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gl\u00fcckselig ist die Stunde", "tokens": ["Gl\u00fcck\u00b7se\u00b7lig", "ist", "die", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da ich die\u00df Lied gestellt,", "tokens": ["Da", "ich", "die\u00df", "Lied", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df sich bei dir im Munde", "tokens": ["Da\u00df", "sich", "bei", "dir", "im", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPR", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vnd im Ged\u00e4chtn\u00fcs h\u00e4lt.", "tokens": ["Vnd", "im", "Ge\u00b7d\u00e4cht\u00b7n\u00fcs", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Nichts wil ich mehr erwehlen,", "tokens": ["Nichts", "wil", "ich", "mehr", "er\u00b7weh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nun mein Gedicht zuletzt", "tokens": ["Nun", "mein", "Ge\u00b7dicht", "zu\u00b7letzt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auch deiner edlen Kehlen", "tokens": ["Auch", "dei\u00b7ner", "ed\u00b7len", "Keh\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht vnwerth wirdt gesch\u00e4tzt,", "tokens": ["Nicht", "vn\u00b7werth", "wirdt", "ge\u00b7sch\u00e4tzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sing, Prei\u00df der edlen Jugend,", "tokens": ["Sing", ",", "Prei\u00df", "der", "ed\u00b7len", "Ju\u00b7gend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nichts s\u00fcssers h\u00f6r ich ie,", "tokens": ["Nichts", "s\u00fcs\u00b7sers", "h\u00f6r", "ich", "ie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch machet deine Tugend", "tokens": ["Doch", "ma\u00b7chet", "dei\u00b7ne", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Noch sch\u00f6ner Harmony.", "tokens": ["Noch", "sch\u00f6\u00b7ner", "Har\u00b7mo\u00b7ny", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}