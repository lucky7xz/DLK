{"dta.poem.9110": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein kleines Haus mit gr\u00fcner Th\u00fcr", "tokens": ["Ein", "klei\u00b7nes", "Haus", "mit", "gr\u00fc\u00b7ner", "Th\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und Herzen in den Fensterl\u00e4den!", "tokens": ["und", "Her\u00b7zen", "in", "den", "Fens\u00b7ter\u00b7l\u00e4\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Abends,", "tokens": ["A\u00b7bends", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "unter den Silberpappeln,", "tokens": ["un\u00b7ter", "den", "Sil\u00b7ber\u00b7pap\u00b7peln", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "sitzen wir mit unsern Jungens.", "tokens": ["sit\u00b7zen", "wir", "mit", "un\u00b7sern", "Jun\u00b7gens", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u201emutter, Mutter, der Mond is kaput!\u201c", "tokens": ["\u201e", "mut\u00b7ter", ",", "Mut\u00b7ter", ",", "der", "Mond", "is", "ka\u00b7put", "!", "\u201c"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NN", "$,", "NN", "$,", "ART", "NN", "FM", "FM", "$.", "$("], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Der Kleinste kuckt auch.", "tokens": ["Der", "Kleins\u00b7te", "kuckt", "auch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "\u201ebiela!", "tokens": ["\u201e", "bie\u00b7la", "!"], "token_info": ["punct", "word", "punct"], "pos": ["$(", "PTKANT", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Bist du ein Maik\u00e4fer?\u201c", "tokens": ["Bist", "du", "ein", "Mai\u00b7k\u00e4\u00b7fer", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u201esa.\u201c", "tokens": ["\u201e", "sa.", "\u201c"], "token_info": ["punct", "abbreviation", "punct"], "pos": ["$(", "VVIMP", "$("]}}}}}