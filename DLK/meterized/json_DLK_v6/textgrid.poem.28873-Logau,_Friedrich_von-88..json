{"textgrid.poem.28873": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "88.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Varillus ist das Jahr; sein Will ist immer rund,", "tokens": ["Va\u00b7ril\u00b7lus", "ist", "das", "Jahr", ";", "sein", "Will", "ist", "im\u00b7mer", "rund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$.", "VAINF", "VMFIN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df morgen Winter steht, wo heute Sommer stund;", "tokens": ["Da\u00df", "mor\u00b7gen", "Win\u00b7ter", "steht", ",", "wo", "heu\u00b7te", "Som\u00b7mer", "stund", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "$,", "PWAV", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nur wann ein Schalt-Jahr ist, k\u00fcmmt Warheit wo mit ein;", "tokens": ["Nur", "wann", "ein", "Schal\u00b7tJahr", "ist", ",", "k\u00fcmmt", "War\u00b7heit", "wo", "mit", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "NN", "VAFIN", "$,", "VVFIN", "NN", "PWAV", "APPR", "ART", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sonst wil ein iede Stund ein eigne L\u00fcge seyn.", "tokens": ["Sonst", "wil", "ein", "ie\u00b7de", "Stund", "ein", "eig\u00b7ne", "L\u00fc\u00b7ge", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "PIAT", "NN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}