{"textgrid.poem.62003": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[tausend M\u00fccken tanzen in der Sonne]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tausend M\u00fccken tanzen in der Sonne,", "tokens": ["Tau\u00b7send", "M\u00fc\u00b7cken", "tan\u00b7zen", "in", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Tausend Sonnen in des Himmelsraum,", "tokens": ["Tau\u00b7send", "Son\u00b7nen", "in", "des", "Him\u00b7mels\u00b7raum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "B\u00fcrgt f\u00fcr Wirklichkeit nicht das Gescheh'ne?", "tokens": ["B\u00fcrgt", "f\u00fcr", "Wirk\u00b7lich\u00b7keit", "nicht", "das", "Ge\u00b7scheh'\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PTKNEG", "ART", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Ist die Gr\u00f6\u00dfe klein genug zum Traum?", "tokens": ["Ist", "die", "Gr\u00f6\u00b7\u00dfe", "klein", "ge\u00b7nug", "zum", "Traum", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "O selbst Traumgebilde, sie sind Wahrheit,", "tokens": ["O", "selbst", "Traum\u00b7ge\u00b7bil\u00b7de", ",", "sie", "sind", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "NN", "$,", "PPER", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Tr\u00e4umerisch nur von uns zusamm'gestellt \u2013", "tokens": ["Tr\u00e4u\u00b7me\u00b7risch", "nur", "von", "uns", "zu\u00b7sam\u00b7m'\u00b7ge\u00b7stellt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Was nie war, wird von uns nicht geschaffen \u2013", "tokens": ["Was", "nie", "war", ",", "wird", "von", "uns", "nicht", "ge\u00b7schaf\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "$,", "VAFIN", "APPR", "PPER", "PTKNEG", "VVPP", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Aus dem Nichts schuf Gott nur eine Welt! \u2013", "tokens": ["Aus", "dem", "Nichts", "schuf", "Gott", "nur", "ei\u00b7ne", "Welt", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "ADV", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}