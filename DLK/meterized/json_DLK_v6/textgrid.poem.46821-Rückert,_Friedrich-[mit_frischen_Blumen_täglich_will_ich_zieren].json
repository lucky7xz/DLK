{"textgrid.poem.46821": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[mit frischen Blumen t\u00e4glich will ich zieren]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit frischen Blumen t\u00e4glich will ich zieren", "tokens": ["Mit", "fri\u00b7schen", "Blu\u00b7men", "t\u00e4g\u00b7lich", "will", "ich", "zie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VMFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein Bild, und wenn sie ihren Glanz verlieren;", "tokens": ["Dein", "Bild", ",", "und", "wenn", "sie", "ih\u00b7ren", "Glanz", "ver\u00b7lie\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "KOUS", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So, sprech' ich dann, verlor sie selber ihren.", "tokens": ["So", ",", "sprech'", "ich", "dann", ",", "ver\u00b7lor", "sie", "sel\u00b7ber", "ih\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "ADV", "$,", "VVFIN", "PPER", "ADV", "PPOSAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dann l\u00e4chelt sanft dein liebes Bild dazwischen", "tokens": ["Dann", "l\u00e4\u00b7chelt", "sanft", "dein", "lie\u00b7bes", "Bild", "da\u00b7zwi\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "PPOSAT", "ADJA", "NN", "PAV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit heller Widerlegung: Meinen frischen", "tokens": ["Mit", "hel\u00b7ler", "Wi\u00b7der\u00b7le\u00b7gung", ":", "Mei\u00b7nen", "fri\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Glanz hab' ich noch, und nichts wird ihn verwischen,", "tokens": ["Glanz", "hab'", "ich", "noch", ",", "und", "nichts", "wird", "ihn", "ver\u00b7wi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "$,", "KON", "PIS", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Dann sprechen auch die Blumen drein, die treuen:", "tokens": ["Dann", "spre\u00b7chen", "auch", "die", "Blu\u00b7men", "drein", ",", "die", "treu\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,", "PRELS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir welken t\u00e4glich, doch uns solls nicht reuen,", "tokens": ["Wir", "wel\u00b7ken", "t\u00e4g\u00b7lich", ",", "doch", "uns", "solls", "nicht", "reu\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "KON", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf jeden Tag f\u00fcr dich uns zu erneuen.", "tokens": ["Auf", "je\u00b7den", "Tag", "f\u00fcr", "dich", "uns", "zu", "er\u00b7neu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "PPER", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}