{"textgrid.poem.36537": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Eine faule Grille sang", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine faule Grille sang", "tokens": ["Ei\u00b7ne", "fau\u00b7le", "Gril\u00b7le", "sang"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Einen ganzen Sommer lang,", "tokens": ["Ei\u00b7nen", "gan\u00b7zen", "Som\u00b7mer", "lang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und war immer ohne Sorgen", "tokens": ["Und", "war", "im\u00b7mer", "oh\u00b7ne", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr den andern Morgen.", "tokens": ["F\u00fcr", "den", "an\u00b7dern", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Weil der Sommer Speisen hat,", "tokens": ["Weil", "der", "Som\u00b7mer", "Spei\u00b7sen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wurde sie auch t\u00e4glich satt;", "tokens": ["Wur\u00b7de", "sie", "auch", "t\u00e4g\u00b7lich", "satt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber als der Winter kam,", "tokens": ["A\u00b7ber", "als", "der", "Win\u00b7ter", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und der Flur das Leben nahm,", "tokens": ["Und", "der", "Flur", "das", "Le\u00b7ben", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.9": {"text": "Alles tot und \u00f6de stand,", "tokens": ["Al\u00b7les", "tot", "und", "\u00f6\u00b7de", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Und kein W\u00fcrmchen mehr sich fand;", "tokens": ["Und", "kein", "W\u00fcrm\u00b7chen", "mehr", "sich", "fand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Da trieb sie der Hunger hin", "tokens": ["Da", "trieb", "sie", "der", "Hun\u00b7ger", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Zu der \u00c4mse: \u2013 Nachbarin,", "tokens": ["Zu", "der", "\u00c4m\u00b7se", ":", "\u2013", "Nach\u00b7ba\u00b7rin", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "$(", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Ich bin hungrig, gieb mir doch", "tokens": ["Ich", "bin", "hung\u00b7rig", ",", "gieb", "mir", "doch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "VVIMP", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Ein klein wenig nur zu leben!", "tokens": ["Ein", "klein", "we\u00b7nig", "nur", "zu", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Deine Kammer hat ja noch", "tokens": ["Dei\u00b7ne", "Kam\u00b7mer", "hat", "ja", "noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Gro\u00dfen Vorrat; und ich will", "tokens": ["Gro\u00b7\u00dfen", "Vor\u00b7rat", ";", "und", "ich", "will"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "KON", "PPER", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Alles gern dir wieder geben,", "tokens": ["Al\u00b7les", "gern", "dir", "wie\u00b7der", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Mit den Zinsen im April.", "tokens": ["Mit", "den", "Zin\u00b7sen", "im", "Ap\u00b7ril", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schwesterchen, wie brachtest du", "tokens": ["Schwes\u00b7ter\u00b7chen", ",", "wie", "brach\u00b7test", "du"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "VVFIN", "PPER"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Deine Zeit im Sommer zu?", "tokens": ["Dei\u00b7ne", "Zeit", "im", "Som\u00b7mer", "zu", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sage mir, was thatest du?", "tokens": ["Sa\u00b7ge", "mir", ",", "was", "tha\u00b7test", "du", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PWS", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Was ich that? du wei\u00dft's ja wohl!", "tokens": ["Was", "ich", "that", "?", "du", "wei\u00dft's", "ja", "wohl", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich, die Freundin vom Apoll,", "tokens": ["Ich", ",", "die", "Freun\u00b7din", "vom", "A\u00b7poll", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Sang best\u00e4ndig; hast du mich", "tokens": ["Sang", "be\u00b7st\u00e4n\u00b7dig", ";", "hast", "du", "mich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "$.", "VAFIN", "PPER", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht vernommen? und konnt' ich,", "tokens": ["Nicht", "ver\u00b7nom\u00b7men", "?", "und", "konnt'", "ich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "$.", "KON", "VMFIN", "PPER", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Schwesterchen, was bessers thun?", "tokens": ["Schwes\u00b7ter\u00b7chen", ",", "was", "bes\u00b7sers", "thun", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Grillchen, nein! doch tanze nun!", "tokens": ["Grill\u00b7chen", ",", "nein", "!", "doch", "tan\u00b7ze", "nun", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PTKANT", "$.", "ADV", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}