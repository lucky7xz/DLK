{"textgrid.poem.60813": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Spanisches Theater", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6chst volksm\u00e4\u00dfig und eigen und reich, voll Gl\u00e4ubiger Andacht,", "tokens": ["H\u00f6chst", "volks\u00b7m\u00e4\u00b7\u00dfig", "und", "ei\u00b7gen", "und", "reich", ",", "voll", "Gl\u00e4u\u00b7bi\u00b7ger", "An\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "KON", "ADJD", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "--+--+--+-+-+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Ist's, an Entwicklung zwar, griechischer B\u00fchne verwandt;", "tokens": ["Ist's", ",", "an", "Ent\u00b7wick\u00b7lung", "zwar", ",", "grie\u00b7chi\u00b7scher", "B\u00fch\u00b7ne", "ver\u00b7wandt", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "NN", "ADV", "$,", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Doch es erscheint sein Ehrengesetz, sein Gl\u00e4ubiger Sinn selbst", "tokens": ["Doch", "es", "er\u00b7scheint", "sein", "Eh\u00b7ren\u00b7ge\u00b7setz", ",", "sein", "Gl\u00e4u\u00b7bi\u00b7ger", "Sinn", "selbst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "ADV"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Gegen des heidnischen Volks sittliche Gr\u00f6\u00dfe Manier.", "tokens": ["Ge\u00b7gen", "des", "heid\u00b7ni\u00b7schen", "Volks", "sitt\u00b7li\u00b7che", "Gr\u00f6\u00b7\u00dfe", "Ma\u00b7nier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "NN", "$."], "meter": "---+---+--+--+", "measure": "iambic.tetra.relaxed"}}}}}