{"textgrid.poem.26050": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Und die Welt ward mein eigen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deine Augen verschweigen nichts mehr,", "tokens": ["Dei\u00b7ne", "Au\u00b7gen", "ver\u00b7schwei\u00b7gen", "nichts", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIS", "ADV", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Und die Welt ward mein eigen.", "tokens": ["Und", "die", "Welt", "ward", "mein", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Mit roter Geigen Genu\u00df spielen die Tage uns auf,", "tokens": ["Mit", "ro\u00b7ter", "Gei\u00b7gen", "Ge\u00b7nu\u00df", "spie\u00b7len", "die", "Ta\u00b7ge", "uns", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die vielen Lieder sind Blumen vor unserm Fu\u00df.", "tokens": ["Die", "vie\u00b7len", "Lie\u00b7der", "sind", "Blu\u00b7men", "vor", "un\u00b7serm", "Fu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Die Liebe steht wie ein singender Vogel", "tokens": ["Die", "Lie\u00b7be", "steht", "wie", "ein", "sin\u00b7gen\u00b7der", "Vo\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "\u00dcber deinem und meinem Haupt;", "tokens": ["\u00dc\u00b7ber", "dei\u00b7nem", "und", "mei\u00b7nem", "Haupt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "KON", "PPOSAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Sie hat die Erde mit Trauben besteckt", "tokens": ["Sie", "hat", "die", "Er\u00b7de", "mit", "Trau\u00b7ben", "be\u00b7steckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN", "VVPP"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und die Masten meiner Schiffe wie frohe B\u00e4ume belaubt.", "tokens": ["Und", "die", "Mas\u00b7ten", "mei\u00b7ner", "Schif\u00b7fe", "wie", "fro\u00b7he", "B\u00e4u\u00b7me", "be\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "KOKOM", "ADJA", "NN", "VVPP", "$."], "meter": "--+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Du hast dein Herz in meinen Garten gelegt,", "tokens": ["Du", "hast", "dein", "Herz", "in", "mei\u00b7nen", "Gar\u00b7ten", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.10": {"text": "Und mein Garten wird von deinem Herzblut gepflegt.", "tokens": ["Und", "mein", "Gar\u00b7ten", "wird", "von", "dei\u00b7nem", "Herz\u00b7blut", "ge\u00b7pflegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-++-+", "measure": "unknown.measure.septa"}}}}}