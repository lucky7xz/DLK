{"textgrid.poem.31682": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich tr\u00e4umte von einer Taube", "tokens": ["Ich", "tr\u00e4um\u00b7te", "von", "ei\u00b7ner", "Tau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und einem Falken scheu;", "tokens": ["Und", "ei\u00b7nem", "Fal\u00b7ken", "scheu", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die sa\u00dfen in einer Laube", "tokens": ["Die", "sa\u00b7\u00dfen", "in", "ei\u00b7ner", "Lau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und schwuren sich ewigliche Treu.", "tokens": ["Und", "schwu\u00b7ren", "sich", "e\u00b7wig\u00b7li\u00b7che", "Treu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Es klang das Morgengel\u00e4ute,", "tokens": ["Es", "klang", "das", "Mor\u00b7gen\u00b7ge\u00b7l\u00e4u\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vor\u00fcber war die Nacht.", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "war", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich wu\u00dft nicht, was es bedeute \u2013", "tokens": ["Ich", "wu\u00dft", "nicht", ",", "was", "es", "be\u00b7deu\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da hab ich an dich und mich gedacht.", "tokens": ["Da", "hab", "ich", "an", "dich", "und", "mich", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPER", "KON", "PPER", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}