{"dta.poem.476": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Auf die falben Lippen der  Phyllis.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Es schw\u00e4rtzet meinen Mund/ spricht Phyllis, gar kein Ru\u00df/", "tokens": ["Es", "schw\u00e4rt\u00b7zet", "mei\u00b7nen", "Mund", "/", "spricht", "Phyl\u00b7lis", ",", "gar", "kein", "Ru\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "VVFIN", "NE", "$,", "ADV", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Warum? sie nimmt den Ku\u00df von allen gerne an.", "tokens": ["Wa\u00b7rum", "?", "sie", "nimmt", "den", "Ku\u00df", "von", "al\u00b7len", "ger\u00b7ne", "an", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PPER", "VVFIN", "ART", "NN", "APPR", "PIS", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch weil sie allzufrey mit ihrem s\u00fcssen Ku\u00df", "tokens": ["Doch", "weil", "sie", "all\u00b7zu\u00b7frey", "mit", "ih\u00b7rem", "s\u00fcs\u00b7sen", "Ku\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So ists um den Corall der Lippen nun gethan.", "tokens": ["So", "ists", "um", "den", "Co\u00b7rall", "der", "Lip\u00b7pen", "nun", "ge\u00b7than", "."], "token_info": ["word", "word", "word