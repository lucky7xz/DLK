{"textgrid.poem.42583": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "7. Schwindelhaber", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ei! wer hat in diesem Jahre", "tokens": ["Ei", "!", "wer", "hat", "in", "die\u00b7sem", "Jah\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PWS", "VAFIN", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "All den Wust ins Korn gebracht,", "tokens": ["All", "den", "Wust", "ins", "Korn", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mutterkorn und andre Ware,", "tokens": ["Mut\u00b7ter\u00b7korn", "und", "and\u00b7re", "Wa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die im Kopfe d\u00e4misch macht,", "tokens": ["Die", "im", "Kop\u00b7fe", "d\u00e4\u00b7misch", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Raden, Ru\u00df, am meisten aber", "tokens": ["Ra\u00b7den", ",", "Ru\u00df", ",", "am", "meis\u00b7ten", "a\u00b7ber"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "APPRART", "PIS", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schwindelhaber, Dippelhaber?", "tokens": ["Schwin\u00b7del\u00b7ha\u00b7ber", ",", "Dip\u00b7pel\u00b7ha\u00b7ber", "?"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was die neuen Fr\u00fcchte taugen,", "tokens": ["Was", "die", "neu\u00b7en", "Fr\u00fcch\u00b7te", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sah man j\u00fcngst beim Sch\u00fctzenfest:", "tokens": ["Sah", "man", "j\u00fcngst", "beim", "Sch\u00fct\u00b7zen\u00b7fest", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Allen tanzt' es vor den Augen,", "tokens": ["Al\u00b7len", "tanzt'", "es", "vor", "den", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und nicht einer traf ins Nest;", "tokens": ["Und", "nicht", "ei\u00b7ner", "traf", "ins", "Nest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PIS", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In dem jungen Bier war aber", "tokens": ["In", "dem", "jun\u00b7gen", "Bier", "war", "a\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schwindelhaber, Dippelhaber.", "tokens": ["Schwin\u00b7del\u00b7ha\u00b7ber", ",", "Dip\u00b7pel\u00b7ha\u00b7ber", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Worfeln soll man, beuteln, sieben,", "tokens": ["Wor\u00b7feln", "soll", "man", ",", "beu\u00b7teln", ",", "sie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "VMFIN", "PIS", "$,", "VVINF", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was der Krankheit Spuren tr\u00e4gt;", "tokens": ["Was", "der", "Krank\u00b7heit", "Spu\u00b7ren", "tr\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "T\u00fcchtig werd es durchgetrieben,", "tokens": ["T\u00fcch\u00b7tig", "werd", "es", "durch\u00b7ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Abgegerbt und ausgefegt!", "tokens": ["Ab\u00b7ge\u00b7gerbt", "und", "aus\u00b7ge\u00b7fegt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weg den Wust, besonders aber", "tokens": ["Weg", "den", "Wust", ",", "be\u00b7son\u00b7ders", "a\u00b7ber"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schwindelhaber, Dippelhaber!", "tokens": ["Schwin\u00b7del\u00b7ha\u00b7ber", ",", "Dip\u00b7pel\u00b7ha\u00b7ber", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Die ihr sorgt in unsrem Namen", "tokens": ["Die", "ihr", "sorgt", "in", "uns\u00b7rem", "Na\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcr die neue, gro\u00dfe Saat,", "tokens": ["F\u00fcr", "die", "neu\u00b7e", ",", "gro\u00b7\u00dfe", "Saat", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sichtet aus den falschen Samen,", "tokens": ["Sich\u00b7tet", "aus", "den", "fal\u00b7schen", "Sa\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der schon so viel B\u00f6ses tat:", "tokens": ["Der", "schon", "so", "viel", "B\u00f6\u00b7ses", "tat", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Raden, Ru\u00df, vor allem aber", "tokens": ["Ra\u00b7den", ",", "Ru\u00df", ",", "vor", "al\u00b7lem", "a\u00b7ber"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "APPR", "PIS", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schwindelhaber, Dippelhaber!", "tokens": ["Schwin\u00b7del\u00b7ha\u00b7ber", ",", "Dip\u00b7pel\u00b7ha\u00b7ber", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}