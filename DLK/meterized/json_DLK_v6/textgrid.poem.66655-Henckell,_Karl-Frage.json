{"textgrid.poem.66655": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Frage", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun fragst du wohl und l\u00e4chelst mild,", "tokens": ["Nun", "fragst", "du", "wohl", "und", "l\u00e4\u00b7chelst", "mild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem jungen Selbstbetrug enthoben:", "tokens": ["Dem", "jun\u00b7gen", "Selbst\u00b7be\u00b7trug", "ent\u00b7ho\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "War das dein eignes Geistesbild,", "tokens": ["War", "das", "dein", "eig\u00b7nes", "Geis\u00b7tes\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das du der Menschheit eingewoben?", "tokens": ["Das", "du", "der", "Menschheit", "ein\u00b7ge\u00b7wo\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "War das der Seele Hochzeitstraum,", "tokens": ["War", "das", "der", "See\u00b7le", "Hoch\u00b7zeit\u00b7straum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit dem die Erde du umspanntest,", "tokens": ["Mit", "dem", "die", "Er\u00b7de", "du", "um\u00b7spann\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Als du vom Leben nur den Saum", "tokens": ["Als", "du", "vom", "Le\u00b7ben", "nur", "den", "Saum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der lichtbegl\u00e4nzten Schwingen kanntest?", "tokens": ["Der", "licht\u00b7be\u00b7gl\u00e4nz\u00b7ten", "Schwin\u00b7gen", "kann\u00b7test", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das Feuer deiner Jugend schlang", "tokens": ["Das", "Feu\u00b7er", "dei\u00b7ner", "Ju\u00b7gend", "schlang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich um die Welt im Glutumfassen,", "tokens": ["Sich", "um", "die", "Welt", "im", "Glu\u00b7tum\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Sturm, der dir im Haupte sang,", "tokens": ["Der", "Sturm", ",", "der", "dir", "im", "Haup\u00b7te", "sang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schlug auf das Trommelfell der Massen?", "tokens": ["Schlug", "auf", "das", "Trom\u00b7mel\u00b7fell", "der", "Mas\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Des Selbstgef\u00fchles Ozean", "tokens": ["Des", "Selbst\u00b7ge\u00b7f\u00fch\u00b7les", "O\u00b7ze\u00b7an"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "War Liebesflut dir von Millionen,", "tokens": ["War", "Lie\u00b7bes\u00b7flut", "dir", "von", "Mil\u00b7lion\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dein sehnsuchtsvoller Zukunftswahn", "tokens": ["Dein", "sehn\u00b7suchts\u00b7vol\u00b7ler", "Zu\u00b7kunfts\u00b7wahn"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Verzweiflung \u00f6der Lebenszonen? ...", "tokens": ["Ver\u00b7zwei\u00b7flung", "\u00f6\u00b7der", "Le\u00b7bens\u00b7zo\u00b7nen", "?", "..."], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}