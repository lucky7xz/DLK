{"textgrid.poem.47433": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[sie, welche nicht an Liebe glauben]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie, welche nicht an Liebe glauben,", "tokens": ["Sie", ",", "wel\u00b7che", "nicht", "an", "Lie\u00b7be", "glau\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PTKNEG", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich lade sie zu meinem Grabe.", "tokens": ["Ich", "la\u00b7de", "sie", "zu", "mei\u00b7nem", "Gra\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So wie sie deinen Namen nennen,", "tokens": ["So", "wie", "sie", "dei\u00b7nen", "Na\u00b7men", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wird hell empor aus meinem Sarge", "tokens": ["Wird", "hell", "em\u00b7por", "aus", "mei\u00b7nem", "Sar\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "PTKVZ", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Klageton die Luft durchzieh'n.", "tokens": ["Ein", "Kla\u00b7ge\u00b7ton", "die", "Luft", "durch\u00b7zieh'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}