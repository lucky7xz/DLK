{"textgrid.poem.60379": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "An die Verstockten", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Torenangst und Narrenzittern,", "tokens": ["To\u00b7re\u00b7nangst", "und", "Nar\u00b7ren\u00b7zit\u00b7tern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ausparieren hin und her,", "tokens": ["Aus\u00b7pa\u00b7rie\u00b7ren", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Macht den Binsenschaft zum Speer,", "tokens": ["Macht", "den", "Bin\u00b7sen\u00b7schaft", "zum", "Speer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schl\u00e4gt die Laffen erst zu Rittern.", "tokens": ["Schl\u00e4gt", "die", "Laf\u00b7fen", "erst", "zu", "Rit\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn ein muntrer Spatz am Dache", "tokens": ["Wenn", "ein", "mun\u00b7trer", "Spatz", "am", "Da\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4rmet \u00fcber eurem Haus,", "tokens": ["L\u00e4r\u00b7met", "\u00fc\u00b7ber", "eu\u00b7rem", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Springet ihr zum Fenster aus,", "tokens": ["Sprin\u00b7get", "ihr", "zum", "Fens\u00b7ter", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ob der Bau zusammenkrache.", "tokens": ["Ob", "der", "Bau", "zu\u00b7sam\u00b7men\u00b7kra\u00b7che", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Schweift in euren Waldesgr\u00fcnden", "tokens": ["Schweift", "in", "eu\u00b7ren", "Wal\u00b7des\u00b7gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von Leuchtk\u00e4fern eine Schar,", "tokens": ["Von", "Leucht\u00b7k\u00e4\u00b7fern", "ei\u00b7ne", "Schar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ha, wie schreckt euch die Gefahr,", "tokens": ["Ha", ",", "wie", "schreckt", "euch", "die", "Ge\u00b7fahr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sie euch den Wald anz\u00fcnden.", "tokens": ["Da\u00df", "sie", "euch", "den", "Wald", "an\u00b7z\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Metaphern und die Tropen,", "tokens": ["Die", "Me\u00b7ta\u00b7phern", "und", "die", "Tro\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die da pfeift ein loser Wicht,", "tokens": ["Die", "da", "pfeift", "ein", "lo\u00b7ser", "Wicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wandeln euch die Schafe nicht", "tokens": ["Wan\u00b7deln", "euch", "die", "Scha\u00b7fe", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Um zu scheuen Antilopen;", "tokens": ["Um", "zu", "scheu\u00b7en", "An\u00b7ti\u00b7lo\u00b7pen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PTKZU", "VVINF", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Oder gar zu wilden B\u00e4ren;", "tokens": ["O\u00b7der", "gar", "zu", "wil\u00b7den", "B\u00e4\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruhig m\u00f6gt ihr und noch lang", "tokens": ["Ru\u00b7hig", "m\u00f6gt", "ihr", "und", "noch", "lang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "KON", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Trotz dem kecken Sang und Klang", "tokens": ["Trotz", "dem", "ke\u00b7cken", "Sang", "und", "Klang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eure Horden scheren, scheren.", "tokens": ["Eu\u00b7re", "Hor\u00b7den", "sche\u00b7ren", ",", "sche\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch vor einem zittert, Toren!", "tokens": ["Doch", "vor", "ei\u00b7nem", "zit\u00b7tert", ",", "To\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "ART", "VVFIN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn er an den Pfeilern r\u00fchrt,", "tokens": ["Wenn", "er", "an", "den", "Pfei\u00b7lern", "r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn er seine Flammen sch\u00fcrt,", "tokens": ["Wenn", "er", "sei\u00b7ne", "Flam\u00b7men", "sch\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wahrt euch, sonst seid ihr verloren!", "tokens": ["Wahrt", "euch", ",", "sonst", "seid", "ihr", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "H\u00f6rt ihrs im Geb\u00e4lke knarren,", "tokens": ["H\u00f6rt", "ihrs", "im", "Ge\u00b7b\u00e4l\u00b7ke", "knar\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Baut ein neues Haus geschwind,", "tokens": ["Baut", "ein", "neu\u00b7es", "Haus", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eh mit Habe, Weib und Kind", "tokens": ["Eh", "mit", "Ha\u00b7be", ",", "Weib", "und", "Kind"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Euch begraben eure Sparren.", "tokens": ["Euch", "be\u00b7gra\u00b7ben", "eu\u00b7re", "Spar\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Funken sind des Feuers Boten,", "tokens": ["Fun\u00b7ken", "sind", "des", "Feu\u00b7ers", "Bo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Funken jagen durch das Land,", "tokens": ["Fun\u00b7ken", "ja\u00b7gen", "durch", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und den gro\u00dfen Gottesbrand", "tokens": ["Und", "den", "gro\u00b7\u00dfen", "Got\u00b7tes\u00b7brand"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "D\u00e4mpft ihr nicht mit euren Pfoten.", "tokens": ["D\u00e4mpft", "ihr", "nicht", "mit", "eu\u00b7ren", "Pfo\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Zitternd seht ihr und erschrocken,", "tokens": ["Zit\u00b7ternd", "seht", "ihr", "und", "er\u00b7schro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Funken, die der Witz gefacht,", "tokens": ["Fun\u00b7ken", ",", "die", "der", "Witz", "ge\u00b7facht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die das Volk, indem es lacht,", "tokens": ["Die", "das", "Volk", ",", "in\u00b7dem", "es", "lacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Haucht in tote Aschenflocken;", "tokens": ["Haucht", "in", "to\u00b7te", "A\u00b7schen\u00b7flo\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Aber nicht wollt ihr erschrecken,", "tokens": ["A\u00b7ber", "nicht", "wollt", "ihr", "er\u00b7schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Wenn es blitzt im Herzensgrund,", "tokens": ["Wenn", "es", "blitzt", "im", "Her\u00b7zens\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn die Sklaven, kettenwund,", "tokens": ["Wenn", "die", "Skla\u00b7ven", ",", "ket\u00b7ten\u00b7wund", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch den Gott in sich entdecken.", "tokens": ["Doch", "den", "Gott", "in", "sich", "ent\u00b7de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "H\u00f6rt, es kann die Stunde kommen;", "tokens": ["H\u00f6rt", ",", "es", "kann", "die", "Stun\u00b7de", "kom\u00b7men", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo das Lamm ein L\u00f6we hei\u00dft,", "tokens": ["Wo", "das", "Lamm", "ein", "L\u00f6\u00b7we", "hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo es br\u00fcllend euch zerrei\u00dft;", "tokens": ["Wo", "es", "br\u00fcl\u00b7lend", "euch", "zer\u00b7rei\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00dft euch Gottes Zeichen frommen! \u2013", "tokens": ["La\u00dft", "euch", "Got\u00b7tes", "Zei\u00b7chen", "from\u00b7men", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "NN", "NN", "ADJA", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}