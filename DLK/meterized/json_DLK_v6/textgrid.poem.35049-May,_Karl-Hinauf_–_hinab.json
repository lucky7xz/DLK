{"textgrid.poem.35049": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Hinauf \u2013 hinab", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hinauf zu dir will ich nur immer denken,", "tokens": ["Hin\u00b7auf", "zu", "dir", "will", "ich", "nur", "im\u00b7mer", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hinauf zu dir, der ewig mein gedenkt.", "tokens": ["Hin\u00b7auf", "zu", "dir", ",", "der", "e\u00b7wig", "mein", "ge\u00b7denkt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$,", "PRELS", "ADJD", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu dir, will meinen Fl\u00fcgelschlag ich lenken,", "tokens": ["Zu", "dir", ",", "will", "mei\u00b7nen", "Fl\u00fc\u00b7gel\u00b7schlag", "ich", "len\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "VMFIN", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu dir, der all mein Sehnen zu sich lenkt.", "tokens": ["Zu", "dir", ",", "der", "all", "mein", "Seh\u00b7nen", "zu", "sich", "lenkt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PRELS", "PIAT", "PPOSAT", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es sind nicht stolze H\u00f6hen zu ersteigen", "tokens": ["Es", "sind", "nicht", "stol\u00b7ze", "H\u00f6\u00b7hen", "zu", "er\u00b7stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Es ist kein Flug, wie der Phantast ihn liebt,", "tokens": ["Es", "ist", "kein", "Flug", ",", "wie", "der", "Phan\u00b7tast", "ihn", "liebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "PWAV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und doch gilt es, das H\u00f6chste zu erreichen,", "tokens": ["Und", "doch", "gilt", "es", ",", "das", "H\u00f6chs\u00b7te", "zu", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "ART", "ADJA", "PTKZU", "VVINF", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Was es auf Erden f\u00fcr den Himmel giebt.", "tokens": ["Was", "es", "auf", "Er\u00b7den", "f\u00fcr", "den", "Him\u00b7mel", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hinab in mich will ich nur immer denken,", "tokens": ["Hin\u00b7ab", "in", "mich", "will", "ich", "nur", "im\u00b7mer", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo es so falsch, so irrig f\u00fcr mich denkt.", "tokens": ["Wo", "es", "so", "falsch", ",", "so", "ir\u00b7rig", "f\u00fcr", "mich", "denkt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "$,", "ADV", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In mich hinab will meine Kraft ich senken,", "tokens": ["In", "mich", "hin\u00b7ab", "will", "mei\u00b7ne", "Kraft", "ich", "sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VMFIN", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der andern nach, die sich dorthin gesenkt.", "tokens": ["Der", "an\u00b7dern", "nach", ",", "die", "sich", "dor\u00b7thin", "ge\u00b7senkt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PTKVZ", "$,", "PRELS", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es sind nicht graus'ge Tiefen zu ergr\u00fcnden,", "tokens": ["Es", "sind", "nicht", "graus'\u00b7ge", "Tie\u00b7fen", "zu", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So weit hinab wie vorher himmelan,", "tokens": ["So", "weit", "hin\u00b7ab", "wie", "vor\u00b7her", "him\u00b7me\u00b7lan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "KOKOM", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und dennoch ist der Abgrund unsrer S\u00fcnden", "tokens": ["Und", "den\u00b7noch", "ist", "der", "Ab\u00b7grund", "uns\u00b7rer", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Das grausig Tiefste, was es geben kann.", "tokens": ["Das", "grau\u00b7sig", "Tiefs\u00b7te", ",", "was", "es", "ge\u00b7ben", "kann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADJA", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hinauf, hinab will ich nur immer denken,", "tokens": ["Hin\u00b7auf", ",", "hin\u00b7ab", "will", "ich", "nur", "im\u00b7mer", "den\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So wie man dort ja meiner stets gedenkt;", "tokens": ["So", "wie", "man", "dort", "ja", "mei\u00b7ner", "stets", "ge\u00b7denkt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PIS", "ADV", "ADV", "PPOSAT", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann werd ich mir das Allerh\u00f6chste schenken,", "tokens": ["Dann", "werd", "ich", "mir", "das", "Al\u00b7ler\u00b7h\u00f6chs\u00b7te", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nachdem ich mir das Tiefste, mich, geschenkt.", "tokens": ["Nach\u00b7dem", "ich", "mir", "das", "Tiefs\u00b7te", ",", "mich", ",", "ge\u00b7schenkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "ADJA", "$,", "PPER", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es ist nicht schwerer R\u00e4tsel Sinn zu l\u00f6sen;", "tokens": ["Es", "ist", "nicht", "schwe\u00b7rer", "R\u00e4t\u00b7sel", "Sinn", "zu", "l\u00f6\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJA", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Es st\u00fcrzt kein Himmel, keine Erde ein;", "tokens": ["Es", "st\u00fcrzt", "kein", "Him\u00b7mel", ",", "kei\u00b7ne", "Er\u00b7de", "ein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nur m\u00f6chte ich mich reinigen vom B\u00f6sen", "tokens": ["Nur", "m\u00f6ch\u00b7te", "ich", "mich", "rei\u00b7ni\u00b7gen", "vom", "B\u00f6\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und gern ein Mensch nach Gottes Willen sein.", "tokens": ["Und", "gern", "ein", "Mensch", "nach", "Got\u00b7tes", "Wil\u00b7len", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}