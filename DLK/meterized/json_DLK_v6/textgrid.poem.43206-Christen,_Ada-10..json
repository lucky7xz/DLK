{"textgrid.poem.43206": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tiefschwarze Nacht \u2013 und rastlos str\u00f6mt der Regen", "tokens": ["Tief\u00b7schwar\u00b7ze", "Nacht", "\u2013", "und", "rast\u00b7los", "str\u00f6mt", "der", "Re\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$(", "KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Eint\u00f6nig nieder auf der Gondel Dach,", "tokens": ["Ein\u00b7t\u00f6\u00b7nig", "nie\u00b7der", "auf", "der", "Gon\u00b7del", "Dach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "APPR", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Der alte Schiffer h\u00e4lt sich singend wach,", "tokens": ["Der", "al\u00b7te", "Schif\u00b7fer", "h\u00e4lt", "sich", "sin\u00b7gend", "wach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zuweilen aber murmelt er den Segen.", "tokens": ["Zu\u00b7wei\u00b7len", "a\u00b7ber", "mur\u00b7melt", "er", "den", "Se\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch er versinket bald in ernstes Schweigen", "tokens": ["Doch", "er", "ver\u00b7sin\u00b7ket", "bald", "in", "erns\u00b7tes", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und lauscht hinunter in das dunkle Meer,", "tokens": ["Und", "lauscht", "hin\u00b7un\u00b7ter", "in", "das", "dunk\u00b7le", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Schaut auf zum Himmel, schwarz und sternenleer,", "tokens": ["Schaut", "auf", "zum", "Him\u00b7mel", ",", "schwarz", "und", "ster\u00b7nen\u00b7leer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPRART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sein Lampenlicht kann keinen Weg ihm zeigen.", "tokens": ["Sein", "Lam\u00b7pen\u00b7licht", "kann", "kei\u00b7nen", "Weg", "ihm", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PIAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und als die Wogen an die Gondel schlagen,", "tokens": ["Und", "als", "die", "Wo\u00b7gen", "an", "die", "Gon\u00b7del", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Lampe schwanket und die Flamme zischt,", "tokens": ["Die", "Lam\u00b7pe", "schwan\u00b7ket", "und", "die", "Flam\u00b7me", "zischt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als sie aufflackernd knistert und erlischt,", "tokens": ["Als", "sie", "auf\u00b7fla\u00b7ckernd", "knis\u00b7tert", "und", "er\u00b7lischt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VVFIN", "KON", "VVFIN", "$,"], "meter": "--++-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Da flucht er laut, um leise dann zu klagen.", "tokens": ["Da", "flucht", "er", "laut", ",", "um", "lei\u00b7se", "dann", "zu", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "KOUI", "ADJD", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbso ist auch Dir Dein letztes Licht versunken,", "tokens": ["\u00bb", "so", "ist", "auch", "Dir", "Dein", "letz\u00b7tes", "Licht", "ver\u00b7sun\u00b7ken", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "ADV", "PPER", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So findest Du den sichern Hafen nicht,\u00ab", "tokens": ["So", "fin\u00b7dest", "Du", "den", "si\u00b7chern", "Ha\u00b7fen", "nicht", ",", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKNEG", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Grollt dumpf mein Herz. ... Da pl\u00f6tzlich ward es licht", "tokens": ["Grollt", "dumpf", "mein", "Herz", ".", "...", "Da", "pl\u00f6tz\u00b7lich", "ward", "es", "licht"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$.", "$(", "ADV", "ADJD", "VAFIN", "PPER", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und auf den Wellen tanzten goldne Funken.", "tokens": ["Und", "auf", "den", "Wel\u00b7len", "tanz\u00b7ten", "gold\u00b7ne", "Fun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Das rothe L\u00e4mpchen eines Seglers sandte", "tokens": ["Das", "ro\u00b7the", "L\u00e4mp\u00b7chen", "ei\u00b7nes", "Seg\u00b7lers", "sand\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Perlenbr\u00fccke leuchtend zu mir her ...", "tokens": ["Die", "Per\u00b7len\u00b7br\u00fc\u00b7cke", "leuch\u00b7tend", "zu", "mir", "her", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Gondoliere klagte nimmermehr,", "tokens": ["Der", "Gon\u00b7do\u00b7lie\u00b7re", "klag\u00b7te", "nim\u00b7mer\u00b7mehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als er sein Ziel durch Nacht und Sturm erkannte.", "tokens": ["Als", "er", "sein", "Ziel", "durch", "Nacht", "und", "Sturm", "er\u00b7kann\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}