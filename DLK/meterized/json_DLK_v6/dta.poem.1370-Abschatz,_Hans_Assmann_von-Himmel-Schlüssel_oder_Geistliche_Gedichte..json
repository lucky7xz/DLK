{"dta.poem.1370": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Himmel-Schl\u00fcssel  \n oder  \n Geistliche Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "GoTT liebt/ drum straffet er; Wir w\u00fcrden/ uns gelassen/", "tokens": ["GoTT", "liebt", "/", "drum", "straf\u00b7fet", "er", ";", "Wir", "w\u00fcr\u00b7den", "/", "uns", "ge\u00b7las\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$(", "PAV", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "$(", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Eitelkeit der Welt zu sehr ins Hertze fassen/", "tokens": ["Die", "Ei\u00b7tel\u00b7keit", "der", "Welt", "zu", "sehr", "ins", "Hert\u00b7ze", "fas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ADV", "APPRART", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und als ein wildes Ro\u00df/ wenn keine Straffe w\u00e4r/", "tokens": ["Und", "als", "ein", "wil\u00b7des", "Ro\u00df", "/", "wenn", "kei\u00b7ne", "Straf\u00b7fe", "w\u00e4r", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "$(", "KOUS", "PIAT", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ohn Zaum und Z\u00fcgel irrn: GOtt liebt/ drum straffet er.", "tokens": ["Ohn", "Zaum", "und", "Z\u00fc\u00b7gel", "irrn", ":", "Gott", "liebt", "/", "drum", "straf\u00b7fet", "er", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$.", "NN", "VVFIN", "$(", "PAV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}}, "stanza.2": {"line.1": {"text": "Gott strafft/ drum liebet er: Damit wir uns erkennen/", "tokens": ["Gott", "strafft", "/", "drum", "lie\u00b7bet", "er", ":", "Da\u00b7mit", "wir", "uns", "er\u00b7ken\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$(", "PAV", "VVFIN", "PPER", "$.", "KOUS", "PPER", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie wir/ zur Straffe reiff/ in Glutt der S\u00fcnden brennen/", "tokens": ["Wie", "wir", "/", "zur", "Straf\u00b7fe", "reiff", "/", "in", "Glutt", "der", "S\u00fcn\u00b7den", "bren\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$(", "APPRART", "NN", "VVFIN", "$(", "APPR", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "K\u00f6mmt seine Z\u00fcchtigung; doch k\u00f6mmt sie nie so schwer/", "tokens": ["K\u00f6mmt", "sei\u00b7ne", "Z\u00fcch\u00b7ti\u00b7gung", ";", "doch", "k\u00f6mmt", "sie", "nie", "so", "schwer", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "ADV", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als unsre Schuld verdient: GOtt strafft/ drum liebet er.", "tokens": ["Als", "uns\u00b7re", "Schuld", "ver\u00b7dient", ":", "Gott", "strafft", "/", "drum", "lie\u00b7bet", "er", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP", "$.", "NN", "VVFIN", "$(", "PAV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}}, "stanza.3": {"line.1": {"text": "Gott liebt/ drum straffet er: Auff da\u00df wir zu ihm fliehen/", "tokens": ["Gott", "liebt", "/", "drum", "straf\u00b7fet", "er", ":", "Auff", "da\u00df", "wir", "zu", "ihm", "flie\u00b7hen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$(", "PAV", "VVFIN", "PPER", "$.", "APPR", "KOUS", "PPER", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+--+-++-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "So mu\u00df uns seine Hand offt lind/ offt harte ziehen;", "tokens": ["So", "mu\u00df", "uns", "sei\u00b7ne", "Hand", "offt", "lind", "/", "offt", "har\u00b7te", "zie\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "ADV", "ADJD", "$(", "ADV", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bald lockt/ bald dringt er uns zur Bu\u00df und Wiederkehr/", "tokens": ["Bald", "lockt", "/", "bald", "dringt", "er", "uns", "zur", "Bu\u00df", "und", "Wie\u00b7der\u00b7kehr", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Will uns doch nicht verderbt: GOtt liebt/ drum straffet er.", "tokens": ["Will", "uns", "doch", "nicht", "ver\u00b7derbt", ":", "Gott", "liebt", "/", "drum", "straf\u00b7fet", "er", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$.", "NN", "VVFIN", "$(", "PAV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}}, "stanza.4": {"line.1": {"text": "Gott/ dessen Vater-Hand uns will in Wehmutt sencken/", "tokens": ["Gott", "/", "des\u00b7sen", "Va\u00b7ter\u00b7Hand", "uns", "will", "in", "Weh\u00b7mutt", "sen\u00b7cken", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PRELAT", "NN", "PPER", "VMFIN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Woll uns in Unmutt Mutt/ nach Regen Sonne schencken;", "tokens": ["Woll", "uns", "in", "Un\u00b7mutt", "Mutt", "/", "nach", "Re\u00b7gen", "Son\u00b7ne", "schen\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "NE", "$(", "APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er wende gr\u00f6sser Leyd/ und geb uns den Verstand/", "tokens": ["Er", "wen\u00b7de", "gr\u00f6s\u00b7ser", "Leyd", "/", "und", "geb", "uns", "den", "Ver\u00b7stand", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$(", "KON", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df wir mit Lieb und Furcht ihm k\u00fcssen Rutt und Hand.", "tokens": ["Da\u00df", "wir", "mit", "Lieb", "und", "Furcht", "ihm", "k\u00fcs\u00b7sen", "Rutt", "und", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN", "PPER", "VVFIN", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}