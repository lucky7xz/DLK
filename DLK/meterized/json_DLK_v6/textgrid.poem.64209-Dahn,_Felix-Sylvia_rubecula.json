{"textgrid.poem.64209": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Sylvia rubecula", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun ist Vollwinters Herrschezeit!", "tokens": ["Nun", "ist", "Voll\u00b7win\u00b7ters", "Herr\u00b7sche\u00b7zeit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Licht ist schmal, die Nacht ist breit,", "tokens": ["Das", "Licht", "ist", "schmal", ",", "die", "Nacht", "ist", "breit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Frau Sonne will kaum blicken:", "tokens": ["Frau", "Son\u00b7ne", "will", "kaum", "bli\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bricht mittags sie durchs Wolkenkleid, \u2013", "tokens": ["Bricht", "mit\u00b7tags", "sie", "durchs", "Wol\u00b7ken\u00b7kleid", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "PPER", "APPRART", "NN", "$,", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Herr Nieselnebel h\u00e4lt bereit", "tokens": ["Herr", "Nie\u00b7sel\u00b7ne\u00b7bel", "h\u00e4lt", "be\u00b7reit"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NE", "VVFIN", "ADJD"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Den Mantel, sie zu sticken.", "tokens": ["Den", "Man\u00b7tel", ",", "sie", "zu", "sti\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da singt kein V\u00f6glein mehr im Feld:", "tokens": ["Da", "singt", "kein", "V\u00f6\u00b7glein", "mehr", "im", "Feld", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zaunk\u00f6nig nur, der wen'ge Held,", "tokens": ["Zaun\u00b7k\u00f6\u00b7nig", "nur", ",", "der", "wen'\u00b7ge", "Held", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PRELS", "PIAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Schwirrt fr\u00f6hlich seine Weise,", "tokens": ["Schwirrt", "fr\u00f6h\u00b7lich", "sei\u00b7ne", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Goldh\u00e4hnchen huscht durchs Flockenzelt", "tokens": ["Gold\u00b7h\u00e4hn\u00b7chen", "huscht", "durchs", "Flo\u00b7cken\u00b7zelt"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und, wem das letzte N\u00fc\u00dflein f\u00e4llt,", "tokens": ["Und", ",", "wem", "das", "letz\u00b7te", "N\u00fc\u00df\u00b7lein", "f\u00e4llt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zankt klopfend Specht und Meise.", "tokens": ["Zankt", "klop\u00b7fend", "Specht", "und", "Mei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Auch ich halt' stumm im Hause Ruh'", "tokens": ["Auch", "ich", "halt'", "stumm", "im", "Hau\u00b7se", "Ruh'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "ADJD", "APPRART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und st\u00f6bre tief in staub'ger Truh", "tokens": ["Und", "st\u00f6b\u00b7re", "tief", "in", "staub'\u00b7ger", "Truh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch Schrift und Pergamente:", "tokens": ["Durch", "Schrift", "und", "Per\u00b7ga\u00b7men\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Rot glimmt der Sandelspan dazu: \u2013", "tokens": ["Rot", "glimmt", "der", "San\u00b7del\u00b7span", "da\u00b7zu", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Ei, duftend Holz, nicht ahntest du,", "tokens": ["Ei", ",", "duf\u00b7tend", "Holz", ",", "nicht", "ahn\u00b7test", "du", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "NN", "$,", "PTKNEG", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df man am Main dich brennte. \u2013", "tokens": ["Da\u00df", "man", "am", "Main", "dich", "brenn\u00b7te", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PIS", "APPRART", "NN", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das war im Goldhaus zu Byzanz,", "tokens": ["Das", "war", "im", "Gold\u00b7haus", "zu", "By\u00b7zanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Bei Myrrhenrauch, in Marmorglanz,", "tokens": ["Bei", "Myr\u00b7rhen\u00b7rauch", ",", "in", "Mar\u00b7mor\u00b7glanz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bei schmucken Griechenknaben,", "tokens": ["Bei", "schmu\u00b7cken", "Grie\u00b7chen\u00b7kna\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df unter Zyproswein und Tanz", "tokens": ["Da\u00df", "un\u00b7ter", "Zy\u00b7pros\u00b7wein", "und", "Tanz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NE", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie dich mit manchem Ring und Kranz", "tokens": ["Sie", "dich", "mit", "man\u00b7chem", "Ring", "und", "Kranz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PRF", "APPR", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zum Gastgeschenk mir gaben.", "tokens": ["Zum", "Gast\u00b7ge\u00b7schenk", "mir", "ga\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da ging, mit rotem Seidenlatz", "tokens": ["Da", "ging", ",", "mit", "ro\u00b7tem", "Sei\u00b7den\u00b7latz"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verh\u00fcllt den keuschen Herzensplatz,", "tokens": ["Ver\u00b7h\u00fcllt", "den", "keu\u00b7schen", "Her\u00b7zen\u00b7splatz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Griechenkind mit Neigen:", "tokens": ["Ein", "Grie\u00b7chen\u00b7kind", "mit", "Nei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hell Scharlach war ihr Busenlatz: \u2013", "tokens": ["Hell", "Schar\u00b7lach", "war", "ihr", "Bu\u00b7sen\u00b7latz", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie war ein anmutvoller Schatz", "tokens": ["Sie", "war", "ein", "an\u00b7mut\u00b7vol\u00b7ler", "Schatz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im Reden und im Schweigen.", "tokens": ["Im", "Re\u00b7den", "und", "im", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Im harten, deutschen Winter lind", "tokens": ["Im", "har\u00b7ten", ",", "deut\u00b7schen", "Win\u00b7ter", "lind"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mahnt mich an jenes Griechenkind", "tokens": ["Mahnt", "mich", "an", "je\u00b7nes", "Grie\u00b7chen\u00b7kind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Neigen, H\u00fcpfen, Klingen:", "tokens": ["Ein", "Nei\u00b7gen", ",", "H\u00fcp\u00b7fen", ",", "Klin\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Denn um mich huscht und schwebt geschwind", "tokens": ["Denn", "um", "mich", "huscht", "und", "schwebt", "ge\u00b7schwind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein V\u00f6glein, wie nicht viele sind, \u2013", "tokens": ["Ein", "V\u00f6\u00b7glein", ",", "wie", "nicht", "vie\u00b7le", "sind", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PTKNEG", "PIS", "VAFIN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Will auch im Winter singen.", "tokens": ["Will", "auch", "im", "Win\u00b7ter", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die Griechin, die hie\u00df Sylvia:", "tokens": ["Die", "Grie\u00b7ch\u00b7in", ",", "die", "hie\u00df", "Syl\u00b7via", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was wohl noch mit dem Kind geschah? \u2013", "tokens": ["Was", "wohl", "noch", "mit", "dem", "Kind", "ge\u00b7schah", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Rein war ihr zartes Seelchen: \u2013", "tokens": ["Rein", "war", "ihr", "zar\u00b7tes", "Seel\u00b7chen", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir ruft ihr lieblich Bildnis nah", "tokens": ["Mir", "ruft", "ihr", "lieb\u00b7lich", "Bild\u00b7nis", "nah"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hier Sylvia rubecula,", "tokens": ["Hier", "Syl\u00b7via", "ru\u00b7be\u00b7cu\u00b7la", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NE", "NE", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Mein Hausgeist, mein Rotkehlchen. \u2013", "tokens": ["Mein", "Haus\u00b7geist", ",", "mein", "Rot\u00b7kehl\u00b7chen", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$.", "$("], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Der Rauch zieht aus dem Sandel schwer:", "tokens": ["Der", "Rauch", "zieht", "aus", "dem", "San\u00b7del", "schwer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bald seh' ich V\u00f6glein um mich her,", "tokens": ["Bald", "seh'", "ich", "V\u00f6\u00b7glein", "um", "mich", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bald Griechenm\u00e4gdlein schweben.", "tokens": ["Bald", "Grie\u00b7chen\u00b7m\u00e4gd\u00b7lein", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich denk', ich schlafe: \u2013 doch vorher", "tokens": ["Ich", "denk'", ",", "ich", "schla\u00b7fe", ":", "\u2013", "doch", "vor\u00b7her"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$.", "$(", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Trink' ich den tiefen Becher leer \u2013:", "tokens": ["Trink'", "ich", "den", "tie\u00b7fen", "Be\u00b7cher", "leer", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "ADJD", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was lieblich ist, soll leben!", "tokens": ["Was", "lieb\u00b7lich", "ist", ",", "soll", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}