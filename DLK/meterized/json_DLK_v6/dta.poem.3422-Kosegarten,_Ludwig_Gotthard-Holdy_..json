{"dta.poem.3422": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Holdy .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hulda, f&#252;hlst du auch diess Beben,", "tokens": ["Hul\u00b7da", ",", "f", "&#252;", "hlst", "du", "auch", "diess", "Be\u00b7ben", ","], "token_info": ["word", "punct", "word", "XML_entity", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$(", "VVFIN", "PPER", "ADV", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dieses Ahnen, dieses Streben,", "tokens": ["Die\u00b7ses", "Ah\u00b7nen", ",", "die\u00b7ses", "Stre\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieses Schauern, Schlag auf Schlag?", "tokens": ["Die\u00b7ses", "Schau\u00b7ern", ",", "Schlag", "auf", "Schlag", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "T\u00e4uscht mich nur ertr\u00e4umte Wonne,", "tokens": ["T\u00e4uscht", "mich", "nur", "er\u00b7tr\u00e4um\u00b7te", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Oder r\u00f6thet uns die Sonne", "tokens": ["O\u00b7der", "r\u00f6\u00b7thet", "uns", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wahrlich heut den Bundestag?", "tokens": ["Wahr\u00b7lich", "heut", "den", "Bun\u00b7des\u00b7tag", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ja, mein Holdy, unser Sehnen,", "tokens": ["Ja", ",", "mein", "Hol\u00b7dy", ",", "un\u00b7ser", "Seh\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unser Harren, unsre Thr\u00e4nen", "tokens": ["Un\u00b7ser", "Har\u00b7ren", ",", "uns\u00b7re", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sind dem hohen Lohne nah.", "tokens": ["Sind", "dem", "ho\u00b7hen", "Loh\u00b7ne", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Holdy, Holdy! ach, der lange", "tokens": ["Hol\u00b7dy", ",", "Hol\u00b7dy", "!", "ach", ",", "der", "lan\u00b7ge"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$,", "NE", "$.", "XY", "$,", "PRELS", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hergesehnte, heiss und bange", "tokens": ["Her\u00b7ge\u00b7sehn\u00b7te", ",", "heiss", "und", "ban\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADJA", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hergeweinte Tag ist da.", "tokens": ["Her\u00b7ge\u00b7wein\u00b7te", "Tag", "ist", "da", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie erbebet, meine Beste,", "tokens": ["Wie", "er\u00b7be\u00b7bet", ",", "mei\u00b7ne", "Bes\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ersch\u00fcttert meine Feste!", "tokens": ["Wie", "er\u00b7sch\u00fct\u00b7tert", "mei\u00b7ne", "Fes\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Werde leiser, Sturm der Lust!", "tokens": ["Wer\u00b7de", "lei\u00b7ser", ",", "Sturm", "der", "Lust", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Herz, ach Herz, zerschmilz in Thr\u00e4nen!", "tokens": ["Herz", ",", "ach", "Herz", ",", "zer\u00b7schmilz", "in", "Thr\u00e4\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "XY", "NN", "$,", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses Schwellen, dieses Sehnen", "tokens": ["Die\u00b7ses", "Schwel\u00b7len", ",", "die\u00b7ses", "Seh\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sprengt mir sonst die wunde Brust.", "tokens": ["Sprengt", "mir", "sonst", "die", "wun\u00b7de", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Du in deinem Brautgeschmeide,", "tokens": ["Du", "in", "dei\u00b7nem", "Braut\u00b7ge\u00b7schmei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deiner hochzeitlichen Seide,", "tokens": ["Dei\u00b7ner", "hoch\u00b7zeit\u00b7li\u00b7chen", "Sei\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Sey mir tausendmal gegr\u00fcsst.", "tokens": ["Sey", "mir", "tau\u00b7send\u00b7mal", "ge\u00b7gr\u00fcs\u00b7st", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Du in deinen Flammenblicken,", "tokens": ["Du", "in", "dei\u00b7nen", "Flam\u00b7men\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deinem Br\u00e4utigamsentz\u00fccken,", "tokens": ["Dei\u00b7nem", "Br\u00e4u\u00b7ti\u00b7gam\u00b7sent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sey mit Brautkuss mir gek\u00fcsst.", "tokens": ["Sey", "mit", "Braut\u00b7kuss", "mir", "ge\u00b7k\u00fcsst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Hulda, Hulda! meine Eine,", "tokens": ["Hul\u00b7da", ",", "Hul\u00b7da", "!", "mei\u00b7ne", "Ei\u00b7ne", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$.", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine S\u00fcsse, meine Reine,", "tokens": ["Mei\u00b7ne", "S\u00fcs\u00b7se", ",", "mei\u00b7ne", "Rei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bist du wahrlich ewig mein?", "tokens": ["Bist", "du", "wahr\u00b7lich", "e\u00b7wig", "mein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ja, mein Liebling, mein Erw\u00e4hlter,", "tokens": ["Ja", ",", "mein", "Lieb\u00b7ling", ",", "mein", "Er\u00b7w\u00e4hl\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mein Verlobter, mein Verm\u00e4hlter,", "tokens": ["Mein", "Ver\u00b7lob\u00b7ter", ",", "mein", "Ver\u00b7m\u00e4hl\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ewig bin ich, ewig dein.", "tokens": ["E\u00b7wig", "bin", "ich", ",", "e\u00b7wig", "dein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "ADJD", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Komm' in meinem Arm, Geliebte!", "tokens": ["Komm'", "in", "mei\u00b7nem", "Arm", ",", "Ge\u00b7lieb\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lass mich dir ins stillgetr\u00fcbte,", "tokens": ["Lass", "mich", "dir", "ins", "still\u00b7ge\u00b7tr\u00fcb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "APPRART", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dunkelbraune Auge sehn.", "tokens": ["Dun\u00b7kel\u00b7brau\u00b7ne", "Au\u00b7ge", "sehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lass der Liebe heilgen Odem,", "tokens": ["Lass", "der", "Lie\u00b7be", "heil\u00b7gen", "O\u00b7dem", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Warm und lieb, wie Mayenodem,", "tokens": ["Warm", "und", "lieb", ",", "wie", "Ma\u00b7yen\u00b7o\u00b7dem", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "PWAV", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Warm und duftig mich umwehn.", "tokens": ["Warm", "und", "duf\u00b7tig", "mich", "um\u00b7wehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Lass mich, dir ins Antlitz blickend,", "tokens": ["Lass", "mich", ",", "dir", "ins", "Ant\u00b7litz", "bli\u00b7ckend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PPER", "APPRART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Innig an mein Herz dich dr\u00fcckend,", "tokens": ["In\u00b7nig", "an", "mein", "Herz", "dich", "dr\u00fc\u00b7ckend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Meiner sch\u00f6nen Wahl mich freun!", "tokens": ["Mei\u00b7ner", "sch\u00f6\u00b7nen", "Wahl", "mich", "freun", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lass uns, Hand in Hand geschlossen,", "tokens": ["Lass", "uns", ",", "Hand", "in", "Hand", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Lass uns, Herz in Herz ergossen,", "tokens": ["Lass", "uns", ",", "Herz", "in", "Herz", "er\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unsern Schwur noch eins ereun.", "tokens": ["Un\u00b7sern", "Schwur", "noch", "eins", "e\u00b7reun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PIS", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Holdy, Holdy, mein Erw\u00e4hlter!", "tokens": ["Hol\u00b7dy", ",", "Hol\u00b7dy", ",", "mein", "Er\u00b7w\u00e4hl\u00b7ter", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mein vor Gott und Welt Verm\u00e4hlter!", "tokens": ["Mein", "vor", "Gott", "und", "Welt", "Ver\u00b7m\u00e4hl\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "APPR", "NN", "KON", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Warum unsern Schwur erneun?", "tokens": ["Wa\u00b7rum", "un\u00b7sern", "Schwur", "er\u00b7neun", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Darf es, dass ich dir geboren,", "tokens": ["Darf", "es", ",", "dass", "ich", "dir", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dass mein Herz nur dich erkohren,", "tokens": ["Dass", "mein", "Herz", "nur", "dich", "er\u00b7koh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Darf es Schwurs, mein Herz sey dein?", "tokens": ["Darf", "es", "Schwurs", ",", "mein", "Herz", "sey", "dein", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "$,", "PPOSAT", "NN", "VAFIN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Himmel wird durch Sturm geheitert,", "tokens": ["Him\u00b7mel", "wird", "durch", "Sturm", "ge\u00b7hei\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gold durch Feuersgluth gel\u00e4utert,", "tokens": ["Gold", "durch", "Feu\u00b7ers\u00b7gluth", "ge\u00b7l\u00e4u\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Liebe wird durch Weh bew\u00e4hrt.", "tokens": ["Lie\u00b7be", "wird", "durch", "Weh", "be\u00b7w\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Holdy, Holdy! unsre Liebe", "tokens": ["Hol\u00b7dy", ",", "Hol\u00b7dy", "!", "uns\u00b7re", "Lie\u00b7be"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$,", "NE", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ward durch Stunden hell und tr\u00fcbe,", "tokens": ["Ward", "durch", "Stun\u00b7den", "hell", "und", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "ADJD", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ward durch Lust und Schmerz bew\u00e4hrt.", "tokens": ["Ward", "durch", "Lust", "und", "Schmerz", "be\u00b7w\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Denk zur\u00fcck an jene Stunden,", "tokens": ["Denk", "zu\u00b7r\u00fcck", "an", "je\u00b7ne", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo ich, ach, zuerst empfunden,", "tokens": ["Wo", "ich", ",", "ach", ",", "zu\u00b7erst", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ITJ", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dass ich dir 