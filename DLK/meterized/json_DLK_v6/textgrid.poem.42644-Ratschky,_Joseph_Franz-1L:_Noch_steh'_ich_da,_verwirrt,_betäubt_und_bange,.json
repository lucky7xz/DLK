{"textgrid.poem.42644": {"metadata": {"author": {"name": "Ratschky, Joseph Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Noch steh' ich da, verwirrt, bet\u00e4ubt und bange,", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch steh' ich da, verwirrt, bet\u00e4ubt und bange,", "tokens": ["Noch", "steh'", "ich", "da", ",", "ver\u00b7wirrt", ",", "be\u00b7t\u00e4ubt", "und", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ADJD", "$,", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit starren Augen, leichenblass,", "tokens": ["Mit", "star\u00b7ren", "Au\u00b7gen", ",", "lei\u00b7chen\u00b7blass", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die F\u00fcsse schwach und wankend, Stirn' und Wange", "tokens": ["Die", "F\u00fcs\u00b7se", "schwach", "und", "wan\u00b7kend", ",", "Stirn'", "und", "Wan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von kaltem Angstschweiss nass.", "tokens": ["Von", "kal\u00b7tem", "A\u00b7ngstschweiss", "nass", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Denn ach! ich sah die schreckliche Verheerung,", "tokens": ["Denn", "ach", "!", "ich", "sah", "die", "schreck\u00b7li\u00b7che", "Ver\u00b7hee\u00b7rung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie unter Prasseln Stein und Kloss,", "tokens": ["Wie", "un\u00b7ter", "Pras\u00b7seln", "Stein", "und", "Kloss", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Emporgetrieben durch des Pulvers G\u00e4hrung,", "tokens": ["Em\u00b7por\u00b7ge\u00b7trie\u00b7ben", "durch", "des", "Pul\u00b7vers", "G\u00e4h\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hoch in die L\u00fcfte schoss.", "tokens": ["Hoch", "in", "die", "L\u00fcf\u00b7te", "schoss", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich sah, wie ringsum, gleich ergrimmten Schlossen,", "tokens": ["Ich", "sah", ",", "wie", "ring\u00b7sum", ",", "gleich", "er\u00b7grimm\u00b7ten", "Schlos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADV", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Kugeln unter Wuth und Graus", "tokens": ["Die", "Ku\u00b7geln", "un\u00b7ter", "Wuth", "und", "Graus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit donnerndem Geknalle niederschossen", "tokens": ["Mit", "don\u00b7nern\u00b7dem", "Ge\u00b7knal\u00b7le", "nie\u00b7der\u00b7schos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf Garten, Feld und Haus;", "tokens": ["Auf", "Gar\u00b7ten", ",", "Feld", "und", "Haus", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sah aufgeschlitzt die Mauern der Geb\u00e4ude,", "tokens": ["Sah", "auf\u00b7ge\u00b7schlitzt", "die", "Mau\u00b7ern", "der", "Ge\u00b7b\u00e4u\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der D\u00e4cher Giebel eingedr\u00fcckt,", "tokens": ["Der", "D\u00e4\u00b7cher", "Gie\u00b7bel", "ein\u00b7ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Flur mit Schutt bedeckt, und Buch' und Weide", "tokens": ["Die", "Flur", "mit", "Schutt", "be\u00b7deckt", ",", "und", "Buch'", "und", "Wei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "$,", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie schwaches Rohr zerknickt.", "tokens": ["Wie", "schwa\u00b7ches", "Rohr", "zer\u00b7knickt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Noch hallt das Schreyn der S\u00e4uglinge, das Wimmern", "tokens": ["Noch", "hallt", "das", "Schreyn", "der", "S\u00e4ug\u00b7lin\u00b7ge", ",", "das", "Wim\u00b7mern"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Mutter, die ihr Kind verlor,", "tokens": ["Der", "Mut\u00b7ter", ",", "die", "ihr", "Kind", "ver\u00b7lor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der bange Todeslaut der unter Tr\u00fcmmern", "tokens": ["Der", "ban\u00b7ge", "To\u00b7des\u00b7laut", "der", "un\u00b7ter", "Tr\u00fcm\u00b7mern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zerquetschten mir im Ohr.", "tokens": ["Zer\u00b7quetschten", "mir", "im", "Ohr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Ha! welch ein Anblick! Ach! auf jeder Seite", "tokens": ["Ha", "!", "welch", "ein", "An\u00b7blick", "!", "Ach", "!", "auf", "je\u00b7der", "Sei\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "PWAT", "ART", "NN", "$.", "ITJ", "$.", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der gr\u00e4sslichen Verw\u00fcstung Spur:", "tokens": ["Der", "gr\u00e4ss\u00b7li\u00b7chen", "Ver\u00b7w\u00fcs\u00b7tung", "Spur", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hier Leichen, dort noch wild im Todesstreite", "tokens": ["Hier", "Lei\u00b7chen", ",", "dort", "noch", "wild", "im", "To\u00b7des\u00b7strei\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "$,", "ADV", "ADV", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die k\u00e4mpfende Natur.", "tokens": ["Die", "k\u00e4mp\u00b7fen\u00b7de", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+---+", "measure": "dactylic.init"}}, "stanza.7": {"line.1": {"text": "Ein Unmensch ist's, den nicht die schwarze Scene", "tokens": ["Ein", "Un\u00b7mensch", "ist's", ",", "den", "nicht", "die", "schwar\u00b7ze", "Sce\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$,", "PRELS", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Elends zum Erbarmen zwingt,", "tokens": ["Des", "E\u00b7lends", "zum", "Er\u00b7bar\u00b7men", "zwingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem sanft und mild des Mitleids edle Thr\u00e4ne", "tokens": ["Dem", "sanft", "und", "mild", "des", "Mit\u00b7leids", "ed\u00b7le", "Thr\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "KON", "ADJD", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht in das Auge dringt.", "tokens": ["Nicht", "in", "das", "Au\u00b7ge", "dringt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "O sieh! wie rang Theresens Sohn die H\u00e4nde,", "tokens": ["O", "sieh", "!", "wie", "rang", "The\u00b7re\u00b7sens", "Sohn", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "PWAV", "VVFIN", "NE", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und stand voll bittern Jammers da,", "tokens": ["Und", "stand", "voll", "bit\u00b7tern", "Jam\u00b7mers", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als er, umringt von Reihn zermalmter W\u00e4nde,", "tokens": ["Als", "er", ",", "um\u00b7ringt", "von", "Reihn", "zer\u00b7malm\u00b7ter", "W\u00e4n\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "VVPP", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das wilde Schauspiel sah!", "tokens": ["Das", "wil\u00b7de", "Schau\u00b7spiel", "sah", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Sein Blick entdeckt ein Schlachtfeld voll von Todten:", "tokens": ["Sein", "Blick", "ent\u00b7deckt", "ein", "Schlacht\u00b7feld", "voll", "von", "Tod\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "ART", "NN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch klomm er mit entschlossnem Sinn", "tokens": ["Doch", "klomm", "er", "mit", "ent\u00b7schloss\u00b7nem", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Schutt, auf Schutt, und sandte seine Boten,", "tokens": ["Von", "Schutt", ",", "auf", "Schutt", ",", "und", "sand\u00b7te", "sei\u00b7ne", "Bo\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu retten, her und hin.", "tokens": ["Zu", "ret\u00b7ten", ",", "her", "und", "hin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Gott lohn' ihm f\u00fcr die v\u00e4terliche Liebe!", "tokens": ["Gott", "lohn'", "ihm", "f\u00fcr", "die", "v\u00e4\u00b7ter\u00b7li\u00b7che", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gott lohn' ihm! Hohe Dankbegier", "tokens": ["Gott", "lohn'", "ihm", "!", "Ho\u00b7he", "Dank\u00b7be\u00b7gier"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$.", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "F\u00fcllt jedes Herz, und mit dem w\u00e4rmsten Triebe", "tokens": ["F\u00fcllt", "je\u00b7des", "Herz", ",", "und", "mit", "dem", "w\u00e4rms\u00b7ten", "Trie\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "$,", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Inbrunst bitten wir:", "tokens": ["Der", "In\u00b7brunst", "bit\u00b7ten", "wir", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Erhalt ihn uns! verleih ihm deinen Segen,", "tokens": ["Er\u00b7halt", "ihn", "uns", "!", "ver\u00b7leih", "ihm", "dei\u00b7nen", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "$.", "VVIMP", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O Gott, und deiner Allmacht Schutz!", "tokens": ["O", "Gott", ",", "und", "dei\u00b7ner", "All\u00b7macht", "Schutz", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Engel sey auf allen seinen Wegen", "tokens": ["Dein", "En\u00b7gel", "sey", "auf", "al\u00b7len", "sei\u00b7nen", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der H\u00fcter seines Muths!", "tokens": ["Der", "H\u00fc\u00b7ter", "sei\u00b7nes", "Muths", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}