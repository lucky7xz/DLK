{"dta.poem.21369": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XVI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Sto\u00dfen des Wagens weckte mich auf,", "tokens": ["Das", "Sto\u00b7\u00dfen", "des", "Wa\u00b7gens", "weck\u00b7te", "mich", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Doch sanken die Augenlieder", "tokens": ["Doch", "san\u00b7ken", "die", "Au\u00b7gen\u00b7lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bald wieder zu, und ich entschlief", "tokens": ["Bald", "wie\u00b7der", "zu", ",", "und", "ich", "ent\u00b7schlief"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "PTKVZ", "$,", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und tr\u00e4umte vom Rothbart wieder.", "tokens": ["Und", "tr\u00e4um\u00b7te", "vom", "Roth\u00b7bart", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ADV", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}