{"textgrid.poem.42032": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Das Karussell", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie standen stumm und lauschten dem Get\u00f6n", "tokens": ["Sie", "stan\u00b7den", "stumm", "und", "lauschten", "dem", "Ge\u00b7t\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Verstimmter Instrumente tief in Schlaf:", "tokens": ["Ver\u00b7stimm\u00b7ter", "Inst\u00b7ru\u00b7men\u00b7te", "tief", "in", "Schlaf", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die starren Tiere, bunt und wundersch\u00f6n.", "tokens": ["Die", "star\u00b7ren", "Tie\u00b7re", ",", "bunt", "und", "wun\u00b7der\u00b7sch\u00f6n", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da sie ein Kinderblick in Schmerz betraf,", "tokens": ["Da", "sie", "ein", "Kin\u00b7der\u00b7blick", "in", "Schmerz", "be\u00b7traf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Erwachten sie. Die L\u00f6wenm\u00e4hne flog", "tokens": ["Er\u00b7wach\u00b7ten", "sie", ".", "Die", "L\u00f6\u00b7wen\u00b7m\u00e4h\u00b7ne", "flog"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im Wind. So klang vom Elefantenzahn", "tokens": ["Im", "Wind", ".", "So", "klang", "vom", "E\u00b7lef\u00b7an\u00b7ten\u00b7zahn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$.", "ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gel\u00e4ut der Schellen. R\u00fcssel schnob. Es zog", "tokens": ["Ge\u00b7l\u00e4ut", "der", "Schel\u00b7len", ".", "R\u00fcs\u00b7sel", "schnob", ".", "Es", "zog"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "NN", "VVFIN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In langem Zug die stolze Karawane", "tokens": ["In", "lan\u00b7gem", "Zug", "die", "stol\u00b7ze", "Ka\u00b7ra\u00b7wa\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dahin. Vor ihrem steilen Aufbruch lag", "tokens": ["Da\u00b7hin", ".", "Vor", "ih\u00b7rem", "stei\u00b7len", "Auf\u00b7bruch", "lag"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "$.", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Palmenwald, verstrickt in Abenteuer,", "tokens": ["Ein", "Pal\u00b7men\u00b7wald", ",", "ver\u00b7strickt", "in", "A\u00b7bent\u00b7eu\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus Lichtraketen scho\u00df der hei\u00dfe Tag,", "tokens": ["Aus", "Lich\u00b7tra\u00b7ke\u00b7ten", "scho\u00df", "der", "hei\u00b7\u00dfe", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kakteen brannten, purpurn, ungeheuer.", "tokens": ["Kak\u00b7te\u00b7en", "brann\u00b7ten", ",", "pur\u00b7purn", ",", "un\u00b7ge\u00b7heu\u00b7er", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NE", "$,", "ADJD", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}