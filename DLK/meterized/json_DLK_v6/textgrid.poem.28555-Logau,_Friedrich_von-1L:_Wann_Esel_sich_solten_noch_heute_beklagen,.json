{"textgrid.poem.28555": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wann Esel sich solten noch heute beklagen,", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann Esel sich solten noch heute beklagen,", "tokens": ["Wann", "E\u00b7sel", "sich", "sol\u00b7ten", "noch", "heu\u00b7te", "be\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PRF", "VMFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wie man sie wil wieder Geb\u00fchrn\u00fcsse plagen,", "tokens": ["Wie", "man", "sie", "wil", "wie\u00b7der", "Ge\u00b7b\u00fchr\u00b7n\u00fcs\u00b7se", "pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "VMFIN", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So trete Schaf, Ochse, Pferd, alles Vieh, bey", "tokens": ["So", "tre\u00b7te", "Schaf", ",", "O\u00b7chse", ",", "Pferd", ",", "al\u00b7les", "Vieh", ",", "bey"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "ADJA", "NN", "$,", "NN", "$,", "NN", "$,", "PIAT", "NN", "$,", "APPR"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und f\u00fchrten auff Krieger ein m\u00e4chtig Geschrey.", "tokens": ["Und", "f\u00fchr\u00b7ten", "auff", "Krie\u00b7ger", "ein", "m\u00e4ch\u00b7tig", "Ge\u00b7schrey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "ADJD", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}