{"textgrid.poem.33590": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf dem Vesuv", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir hatten uns am Kraterrand", "tokens": ["Wir", "hat\u00b7ten", "uns", "am", "Kra\u00b7ter\u00b7rand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Fackeln angez\u00fcndet", "tokens": ["Die", "Fa\u00b7ckeln", "an\u00b7ge\u00b7z\u00fcn\u00b7det"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und schwangen nun in unsrer Hand", "tokens": ["Und", "schwan\u00b7gen", "nun", "in", "uns\u00b7rer", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Glut vom Feuerherde,", "tokens": ["Die", "Glut", "vom", "Feu\u00b7er\u00b7her\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der aus dem Grund der Erde", "tokens": ["Der", "aus", "dem", "Grund", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In Flammen sich entschl\u00fcndet.", "tokens": ["In", "Flam\u00b7men", "sich", "ent\u00b7schl\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich lie\u00df voraus den F\u00fchrer gehn", "tokens": ["Ich", "lie\u00df", "vo\u00b7raus", "den", "F\u00fch\u00b7rer", "gehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und blieb in Nacht und Stille", "tokens": ["Und", "blieb", "in", "Nacht", "und", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Allein noch bei den Felsen stehn,", "tokens": ["Al\u00b7lein", "noch", "bei", "den", "Fel\u00b7sen", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur \u00fcber mir die Sterne,", "tokens": ["Nur", "\u00fc\u00b7ber", "mir", "die", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nur tief aus dunkler Ferne", "tokens": ["Nur", "tief", "aus", "dunk\u00b7ler", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Nachtgesang der Grille.", "tokens": ["Der", "Nacht\u00b7ge\u00b7sang", "der", "Gril\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nur hie und da ein Meteor", "tokens": ["Nur", "hie", "und", "da", "ein", "Me\u00b7te\u00b7or"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "KON", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stieg aus den Kratertiefen", "tokens": ["Stieg", "aus", "den", "Kra\u00b7ter\u00b7tie\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ins schweigende Azur empor", "tokens": ["Ins", "schwei\u00b7gen\u00b7de", "A\u00b7zur", "em\u00b7por"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Und zeigte mir die Spuren", "tokens": ["Und", "zeig\u00b7te", "mir", "die", "Spu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Erloschner Lavafluren,", "tokens": ["Er\u00b7lo\u00b7schner", "La\u00b7va\u00b7flu\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die ringsum lautlos schliefen.", "tokens": ["Die", "ring\u00b7sum", "laut\u00b7los", "schlie\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Welch ungeheures Totenreich!", "tokens": ["Welch", "un\u00b7ge\u00b7heu\u00b7res", "To\u00b7ten\u00b7reich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und au\u00dfer mir kein Leben,", "tokens": ["Und", "au\u00b7\u00dfer", "mir", "kein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kein Leben f\u00fchlt' ich, und zugleich", "tokens": ["Kein", "Le\u00b7ben", "f\u00fchlt'", "ich", ",", "und", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "$,", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fchlt' ich ein t\u00f6dlich Trauern,", "tokens": ["F\u00fchlt'", "ich", "ein", "t\u00f6d\u00b7lich", "Trau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein namenloses Schauern", "tokens": ["Ein", "na\u00b7men\u00b7lo\u00b7ses", "Schau\u00b7ern"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mein einsam Herz durchbeben.", "tokens": ["Mein", "ein\u00b7sam", "Herz", "durch\u00b7be\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich sah in dieser dunkeln Kraft,", "tokens": ["Ich", "sah", "in", "die\u00b7ser", "dun\u00b7keln", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die ewig g\u00e4rt und nimmer", "tokens": ["Die", "e\u00b7wig", "g\u00e4rt", "und", "nim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVFIN", "KON", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Trotz aller Gluten Segen schafft,", "tokens": ["Trotz", "al\u00b7ler", "Glu\u00b7ten", "Se\u00b7gen", "schafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Abbild eines Strebens,", "tokens": ["Das", "Ab\u00b7bild", "ei\u00b7nes", "Stre\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das gro\u00df ist, doch vergebens,", "tokens": ["Das", "gro\u00df", "ist", ",", "doch", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "$,", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das sch\u00f6n ist, doch nur Schimmer.", "tokens": ["Das", "sch\u00f6n", "ist", ",", "doch", "nur", "Schim\u00b7mer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VAFIN", "$,", "ADV", "ADV", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Unendlich einsam f\u00fchlt' ich mich;", "tokens": ["Un\u00b7end\u00b7lich", "ein\u00b7sam", "f\u00fchlt'", "ich", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir war's, als ob der warme", "tokens": ["Mir", "wa\u00b7r's", ",", "als", "ob", "der", "war\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "KOUS", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus meiner Brust der Odem wich',", "tokens": ["Aus", "mei\u00b7ner", "Brust", "der", "O\u00b7dem", "wich'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als s\u00e4nk' ich schon den kalten", "tokens": ["Als", "s\u00e4nk'", "ich", "schon", "den", "kal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPER", "ADV", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Planetischen Gewalten", "tokens": ["Pla\u00b7ne\u00b7ti\u00b7schen", "Ge\u00b7wal\u00b7ten"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Versteinert in die Arme.", "tokens": ["Ver\u00b7stei\u00b7nert", "in", "die", "Ar\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und eine Sehnsucht ging mich an", "tokens": ["Und", "ei\u00b7ne", "Sehn\u00b7sucht", "ging", "mich", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach oft geschm\u00e4hten Banden;", "tokens": ["Nach", "oft", "ge\u00b7schm\u00e4h\u00b7ten", "Ban\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mich zog's nach allem Weh und Wahn", "tokens": ["Mich", "zo\u00b7g's", "nach", "al\u00b7lem", "Weh", "und", "Wahn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIS", "NN", "KON", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Des Erdenlebens wieder.", "tokens": ["Des", "Er\u00b7den\u00b7le\u00b7bens", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Erh\u00f6hter stieg ich nieder,", "tokens": ["Er\u00b7h\u00f6h\u00b7ter", "stieg", "ich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Als oben ich gestanden.", "tokens": ["Als", "o\u00b7ben", "ich", "ge\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wie leuchtete das Licht so sch\u00f6n", "tokens": ["Wie", "leuch\u00b7te\u00b7te", "das", "Licht", "so", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus den gestirnten Fluren", "tokens": ["Aus", "den", "ge\u00b7stirn\u00b7ten", "Flu\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auf Buchten, Haine, Rebenh\u00f6h'n", "tokens": ["Auf", "Buch\u00b7ten", ",", "Hai\u00b7ne", ",", "Re\u00b7ben\u00b7h\u00f6h'n"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "NN", "$,", "NE", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durchs Dunkel der Kastanien!", "tokens": ["Durchs", "Dun\u00b7kel", "der", "Kas\u00b7ta\u00b7ni\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+---", "measure": "iambic.di.relaxed"}, "line.5": {"text": "Die Nacht lag auf Campanien", "tokens": ["Die", "Nacht", "lag", "auf", "Cam\u00b7pa\u00b7ni\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NE"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und auf dem Meer azuren.", "tokens": ["Und", "auf", "dem", "Meer", "a\u00b7zu\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}