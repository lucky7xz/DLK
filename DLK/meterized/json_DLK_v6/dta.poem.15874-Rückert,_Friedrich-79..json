{"dta.poem.15874": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "79.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Einst meine Leserinn bist du als Braut gewesen;", "tokens": ["Einst", "mei\u00b7ne", "Le\u00b7se\u00b7rinn", "bist", "du", "als", "Braut", "ge\u00b7we\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "PPER", "KOUS", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie solltest du nicht gern dein sch\u00f6nes Brautlied lesen?", "tokens": ["Wie", "soll\u00b7test", "du", "nicht", "gern", "dein", "sch\u00f6\u00b7nes", "Braut\u00b7lied", "le\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PTKNEG", "ADV", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dem Dichter zum Verlust, dem Manne zum Gewinn,", "tokens": ["Dem", "Dich\u00b7ter", "zum", "Ver\u00b7lust", ",", "dem", "Man\u00b7ne", "zum", "Ge\u00b7winn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bist du nun meine Frau, nicht meine Leserinn.", "tokens": ["Bist", "du", "nun", "mei\u00b7ne", "Frau", ",", "nicht", "mei\u00b7ne", "Le\u00b7se\u00b7rinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPOSAT", "NN", "$,", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und ich verdenke dir es nicht; den ganzen Mann", "tokens": ["Und", "ich", "ver\u00b7den\u00b7ke", "dir", "es", "nicht", ";", "den", "gan\u00b7zen", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PPER", "PTKNEG", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Besitzest du, was gehn dich seine Bruchst\u00fcck' an?", "tokens": ["Be\u00b7sit\u00b7zest", "du", ",", "was", "gehn", "dich", "sei\u00b7ne", "Bruch\u00b7st\u00fcck", "an", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWS", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Knaben nehm' ich aus, die Gott uns hat verliehn;", "tokens": ["Die", "Kna\u00b7ben", "nehm'", "ich", "aus", ",", "die", "Gott", "uns", "hat", "ver\u00b7liehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die hilf zu M\u00e4nnern auch, zu ganzen, mir erziehn.", "tokens": ["Die", "hilf", "zu", "M\u00e4n\u00b7nern", "auch", ",", "zu", "gan\u00b7zen", ",", "mir", "er\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ADV", "$,", "PTKZU", "VVINF", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}