{"textgrid.poem.56290": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Der M\u00fcllerin Nachbar", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die M\u00fchle, die dreht ihre Fl\u00fcgel,", "tokens": ["Die", "M\u00fch\u00b7le", ",", "die", "dreht", "ih\u00b7re", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Der Wind, der sauset darin:", "tokens": ["Der", "Wind", ",", "der", "sau\u00b7set", "da\u00b7rin", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PAV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ich wollte, ich w\u00e4re der M\u00fcller,", "tokens": ["Ich", "woll\u00b7te", ",", "ich", "w\u00e4\u00b7re", "der", "M\u00fcl\u00b7ler", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VAFIN", "ART", "NE", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Von wegen der M\u00fcllerin.", "tokens": ["Von", "we\u00b7gen", "der", "M\u00fcl\u00b7le\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Der M\u00fcller ist gestorben,", "tokens": ["Der", "M\u00fcl\u00b7ler", "ist", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gott schenk ihm die ewige Ruh!", "tokens": ["Gott", "schenk", "ihm", "die", "e\u00b7wi\u00b7ge", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ich wollte, es holte der Henker", "tokens": ["Ich", "woll\u00b7te", ",", "es", "hol\u00b7te", "der", "Hen\u00b7ker"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Den Flegel von Knecht noch dazu.", "tokens": ["Den", "Fle\u00b7gel", "von", "Knecht", "noch", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "ADV", "PAV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Am Sonntag in der Kirche,", "tokens": ["Am", "Sonn\u00b7tag", "in", "der", "Kir\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da glaubt ich, sie schiele nach mir;", "tokens": ["Da", "glaubt", "ich", ",", "sie", "schie\u00b7le", "nach", "mir", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sie schielte an mir nur vor\u00fcber,", "tokens": ["Sie", "schiel\u00b7te", "an", "mir", "nur", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Knecht, der stand an der T\u00fcr.", "tokens": ["Der", "Knecht", ",", "der", "stand", "an", "der", "T\u00fcr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Und als es ging zum Tanze,", "tokens": ["Und", "als", "es", "ging", "zum", "Tan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da kam sie eben mir recht,", "tokens": ["Da", "kam", "sie", "e\u00b7ben", "mir", "recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sie gr\u00fc\u00dfte mich freundlich und fragte \u2013", "tokens": ["Sie", "gr\u00fc\u00df\u00b7te", "mich", "freund\u00b7lich", "und", "frag\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und fragte mich gar nach dem Knecht.", "tokens": ["Und", "frag\u00b7te", "mich", "gar", "nach", "dem", "Knecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Der Knecht, der Knecht! \u2013 Ich wollte...", "tokens": ["Der", "Knecht", ",", "der", "Knecht", "!", "\u2013", "Ich", "woll\u00b7te", "..."], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$.", "$(", "PPER", "VMFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir kocht in den Adern das Blut \u2013", "tokens": ["Mir", "kocht", "in", "den", "A\u00b7dern", "das", "Blut", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ich wollte an ihm mich r\u00e4chen,", "tokens": ["Ich", "woll\u00b7te", "an", "ihm", "mich", "r\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ich wollte, ich h\u00e4tte den Mut.", "tokens": ["Ich", "woll\u00b7te", ",", "ich", "h\u00e4t\u00b7te", "den", "Mut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Ich wollte... Nun, was wei\u00df ich?", "tokens": ["Ich", "woll\u00b7te", "...", "Nun", ",", "was", "wei\u00df", "ich", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$(", "ADV", "$,", "PWS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich wei\u00df nicht, wo ich bin. \u2013", "tokens": ["Ich", "wei\u00df", "nicht", ",", "wo", "ich", "bin", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VAFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die M\u00fchle, die dreht ihre Fl\u00fcgel,", "tokens": ["Die", "M\u00fch\u00b7le", ",", "die", "dreht", "ih\u00b7re", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Der Wind, der sauset darin.", "tokens": ["Der", "Wind", ",", "der", "sau\u00b7set", "da\u00b7rin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PAV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}