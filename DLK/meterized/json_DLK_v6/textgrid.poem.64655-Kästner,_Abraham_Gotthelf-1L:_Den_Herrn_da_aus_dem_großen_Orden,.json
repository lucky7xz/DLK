{"textgrid.poem.64655": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Den Herrn da aus dem gro\u00dfen Orden,", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Herrn da aus dem gro\u00dfen Orden,", "tokens": ["Den", "Herrn", "da", "aus", "dem", "gro\u00b7\u00dfen", "Or\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von dem es ohnl\u00e4ngst hie\u00df, als sey er toll geworden,", "tokens": ["Von", "dem", "es", "ohn\u00b7l\u00e4ngst", "hie\u00df", ",", "als", "sey", "er", "toll", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "VVFIN", "$,", "KOKOM", "VAFIN", "PPER", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df man ihn jetzt mit Namen nennt,", "tokens": ["Da\u00df", "man", "ihn", "jetzt", "mit", "Na\u00b7men", "nennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das macht sein Freund, der Rezensent.", "tokens": ["Das", "macht", "sein", "Freund", ",", "der", "Re\u00b7zen\u00b7sent", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}