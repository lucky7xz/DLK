{"textgrid.poem.35263": {"metadata": {"author": {"name": "Harsd\u00f6rffer, Georg Philipp", "birth": "N.A.", "death": "N.A."}, "title": "Das Schiff", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn man mich r\u00fcckw\u00e4rts schaut, so gleich' ich einem Fisch,", "tokens": ["Wenn", "man", "mich", "r\u00fcck\u00b7w\u00e4rts", "schaut", ",", "so", "gleich'", "ich", "ei\u00b7nem", "Fisch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "ADV", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verlier' ich meinen Schwanz, so bleibet das Gezisch.", "tokens": ["Ver\u00b7lier'", "ich", "mei\u00b7nen", "Schwanz", ",", "so", "blei\u00b7bet", "das", "Ge\u00b7zisch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Leib hei\u00dft r\u00fcckw\u00e4rts ", "tokens": ["Mein", "Leib", "hei\u00dft", "r\u00fcck\u00b7w\u00e4rts"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Wer meinen Namen r\u00e4th, der ist ein R\u00e4thselmann.", "tokens": ["Wer", "mei\u00b7nen", "Na\u00b7men", "r\u00e4\u00b7th", ",", "der", "ist", "ein", "R\u00e4th\u00b7sel\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,", "PRELS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}}}}