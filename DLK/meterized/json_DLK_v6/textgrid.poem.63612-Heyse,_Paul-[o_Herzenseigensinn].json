{"textgrid.poem.63612": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "[o Herzenseigensinn]", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbo Herzenseigensinn!", "tokens": ["\u00bb", "o", "Her\u00b7zen\u00b7sei\u00b7gen\u00b7sinn", "!"], "token_info": ["punct", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wie viel ist dir geblieben,", "tokens": ["Wie", "viel", "ist", "dir", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie viel noch kannst du lieben,", "tokens": ["Wie", "viel", "noch", "kannst", "du", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und wirfst doch alles hin?", "tokens": ["Und", "wirfst", "doch", "al\u00b7les", "hin", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIS", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Seit ein Geliebtes fehlt,", "tokens": ["Seit", "ein", "Ge\u00b7lieb\u00b7tes", "fehlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Zwei Augen sich geschlossen,", "tokens": ["Zwei", "Au\u00b7gen", "sich", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bleibt alles ungenossen?", "tokens": ["Bleibt", "al\u00b7les", "un\u00b7ge\u00b7nos\u00b7sen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist dir die Welt entseelt?\u00ab \u2013", "tokens": ["Ist", "dir", "die", "Welt", "ent\u00b7seelt", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und hat denn Liebe je", "tokens": ["Und", "hat", "denn", "Lie\u00b7be", "je"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Gelernt vorlieb zu nehmen?", "tokens": ["Ge\u00b7lernt", "vor\u00b7lieb", "zu", "neh\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mu\u00df Treue nicht sich sch\u00e4men,", "tokens": ["Mu\u00df", "Treu\u00b7e", "nicht", "sich", "sch\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PTKNEG", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn sanfter wird das Weh?", "tokens": ["Wenn", "sanf\u00b7ter", "wird", "das", "Weh", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Euch ist die Welt so viel,", "tokens": ["Euch", "ist", "die", "Welt", "so", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mir gilt sie nur geringe,", "tokens": ["Mir", "gilt", "sie", "nur", "ge\u00b7rin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gleich einem goldnen Ringe,", "tokens": ["Gleich", "ei\u00b7nem", "gold\u00b7nen", "Rin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus dem die Perle fiel.", "tokens": ["Aus", "dem", "die", "Per\u00b7le", "fiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}