{"dta.poem.7698": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "70.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gar manches, was gewis du nennst, ist ungewis;", "tokens": ["Gar", "man\u00b7ches", ",", "was", "ge\u00b7wis", "du", "nennst", ",", "ist", "un\u00b7ge\u00b7wis", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "PRELS", "ADV", "PPER", "VVFIN", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Sprache selber, die du redest, sagt mir dis.", "tokens": ["Die", "Spra\u00b7che", "sel\u00b7ber", ",", "die", "du", "re\u00b7dest", ",", "sagt", "mir", "dis", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PDS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn ich will wissen: wer? und du's nicht sagen willst;", "tokens": ["Wenn", "ich", "will", "wis\u00b7sen", ":", "wer", "?", "und", "du's", "nicht", "sa\u00b7gen", "willst", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "VVINF", "$.", "PWS", "$.", "KON", "PIS", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was ist das Wort, womit du meine Neugier stillst?", "tokens": ["Was", "ist", "das", "Wort", ",", "wo\u00b7mit", "du", "mei\u00b7ne", "Neu\u00b7gier", "stillst", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$,", "PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dis: Ein gewisser ist's. Wei\u00df ich es nun gewisser?", "tokens": ["Dis", ":", "Ein", "ge\u00b7wis\u00b7ser", "ist'", "s.", "Wei\u00df", "ich", "es", "nun", "ge\u00b7wis\u00b7ser", "?"], "token_info": ["word", "punct", "word", "word", "word", "abbreviation", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$.", "ART", "ADJA", "VAFIN", "ADJA", "NN", "PPER", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nein! Dein Gewisser ist f\u00fcr mich ein Ungewisser.", "tokens": ["Nein", "!", "Dein", "Ge\u00b7wis\u00b7ser", "ist", "f\u00fcr", "mich", "ein", "Un\u00b7ge\u00b7wis\u00b7ser", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPOSAT", "NN", "VAFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Du gibst dir nur den Schein, indem du Ungewisses", "tokens": ["Du", "gibst", "dir", "nur", "den", "Schein", ",", "in\u00b7dem", "du", "Un\u00b7ge\u00b7wis\u00b7ses"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$,", "KOUS", "PPER", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mir kund thust, da\u00df du kund auch k\u00f6nntest thun Gewisses.", "tokens": ["Mir", "kund", "thust", ",", "da\u00df", "du", "kund", "auch", "k\u00f6nn\u00b7test", "thun", "Ge\u00b7wis\u00b7ses", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "VVFIN", "$,", "KOUS", "PPER", "PTKVZ", "ADV", "VMFIN", "VVINF", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}