{"textgrid.poem.55535": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Madrigal", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein M\u00e4dchen sagte mir: \u00bbWie sch\u00f6n", "tokens": ["Mein", "M\u00e4d\u00b7chen", "sag\u00b7te", "mir", ":", "\u00bb", "Wie", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$.", "$(", "PWAV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist nicht Olind! ich hab ihn heut gesehn,", "tokens": ["Ist", "nicht", "O\u00b7lind", "!", "ich", "hab", "ihn", "heut", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "NN", "$.", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Lang sah ich ihn bewundernd an;", "tokens": ["Lang", "sah", "ich", "ihn", "be\u00b7wun\u00b7dernd", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer h\u00e4tt ihn nicht bewundern sollen?", "tokens": ["Wer", "h\u00e4tt", "ihn", "nicht", "be\u00b7wun\u00b7dern", "sol\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Geliebter, du wirst doch nicht schmollen,", "tokens": ["Ge\u00b7lieb\u00b7ter", ",", "du", "wirst", "doch", "nicht", "schmol\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich's getan?\u00ab", "tokens": ["Da\u00df", "ich's", "ge\u00b7tan", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PIS", "VVPP", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Ich sprach: \u00bbMein Herz f\u00fchlt nichts vom Neide,", "tokens": ["Ich", "sprach", ":", "\u00bb", "Mein", "Herz", "f\u00fchlt", "nichts", "vom", "Nei\u00b7de", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "PPOSAT", "NN", "VVFIN", "PIS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Was auch dein Mund f\u00fcr Lob der Sch\u00f6nheit gibt;", "tokens": ["Was", "auch", "dein", "Mund", "f\u00fcr", "Lob", "der", "Sch\u00f6n\u00b7heit", "gibt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPOSAT", "NN", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Denn liebtest du die sch\u00f6nen Leute,", "tokens": ["Denn", "lieb\u00b7test", "du", "die", "sch\u00f6\u00b7nen", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sprich, h\u00e4ttest du mich je geliebt?\u00ab", "tokens": ["Sprich", ",", "h\u00e4t\u00b7test", "du", "mich", "je", "ge\u00b7liebt", "?", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "$,", "VAFIN", "PPER", "PRF", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}