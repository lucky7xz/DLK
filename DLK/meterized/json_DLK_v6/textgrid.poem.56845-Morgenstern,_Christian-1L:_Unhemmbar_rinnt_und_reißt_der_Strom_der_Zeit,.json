{"textgrid.poem.56845": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Unhemmbar rinnt und rei\u00dft der Strom der Zeit,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unhemmbar rinnt und rei\u00dft der Strom der Zeit,", "tokens": ["Un\u00b7hemm\u00b7bar", "rinnt", "und", "rei\u00dft", "der", "Strom", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "KON", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "in dem wir gleich verstreuten Blumen schwimmen,", "tokens": ["in", "dem", "wir", "gleich", "ver\u00b7streu\u00b7ten", "Blu\u00b7men", "schwim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "unhemmbar braust und fegt der Sturm der Zeit,", "tokens": ["un\u00b7hemm\u00b7bar", "braust", "und", "fegt", "der", "Sturm", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "KON", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "wir riefen kaum, verweht sind unsre Stimmen.", "tokens": ["wir", "rie\u00b7fen", "kaum", ",", "ver\u00b7weht", "sind", "uns\u00b7re", "Stim\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "VVFIN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein kurzer Augenaufschlag ist der Mensch,", "tokens": ["Ein", "kur\u00b7zer", "Au\u00b7gen\u00b7auf\u00b7schlag", "ist", "der", "Mensch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "den ewige Kraft auf ihre Werke tut,", "tokens": ["den", "e\u00b7wi\u00b7ge", "Kraft", "auf", "ih\u00b7re", "Wer\u00b7ke", "tut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "ein Blinzeln \u2013 der Geschlechter lange Reihn,", "tokens": ["ein", "Blin\u00b7zeln", "\u2013", "der", "Ge\u00b7schlech\u00b7ter", "lan\u00b7ge", "Reihn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "ADV", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "ein Blick \u2013 des Erdballs Werdnis und Verglut.", "tokens": ["ein", "Blick", "\u2013", "des", "Erd\u00b7balls", "Werd\u00b7nis", "und", "Ver\u00b7glut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}