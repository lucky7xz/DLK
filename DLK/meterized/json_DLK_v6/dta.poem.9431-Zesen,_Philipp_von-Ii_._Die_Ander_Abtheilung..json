{"dta.poem.9431": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Ii .  \n Die Ander Abtheilung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Liebster ist mein/", "tokens": ["Mein", "Liebs\u00b7ter", "ist", "mein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Das wei\u00df ich/ allein;", "tokens": ["Das", "wei\u00df", "ich", "/", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$(", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Auch bleib\u2019 ich sein eigen/", "tokens": ["Auch", "bleib'", "ich", "sein", "ei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJD", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "und will mich erzeigen/", "tokens": ["und", "will", "mich", "er\u00b7zei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "VVINF", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Wie jrgend die Freundinnen pflegen zu seyn.", "tokens": ["Wie", "jr\u00b7gend", "die", "Freun\u00b7din\u00b7nen", "pfle\u00b7gen", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVINF", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Mein Liebster ist mein/", "tokens": ["Mein", "Liebs\u00b7ter", "ist", "mein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Der unter den Rosen im gr\u00fcnen sich weidet/", "tokens": ["Der", "un\u00b7ter", "den", "Ro\u00b7sen", "im", "gr\u00fc\u00b7nen", "sich", "wei\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "APPRART", "ADJA", "PRF", "VVFIN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Bi\u00df wider der schatten von hinnen abscheidet/", "tokens": ["Bi\u00df", "wi\u00b7der", "der", "schat\u00b7ten", "von", "hin\u00b7nen", "ab\u00b7schei\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "APPR", "ADV", "VVFIN", "$("], "meter": "-+--+--+-++-", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Wann v\u00f6llig anbricht", "tokens": ["Wann", "v\u00f6l\u00b7lig", "an\u00b7bricht"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Des morgens das r\u00f6tliche Licht.", "tokens": ["Des", "mor\u00b7gens", "das", "r\u00f6t\u00b7li\u00b7che", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.11": {"text": "Komm Sch\u00f6ner/ herwider und gleiche den Rehen/", "tokens": ["Komm", "Sch\u00f6\u00b7ner", "/", "her\u00b7wi\u00b7der", "und", "glei\u00b7che", "den", "Re\u00b7hen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$(", "PTKVZ", "KON", "ADJA", "ART", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.12": {"text": "Den Hirschen/ die ober den H\u00fcgeln hergehen.", "tokens": ["Den", "Hir\u00b7schen", "/", "die", "o\u00b7ber", "den", "H\u00fc\u00b7geln", "her\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}}}}