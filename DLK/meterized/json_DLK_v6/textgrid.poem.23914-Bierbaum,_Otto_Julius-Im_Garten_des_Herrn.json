{"textgrid.poem.23914": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Im Garten des Herrn", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heut sieht die Welt mir lustig aus,", "tokens": ["Heut", "sieht", "die", "Welt", "mir", "lus\u00b7tig", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein hell und bunter Blumenstrau\u00df,", "tokens": ["Ein", "hell", "und", "bun\u00b7ter", "Blu\u00b7men\u00b7strau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir ist so froh zu Mute.", "tokens": ["Mir", "ist", "so", "froh", "zu", "Mu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wer hat das gethan? Ein roter Mund,", "tokens": ["Wer", "hat", "das", "ge\u00b7than", "?", "Ein", "ro\u00b7ter", "Mund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "VVPP", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Zwei volle Arme und Br\u00fcste rund,", "tokens": ["Zwei", "vol\u00b7le", "Ar\u00b7me", "und", "Br\u00fcs\u00b7te", "rund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "KON", "NN", "ADJD", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "An denen ich n\u00e4chten ruhte.", "tokens": ["An", "de\u00b7nen", "ich", "n\u00e4ch\u00b7ten", "ruh\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Aus Liebe hat die Welt gemacht", "tokens": ["Aus", "Lie\u00b7be", "hat", "die", "Welt", "ge\u00b7macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der liebe Gott bei Tag und Nacht", "tokens": ["Der", "lie\u00b7be", "Gott", "bei", "Tag", "und", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In einer Wochenrunde.", "tokens": ["In", "ei\u00b7ner", "Wo\u00b7chen\u00b7run\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Drum folge deines Gottes Spur", "tokens": ["Drum", "fol\u00b7ge", "dei\u00b7nes", "Got\u00b7tes", "Spur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und lieb auch du, o Kreatur,", "tokens": ["Und", "lieb", "auch", "du", ",", "o", "Kre\u00b7a\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "PPER", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu jeder guten Stunde.", "tokens": ["Zu", "je\u00b7der", "gu\u00b7ten", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dann ist die Welt dir wie ein Strau\u00df,", "tokens": ["Dann", "ist", "die", "Welt", "dir", "wie", "ein", "Strau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "KOKOM", "ART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sieht hell und bunt und fr\u00f6hlich aus", "tokens": ["Sieht", "hell", "und", "bunt", "und", "fr\u00f6h\u00b7lich", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "KON", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und du wirst selber bl\u00fchen", "tokens": ["Und", "du", "wirst", "sel\u00b7ber", "bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Garten der Gottseligkeit,", "tokens": ["Im", "Gar\u00b7ten", "der", "Gott\u00b7se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo in des Himmels Licht gedeiht", "tokens": ["Wo", "in", "des", "Him\u00b7mels", "Licht", "ge\u00b7deiht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Bl\u00fcmlein Gl\u00fcck-ohn-M\u00fchen.", "tokens": ["Das", "Bl\u00fcm\u00b7lein", "Gl\u00fc\u00b7ck\u00b7ohn\u00b7M\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}