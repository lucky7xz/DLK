{"textgrid.poem.61980": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein unbekanntes Band der Seelen kettet", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein unbekanntes Band der Seelen kettet", "tokens": ["Ein", "un\u00b7be\u00b7kann\u00b7tes", "Band", "der", "See\u00b7len", "ket\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Menschen an das arme Tier,", "tokens": ["Den", "Men\u00b7schen", "an", "das", "ar\u00b7me", "Tier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Tier hat seinen Willen \u2013 ", "tokens": ["Das", "Tier", "hat", "sei\u00b7nen", "Wil\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn auch 'ne kleinere als wir.", "tokens": ["Wenn", "auch", "'ne", "klei\u00b7ne\u00b7re", "als", "wir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "KOUS", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Mensch, mi\u00dfbrauchend die Gewalt und St\u00e4rke,", "tokens": ["Ein", "Mensch", ",", "mi\u00df\u00b7brau\u00b7chend", "die", "Ge\u00b7walt", "und", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein lebend Herz zerrei\u00dfend \u2013 wie?", "tokens": ["Ein", "le\u00b7bend", "Herz", "zer\u00b7rei\u00b7\u00dfend", "\u2013", "wie", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVPP", "$(", "PWAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer gleicht denn hier dem wilden Tiere,", "tokens": ["Wer", "gleicht", "denn", "hier", "dem", "wil\u00b7den", "Tie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist es der Mensch, ist es das Vieh?", "tokens": ["Ist", "es", "der", "Mensch", ",", "ist", "es", "das", "Vieh", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}