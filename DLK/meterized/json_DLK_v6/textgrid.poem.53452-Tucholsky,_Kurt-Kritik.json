{"textgrid.poem.53452": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Kritik", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da oben spielen sie ein schweres Drama", "tokens": ["Da", "o\u00b7ben", "spie\u00b7len", "sie", "ein", "schwe\u00b7res", "Dra\u00b7ma"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "mit Weltanschauung, Kampf von Herz und Pflicht:", "tokens": ["mit", "Welt\u00b7an\u00b7schau\u00b7ung", ",", "Kampf", "von", "Herz", "und", "Pflicht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Susannen attackiert ein ganz infama", "tokens": ["Su\u00b7san\u00b7nen", "at\u00b7ta\u00b7ckiert", "ein", "ganz", "in\u00b7fa\u00b7ma"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADV", "NE"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Patron und l\u00e4\u00dft sie nicht.", "tokens": ["Pat\u00b7ron", "und", "l\u00e4\u00dft", "sie", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich sitze im Parkett und z\u00fcck den Faber", "tokens": ["Ich", "sit\u00b7ze", "im", "Par\u00b7kett", "und", "z\u00fcck", "den", "Fa\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "VVIMP", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und schreibe auf, ob alles richtig sei;", "tokens": ["und", "schrei\u00b7be", "auf", ",", "ob", "al\u00b7les", "rich\u00b7tig", "sei", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "KOUS", "PIS", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Exposition, gesch\u00fcrzter Knoten \u2013 aber", "tokens": ["Ex\u00b7po\u00b7si\u00b7ti\u00b7on", ",", "ge\u00b7sch\u00fcrz\u00b7ter", "Kno\u00b7ten", "\u2013", "a\u00b7ber"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "$,", "ADJA", "NN", "$(", "ADV"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "ich denk mir nichts dabei.", "tokens": ["ich", "denk", "mir", "nichts", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mein Herz weilt fromm bei jenem lieben Kinde,", "tokens": ["Mein", "Herz", "weilt", "fromm", "bei", "je\u00b7nem", "lie\u00b7ben", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "das l\u00e4chelnd eine Kindermagd agiert:", "tokens": ["das", "l\u00e4\u00b7chelnd", "ei\u00b7ne", "Kin\u00b7der\u00b7magd", "a\u00b7giert", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "ich streichle ihr im Geiste sehr gelinde,", "tokens": ["ich", "streich\u00b7le", "ihr", "im", "Geis\u00b7te", "sehr", "ge\u00b7lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "was sie so lieblich ziert,", "tokens": ["was", "sie", "so", "lieb\u00b7lich", "ziert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nun sieh mal einer diese s\u00fc\u00dfen Pfoten,", "tokens": ["Nun", "sieh", "mal", "ei\u00b7ner", "die\u00b7se", "s\u00fc\u00b7\u00dfen", "Pfo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "PIS", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "dies Seidenhaar mit einem H\u00e4ubchen drauf \u2013", "tokens": ["dies", "Sei\u00b7den\u00b7haar", "mit", "ei\u00b7nem", "H\u00e4ub\u00b7chen", "drauf", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "APPR", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "es gibt da sicher manch gesch\u00fcrzten Knoten:", "tokens": ["es", "gibt", "da", "si\u00b7cher", "manch", "ge\u00b7sch\u00fcrz\u00b7ten", "Kno\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "ich l\u00f6st ihn gerne auf.", "tokens": ["ich", "l\u00f6st", "ihn", "ger\u00b7ne", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wer sagte da, da\u00df ich nicht sachlich bliebe?", "tokens": ["Wer", "sag\u00b7te", "da", ",", "da\u00df", "ich", "nicht", "sach\u00b7lich", "blie\u00b7be", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "$,", "KOUS", "PPER", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "(nu sieh mal einer dieses schlanke Bein!)", "tokens": ["(", "nu", "sieh", "mal", "ei\u00b7ner", "die\u00b7ses", "schlan\u00b7ke", "Bein", "!", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVIMP", "ADV", "PIS", "PDAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Begeisterung, Freude am Beruf und \u203aLiebe\u2039 \u2013:", "tokens": ["Be\u00b7geis\u00b7te\u00b7rung", ",", "Freu\u00b7de", "am", "Be\u00b7ruf", "und", "\u203a", "Lie\u00b7be", "\u2039", "\u2013", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["NN", "$,", "NN", "APPRART", "NN", "KON", "ADJA", "NN", "$(", "$(", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "So soll es sein!", "tokens": ["So", "soll", "es", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}