{"textgrid.poem.55385": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "7", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn du am breiten Flusse wohnst,", "tokens": ["Wenn", "du", "am", "brei\u00b7ten", "Flus\u00b7se", "wohnst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Seicht stockt er manchmal auch vorbei;", "tokens": ["Seicht", "stockt", "er", "manch\u00b7mal", "auch", "vor\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann, wenn du deine Wiesen schonst,", "tokens": ["Dann", ",", "wenn", "du", "dei\u00b7ne", "Wie\u00b7sen", "schonst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Her\u00fcber schl\u00e4mmt er, es ist ein Brei.", "tokens": ["Her\u00b7\u00fc\u00b7ber", "schl\u00e4mmt", "er", ",", "es", "ist", "ein", "Brei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Am klaren Tag hinab die Schiffe,", "tokens": ["Am", "kla\u00b7ren", "Tag", "hin\u00b7ab", "die", "Schif\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Fischer weislich streicht hinan;", "tokens": ["Der", "Fi\u00b7scher", "weis\u00b7lich", "streicht", "hi\u00b7nan", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun starret Eis am Kies und Riffe,", "tokens": ["Nun", "star\u00b7ret", "Eis", "am", "Kies", "und", "Rif\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Knabenvolk ist Herr der Bahn.", "tokens": ["Das", "Kna\u00b7ben\u00b7volk", "ist", "Herr", "der", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das mu\u00dft du sehn und unterweilen", "tokens": ["Das", "mu\u00dft", "du", "sehn", "und", "un\u00b7ter\u00b7wei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PPER", "VVINF", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch immer, was du willst, vollziehn!", "tokens": ["Doch", "im\u00b7mer", ",", "was", "du", "willst", ",", "voll\u00b7ziehn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWS", "PPER", "VMFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht stocken darfst du, vor nicht eilen;", "tokens": ["Nicht", "sto\u00b7cken", "darfst", "du", ",", "vor", "nicht", "ei\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "PPER", "$,", "APPR", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Zeit, sie geht gemessen hin.", "tokens": ["Die", "Zeit", ",", "sie", "geht", "ge\u00b7mes\u00b7sen", "hin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "VVPP", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}