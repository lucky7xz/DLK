{"textgrid.poem.41044": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Vorgef\u00fchl", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als in mir noch Lust und Hoffen,", "tokens": ["Als", "in", "mir", "noch", "Lust", "und", "Hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "ADV", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00dft' ich singen doch von Gram,", "tokens": ["Mu\u00dft'", "ich", "sin\u00b7gen", "doch", "von", "Gram", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "ADV", "APPR", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "War mir oft, als h\u00e4tt' getroffen", "tokens": ["War", "mir", "oft", ",", "als", "h\u00e4tt'", "ge\u00b7trof\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "$,", "KOKOM", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich das Leid, das sp\u00e4ter kam.", "tokens": ["Mich", "das", "Leid", ",", "das", "sp\u00e4\u00b7ter", "kam", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Herz! du hast es vorempfunden,", "tokens": ["Herz", "!", "du", "hast", "es", "vor\u00b7emp\u00b7fun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil dich traf schon mancher Schlag,", "tokens": ["Weil", "dich", "traf", "schon", "man\u00b7cher", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein Glied mit alten Wunden", "tokens": ["Und", "ein", "Glied", "mit", "al\u00b7ten", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vorausf\u00fchlt den tr\u00fcben Tag.", "tokens": ["Vor\u00b7aus\u00b7f\u00fchlt", "den", "tr\u00fc\u00b7ben", "Tag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}