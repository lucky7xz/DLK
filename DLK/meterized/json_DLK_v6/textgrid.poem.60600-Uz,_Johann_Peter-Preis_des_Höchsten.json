{"textgrid.poem.60600": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Preis des H\u00f6chsten", "genre": "verse", "period": "N.A.", "pub_year": 1758, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer sollte dich, o Gott, dich, Ewiger, nicht preisen,", "tokens": ["Wer", "soll\u00b7te", "dich", ",", "o", "Gott", ",", "dich", ",", "E\u00b7wi\u00b7ger", ",", "nicht", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "$,", "FM", "NN", "$,", "PPER", "$,", "ADJA", "$,", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um dessen g\u00fcldnen Thron unsterblich Lob erschallt,", "tokens": ["Um", "des\u00b7sen", "g\u00fcld\u00b7nen", "Thron", "uns\u00b7terb\u00b7lich", "Lob", "er\u00b7schallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der ganzen Sch\u00f6pfung Lob, das in unz\u00e4hlbarn Weisen", "tokens": ["Der", "gan\u00b7zen", "Sch\u00f6p\u00b7fung", "Lob", ",", "das", "in", "un\u00b7z\u00e4hl\u00b7barn", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Von tausend Welten widerhallt?", "tokens": ["Von", "tau\u00b7send", "Wel\u00b7ten", "wi\u00b7der\u00b7hallt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O Sch\u00f6pfer, h\u00f6r auch mich von deiner G\u00fcte lallen!", "tokens": ["O", "Sch\u00f6p\u00b7fer", ",", "h\u00f6r", "auch", "mich", "von", "dei\u00b7ner", "G\u00fc\u00b7te", "lal\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "ADV", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was w\u00e4r ich, dein Gesch\u00f6pf, erhieltest du mich nicht?", "tokens": ["Was", "w\u00e4r", "ich", ",", "dein", "Ge\u00b7sch\u00f6pf", ",", "er\u00b7hiel\u00b7test", "du", "mich", "nicht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "PRF", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich w\u00e4re schon vorl\u00e4ngst, wie m\u00fcrber Thon, zerfallen,", "tokens": ["Ich", "w\u00e4\u00b7re", "schon", "vor\u00b7l\u00e4ngst", ",", "wie", "m\u00fcr\u00b7ber", "Thon", ",", "zer\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "$,", "PWAV", "ADJD", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der in des T\u00f6pfers H\u00e4nden bricht.", "tokens": ["Der", "in", "des", "T\u00f6p\u00b7fers", "H\u00e4n\u00b7den", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was unser Gott gemacht, das will er auch erhalten!", "tokens": ["Was", "un\u00b7ser", "Gott", "ge\u00b7macht", ",", "das", "will", "er", "auch", "er\u00b7hal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVPP", "$,", "PDS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er wacht f\u00fcr seine Welt; er, er vermag allein,", "tokens": ["Er", "wacht", "f\u00fcr", "sei\u00b7ne", "Welt", ";", "er", ",", "er", "ver\u00b7mag", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "PPER", "$,", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was seine G\u00fcte schuf, mit Weisheit zu verwalten:", "tokens": ["Was", "sei\u00b7ne", "G\u00fc\u00b7te", "schuf", ",", "mit", "Weis\u00b7heit", "zu", "ver\u00b7wal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gott wacht! Wie ruhig kann ich seyn!", "tokens": ["Gott", "wacht", "!", "Wie", "ru\u00b7hig", "kann", "ich", "seyn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "PWAV", "ADJD", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mehr Gutes flie\u00dft uns zu, von seinen milden H\u00e4nden,", "tokens": ["Mehr", "Gu\u00b7tes", "flie\u00dft", "uns", "zu", ",", "von", "sei\u00b7nen", "mil\u00b7den", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als B\u00f6ses uns verfolgt, und Gutes vor uns flieht.", "tokens": ["Als", "B\u00f6\u00b7ses", "uns", "ver\u00b7folgt", ",", "und", "Gu\u00b7tes", "vor", "uns", "flieht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVFIN", "$,", "KON", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach! da\u00df die Sterblichen ihr stolzes Nichts empf\u00e4nden!", "tokens": ["Ach", "!", "da\u00df", "die", "Sterb\u00b7li\u00b7chen", "ihr", "stol\u00b7zes", "Nichts", "em\u00b7pf\u00e4n\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "ART", "NN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Vorn Herrn geschiehet, was geschieht.", "tokens": ["Vorn", "Herrn", "ge\u00b7schie\u00b7het", ",", "was", "ge\u00b7schieht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "VVPP", "$,", "PWS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Mit Nacht bekleidet, fliegt von Gottes lichtem Throne,", "tokens": ["Mit", "Nacht", "be\u00b7klei\u00b7det", ",", "fliegt", "von", "Got\u00b7tes", "lich\u00b7tem", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "VVFIN", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das ernste Schicksal aus, und ist ihm unterthan.", "tokens": ["Das", "erns\u00b7te", "Schick\u00b7sal", "aus", ",", "und", "ist", "ihm", "un\u00b7ter\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,", "KON", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er macht, was ihm gef\u00e4llt, aus dem beseelten Thone:", "tokens": ["Er", "macht", ",", "was", "ihm", "ge\u00b7f\u00e4llt", ",", "aus", "dem", "be\u00b7seel\u00b7ten", "Tho\u00b7ne", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VVPP", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sein Rathschlu\u00df ist nicht unser Wahn.", "tokens": ["Sein", "Rath\u00b7schlu\u00df", "ist", "nicht", "un\u00b7ser", "Wahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Tage gleiten hin, und Jahre folgen Jahren:", "tokens": ["Die", "Ta\u00b7ge", "glei\u00b7ten", "hin", ",", "und", "Jah\u00b7re", "fol\u00b7gen", "Jah\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "KON", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er spricht, so kommen sie, sie fliehen, wenn er winkt;", "tokens": ["Er", "spricht", ",", "so", "kom\u00b7men", "sie", ",", "sie", "flie\u00b7hen", ",", "wenn", "er", "winkt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "$,", "PPER", "VVINF", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und m\u00fcssen wechselnd sich mit Gl\u00fcck und Ungl\u00fcck paaren,", "tokens": ["Und", "m\u00fcs\u00b7sen", "wech\u00b7selnd", "sich", "mit", "Gl\u00fcck", "und", "Un\u00b7gl\u00fcck", "paa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADJD", "PRF", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wies ihm, dem Herrn, am besten d\u00fcnkt.", "tokens": ["Wies", "ihm", ",", "dem", "Herrn", ",", "am", "bes\u00b7ten", "d\u00fcnkt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "$,", "ART", "NN", "$,", "PTKA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Er z\u00fcrnt! Ihr S\u00fcnder bebt! Er k\u00f6mmt in schnellen Wettern,", "tokens": ["Er", "z\u00fcrnt", "!", "Ihr", "S\u00fcn\u00b7der", "bebt", "!", "Er", "k\u00f6mmt", "in", "schnel\u00b7len", "Wet\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPOSAT", "NN", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die rothen Blitze gl\u00fchn in seiner starken Hand.", "tokens": ["Die", "ro\u00b7then", "Blit\u00b7ze", "gl\u00fchn", "in", "sei\u00b7ner", "star\u00b7ken", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der H\u00f6chste macht sich auf, die Stolzen zu zerschmettern,", "tokens": ["Der", "H\u00f6chs\u00b7te", "macht", "sich", "auf", ",", "die", "Stol\u00b7zen", "zu", "zer\u00b7schmet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKVZ", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und legt ihr Leben in den Sand.", "tokens": ["Und", "legt", "ihr", "Le\u00b7ben", "in", "den", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Von Zwietracht angef\u00fchrt, w\u00fcrgt \u00fcber tausend Leichen", "tokens": ["Von", "Zwiet\u00b7racht", "an\u00b7ge\u00b7f\u00fchrt", ",", "w\u00fcrgt", "\u00fc\u00b7ber", "tau\u00b7send", "Lei\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "$,", "VVFIN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der blutbespritzte Krieg, wann ihm die Rache ruft.", "tokens": ["Der", "blut\u00b7be\u00b7spritz\u00b7te", "Krieg", ",", "wann", "ihm", "die", "Ra\u00b7che", "ruft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Hunger \u00e4chzt ihm nach: Die Fl\u00fcgel fauler Seuchen", "tokens": ["Der", "Hun\u00b7ger", "\u00e4chzt", "ihm", "nach", ":", "Die", "Fl\u00fc\u00b7gel", "fau\u00b7ler", "Seu\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vergiften die geschw\u00e4rzte Luft.", "tokens": ["Ver\u00b7gif\u00b7ten", "die", "ge\u00b7schw\u00e4rz\u00b7te", "Luft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Doch wann Gott gn\u00e4dig ist, erheitern sich die Zeiten;", "tokens": ["Doch", "wann", "Gott", "gn\u00e4\u00b7dig", "ist", ",", "er\u00b7hei\u00b7tern", "sich", "die", "Zei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NN", "ADJD", "VAFIN", "$,", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann scherzt auf sichrer Flur der Friede, der entwich:", "tokens": ["Dann", "scherzt", "auf", "sich\u00b7rer", "Flur", "der", "Frie\u00b7de", ",", "der", "ent\u00b7wich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "$,", "PRELS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bekr\u00e4nzt mit Bluhmen, geht ihm Ueberflu\u00df zur Seiten,", "tokens": ["Be\u00b7kr\u00e4nzt", "mit", "Bluh\u00b7men", ",", "geht", "ihm", "Ue\u00b7berf\u00b7lu\u00df", "zur", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$,", "VVFIN", "PPER", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und alles lacht und freuet sich.", "tokens": ["Und", "al\u00b7les", "lacht", "und", "freu\u00b7et", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "KON", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Erhebe dich, o Herr, die Rotte zu zerst\u00f6ren,", "tokens": ["Er\u00b7he\u00b7be", "dich", ",", "o", "Herr", ",", "die", "Rot\u00b7te", "zu", "zer\u00b7st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "FM", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die sagt: es ist kein Gott! es laut und spottend sagt,", "tokens": ["Die", "sagt", ":", "es", "ist", "kein", "Gott", "!", "es", "laut", "und", "spot\u00b7tend", "sagt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "PPER", "VAFIN", "PIAT", "NN", "$.", "PPER", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und, Sch\u00f6pfer, wider dich, die Sch\u00f6pfung zu emp\u00f6ren,", "tokens": ["Und", ",", "Sch\u00f6p\u00b7fer", ",", "wi\u00b7der", "dich", ",", "die", "Sch\u00f6p\u00b7fung", "zu", "em\u00b7p\u00f6\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "APPR", "PPER", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Welten aufzuwiegeln wagt!", "tokens": ["Die", "Wel\u00b7ten", "auf\u00b7zu\u00b7wie\u00b7geln", "wagt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Ihr Netz ber\u00fccke nicht die Seelen deiner Frommen!", "tokens": ["Ihr", "Netz", "be\u00b7r\u00fc\u00b7cke", "nicht", "die", "See\u00b7len", "dei\u00b7ner", "From\u00b7men", "!"], "tok