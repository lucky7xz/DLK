{"textgrid.poem.37357": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Abends, wenn die Heimchen singen,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Abends, wenn die Heimchen singen,", "tokens": ["A\u00b7bends", ",", "wenn", "die", "Heim\u00b7chen", "sin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn die Lampe d\u00fcster schwelt,", "tokens": ["Wenn", "die", "Lam\u00b7pe", "d\u00fcs\u00b7ter", "schwelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6r ich gern von Spukedingen,", "tokens": ["H\u00f6r", "ich", "gern", "von", "Spu\u00b7ke\u00b7din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was die Tante mir erz\u00e4hlt.", "tokens": ["Was", "die", "Tan\u00b7te", "mir", "er\u00b7z\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie es klopfte in den W\u00e4nden,", "tokens": ["Wie", "es", "klopf\u00b7te", "in", "den", "W\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie der alte Schrank geknackt,", "tokens": ["Wie", "der", "al\u00b7te", "Schrank", "ge\u00b7knackt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie es einst mit kalten H\u00e4nden", "tokens": ["Wie", "es", "einst", "mit", "kal\u00b7ten", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mutter Urschel angepackt,", "tokens": ["Mut\u00b7ter", "Ur\u00b7schel", "an\u00b7ge\u00b7packt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie man oft ein leises Jammern", "tokens": ["Wie", "man", "oft", "ein", "lei\u00b7ses", "Jam\u00b7mern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Grad um Mitternacht geh\u00f6rt", "tokens": ["Grad", "um", "Mit\u00b7ter\u00b7nacht", "ge\u00b7h\u00f6rt"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oben in den Bodenkammern,", "tokens": ["O\u00b7ben", "in", "den", "Bo\u00b7den\u00b7kam\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Scheint mir h\u00f6chst bemerkenswert.", "tokens": ["Scheint", "mir", "h\u00f6chst", "be\u00b7mer\u00b7kens\u00b7wert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch erz\u00e4hlt sie gar das M\u00e4rchen", "tokens": ["Doch", "er\u00b7z\u00e4hlt", "sie", "gar", "das", "M\u00e4r\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von dem Geiste ohne Kopf,", "tokens": ["Von", "dem", "Geis\u00b7te", "oh\u00b7ne", "Kopf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dann erhebt sich jedes H\u00e4rchen", "tokens": ["Dann", "er\u00b7hebt", "sich", "je\u00b7des", "H\u00e4r\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schaudervoll in meinem Schopf.", "tokens": ["Schau\u00b7der\u00b7voll", "in", "mei\u00b7nem", "Schopf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und ich kann es nicht verneinen,", "tokens": ["Und", "ich", "kann", "es", "nicht", "ver\u00b7nei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df es b\u00f6se Geister gibt;", "tokens": ["Da\u00df", "es", "b\u00f6\u00b7se", "Geis\u00b7ter", "gibt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn ich habe selber einen,", "tokens": ["Denn", "ich", "ha\u00b7be", "sel\u00b7ber", "ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ART", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der schon manchen Streich ver\u00fcbt.", "tokens": ["Der", "schon", "man\u00b7chen", "Streich", "ver\u00b7\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}