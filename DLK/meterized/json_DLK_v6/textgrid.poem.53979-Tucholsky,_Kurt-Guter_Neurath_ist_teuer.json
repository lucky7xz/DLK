{"textgrid.poem.53979": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Guter Neurath ist teuer", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ssiss kaum zu gloom:", "tokens": ["Ssiss", "kaum", "zu", "gloom", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Da haben wir einen in Rom,", "tokens": ["Da", "ha\u00b7ben", "wir", "ei\u00b7nen", "in", "Rom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "APPR", "NE", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "aus ziemlich echtem Adelsmark,", "tokens": ["aus", "ziem\u00b7lich", "ech\u00b7tem", "A\u00b7dels\u00b7mark", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "kriegt pro Tag 500 Mark \u2013", "tokens": ["kriegt", "pro", "Tag", "500", "Mark", "\u2013"], "token_info": ["word", "word", "word", "number", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "CARD", "NN", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "macht im Monat 15000 \u2013", "tokens": ["macht", "im", "Mo\u00b7nat", "15000", "\u2013"], "token_info": ["word", "word", "word", "number", "punct"], "pos": ["VVFIN", "APPRART", "NN", "CARD", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "(daf\u00fcr kann man schon mal).", "tokens": ["(", "da\u00b7f\u00fcr", "kann", "man", "schon", "mal", ")", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PAV", "VMFIN", "PIS", "ADV", "ADV", "$(", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Drum fragen wir brausend,", "tokens": ["Drum", "fra\u00b7gen", "wir", "brau\u00b7send", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "da\u00df es bis in die Wilhelmstra\u00dfe schallt:", "tokens": ["da\u00df", "es", "bis", "in", "die", "Wil\u00b7helms\u00b7tra\u00b7\u00dfe", "schallt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Was tut der Mann f\u00fcr sein Gehalt \u2013?", "tokens": ["Was", "tut", "der", "Mann", "f\u00fcr", "sein", "Ge\u00b7halt", "\u2013", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn die Republik Geburtstag hat \u2013:", "tokens": ["Wenn", "die", "Re\u00b7pub\u00b7lik", "Ge\u00b7burts\u00b7tag", "hat", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VAFIN", "$(", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "ist er nicht zu Hause;", "tokens": ["ist", "er", "nicht", "zu", "Hau\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "besucht Gerhart Hauptmann die ewige Stadt \u2013:", "tokens": ["be\u00b7sucht", "Ger\u00b7hart", "Haupt\u00b7mann", "die", "e\u00b7wi\u00b7ge", "Stadt", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "NN", "NN", "ART", "ADJA", "NN", "$(", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "ist er nicht zu Hause.", "tokens": ["ist", "er", "nicht", "zu", "Hau\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Unter den v\u00f6lkischen Belangen", "tokens": ["Un\u00b7ter", "den", "v\u00f6l\u00b7ki\u00b7schen", "Be\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "liebt er Artur Dintern.", "tokens": ["liebt", "er", "Ar\u00b7tur", "Din\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Kommt der Emil Ludwig gegangen \u2013:", "tokens": ["Kommt", "der", "E\u00b7mil", "Lud\u00b7wig", "ge\u00b7gan\u00b7gen", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NE", "NE", "VVPP", "$(", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "zeigt er ihm den Hintern.", "tokens": ["zeigt", "er", "ihm", "den", "Hin\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Diese Aussicht lohnt sicher den Aufenthalt \u2013", "tokens": ["Die\u00b7se", "Aus\u00b7sicht", "lohnt", "si\u00b7cher", "den", "Auf\u00b7ent\u00b7halt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "ADJD", "ART", "NN", "$("], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "aber was tut der Mann f\u00fcr sein Gehalt \u2013?", "tokens": ["a\u00b7ber", "was", "tut", "der", "Mann", "f\u00fcr", "sein", "Ge\u00b7halt", "\u2013", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PWS", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$(", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "La\u00dft ihn ruhn.", "tokens": ["La\u00dft", "ihn", "ruhn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Der tut, was sie alle tun:", "tokens": ["Der", "tut", ",", "was", "sie", "al\u00b7le", "tun", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PIS", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er nimmt das Geld von seinem Land", "tokens": ["Er", "nimmt", "das", "Geld", "von", "sei\u00b7nem", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und spuckt dem Geber auf die Hand.", "tokens": ["und", "spuckt", "dem", "Ge\u00b7ber", "auf", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gut leben. Mit Cliquen intrigieren.", "tokens": ["Gut", "le\u00b7ben", ".", "Mit", "Cli\u00b7quen", "int\u00b7ri\u00b7gie\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$.", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Die Republikaner sabotieren.", "tokens": ["Die", "Re\u00b7pub\u00b7li\u00b7ka\u00b7ner", "sa\u00b7bo\u00b7tie\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Auf den Arbeiter pfeifen. Zum Rennen gehn.", "tokens": ["Auf", "den", "Ar\u00b7bei\u00b7ter", "pfei\u00b7fen", ".", "Zum", "Ren\u00b7nen", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$.", "APPRART", "NN", "VVINF", "$."], "meter": "---+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Die Welt durch ein Monokel sehn.", "tokens": ["Die", "Welt", "durch", "ein", "Mo\u00b7no\u00b7kel", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Uns \u00fcberall schaden, da\u00df es so knallt \u2013:", "tokens": ["Uns", "\u00fc\u00b7be\u00b7rall", "scha\u00b7den", ",", "da\u00df", "es", "so", "knallt", "\u2013", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADV", "VVINF", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$(", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "das tut jener f\u00fcr sein Gehalt.", "tokens": ["das", "tut", "je\u00b7ner", "f\u00fcr", "sein", "Ge\u00b7halt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PDAT", "APPR", "PPOSAT", "NN", "$."], "meter": "-++-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Merke, zum Schlusse des Gedichts \u2013:", "tokens": ["Mer\u00b7ke", ",", "zum", "Schlus\u00b7se", "des", "Ge\u00b7dichts", "\u2013", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "APPRART", "NN", "ART", "NN", "$(", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Uns kostet das viel.", "tokens": ["Uns", "kos\u00b7tet", "das", "viel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PIAT", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ihn kostet das nichts.", "tokens": ["Ihn", "kos\u00b7tet", "das", "nichts", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PIS", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}