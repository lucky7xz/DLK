{"textgrid.poem.47415": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[in's Gr\u00fcbchen deiner Wange fiel die Seele mir]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In's Gr\u00fcbchen deiner Wange fiel die Seele mir;", "tokens": ["In's", "Gr\u00fcb\u00b7chen", "dei\u00b7ner", "Wan\u00b7ge", "fiel", "die", "See\u00b7le", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da langte sie nach deines Haares Schl\u00e4ngel-Zier.", "tokens": ["Da", "lang\u00b7te", "sie", "nach", "dei\u00b7nes", "Haa\u00b7res", "Schl\u00e4n\u00b7gel\u00b7Zier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach, armer Jussuf! Da du aus der Grube kamst,", "tokens": ["Ach", ",", "ar\u00b7mer", "Jus\u00b7suf", "!", "Da", "du", "aus", "der", "Gru\u00b7be", "kamst", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "NN", "$.", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Umstrickte dich das Fesselband der Locke hier.", "tokens": ["Um\u00b7strick\u00b7te", "dich", "das", "Fes\u00b7sel\u00b7band", "der", "Lo\u00b7cke", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine