, "line.44": {"text": "Du findest Wollust im Verzeihn.\u00ab", "tokens": ["Du", "fin\u00b7dest", "Wol\u00b7lust", "im", "Ver\u00b7zeihn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Geister-Stimme hatte Recht!", "tokens": ["Die", "Geis\u00b7ter\u00b7Stim\u00b7me", "hat\u00b7te", "Recht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit keinem liebenden Geschlecht", "tokens": ["Mit", "kei\u00b7nem", "lie\u00b7ben\u00b7den", "Ge\u00b7schlecht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erneuert' ich den ersten Frieden,", "tokens": ["Er\u00b7neu\u00b7ert'", "ich", "den", "ers\u00b7ten", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Befand mich trefflich wohl hienieden,", "tokens": ["Be\u00b7fand", "mich", "treff\u00b7lich", "wohl", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Verga\u00df mein hohes Ideal,", "tokens": ["Ver\u00b7ga\u00df", "mein", "ho\u00b7hes", "I\u00b7deal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und baute mir im Erdenthal", "tokens": ["Und", "bau\u00b7te", "mir", "im", "Er\u00b7dent\u00b7hal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nicht mehr den ew'gen Freudensaal.", "tokens": ["Nicht", "mehr", "den", "ew'\u00b7gen", "Freu\u00b7den\u00b7saal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O du, mit deinen s\u00fc\u00dfen Tr\u00e4umen!", "tokens": ["O", "du", ",", "mit", "dei\u00b7nen", "s\u00fc\u00b7\u00dfen", "Tr\u00e4u\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "K\u00e4m' unter sanft bewegten B\u00e4umen", "tokens": ["K\u00e4m'", "un\u00b7ter", "sanft", "be\u00b7weg\u00b7ten", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein solches Fl\u00fcstern auch zu dir!", "tokens": ["Ein", "sol\u00b7ches", "Fl\u00fcs\u00b7tern", "auch", "zu", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir armen Erden-S\u00f6hne, wir", "tokens": ["Wir", "ar\u00b7men", "Er\u00b7den\u00b7S\u00f6h\u00b7ne", ",", "wir"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sind allesammt, wie unsre V\u00e4ter,", "tokens": ["Sind", "al\u00b7le\u00b7sammt", ",", "wie", "uns\u00b7re", "V\u00e4\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "PWAV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und minder noch aus zartem Aether,", "tokens": ["Und", "min\u00b7der", "noch", "aus", "zar\u00b7tem", "A\u00b7e\u00b7ther", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Aus Geistes-Stoff gebaut, als ihr.", "tokens": ["Aus", "Geis\u00b7tes\u00b7Stoff", "ge\u00b7baut", ",", "als", "ihr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "KOUS", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Gelingt's dem M\u00e4dchen dann und wann,", "tokens": ["Ge\u00b7lingt's", "dem", "M\u00e4d\u00b7chen", "dann", "und", "wann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "KON", "PWAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sich einen guten, lieben Mann", "tokens": ["Sich", "ei\u00b7nen", "gu\u00b7ten", ",", "lie\u00b7ben", "Mann"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PRF", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "In einen Seraph umzukleiden \u2013", "tokens": ["In", "ei\u00b7nen", "Se\u00b7raph", "um\u00b7zu\u00b7klei\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVIZU", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Wie kurz die stolzen G\u00f6tter Freuden!", "tokens": ["Wie", "kurz", "die", "stol\u00b7zen", "G\u00f6t\u00b7ter", "Freu\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und wenn er noch so weise spricht,", "tokens": ["Und", "wenn", "er", "noch", "so", "wei\u00b7se", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Er h\u00e4lt die Engel-Probe nicht.", "tokens": ["Er", "h\u00e4lt", "die", "En\u00b7gel\u00b7Pro\u00b7be", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Mag er! doch giebt es M\u00e4nner-Seelen,", "tokens": ["Mag", "er", "!", "doch", "giebt", "es", "M\u00e4n\u00b7ner\u00b7See\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$.", "ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Ein Mahl nur ein Liebchen w\u00e4hlen,", "tokens": ["Die", "Ein", "Mahl", "nur", "ein", "Lieb\u00b7chen", "w\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Die fest im Wort, im Bunde wahr,", "tokens": ["Die", "fest", "im", "Wort", ",", "im", "Bun\u00b7de", "wahr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "NN", "$,", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Leid, in Mangel und Gefahr,", "tokens": ["In", "Leid", ",", "in", "Man\u00b7gel", "und", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im Tode selbst unwandelbar,", "tokens": ["Im", "To\u00b7de", "selbst", "un\u00b7wan\u00b7del\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit ihrem Leben euch besch\u00fctzen.", "tokens": ["Mit", "ih\u00b7rem", "Le\u00b7ben", "euch", "be\u00b7sch\u00fct\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "F\u00fcr euch ist jeder Tropfen Blut;", "tokens": ["F\u00fcr", "euch", "ist", "je\u00b7der", "Trop\u00b7fen", "Blut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ihr k\u00f6nnt auf ihren st\u00e4rkern Muth", "tokens": ["Ihr", "k\u00f6nnt", "auf", "ih\u00b7ren", "st\u00e4r\u00b7kern", "Muth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die holde Schw\u00e4che ruhig st\u00fctzen.", "tokens": ["Die", "hol\u00b7de", "Schw\u00e4\u00b7che", "ru\u00b7hig", "st\u00fct\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und mancher ist, der einsam geht,", "tokens": ["Und", "man\u00b7cher", "ist", ",", "der", "ein\u00b7sam", "geht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "U