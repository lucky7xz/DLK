{"textgrid.poem.60083": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Geister weichen allgemach,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Geister weichen allgemach,", "tokens": ["Die", "Geis\u00b7ter", "wei\u00b7chen", "all\u00b7ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die, gleich den St\u00fcrmen hoch am Dach,", "tokens": ["Die", ",", "gleich", "den", "St\u00fcr\u00b7men", "hoch", "am", "Dach", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "ART", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In meinem Kopfe Sabbath hielten,", "tokens": ["In", "mei\u00b7nem", "Kop\u00b7fe", "Sab\u00b7ba\u00b7th", "hiel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und j\u00e4mmerlich den Meister spielten;", "tokens": ["Und", "j\u00e4m\u00b7mer\u00b7lich", "den", "Meis\u00b7ter", "spiel\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mich h\u00e4misch neckten, jung und alt,", "tokens": ["Mich", "h\u00e4\u00b7misch", "neck\u00b7ten", ",", "jung", "und", "alt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In hundertf\u00e4ltiger Gestalt,", "tokens": ["In", "hun\u00b7dert\u00b7f\u00e4l\u00b7ti\u00b7ger", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit Horn und Krall' und Pferdefu\u00df,", "tokens": ["Mit", "Horn", "und", "Krall'", "und", "Pfer\u00b7de\u00b7fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Als w\u00e4r' ich ", "tokens": ["Als", "w\u00e4r'", "ich"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Die Geister weichen allgemach", "tokens": ["Die", "Geis\u00b7ter", "wei\u00b7chen", "all\u00b7ge\u00b7mach"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Zur\u00fcck in ihre Zauberh\u00f6hle;", "tokens": ["Zu\u00b7r\u00fcck", "in", "ih\u00b7re", "Zau\u00b7ber\u00b7h\u00f6h\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Schon wieder f\u00fchl' ich in der Seele", "tokens": ["Schon", "wie\u00b7der", "f\u00fchl'", "ich", "in", "der", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Die Hoffnung und die Freude wach,", "tokens": ["Die", "Hoff\u00b7nung", "und", "die", "Freu\u00b7de", "wach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ergetze mich am Stadt-Get\u00fcmmel,", "tokens": ["Er\u00b7get\u00b7ze", "mich", "am", "Stadt\u00b7Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und in der Fern' am freyen Himmel,", "tokens": ["Und", "in", "der", "Fern'", "am", "frey\u00b7en", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Am offnen Feld, und am Gemisch", "tokens": ["Am", "off\u00b7nen", "Feld", ",", "und", "am", "Ge\u00b7misch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "KON", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Des falben Laubes im Geb\u00fcsch.", "tokens": ["Des", "fal\u00b7ben", "Lau\u00b7bes", "im", "Ge\u00b7b\u00fcsch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Mein Auge weilt auf jenen B\u00e4umen,", "tokens": ["Mein", "Au\u00b7ge", "weilt", "auf", "je\u00b7nen", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Worunter du in s\u00fc\u00dfen Tr\u00e4umen,", "tokens": ["Wo\u00b7run\u00b7ter", "du", "in", "s\u00fc\u00b7\u00dfen", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Voll jungfr\u00e4ulicher Sehnsucht, gehst,", "tokens": ["Voll", "jung\u00b7fr\u00e4u\u00b7li\u00b7cher", "Sehn\u00b7sucht", ",", "gehst", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.20": {"text": "Und stets dich um ein Bildchen drehst", "tokens": ["Und", "stets", "dich", "um", "ein", "Bild\u00b7chen", "drehst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Von Seligkeit aus obern Welten,", "tokens": ["Von", "Se\u00b7lig\u00b7keit", "aus", "o\u00b7bern", "Wel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Von reiner Liebe, die nur selten,", "tokens": ["Von", "rei\u00b7ner", "Lie\u00b7be", ",", "die", "nur", "sel\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "So rein, wie sie vom Himmel kam,", "tokens": ["So", "rein", ",", "wie", "sie", "vom", "Him\u00b7mel", "kam", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "In Erden-H\u00fctten Wohnung nahm.", "tokens": ["In", "Er\u00b7den\u00b7H\u00fct\u00b7ten", "Woh\u00b7nung", "nahm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Durch manchen Irrweg dieses Lebens", "tokens": ["Durch", "man\u00b7chen", "Irr\u00b7weg", "die\u00b7ses", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ging ich, und suchte sie vergebens.", "tokens": ["Ging", "ich", ",", "und", "such\u00b7te", "sie", "ver\u00b7ge\u00b7bens", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da wollt' ich oft im Mondesstrahl", "tokens": ["Da", "wollt'", "ich", "oft", "im", "Mon\u00b7des\u00b7strahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein fein gewebtes Ideal", "tokens": ["Mein", "fein", "ge\u00b7web\u00b7tes", "I\u00b7deal"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mit allen seinen Herrlichkeiten", "tokens": ["Mit", "al\u00b7len", "sei\u00b7nen", "Herr\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mir unverdorben nieder leiten;", "tokens": ["Mir", "un\u00b7ver\u00b7dor\u00b7ben", "nie\u00b7der", "lei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKVZ", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und hat's und dr\u00fcckt' es froh und warm;", "tokens": ["Und", "hat's", "und", "dr\u00fcckt'", "es", "froh", "und", "warm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "KON", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und ruhig lag's in meinem Arm,", "tokens": ["Und", "ru\u00b7hig", "lag's", "in", "mei\u00b7nem", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Bis mir der neue Tag begann,", "tokens": ["Bis", "mir", "der", "neu\u00b7e", "Tag", "be\u00b7gann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und es im Morgen-Duft zerrann.", "tokens": ["Und", "es", "im", "Mor\u00b7gen\u00b7Duft", "zer\u00b7rann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Dann klagt' ich's aller Welt, erschreckte", "tokens": ["Dann", "klagt'", "ich's", "al\u00b7ler", "Welt", ",", "er\u00b7schreck\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PIS", "PIAT", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Die Nymph' am Bach, den jungen West,", "tokens": ["Die", "Nym\u00b7ph'", "am", "Bach", ",", "den", "jun\u00b7gen", "West", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Vertraut' es jedem Baum und weckte", "tokens": ["Ver\u00b7traut'", "es", "je\u00b7dem", "Baum", "und", "weck\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Die V\u00f6gelchen in ihrem Nest.", "tokens": ["Die", "V\u00f6\u00b7gel\u00b7chen", "in", "ih\u00b7rem", "Nest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Auf Rosen-Lippen sah ich Trug,", "tokens": ["Auf", "Ro\u00b7sen\u00b7\u00b7Lip\u00b7pen", "sah", "ich", "Trug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und mit den M\u00e4dchen wollt' ich hadern;", "tokens": ["Und", "mit", "den", "M\u00e4d\u00b7chen", "wollt'", "ich", "ha\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Was aber halfs? Zu m\u00e4chtig schlug", "tokens": ["Was", "a\u00b7ber", "halfs", "?", "Zu", "m\u00e4ch\u00b7tig", "schlug"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ADV", "NN", "$.", "PTKA", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Die Liebe noch in Herz und Adern;", "tokens": ["Die", "Lie\u00b7be", "noch", "in", "Herz", "und", "A\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Und als die Wiese Veilchen trug,", "tokens": ["Und", "als", "die", "Wie\u00b7se", "Veil\u00b7chen", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Da sah ich lauter Liebes-Flug,", "tokens": ["Da", "sah", "ich", "lau\u00b7ter", "Lie\u00b7bes\u00b7Flug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Sah in der Luft, im Wald, an Quellen", "tokens": ["Sah", "in", "der", "Luft", ",", "im", "Wald", ",", "an", "Quel\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "APPRART", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Sich eins dem andern zugesellen.", "tokens": ["Sich", "eins", "dem", "an\u00b7dern", "zu\u00b7ge\u00b7sel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PIS", "ART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Da war um mich ein Paradies,", "tokens": ["Da", "war", "um", "mich", "ein", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Und jeder Bl\u00fcthen-Hain verhie\u00df", "tokens": ["Und", "je\u00b7der", "Bl\u00fcthen\u00b7Hain", "ver\u00b7hie\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.25": {"text": "Mir gleiche Wonn', und aller Wegen", "tokens": ["Mir", "glei\u00b7che", "Wonn'", ",", "und", "al\u00b7ler", "We\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Kam ein Gefl\u00fcster mir entgegen:", "tokens": ["Kam", "ein", "Ge\u00b7fl\u00fcs\u00b7ter", "mir", "ent\u00b7ge\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "\u00bbdu Sohn des Staubs, der Himmels-Lust", "tokens": ["\u00bb", "du", "Sohn", "des", "Staubs", ",", "der", "Him\u00b7mels\u00b7Lust"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "PPER", "NN", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Begehrt! Die Hand auf deine Brust!", "tokens": ["Be\u00b7gehrt", "!", "Die", "Hand", "auf", "dei\u00b7ne", "Brust", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Wie leicht, wie schwach, wie voller M\u00e4ngel!", "tokens": ["Wie", "leicht", ",", "wie", "schwach", ",", "wie", "vol\u00b7ler", "M\u00e4n\u00b7gel", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "$,", "PWAV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Und fordern darfst du einen Engel", "tokens": ["Und", "for\u00b7dern", "darfst", "du", "ei\u00b7nen", "En\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "F\u00fcr deinen Ku\u00df? Gen\u00fcgt's dir nicht,", "tokens": ["F\u00fcr", "dei\u00b7nen", "Ku\u00df", "?", "Ge\u00b7n\u00fcgt's", "dir", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "NE", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Wenn Uns