{"textgrid.poem.24213": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Meine armen Veilchen sind erfroren,", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine armen Veilchen sind erfroren,", "tokens": ["Mei\u00b7ne", "ar\u00b7men", "Veil\u00b7chen", "sind", "er\u00b7fro\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Liegen nun im Schnee vorm Fenster drau\u00dfen,", "tokens": ["Lie\u00b7gen", "nun", "im", "Schnee", "vorm", "Fens\u00b7ter", "drau\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "APPRART", "NN", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Na\u00df und duftlos. Meine holde Hoffnung", "tokens": ["Na\u00df", "und", "duft\u00b7los", ".", "Mei\u00b7ne", "hol\u00b7de", "Hoff\u00b7nung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "$.", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ist gestorben.", "tokens": ["Ist", "ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Einsam weiter durch das leere Leben,", "tokens": ["Ein\u00b7sam", "wei\u00b7ter", "durch", "das", "lee\u00b7re", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mit erfrornem Herzen einsam weiter!", "tokens": ["Mit", "er\u00b7fror\u00b7nem", "Her\u00b7zen", "ein\u00b7sam", "wei\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Irgendwo in einem tiefen Walde", "tokens": ["Ir\u00b7gend\u00b7wo", "in", "ei\u00b7nem", "tie\u00b7fen", "Wal\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sink ich nieder.", "tokens": ["Sink", "ich", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}