{"dta.poem.10637": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Elegie   au\u00df   Dan. Heinsij Monobiblo.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr aber wisset nichts als nur auff Gut zusinnen/", "tokens": ["Ihr", "a\u00b7ber", "wis\u00b7set", "nichts", "als", "nur", "auff", "Gut", "zu\u00b7sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PIS", "KOKOM", "ADV", "APPR", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd zieht bald vber Feld/ bald durch das wilde Meer/", "tokens": ["Vnd", "zieht", "bald", "vber", "Feld", "/", "bald", "durch", "das", "wil\u00b7de", "Meer", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "$(", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ja wohin auch die Sonn hat niemals reichen k\u00fcnnen/", "tokens": ["Ja", "wo\u00b7hin", "auch", "die", "Sonn", "hat", "nie\u00b7mals", "rei\u00b7chen", "k\u00fcn\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PWAV", "ADV", "ART", "NN", "VAFIN", "ADV", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da bringet jhr das Golt/ den sch\u00f6nen Koth/ anher.", "tokens": ["Da", "brin\u00b7get", "jhr", "das", "Golt", "/", "den", "sch\u00f6\u00b7nen", "Koth", "/", "an\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$(", "ART", "ADJA", "NN", "$(", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vnd ich bedarff di\u00df nicht/ was jhr an allenenden", "tokens": ["Vnd", "ich", "be\u00b7darff", "di\u00df", "nicht", "/", "was", "jhr", "an", "al\u00b7le\u00b7nen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PDS", "PTKNEG", "$(", "PWS", "PPER", "APPR", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Zu Land vnd Wasser sucht/ das hab ich schon bey mir/", "tokens": ["Zu", "Land", "vnd", "Was\u00b7ser", "sucht", "/", "das", "hab", "ich", "schon", "bey", "mir", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$(", "PDS", "VAFIN", "PPER", "ADV", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mein Gut ist/ da\u00df ich sterb in meiner Liebsten H\u00e4nden/", "tokens": ["Mein", "Gut", "ist", "/", "da\u00df", "ich", "sterb", "in", "mei\u00b7ner", "Liebs\u00b7ten", "H\u00e4n\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$(", "KOUS", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Strasse wandel ich gar sicher f\u00fcr vnd f\u00fcr.", "tokens": ["Die", "Stras\u00b7se", "wan\u00b7del", "ich", "gar", "si\u00b7cher", "f\u00fcr", "vnd", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADJD", "APPR", "KON", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dann jetzundt wirdt mein Geist von jhrem Geist empfangen/", "tokens": ["Dann", "je\u00b7tzundt", "wirdt", "mein", "Geist", "von", "jhrem", "Geist", "emp\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Wenn er das sch\u00f6ne Thor des Mundes kompt hinein/", "tokens": ["Wenn", "er", "das", "sch\u00f6\u00b7ne", "Thor", "des", "Mun\u00b7des", "kompt", "hin\u00b7ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Jetzundt ergeh ich mich bey den liebreichen Wangen/", "tokens": ["Je\u00b7tzundt", "er\u00b7geh", "ich", "mich", "bey", "den", "lieb\u00b7rei\u00b7chen", "Wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Da Venus vnd jhr Sohn pers\u00f6nlich wohnhafft sein.", "tokens": ["Da", "Ve\u00b7nus", "vnd", "jhr", "Sohn", "per\u00b7s\u00f6n\u00b7lich", "wohn\u00b7hafft", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "PPOSAT", "NN", "ADJD", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Bald hatt sie mir/ ich jhr den zarten Hal\u00df vmbgeben/", "tokens": ["Bald", "hatt", "sie", "mir", "/", "ich", "jhr", "den", "zar\u00b7ten", "Hal\u00df", "vmb\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$(", "PPER", "PPER", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Vnd schaw/ wie die Natur so trefflich sie geziert/", "tokens": ["Vnd", "schaw", "/", "wie", "die", "Na\u00b7tur", "so", "treff\u00b7lich", "sie", "ge\u00b7ziert", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOKOM", "ART", "NN", "ADV", "ADJD", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Bald in den \u00e4ugelein enthalt ich mir das Leben/", "tokens": ["Bald", "in", "den", "\u00e4u\u00b7ge\u00b7lein", "ent\u00b7halt", "ich", "mir", "das", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PPER", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Dahin werdt ich zugleich mit Sinn vnd Muth gef\u00fchrt/", "tokens": ["Da\u00b7hin", "werdt", "ich", "zu\u00b7gleich", "mit", "Sinn", "vnd", "Muth", "ge\u00b7f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wie der so vnverschuldt sein Vatterland verlassen/", "tokens": ["Wie", "der", "so", "vn\u00b7ver\u00b7schuldt", "sein", "Vat\u00b7ter\u00b7land", "ver\u00b7las\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADV", "ADJD", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Mu\u00df suchen einen Weg der jhm gantz vnbekandt/", "tokens": ["Mu\u00df", "su\u00b7chen", "ei\u00b7nen", "Weg", "der", "jhm", "gantz", "vn\u00b7be\u00b7kandt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "ART", "NN", "ART", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Geht vber Berg vnd Thal durch angenehme Strassen/", "tokens": ["Geht", "vber", "Berg", "vnd", "Thal", "durch", "an\u00b7ge\u00b7neh\u00b7me", "Stras\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.20": {"text": "Nichts achtendt/ als allein sein liebes Vatterlandt.", "tokens": ["Nichts", "ach\u00b7tendt", "/", "als", "al\u00b7lein", "sein", "lie\u00b7bes", "Vat\u00b7ter\u00b7landt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$(", "KOKOM", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wann er dann ohn gefehr erblicket einen Bronnen/", "tokens": ["Wann", "er", "dann", "ohn", "ge\u00b7fehr", "er\u00b7bli\u00b7cket", "ei\u00b7nen", "Bron\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPR", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der sonst verborgen ist in mitten in dem Wald/", "tokens": ["Der", "sonst", "ver\u00b7bor\u00b7gen", "ist", "in", "mit\u00b7ten", "in", "dem", "Wald", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "VAFIN", "APPR", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Befreyet vor der Hitz vnd vngedult der Sonnen/", "tokens": ["Be\u00b7fre\u00b7yet", "vor", "der", "Hitz", "vnd", "vn\u00b7ge\u00b7dult", "der", "Son\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "KON", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Da nichts als nur das Wild hat seinen aufsenthalt/", "tokens": ["Da", "nichts", "als", "nur", "das", "Wild", "hat", "sei\u00b7nen", "auf\u00b7sen\u00b7thalt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "KOKOM", "ADV", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "So ist er wolgemuth/ vergisset aller dinge/", "tokens": ["So", "ist", "er", "wol\u00b7ge\u00b7muth", "/", "ver\u00b7gis\u00b7set", "al\u00b7ler", "din\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$(", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Erforschet nur den Quell de\u00df Br\u00fcnneleins mit flei\u00df/", "tokens": ["Er\u00b7for\u00b7schet", "nur", "den", "Quell", "de\u00df", "Br\u00fcn\u00b7nel\u00b7eins", "mit", "flei\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Vnd w\u00fcnscht/ da\u00df jhn alldar der sanffte Schlaf vmbringe/", "tokens": ["Vnd", "w\u00fcnscht", "/", "da\u00df", "jhn", "all\u00b7dar", "der", "sanff\u00b7te", "Schlaf", "vm\u00b7brin\u00b7ge", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Weil er vor grosser Lust sich selber auch nicht wei\u00df.", "tokens": ["Weil", "er", "vor", "gros\u00b7ser", "Lust", "sich", "sel\u00b7ber", "auch", "nicht", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "PRF", "ADV", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Nicht weniger auch mich/ weil ich so sehr gejrret", "tokens": ["Nicht", "we\u00b7ni\u00b7ger", "auch", "mich", "/", "weil", "ich", "so", "sehr", "ge\u00b7jr\u00b7ret"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ADV", "PPER", "$(", "KOUS", "PPER", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Durch Frewd vnd h\u00f6chste Lust der s\u00fcssen Liebes pein/", "tokens": ["Durch", "Frewd", "vnd", "h\u00f6chs\u00b7te", "Lust", "der", "s\u00fcs\u00b7sen", "Lie\u00b7bes", "pein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "ART", "ADJA", "NN", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Weil mein Gem\u00fcthe sich in Wollust gantz verwirret/", "tokens": ["Weil", "mein", "Ge\u00b7m\u00fc\u00b7the", "sich", "in", "Wol\u00b7lust", "gantz", "ver\u00b7wir\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "APPR", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wirdt nichts nicht machen lo\u00df/ als nur der Todt allein.", "tokens": ["Wirdt", "nichts", "nicht", "ma\u00b7chen", "lo\u00df", "/", "als", "nur", "der", "Todt", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PTKNEG", "VVINF", "PTKVZ", "$(", "KOKOM", "ADV", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}