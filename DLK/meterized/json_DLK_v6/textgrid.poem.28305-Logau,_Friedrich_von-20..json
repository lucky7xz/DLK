{"textgrid.poem.28305": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bey so wildem w\u00fcsten Wesen,", "tokens": ["Bey", "so", "wil\u00b7dem", "w\u00fcs\u00b7ten", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da fast niemand kan genesen,", "tokens": ["Da", "fast", "nie\u00b7mand", "kan", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "VMFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da die Wolfahrt gar verf\u00e4hret,", "tokens": ["Da", "die", "Wol\u00b7fahrt", "gar", "ver\u00b7f\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da das Heil sich abezehret,", "tokens": ["Da", "das", "Heil", "sich", "a\u00b7be\u00b7zeh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wil von ihren besten Sachen", "tokens": ["Wil", "von", "ih\u00b7ren", "bes\u00b7ten", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ordnung eine Jungfer machen.", "tokens": ["Ord\u00b7nung", "ei\u00b7ne", "Jung\u00b7fer", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "N\u00e4mlich alles liebe Ding,", "tokens": ["N\u00e4m\u00b7lich", "al\u00b7les", "lie\u00b7be", "Ding", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Das sie auch zum Erb empfing,", "tokens": ["Das", "sie", "auch", "zum", "Erb", "emp\u00b7fing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Wil sie einem Freunde geben,", "tokens": ["Wil", "sie", "ei\u00b7nem", "Freun\u00b7de", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Weil sie noch f\u00fchlt W\u00e4rmd und Leben.", "tokens": ["Weil", "sie", "noch", "f\u00fchlt", "W\u00e4rmd", "und", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Nun, die Testamenterin", "tokens": ["Nun", ",", "die", "Tes\u00b7ta\u00b7men\u00b7te\u00b7rin"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADV", "$,", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Frisch von Leibe, frisch von Sinn,", "tokens": ["Frisch", "von", "Lei\u00b7be", ",", "frisch", "von", "Sinn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "F\u00fchrt ihr volles Wolbelieben", "tokens": ["F\u00fchrt", "ihr", "vol\u00b7les", "Wol\u00b7be\u00b7lie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "In dem Busem auffgeschrieben,", "tokens": ["In", "dem", "Bu\u00b7sem", "auff\u00b7ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Hat auff Jungfern-Pergament", "tokens": ["Hat", "auff", "Jung\u00b7fern\u00b7Per\u00b7ga\u00b7ment"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "APPR", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.16": {"text": "Erb und Erben selbst benent,", "tokens": ["Erb", "und", "Er\u00b7ben", "selbst", "be\u00b7nent", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Sagt: Hierinne steckt mein Wille,", "tokens": ["Sagt", ":", "Hie\u00b7rin\u00b7ne", "steckt", "mein", "Wil\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "NE", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Bittet aber in der Stille,", "tokens": ["Bit\u00b7tet", "a\u00b7ber", "in", "der", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Da\u00df erst morgen auff die Nacht", "tokens": ["Da\u00df", "erst", "mor\u00b7gen", "auff", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.20": {"text": "Dieser Brieff werd auffgemacht;", "tokens": ["Die\u00b7ser", "Brieff", "werd", "auff\u00b7ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Dann sie sch\u00e4mt sich, da\u00df bey Leben", "tokens": ["Dann", "sie", "sch\u00e4mt", "sich", ",", "da\u00df", "bey", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "PRF", "$,", "KOUS", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Dieses Ding sie auff soll geben,", "tokens": ["Die\u00b7ses", "Ding", "sie", "auff", "soll", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPER", "APPR", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Wil auch, da\u00df kein andrer nicht", "tokens": ["Wil", "auch", ",", "da\u00df", "kein", "an\u00b7drer", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "$,", "KOUS", "PIAT", "PIS", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Ihres Willens Siegel bricht", "tokens": ["Ih\u00b7res", "Wil\u00b7lens", "Sie\u00b7gel", "bricht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Als der Erbe, den zu nennen", "tokens": ["Als", "der", "Er\u00b7be", ",", "den", "zu", "nen\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "$,", "ART", "PTKZU", "VVINF"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.26": {"text": "Sie err\u00f6thet, doch zu kennen", "tokens": ["Sie", "er\u00b7r\u00f6t\u00b7het", ",", "doch", "zu", "ken\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ADV", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "T\u00fcckisch richtet einen Blick", "tokens": ["T\u00fc\u00b7ckisch", "rich\u00b7tet", "ei\u00b7nen", "Blick"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Hin auff Nachbar Ludewig.", "tokens": ["Hin", "auff", "Nach\u00b7bar", "Lu\u00b7de\u00b7wig", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "NE", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.29": {"text": "Merckt, ihr Zeugen, da\u00df der Erbe", "tokens": ["Merckt", ",", "ihr", "Zeu\u00b7gen", ",", "da\u00df", "der", "Er\u00b7be"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Um bedenck-Zeit gar nicht werbe,", "tokens": ["Um", "be\u00b7den\u00b7ck\u00b7Zeit", "gar", "nicht", "wer\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.31": {"text": "Wil das Erbe treten an,", "tokens": ["Wil", "das", "Er\u00b7be", "tre\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "Wann er soll, und wann er kan.", "tokens": ["Wann", "er", "soll", ",", "und", "wann", "er", "kan", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VMFIN", "$,", "KON", "PWAV", "PPER", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Nur er dingt ihm au\u00df zu lachen,", "tokens": ["Nur", "er", "dingt", "ihm", "au\u00df", "zu", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Wie der Erben Brauch; wil machen,", "tokens": ["Wie", "der", "Er\u00b7ben", "Brauch", ";", "wil", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "$.", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Da\u00df auch sie dann lachen soll,", "tokens": ["Da\u00df", "auch", "sie", "dann", "la\u00b7chen", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.36": {"text": "Wann sie sp\u00fcrt, es thu so wol,", "tokens": ["Wann", "sie", "sp\u00fcrt", ",", "es", "thu", "so", "wol", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Wann man siht noch f\u00fcr dem sterben,", "tokens": ["Wann", "man", "siht", "noch", "f\u00fcr", "dem", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "ADV", "APPR", "ART", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Wie so danckbar sind die Erben.", "tokens": ["Wie", "so", "dan\u00b7ck\u00b7bar", "sind", "die", "Er\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.39": {"text": "Denn der Erbe bleibt bedacht,", "tokens": ["Denn", "der", "Er\u00b7be", "bleibt", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.40": {"text": "Wie es so werd au\u00dfgemacht,", "tokens": ["Wie", "es", "so", "werd", "au\u00df\u00b7ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.41": {"text": "Da\u00df man steiffes Wolbeginnen", "tokens": ["Da\u00df", "man", "steif\u00b7fes", "Wol\u00b7be\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Mercke nicht sehr weit von hinnen,", "tokens": ["Mer\u00b7cke", "nicht", "sehr", "weit", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "ADJD", "APPR", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Da\u00df die liebe Danckbarkeit", "tokens": ["Da\u00df", "die", "lie\u00b7be", "Dan\u00b7ck\u00b7bar\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.44": {"text": "J\u00e4hrlich au\u00df der Wiege schreyt.", "tokens": ["J\u00e4hr\u00b7lich", "au\u00df", "der", "Wie\u00b7ge", "schreyt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}