{"textgrid.poem.64510": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "134. Desgleichen", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der erste Sohn von ", "tokens": ["Der", "ers\u00b7te", "Sohn", "von"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Wie der gerathen wird, das wei\u00df man jetzt noch nicht;", "tokens": ["Wie", "der", "ge\u00b7ra\u00b7then", "wird", ",", "das", "wei\u00df", "man", "jetzt", "noch", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "VVPP", "VAFIN", "$,", "PDS", "VVFIN", "PIS", "ADV", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch erbt er nur vom Vater Eine Tugend,", "tokens": ["Doch", "erbt", "er", "nur", "vom", "Va\u00b7ter", "Ei\u00b7ne", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So wird er ganz gewi\u00df ein Kind, das viel verspricht.", "tokens": ["So", "wird", "er", "ganz", "ge\u00b7wi\u00df", "ein", "Kind", ",", "das", "viel", "ver\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "$,", "PRELS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}