{"textgrid.poem.44457": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "4. Reiselust", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kam zur\u00fcck die Lust zu schweifen?", "tokens": ["Kam", "zu\u00b7r\u00fcck", "die", "Lust", "zu", "schwei\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wunsch zugleich und Scheu der Rast;", "tokens": ["Wunsch", "zu\u00b7gleich", "und", "Scheu", "der", "Rast", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dr\u00e4ngts den Mi\u00dfmut abzustreifen", "tokens": ["Dr\u00e4ngts", "den", "Mi\u00df\u00b7mut", "ab\u00b7zu\u00b7strei\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVIZU"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In gedankenloser Hast?", "tokens": ["In", "ge\u00b7dan\u00b7ken\u00b7lo\u00b7ser", "Hast", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sieh die Pferde schon bereitet,", "tokens": ["Sieh", "die", "Pfer\u00b7de", "schon", "be\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das Ger\u00e4te schon beschickt,", "tokens": ["Das", "Ge\u00b7r\u00e4\u00b7te", "schon", "be\u00b7schickt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der Gesichtskreis ist erweitet,", "tokens": ["Der", "Ge\u00b7sichts\u00b7kreis", "ist", "er\u00b7wei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der Gesichtspunkt ist verr\u00fcckt.", "tokens": ["Der", "Ge\u00b7sichts\u00b7punkt", "ist", "ver\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und so gehts durch Deutschlands Gauen,", "tokens": ["Und", "so", "gehts", "durch", "Deutschlands", "Gau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "NE", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Peitschenstreichs von Ort zu Ort;", "tokens": ["Peit\u00b7schen\u00b7streichs", "von", "Ort", "zu", "Ort", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00fcd das Auge schon zu schauen,", "tokens": ["M\u00fcd", "das", "Au\u00b7ge", "schon", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Lippe m\u00fcd des Worts. \u2013", "tokens": ["Und", "die", "Lip\u00b7pe", "m\u00fcd", "des", "Worts", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Roma, Roma! Goldne Stunden,", "tokens": ["Ro\u00b7ma", ",", "Ro\u00b7ma", "!", "Gold\u00b7ne", "Stun\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$.", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als ich deine Zauber sah.", "tokens": ["Als", "ich", "dei\u00b7ne", "Zau\u00b7ber", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jahre sind seitdem entschwunden,", "tokens": ["Jah\u00b7re", "sind", "seit\u00b7dem", "ent\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PAV", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Und dein Reiz noch immer nah.", "tokens": ["Und", "dein", "Reiz", "noch", "im\u00b7mer", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Damals auch trieb bittrer Kummer", "tokens": ["Da\u00b7mals", "auch", "trieb", "bit\u00b7trer", "Kum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ADJA", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Mich aus meinem Heimatland,", "tokens": ["Mich", "aus", "mei\u00b7nem", "Hei\u00b7mat\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einer Mutter Grabesschlummer,", "tokens": ["Ei\u00b7ner", "Mut\u00b7ter", "Gra\u00b7bes\u00b7schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00fcb ein mi\u00dfgeschlungnes Band.", "tokens": ["Tr\u00fcb", "ein", "mi\u00df\u00b7ge\u00b7schlung\u00b7nes", "Band", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch wie anders und wie besser!", "tokens": ["Doch", "wie", "an\u00b7ders", "und", "wie", "bes\u00b7ser", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "KON", "PWAV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Erinnrung kam zur Rast,", "tokens": ["Die", "E\u00b7rinn\u00b7rung", "kam", "zur", "Rast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schw\u00e4cher wie der Abstand gr\u00f6\u00dfer,", "tokens": ["Schw\u00e4\u00b7cher", "wie", "der", "Ab\u00b7stand", "gr\u00f6\u00b7\u00dfer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jeder Schritt nahm eine Last;", "tokens": ["Je\u00b7der", "Schritt", "nahm", "ei\u00b7ne", "Last", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und von jeder hohen Schwelle", "tokens": ["Und", "von", "je\u00b7der", "ho\u00b7hen", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sah ein Himmlischer mich an,", "tokens": ["Sah", "ein", "Himm\u00b7li\u00b7scher", "mich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "R\u00fcckte sacht auf dem Gestelle,", "tokens": ["R\u00fcck\u00b7te", "sacht", "auf", "dem", "Ge\u00b7stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lud zu sich den Wandersmann.", "tokens": ["Lud", "zu", "sich", "den", "Wan\u00b7ders\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Nun sind m\u00fcder meine F\u00fc\u00dfe,", "tokens": ["Nun", "sind", "m\u00fc\u00b7der", "mei\u00b7ne", "F\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kummer h\u00e4lt schon gleichen Schritt,", "tokens": ["Kum\u00b7mer", "h\u00e4lt", "schon", "glei\u00b7chen", "Schritt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo ich Tempel ehrend gr\u00fc\u00dfe,", "tokens": ["Wo", "ich", "Tem\u00b7pel", "eh\u00b7rend", "gr\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nahm die Zeit die G\u00f6tter mit.", "tokens": ["Nahm", "die", "Zeit", "die", "G\u00f6t\u00b7ter", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Einer nur ist mir erschienen,", "tokens": ["Ei\u00b7ner", "nur", "ist", "mir", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Aber ich ertrug ihn nicht,", "tokens": ["A\u00b7ber", "ich", "er\u00b7trug", "ihn", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Abglanz seiner Mienen", "tokens": ["Und", "der", "Ab\u00b7glanz", "sei\u00b7ner", "Mie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ward statt Fl\u00fcgel mir Gewicht.", "tokens": ["Ward", "statt", "Fl\u00fc\u00b7gel", "mir", "Ge\u00b7wicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Schien er wie ein Zeus zu schreiten,", "tokens": ["Schien", "er", "wie", "ein", "Zeus", "zu", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "ART", "NE", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir hielt er, ein Chronos, vor", "tokens": ["Mir", "hielt", "er", ",", "ein", "Chro\u00b7nos", ",", "vor"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ART", "NN", "$,", "APPR"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "All den Unterschied der Zeiten,", "tokens": ["All", "den", "Un\u00b7ter\u00b7schied", "der", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, und all, was ich verlor.", "tokens": ["Ach", ",", "und", "all", ",", "was", "ich", "ver\u00b7lor", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KON", "PIAT", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}