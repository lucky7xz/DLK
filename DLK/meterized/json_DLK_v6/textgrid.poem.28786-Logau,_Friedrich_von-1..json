{"textgrid.poem.28786": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1650, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob uns gleich der Tod reist hin, ist von uns doch nichts nicht seine.", "tokens": ["Ob", "uns", "gleich", "der", "Tod", "reist", "hin", ",", "ist", "von", "uns", "doch", "nichts", "nicht", "sei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "PTKVZ", "$,", "VAFIN", "APPR", "PPER", "ADV", "PIS", "PTKNEG", "PPOSAT", "$."], "meter": "-++--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Unsre Seele k\u00fcmmt ihm nicht; unser Haut, Fleisch und Gebeine", "tokens": ["Uns\u00b7re", "See\u00b7le", "k\u00fcmmt", "ihm", "nicht", ";", "un\u00b7ser", "Haut", ",", "Fleisch", "und", "Ge\u00b7bei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$.", "PPOSAT", "NN", "$,", "NN", "KON", "NN"], "meter": "--+-+--+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wird uns sch\u00f6ner und verkl\u00e4rt sam der Seele wieder geben", "tokens": ["Wird", "uns", "sch\u00f6\u00b7ner", "und", "ver\u00b7kl\u00e4rt", "sam", "der", "See\u00b7le", "wie\u00b7der", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJA", "KON", "VVFIN", "ADJD", "ART", "NN", "ADV", "VVINF"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Jene Zeit, die ohne Zeit uns auffs neue heisset leben.", "tokens": ["Je\u00b7ne", "Zeit", ",", "die", "oh\u00b7ne", "Zeit", "uns", "auffs", "neu\u00b7e", "heis\u00b7set", "le\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "APPR", "NN", "PPER", "APPRART", "ADJA", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}