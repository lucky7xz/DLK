{"textgrid.poem.56025": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Die Brandst\u00e4tte", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gemieden von dem Fr\u00fchherbstmorgen, der", "tokens": ["Ge\u00b7mie\u00b7den", "von", "dem", "Fr\u00fch\u00b7herbst\u00b7mor\u00b7gen", ",", "der"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PRELS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "mi\u00dftrauisch war, lag hinter den versengten", "tokens": ["mi\u00df\u00b7trau\u00b7isch", "war", ",", "lag", "hin\u00b7ter", "den", "ver\u00b7seng\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "$,", "VVFIN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hauslinden, die das Heidehaus beengten,", "tokens": ["Haus\u00b7lin\u00b7den", ",", "die", "das", "Hei\u00b7de\u00b7haus", "be\u00b7eng\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "ein Neues, Leeres. Eine Stelle mehr,", "tokens": ["ein", "Neu\u00b7es", ",", "Lee\u00b7res", ".", "Ei\u00b7ne", "Stel\u00b7le", "mehr", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NE", "$.", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "auf welcher Kinder, von Gott wei\u00df woher,", "tokens": ["auf", "wel\u00b7cher", "Kin\u00b7der", ",", "von", "Gott", "wei\u00df", "wo\u00b7her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "$,", "APPR", "NN", "VVFIN", "ADJD", "$,"], "meter": "+--+--++-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "einander zuschrien und nach Fetzen haschten.", "tokens": ["ein\u00b7an\u00b7der", "zu\u00b7schri\u00b7en", "und", "nach", "Fet\u00b7zen", "haschten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "APPR", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Doch alle wurden stille, sooft er,", "tokens": ["Doch", "al\u00b7le", "wur\u00b7den", "stil\u00b7le", ",", "sooft", "er", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "VVFIN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "der Sohn von hier, aus hei\u00dfen, halbveraschten", "tokens": ["der", "Sohn", "von", "hier", ",", "aus", "hei\u00b7\u00dfen", ",", "halb\u00b7ve\u00b7raschten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "APPR", "ADV", "$,", "APPR", "VVINF", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Geb\u00e4lken Kessel und verbogne Tr\u00f6ge", "tokens": ["Ge\u00b7b\u00e4l\u00b7ken", "Kes\u00b7sel", "und", "ver\u00b7bog\u00b7ne", "Tr\u00f6\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "an einem langen Gabelaste zog, \u2013", "tokens": ["an", "ei\u00b7nem", "lan\u00b7gen", "Ga\u00b7be\u00b7las\u00b7te", "zog", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "um dann mit einem Blick als ob er l\u00f6ge", "tokens": ["um", "dann", "mit", "ei\u00b7nem", "Blick", "als", "ob", "er", "l\u00f6\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ADV", "APPR", "ART", "NN", "KOKOM", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "die andern anzusehn, die er bewog", "tokens": ["die", "an\u00b7dern", "an\u00b7zu\u00b7sehn", ",", "die", "er", "be\u00b7wog"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "VVIZU", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "zu glauben, was an dieser Stelle stand.", "tokens": ["zu", "glau\u00b7ben", ",", "was", "an", "die\u00b7ser", "Stel\u00b7le", "stand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PRELS", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Denn seit es nicht mehr war, schien es ihm so", "tokens": ["Denn", "seit", "es", "nicht", "mehr", "war", ",", "schien", "es", "ihm", "so"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "ADV", "VAFIN", "$,", "VVFIN", "PPER", "PPER", "ADV"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "seltsam: phantastischer als Pharao.", "tokens": ["selt\u00b7sam", ":", "phan\u00b7tas\u00b7ti\u00b7scher", "als", "Pha\u00b7rao", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "ADJD", "KOKOM", "NE", "$."], "meter": "+--+-++-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Und er war anders. Wie aus fernem Land.", "tokens": ["Und", "er", "war", "an\u00b7ders", ".", "Wie", "aus", "fer\u00b7nem", "Land", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "$.", "PWAV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}