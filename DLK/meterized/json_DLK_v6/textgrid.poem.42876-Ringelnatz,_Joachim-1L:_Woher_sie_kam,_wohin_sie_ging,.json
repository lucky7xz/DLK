{"textgrid.poem.42876": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "1L: Woher sie kam, wohin sie ging,", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Woher sie kam, wohin sie ging,", "tokens": ["Wo\u00b7her", "sie", "kam", ",", "wo\u00b7hin", "sie", "ging", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das hab' ich nie erfahren.", "tokens": ["Das", "hab'", "ich", "nie", "er\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie war ein namenloses Ding", "tokens": ["Sie", "war", "ein", "na\u00b7men\u00b7lo\u00b7ses", "Ding"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von etwa achtzehn Jahren,", "tokens": ["Von", "et\u00b7wa", "acht\u00b7zehn", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie k\u00fc\u00dfte selten ungest\u00fcm.", "tokens": ["Sie", "k\u00fc\u00df\u00b7te", "sel\u00b7ten", "un\u00b7ge\u00b7st\u00fcm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dann duftete es wie Parf\u00fcm", "tokens": ["Dann", "duf\u00b7te\u00b7te", "es", "wie", "Par\u00b7f\u00fcm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Aus ihren keuschen Haaren.", "tokens": ["Aus", "ih\u00b7ren", "keu\u00b7schen", "Haa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wir spielten nur, wir scherzten nur;", "tokens": ["Wir", "spiel\u00b7ten", "nur", ",", "wir", "scherz\u00b7ten", "nur", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir haben nie ges\u00fcndigt.", "tokens": ["Wir", "ha\u00b7ben", "nie", "ge\u00b7s\u00fcn\u00b7digt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie leistete mir jeden Schwur", "tokens": ["Sie", "leis\u00b7te\u00b7te", "mir", "je\u00b7den", "Schwur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und floh dann ungek\u00fcndigt.", "tokens": ["Und", "floh", "dann", "un\u00b7ge\u00b7k\u00fcn\u00b7digt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Entfloh mit meiner goldnen Uhr", "tokens": ["Ent\u00b7floh", "mit", "mei\u00b7ner", "gold\u00b7nen", "Uhr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Am selben Tag, da ich erfuhr,", "tokens": ["Am", "sel\u00b7ben", "Tag", ",", "da", "ich", "er\u00b7fuhr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Man habe mich entm\u00fcndigt.", "tokens": ["Man", "ha\u00b7be", "mich", "ent\u00b7m\u00fcn\u00b7digt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Verschwunden war mein Siegelring", "tokens": ["Ver\u00b7schwun\u00b7den", "war", "mein", "Sie\u00b7gel\u00b7ring"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beim Spielen oder Scherzen.", "tokens": ["Beim", "Spie\u00b7len", "o\u00b7der", "Scher\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie war ein zarter Schmetterling.", "tokens": ["Sie", "war", "ein", "zar\u00b7ter", "Schmet\u00b7ter\u00b7ling", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich werde nie verschmerzen,", "tokens": ["Ich", "wer\u00b7de", "nie", "ver\u00b7schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie vieles Goldene sie stahl,", "tokens": ["Wie", "vie\u00b7les", "Gol\u00b7de\u00b7ne", "sie", "stahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.6": {"text": "Das M\u00e4dchen mit dem Muttermal", "tokens": ["Das", "M\u00e4d\u00b7chen", "mit", "dem", "Mut\u00b7ter\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zwei Handbreit unterm Herzen.", "tokens": ["Zwei", "Hand\u00b7breit", "un\u00b7term", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}