{"textgrid.poem.33107": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Blume, die aus ihrem stillen Thale", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Blume, die aus ihrem stillen Thale", "tokens": ["Die", "Blu\u00b7me", ",", "die", "aus", "ih\u00b7rem", "stil\u00b7len", "Tha\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verpflanzt wird in ein fernes, fremdes Land,", "tokens": ["Ver\u00b7pflanzt", "wird", "in", "ein", "fer\u00b7nes", ",", "frem\u00b7des", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Prangt dort wie hier in holder Anmuth F\u00fclle,", "tokens": ["Prangt", "dort", "wie", "hier", "in", "hol\u00b7der", "An\u00b7muth", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KOKOM", "ADV", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist sie gepflegt von zarter Liebe Hand;", "tokens": ["Ist", "sie", "ge\u00b7pflegt", "von", "zar\u00b7ter", "Lie\u00b7be", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und siehst an ihrem Kelch du manchmal hangen", "tokens": ["Und", "siehst", "an", "ih\u00b7rem", "Kelch", "du", "manch\u00b7mal", "han\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein Tr\u00f6pflein, drauf die Morgensonne scheint,", "tokens": ["Ein", "Tr\u00f6pf\u00b7lein", ",", "drauf", "die", "Mor\u00b7gen\u00b7son\u00b7ne", "scheint", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So denk' nicht, da\u00df es Gramesthr\u00e4nen seien,", "tokens": ["So", "denk'", "nicht", ",", "da\u00df", "es", "Gra\u00b7mest\u00b7hr\u00e4\u00b7nen", "sei\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die tr\u00fcbe sie in stiller Nacht geweint.", "tokens": ["Die", "tr\u00fc\u00b7be", "sie", "in", "stil\u00b7ler", "Nacht", "ge\u00b7weint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPER", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wohl sind es Thr\u00e4nen, aber die nicht schmerzen,", "tokens": ["Wohl", "sind", "es", "Thr\u00e4\u00b7nen", ",", "a\u00b7ber", "die", "nicht", "schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "ADV", "ART", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erinn'rung hat sie leise wach gek\u00fc\u00dft:", "tokens": ["Er\u00b7inn'\u00b7rung", "hat", "sie", "lei\u00b7se", "wach", "ge\u00b7k\u00fc\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADJD", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn sanft ein Ton aus heimathlichen Fernen", "tokens": ["Wenn", "sanft", "ein", "Ton", "aus", "hei\u00b7math\u00b7li\u00b7chen", "Fer\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Verwandten Klangs der Blume Seele gr\u00fc\u00dft,", "tokens": ["Ver\u00b7wand\u00b7ten", "Klangs", "der", "Blu\u00b7me", "See\u00b7le", "gr\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dann rinnt die Thr\u00e4ne, doch nicht herb und bitter,", "tokens": ["Dann", "rinnt", "die", "Thr\u00e4\u00b7ne", ",", "doch", "nicht", "herb", "und", "bit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "PTKNEG", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein Opfer, der vergangnen Zeit geweiht,", "tokens": ["Ein", "Op\u00b7fer", ",", "der", "ver\u00b7gang\u00b7nen", "Zeit", "ge\u00b7weiht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dann s\u00e4uselt durch die Seele linde Wehmuth", "tokens": ["Dann", "s\u00e4u\u00b7selt", "durch", "die", "See\u00b7le", "lin\u00b7de", "Weh\u00b7muth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und sanfte Ruhe folgt dem s\u00fc\u00dfen Leid.", "tokens": ["Und", "sanf\u00b7te", "Ru\u00b7he", "folgt", "dem", "s\u00fc\u00b7\u00dfen", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So tr\u00e4umst gewi\u00df auch du, wenn du geschieden,", "tokens": ["So", "tr\u00e4umst", "ge\u00b7wi\u00df", "auch", "du", ",", "wenn", "du", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "PPER", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch oft von deinem deutschen Vaterland,", "tokens": ["Noch", "oft", "von", "dei\u00b7nem", "deut\u00b7schen", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von deinen Kinder-, deinen Jugendtagen \u2013", "tokens": ["Von", "dei\u00b7nen", "Kin\u00b7der", ",", "dei\u00b7nen", "Ju\u00b7gend\u00b7ta\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "TRUNC", "$,", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bis dich verpflanzte zarter Liebe Hand;", "tokens": ["Bis", "dich", "ver\u00b7pflanz\u00b7te", "zar\u00b7ter", "Lie\u00b7be", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und wie ich hoffe, da\u00df dem Angedenken", "tokens": ["Und", "wie", "ich", "hof\u00b7fe", ",", "da\u00df", "dem", "An\u00b7ge\u00b7den\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Es manchmal hei\u00df von deiner Wange quillt,", "tokens": ["Es", "manch\u00b7mal", "hei\u00df", "von", "dei\u00b7ner", "Wan\u00b7ge", "quillt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So soll dich stets mein treuster Wunsch geleiten,", "tokens": ["So", "soll", "dich", "stets", "mein", "treus\u00b7ter", "Wunsch", "ge\u00b7lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df andres Weinen nie dein Auge f\u00fcllt!", "tokens": ["Da\u00df", "and\u00b7res", "Wei\u00b7nen", "nie", "dein", "Au\u00b7ge", "f\u00fcllt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}