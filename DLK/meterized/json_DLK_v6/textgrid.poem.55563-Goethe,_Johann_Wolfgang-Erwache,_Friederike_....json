{"textgrid.poem.55563": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Erwache, Friederike ...", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erwache, Friederike,", "tokens": ["Er\u00b7wa\u00b7che", ",", "Frie\u00b7de\u00b7ri\u00b7ke", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vertreib die Nacht,", "tokens": ["Ver\u00b7treib", "die", "Nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die einer deiner Blicke", "tokens": ["Die", "ei\u00b7ner", "dei\u00b7ner", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum Tage macht.", "tokens": ["Zum", "Ta\u00b7ge", "macht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Der V\u00f6gel sanft Gefl\u00fcster", "tokens": ["Der", "V\u00f6\u00b7gel", "sanft", "Ge\u00b7fl\u00fcs\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ruft liebevoll,", "tokens": ["Ruft", "lie\u00b7be\u00b7voll", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Da\u00df mein geliebt Geschwister", "tokens": ["Da\u00df", "mein", "ge\u00b7liebt", "Ge\u00b7schwis\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "VVPP", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Erwachen soll.", "tokens": ["Er\u00b7wa\u00b7chen", "soll", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VMFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Es zittert Morgenschimmer", "tokens": ["Es", "zit\u00b7tert", "Mor\u00b7gen\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit bl\u00f6dem Licht", "tokens": ["Mit", "bl\u00f6\u00b7dem", "Licht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Err\u00f6tend durch dein Zimmer", "tokens": ["Er\u00b7r\u00f6\u00b7tend", "durch", "dein", "Zim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und weckt dich nicht.", "tokens": ["Und", "weckt", "dich", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Am Busen deiner Schwester,", "tokens": ["Am", "Bu\u00b7sen", "dei\u00b7ner", "Schwes\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der f\u00fcr dich schlagt,", "tokens": ["Der", "f\u00fcr", "dich", "schlagt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Entschl\u00e4fst du immer fester,", "tokens": ["Ent\u00b7schl\u00e4fst", "du", "im\u00b7mer", "fes\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Je mehr es tagt.", "tokens": ["Je", "mehr", "es", "tagt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Die Nachtigall im Schlafe", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "im", "Schla\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hast du vers\u00e4umt;", "tokens": ["Hast", "du", "ver\u00b7s\u00e4umt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "So h\u00f6re nun zur Strafe,", "tokens": ["So", "h\u00f6\u00b7re", "nun", "zur", "Stra\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was ich gereimt.", "tokens": ["Was", "ich", "ge\u00b7reimt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Schwer lag auf meinem Busen", "tokens": ["Schwer", "lag", "auf", "mei\u00b7nem", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Des Reimes Joch;", "tokens": ["Des", "Rei\u00b7mes", "Joch", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Die sch\u00f6nste meiner Musen,", "tokens": ["Die", "sch\u00f6ns\u00b7te", "mei\u00b7ner", "Mu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Du \u2013 schliefst ja noch.", "tokens": ["Du", "\u2013", "schliefst", "ja", "noch", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}