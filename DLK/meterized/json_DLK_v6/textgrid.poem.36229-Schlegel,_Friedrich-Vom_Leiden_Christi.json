{"textgrid.poem.36229": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Vom Leiden Christi", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erd' und Himmel sich beklagten,", "tokens": ["Erd'", "und", "Him\u00b7mel", "sich", "be\u00b7klag\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00fcbe war das Licht verborgen,", "tokens": ["Tr\u00fc\u00b7be", "war", "das", "Licht", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00fctender das Meer zu br\u00fcllen", "tokens": ["W\u00fc\u00b7ten\u00b7der", "das", "Meer", "zu", "br\u00fcl\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00e4lzte dunkel seine Wogen,", "tokens": ["W\u00e4lz\u00b7te", "dun\u00b7kel", "sei\u00b7ne", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Als der hohe Welterl\u00f6ser,", "tokens": ["Als", "der", "ho\u00b7he", "Wel\u00b7ter\u00b7l\u00f6\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "An dem Kreuze bald gestorben,", "tokens": ["An", "dem", "Kreu\u00b7ze", "bald", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Worte, w\u00fcrdig hei\u00dfer Tr\u00e4nen,", "tokens": ["Wor\u00b7te", ",", "w\u00fcr\u00b7dig", "hei\u00b7\u00dfer", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Also sagte, wie sie folgen:", "tokens": ["Al\u00b7so", "sag\u00b7te", ",", "wie", "sie", "fol\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "\u00bbnun, o Herr, in deine H\u00e4nde", "tokens": ["\u00bb", "nun", ",", "o", "Herr", ",", "in", "dei\u00b7ne", "H\u00e4n\u00b7de"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ADV", "$,", "FM", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Sei anjetzt mein Geist befohlen.\u00ab", "tokens": ["Sei", "an\u00b7jetzt", "mein", "Geist", "be\u00b7foh\u00b7len", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "O uns\u00e4glich tiefe Trauer,", "tokens": ["O", "un\u00b7s\u00e4g\u00b7lich", "tie\u00b7fe", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Unvergleichbar bittre Lose,", "tokens": ["Un\u00b7ver\u00b7gleich\u00b7bar", "bitt\u00b7re", "Lo\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df der unerschaffne Sch\u00f6pfer", "tokens": ["Da\u00df", "der", "un\u00b7er\u00b7schaff\u00b7ne", "Sch\u00f6p\u00b7fer"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Selber zum Gesch\u00f6pf geworden,", "tokens": ["Sel\u00b7ber", "zum", "Ge\u00b7sch\u00f6pf", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Um dieselben zu erretten,", "tokens": ["Um", "die\u00b7sel\u00b7ben", "zu", "er\u00b7ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PDAT", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die ihm gaben Tod zum Lohne!", "tokens": ["Die", "ihm", "ga\u00b7ben", "Tod", "zum", "Loh\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nur du seine hohe Mutter,", "tokens": ["Nur", "du", "sei\u00b7ne", "ho\u00b7he", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Reiner Jungfrau'n heil'ge Krone,", "tokens": ["Rei\u00b7ner", "Jung\u00b7frau'n", "heil'\u00b7ge", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Du allein vom Trost entkleidet,", "tokens": ["Du", "al\u00b7lein", "vom", "Trost", "ent\u00b7klei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Magst es sagen, Freudenlose!", "tokens": ["Magst", "es", "sa\u00b7gen", ",", "Freu\u00b7den\u00b7lo\u00b7se", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}