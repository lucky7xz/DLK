{"textgrid.poem.24191": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Schwerthuldigung", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit meinen Speeren", "tokens": ["Mit", "mei\u00b7nen", "Spee\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Will ich dich ehren,", "tokens": ["Will", "ich", "dich", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Mit meinen Schwerten", "tokens": ["Mit", "mei\u00b7nen", "Schwer\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Will ich dich werten,", "tokens": ["Will", "ich", "dich", "wer\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Mit Stechen und Hauen", "tokens": ["Mit", "Ste\u00b7chen", "und", "Hau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Will ich dir trauen,", "tokens": ["Will", "ich", "dir", "trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Herr Feind!", "tokens": ["Herr", "Feind", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "-+", "measure": "iambic.single"}}}}}