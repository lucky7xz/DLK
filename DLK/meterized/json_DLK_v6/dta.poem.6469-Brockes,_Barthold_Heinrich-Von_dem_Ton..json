{"dta.poem.6469": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Ton.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn wir bem\u00fcht sind zu ergr\u00fcnden,", "tokens": ["Wenn", "wir", "be\u00b7m\u00fcht", "sind", "zu", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und gr\u00fcndlich zu verstehn, wie er formirt;", "tokens": ["Und", "gr\u00fcnd\u00b7lich", "zu", "ver\u00b7stehn", ",", "wie", "er", "for\u00b7mirt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKZU", "VVINF", "$,", "PWAV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So werden wir sogleich befinden,", "tokens": ["So", "wer\u00b7den", "wir", "sog\u00b7leich", "be\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df unsre Ohren sind ger\u00fchrt", "tokens": ["Da\u00df", "uns\u00b7re", "Oh\u00b7ren", "sind", "ge\u00b7r\u00fchrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Durch eine Sch\u00fctterung, die in die Lufft gedr\u00fcckt.", "tokens": ["Durch", "ei\u00b7ne", "Sch\u00fct\u00b7te\u00b7rung", ",", "die", "in", "die", "Lufft", "ge\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es m\u00fcssen C\u00f6rper stets, aus welchen T\u00f6n\u2019 entstehn,", "tokens": ["Es", "m\u00fcs\u00b7sen", "C\u00f6r\u00b7per", "stets", ",", "aus", "wel\u00b7chen", "T\u00f6n'", "ent\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NE", "ADV", "$,", "APPR", "PWAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die L\u00fcffte, durch ein reges Zittern,", "tokens": ["Die", "L\u00fcff\u00b7te", ",", "durch", "ein", "re\u00b7ges", "Zit\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Auf solche Art bewegen und ersch\u00fcttern,", "tokens": ["Auf", "sol\u00b7che", "Art", "be\u00b7we\u00b7gen", "und", "er\u00b7sch\u00fct\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wie Wellen, die sich kr\u00e4uselnd drehn,", "tokens": ["Wie", "Wel\u00b7len", ",", "die", "sich", "kr\u00e4u\u00b7selnd", "drehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PRELS", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wovon die eine stets die andere ber\u00fchrt.", "tokens": ["Wo\u00b7von", "die", "ei\u00b7ne", "stets", "die", "an\u00b7de\u00b7re", "be\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ART", "ADV", "ART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wenn man die Instrumenten sp\u00fcrt,", "tokens": ["Wenn", "man", "die", "Inst\u00b7ru\u00b7men\u00b7ten", "sp\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Geschieht es durch ein reges Wallen,", "tokens": ["Ge\u00b7schieht", "es", "durch", "ein", "re\u00b7ges", "Wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.13": {"text": "Durch zitterndes Bewegen, R\u00fcckw\u00e4rts-Prallen,", "tokens": ["Durch", "zit\u00b7tern\u00b7des", "Be\u00b7we\u00b7gen", ",", "R\u00fc\u00b7ck\u00b7w\u00e4rts\u00b7Pral\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}