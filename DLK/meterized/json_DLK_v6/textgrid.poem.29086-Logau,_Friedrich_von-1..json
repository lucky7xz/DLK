{"textgrid.poem.29086": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1650, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein altes M\u00fctterlein, die hatt ein hitzig Fieber.", "tokens": ["Ein", "al\u00b7tes", "M\u00fct\u00b7ter\u00b7lein", ",", "die", "hatt", "ein", "hit\u00b7zig", "Fie\u00b7ber", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Tod, der war ihr lieb; das Leben war ihr lieber.", "tokens": ["Der", "Tod", ",", "der", "war", "ihr", "lieb", ";", "das", "Le\u00b7ben", "war", "ihr", "lie\u00b7ber", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "PPER", "ADJD", "$.", "ART", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie fuhr im Geiste fort; im Leibe blieb sie hier;", "tokens": ["Sie", "fuhr", "im", "Geis\u00b7te", "fort", ";", "im", "Lei\u00b7be", "blieb", "sie", "hier", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$.", "APPRART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie a\u00df noch gerne gut, tranck lieber Wein als Bier.", "tokens": ["Sie", "a\u00df", "noch", "ger\u00b7ne", "gut", ",", "tranck", "lie\u00b7ber", "Wein", "als", "Bier", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJD", "$,", "VVFIN", "ADV", "NN", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}