{"dta.poem.10332": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Iv.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Lieb Liebchen, leg's H\u00e4ndchen auf's Herze mein;", "tokens": ["Lieb", "Lieb\u00b7chen", ",", "leg's", "H\u00e4nd\u00b7chen", "auf's", "Her\u00b7ze", "mein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ADJA", "NN", "APPRART", "VVFIN", "PPOSAT", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ach, h\u00f6rst du, wie's pochet im K\u00e4mmerlein?", "tokens": ["Ach", ",", "h\u00f6rst", "du", ",", "wie's", "po\u00b7chet", "im", "K\u00e4m\u00b7mer\u00b7lein", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "$,", "PWAV", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Da hauset ein Zimmermann schlimm und arg,", "tokens": ["Da", "hau\u00b7set", "ein", "Zim\u00b7mer\u00b7mann", "schlimm", "und", "arg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Der zimmert mir einen Todtensarg.", "tokens": ["Der", "zim\u00b7mert", "mir", "ei\u00b7nen", "Tod\u00b7ten\u00b7sarg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Es h\u00e4mmert und klopfet bei Tag und bei Nacht;", "tokens": ["Es", "h\u00e4m\u00b7mert", "und", "klop\u00b7fet", "bei", "Tag", "und", "bei", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Es hat mich schon l\u00e4ngst um den Schlaf gebracht.", "tokens": ["Es", "hat", "mich", "schon", "l\u00e4ngst", "um", "den", "Schlaf", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ach! sputet Euch, Meister Zimmermann,", "tokens": ["Ach", "!", "spu\u00b7tet", "Euch", ",", "Meis\u00b7ter", "Zim\u00b7mer\u00b7mann", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "$,", "NE", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Damit ich balde schlafen kann.", "tokens": ["Da\u00b7mit", "ich", "bal\u00b7de", "schla\u00b7fen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}