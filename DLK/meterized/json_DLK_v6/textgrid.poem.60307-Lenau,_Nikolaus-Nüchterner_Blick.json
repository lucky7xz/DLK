{"textgrid.poem.60307": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "N\u00fcchterner Blick", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Grund begraben wird hier, dort gefunden", "tokens": ["Im", "Grund", "be\u00b7gra\u00b7ben", "wird", "hier", ",", "dort", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "VVPP", "VAFIN", "ADV", "$,", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vergangner Pflanzen steingewordne Spur,", "tokens": ["Ver\u00b7gang\u00b7ner", "Pflan\u00b7zen", "stein\u00b7ge\u00b7word\u00b7ne", "Spur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gebein von Tierart, die vorl\u00e4ngst entschwunden,", "tokens": ["Ge\u00b7bein", "von", "Tier\u00b7art", ",", "die", "vor\u00b7l\u00e4ngst", "ent\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die abgelegten Kleider der Natur.", "tokens": ["Die", "ab\u00b7ge\u00b7leg\u00b7ten", "Klei\u00b7der", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und wollt ihr dann in staunenden Gedanken", "tokens": ["Und", "wollt", "ihr", "dann", "in", "stau\u00b7nen\u00b7den", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Gliedermassen euch zusammenf\u00fcgen,", "tokens": ["Die", "Glie\u00b7der\u00b7mas\u00b7sen", "euch", "zu\u00b7sam\u00b7men\u00b7f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sinds Riesen, \u00fcberragend alle Schranken,", "tokens": ["Sinds", "Rie\u00b7sen", ",", "\u00fc\u00b7berr\u00b7a\u00b7gend", "al\u00b7le", "Schran\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "VVPP", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ihr schaut Urwelt in gro\u00dfen Schreckensz\u00fcgen.", "tokens": ["Ihr", "schaut", "Ur\u00b7welt", "in", "gro\u00b7\u00dfen", "Schre\u00b7ckens\u00b7z\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der Riese wandelt \u2013 und es bebt der Grund;", "tokens": ["Der", "Rie\u00b7se", "wan\u00b7delt", "\u2013", "und", "es", "bebt", "der", "Grund", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Er z\u00fcrnt \u2013 sein Sturmesodem gl\u00fcht und qualmt,", "tokens": ["Er", "z\u00fcrnt", "\u2013", "sein", "Stur\u00b7me\u00b7so\u00b7dem", "gl\u00fcht", "und", "qualmt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Von seinem Tritt wird jeder Feind zermalmt;", "tokens": ["Von", "sei\u00b7nem", "Tritt", "wird", "je\u00b7der", "Feind", "zer\u00b7malmt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Wie freut ihr euch, da\u00df tot der gro\u00dfe Fund!", "tokens": ["Wie", "freut", "ihr", "euch", ",", "da\u00df", "tot", "der", "gro\u00b7\u00dfe", "Fund", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "$,", "KOUS", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "So d\u00fcnkt euch schier des Mittelalters Glaube", "tokens": ["So", "d\u00fcnkt", "euch", "schier", "des", "Mit\u00b7tel\u00b7al\u00b7ters", "Glau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Ein Unget\u00fcm, das einst von Land zu Land", "tokens": ["Ein", "Un\u00b7ge\u00b7t\u00fcm", ",", "das", "einst", "von", "Land", "zu", "Land"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Verheerend zog und von der Erde schwand;", "tokens": ["Ver\u00b7hee\u00b7rend", "zog", "und", "von", "der", "Er\u00b7de", "schwand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Ihr w\u00fcnscht dem Tode Gl\u00fcck zu seinem Raube.", "tokens": ["Ihr", "w\u00fcnscht", "dem", "To\u00b7de", "Gl\u00fcck", "zu", "sei\u00b7nem", "Rau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Doch stehn, von allen St\u00fcrmen unersch\u00fcttert,", "tokens": ["Doch", "stehn", ",", "von", "al\u00b7len", "St\u00fcr\u00b7men", "un\u00b7er\u00b7sch\u00fct\u00b7tert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "APPR", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Die M\u00fcnster da, der klugen Zeit ein Grauen,", "tokens": ["Die", "M\u00fcns\u00b7ter", "da", ",", "der", "klu\u00b7gen", "Zeit", "ein", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Wie hohe Felsenkrippen anzuschauen,", "tokens": ["Wie", "ho\u00b7he", "Fel\u00b7sen\u00b7krip\u00b7pen", "an\u00b7zu\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Wo jenes Ungeheuer ward gef\u00fcttert.", "tokens": ["Wo", "je\u00b7nes", "Un\u00b7ge\u00b7heu\u00b7er", "ward", "ge\u00b7f\u00fct\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}