{"dta.poem.19257": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "104.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sieh, wie das R\u00e4uplein auf dem schwanken Blatte geht,", "tokens": ["Sieh", ",", "wie", "das", "R\u00e4up\u00b7lein", "auf", "dem", "schwan\u00b7ken", "Blat\u00b7te", "geht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das K\u00f6pfchen her und hin nach seinem Futter dreht!", "tokens": ["Das", "K\u00f6pf\u00b7chen", "her", "und", "hin", "nach", "sei\u00b7nem", "Fut\u00b7ter", "dreht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "KON", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn es ein Hauch ber\u00fchrt, wenn einen Feind es sp\u00fcrt,", "tokens": ["Wenn", "es", "ein", "Hauch", "be\u00b7r\u00fchrt", ",", "wenn", "ei\u00b7nen", "Feind", "es", "sp\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schnell wirfts ein Seil aus, das es immer bei sich f\u00fchrt.", "tokens": ["Schnell", "wirfts", "ein", "Seil", "aus", ",", "das", "es", "im\u00b7mer", "bei", "sich", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$,", "PRELS", "PPER", "ADV", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Aus seinem Leibe spinnt es selber sich das Seil,", "tokens": ["Aus", "sei\u00b7nem", "Lei\u00b7be", "spinnt", "es", "sel\u00b7ber", "sich", "das", "Seil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo's Noth thut, und daran h\u00e4ngt seines Lebens Heil.", "tokens": ["Wo's", "Noth", "thut", ",", "und", "da\u00b7ran", "h\u00e4ngt", "sei\u00b7nes", "Le\u00b7bens", "Heil", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "$,", "KON", "PAV", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Am Seile selbstgewebt, sieh, wie's hernieder schwebt,", "tokens": ["Am", "Sei\u00b7le", "selbst\u00b7ge\u00b7webt", ",", "sieh", ",", "wie's", "her\u00b7nie\u00b7der", "schwebt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ohne zu fallen h\u00e4ngt, und wieder sich erhebt!", "tokens": ["Oh\u00b7ne", "zu", "fal\u00b7len", "h\u00e4ngt", ",", "und", "wie\u00b7der", "sich", "er\u00b7hebt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PTKZU", "VVINF", "VVFIN", "$,", "KON", "ADV", "PRF", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.5": {"line.1": {"text": "Was sein Bed\u00fcrfnis heischt zur Sicherheit und Nahrung,", "tokens": ["Was", "sein", "Be\u00b7d\u00fcrf\u00b7nis", "heischt", "zur", "Si\u00b7cher\u00b7heit", "und", "Nah\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat es sein Trieb gelehrt, nicht Kunst und nicht Erfahrung.", "tokens": ["Hat", "es", "sein", "Trieb", "ge\u00b7lehrt", ",", "nicht", "Kunst", "und", "nicht", "Er\u00b7fah\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$,", "PTKNEG", "NN", "KON", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "H\u00e4tt' einen solchen Strick in jedem Augenblick", "tokens": ["H\u00e4tt'", "ei\u00b7nen", "sol\u00b7chen", "Strick", "in", "je\u00b7dem", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "PIAT", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der T\u00e4nzer auf dem Seil', nie br\u00e4ch' er sein Genick.", "tokens": ["Der", "T\u00e4n\u00b7zer", "auf", "dem", "Seil'", ",", "nie", "br\u00e4ch'", "er", "sein", "Ge\u00b7nick", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wol wandelt wie die Raup' auch er auf schwankem Steig,", "tokens": ["Wol", "wan\u00b7delt", "wie", "die", "Raup'", "auch", "er", "auf", "schwan\u00b7kem", "Steig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "ART", "NN", "ADV", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Und in den L\u00fcften sucht er seinen Nahrungszweig.", "tokens": ["Und", "in", "den", "L\u00fcf\u00b7ten", "sucht", "er", "sei\u00b7nen", "Nah\u00b7rungs\u00b7zweig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Doch treibt der Vorwitz ihn, das R\u00e4upchen die Natur,", "tokens": ["Doch", "treibt", "der", "Vor\u00b7witz", "ihn", ",", "das", "R\u00e4upc\u00b7hen", "die", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drum schwebt er in Gefahr, und es ist sicher nur.", "tokens": ["Drum", "schwebt", "er", "in", "Ge\u00b7fahr", ",", "und", "es", "ist", "si\u00b7cher", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "NN", "$,", "KON", "PPER", "VAFIN", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}