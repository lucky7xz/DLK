{"dta.poem.7660": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "32.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du sondre stolz und kalt dich nicht von der Gemeine", "tokens": ["Du", "sond\u00b7re", "stolz", "und", "kalt", "dich", "nicht", "von", "der", "Ge\u00b7mei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "PPER", "PTKNEG", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Betenden, weil du so gut es kannst alleine.", "tokens": ["Der", "Be\u00b7ten\u00b7den", ",", "weil", "du", "so", "gut", "es", "kannst", "al\u00b7lei\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ADV", "ADJD", "PPER", "VMFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zwar Gott ist \u00fcberall, und nie wird in der Schaar", "tokens": ["Zwar", "Gott", "ist", "\u00fc\u00b7be\u00b7rall", ",", "und", "nie", "wird", "in", "der", "Schaar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VAFIN", "ADV", "$,", "KON", "ADV", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihn finden, wem er nicht bereits im Herzen war.", "tokens": ["Ihn", "fin\u00b7den", ",", "wem", "er", "nicht", "be\u00b7reits", "im", "Her\u00b7zen", "war", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "PWS", "PPER", "PTKNEG", "ADV", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch wo der Scheiter viel in einer Flamme brennen,", "tokens": ["Doch", "wo", "der", "Schei\u00b7ter", "viel", "in", "ei\u00b7ner", "Flam\u00b7me", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird das Gef\u00fchl es an vermehrter Glut erkennen.", "tokens": ["Wird", "das", "Ge\u00b7f\u00fchl", "es", "an", "ver\u00b7mehr\u00b7ter", "Glut", "er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}