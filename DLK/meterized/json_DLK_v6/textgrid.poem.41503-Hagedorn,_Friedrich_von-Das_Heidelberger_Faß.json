{"textgrid.poem.41503": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Das Heidelberger Fa\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Freunde! la\u00dft uns altklug werden", "tokens": ["Ihr", "Freun\u00b7de", "!", "la\u00dft", "uns", "alt\u00b7klug", "wer\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "VVIMP", "PPER", "ADJD", "VAINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und weiser, als die Weisen, sein;", "tokens": ["Und", "wei\u00b7ser", ",", "als", "die", "Wei\u00b7sen", ",", "sein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJA", "$,", "KOUS", "ART", "NN", "$,", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Entsaget aller Lust auf Erden;", "tokens": ["Ent\u00b7sa\u00b7get", "al\u00b7ler", "Lust", "auf", "Er\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Entsagt den Sch\u00f6nen und dem Wein!", "tokens": ["Ent\u00b7sagt", "den", "Sch\u00f6\u00b7nen", "und", "dem", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr lacht und spitzt den Mund auf K\u00fcsse:", "tokens": ["Ihr", "lacht", "und", "spitzt", "den", "Mund", "auf", "K\u00fcs\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr lacht und f\u00fcllt das Deckelglas;", "tokens": ["Ihr", "lacht", "und", "f\u00fcllt", "das", "De\u00b7ckel\u00b7glas", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Euch meistern keine strengen Schl\u00fcsse;", "tokens": ["Euch", "meis\u00b7tern", "kei\u00b7ne", "stren\u00b7gen", "Schl\u00fcs\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Euch lehrt das Heidelberger Fa\u00df.", "tokens": ["Euch", "lehrt", "das", "Hei\u00b7del\u00b7ber\u00b7ger", "Fa\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Was lehret das?", "tokens": ["Was", "leh\u00b7ret", "das", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PDS", "$."], "meter": "-+--", "measure": "dactylic.init"}}}}}