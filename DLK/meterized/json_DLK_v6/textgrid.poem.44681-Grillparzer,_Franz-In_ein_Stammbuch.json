{"textgrid.poem.44681": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "In ein Stammbuch", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer die Tonkunst liebt, wie ich,", "tokens": ["Wer", "die", "Ton\u00b7kunst", "liebt", ",", "wie", "ich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "PWAV", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und, wie ich, die Klugen und die Guten;", "tokens": ["Und", ",", "wie", "ich", ",", "die", "Klu\u00b7gen", "und", "die", "Gu\u00b7ten", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPER", "$,", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "L\u00e4\u00dft von dem sich wohl vermuten,", "tokens": ["L\u00e4\u00dft", "von", "dem", "sich", "wohl", "ver\u00b7mu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRELS", "PRF", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df er nicht auch liebte dich?", "tokens": ["Da\u00df", "er", "nicht", "auch", "lieb\u00b7te", "dich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}