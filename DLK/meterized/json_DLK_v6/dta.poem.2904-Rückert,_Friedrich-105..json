{"dta.poem.2904": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "105.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Geldhunger nicht allein hat nie gestopft den Mund,", "tokens": ["Geld\u00b7hun\u00b7ger", "nicht", "al\u00b7lein", "hat", "nie", "ge\u00b7stopft", "den", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "VAFIN", "ADV", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Ruhmdurst noch vielmehr hat immer trocknen Schlund.", "tokens": ["Der", "Ruhm\u00b7durst", "noch", "viel\u00b7mehr", "hat", "im\u00b7mer", "trock\u00b7nen", "Schlund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Er schlinget Strom auf Strom, und f\u00fchlt sich nicht geletzt;", "tokens": ["Er", "schlin\u00b7get", "Strom", "auf", "Strom", ",", "und", "f\u00fchlt", "sich", "nicht", "ge\u00b7letzt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN", "$,", "KON", "VVFIN", "PRF", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Tr\u00f6pflein brennet ihn, das fremde Gaumen netzt.", "tokens": ["Das", "Tr\u00f6pf\u00b7lein", "bren\u00b7net", "ihn", ",", "das", "frem\u00b7de", "Gau\u00b7men", "netzt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}