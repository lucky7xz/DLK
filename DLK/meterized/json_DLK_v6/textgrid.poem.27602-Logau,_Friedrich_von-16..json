{"textgrid.poem.27602": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lebt, liebes Paar, mit Gott; lebt, liebes Paar, mit Segen;", "tokens": ["Lebt", ",", "lie\u00b7bes", "Paar", ",", "mit", "Gott", ";", "lebt", ",", "lie\u00b7bes", "Paar", ",", "mit", "Se\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "APPR", "NN", "$.", "VVFIN", "$,", "ADJA", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lebt, liebes Paar, im Gl\u00fcck, da\u00df Neid euch k\u00f6nn' erregen;", "tokens": ["Lebt", ",", "lie\u00b7bes", "Paar", ",", "im", "Gl\u00fcck", ",", "da\u00df", "Neid", "euch", "k\u00f6nn'", "er\u00b7re\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "APPRART", "NN", "$,", "KOUS", "NN", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich sage noch einmal: lebt hin in s\u00fcsser Ruh,", "tokens": ["Ich", "sa\u00b7ge", "noch", "ein\u00b7mal", ":", "lebt", "hin", "in", "s\u00fcs\u00b7ser", "Ruh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$.", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Bi\u00df Kindes-Kindeskind dr\u00fcck euer Augen zu!", "tokens": ["Bi\u00df", "Kin\u00b7des\u00b7Kin\u00b7des\u00b7kind", "dr\u00fcck", "eu\u00b7er", "Au\u00b7gen", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}