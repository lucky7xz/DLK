{"textgrid.poem.31833": {"metadata": {"author": {"name": "Angelus Silesius", "birth": "N.A.", "death": "N.A."}, "title": "222. Die Hoffnung", "genre": "verse", "period": "N.A.", "pub_year": 1650, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Hoffnung ist ein Seil; k\u00f6nnt ein Verdammter hoffen,", "tokens": ["Die", "Hoff\u00b7nung", "ist", "ein", "Seil", ";", "k\u00f6nnt", "ein", "Ver\u00b7damm\u00b7ter", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$.", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gott z\u00f6g ihn aus dem Pfuhl, in dem er ist ersoffen.", "tokens": ["Gott", "z\u00f6g", "ihn", "aus", "dem", "Pfuhl", ",", "in", "dem", "er", "ist", "er\u00b7sof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}