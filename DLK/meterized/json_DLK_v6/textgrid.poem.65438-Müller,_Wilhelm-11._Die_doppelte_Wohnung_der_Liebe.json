{"textgrid.poem.65438": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "11. Die doppelte Wohnung der Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei H\u00e4user hat die Liebe f\u00fcr des Geliebten Bild:", "tokens": ["Zwei", "H\u00e4u\u00b7ser", "hat", "die", "Lie\u00b7be", "f\u00fcr", "des", "Ge\u00b7lieb\u00b7ten", "Bild", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Erst wohnet es im Auge, bis dieses \u00fcberschwillt", "tokens": ["Erst", "woh\u00b7net", "es", "im", "Au\u00b7ge", ",", "bis", "die\u00b7ses", "\u00fc\u00b7bersc\u00b7hwillt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,", "KOUS", "PDS", "VVFIN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Vom Thr\u00e4nenstrom der Schmerzen und zwingt zur Flucht den Gast;", "tokens": ["Vom", "Thr\u00e4\u00b7nen\u00b7strom", "der", "Schmer\u00b7zen", "und", "zwingt", "zur", "Flucht", "den", "Gast", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "KON", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Dann steigt in's Herz er nieder und findet sichre Rast.", "tokens": ["Dann", "steigt", "in's", "Herz", "er", "nie\u00b7der", "und", "fin\u00b7det", "sich\u00b7re", "Rast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PPER", "PTKVZ", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}