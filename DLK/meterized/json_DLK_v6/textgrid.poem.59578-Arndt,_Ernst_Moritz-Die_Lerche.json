{"textgrid.poem.59578": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Die Lerche", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als man das achte Jahr zu Achtzehnhundert nach Christi", "tokens": ["Als", "man", "das", "ach\u00b7te", "Jahr", "zu", "Acht\u00b7zehn\u00b7hun\u00b7dert", "nach", "Chris\u00b7ti"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN", "APPR", "NE", "APPR", "NE"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Unsers Herrn Geburt z\u00e4hlte, zur Zeit, wo der Klang", "tokens": ["Un\u00b7sers", "Herrn", "Ge\u00b7burt", "z\u00e4hl\u00b7te", ",", "zur", "Zeit", ",", "wo", "der", "Klang"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "$,", "APPRART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+---+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Geht der Sicheln ins Feld, da lag ich einsamen Schlummers", "tokens": ["Geht", "der", "Si\u00b7cheln", "ins", "Feld", ",", "da", "lag", "ich", "ein\u00b7sa\u00b7men", "Schlum\u00b7mers"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$,", "ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Fern in dem Lande, wo Jo klinget zugleich mit dem Ja.", "tokens": ["Fern", "in", "dem", "Lan\u00b7de", ",", "wo", "Jo", "klin\u00b7get", "zu\u00b7gleich", "mit", "dem", "Ja", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PWAV", "NE", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Da ward \u00f6fters der Fremdling besucht von Tr\u00e4umen von jenseits", "tokens": ["Da", "ward", "\u00f6f\u00b7ters", "der", "Fremd\u00b7ling", "be\u00b7sucht", "von", "Tr\u00e4u\u00b7men", "von", "jen\u00b7seits"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "VVPP", "APPR", "NN", "APPR", "NE"], "meter": "--+--+--+-+-+-+", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Her des Wassers, von dem, was \u00fcber Land, \u00fcber Meer", "tokens": ["Her", "des", "Was\u00b7sers", ",", "von", "dem", ",", "was", "\u00fc\u00b7ber", "Land", ",", "\u00fc\u00b7ber", "Meer"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "APPR", "ART", "$,", "PRELS", "APPR", "NN", "$,", "APPR", "NN"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Tr\u00e4gt der Ruf, und von dem, was Liebe innigst im Herzen", "tokens": ["Tr\u00e4gt", "der", "Ruf", ",", "und", "von", "dem", ",", "was", "Lie\u00b7be", "in\u00b7nigst", "im", "Her\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "KON", "APPR", "ART", "$,", "PWS", "NN", "ADV", "APPRART", "NN"], "meter": "+-+-+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Von den Geliebten so s\u00fc\u00df, s\u00fc\u00dfer vom Vaterland spinnt.", "tokens": ["Von", "den", "Ge\u00b7lieb\u00b7ten", "so", "s\u00fc\u00df", ",", "s\u00fc\u00b7\u00dfer", "vom", "Va\u00b7ter\u00b7land", "spinnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADJD", "$,", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Einst als der Mond mit dem lieblichen Strahl schon bleicher und tiefer", "tokens": ["Einst", "als", "der", "Mond", "mit", "dem", "lieb\u00b7li\u00b7chen", "Strahl", "schon", "blei\u00b7cher", "und", "tie\u00b7fer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "NN", "APPR", "ART", "ADJA", "NN", "ADV", "ADJD", "KON", "ADJD"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Funkelte, schon ein Stern hinter dem andern erlosch,", "tokens": ["Fun\u00b7kel\u00b7te", ",", "schon", "ein", "Stern", "hin\u00b7ter", "dem", "an\u00b7dern", "er\u00b7losch", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "ART", "NN", "APPR", "ART", "ADJA", "ADJD", "$,"], "meter": "+--+-+---+--+", "measure": "iambic.penta.invert"}, "line.11": {"text": "Schlug es mit rauschenden Fl\u00fcgeln fast hart ans Fenster und rief mir:", "tokens": ["Schlug", "es", "mit", "rau\u00b7schen\u00b7den", "Fl\u00fc\u00b7geln", "fast", "hart", "ans", "Fens\u00b7ter", "und", "rief", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "ADV", "ADJD", "APPRART", "NN", "KON", "VVFIN", "PPER", "$."], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.12": {"text": "\u00bbmache gleich auf, es ist kalt, auch ist der Falke nicht weit.\u00ab", "tokens": ["\u00bb", "ma\u00b7che", "gleich", "auf", ",", "es", "ist", "kalt", ",", "auch", "ist", "der", "Fal\u00b7ke", "nicht", "weit", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "ADV", "PTKVZ", "$,", "PPER", "VAFIN", "ADJD", "$,", "ADV", "VAFIN", "ART", "NN", "PTKNEG", "ADJD", "$.", "$("], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.13": {"text": "Ich aus dem Bett und \u00f6ffne das Fenster, da flattert ein V\u00f6glein", "tokens": ["Ich", "aus", "dem", "Bett", "und", "\u00f6ff\u00b7ne", "das", "Fens\u00b7ter", ",", "da", "flat\u00b7tert", "ein", "V\u00f6\u00b7glein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Schwirrend mir dicht in den Scho\u00df, zitternd und wimmernd und na\u00df.", "tokens": ["Schwir\u00b7rend", "mir", "dicht", "in", "den", "Scho\u00df", ",", "zit\u00b7ternd", "und", "wim\u00b7mernd", "und", "na\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "ADJD", "APPR", "ART", "NN", "$,", "VVPP", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.15": {"text": "Und ich beschaue das V\u00f6glein mir, da ist es die Lerche,", "tokens": ["Und", "ich", "be\u00b7schau\u00b7e", "das", "V\u00f6\u00b7glein", "mir", ",", "da", "ist", "es", "die", "Ler\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "PPER", "$,", "ADV", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Und ich denke bei mir: V\u00f6glein, wo flatterst du her?", "tokens": ["Und", "ich", "den\u00b7ke", "bei", "mir", ":", "V\u00f6\u00b7glein", ",", "wo", "flat\u00b7terst", "du", "her", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPER", "$.", "NN", "$,", "PWAV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Und es sieht so bedeutend mich an, halb menschlichen Blickes,", "tokens": ["Und", "es", "sieht", "so", "be\u00b7deu\u00b7tend", "mich", "an", ",", "halb", "menschli\u00b7chen", "Bli\u00b7ckes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJD", "PPER", "PTKVZ", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "--+--+--+-+-+-", "measure": "anapaest.tri.plus"}, "line.18": {"text": "Spricht dann:\u00bbKennst du mich nicht? Kennst du den Vater nicht mehr?", "tokens": ["Spricht", "dann", ":", "\u00bb", "Kennst", "du", "mich", "nicht", "?", "Kennst", "du", "den", "Va\u00b7ter", "nicht", "mehr", "?"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$.", "$(", "VVFIN", "PPER", "PRF", "PTKNEG", "$.", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "ADV", "$."], "meter": "------+--+--+", "measure": "iambic.tri.relaxed"}, "line.19": {"text": "Habe mich auch recht lieb und hege mich warm in dem Busen;", "tokens": ["Ha\u00b7be", "mich", "auch", "recht", "lieb", "und", "he\u00b7ge", "mich", "warm", "in", "dem", "Bu\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "ADJD", "KON", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.20": {"text": "Fern ist der Weg, den ich flog, fern, den ich fliegen noch mu\u00df.\u00ab", "tokens": ["Fern", "ist", "der", "Weg", ",", "den", "ich", "flog", ",", "fern", ",", "den", "ich", "flie\u00b7gen", "noch", "mu\u00df", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "ADJD", "$,", "PRELS", "PPER", "VVFIN", "ADV", "VMFIN", "$.", "$("], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.21": {"text": "Und es fiel mir aufs Herz, es schossen mir schwere Gedanken", "tokens": ["Und", "es", "fiel", "mir", "aufs", "Herz", ",", "es", "schos\u00b7sen", "mir", "schwe\u00b7re", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$,", "PPER", "VVFIN", "PPER", "ADJA", "NN"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.22": {"text": "Hin durch die Seele, und hei\u00df flo\u00df aus den Augen ein Strom.", "tokens": ["Hin", "durch", "die", "See\u00b7le", ",", "und", "hei\u00df", "flo\u00df", "aus", "den", "Au\u00b7gen", "ein", "Strom", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "KON", "ADJD", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.23": {"text": "Wimmerndes V\u00f6glein, du kamst ein Bote der Sehnsucht und Treue:", "tokens": ["Wim\u00b7mern\u00b7des", "V\u00f6\u00b7glein", ",", "du", "kamst", "ein", "Bo\u00b7te", "der", "Sehn\u00b7sucht", "und", "Treu\u00b7e", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.24": {"text": "Also findet der Geist Boten der Liebe dem Geist;", "tokens": ["Al\u00b7so", "fin\u00b7det", "der", "Geist", "Bo\u00b7ten", "der", "Lie\u00b7be", "dem", "Geist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.25": {"text": "Denn mein Vater verlie\u00df die irdische Heimat und gr\u00fc\u00dfte,", "tokens": ["Denn", "mein", "Va\u00b7ter", "ver\u00b7lie\u00df", "die", "ir\u00b7di\u00b7sche", "Hei\u00b7mat", "und", "gr\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "KON", "VVFIN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.26": {"text": "Wandelnd die himmlische Fahrt, noch den Entfernten durch dich.", "tokens": ["Wan\u00b7delnd", "die", "himm\u00b7li\u00b7sche", "Fahrt", ",", "noch", "den", "Ent\u00b7fern\u00b7ten", "durch", "dich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$,", "ADV", "ART", "NN", "APPR", "PPER", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.27": {"text": "O er war dir ja gleich an fr\u00f6hlichen Liedern und Freuden,", "tokens": ["O", "er", "war", "dir", "ja", "gleich", "an", "fr\u00f6h\u00b7li\u00b7chen", "Lie\u00b7dern", "und", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VAFIN", "PPER", "ADV", "ADV", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.28": {"text": "Liebte das gl\u00fcckliche Land, liebte die Felder wie du.", "tokens": ["Lieb\u00b7te", "das", "gl\u00fcck\u00b7li\u00b7che", "Land", ",", "lieb\u00b7te", "die", "Fel\u00b7der", "wie", "du", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "KOKOM", "PPER", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.29": {"text": "Heil dir, Herold des Himmels, und Heil dem frommen Geschlechte,", "tokens": ["Heil", "dir", ",", "He\u00b7rold", "des", "Him\u00b7mels", ",", "und", "Heil", "dem", "from\u00b7men", "Ge\u00b7schlech\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "ART", "NN", "$,", "KON", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.30": {"text": "Das in den Furchen sein Nest baut und die Wiesen bewohnt!", "tokens": ["Das", "in", "den", "Fur\u00b7chen", "sein", "Nest", "baut", "und", "die", "Wie\u00b7sen", "be\u00b7wohnt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN", "KON", "ART", "NN", "VVPP", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.31": {"text": "Friede soll ewig bestehn den sp\u00e4testen Enkeln von beiden,", "tokens": ["Frie\u00b7de", "soll", "e\u00b7wig", "be\u00b7stehn", "den", "sp\u00e4\u00b7tes\u00b7ten", "En\u00b7keln", "von", "bei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADJD", "VVFIN", "ART", "ADJA", "NN", "APPR", "PIAT", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.32": {"text": "Und es erlahme die Hand meines Geschlechts, die den Tod", "tokens": ["Und", "es", "er\u00b7lah\u00b7me", "die", "Hand", "mei\u00b7nes", "Ge\u00b7schlechts", ",", "die", "den", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.33": {"text": "Schickt auf die Deinen mit Blei und Schlingen stellet und Netze,", "tokens": ["Schickt", "auf", "die", "Dei\u00b7nen", "mit", "Blei", "und", "Schlin\u00b7gen", "stel\u00b7let", "und", "Net\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "PPOSAT", "APPR", "NN", "KON", "NN", "VVFIN", "KON", "NN", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.34": {"text": "Oder mit diebischer Lust f\u00e4hrt auf die piepende Brut!", "tokens": ["O\u00b7der", "mit", "die\u00b7bi\u00b7scher", "Lust", "f\u00e4hrt", "auf", "die", "pie\u00b7pen\u00b7de", "Brut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}}}}}