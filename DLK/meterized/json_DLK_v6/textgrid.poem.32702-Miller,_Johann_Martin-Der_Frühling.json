{"textgrid.poem.32702": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Der Fr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O seht, die liebe Sonne lacht;", "tokens": ["O", "seht", ",", "die", "lie\u00b7be", "Son\u00b7ne", "lacht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Wiese kleidet sich in Pracht;", "tokens": ["Die", "Wie\u00b7se", "klei\u00b7det", "sich", "in", "Pracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zerronnen ist der Winterschnee;", "tokens": ["Zer\u00b7ron\u00b7nen", "ist", "der", "Win\u00b7ter\u00b7schnee", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Blumen dringen aus dem Klee!", "tokens": ["Und", "Blu\u00b7men", "drin\u00b7gen", "aus", "dem", "Klee", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf blaue Veilchen sammlen sich", "tokens": ["Auf", "blau\u00b7e", "Veil\u00b7chen", "samm\u00b7len", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die kleinen Bienen emsiglich;", "tokens": ["Die", "klei\u00b7nen", "Bie\u00b7nen", "em\u00b7sig\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der bunte Buttervogel freut", "tokens": ["Der", "bun\u00b7te", "But\u00b7ter\u00b7vo\u00b7gel", "freut"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich \u00fcber sein bemaltes Kleid.", "tokens": ["Sich", "\u00fc\u00b7ber", "sein", "be\u00b7mal\u00b7tes", "Kleid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Lerche schwingt sich hoch empor;", "tokens": ["Die", "Ler\u00b7che", "schwingt", "sich", "hoch", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Hain erschallt der V\u00f6gel Chor;", "tokens": ["Im", "Hain", "er\u00b7schallt", "der", "V\u00f6\u00b7gel", "Chor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vor allen aber t\u00f6nt der Schall", "tokens": ["Vor", "al\u00b7len", "a\u00b7ber", "t\u00f6nt", "der", "Schall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der lieben, kleinen Nachtigall.", "tokens": ["Der", "lie\u00b7ben", ",", "klei\u00b7nen", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Von dir, o Liebe! schallt ihr Lied,", "tokens": ["Von", "dir", ",", "o", "Lie\u00b7be", "!", "schallt", "ihr", "Lied", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "FM", "NN", "$.", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und das geliebte Weibchen flieht", "tokens": ["Und", "das", "ge\u00b7lieb\u00b7te", "Weib\u00b7chen", "flieht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum M\u00e4nnchen hin, und inniglich", "tokens": ["Zum", "M\u00e4nn\u00b7chen", "hin", ",", "und", "in\u00b7nig\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schmiegt sie an seine Seite sich.", "tokens": ["Schmiegt", "sie", "an", "sei\u00b7ne", "Sei\u00b7te", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "PRF", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "O h\u00e4tt' ich, liebe Nachtigall,", "tokens": ["O", "h\u00e4tt'", "ich", ",", "lie\u00b7be", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "$,", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie du so reinen, s\u00fc\u00dfen Schall!", "tokens": ["Wie", "du", "so", "rei\u00b7nen", ",", "s\u00fc\u00b7\u00dfen", "Schall", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann k\u00e4m' mein R\u00f6schen auch zu mir,", "tokens": ["Dann", "k\u00e4m'", "mein", "R\u00f6\u00b7schen", "auch", "zu", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und freuen k\u00f6nnt' ich mich mit dir!", "tokens": ["Und", "freu\u00b7en", "k\u00f6nnt'", "ich", "mich", "mit", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PRF", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}