{"textgrid.poem.65329": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Das Verh\u00f6r", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Thu' auf die Th\u00fcr, du holde Maid, thu' auf und la\u00df mich ein! \u2013", "tokens": ["Thu'", "auf", "die", "Th\u00fcr", ",", "du", "hol\u00b7de", "Maid", ",", "thu'", "auf", "und", "la\u00df", "mich", "ein", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,", "PPER", "ADJA", "NN", "$,", "VVFIN", "PTKVZ", "KON", "VVIMP", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Wer klopft, wer ruft in stiller Nacht? Ein T\u00fcrke wird es sein. \u2013", "tokens": ["Wer", "klopft", ",", "wer", "ruft", "in", "stil\u00b7ler", "Nacht", "?", "Ein", "T\u00fcr\u00b7ke", "wird", "es", "sein", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWS", "VVFIN", "APPR", "ADJA", "NN", "$.", "ART", "NN", "VAFIN", "PPER", "VAINF", "$.", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Es ist kein T\u00fcrk', es ist ein Christ, es ist ein guter Christ,", "tokens": ["Es", "ist", "kein", "T\u00fcrk'", ",", "es", "ist", "ein", "Christ", ",", "es", "ist", "ein", "gu\u00b7ter", "Christ", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Der deinen purpurrothen Mund viel tausendmal gek\u00fc\u00dft. \u2013", "tokens": ["Der", "dei\u00b7nen", "pur\u00b7pur\u00b7ro\u00b7then", "Mund", "viel", "tau\u00b7send\u00b7mal", "ge\u00b7k\u00fc\u00dft", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "ADV", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Ich sehe dich im Dunkel nicht, so sag' ein Zeichen mir", "tokens": ["Ich", "se\u00b7he", "dich", "im", "Dun\u00b7kel", "nicht", ",", "so", "sag'", "ein", "Zei\u00b7chen", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "PTKNEG", "$,", "ADV", "VVFIN", "ART", "NN", "PPER"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Von Hof und Haus und K\u00e4mmerlein, damit ich traue dir. \u2013", "tokens": ["Von", "Hof", "und", "Haus", "und", "K\u00e4m\u00b7mer\u00b7lein", ",", "da\u00b7mit", "ich", "trau\u00b7e", "dir", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "KON", "NN", "KON", "NN", "$,", "KOUS", "PPER", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "Im Hofe springt ein Silberquell, und wie der Wind auch weht,", "tokens": ["Im", "Ho\u00b7fe", "springt", "ein", "Sil\u00b7ber\u00b7quell", ",", "und", "wie", "der", "Wind", "auch", "weht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,", "KON", "PWAV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Er springt nach deinem Fenster nur, wenn eines offen steht.", "tokens": ["Er", "springt", "nach", "dei\u00b7nem", "Fens\u00b7ter", "nur", ",", "wenn", "ei\u00b7nes", "of\u00b7fen", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ADV", "$,", "KOUS", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.9": {"text": "Am Hause rankt die Rebe sich hinauf von Stein zu Stein,", "tokens": ["Am", "Hau\u00b7se", "rankt", "die", "Re\u00b7be", "sich", "hin\u00b7auf", "von", "Stein", "zu", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "PRF", "ADV", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "Bis mit den nassen Augen sie kann sehn zu dir hinein.", "tokens": ["Bis", "mit", "den", "nas\u00b7sen", "Au\u00b7gen", "sie", "kann", "sehn", "zu", "dir", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "PPER", "VMFIN", "VVINF", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.11": {"text": "Du trocknest ihre Thr\u00e4nen ab, sie brechen auf zumal,", "tokens": ["Du", "trock\u00b7nest", "ih\u00b7re", "Thr\u00e4\u00b7nen", "ab", ",", "sie", "bre\u00b7chen", "auf", "zu\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,", "PPER", "VVFIN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.12": {"text": "Und goldne Nektartrauben gl\u00fchn in deiner Sterne Strahl.", "tokens": ["Und", "gold\u00b7ne", "Nekt\u00b7ar\u00b7trau\u00b7ben", "gl\u00fchn", "in", "dei\u00b7ner", "Ster\u00b7ne", "Strahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.13": {"text": "In deiner Kammer an der Wand ist ein verh\u00e4ngter Schrein,", "tokens": ["In", "dei\u00b7ner", "Kam\u00b7mer", "an", "der", "Wand", "ist", "ein", "ver\u00b7h\u00e4ng\u00b7ter", "Schrein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.14": {"text": "Es blickt kein Mond, es blinkt kein Stern, kein L\u00e4mpchen flimmt hinein:", "tokens": ["Es", "blickt", "kein", "Mond", ",", "es", "blinkt", "kein", "Stern", ",", "kein", "L\u00e4mp\u00b7chen", "flimmt", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "PPER", "VVFIN", "PIAT", "NN", "$,", "PIAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.15": {"text": "Darinnen liegt die Lilie auf einem Rosenbeet \u2013", "tokens": ["Da\u00b7rin\u00b7nen", "liegt", "die", "Li\u00b7lie", "auf", "ei\u00b7nem", "Ro\u00b7sen\u00b7beet", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Ich komme schon, ich \u00f6ffne schon! Herein, wer drau\u00dfen steht!", "tokens": ["Ich", "kom\u00b7me", "schon", ",", "ich", "\u00f6ff\u00b7ne", "schon", "!", "Her\u00b7ein", ",", "wer", "drau\u00b7\u00dfen", "steht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "ADV", "$.", "PTKVZ", "$,", "PWS", "ADV", "VVFIN", "$."], "meter": "-+-+-+--++-+-+", "measure": "iambic.septa.relaxed"}}}}}