{"textgrid.poem.62830": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie hei\u00dft das sechste der Gebote?", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie hei\u00dft das sechste der Gebote?", "tokens": ["Wie", "hei\u00dft", "das", "sechs\u00b7te", "der", "Ge\u00b7bo\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PDS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So fragte j\u00fcngst beym Kirchenunterricht", "tokens": ["So", "frag\u00b7te", "j\u00fcngst", "beym", "Kir\u00b7chen\u00b7un\u00b7ter\u00b7richt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ignaz, der finstre Dorfzelote,", "tokens": ["Ig\u00b7naz", ",", "der", "finst\u00b7re", "Dorf\u00b7ze\u00b7lo\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein kleines artiges Gesicht.", "tokens": ["Ein", "klei\u00b7nes", "ar\u00b7ti\u00b7ges", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Antwort war: Ihr sollt nicht ehebrechen.", "tokens": ["Die", "Ant\u00b7wort", "war", ":", "Ihr", "sollt", "nicht", "e\u00b7heb\u00b7re\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$.", "PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ey, rief Ignaz, wer wird so albern sprechen?", "tokens": ["Ey", ",", "rief", "Ig\u00b7naz", ",", "wer", "wird", "so", "al\u00b7bern", "spre\u00b7chen", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "NE", "$,", "PWS", "VAFIN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Es hei\u00dft: Du sollst nicht ehebrechen.", "tokens": ["Es", "hei\u00dft", ":", "Du", "sollst", "nicht", "e\u00b7heb\u00b7re\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das arme kleine M\u00e4dchen warf", "tokens": ["Das", "ar\u00b7me", "klei\u00b7ne", "M\u00e4d\u00b7chen", "warf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die Augen auf den Katecheten:", "tokens": ["Die", "Au\u00b7gen", "auf", "den", "Ka\u00b7te\u00b7che\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ich wu\u00dfte nicht, versetzt es mit Err\u00f6then,", "tokens": ["Ich", "wu\u00df\u00b7te", "nicht", ",", "ver\u00b7setzt", "es", "mit", "Er\u00b7r\u00f6\u00b7then", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Da\u00df man den Pfarrer duzen darf.", "tokens": ["Da\u00df", "man", "den", "Pfar\u00b7rer", "du\u00b7zen", "darf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}