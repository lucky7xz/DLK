{"dta.poem.19441": {"metadata": {"author": {"name": "Stolberg-Stolberg, Friedrich Leopold zu", "birth": "N.A.", "death": "N.A."}, "title": "Der Gesang.  \n An Sch\u00f6nborn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1779", "urn": "urn:nbn:de:kobv:b4-20589-5", "language": ["de:0.99"], "booktitle": "Stolberg-Stolberg, Christian zu; Stolberg-Stolberg, Friedrich Leopold zu: Gedichte. Leipzig, 1779."}, "poem": {"stanza.1": {"line.1": {"text": "Wie dem erwachenden J\u00fcnglinge schnell im", "tokens": ["Wie", "dem", "er\u00b7wa\u00b7chen\u00b7den", "J\u00fcng\u00b7lin\u00b7ge", "schnell", "im"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADJD", "APPRART"], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "br\u00e4utlichen Bette", "tokens": ["br\u00e4ut\u00b7li\u00b7chen", "Bet\u00b7te"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Seine gaukelnden Tr\u00e4um\u2019 auf nichtigen Fl\u00fcgeln", "tokens": ["Sei\u00b7ne", "gau\u00b7keln\u00b7den", "Tr\u00e4um'", "auf", "nich\u00b7ti\u00b7gen", "Fl\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "entschwinden;", "tokens": ["ent\u00b7schwin\u00b7den", ";"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Sonst umirrten sie, langsam schwebend, weilend", "tokens": ["Sonst", "um\u00b7irr\u00b7ten", "sie", ",", "lang\u00b7sam", "schwe\u00b7bend", ",", "wei\u00b7lend"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "ADJD", "$,", "VVPP"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "im Fluge,", "tokens": ["im", "Flu\u00b7ge", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Noch sein Haupt, wenn schon der Rosenwangi-", "tokens": ["Noch", "sein", "Haupt", ",", "wenn", "schon", "der", "Ro\u00b7sen\u00b7wan\u00b7gi"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "$,", "KOUS", "ADV", "ART", "TRUNC"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "gen Stunde,", "tokens": ["gen", "Stun\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Und dem erbleichenden Stern der Liebe sein Auge", "tokens": ["Und", "dem", "er\u00b7blei\u00b7chen\u00b7den", "Stern", "der", "Lie\u00b7be", "sein", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "sich aufschlo\u00df;", "tokens": ["sich", "auf\u00b7schlo\u00df", ";"], "token_info": ["word", "word", "punct"], "pos": ["PRF", "VVFIN", "$."], "meter": "--+", "measure": "anapaest.init"}, "line.11": {"text": "Nun verschwinden sie schnell; denn neben sich", "tokens": ["Nun", "ver\u00b7schwin\u00b7den", "sie", "schnell", ";", "denn", "ne\u00b7ben", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$.", "KON", "APPR", "PRF"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.12": {"text": "sieht der Begl\u00fcckte,", "tokens": ["sieht", "der", "Be\u00b7gl\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Sein sanftathmendes Weib, in schlummernden", "tokens": ["Sein", "sanf\u00b7tath\u00b7men\u00b7des", "Weib", ",", "in", "schlum\u00b7mern\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "APPR", "ADJA"], "meter": "--+--+-+--", "measure": "anapaest.di.plus"}, "line.14": {"text": "Reizen der Jugend,", "tokens": ["Rei\u00b7zen", "der", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Lieblich wie den thauenden Abend im blumigen", "tokens": ["Lieb\u00b7lich", "wie", "den", "thau\u00b7en\u00b7den", "A\u00b7bend", "im", "blu\u00b7mi\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "+-+-+--+--+--", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "Thale.", "tokens": ["Tha\u00b7le", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Ach! sie erwacht! und \u00f6fnet Liebeschmachtende", "tokens": ["Ach", "!", "sie", "er\u00b7wacht", "!", "und", "\u00f6f\u00b7net", "Lie\u00b7be\u00b7schmach\u00b7ten\u00b7de"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "PPER", "VVPP", "$.", "KON", "VVFIN", "NN"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.18": {"text": "Augen,", "tokens": ["Au\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "Wonnetrunken begr\u00fc\u00dft sie der Blick des feutigen", "tokens": ["Won\u00b7ne\u00b7trun\u00b7ken", "be\u00b7gr\u00fc\u00dft", "sie", "der", "Blick", "des", "feu\u00b7ti\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "ART", "NN"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.20": {"text": "J\u00fcnglings,", "tokens": ["J\u00fcng\u00b7lings", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.21": {"text": "Wie den err\u00f6thenden Mond die flammende Son-", "tokens": ["Wie", "den", "er\u00b7r\u00f6\u00b7then\u00b7den", "Mond", "die", "flam\u00b7men\u00b7de", "Son"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "ART", "ADJA", "TRUNC"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "ne begr\u00fcsset!", "tokens": ["ne", "be\u00b7gr\u00fcs\u00b7set", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "--+-", "measure": "anapaest.init"}, "line.23": {"text": "Wie dem J\u00fcnglinge dann die Traumgestalten", "tokens": ["Wie", "dem", "J\u00fcng\u00b7lin\u00b7ge", "dann", "die", "Traum\u00b7ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADV", "ART", "NN"], "meter": "+-++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.24": {"text": "entflattern,", "tokens": ["ent\u00b7flat\u00b7tern", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.25": {"text": "So enteilen auch mir die bunten Tr\u00e4ume des", "tokens": ["So", "ent\u00b7ei\u00b7len", "auch", "mir", "die", "bun\u00b7ten", "Tr\u00e4u\u00b7me", "des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "ART", "ADJA", "NN", "ART"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.26": {"text": "Tages,", "tokens": ["Ta\u00b7ges", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.27": {"text": "Und wie Zephyr der hangenden Spinne Gewebe", "tokens": ["Und", "wie", "Ze\u00b7phyr", "der", "han\u00b7gen\u00b7den", "Spin\u00b7ne", "Ge\u00b7we\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "NN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "zerwehet,", "tokens": ["zer\u00b7we\u00b7het", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.29": {"text": "So entschwindet auch mir das Gewebe gesch\u00e4fti-", "tokens": ["So", "ent\u00b7schwin\u00b7det", "auch", "mir", "das", "Ge\u00b7we\u00b7be", "ge\u00b7sch\u00e4f\u00b7ti"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "ART", "NN", "TRUNC"], "meter": "--+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "ger Stunden,", "tokens": ["ger", "Stun\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.31": {"text": "Wenn der Entz\u00fcckung Sohn, der Gesang, in", "tokens": ["Wenn", "der", "Ent\u00b7z\u00fc\u00b7ckung", "Sohn", ",", "der", "Ge\u00b7sang", ",", "in"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KOUS", "ART", "NN", "NN", "$,", "ART", "NN", "$,", "APPR"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.32": {"text": "goldenen Locken,", "tokens": ["gol\u00b7de\u00b7nen", "Lo\u00b7cken", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.33": {"text": "T\u00f6nend, von Harmonien ums\u00e4uselt, melodisch", "tokens": ["T\u00f6\u00b7nend", ",", "von", "Har\u00b7mo\u00b7ni\u00b7en", "um\u00b7s\u00e4u\u00b7selt", ",", "me\u00b7lo\u00b7disch"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "APPR", "NE", "VVFIN", "$,", "ADJD"], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.34": {"text": "einherschwebt!", "tokens": ["ein\u00b7her\u00b7schwebt", "!"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}