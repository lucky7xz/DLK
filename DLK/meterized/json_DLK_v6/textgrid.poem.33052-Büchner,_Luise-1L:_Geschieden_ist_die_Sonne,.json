{"textgrid.poem.33052": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Geschieden ist die Sonne,", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geschieden ist die Sonne,", "tokens": ["Ge\u00b7schie\u00b7den", "ist", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kein Bl\u00fcmlein mehr mag bl\u00fch'n,", "tokens": ["Kein", "Bl\u00fcm\u00b7lein", "mehr", "mag", "bl\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und nur des Epheus Bl\u00e4tter", "tokens": ["Und", "nur", "des", "E\u00b7pheus", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schm\u00fcckt noch ein sanftes Gr\u00fcn.", "tokens": ["Schm\u00fcckt", "noch", "ein", "sanf\u00b7tes", "Gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und freudig uns're Seele", "tokens": ["Und", "freu\u00b7dig", "un\u00b7s'\u00b7re", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Darauf die Hoffnung baut,", "tokens": ["Da\u00b7rauf", "die", "Hoff\u00b7nung", "baut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df es nach \u00f6dem Winter", "tokens": ["Da\u00df", "es", "nach", "\u00f6\u00b7dem", "Win\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Fr\u00fchling wieder schaut. \u2013", "tokens": ["Den", "Fr\u00fch\u00b7ling", "wie\u00b7der", "schaut", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So wird der bangen Seele", "tokens": ["So", "wird", "der", "ban\u00b7gen", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die tiefer Schmerz erf\u00fcllt,", "tokens": ["Die", "tie\u00b7fer", "Schmerz", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Lebensgr\u00fcn der Hoffnung", "tokens": ["Im", "Le\u00b7bens\u00b7gr\u00fcn", "der", "Hoff\u00b7nung"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein neuer Trost enth\u00fcllt.", "tokens": ["Ein", "neu\u00b7er", "Trost", "ent\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ein Fr\u00fchling lacht ihr wieder,", "tokens": ["Ein", "Fr\u00fch\u00b7ling", "lacht", "ihr", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Blumen pfl\u00fcckt die Hand,", "tokens": ["Und", "Blu\u00b7men", "pfl\u00fcckt", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "F\u00e4llt manche Wehmuthsthr\u00e4ne", "tokens": ["F\u00e4llt", "man\u00b7che", "Weh\u00b7muthst\u00b7hr\u00e4\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auch auf des Kelches Rand.", "tokens": ["Auch", "auf", "des", "Kel\u00b7ches", "Rand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und wie der Epheu innig", "tokens": ["Und", "wie", "der", "E\u00b7pheu", "in\u00b7nig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich Rank' an Ranke schmiegt,", "tokens": ["Sich", "Rank'", "an", "Ran\u00b7ke", "schmiegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So wird die Seele stiller", "tokens": ["So", "wird", "die", "See\u00b7le", "stil\u00b7ler"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An Freundes Herz gewiegt.", "tokens": ["An", "Freun\u00b7des", "Herz", "ge\u00b7wiegt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}