{"textgrid.poem.35206": {"metadata": {"author": {"name": "Harsd\u00f6rffer, Georg Philipp", "birth": "N.A.", "death": "N.A."}, "title": "[was unser Muht vermocht/ wei\u00df alle Welt zu melden]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was unser Muht vermocht/ wei\u00df alle Welt zu melden/", "tokens": ["Was", "un\u00b7ser", "Muht", "ver\u00b7mocht", "/", "wei\u00df", "al\u00b7le", "Welt", "zu", "mel\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVPP", "$(", "VVFIN", "PIAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Man frag' Antenors Stadt", "tokens": ["Man", "frag'", "An\u00b7te\u00b7nors", "Stadt"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "NE", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wir d\u00e4mpften erstes Dampfs den Teutschen Kriegesbrand.", "tokens": ["Wir", "d\u00e4mpf\u00b7ten", "ers\u00b7tes", "Dampfs", "den", "Teut\u00b7schen", "Krie\u00b7ges\u00b7brand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bald grub uns Vngarn ein/ das Grab beruchter Helden.", "tokens": ["Bald", "grub", "uns", "Vn\u00b7garn", "ein", "/", "das", "Grab", "be\u00b7ruch\u00b7ter", "Hel\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "ART", "$(", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich war ein k\u00fcner Held/ dem mancher Held zuwider/", "tokens": ["Ich", "war", "ein", "k\u00fc\u00b7ner", "Held", "/", "dem", "man\u00b7cher", "Held", "zu\u00b7wi\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$(", "ART", "PIAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch legte meinen Ruhm und mich das Beuten nieder.", "tokens": ["Doch", "leg\u00b7te", "mei\u00b7nen", "Ruhm", "und", "mich", "das", "Beu\u00b7ten", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "KON", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich hab den Feind mit Krieg/ und sonder Geld/ gefehrt/", "tokens": ["Ich", "hab", "den", "Feind", "mit", "Krieg", "/", "und", "son\u00b7der", "Geld", "/", "ge\u00b7fehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN", "$(", "KON", "ADJA", "NN", "$(", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die That erwehrtet Lob/ wann Kriegen Krieger nehrt.", "tokens": ["Die", "That", "er\u00b7wehr\u00b7tet", "Lob", "/", "wann", "Krie\u00b7gen", "Krie\u00b7ger", "nehrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$(", "PWAV", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mein Sinn sann \u00fcberweit/ das hat mein Tod verk\u00fcrtzt/", "tokens": ["Mein", "Sinn", "sann", "\u00fc\u00b7ber\u00b7weit", "/", "das", "hat", "mein", "Tod", "ver\u00b7k\u00fcrtzt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$(", "PDS", "VAFIN", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Tod hat dessen Glut ins k\u00fcle Grab gest\u00fcrtzt:", "tokens": ["Mein", "Tod", "hat", "des\u00b7sen", "Glut", "ins", "k\u00fc\u00b7le", "Grab", "ge\u00b7st\u00fcrtzt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PDS", "NN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Noch glimmt der hohe Raht/ man wird sein nicht vergessen/", "tokens": ["Noch", "glimmt", "der", "ho\u00b7he", "Raht", "/", "man", "wird", "sein", "nicht", "ver\u00b7ges\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$(", "PIS", "VAFIN", "PPOSAT", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es bl\u00fchen Lorbeer auf nechst meinen Grab Zypressen.", "tokens": ["Es", "bl\u00fc\u00b7hen", "Lor\u00b7beer", "auf", "nechst", "mei\u00b7nen", "Grab", "Zyp\u00b7res\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "ADV", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Gleichwie der Ph\u00f6nix wird von Flammen aufgefressen/", "tokens": ["Gleich\u00b7wie", "der", "Ph\u00f6\u00b7nix", "wird", "von", "Flam\u00b7men", "auf\u00b7ge\u00b7fres\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd einer seiner Art kreucht aus der Asch hervor:", "tokens": ["Vnd", "ei\u00b7ner", "sei\u00b7ner", "Art", "kreucht", "aus", "der", "Asch", "her\u00b7vor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So stieg ich auf dem Grab/ als Moritz fiel/ empor.", "tokens": ["So", "stieg", "ich", "auf", "dem", "Grab", "/", "als", "Mo\u00b7ritz", "fiel", "/", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$(", "KOUS", "NE", "VVFIN", "$(", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vns Beyde r\u00fcmt die Welt/ wir bleiben unvergessen.", "tokens": ["Vns", "Bey\u00b7de", "r\u00fcmt", "die", "Welt", "/", "wir", "blei\u00b7ben", "un\u00b7ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "ART", "NN", "$(", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wer Krieges-Zepter f\u00fcrt/ der lerne von mir Siegen/", "tokens": ["Wer", "Krie\u00b7ge\u00b7sZep\u00b7ter", "f\u00fcrt", "/", "der", "ler\u00b7ne", "von", "mir", "Sie\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "$(", "ART", "VVFIN", "APPR", "PPER", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Krieg war bald geendt/ doch sonder Siegers-zwang.", "tokens": ["Mein", "Krieg", "war", "bald", "ge\u00b7endt", "/", "doch", "son\u00b7der", "Sie\u00b7ger\u00b7szwang", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$(", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer friedlich siegen will/ der kriege nicht zu lang:", "tokens": ["Wer", "fried\u00b7lich", "sie\u00b7gen", "will", "/", "der", "krie\u00b7ge", "nicht", "zu", "lang", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVINF", "VMFIN", "$(", "ART", "NN", "PTKNEG", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der l\u00e4ngste Sieges-Ruhm ist allerk\u00fcrtztes Kriegen.", "tokens": ["Der", "l\u00e4ngs\u00b7te", "Sie\u00b7ges\u00b7Ruhm", "ist", "al\u00b7ler\u00b7k\u00fcrtz\u00b7tes", "Krie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Man sagt/ ich trage Schuld/ da\u00df man noch Waffen tr\u00e4gt/", "tokens": ["Man", "sagt", "/", "ich", "tra\u00b7ge", "Schuld", "/", "da\u00df", "man", "noch", "Waf\u00b7fen", "tr\u00e4gt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$(", "PPER", "VVFIN", "NN", "$(", "KOUS", "PIS", "ADV", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ich der erste nahm. Nein/ Landgemeine S\u00fcnden/", "tokens": ["Die", "ich", "der", "ers\u00b7te", "nahm", ".", "Nein", "/", "Land\u00b7ge\u00b7mei\u00b7ne", "S\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "VVFIN", "$.", "PTKANT", "$(", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach! S\u00fcnden konden so den Jammerkrieg anz\u00fcnden.", "tokens": ["Ach", "!", "S\u00fcn\u00b7den", "kon\u00b7den", "so", "den", "Jam\u00b7mer\u00b7krieg", "an\u00b7z\u00fcn\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NN", "VMFIN", "ADV", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein jeder/ was er mich beschuldet/ bey sich hegt.", "tokens": ["Ein", "je\u00b7der", "/", "was", "er", "mich", "be\u00b7schul\u00b7det", "/", "bey", "sich", "hegt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "$(", "PWS", "PPER", "PRF", "VVPP", "$(", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Den Feinden war ich st\u00e4ts ein scharfer Stachel-Dorn/", "tokens": ["Den", "Fein\u00b7den", "war", "ich", "st\u00e4ts", "ein", "schar\u00b7fer", "Sta\u00b7chel\u00b7Dorn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Pfaltz und Niederland bez\u00e4rt noch meinen Zorn/", "tokens": ["Die", "Pfaltz", "und", "Nie\u00b7der\u00b7land", "be\u00b7z\u00e4rt", "noch", "mei\u00b7nen", "Zorn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PIS", "VVFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Mantuaner auch. H\u00e4lf Dapferkeit f\u00fcr Sterben/", "tokens": ["Der", "Man\u00b7tu\u00b7a\u00b7ner", "auch", ".", "H\u00e4lf", "Dap\u00b7fer\u00b7keit", "f\u00fcr", "Ster\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$.", "NN", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Man l\u00e4se dieses nicht: Mein Ruhm wird nie verderben.", "tokens": ["Man", "l\u00e4\u00b7se", "die\u00b7ses", "nicht", ":", "Mein", "Ruhm", "wird", "nie", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PDS", "PTKNEG", "$.", "PPOSAT", "NN", "VAFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Ich war mit Raht und That ein klugbehertzter Held/", "tokens": ["Ich", "war", "mit", "Raht", "und", "That", "ein", "klug\u00b7be\u00b7hertz\u00b7ter", "Held", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der sonsten eine Red nie zweymahl vorgestellt.", "tokens": ["Der", "sons\u00b7ten", "ei\u00b7ne", "Red", "nie", "zwey\u00b7mahl", "vor\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die mir vertraute Magd hab ich mit Macht geschutzet/", "tokens": ["Die", "mir", "ver\u00b7trau\u00b7te", "Magd", "hab", "ich", "mit", "Macht", "ge\u00b7schut\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd bi\u00df in meinenTod des Feindes Trutz getrutzet.", "tokens": ["Vnd", "bi\u00df", "in", "mei\u00b7nen", "Tod", "des", "Fein\u00b7des", "Trutz", "ge\u00b7trut\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Mein Ruhm sagt/ wer ich bin: Ein alter Kriegesmann/", "tokens": ["Mein", "Ruhm", "sagt", "/", "wer", "ich", "bin", ":", "Ein", "al\u00b7ter", "Krie\u00b7ges\u00b7mann", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$(", "PWS", "PPER", "VAFIN", "$.", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Trunk noch Lieb bet\u00f6rt/ Doch nahm ich Krantz und Ehre", "tokens": ["Den", "Trunk", "noch", "Lieb", "be\u00b7t\u00f6rt", "/", "Doch", "nahm", "ich", "Krantz", "und", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "NN", "VVFIN", "$(", "KON", "VVFIN", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der grossen Burge-Magd/ von der ein Blutbad rann:", "tokens": ["Der", "gros\u00b7sen", "Bur\u00b7ge\u00b7Magd", "/", "von", "der", "ein", "Blut\u00b7bad", "rann", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "APPR", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es mordet auch/ der ficht/ Mich kr\u00e4nkt es ja so sehre.", "tokens": ["Es", "mor\u00b7det", "auch", "/", "der", "ficht", "/", "Mich", "kr\u00e4nkt", "es", "ja", "so", "seh\u00b7re", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "ART", "NN", "$(", "PPER", "VVFIN", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Ich fiel/ wann fallen kan/ der st\u00e4ts den Feind bestanden/", "tokens": ["Ich", "fiel", "/", "wann", "fal\u00b7len", "kan", "/", "der", "st\u00e4ts", "den", "Feind", "be\u00b7stan\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWAV", "VVINF", "VMFIN", "$(", "ART", "VVFIN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch hab ich eine Seul im Fallen aufgestellt/", "tokens": ["Doch", "hab", "ich", "ei\u00b7ne", "Seul", "im", "Fal\u00b7len", "auf\u00b7ge\u00b7stellt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Seule meines Siegs. So steht in Teutschen Landen", "tokens": ["Die", "Seu\u00b7le", "mei\u00b7nes", "Siegs", ".", "So", "steht", "in", "Teut\u00b7schen", "Lan\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "$.", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein Wunder-Helden Nahm/ der nimmer nimmer f\u00e4llt.", "tokens": ["Mein", "Wun\u0