{"textgrid.poem.56079": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nein: ein Turm soll sein aus meinem Herzen", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nein: ein Turm soll sein aus meinem Herzen", "tokens": ["Nein", ":", "ein", "Turm", "soll", "sein", "aus", "mei\u00b7nem", "Her\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "ART", "NN", "VMFIN", "PPOSAT", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "und ich selbst an seinen Rand gestellt:", "tokens": ["und", "ich", "selbst", "an", "sei\u00b7nen", "Rand", "ge\u00b7stellt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "wo sonst nichts mehr ist, noch einmal Schmerzen", "tokens": ["wo", "sonst", "nichts", "mehr", "ist", ",", "noch", "ein\u00b7mal", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADV", "PIS", "ADV", "VAFIN", "$,", "ADV", "ADV", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "und Uns\u00e4glichkeit, noch einmal Welt.", "tokens": ["und", "Un\u00b7s\u00e4g\u00b7lich\u00b7keit", ",", "noch", "ein\u00b7mal", "Welt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADV", "ADV", "NN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Noch ein Ding allein im \u00dcbergro\u00dfen,", "tokens": ["Noch", "ein", "Ding", "al\u00b7lein", "im", "\u00dc\u00b7ber\u00b7gro\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "welches dunkel wird und wieder licht,", "tokens": ["wel\u00b7ches", "dun\u00b7kel", "wird", "und", "wie\u00b7der", "licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "KON", "ADV", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "noch ein letztes, sehnendes Gesicht", "tokens": ["noch", "ein", "letz\u00b7tes", ",", "seh\u00b7nen\u00b7des", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "in das Nie-zu-Stillende versto\u00dfen,", "tokens": ["in", "das", "Nie\u00b7zu\u00b7Stil\u00b7len\u00b7de", "ver\u00b7sto\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "noch ein \u00e4u\u00dferstes Gesicht aus Stein,", "tokens": ["noch", "ein", "\u00e4u\u00b7\u00dfers\u00b7tes", "Ge\u00b7sicht", "aus", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "willig seinen inneren Gewichten,", "tokens": ["wil\u00b7lig", "sei\u00b7nen", "in\u00b7ne\u00b7ren", "Ge\u00b7wich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "das die Weiten, die es still vernichten,", "tokens": ["das", "die", "Wei\u00b7ten", ",", "die", "es", "still", "ver\u00b7nich\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "zwingen, immer seliger zu sein.", "tokens": ["zwin\u00b7gen", ",", "im\u00b7mer", "se\u00b7li\u00b7ger", "zu", "sein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVINF", "$,", "ADV", "ADJD", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}