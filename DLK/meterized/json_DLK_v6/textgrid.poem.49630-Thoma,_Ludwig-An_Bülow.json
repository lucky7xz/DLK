{"textgrid.poem.49630": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An B\u00fclow", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wieso, du Vielgesch\u00e4tzter,", "tokens": ["Wie\u00b7so", ",", "du", "Viel\u00b7ge\u00b7sch\u00e4tz\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PWAV", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du willst uns Suppe geben?", "tokens": ["Du", "willst", "uns", "Sup\u00b7pe", "ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir sollen davon leben?", "tokens": ["Wir", "sol\u00b7len", "da\u00b7von", "le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PAV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du hast als liebe G\u00e4ste", "tokens": ["Du", "hast", "als", "lie\u00b7be", "G\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "KOKOM", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Uns alle eingeladen.", "tokens": ["Uns", "al\u00b7le", "ein\u00b7ge\u00b7la\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Heraus mit deinen Schinken", "tokens": ["He\u00b7raus", "mit", "dei\u00b7nen", "Schin\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und deinen Eierfladen!", "tokens": ["Und", "dei\u00b7nen", "Ei\u00b7er\u00b7fla\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "He! Wirtshaus! Nicht so s\u00e4umig!", "tokens": ["He", "!", "Wirts\u00b7haus", "!", "Nicht", "so", "s\u00e4u\u00b7mig", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NN", "$.", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo bleiben denn die Fische?", "tokens": ["Wo", "blei\u00b7ben", "denn", "die", "Fi\u00b7sche", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gebraten und gebacken", "tokens": ["Ge\u00b7bra\u00b7ten", "und", "ge\u00b7ba\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Will ich sie auf dem Tische.", "tokens": ["Will", "ich", "sie", "auf", "dem", "Ti\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dann mu\u00df ich dringend bitten,", "tokens": ["Dann", "mu\u00df", "ich", "drin\u00b7gend", "bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Fleisch nicht zu vergessen,", "tokens": ["Das", "Fleisch", "nicht", "zu", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kompott und etwas S\u00fc\u00dfes;", "tokens": ["Kom\u00b7pott", "und", "et\u00b7was", "S\u00fc\u00b7\u00dfes", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So sind wir sattgefressen.", "tokens": ["So", "sind", "wir", "satt\u00b7ge\u00b7fres\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wir r\u00fchmen deine G\u00fcte", "tokens": ["Wir", "r\u00fch\u00b7men", "dei\u00b7ne", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht, vor die Kn\u00f6pfe springen.", "tokens": ["Nicht", ",", "vor", "die", "Kn\u00f6p\u00b7fe", "sprin\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dann aber, alter Bursche,", "tokens": ["Dann", "a\u00b7ber", ",", "al\u00b7ter", "Bur\u00b7sche", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Soll weit dein Lob erklingen.", "tokens": ["Soll", "weit", "dein", "Lob", "er\u00b7klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}