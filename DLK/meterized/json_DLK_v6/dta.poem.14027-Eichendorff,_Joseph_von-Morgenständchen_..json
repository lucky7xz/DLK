{"dta.poem.14027": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Morgenst\u00e4ndchen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "In den Wipfeln frische L\u00fcfte,", "tokens": ["In", "den", "Wip\u00b7feln", "fri\u00b7sche", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fern melod'scher Quellen Fall,", "tokens": ["Fern", "me\u00b7lo\u00b7d'\u00b7scher", "Quel\u00b7len", "Fall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Durch die Einsamkeit der Kl\u00fcfte", "tokens": ["Durch", "die", "Ein\u00b7sam\u00b7keit", "der", "Kl\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Waldeslaut und Vogelschall,", "tokens": ["Wal\u00b7des\u00b7laut", "und", "Vo\u00b7gel\u00b7schall", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Scheuer Tr\u00e4ume Spielgenossen,", "tokens": ["Scheu\u00b7er", "Tr\u00e4u\u00b7me", "Spiel\u00b7ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Steigen all' beim Morgenschein", "tokens": ["Stei\u00b7gen", "all'", "beim", "Mor\u00b7gen\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Auf des Weinlaubs schwanken Sprossen,", "tokens": ["Auf", "des", "Wein\u00b7laubs", "schwan\u00b7ken", "Spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dir in's Fenster aus und ein.", "tokens": ["Dir", "in's", "Fens\u00b7ter", "aus", "und", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Und wir nah'n noch halb in Tr\u00e4umen,", "tokens": ["Und", "wir", "nah'n", "noch", "halb", "in", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und wir thun in Kl\u00e4ngen kund,", "tokens": ["Und", "wir", "thun", "in", "Kl\u00e4n\u00b7gen", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Was da drau\u00dfen in den B\u00e4umen", "tokens": ["Was", "da", "drau\u00b7\u00dfen", "in", "den", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Singt der weite Fr\u00fchlingsgrund.", "tokens": ["Singt", "der", "wei\u00b7te", "Fr\u00fch\u00b7lings\u00b7grund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Regt der Tag erst laut die Schwingen:", "tokens": ["Regt", "der", "Tag", "erst", "laut", "die", "Schwin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Sind wir Alle wieder weit \u2014", "tokens": ["Sind", "wir", "Al\u00b7le", "wie\u00b7der", "weit"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIS", "ADV", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Aber tief im Herzen klingen", "tokens": ["A\u00b7ber", "tief", "im", "Her\u00b7zen", "klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Lange nach noch Lust und Leid.", "tokens": ["Lan\u00b7ge", "nach", "noch", "Lust", "und", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADV", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}