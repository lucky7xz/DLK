{"dta.poem.14436": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Jugendandacht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ob neu der Morgenschmuck die Erde kleidet,", "tokens": ["Ob", "neu", "der", "Mor\u00b7gen\u00b7schmuck", "die", "Er\u00b7de", "klei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ob Nachtigallen Nacht und Stern' begr\u00fc\u00dfen,", "tokens": ["Ob", "Nach\u00b7ti\u00b7gal\u00b7len", "Nacht", "und", "Stern'", "be\u00b7gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Stets fern und nah bleibt ", "tokens": ["Stets", "fern", "und", "nah", "bleibt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Die in dem Lenz mich ewig sucht und meidet. \u2014", "tokens": ["Die", "in", "dem", "Lenz", "mich", "e\u00b7wig", "sucht", "und", "mei\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "ADJD", "VVFIN", "KON", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}