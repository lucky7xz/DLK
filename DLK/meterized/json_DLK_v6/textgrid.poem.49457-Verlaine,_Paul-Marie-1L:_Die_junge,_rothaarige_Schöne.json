{"textgrid.poem.49457": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die junge, rothaarige Sch\u00f6ne", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die junge, rothaarige Sch\u00f6ne", "tokens": ["Die", "jun\u00b7ge", ",", "rot\u00b7haa\u00b7ri\u00b7ge", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sprach, liebentflammt f\u00fcr die reine", "tokens": ["Sprach", ",", "lie\u00b7bent\u00b7flammt", "f\u00fcr", "die", "rei\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "APPR", "ART", "ADJA"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Hellblonde, unschuldige Kleine", "tokens": ["Hell\u00b7blon\u00b7de", ",", "un\u00b7schul\u00b7di\u00b7ge", "Klei\u00b7ne"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die z\u00e4rtlich fl\u00fcsternden T\u00f6ne:", "tokens": ["Die", "z\u00e4rt\u00b7lich", "fl\u00fcs\u00b7tern\u00b7den", "T\u00f6\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Dass die Bl\u00fcte die Pflanze kr\u00f6ne,", "tokens": ["Dass", "die", "Bl\u00fc\u00b7te", "die", "Pflan\u00b7ze", "kr\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Lass in deiner Kindheit Haine", "tokens": ["Lass", "in", "dei\u00b7ner", "Kind\u00b7heit", "Hai\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "PPOSAT", "NN", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mich tasten im Moos, dass die sch\u00f6ne", "tokens": ["Mich", "tas\u00b7ten", "im", "Moos", ",", "dass", "die", "sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "KOUS", "ART", "ADJA"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Holdleuchtende Rose erscheine.", "tokens": ["Hold\u00b7leuch\u00b7ten\u00b7de", "Ro\u00b7se", "er\u00b7schei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Lass im Gras, das schimmernd mich gr\u00fcsste,", "tokens": ["Lass", "im", "Gras", ",", "das", "schim\u00b7mernd", "mich", "gr\u00fcs\u00b7ste", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Die Tropfen des Taus mich trinken,", "tokens": ["Die", "Trop\u00b7fen", "des", "Taus", "mich", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die zart im Blumenkelch blinken.", "tokens": ["Die", "zart", "im", "Blu\u00b7men\u00b7kelch", "blin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Dass die Lust der Liebe, o S\u00fcss'ste", "tokens": ["Dass", "die", "Lust", "der", "Lie\u00b7be", ",", "o", "S\u00fcss'\u00b7ste"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "$,", "FM", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Auf der reinen Stirn dir ergl\u00fche,", "tokens": ["Auf", "der", "rei\u00b7nen", "Stirn", "dir", "er\u00b7gl\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie im sch\u00fcchternen \u00c4ther die Fr\u00fche.", "tokens": ["Wie", "im", "sch\u00fcch\u00b7ter\u00b7nen", "\u00c4\u00b7ther", "die", "Fr\u00fc\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}}}}