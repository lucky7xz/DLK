{"textgrid.poem.60029": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Stammte der Geist, Elise!", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stammte der Geist, Elise!", "tokens": ["Stamm\u00b7te", "der", "Geist", ",", "E\u00b7li\u00b7se", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "NE", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Den ein Gedanke der Liebe schuf,", "tokens": ["Den", "ein", "Ge\u00b7dan\u00b7ke", "der", "Lie\u00b7be", "schuf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "++-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Nicht vom Himmel, w\u00e4ren Paradiese", "tokens": ["Nicht", "vom", "Him\u00b7mel", ",", "w\u00e4\u00b7ren", "Pa\u00b7ra\u00b7die\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "APPRART", "NN", "$,", "VAFIN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nicht sein k\u00fcnftiger Beruf:", "tokens": ["Nicht", "sein", "k\u00fcnf\u00b7ti\u00b7ger", "Be\u00b7ruf", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sollt' ihm keine neue Sonne gl\u00e4nzen;", "tokens": ["Sollt'", "ihm", "kei\u00b7ne", "neu\u00b7e", "Son\u00b7ne", "gl\u00e4n\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hielt' ein niedriges Geschick", "tokens": ["Hielt'", "ein", "nied\u00b7ri\u00b7ges", "Ge\u00b7schick"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihn auf ewig in den Grenzen", "tokens": ["Ihn", "auf", "e\u00b7wig", "in", "den", "Gren\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieser Sterblichkeit zur\u00fcck;", "tokens": ["Die\u00b7ser", "Sterb\u00b7lich\u00b7keit", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "M\u00fc\u00dft' er durch die weite Sch\u00f6pfung wandeln,", "tokens": ["M\u00fc\u00dft'", "er", "durch", "die", "wei\u00b7te", "Sch\u00f6p\u00b7fung", "wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die Natur beleben \u00fcberall;", "tokens": ["Die", "Na\u00b7tur", "be\u00b7le\u00b7ben", "\u00fc\u00b7be\u00b7rall", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Jetzt im Weisen, wie die G\u00f6tter, handeln;", "tokens": ["Jetzt", "im", "Wei\u00b7sen", ",", "wie", "die", "G\u00f6t\u00b7ter", ",", "han\u00b7deln", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "PWAV", "ART", "NN", "$,", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Jetzt ein Liedchen singen in der Nachtigall;", "tokens": ["Jetzt", "ein", "Lied\u00b7chen", "sin\u00b7gen", "in", "der", "Nach\u00b7ti\u00b7gall", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.4": {"line.1": {"text": "Um die gr\u00fcne Quelle schweben,", "tokens": ["Um", "die", "gr\u00fc\u00b7ne", "Quel\u00b7le", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der hohen Linde bl\u00fchn,", "tokens": ["In", "der", "ho\u00b7hen", "Lin\u00b7de", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder aus gestorbnen Reben", "tokens": ["O\u00b7der", "aus", "ge\u00b7storb\u00b7nen", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den Keim der Veilchen ziehn.", "tokens": ["In", "den", "Keim", "der", "Veil\u00b7chen", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ach! Elise, w\u00e4re dann ein Schimmer", "tokens": ["Ach", "!", "E\u00b7li\u00b7se", ",", "w\u00e4\u00b7re", "dann", "ein", "Schim\u00b7mer"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "NE", "$,", "VAFIN", "ADV", "ART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "S\u00fc\u00dfer Angedenken mein;", "tokens": ["S\u00fc\u00b7\u00dfer", "An\u00b7ge\u00b7den\u00b7ken", "mein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In der weiten Sch\u00f6pfung sollte nimmer", "tokens": ["In", "der", "wei\u00b7ten", "Sch\u00f6p\u00b7fung", "soll\u00b7te", "nim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VMFIN", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dein Geist f\u00fcr mich verlohren seyn.", "tokens": ["Dein", "Geist", "f\u00fcr", "mich", "ver\u00b7loh\u00b7ren", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich wollte neben dir im Rosenhaine sprie\u00dfen,", "tokens": ["Ich", "woll\u00b7te", "ne\u00b7ben", "dir", "im", "Ro\u00b7sen\u00b7hai\u00b7ne", "sprie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als Mirthe dir zur Seite stehn,", "tokens": ["Als", "Mir\u00b7the", "dir", "zur", "Sei\u00b7te", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Bache dir entgegen flie\u00dfen,", "tokens": ["Im", "Ba\u00b7che", "dir", "ent\u00b7ge\u00b7gen", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PTKVZ", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit dir im leisen Weste wehn.", "tokens": ["Mit", "dir", "im", "lei\u00b7sen", "Wes\u00b7te", "wehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und holde M\u00e4dchen giengen", "tokens": ["Und", "hol\u00b7de", "M\u00e4d\u00b7chen", "gien\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Rosenhaine dann;", "tokens": ["Im", "Ro\u00b7sen\u00b7hai\u00b7ne", "dann", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Elise! wir empfiengen", "tokens": ["E\u00b7li\u00b7se", "!", "wir", "emp\u00b7fi\u00b7en\u00b7gen"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den m\u00fcden Wandersmann,", "tokens": ["Den", "m\u00fc\u00b7den", "Wan\u00b7ders\u00b7mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Beschatteten gelinde", "tokens": ["Be\u00b7schat\u00b7te\u00b7ten", "ge\u00b7lin\u00b7de"], "token_info": ["word", "word"], "pos": ["NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sein armes, kleines Mahl,", "tokens": ["Sein", "ar\u00b7mes", ",", "klei\u00b7nes", "Mahl", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und lispelten im Winde", "tokens": ["Und", "lis\u00b7pel\u00b7ten", "im", "Win\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.4": {"text": "Durch ein bebl\u00fcmtes Thal,", "tokens": ["Durch", "ein", "be\u00b7bl\u00fcm\u00b7tes", "Thal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wo Hirtenknaben spielten;", "tokens": ["Wo", "Hir\u00b7ten\u00b7kna\u00b7ben", "spiel\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verdoppelten den Flug", "tokens": ["Ver\u00b7dop\u00b7pel\u00b7ten", "den", "Flug"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+---+", "measure": "dactylic.init"}, "line.3": {"text": "Zum Schnitter hin, und k\u00fchlten", "tokens": ["Zum", "Schnit\u00b7ter", "hin", ",", "und", "k\u00fchl\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "KON", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihm seinen Wasserkrug.", "tokens": ["Ihm", "sei\u00b7nen", "Was\u00b7ser\u00b7krug", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wir eilten in dem Flusse,", "tokens": ["Wir", "eil\u00b7ten", "in", "dem", "Flus\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verk\u00fcndigten den May,", "tokens": ["Ver\u00b7k\u00fcn\u00b7dig\u00b7ten", "den", "May", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+---+", "measure": "dactylic.init"}, "line.3": {"text": "Und murmelten sanfter bey dem Kusse", "tokens": ["Und", "mur\u00b7mel\u00b7ten", "sanf\u00b7ter", "bey", "dem", "Kus\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Z\u00e4rtlicher Br\u00e4ute vorbey.", "tokens": ["Z\u00e4rt\u00b7li\u00b7cher", "Br\u00e4u\u00b7te", "vor\u00b7bey", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.11": {"line.1": {"text": "Aber o! ich f\u00fchl es: Paradiese", "tokens": ["A\u00b7ber", "o", "!", "ich", "f\u00fchl", "es", ":", "Pa\u00b7ra\u00b7die\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "FM", "$.", "PPER", "VVFIN", "PPER", "$.", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Warten auf uns; g\u00f6ttlich ist unser Beruf:", "tokens": ["War\u00b7ten", "auf", "uns", ";", "g\u00f6tt\u00b7lich", "ist", "un\u00b7ser", "Be\u00b7ruf", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$.", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Dein L\u00e4cheln sagt es mir, Elise!", "tokens": ["Dein", "L\u00e4\u00b7cheln", "sagt", "es", "mir", ",", "E\u00b7li\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PPER", "$,", "NE", "$."], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da\u00df uns die Liebe schuf.", "tokens": ["Da\u00df", "uns", "die", "Lie\u00b7be", "schuf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Dein L\u00e4cheln soll, in sch\u00f6nern Welten,", "tokens": ["Dein", "L\u00e4\u00b7cheln", "soll", ",", "in", "sch\u00f6\u00b7nern", "Wel\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zur Seligkeit die Geister weihn,", "tokens": ["Zur", "Se\u00b7lig\u00b7keit", "die", "Geis\u00b7ter", "weihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Engeln Tugenden vergelten,", "tokens": ["Und", "En\u00b7geln", "Tu\u00b7gen\u00b7den", "ver\u00b7gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mir ein Lohn der Unschuld seyn.", "tokens": ["Und", "mir", "ein", "Lohn", "der", "Un\u00b7schuld", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}