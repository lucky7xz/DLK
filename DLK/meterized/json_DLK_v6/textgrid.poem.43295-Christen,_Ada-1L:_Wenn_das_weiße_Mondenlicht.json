{"textgrid.poem.43295": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn das wei\u00dfe Mondenlicht", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn das wei\u00dfe Mondenlicht", "tokens": ["Wenn", "das", "wei\u00b7\u00dfe", "Mon\u00b7den\u00b7licht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die klaren Scheiben rinnt", "tokens": ["Durch", "die", "kla\u00b7ren", "Schei\u00b7ben", "rinnt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und Dein holdes Angesicht", "tokens": ["Und", "Dein", "hol\u00b7des", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sacht mit Schleiern \u00fcberspinnt,", "tokens": ["Sacht", "mit", "Schlei\u00b7ern", "\u00fc\u00b7bers\u00b7pinnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn das Kind an Deiner Brust", "tokens": ["Wenn", "das", "Kind", "an", "Dei\u00b7ner", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00e4umend l\u00e4chelt \u2013 fremd der Welt \u2013", "tokens": ["Tr\u00e4u\u00b7mend", "l\u00e4\u00b7chelt", "\u2013", "fremd", "der", "Welt", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$(", "ADJD", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ahnt mir, da\u00df es unbewu\u00dft", "tokens": ["Ahnt", "mir", ",", "da\u00df", "es", "un\u00b7be\u00b7wu\u00dft"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch mit Engeln Zwiesprach h\u00e4lt ...", "tokens": ["Noch", "mit", "En\u00b7geln", "Zwie\u00b7sprach", "h\u00e4lt", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}