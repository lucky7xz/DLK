{"dta.poem.20079": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Um Dich .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Ach, da\u00df ich Dich so hei\u00df ersehne,", "tokens": ["Ach", ",", "da\u00df", "ich", "Dich", "so", "hei\u00df", "er\u00b7seh\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "PRF", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Weckt aller Himmel Widerspruch,", "tokens": ["Weckt", "al\u00b7ler", "Him\u00b7mel", "Wi\u00b7der\u00b7spruch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und jede neue bittre Thr\u00e4ne", "tokens": ["Und", "je\u00b7de", "neu\u00b7e", "bitt\u00b7re", "Thr\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Macht tiefer nur den Friedensbruch.", "tokens": ["Macht", "tie\u00b7fer", "nur", "den", "Frie\u00b7dens\u00b7bruch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der G\u00f6tter Ohr ist Keinem offen,", "tokens": ["Der", "G\u00f6t\u00b7ter", "Ohr", "ist", "Kei\u00b7nem", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der sich zergr\u00e4mt in banger Nacht, \u2014", "tokens": ["Der", "sich", "zer\u00b7gr\u00e4mt", "in", "ban\u00b7ger", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PRF", "VVPP", "APPR", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Komm Herz, wir wollen gar nichts hoffen,", "tokens": ["Komm", "Herz", ",", "wir", "wol\u00b7len", "gar", "nichts", "hof\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "PPER", "VMFIN", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sehn ob ", "tokens": ["Und", "sehn", "ob"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "KOUS"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Vergebnes M\u00fchen, eitles Wollen,", "tokens": ["Ver\u00b7geb\u00b7nes", "M\u00fc\u00b7hen", ",", "eit\u00b7les", "Wol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Lippe wei\u00df kaum was sie spricht,", "tokens": ["Die", "Lip\u00b7pe", "wei\u00df", "kaum", "was", "sie", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und, nach wie vor, die Thr\u00e4nen rollen", "tokens": ["Und", ",", "nach", "wie", "vor", ",", "die", "Thr\u00e4\u00b7nen", "rol\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "APPR", "KOKOM", "APPR", "$,", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir \u00fcber Wang und Angesicht.", "tokens": ["Mir", "\u00fc\u00b7ber", "Wang", "und", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}