{"dta.poem.19278": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie k\u00f6nnte Denken denn und Seyn verschieden seyn?", "tokens": ["Wie", "k\u00f6nn\u00b7te", "Den\u00b7ken", "denn", "und", "Seyn", "ver\u00b7schie\u00b7den", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "NN", "ADV", "KON", "PPOSAT", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was in dir denket, ist; dein Denken ist dein Seyn.", "tokens": ["Was", "in", "dir", "den\u00b7ket", ",", "ist", ";", "dein", "Den\u00b7ken", "ist", "dein", "Seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "VVFIN", "$,", "VAFIN", "$.", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Seyn, das nicht Denken ist, hat nur sich selbst verloren,", "tokens": ["Seyn", ",", "das", "nicht", "Den\u00b7ken", "ist", ",", "hat", "nur", "sich", "selbst", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$,", "PRELS", "PTKNEG", "NN", "VAFIN", "$,", "VAFIN", "ADV", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wird im Denken erst zu sich zur\u00fcck geboren.", "tokens": ["Und", "wird", "im", "Den\u00b7ken", "erst", "zu", "sich", "zu\u00b7r\u00fcck", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "NN", "ADV", "APPR", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das ist, der die Natur verkl\u00e4ren soll, der Geist;", "tokens": ["Das", "ist", ",", "der", "die", "Na\u00b7tur", "ver\u00b7kl\u00e4\u00b7ren", "soll", ",", "der", "Geist", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PRELS", "ART", "NN", "VVINF", "VMFIN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein Leben ist, da\u00df du in ihm lebendig seist.", "tokens": ["Dein", "Le\u00b7ben", "ist", ",", "da\u00df", "du", "in", "ihm", "le\u00b7ben\u00b7dig", "seist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$,", "KOUS", "PPER", "APPR", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}}}}}