{"dta.poem.19992": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Der Rosenstrauch .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bei Winters Frost in Kluft und Wald", "tokens": ["Bei", "Win\u00b7ters", "Frost", "in", "Kluft", "und", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich Kaiser Carl verloren,", "tokens": ["Sich", "Kai\u00b7ser", "Carl", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "NE", "NE", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Diener treu, die liegen bald", "tokens": ["Die", "Die\u00b7ner", "treu", ",", "die", "lie\u00b7gen", "bald"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "$,", "PRELS", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Rings um den Herrn erfroren.", "tokens": ["Rings", "um", "den", "Herrn", "er\u00b7fro\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er niederkniet auf kalten Stein,", "tokens": ["Er", "nie\u00b7der\u00b7kniet", "auf", "kal\u00b7ten", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Legt ab die g\u00fcldnen Ketten,", "tokens": ["Legt", "ab", "die", "g\u00fcld\u00b7nen", "Ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Legt ab den Purpurmantel sein,", "tokens": ["Legt", "ab", "den", "Pur\u00b7pur\u00b7man\u00b7tel", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und th\u00e4t dem\u00fcthig beten.", "tokens": ["Und", "th\u00e4t", "de\u00b7m\u00fct\u00b7hig", "be\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ach, weh! ach, weh! der Rosenkranz", "tokens": ["Ach", ",", "weh", "!", "ach", ",", "weh", "!", "der", "Ro\u00b7sen\u00b7kranz"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["ITJ", "$,", "PTKVZ", "$.", "XY", "$,", "PTKVZ", "$.", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der starren Hand entsinket,", "tokens": ["Der", "star\u00b7ren", "Hand", "ent\u00b7sin\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch wie er sinkt, wie Sonnenglanz", "tokens": ["Doch", "wie", "er", "sinkt", ",", "wie", "Son\u00b7nen\u00b7glanz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "PWAV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er auf der Erde blinket.", "tokens": ["Er", "auf", "der", "Er\u00b7de", "blin\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ein Rosenstock schnell aus ihm spro\u00dft,", "tokens": ["Ein", "Ro\u00b7sen\u00b7stock", "schnell", "aus", "ihm", "spro\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Th\u00e4t \u00fcber Eichen steigen,", "tokens": ["Th\u00e4t", "\u00fc\u00b7ber", "Ei\u00b7chen", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein s\u00fc\u00dfes D\u00fcften sich ergo\u00df", "tokens": ["Ein", "s\u00fc\u00b7\u00dfes", "D\u00fcf\u00b7ten", "sich", "er\u00b7go\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus seinen Bl\u00fcthen und Zweigen.", "tokens": ["Aus", "sei\u00b7nen", "Bl\u00fc\u00b7then", "und", "Zwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Auch rings, so weit sein Duft gereicht,", "tokens": ["Auch", "rings", ",", "so", "weit", "sein", "Duft", "ge\u00b7reicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "ADJD", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die B\u00e4ume gr\u00fcnend standen,", "tokens": ["Die", "B\u00e4u\u00b7me", "gr\u00fc\u00b7nend", "stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die V\u00f6gel sich mit Singen leicht", "tokens": ["Die", "V\u00f6\u00b7gel", "sich", "mit", "Sin\u00b7gen", "leicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohl durch die L\u00fcfte schwangen.", "tokens": ["Wohl", "durch", "die", "L\u00fcf\u00b7te", "schwan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Sonne auch durch Kluft und Wald", "tokens": ["Die", "Son\u00b7ne", "auch", "durch", "Kluft", "und", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit mildem Glanz geschienen,", "tokens": ["Mit", "mil\u00b7dem", "Glanz", "ge\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Knappen treu erstehen all,", "tokens": ["Die", "Knap\u00b7pen", "treu", "er\u00b7ste\u00b7hen", "all", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "PIAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Herren zu bedienen.", "tokens": ["Den", "Her\u00b7ren", "zu", "be\u00b7die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und wo den Rosenstock man schaut", "tokens": ["Und", "wo", "den", "Ro\u00b7sen\u00b7stock", "man", "schaut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "PIS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf der geweihten Stelle,", "tokens": ["Auf", "der", "ge\u00b7weih\u00b7ten", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zur Andacht ward gar wohl erbaut", "tokens": ["Zur", "An\u00b7dacht", "ward", "gar", "wohl", "er\u00b7baut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Eine heilige Kapelle.", "tokens": ["Ei\u00b7ne", "hei\u00b7li\u00b7ge", "Ka\u00b7pel\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ein Rosenkranz umf\u00e4ngt sie bald,", "tokens": ["Ein", "Ro\u00b7sen\u00b7kranz", "um\u00b7f\u00e4ngt", "sie", "bald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Unter'n Altar die Wurzeln dringen.", "tokens": ["Un\u00b7ter'n", "Al\u00b7tar", "die", "Wur\u00b7zeln", "drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVINF", "$."], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Da innen Chor und Orgel schallt,", "tokens": ["Da", "in\u00b7nen", "Chor", "und", "Or\u00b7gel", "schallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da drau\u00dfen die V\u00f6gel singen.", "tokens": ["Da", "drau\u00b7\u00dfen", "die", "V\u00f6\u00b7gel", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}