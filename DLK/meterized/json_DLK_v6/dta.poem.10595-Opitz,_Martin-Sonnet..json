{"dta.poem.10595": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "So offt ich ewren Glantz/ jhr hellen \u00e4uglein/ schawe/", "tokens": ["So", "offt", "ich", "ew\u00b7ren", "Glantz", "/", "jhr", "hel\u00b7len", "\u00e4ug\u00b7lein", "/", "scha\u00b7we", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPOSAT", "NN", "$(", "PPOSAT", "ADJA", "NN", "$(", "VVFIN", "$("], "meter": "-+-+-+-+----+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Empfindt ich bey mir selbst so grosse Lust vnd Freudt/", "tokens": ["Emp\u00b7findt", "ich", "bey", "mir", "selbst", "so", "gros\u00b7se", "Lust", "vnd", "Freudt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "ADV", "ADV", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df ich mich frewen mu\u00df auch in Tr\u00fcbseligkeit", "tokens": ["Da\u00df", "ich", "mich", "fre\u00b7wen", "mu\u00df", "auch", "in", "Tr\u00fcb\u00b7se\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VMFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Vnd eusserster Fortun/ in dem ich auff euch bawe/", "tokens": ["Vnd", "eus\u00b7sers\u00b7ter", "For\u00b7tun", "/", "in", "dem", "ich", "auff", "euch", "ba\u00b7we", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$(", "APPR", "PRELS", "PPER", "APPR", "PPER", "VVFIN", "$("], "meter": "-+--+-++-+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Hergegen sch\u00e4tz ich mich f\u00fcr die betr\u00fcbtste Frawe/", "tokens": ["Her\u00b7ge\u00b7gen", "sch\u00e4tz", "ich", "mich", "f\u00fcr", "die", "be\u00b7tr\u00b7\u00fcbts\u00b7te", "Fra\u00b7we", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.6": {"text": "Wann jhr nicht wie zuvor geneigt vnd freundlich seyt/", "tokens": ["Wann", "jhr", "nicht", "wie", "zu\u00b7vor", "ge\u00b7neigt", "vnd", "freund\u00b7lich", "seyt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "KOKOM", "ADV", "VVPP", "KON", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich bin mir selber gramm/ mein Leben ist mir leydt/", "tokens": ["Ich", "bin", "mir", "sel\u00b7ber", "gramm", "/", "mein", "Le\u00b7ben", "ist", "mir", "leydt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADJD", "$(", "PPOSAT", "NN", "VAFIN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil ich von euch geha\u00dft/ auff die ich einig trawe.", "tokens": ["Weil", "ich", "von", "euch", "ge\u00b7ha\u00dft", "/", "auff", "die", "ich", "ei\u00b7nig", "tra\u00b7we", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVPP", "$(", "APPR", "PRELS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.9": {"text": "Ihr jrrdisches Gestirn/ jhr sterblichen Planeten/", "tokens": ["Ihr", "jrr\u00b7di\u00b7sches", "Ge\u00b7stirn", "/", "jhr", "sterb\u00b7li\u00b7chen", "Pla\u00b7ne\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Ihr meine Sonn vnd Mond/ die jhr mich k\u00f6nnet t\u00f6dten;", "tokens": ["Ihr", "mei\u00b7ne", "Sonn", "vnd", "Mond", "/", "die", "jhr", "mich", "k\u00f6n\u00b7net", "t\u00f6d\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "KON", "NN", "$(", "PRELS", "PPER", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ohn euch ist alle Lust nichts als ein blosses bildt/", "tokens": ["Ohn", "euch", "ist", "al\u00b7le", "Lust", "nichts", "als", "ein", "blos\u00b7ses", "bildt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VAFIN", "PIAT", "NN", "PIS", "KOKOM", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Was wundert jhr euch dann/ da\u00df ich zu euch mu\u00df eilen/", "tokens": ["Was", "wun\u00b7dert", "jhr", "euch", "dann", "/", "da\u00df", "ich", "zu", "euch", "mu\u00df", "ei\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "ADV", "$(", "KOUS", "PPER", "APPR", "PPER", "VMFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mein Zuversicht? Es fleucht ein jeder f\u00fcr den Pfeilen", "tokens": ["Mein", "Zu\u00b7ver\u00b7sicht", "?", "Es", "fleucht", "ein", "je\u00b7der", "f\u00fcr", "den", "Pfei\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PPER", "VVFIN", "ART", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "De\u00df Todes/ wider die jhr seyt mein starcker Schildt.", "tokens": ["De\u00df", "To\u00b7des", "/", "wi\u00b7der", "die", "jhr", "seyt", "mein", "star\u00b7cker", "Schildt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "APPR", "PRELS", "PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}