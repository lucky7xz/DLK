{"textgrid.poem.24579": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Men. an Tr.", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Spricht von der Tugend mehr/als alle Weisen sprechen", "tokens": ["Spricht", "von", "der", "Tu\u00b7gend", "mehr", "/", "als", "al\u00b7le", "Wei\u00b7sen", "spre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "$(", "KOKOM", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und lebe mit der Welt/so ist es ein Verbrechen.", "tokens": ["Und", "le\u00b7be", "mit", "der", "Welt", "/", "so", "ist", "es", "ein", "Ver\u00b7bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$(", "ADV", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der wahren Tugend steht das Hoff-Recht \u00fcbel an/", "tokens": ["Der", "wah\u00b7ren", "Tu\u00b7gend", "steht", "das", "Hoff\u00b7Recht", "\u00fc\u00b7bel", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die That/kein Compliment, ist was sie letzen kan.", "tokens": ["Die", "That", "/", "kein", "Com\u00b7pli\u00b7ment", ",", "ist", "was", "sie", "let\u00b7zen", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PIAT", "NN", "$,", "VAFIN", "PWS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Im Munde wohnt sie nicht; da sie vom Himmel kommen/", "tokens": ["Im", "Mun\u00b7de", "wohnt", "sie", "nicht", ";", "da", "sie", "vom", "Him\u00b7mel", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PTKNEG", "$.", "KOUS", "PPER", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hat sie den rechten Sitz in unsrer Brust genommen.", "tokens": ["Hat", "sie", "den", "rech\u00b7ten", "Sitz", "in", "uns\u00b7rer", "Brust", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Aus dieser gehet sie durch der Gedancken Reich/", "tokens": ["Aus", "die\u00b7ser", "ge\u00b7het", "sie", "durch", "der", "Ge\u00b7dan\u00b7cken", "Reich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$("], "meter": "-+--+-++-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Beherrscht die Sinnen Lust/macht uns den Engeln gleich/", "tokens": ["Be\u00b7herrscht", "die", "Sin\u00b7nen", "Lust", "/", "macht", "uns", "den", "En\u00b7geln", "gleich", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$(", "VVFIN", "PPER", "ART", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Thut/was der Himmel will/wird wenig von sich schreiben/", "tokens": ["Thut", "/", "was", "der", "Him\u00b7mel", "will", "/", "wird", "we\u00b7nig", "von", "sich", "schrei\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PWS", "ART", "NN", "VMFIN", "$(", "VAFIN", "PIS", "APPR", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und ihrer besten That die Ehre schuldig bleiben.", "tokens": ["Und", "ih\u00b7rer", "bes\u00b7ten", "That", "die", "Eh\u00b7re", "schul\u00b7dig", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Schlie\u00dft deine Liebe nun ein reines Hertz in sich/", "tokens": ["Schlie\u00dft", "dei\u00b7ne", "Lie\u00b7be", "nun", "ein", "rei\u00b7nes", "Hertz", "in", "sich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "ART", "ADJA", "NN", "APPR", "PRF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Es liebe/denn es liebt dadurch die Tugend dich.", "tokens": ["Es", "lie\u00b7be", "/", "denn", "es", "liebt", "da\u00b7durch", "die", "Tu\u00b7gend", "dich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KON", "PPER", "VVFIN", "PAV", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Du kanst dabey ein Lied von edler Freundschafft setzen:", "tokens": ["Du", "kanst", "da\u00b7bey", "ein", "Lied", "von", "ed\u00b7ler", "Freund\u00b7schafft", "set\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PAV", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der Menschen Liebe bleibt des Himmels seyn Ergetzen.", "tokens": ["Der", "Men\u00b7schen", "Lie\u00b7be", "bleibt", "des", "Him\u00b7mels", "seyn", "Er\u00b7get\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Auch liebe das Geschlecht/bey dem die Sch\u00f6nheit steht/", "tokens": ["Auch", "lie\u00b7be", "das", "Ge\u00b7schlecht", "/", "bey", "dem", "die", "Sch\u00f6n\u00b7heit", "steht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$(", "APPR", "ART", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Nur lieb es/wie dich liebt/der heunte zu dir geht.", "tokens": ["Nur", "lieb", "es", "/", "wie", "dich", "liebt", "/", "der", "heun\u00b7te", "zu", "dir", "geht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "$(", "PWAV", "PPER", "VVFIN", "$(", "ART", "ADJA", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}