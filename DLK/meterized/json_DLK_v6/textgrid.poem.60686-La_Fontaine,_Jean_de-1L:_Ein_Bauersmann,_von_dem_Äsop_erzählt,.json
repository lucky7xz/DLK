{"textgrid.poem.60686": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Bauersmann, von dem \u00c4sop erz\u00e4hlt,", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Bauersmann, von dem \u00c4sop erz\u00e4hlt,", "tokens": ["Ein", "Bau\u00b7ers\u00b7mann", ",", "von", "dem", "\u00c4\u00b7sop", "er\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "War zwar nicht sonderlich gescheit,", "tokens": ["War", "zwar", "nicht", "son\u00b7der\u00b7lich", "ge\u00b7scheit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch, sah er irgendwen in Not und Leid,", "tokens": ["Doch", ",", "sah", "er", "ir\u00b7gend\u00b7wen", "in", "Not", "und", "Leid", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "War er von tiefstem Mitgef\u00fchl beseelt.", "tokens": ["War", "er", "von", "tiefs\u00b7tem", "Mit\u00b7ge\u00b7f\u00fchl", "be\u00b7seelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein b\u00f6ser Winter war im Land,", "tokens": ["Ein", "b\u00f6\u00b7ser", "Win\u00b7ter", "war", "im", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als dieser Mann einst durch die \u00c4cker schritt", "tokens": ["Als", "die\u00b7ser", "Mann", "einst", "durch", "die", "\u00c4\u00b7cker", "schritt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "NN", "ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und auf dem Schneefeld eine Schlange fand.", "tokens": ["Und", "auf", "dem", "Schnee\u00b7feld", "ei\u00b7ne", "Schlan\u00b7ge", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Er nahm die Halberstarrte mit,", "tokens": ["Er", "nahm", "die", "Hal\u00b7ber\u00b7starr\u00b7te", "mit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Erw\u00e4rmte sie in seiner Hand", "tokens": ["Er\u00b7w\u00e4rm\u00b7te", "sie", "in", "sei\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und trug die Arme dann voll G\u00fcte", "tokens": ["Und", "trug", "die", "Ar\u00b7me", "dann", "voll", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Im Mantel heim an seinen warmen Herd,", "tokens": ["Im", "Man\u00b7tel", "heim", "an", "sei\u00b7nen", "war\u00b7men", "Herd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Wo er sich lange um sie m\u00fchte,", "tokens": ["Wo", "er", "sich", "lan\u00b7ge", "um", "sie", "m\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "APPR", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Doch wenig fragte, ob das Tier der M\u00fche wert.", "tokens": ["Doch", "we\u00b7nig", "frag\u00b7te", ",", "ob", "das", "Tier", "der", "M\u00fc\u00b7he", "wert", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "KOUS", "ART", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und endlich ist die Schlange neubelebt!", "tokens": ["Und", "end\u00b7lich", "ist", "die", "Schlan\u00b7ge", "neu\u00b7be\u00b7lebt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Kaum aber f\u00fchlt sie W\u00e4rme durch den K\u00f6rper rinnen,", "tokens": ["Kaum", "a\u00b7ber", "f\u00fchlt", "sie", "W\u00e4r\u00b7me", "durch", "den", "K\u00f6r\u00b7per", "rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Als sie voll Bosheit ihren Kopf erhebt", "tokens": ["Als", "sie", "voll", "Bos\u00b7heit", "ih\u00b7ren", "Kopf", "er\u00b7hebt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Und ohne viel Besinnen", "tokens": ["Und", "oh\u00b7ne", "viel", "Be\u00b7sin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Mit b\u00f6sem Zischen ihre Wut bekundet.", "tokens": ["Mit", "b\u00f6\u00b7sem", "Zi\u00b7schen", "ih\u00b7re", "Wut", "be\u00b7kun\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nun biegt sie sich zur\u00fcck und schnellt sich dann empor,", "tokens": ["Nun", "biegt", "sie", "sich", "zu\u00b7r\u00fcck", "und", "schnellt", "sich", "dann", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "KON", "VVFIN", "PRF", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Um ihren Retter zu verderben;", "tokens": ["Um", "ih\u00b7ren", "Ret\u00b7ter", "zu", "ver\u00b7der\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch dieser ruft, im Herzen tief verwundet:", "tokens": ["Doch", "die\u00b7ser", "ruft", ",", "im", "Her\u00b7zen", "tief", "ver\u00b7wun\u00b7det", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "$,", "APPRART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbdu Undankbare, du mu\u00dft sterben!\u00ab", "tokens": ["\u00bb", "du", "Un\u00b7dank\u00b7ba\u00b7re", ",", "du", "mu\u00dft", "ster\u00b7ben", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "NN", "$,", "PPER", "VMFIN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und in gerechtem Zorn holt er sein Beil hervor,", "tokens": ["Und", "in", "ge\u00b7rech\u00b7tem", "Zorn", "holt", "er", "sein", "Beil", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zerhackt das Vieh und macht aus einer Schlange drei:", "tokens": ["Zer\u00b7hackt", "das", "Vieh", "und", "macht", "aus", "ei\u00b7ner", "Schlan\u00b7ge", "drei", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "APPR", "ART", "NN", "CARD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den Kopf, den Rumpf, den Schwanz.", "tokens": ["Den", "Kopf", ",", "den", "Rumpf", ",", "den", "Schwanz", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Schlange w\u00e4re gerne wieder ganz;", "tokens": ["Die", "Schlan\u00b7ge", "w\u00e4\u00b7re", "ger\u00b7ne", "wie\u00b7der", "ganz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Jedoch, sie war und blieb entzwei,", "tokens": ["Je\u00b7doch", ",", "sie", "war", "und", "blieb", "ent\u00b7zwei", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VAFIN", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und mit dem Leben war's vorbei.", "tokens": ["Und", "mit", "dem", "Le\u00b7ben", "wa\u00b7r's", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "PTKVZ", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.10": {"text": "Wohl ist Barmherzigkeit ein sch\u00f6ner Zug,", "tokens": ["Wohl", "ist", "Barm\u00b7her\u00b7zig\u00b7keit", "ein", "sch\u00f6\u00b7ner", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Doch \u00fcbe sie bedacht und klug.", "tokens": ["Doch", "\u00fc\u00b7be", "sie", "be\u00b7dacht", "und", "klug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVPP", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Den Undankbaren aber wird es schlecht ergehen,", "tokens": ["Den", "Un\u00b7dank\u00b7ba\u00b7ren", "a\u00b7ber", "wird", "es", "schlecht", "er\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So wie es hier dem Schlangenvieh geschehen.", "tokens": ["So", "wie", "es", "hier", "dem", "Schlan\u00b7gen\u00b7vieh", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}