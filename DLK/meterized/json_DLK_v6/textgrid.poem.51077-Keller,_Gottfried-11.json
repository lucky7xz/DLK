{"textgrid.poem.51077": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "11", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie herrlich w\u00e4r's, zerschnittner Tannenbaum,", "tokens": ["Wie", "herr\u00b7lich", "w\u00e4r's", ",", "zer\u00b7schnitt\u00b7ner", "Tan\u00b7nen\u00b7baum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du ragtest als ein schlanker Mast empor,", "tokens": ["Du", "rag\u00b7test", "als", "ein", "schlan\u00b7ker", "Mast", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bewimpelt, in den blauen Himmelsraum,", "tokens": ["Be\u00b7wim\u00b7pelt", ",", "in", "den", "blau\u00b7en", "Him\u00b7mels\u00b7raum", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vor einem sonnig heitern Hafentor!", "tokens": ["Vor", "ei\u00b7nem", "son\u00b7nig", "hei\u00b7tern", "Ha\u00b7fen\u00b7tor", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da, m\u00fcssen wir einmal beisammen sein,", "tokens": ["Da", ",", "m\u00fcs\u00b7sen", "wir", "ein\u00b7mal", "bei\u00b7sam\u00b7men", "sein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VMFIN", "PPER", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.2": {"text": "Lehnt' ich an dir im schwanken Segelhaus:", "tokens": ["Lehnt'", "ich", "an", "dir", "im", "schwan\u00b7ken", "Se\u00b7gel\u00b7haus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du aus dem Schwarzwald, dr\u00fcben ich vom Rhein,", "tokens": ["Du", "aus", "dem", "Schwarz\u00b7wald", ",", "dr\u00fc\u00b7ben", "ich", "vom", "Rhein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$,", "VVFIN", "PPER", "APPRART", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kamraden, reisten wir aufs Meer hinaus.", "tokens": ["Kam\u00b7ra\u00b7den", ",", "reis\u00b7ten", "wir", "aufs", "Meer", "hin\u00b7aus", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Und br\u00e4ch das Schiff zu Splittern auseinand,", "tokens": ["Und", "br\u00e4ch", "das", "Schiff", "zu", "Split\u00b7tern", "aus\u00b7ei\u00b7nand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Geborsten du und \u00fcber Bord gef\u00e4llt,", "tokens": ["Ge\u00b7bors\u00b7ten", "du", "und", "\u00fc\u00b7ber", "Bord", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Umfa\u00dft' ich dich mit eisenfester Hand:", "tokens": ["Um\u00b7fa\u00dft'", "ich", "dich", "mit", "ei\u00b7sen\u00b7fes\u00b7ter", "Hand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So schw\u00e4mmen beide wir ans End der Welt.", "tokens": ["So", "schw\u00e4m\u00b7men", "bei\u00b7de", "wir", "ans", "End", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Am besten w\u00e4r's, du st\u00fcndest hoch und frei", "tokens": ["Am", "bes\u00b7ten", "w\u00e4r's", ",", "du", "st\u00fcn\u00b7dest", "hoch", "und", "frei"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "VAFIN", "$,", "PPER", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im Tannenwald, das Haupt voll Vogelsang,", "tokens": ["Im", "Tan\u00b7nen\u00b7wald", ",", "das", "Haupt", "voll", "Vo\u00b7gel\u00b7sang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich aber schlenderte an dir vorbei,", "tokens": ["Ich", "a\u00b7ber", "schlen\u00b7der\u00b7te", "an", "dir", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.4": {"text": "Wohin ich wollt', den gr\u00fcnen Berg entlang!", "tokens": ["Wo\u00b7hin", "ich", "wollt'", ",", "den", "gr\u00fc\u00b7nen", "Berg", "ent\u00b7lang", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VMFIN", "$,", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}