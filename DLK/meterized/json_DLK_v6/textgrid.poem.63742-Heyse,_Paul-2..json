{"textgrid.poem.63742": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4r' Vater Adam hier am Golf geboren,", "tokens": ["W\u00e4r'", "Va\u00b7ter", "A\u00b7dam", "hier", "am", "Golf", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "NE", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nie h\u00e4tt' er sich ums Paradies gebracht;", "tokens": ["Nie", "h\u00e4tt'", "er", "sich", "ums", "Pa\u00b7ra\u00b7dies", "ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den Zorn des Herrn h\u00e4tt' er hinweggelacht", "tokens": ["Den", "Zorn", "des", "Herrn", "h\u00e4tt'", "er", "hin\u00b7weg\u00b7ge\u00b7lacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit echt napoletanischen Humoren.", "tokens": ["Mit", "echt", "na\u00b7po\u00b7le\u00b7ta\u00b7ni\u00b7schen", "Hu\u00b7mo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Heut, da ich wandelt' ins Gew\u00fchl verloren", "tokens": ["Heut", ",", "da", "ich", "wan\u00b7delt'", "ins", "Ge\u00b7w\u00fchl", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "VVFIN", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Am Hafen, f\u00fchlt' ich eine Hand, die sacht", "tokens": ["Am", "Ha\u00b7fen", ",", "f\u00fchlt'", "ich", "ei\u00b7ne", "Hand", ",", "die", "sacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "An meinem Rockscho\u00df sich zu schaffen macht';", "tokens": ["An", "mei\u00b7nem", "Rock\u00b7scho\u00df", "sich", "zu", "schaf\u00b7fen", "macht'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Griff \u2013 den Schlingel hatt' ich bei den Ohren.", "tokens": ["Ein", "Griff", "\u2013", "den", "Schlin\u00b7gel", "hatt'", "ich", "bei", "den", "Oh\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch wie ein Aal entschl\u00fcpft' er mir und stand", "tokens": ["Doch", "wie", "ein", "Aal", "ent\u00b7schl\u00fcpft'", "er", "mir", "und", "stand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "PPER", "PPER", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erst in der Ferne still, mit Sehnsuchtsblicken,", "tokens": ["Erst", "in", "der", "Fer\u00b7ne", "still", ",", "mit", "Sehn\u00b7suchts\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKVZ", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Recht wie vom tiefsten Mitgef\u00fchl durchdrungen.", "tokens": ["Recht", "wie", "vom", "tiefs\u00b7ten", "Mit\u00b7ge\u00b7f\u00fchl", "durch\u00b7drun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und mich vertr\u00f6stend winkt' er mit der Hand:", "tokens": ["Und", "mich", "ver\u00b7tr\u00f6s\u00b7tend", "winkt'", "er", "mit", "der", "Hand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbgeduld, Signor! 's wird n\u00e4chstens besser gl\u00fccken!\u00ab \u2013", "tokens": ["\u00bb", "ge\u00b7duld", ",", "Sig\u00b7nor", "!", "'s", "wird", "n\u00e4chs\u00b7tens", "bes\u00b7ser", "gl\u00fc\u00b7cken", "!", "\u00ab", "\u2013"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "ADJD", "$,", "NE", "$.", "PPER", "VAFIN", "ADV", "ADJD", "VVINF", "$.", "$(", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Fast tat's mir selber leid, da\u00df es mi\u00dflungen.", "tokens": ["Fast", "tat's", "mir", "sel\u00b7ber", "leid", ",", "da\u00df", "es", "mi\u00df\u00b7lun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "$,", "KOUS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}