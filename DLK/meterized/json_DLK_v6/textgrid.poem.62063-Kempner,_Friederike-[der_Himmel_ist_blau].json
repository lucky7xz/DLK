{"textgrid.poem.62063": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[der Himmel ist blau]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Himmel ist blau,", "tokens": ["Der", "Him\u00b7mel", "ist", "blau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Die Erde so gr\u00fcn,", "tokens": ["Die", "Er\u00b7de", "so", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "O la\u00df uns ein wenig", "tokens": ["O", "la\u00df", "uns", "ein", "we\u00b7nig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ART", "PIS"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Nach S\u00fcden hin ziehn!", "tokens": ["Nach", "S\u00fc\u00b7den", "hin", "ziehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVINF", "$."], "meter": "-+---", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Dort bl\u00fchet die Myrte,", "tokens": ["Dort", "bl\u00fc\u00b7het", "die", "Myr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Orangen sind frisch,", "tokens": ["O\u00b7ran\u00b7gen", "sind", "frisch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Dort decken die Bl\u00fcten", "tokens": ["Dort", "de\u00b7cken", "die", "Bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Dir freundlich den Tisch.", "tokens": ["Dir", "freund\u00b7lich", "den", "Tisch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}