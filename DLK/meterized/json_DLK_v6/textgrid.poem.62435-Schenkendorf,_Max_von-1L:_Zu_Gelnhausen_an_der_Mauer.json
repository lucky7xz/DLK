{"textgrid.poem.62435": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zu Gelnhausen an der Mauer", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu Gelnhausen an der Mauer", "tokens": ["Zu", "Geln\u00b7hau\u00b7sen", "an", "der", "Mau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steht ein steinern altes Haupt", "tokens": ["Steht", "ein", "stei\u00b7nern", "al\u00b7tes", "Haupt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einsam in dem Haus der Trauer,", "tokens": ["Ein\u00b7sam", "in", "dem", "Haus", "der", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das der Epheu gr\u00fcn umlaubt.", "tokens": ["Das", "der", "E\u00b7pheu", "gr\u00fcn", "um\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Und das Haupt, es scheint zu sprechen:", "tokens": ["Und", "das", "Haupt", ",", "es", "scheint", "zu", "spre\u00b7chen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PPER", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Starb die ganze deutsche Welt?", "tokens": ["Starb", "die", "gan\u00b7ze", "deut\u00b7sche", "Welt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Will kein Mann die Unbill r\u00e4chen,", "tokens": ["Will", "kein", "Mann", "die", "Un\u00b7bill", "r\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis der Erde Bau zerf\u00e4llt?", "tokens": ["Bis", "der", "Er\u00b7de", "Bau", "zer\u00b7f\u00e4llt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und das Haupt, es scheint zu gr\u00fc\u00dfen", "tokens": ["Und", "das", "Haupt", ",", "es", "scheint", "zu", "gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PPER", "VVFIN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fragend uns halb streng, halb mild;", "tokens": ["Fra\u00b7gend", "uns", "halb", "streng", ",", "halb", "mild", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJD", "VVFIN", "$,", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df es uns in Demuth k\u00fcssen,", "tokens": ["La\u00df", "es", "uns", "in", "De\u00b7muth", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das ist Kaiser Friedrichs Bild.", "tokens": ["Das", "ist", "Kai\u00b7ser", "Fried\u00b7richs", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NE", "NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Herrlich hat sein Schlo\u00df gestanden", "tokens": ["Herr\u00b7lich", "hat", "sein", "Schlo\u00df", "ge\u00b7stan\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier vor langer ferner Zeit,", "tokens": ["Hier", "vor", "lan\u00b7ger", "fer\u00b7ner", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als er nach den Morgenlanden", "tokens": ["Als", "er", "nach", "den", "Mor\u00b7gen\u00b7lan\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zog in Gottes heil'gem Streit.", "tokens": ["Zog", "in", "Got\u00b7tes", "heil'\u00b7gem", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Rothbart, wie so fest gebunden", "tokens": ["Roth\u00b7bart", ",", "wie", "so", "fest", "ge\u00b7bun\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ADV", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4lt ein Zauber dich gebannt?", "tokens": ["H\u00e4lt", "ein", "Zau\u00b7ber", "dich", "ge\u00b7bannt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Flie\u00dft hier Blut aus offnen Wunden,", "tokens": ["Flie\u00dft", "hier", "Blut", "aus", "off\u00b7nen", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind das Thr\u00e4nen an der Wand?", "tokens": ["Sind", "das", "Thr\u00e4\u00b7nen", "an", "der", "Wand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Alter Herr, ich kann dir melden", "tokens": ["Al\u00b7ter", "Herr", ",", "ich", "kann", "dir", "mel\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "$,", "PPER", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Reiches, sch\u00f6nes Freudenwort.", "tokens": ["Rei\u00b7ches", ",", "sch\u00f6\u00b7nes", "Freu\u00b7den\u00b7wort", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schau, dort zielen viel tausend Helden", "tokens": ["Schau", ",", "dort", "zie\u00b7len", "viel", "tau\u00b7send", "Hel\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VVFIN", "ADV", "CARD", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "In die Schlachten Gottes fort.", "tokens": ["In", "die", "Schlach\u00b7ten", "Got\u00b7tes", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und die Welschen sind geschlagen,", "tokens": ["Und", "die", "Wel\u00b7schen", "sind", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und es siegt das heil'ge Kreuz,", "tokens": ["Und", "es", "siegt", "das", "heil'\u00b7ge", "Kreuz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wieder kehrt aus deinen Tagen", "tokens": ["Wie\u00b7der", "kehrt", "aus", "dei\u00b7nen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lebensf\u00fclle, Lebensreiz.", "tokens": ["Le\u00b7bens\u00b7f\u00fcl\u00b7le", ",", "Le\u00b7bens\u00b7reiz", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Magst nun dich zur Ruhe legen,", "tokens": ["Magst", "nun", "dich", "zur", "Ru\u00b7he", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Altes stolzes Kaiserhaupt,", "tokens": ["Al\u00b7tes", "stol\u00b7zes", "Kai\u00b7ser\u00b7haupt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine Kraft, dein Waffensegen", "tokens": ["Dei\u00b7ne", "Kraft", ",", "dein", "Waf\u00b7fen\u00b7se\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird uns nimmermehr geraubt! \u2013", "tokens": ["Wird", "uns", "nim\u00b7mer\u00b7mehr", "ge\u00b7raubt", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}