{"textgrid.poem.54780": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "94. Auff Claudi\u00e6 Hintritt", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In dem die m\u00fcde Welt ihr Jubel-Jahr beginnt/", "tokens": ["In", "dem", "die", "m\u00fc\u00b7de", "Welt", "ihr", "Ju\u00b7bel\u00b7Jahr", "be\u00b7ginnt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Werd ich von S\u00fcnden frey doch meine Zeitt zerrinnt", "tokens": ["Werd", "ich", "von", "S\u00fcn\u00b7den", "frey", "doch", "mei\u00b7ne", "Zeitt", "zer\u00b7rinnt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "NN", "ADJD", "ADV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit ihr Schmertz/ Furcht und Ach/ und Weh und Angst und Streitt/", "tokens": ["Mit", "ihr", "Schmertz", "/", "Furcht", "und", "Ach", "/", "und", "Weh", "und", "Angst", "und", "Streitt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "NN", "KON", "NN", "$(", "KON", "NN", "KON", "NN", "KON", "NN", "$("], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Was jubilirt die Welt die noch in Dinstbarkeitt.", "tokens": ["Was", "ju\u00b7bi\u00b7lirt", "die", "Welt", "die", "noch", "in", "Dinst\u00b7bar\u00b7keitt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ART", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}