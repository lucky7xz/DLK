{"dta.poem.13923": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "l .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Das ewig Alt' und Neue,", "tokens": ["Das", "e\u00b7wig", "Alt'", "und", "Neu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "NN", "KON", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das mit den Zeiten ringt,", "tokens": ["Das", "mit", "den", "Zei\u00b7ten", "ringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das, F\u00fcrst, ist's, was das treue", "tokens": ["Das", ",", "F\u00fcrst", ",", "ist's", ",", "was", "das", "treu\u00b7e"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PDS", "$,", "NN", "$,", "VAFIN", "$,", "PRELS", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Herz Deines Volts durchdringt.", "tokens": ["Herz", "Dei\u00b7nes", "Volts", "durch\u00b7dringt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}