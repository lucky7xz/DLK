{"textgrid.poem.41825": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom Schwarzwald wie ein Silberstreifen,", "tokens": ["Vom", "Schwarz\u00b7wald", "wie", "ein", "Sil\u00b7ber\u00b7strei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die blauen Donauwellen eilen \u2013", "tokens": ["Die", "blau\u00b7en", "Do\u00b7nau\u00b7wel\u00b7len", "ei\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Fast lockt's, die Blumen selbst zu greifen,", "tokens": ["Fast", "lockt's", ",", "die", "Blu\u00b7men", "selbst", "zu", "grei\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die doch am andern Ufer weilen,", "tokens": ["Die", "doch", "am", "an\u00b7dern", "U\u00b7fer", "wei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So ruhig wogt sie hin, so schmal,", "tokens": ["So", "ru\u00b7hig", "wogt", "sie", "hin", ",", "so", "schmal", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durch Wald und Fels im engen Thal.", "tokens": ["Durch", "Wald", "und", "Fels", "im", "en\u00b7gen", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dort wo auf hohen Bergesr\u00fccken", "tokens": ["Dort", "wo", "auf", "ho\u00b7hen", "Ber\u00b7ge\u00b7sr\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Viel alte Burgruinen stehen,", "tokens": ["Viel", "al\u00b7te", "Bur\u00b7gru\u00b7i\u00b7nen", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zum Schrecken bald, bald im Begl\u00fccken", "tokens": ["Zum", "Schre\u00b7cken", "bald", ",", "bald", "im", "Be\u00b7gl\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "$,", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zum jungen Strom herniedersehen,", "tokens": ["Zum", "jun\u00b7gen", "Strom", "her\u00b7nie\u00b7der\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da h\u00f6rt ich einst im Bl\u00fcthenhag", "tokens": ["Da", "h\u00f6rt", "ich", "einst", "im", "Bl\u00fc\u00b7then\u00b7hag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die steile H\u00f6he war erklommen,", "tokens": ["Die", "stei\u00b7le", "H\u00f6\u00b7he", "war", "er\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Abgrund schaut ich schwindelnd nieder,", "tokens": ["Zum", "Ab\u00b7grund", "schaut", "ich", "schwin\u00b7delnd", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da hab' in T\u00f6nen ich vernommen", "tokens": ["Da", "hab'", "in", "T\u00f6\u00b7nen", "ich", "ver\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Echo alter Minnelieder", "tokens": ["Das", "E\u00b7cho", "al\u00b7ter", "Min\u00b7ne\u00b7lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die einstens sang zum Harfenschlag", "tokens": ["Die", "eins\u00b7tens", "sang", "zum", "Har\u00b7fen\u00b7schlag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Herr Hug und Ott von Werawag,", "tokens": ["Herr", "Hug", "und", "Ott", "von", "We\u00b7ra\u00b7wag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Das waren edle Minnes\u00e4nger,", "tokens": ["Das", "wa\u00b7ren", "ed\u00b7le", "Min\u00b7ne\u00b7s\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die einst auf dieser Burg geboren,", "tokens": ["Die", "einst", "auf", "die\u00b7ser", "Burg", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Saitenspiel, je mehr, je l\u00e4nger,", "tokens": ["Das", "Sai\u00b7ten\u00b7spiel", ",", "je", "mehr", ",", "je", "l\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "$,", "ADV", "ADJD", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Statt rauhen Waffendienst erkoren,", "tokens": ["Statt", "rau\u00b7hen", "Waf\u00b7fen\u00b7dienst", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Erst leis' dann lauter sang danach,", "tokens": ["Erst", "leis'", "dann", "lau\u00b7ter", "sang", "da\u00b7nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ADJD", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Nachtigall von Werawag.", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "von", "We\u00b7ra\u00b7wag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ein M\u00e4gdlein, eine stolze Sch\u00f6ne,", "tokens": ["Ein", "M\u00e4gd\u00b7lein", ",", "ei\u00b7ne", "stol\u00b7ze", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem adligem Geschlecht entsprossen,", "tokens": ["Dem", "ad\u00b7li\u00b7gem", "Ge\u00b7schlecht", "ent\u00b7spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie h\u00f6rte fr\u00fch der Harfen T\u00f6ne,", "tokens": ["Sie", "h\u00f6r\u00b7te", "fr\u00fch", "der", "Har\u00b7fen", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Harfner waren ihr Genossen,", "tokens": ["Die", "Harf\u00b7ner", "wa\u00b7ren", "ihr", "Ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Als Ahnen standen sie ihr nah.", "tokens": ["Als", "Ah\u00b7nen", "stan\u00b7den", "sie", "ihr", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wars' da ein Wunder, was geschah?", "tokens": ["War\u00b7s'", "da", "ein", "Wun\u00b7der", ",", "was", "ge\u00b7schah", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "PWS", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "L\u00fctgarde diente selbst dem Sange", "tokens": ["L\u00fct\u00b7gar\u00b7de", "dien\u00b7te", "selbst", "dem", "San\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der Minne, den sie fr\u00fch vernommen,", "tokens": ["Der", "Min\u00b7ne", ",", "den", "sie", "fr\u00fch", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gehorchend einem s\u00fc\u00dfen Drange,", "tokens": ["Ge\u00b7hor\u00b7chend", "ei\u00b7nem", "s\u00fc\u00b7\u00dfen", "Dran\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der machtvoll in ihr Herz gekommen,", "tokens": ["Der", "macht\u00b7voll", "in", "ihr", "Herz", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So hie\u00df sie denn seit diesem Tag:", "tokens": ["So", "hie\u00df", "sie", "denn", "seit", "die\u00b7sem", "Tag", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Nachtigall von Werawag.", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "von", "We\u00b7ra\u00b7wag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Du M\u00e4dchenherz aus alten Zeiten,", "tokens": ["Du", "M\u00e4d\u00b7chen\u00b7herz", "aus", "al\u00b7ten", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Lob um Minnesang und Minne", "tokens": ["Dein", "Lob", "um", "Min\u00b7ne\u00b7sang", "und", "Min\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Will Dir ein ander Weib bereiten!", "tokens": ["Will", "Dir", "ein", "an\u00b7der", "Weib", "be\u00b7rei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn es kommt nicht aus meinem Sinne,", "tokens": ["Denn", "es", "kommt", "nicht", "aus", "mei\u00b7nem", "Sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was mir von Dir die Donau sprach,", "tokens": ["Was", "mir", "von", "Dir", "die", "Do\u00b7nau", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPER", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du Nachtigall von Werawag!", "tokens": ["Du", "Nach\u00b7ti\u00b7gall", "von", "We\u00b7ra\u00b7wag", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}