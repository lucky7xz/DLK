{"textgrid.poem.36301": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Schirin", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie Sterne leuchtend in der blauen Luft,", "tokens": ["Wie", "Ster\u00b7ne", "leuch\u00b7tend", "in", "der", "blau\u00b7en", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Berauschend wie des Fr\u00fchlings Blumenduft,", "tokens": ["Be\u00b7rau\u00b7schend", "wie", "des", "Fr\u00fch\u00b7lings", "Blu\u00b7men\u00b7duft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KOKOM", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von Nelken, Rosen und Jasmin;", "tokens": ["Von", "Nel\u00b7ken", ",", "Ro\u00b7sen", "und", "Jas\u00b7min", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NE", "$."], "meter": "-+-+-+++", "measure": "unknown.measure.penta"}, "line.4": {"text": "S\u00fc\u00df wie beim Mond das Lied der Nachtigall,", "tokens": ["S\u00fc\u00df", "wie", "beim", "Mond", "das", "Lied", "der", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "APPRART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In Sommernacht der ferne Wasserfall;", "tokens": ["In", "Som\u00b7mer\u00b7nacht", "der", "fer\u00b7ne", "Was\u00b7ser\u00b7fall", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So malt die Lieb' und strahlt Schirin.", "tokens": ["So", "malt", "die", "Lieb'", "und", "strahlt", "Schi\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}}}}