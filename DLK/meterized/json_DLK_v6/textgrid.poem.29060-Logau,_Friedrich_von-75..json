{"textgrid.poem.29060": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "75.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df mehr als Hurerey,", "tokens": ["Da\u00df", "mehr", "als", "Hu\u00b7re\u00b7rey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "KOKOM", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df L\u00fcgen S\u00fcnde sey,", "tokens": ["Da\u00df", "L\u00fc\u00b7gen", "S\u00fcn\u00b7de", "sey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "K\u00fcmmt her, weil dieses fuhr", "tokens": ["K\u00fcmmt", "her", ",", "weil", "die\u00b7ses", "fuhr"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "KOUS", "PDS", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Gar wider die Natur,", "tokens": ["Gar", "wi\u00b7der", "die", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und jenes in gemein", "tokens": ["Und", "je\u00b7nes", "in", "ge\u00b7mein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDS", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Nat\u00fcrlich pflegt zu seyn.", "tokens": ["Na\u00b7t\u00fcr\u00b7lich", "pflegt", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}