{"textgrid.poem.65933": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "An Homer", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gedichteter, noch m\u00e4chtiger, weil freier", "tokens": ["Ge\u00b7dich\u00b7te\u00b7ter", ",", "noch", "m\u00e4ch\u00b7ti\u00b7ger", ",", "weil", "frei\u00b7er"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "ADV", "ADJA", "$,", "KOUS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als der Kronide, Herr im gro\u00dfen Meer,", "tokens": ["Als", "der", "Kro\u00b7ni\u00b7de", ",", "Herr", "im", "gro\u00b7\u00dfen", "Meer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Umpl\u00e4tschre uns, Bes\u00e4nftiger: Homer,", "tokens": ["Um\u00b7pl\u00e4tschre", "uns", ",", "Be\u00b7s\u00e4nf\u00b7ti\u00b7ger", ":", "Ho\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$.", "NE", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Dir sei, in Hellas, blauer Tag die Feier!", "tokens": ["Dir", "sei", ",", "in", "Hel\u00b7las", ",", "blau\u00b7er", "Tag", "die", "Fei\u00b7er", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "APPR", "NE", "$,", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Drei Musen lobten, zu Apollos Leier,", "tokens": ["Drei", "Mu\u00b7sen", "lob\u00b7ten", ",", "zu", "A\u00b7pol\u00b7los", "Lei\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "$,", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der M\u00e4nner heldenhaften Kampfbegehr,", "tokens": ["Der", "M\u00e4n\u00b7ner", "hel\u00b7den\u00b7haf\u00b7ten", "Kampf\u00b7be\u00b7gehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Athena bracht ihn dem Achajerheer", "tokens": ["A\u00b7the\u00b7na", "bracht", "ihn", "dem", "A\u00b7cha\u00b7jer\u00b7heer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Und dachte, bei Atriden gl\u00fch, gedeih er!", "tokens": ["Und", "dach\u00b7te", ",", "bei", "At\u00b7ri\u00b7den", "gl\u00fch", ",", "ge\u00b7deih", "er", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "NN", "ADJD", "$,", "VVIMP", "PPER", "$."], "meter": "-+---+-+-+-", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Homer, du hast Achill zur Schlacht erkoren,", "tokens": ["Ho\u00b7mer", ",", "du", "hast", "A\u00b7chill", "zur", "Schlacht", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "NE", "APPRART", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Warst seine Stimme durch der Zelte Flucht,", "tokens": ["Warst", "sei\u00b7ne", "Stim\u00b7me", "durch", "der", "Zel\u00b7te", "Flucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Ein Schrei der Seherin vor Ilions Toren;", "tokens": ["Ein", "Schrei", "der", "Se\u00b7he\u00b7rin", "vor", "I\u00b7li\u00b7ons", "To\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Dein Rufen sch\u00fcrte der Verwegnen Wucht,", "tokens": ["Dein", "Ru\u00b7fen", "sch\u00fcr\u00b7te", "der", "Ver\u00b7weg\u00b7nen", "Wucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Ruhm, in heilem Sang zum Tod, erkoren:", "tokens": ["Die", "Ruhm", ",", "in", "hei\u00b7lem", "Sang", "zum", "Tod", ",", "er\u00b7ko\u00b7ren", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ADJA", "NN", "APPRART", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann ruhtest du \u2013 wie Meer \u2013 in runder Bucht.", "tokens": ["Dann", "ruh\u00b7test", "du", "\u2013", "wie", "Meer", "\u2013", "in", "run\u00b7der", "Bucht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOKOM", "NN", "$(", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}