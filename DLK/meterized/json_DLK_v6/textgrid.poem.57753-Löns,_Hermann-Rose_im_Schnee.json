{"textgrid.poem.57753": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Rose im Schnee", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rose wei\u00df, Rose rot,", "tokens": ["Ro\u00b7se", "wei\u00df", ",", "Ro\u00b7se", "rot", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "NE", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Wie s\u00fc\u00df ist doch dein Mund,", "tokens": ["Wie", "s\u00fc\u00df", "ist", "doch", "dein", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Rose rot, Rose wei\u00df,", "tokens": ["Ro\u00b7se", "rot", ",", "Ro\u00b7se", "wei\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "NE", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Dein denk ich alle Stund,", "tokens": ["Dein", "denk", "ich", "al\u00b7le", "Stund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Alle Stund bei Tag und Nacht,", "tokens": ["Al\u00b7le", "Stund", "bei", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df dein Mund mir zugelacht,", "tokens": ["Da\u00df", "dein", "Mund", "mir", "zu\u00b7ge\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dein roter Mund.", "tokens": ["Dein", "ro\u00b7ter", "Mund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ein Vogel sang im Lindenbaum,", "tokens": ["Ein", "Vo\u00b7gel", "sang", "im", "Lin\u00b7den\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein s\u00fc\u00dfes Lied er sang,", "tokens": ["Ein", "s\u00fc\u00b7\u00dfes", "Lied", "er", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Rose wei\u00df, Rose rot,", "tokens": ["Ro\u00b7se", "wei\u00df", ",", "Ro\u00b7se", "rot", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "NE", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Das Herz im Leib mir sprang,", "tokens": ["Das", "Herz", "im", "Leib", "mir", "sprang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sprang vor Freude hin und her,", "tokens": ["Sprang", "vor", "Freu\u00b7de", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Als ob dein Lachen bei ihm w\u00e4r,", "tokens": ["Als", "ob", "dein", "La\u00b7chen", "bei", "ihm", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPOSAT", "NN", "APPR", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So s\u00fc\u00df es klang.", "tokens": ["So", "s\u00fc\u00df", "es", "klang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Rose wei\u00df, Rose rot,", "tokens": ["Ro\u00b7se", "wei\u00df", ",", "Ro\u00b7se", "rot", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "NE", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Was wird aus mir und dir?", "tokens": ["Was", "wird", "aus", "mir", "und", "dir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich glaube gar, es fiel ein Schnee,", "tokens": ["Ich", "glau\u00b7be", "gar", ",", "es", "fiel", "ein", "Schnee", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Herz ist nicht bei mir,", "tokens": ["Dein", "Herz", "ist", "nicht", "bei", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nicht bei mir, geht andern Gang,", "tokens": ["Nicht", "bei", "mir", ",", "geht", "an\u00b7dern", "Gang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPER", "$,", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Falsches Lied der Vogel sang", "tokens": ["Fal\u00b7sches", "Lied", "der", "Vo\u00b7gel", "sang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NE", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Von mir und dir.", "tokens": ["Von", "mir", "und", "dir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}