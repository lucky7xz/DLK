{"textgrid.poem.63752": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr z\u00e4hlt, mein sch\u00f6nes Kind, kaum vierzehn Jahr", "tokens": ["Ihr", "z\u00e4hlt", ",", "mein", "sch\u00f6\u00b7nes", "Kind", ",", "kaum", "vier\u00b7zehn", "Jahr"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,", "ADV", "CARD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und habt ein so erwachsen kluges Lachen", "tokens": ["Und", "habt", "ein", "so", "er\u00b7wach\u00b7sen", "klu\u00b7ges", "La\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "ADV", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und schwatzt so allerliebst von Liebessachen,", "tokens": ["Und", "schwatzt", "so", "al\u00b7ler\u00b7liebst", "von", "Lie\u00b7bes\u00b7sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schon aus Erfahrung, d\u00e4chte man f\u00fcrwahr.", "tokens": ["Schon", "aus", "Er\u00b7fah\u00b7rung", ",", "d\u00e4ch\u00b7te", "man", "f\u00fcr\u00b7wahr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Auch ist schon einer, oder zwei sogar,", "tokens": ["Auch", "ist", "schon", "ei\u00b7ner", ",", "o\u00b7der", "zwei", "so\u00b7gar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIS", "$,", "KON", "CARD", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit Ernst beflissen, Euch den Hof zu machen;", "tokens": ["Mit", "Ernst", "be\u00b7flis\u00b7sen", ",", "Euch", "den", "Hof", "zu", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVPP", "$,", "PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Selbst dem Verehrer Eurer eignen schwachen", "tokens": ["Selbst", "dem", "Ver\u00b7eh\u00b7rer", "Eu\u00b7rer", "eig\u00b7nen", "schwa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "PPOSAT", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mama bringt Euer \u00c4ugeln schon Gefahr.", "tokens": ["Ma\u00b7ma", "bringt", "Eu\u00b7er", "\u00c4u\u00b7geln", "schon", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Was Ihr nur tragt und tut und sprecht, hat Schick.", "tokens": ["Was", "Ihr", "nur", "tragt", "und", "tut", "und", "sprecht", ",", "hat", "Schick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "KON", "ADJD", "KON", "VVFIN", "$,", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihr habt den besten Koch, den ersten Schneider,", "tokens": ["Ihr", "habt", "den", "bes\u00b7ten", "Koch", ",", "den", "ers\u00b7ten", "Schnei\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NE", "$,", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der fr\u00f6mmste Beicht'ger sorgt f\u00fcr Eure Tugend.", "tokens": ["Der", "fr\u00f6mms\u00b7te", "Beicht'\u00b7ger", "sorgt", "f\u00fcr", "Eu\u00b7re", "Tu\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Begehrlich folgt Euch aller M\u00e4nner Blick.", "tokens": ["Be\u00b7gehr\u00b7lich", "folgt", "Euch", "al\u00b7ler", "M\u00e4n\u00b7ner", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ja, Ihr habt alles, Signorina! Leider", "tokens": ["Ja", ",", "Ihr", "habt", "al\u00b7les", ",", "Sig\u00b7no\u00b7ri\u00b7na", "!", "Lei\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "PIS", "$,", "NE", "$.", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Fehlt Euch nur eine Kleinigkeit: die Jugend.", "tokens": ["Fehlt", "Euch", "nur", "ei\u00b7ne", "Klei\u00b7nig\u00b7keit", ":", "die", "Ju\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}