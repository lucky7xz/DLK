{"textgrid.poem.62322": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[und g\u00e4b ich ihnen all' mein Blut]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und g\u00e4b ich ihnen all' mein Blut,", "tokens": ["Und", "g\u00e4b", "ich", "ih\u00b7nen", "all'", "mein", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und g\u00e4b' ich ihnen all' mein Sein,", "tokens": ["Und", "g\u00e4b'", "ich", "ih\u00b7nen", "all'", "mein", "Sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie h\u00f6ren und versteh'n mich nicht", "tokens": ["Sie", "h\u00f6\u00b7ren", "und", "ver\u00b7steh'n", "mich", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "KON", "VVFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sagen d\u00e4mlich immer nein.", "tokens": ["Und", "sa\u00b7gen", "d\u00e4m\u00b7lich", "im\u00b7mer", "nein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "PTKANT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch naht die letzte Stunde einst,", "tokens": ["Doch", "naht", "die", "letz\u00b7te", "Stun\u00b7de", "einst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der gute Engel dr\u00fcber weint,", "tokens": ["Der", "gu\u00b7te", "En\u00b7gel", "dr\u00fc\u00b7ber", "weint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kommt Angst und Reue viel zu sp\u00e4t,", "tokens": ["Kommt", "Angst", "und", "Reu\u00b7e", "viel", "zu", "sp\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "KON", "NN", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann wissen sie, wer's gut gemeint.", "tokens": ["Dann", "wis\u00b7sen", "sie", ",", "wer's", "gut", "ge\u00b7meint", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}