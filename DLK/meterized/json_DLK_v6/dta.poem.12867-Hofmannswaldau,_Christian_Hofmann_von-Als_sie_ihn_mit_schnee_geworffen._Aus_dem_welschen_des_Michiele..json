{"dta.poem.12867": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als sie ihn mit schnee geworffen.  \n Aus dem welschen des   Michiele.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dorine lie\u00df sich einst gel\u00fcsten,", "tokens": ["Do\u00b7ri\u00b7ne", "lie\u00df", "sich", "einst", "ge\u00b7l\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In aller eyl", "tokens": ["In", "al\u00b7ler", "eyl"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mit einem kalten pfeil", "tokens": ["Mit", "ei\u00b7nem", "kal\u00b7ten", "pfeil"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sich wider mich zu r\u00fcsten.", "tokens": ["Sich", "wi\u00b7der", "mich", "zu", "r\u00fcs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich lie\u00df ohn alle furcht dem wurffe freyen lauff,", "tokens": ["Ich", "lie\u00df", "ohn", "al\u00b7le", "furcht", "dem", "wurf\u00b7fe", "frey\u00b7en", "lauff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und nahm den sanfften streich mit blosem lachen auf,", "tokens": ["Und", "nahm", "den", "sanff\u00b7ten", "streich", "mit", "blo\u00b7sem", "la\u00b7chen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "VVFIN", "ADJD", "APPR", "ADJA", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Weil ich nicht glauben wolte,", "tokens": ["Weil", "ich", "nicht", "glau\u00b7ben", "wol\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df der gelinde schnee, den sie zusammen rollte,", "tokens": ["Da\u00df", "der", "ge\u00b7lin\u00b7de", "schnee", ",", "den", "sie", "zu\u00b7sam\u00b7men", "roll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Jm ernst verwunden solte;", "tokens": ["Jm", "ernst", "ver\u00b7wun\u00b7den", "sol\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Allein der glaube kam mir zeitlich in die hand,", "tokens": ["Al\u00b7lein", "der", "glau\u00b7be", "kam", "mir", "zeit\u00b7lich", "in", "die", "hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Denn ich befaud", "tokens": ["Denn", "ich", "be\u00b7faud"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Auf diesen kalten wurff, obwol mit s\u00fc\u00dfen schmertzen,", "tokens": ["Auf", "die\u00b7sen", "kal\u00b7ten", "wurff", ",", "ob\u00b7wol", "mit", "s\u00fc\u00b7\u00dfen", "schmert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,", "KOUS", "APPR", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der liebe brandmahl in dem hertzen.", "tokens": ["Der", "lie\u00b7be", "brand\u00b7mahl", "in", "dem", "hert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}