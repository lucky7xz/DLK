{"textgrid.poem.48089": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Die Ernte", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wei\u00dfe flimmernde Sonnenflut", "tokens": ["Wei\u00b7\u00dfe", "flim\u00b7mern\u00b7de", "Son\u00b7nen\u00b7flut"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "rings auf den wogenden Weiten ruht;", "tokens": ["rings", "auf", "den", "wo\u00b7gen\u00b7den", "Wei\u00b7ten", "ruht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "r\u00fcstige M\u00e4her bei scharfem Schnitt \u2013 \u2013", "tokens": ["r\u00fcs\u00b7ti\u00b7ge", "M\u00e4\u00b7her", "bei", "schar\u00b7fem", "Schnitt", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$(", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "schwirrende Sensen singen mit:", "tokens": ["schwir\u00b7ren\u00b7de", "Sen\u00b7sen", "sin\u00b7gen", "mit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Die Halme fallen.", "tokens": ["Die", "Hal\u00b7me", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Und hart am staubigen Stra\u00dfenrain", "tokens": ["Und", "hart", "am", "stau\u00b7bi\u00b7gen", "Stra\u00b7\u00dfen\u00b7rain"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "schafft tief geb\u00fcckt ein M\u00fctterlein;", "tokens": ["schafft", "tief", "ge\u00b7b\u00fcckt", "ein", "M\u00fct\u00b7ter\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "schon manche brennende Stunde lang", "tokens": ["schon", "man\u00b7che", "bren\u00b7nen\u00b7de", "Stun\u00b7de", "lang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "ADJA", "NN", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "sirrt und surrt der Sichelklang \u2013 \u2013", "tokens": ["sirrt", "und", "surrt", "der", "Si\u00b7chel\u00b7klang", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "NN", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die Halme fallen.", "tokens": ["Die", "Hal\u00b7me", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Da schaut aus schimmerndem Aehrenfeld", "tokens": ["Da", "schaut", "aus", "schim\u00b7mern\u00b7dem", "A\u00b7eh\u00b7ren\u00b7feld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "der Gutsherr auf zum Wolkenzelt:", "tokens": ["der", "Guts\u00b7herr", "auf", "zum", "Wol\u00b7ken\u00b7zelt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u2013 \u00bbVorw\u00e4rts, ihr Leute, die Stunde rinnt!", "tokens": ["\u2013", "\u00bb", "Vor\u00b7w\u00e4rts", ",", "ihr", "Leu\u00b7te", ",", "die", "Stun\u00b7de", "rinnt", "!"], "token_info": ["punct", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "$(", "ADV", "$,", "PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "In den Kl\u00fcften murrt der Gewitterwind \u2013\u00ab", "tokens": ["In", "den", "Kl\u00fcf\u00b7ten", "murrt", "der", "Ge\u00b7wit\u00b7ter\u00b7wind", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$(", "$("], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die Halme fallen.", "tokens": ["Die", "Hal\u00b7me", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Und in den perlenden Abendtau", "tokens": ["Und", "in", "den", "per\u00b7len\u00b7den", "A\u00b7bend\u00b7tau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "blickt so fr\u00f6hlich die alte Frau;", "tokens": ["blickt", "so", "fr\u00f6h\u00b7lich", "die", "al\u00b7te", "Frau", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "sie wischt von der Stirne den hellen Schwei\u00df", "tokens": ["sie", "wischt", "von", "der", "Stir\u00b7ne", "den", "hel\u00b7len", "Schwei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "und z\u00e4hlt im Geiste der Garben Preis.", "tokens": ["und", "z\u00e4hlt", "im", "Geis\u00b7te", "der", "Gar\u00b7ben", "Preis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die Halme fallen.", "tokens": ["Die", "Hal\u00b7me", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "\u2013 \u00bbVorw\u00e4rts, ihr Knechte! die Stunde rinnt!", "tokens": ["\u2013", "\u00bb", "Vor\u00b7w\u00e4rts", ",", "ihr", "Knech\u00b7te", "!", "die", "Stun\u00b7de", "rinnt", "!"], "token_info": ["punct", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "$(", "ADV", "$,", "PPOSAT", "NN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Mein Mahl bereitet das Ingesind;", "tokens": ["Mein", "Mahl", "be\u00b7rei\u00b7tet", "das", "In\u00b7ge\u00b7sind", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "mein Weib umrauscht ein seidener Flor \u2013 \u2013", "tokens": ["mein", "Weib", "um\u00b7rauscht", "ein", "sei\u00b7de\u00b7ner", "Flor", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "PPOSAT", "NN", "$(", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "und der Jude wartet am Gartentor.\u00ab", "tokens": ["und", "der", "Ju\u00b7de", "war\u00b7tet", "am", "Gar\u00b7ten\u00b7tor", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "$.", "$("], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die Halme fallen!", "tokens": ["Die", "Hal\u00b7me", "fal\u00b7len", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Und m\u00fcde legt nach des Tages Brand", "tokens": ["Und", "m\u00fc\u00b7de", "legt", "nach", "des", "Ta\u00b7ges", "Brand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "das Weib die Sichel aus der Hand:", "tokens": ["das", "Weib", "die", "Si\u00b7chel", "aus", "der", "Hand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbdu goldner Segen auf schmalem Feld,", "tokens": ["\u00bb", "du", "gold\u00b7ner", "Se\u00b7gen", "auf", "schma\u00b7lem", "Feld", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "du gibst mir Brot und du schaffst mir Geld!\u00ab \u2013", "tokens": ["du", "gibst", "mir", "Brot", "und", "du", "schaffst", "mir", "Geld", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "PPER", "VVFIN", "PPER", "NN", "$.", "$(", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die Halme fallen.", "tokens": ["Die", "Hal\u00b7me", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "\u2013 \u00bbVorw\u00e4rts, ihr Hunde, verdient den Lohn!\u00ab", "tokens": ["\u2013", "\u00bb", "Vor\u00b7w\u00e4rts", ",", "ihr", "Hun\u00b7de", ",", "ver\u00b7dient", "den", "Lohn", "!", "\u00ab"], "token_info": ["punct", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "$(", "ADV", "$,", "PPOSAT", "NN", "$,", "VVFIN", "ART", "NN", "$.", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Er denkt an seinen fernen Sohn.", "tokens": ["Er", "denkt", "an", "sei\u00b7nen", "fer\u00b7nen", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der schnellste Reiter auf blachem Feld", "tokens": ["Der", "schnells\u00b7te", "Rei\u00b7ter", "auf", "bla\u00b7chem", "Feld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+----+", "measure": "unknown.measure.tri"}, "line.4": {"text": "und der Gott der Weiber \u2013 das kostet Geld! \u2013", "tokens": ["und", "der", "Gott", "der", "Wei\u00b7ber", "\u2013", "das", "kos\u00b7tet", "Geld", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$(", "PDS", "VVFIN", "NN", "$.", "$("], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die Halme fallen.", "tokens": ["Die", "Hal\u00b7me", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "\u00bbund all das Gold\u00ab \u2013 die Alte sinnt \u2013", "tokens": ["\u00bb", "und", "all", "das", "Gold", "\u00ab", "\u2013", "die", "Al\u00b7te", "sinnt", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PIAT", "ART", "NN", "$(", "$(", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbin die Ferne schickt ich's dem einzigen Kind.", "tokens": ["\u00bb", "in", "die", "Fer\u00b7ne", "schickt", "ich's", "dem", "ein\u00b7zi\u00b7gen", "Kind", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "VVFIN", "PIS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Sie trieben ihn fort von Haus und Huf,", "tokens": ["Sie", "trie\u00b7ben", "ihn", "fort", "von", "Haus", "und", "Huf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "nun harrt er dr\u00fcben der Heimat Ruf:", "tokens": ["nun", "harrt", "er", "dr\u00fc\u00b7ben", "der", "Hei\u00b7mat", "Ruf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die Halme fallen.", "tokens": ["Die", "Hal\u00b7me", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Und kehrt er heim, wenn der Himmel loht,", "tokens": ["Und", "kehrt", "er", "heim", ",", "wenn", "der", "Him\u00b7mel", "loht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "wenn der Weizen reif und das Mohnfeld rot,", "tokens": ["wenn", "der", "Wei\u00b7zen", "reif", "und", "das", "Mohn\u00b7feld", "rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "KON", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "dann fa\u00dft er die Sense zu hei\u00dfem Schnitt \u2013", "tokens": ["dann", "fa\u00dft", "er", "die", "Sen\u00b7se", "zu", "hei\u00b7\u00dfem", "Schnitt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "und ich laufe und sammle und jauchze mit:", "tokens": ["und", "ich", "lau\u00b7fe", "und", "samm\u00b7le", "und", "jauch\u00b7ze", "mit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "PTKVZ", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "\u00bbdie Halme fallen!\u00ab", "tokens": ["\u00bb", "die", "Hal\u00b7me", "fal\u00b7len", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}}}}}