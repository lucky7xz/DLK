{"textgrid.poem.25009": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Seeadler", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wob, K\u00f6nig der L\u00fcfte, f\u00fcr deinen Flug", "tokens": ["Wob", ",", "K\u00f6\u00b7nig", "der", "L\u00fcf\u00b7te", ",", "f\u00fcr", "dei\u00b7nen", "Flug"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "NN", "ART", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der Sturm dir die Schwingen, die wei\u00dfen,", "tokens": ["Der", "Sturm", "dir", "die", "Schwin\u00b7gen", ",", "die", "wei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Da\u00df sie geschwind, wie ein Atemzug,", "tokens": ["Da\u00df", "sie", "ge\u00b7schwind", ",", "wie", "ein", "A\u00b7tem\u00b7zug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vom Meer gen Himmel dich rei\u00dfen?", "tokens": ["Vom", "Meer", "gen", "Him\u00b7mel", "dich", "rei\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Hat dir die Sonne das Auge gefeit,", "tokens": ["Hat", "dir", "die", "Son\u00b7ne", "das", "Au\u00b7ge", "ge\u00b7feit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Da\u00df du nicht droben erblindest,", "tokens": ["Da\u00df", "du", "nicht", "dro\u00b7ben", "er\u00b7blin\u00b7dest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Wenn du in blauer Unendlichkeit", "tokens": ["Wenn", "du", "in", "blau\u00b7er", "Un\u00b7end\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Dem Sehrohr selber entschwindest?", "tokens": ["Dem", "Seh\u00b7rohr", "sel\u00b7ber", "ent\u00b7schwin\u00b7dest", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Hoch, hoch, wo der Alpen m\u00e4chtigste Piks", "tokens": ["Hoch", ",", "hoch", ",", "wo", "der", "Al\u00b7pen", "m\u00e4ch\u00b7tigs\u00b7te", "Piks"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "$,", "PWAV", "ART", "NN", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In D\u00e4mmernebel verschwinden,", "tokens": ["In", "D\u00e4m\u00b7mer\u00b7ne\u00b7bel", "ver\u00b7schwin\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hinunter sp\u00e4hst du leuchtenden Blicks", "tokens": ["Hin\u00b7un\u00b7ter", "sp\u00e4hst", "du", "leuch\u00b7ten\u00b7den", "Blicks"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Zu des Weltalls g\u00e4hnenden Schl\u00fcnden;", "tokens": ["Zu", "des", "Welt\u00b7alls", "g\u00e4h\u00b7nen\u00b7den", "Schl\u00fcn\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und siehst von deiner himmlischen Wacht", "tokens": ["Und", "siehst", "von", "dei\u00b7ner", "himm\u00b7li\u00b7schen", "Wacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Jenseits von der Erde Grenzen", "tokens": ["Jen\u00b7seits", "von", "der", "Er\u00b7de", "Gren\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Den Tag, der Abend nicht kennt noch Nacht,", "tokens": ["Den", "Tag", ",", "der", "A\u00b7bend", "nicht", "kennt", "noch", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PTKNEG", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Den unverg\u00e4nglichen gl\u00e4nzen.", "tokens": ["Den", "un\u00b7ver\u00b7g\u00e4ng\u00b7li\u00b7chen", "gl\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Wenn wirbelnd daher das Gewitter saust", "tokens": ["Wenn", "wir\u00b7belnd", "da\u00b7her", "das", "Ge\u00b7wit\u00b7ter", "saust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "PAV", "ART", "NN", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und aus unterstem Oceane", "tokens": ["Und", "aus", "un\u00b7ters\u00b7tem", "O\u00b7cea\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Flut aufpeitscht, da\u00df sie himmelan braust,", "tokens": ["Die", "Flut", "auf\u00b7peitscht", ",", "da\u00df", "sie", "him\u00b7me\u00b7lan", "braust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wiegst du dich auf dem Orkane;", "tokens": ["Wiegst", "du", "dich", "auf", "dem", "Or\u00b7ka\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Und ob in den Wellen, zu Bergen get\u00fcrmt,", "tokens": ["Und", "ob", "in", "den", "Wel\u00b7len", ",", "zu", "Ber\u00b7gen", "ge\u00b7t\u00fcrmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "ART", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Auch ganze Flotten versinken,", "tokens": ["Auch", "gan\u00b7ze", "Flot\u00b7ten", "ver\u00b7sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Du jubelst, wo es am wildesten st\u00fcrmt,", "tokens": ["Du", "ju\u00b7belst", ",", "wo", "es", "am", "wil\u00b7des\u00b7ten", "st\u00fcrmt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Der Windsbraut Odem zu trinken.", "tokens": ["Der", "Winds\u00b7braut", "O\u00b7dem", "zu", "trin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Das Fr\u00fchrot bleibt, das purpurnen Saums", "tokens": ["Das", "Fr\u00fch\u00b7rot", "bleibt", ",", "das", "pur\u00b7pur\u00b7nen", "Saums"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Aufsteigt ob Meeren und L\u00e4ndern,", "tokens": ["Auf\u00b7steigt", "ob", "Mee\u00b7ren", "und", "L\u00e4n\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KOUS", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Matt hinter dir, Beherrscher des Raums,", "tokens": ["Matt", "hin\u00b7ter", "dir", ",", "Be\u00b7herr\u00b7scher", "des", "Raums", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Zur\u00fcck an den Himmelsr\u00e4ndern;", "tokens": ["Zu\u00b7r\u00fcck", "an", "den", "Him\u00b7mels\u00b7r\u00e4n\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ans Nordkap h\u00f6rtest du wilden Schlags", "tokens": ["Ans", "Nord\u00b7kap", "h\u00f6r\u00b7test", "du", "wil\u00b7den", "Schlags"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Bei Nacht die Wogen noch branden", "tokens": ["Bei", "Nacht", "die", "Wo\u00b7gen", "noch", "bran\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN", "ADV", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und gr\u00fc\u00dfest den Strahl des werdenden Tags", "tokens": ["Und", "gr\u00fc\u00b7\u00dfest", "den", "Strahl", "des", "wer\u00b7den\u00b7den", "Tags"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Schon hoch vom Gipfel der Anden.", "tokens": ["Schon", "hoch", "vom", "Gip\u00b7fel", "der", "An\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Wie dir \u2013 o lang versunkene Zeit! \u2013", "tokens": ["Wie", "dir", "\u2013", "o", "lang", "ver\u00b7sun\u00b7ke\u00b7ne", "Zeit", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "$(", "FM", "ADJD", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Einst wollte zu ihren Fl\u00fcgen", "tokens": ["Einst", "woll\u00b7te", "zu", "ih\u00b7ren", "Fl\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Des Raumes weite Unendlichkeit", "tokens": ["Des", "Rau\u00b7mes", "wei\u00b7te", "Un\u00b7end\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Kaum meiner Seele gen\u00fcgen;", "tokens": ["Kaum", "mei\u00b7ner", "See\u00b7le", "ge\u00b7n\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Nun seufzt sie, gebeugt vom niederen Joch,", "tokens": ["Nun", "seufzt", "sie", ",", "ge\u00b7beugt", "vom", "nie\u00b7de\u00b7ren", "Joch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "In des Lebens finsterer Enge;", "tokens": ["In", "des", "Le\u00b7bens", "fins\u00b7te\u00b7rer", "En\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Ach! da\u00df sie nur einmal jubelnd noch", "tokens": ["Ach", "!", "da\u00df", "sie", "nur", "ein\u00b7mal", "ju\u00b7belnd", "noch"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "KOUS", "PPER", "ADV", "ADV", "ADJD", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "In den leuchtenden Aether sich schw\u00e4nge!", "tokens": ["In", "den", "leuch\u00b7ten\u00b7den", "A\u00b7e\u00b7ther", "sich", "schw\u00e4n\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "--+---+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "In durstigen Z\u00fcgen, voll und stark,", "tokens": ["In", "durs\u00b7ti\u00b7gen", "Z\u00fc\u00b7gen", ",", "voll", "und", "stark", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Luft des Himmels zu schl\u00fcrfen,", "tokens": ["Die", "Luft", "des", "Him\u00b7mels", "zu", "schl\u00fcr\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hinab zu der Sch\u00f6pfung entlegenster Mark", "tokens": ["Hin\u00b7ab", "zu", "der", "Sch\u00f6p\u00b7fung", "ent\u00b7le\u00b7gens\u00b7ter", "Mark"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die Blicke senden zu d\u00fcrfen \u2013", "tokens": ["Die", "Bli\u00b7cke", "sen\u00b7den", "zu", "d\u00fcr\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "PTKZU", "VMINF", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "O Adler! dir neid' ich den seligen Tod,", "tokens": ["O", "Ad\u00b7ler", "!", "dir", "neid'", "ich", "den", "se\u00b7li\u00b7gen", "Tod", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Der dir dort oben bereitet,", "tokens": ["Der", "dir", "dort", "o\u00b7ben", "be\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Wenn die ewige Sonne ihr gl\u00fchendes Rot", "tokens": ["Wenn", "die", "e\u00b7wi\u00b7ge", "Son\u00b7ne", "ihr", "gl\u00fc\u00b7hen\u00b7des", "Rot"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.8": {"text": "Um die brechenden Schwingen dir breitet!", "tokens": ["Um", "die", "bre\u00b7chen\u00b7den", "Schwin\u00b7gen", "dir", "brei\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}}}}