{"textgrid.poem.59246": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Zorn", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seh ich im verfallnen, dunkeln", "tokens": ["Seh", "ich", "im", "ver\u00b7fall\u00b7nen", ",", "dun\u00b7keln"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Haus die alten Waffen hangen,", "tokens": ["Haus", "die", "al\u00b7ten", "Waf\u00b7fen", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zornig aus dem Roste funkeln,", "tokens": ["Zor\u00b7nig", "aus", "dem", "Ros\u00b7te", "fun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn der Morgen aufgegangen,", "tokens": ["Wenn", "der", "Mor\u00b7gen", "auf\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und den letzten Klang verflogen,", "tokens": ["Und", "den", "letz\u00b7ten", "Klang", "ver\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo im wilden Zug der Wetter,", "tokens": ["Wo", "im", "wil\u00b7den", "Zug", "der", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aufs gekreuzte Schwert gebogen,", "tokens": ["Aufs", "ge\u00b7kreuz\u00b7te", "Schwert", "ge\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einst gehaust des Landes Retter;", "tokens": ["Einst", "ge\u00b7haust", "des", "Lan\u00b7des", "Ret\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und ein neu Geschlecht von Zwergen", "tokens": ["Und", "ein", "neu", "Ge\u00b7schlecht", "von", "Zwer\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwindelnd um die Felsen klettern,", "tokens": ["Schwin\u00b7delnd", "um", "die", "Fel\u00b7sen", "klet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Frech, wenn's sonnig auf den Bergen,", "tokens": ["Frech", ",", "wenn's", "son\u00b7nig", "auf", "den", "Ber\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Feige kr\u00fcmmend sich in Wettern,", "tokens": ["Fei\u00b7ge", "kr\u00fcm\u00b7mend", "sich", "in", "Wet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ihres Heilands Blut und Tr\u00e4nen", "tokens": ["Ih\u00b7res", "Hei\u00b7lands", "Blut", "und", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spottend noch einmal verkaufen,", "tokens": ["Spot\u00b7tend", "noch", "ein\u00b7mal", "ver\u00b7kau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ohne Klage, Wunsch und Sehnen", "tokens": ["Oh\u00b7ne", "Kla\u00b7ge", ",", "Wunsch", "und", "Seh\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Zeiten Strom ersaufen;", "tokens": ["In", "der", "Zei\u00b7ten", "Strom", "er\u00b7sau\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Denk ich dann, wie du gestanden", "tokens": ["Denk", "ich", "dann", ",", "wie", "du", "ge\u00b7stan\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "$,", "PWAV", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Treu, da niemand treu geblieben:", "tokens": ["Treu", ",", "da", "nie\u00b7mand", "treu", "ge\u00b7blie\u00b7ben", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PIS", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00f6cht ich, \u00fcber unsre Schande", "tokens": ["M\u00f6cht", "ich", ",", "\u00fc\u00b7ber", "uns\u00b7re", "Schan\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tiefentbrannt in zorn'gem Lieben,", "tokens": ["Tie\u00b7fent\u00b7brannt", "in", "zorn'\u00b7gem", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wurzeln in der Felsen Marke,", "tokens": ["Wur\u00b7zeln", "in", "der", "Fel\u00b7sen", "Mar\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und empor zu Himmelslichten", "tokens": ["Und", "em\u00b7por", "zu", "Him\u00b7mels\u00b7lich\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PTKVZ", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Stumm anstrebend, wie die starke", "tokens": ["Stumm", "an\u00b7stre\u00b7bend", ",", "wie", "die", "star\u00b7ke"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVPP", "$,", "PWAV", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Riesentanne, mich aufrichten.", "tokens": ["Rie\u00b7sen\u00b7tan\u00b7ne", ",", "mich", "auf\u00b7rich\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}