{"dta.poem.19301": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "34.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was unterscheidet dich, o Mensch, von Thier und Pflanze?", "tokens": ["Was", "un\u00b7ter\u00b7schei\u00b7det", "dich", ",", "o", "Mensch", ",", "von", "Thier", "und", "Pflan\u00b7ze", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "FM", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df du f\u00fcr dich auch bist, nicht blos wie sie f\u00fcrs Ganze.", "tokens": ["Da\u00df", "du", "f\u00fcr", "dich", "auch", "bist", ",", "nicht", "blos", "wie", "sie", "f\u00fcrs", "Gan\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADV", "VAFIN", "$,", "PTKNEG", "ADV", "KOKOM", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "F\u00fcrs Ganze bist auch du, wie Thier und Pflanze sind,", "tokens": ["F\u00fcrs", "Gan\u00b7ze", "bist", "auch", "du", ",", "wie", "Thier", "und", "Pflan\u00b7ze", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "ADV", "PPER", "$,", "PWAV", "NN", "KON", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch bist du's nicht wie sie, du selbstbewust, sie blind.", "tokens": ["Doch", "bist", "du's", "nicht", "wie", "sie", ",", "du", "selbst\u00b7be\u00b7wust", ",", "sie", "blind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "PTKNEG", "PWAV", "PPER", "$,", "PPER", "VVFIN", "$,", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sie sind f\u00fcrs Ganze nur, weil sie nur sind f\u00fcr sich;", "tokens": ["Sie", "sind", "f\u00fcrs", "Gan\u00b7ze", "nur", ",", "weil", "sie", "nur", "sind", "f\u00fcr", "sich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ADV", "$,", "KOUS", "PPER", "ADV", "VAFIN", "APPR", "PRF", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Weil du f\u00fcr's Ganze bist, sind sie und es f\u00fcr dich.", "tokens": ["Weil", "du", "f\u00fcr's", "Gan\u00b7ze", "bist", ",", "sind", "sie", "und", "es", "f\u00fcr", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VAFIN", "$,", "VAFIN", "PPER", "KON", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "F\u00fcrs Ganze bist du ganz, wenn ganz f\u00fcr dich du bist,", "tokens": ["F\u00fcrs", "Gan\u00b7ze", "bist", "du", "ganz", ",", "wenn", "ganz", "f\u00fcr", "dich", "du", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "PPER", "ADV", "$,", "KOUS", "ADV", "APPR", "PPER", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erkennend, da\u00df durch dich das Ganze ganz nur ist.", "tokens": ["Er\u00b7ken\u00b7nend", ",", "da\u00df", "durch", "dich", "das", "Gan\u00b7ze", "ganz", "nur", "ist", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "APPR", "PPER", "ART", "NN", "ADV", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}