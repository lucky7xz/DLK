{"textgrid.poem.25284": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von Purpursonnenblitzen", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Purpursonnenblitzen", "tokens": ["Von", "Pur\u00b7pur\u00b7son\u00b7nen\u00b7blit\u00b7zen"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Forstes L\u00fccken spr\u00fchn;", "tokens": ["Des", "Fors\u00b7tes", "L\u00fc\u00b7cken", "spr\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Abendwolken Spitzen", "tokens": ["Der", "Ab\u00b7end\u00b7wol\u00b7ken", "Spit\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie Gletscherstirnen gl\u00fchn", "tokens": ["Wie", "Glet\u00b7schers\u00b7tir\u00b7nen", "gl\u00fchn"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "NN", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In klaren Himmelsr\u00e4umen", "tokens": ["In", "kla\u00b7ren", "Him\u00b7mels\u00b7r\u00e4u\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Des Klausners Augen tr\u00e4umen,", "tokens": ["Des", "Klaus\u00b7ners", "Au\u00b7gen", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Vor Wehmut feucht.", "tokens": ["Vor", "Weh\u00b7mut", "feucht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Da sitz ich nun gefangen/", "tokens": ["Da", "sitz", "ich", "nun", "ge\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Kerker ist die Welt/", "tokens": ["Mein", "Ker\u00b7ker", "ist", "die", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und m\u00f6cht emporgelangen", "tokens": ["Und", "m\u00f6cht", "em\u00b7por\u00b7ge\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum freien Lichtgezelt.", "tokens": ["Zum", "frei\u00b7en", "Licht\u00b7ge\u00b7zelt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch harte Fenstersprossen", "tokens": ["Doch", "har\u00b7te", "Fens\u00b7ter\u00b7spros\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Behalten abgeschlossen", "tokens": ["Be\u00b7hal\u00b7ten", "ab\u00b7ge\u00b7schlos\u00b7sen"], "token_info": ["word", "word"], "pos": ["NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mich bis zum Tod.", "tokens": ["Mich", "bis", "zum", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Wohl bin mit blonden Haaren", "tokens": ["Wohl", "bin", "mit", "blon\u00b7den", "Haa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich wie ein Fr\u00fchlingswind", "tokens": ["Ich", "wie", "ein", "Fr\u00fch\u00b7lings\u00b7wind"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "KOKOM", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Viel Wonnen nachgefahren/", "tokens": ["Viel", "Won\u00b7nen", "nach\u00b7ge\u00b7fah\u00b7ren", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O weh, ich t\u00f6richt Kind!", "tokens": ["O", "weh", ",", "ich", "t\u00f6\u00b7richt", "Kind", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sp\u00e4t unter Trauerweiden", "tokens": ["Sp\u00e4t", "un\u00b7ter", "Trau\u00b7er\u00b7wei\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Lernt ich mich still bescheiden", "tokens": ["Lernt", "ich", "mich", "still", "be\u00b7schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und ward bekehrt.", "tokens": ["Und", "ward", "be\u00b7kehrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Mir kam von seligen Auen", "tokens": ["Mir", "kam", "von", "se\u00b7li\u00b7gen", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die eine Gabe nur:", "tokens": ["Die", "ei\u00b7ne", "Ga\u00b7be", "nur", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Inbr\u00fcnstig aufzuschauen", "tokens": ["In\u00b7br\u00fcns\u00b7tig", "auf\u00b7zu\u00b7schau\u00b7en"], "token_info": ["word", "word"], "pos": ["ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zur sternbes\u00e4ten Flur.", "tokens": ["Zur", "stern\u00b7be\u00b7s\u00e4\u00b7ten", "Flur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Aus tr\u00fcben Kerkerschachten", "tokens": ["Aus", "tr\u00fc\u00b7ben", "Ker\u00b7ker\u00b7schach\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zum Born des Lichtes schmachten", "tokens": ["Zum", "Born", "des", "Lich\u00b7tes", "schmach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ist all mein Trost.", "tokens": ["Ist", "all", "mein", "Trost", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Nun sei mir hochwillkommen", "tokens": ["Nun", "sei", "mir", "hoch\u00b7will\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zur Andacht, lauschige Nacht!", "tokens": ["Zur", "An\u00b7dacht", ",", "lau\u00b7schi\u00b7ge", "Nacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Verhei\u00dfend ist entglommen", "tokens": ["Ver\u00b7hei\u00b7\u00dfend", "ist", "ent\u00b7glom\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Sterngewimmels Pracht:", "tokens": ["Des", "Stern\u00b7ge\u00b7wim\u00b7mels", "Pracht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Endlose Weltenscharen", "tokens": ["End\u00b7lo\u00b7se", "Wel\u00b7ten\u00b7scha\u00b7ren"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Sollst, Seele, du befahren;", "tokens": ["Sollst", ",", "See\u00b7le", ",", "du", "be\u00b7fah\u00b7ren", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "$,", "NN", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Drum r\u00fcste dich!", "tokens": ["Drum", "r\u00fcs\u00b7te", "dich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Einst wird dir aufgeschlossen", "tokens": ["Einst", "wird", "dir", "auf\u00b7ge\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Gitterzelle T\u00fcr;", "tokens": ["Der", "Git\u00b7ter\u00b7zel\u00b7le", "T\u00fcr", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du wandelst wei\u00dfumflossen", "tokens": ["Du", "wan\u00b7delst", "wei\u00b7\u00dfum\u00b7flos\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An Pf\u00f6rtners Hand herf\u00fcr.", "tokens": ["An", "Pf\u00f6rt\u00b7ners", "Hand", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Segelschwingen breite", "tokens": ["Die", "Se\u00b7gel\u00b7schwin\u00b7gen", "brei\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und such in \u00c4therweite", "tokens": ["Und", "such", "in", "\u00c4t\u00b7her\u00b7wei\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die neue Welt.", "tokens": ["Die", "neu\u00b7e", "Welt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}