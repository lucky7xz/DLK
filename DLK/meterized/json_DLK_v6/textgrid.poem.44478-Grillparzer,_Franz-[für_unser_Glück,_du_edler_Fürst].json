{"textgrid.poem.44478": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[f\u00fcr unser Gl\u00fcck, du edler F\u00fcrst]", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcr unser Gl\u00fcck, du edler F\u00fcrst,", "tokens": ["F\u00fcr", "un\u00b7ser", "Gl\u00fcck", ",", "du", "ed\u00b7ler", "F\u00fcrst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bem\u00fchst du dich gar sehr;", "tokens": ["Be\u00b7m\u00fchst", "du", "dich", "gar", "sehr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist der Profit nicht immer klar,", "tokens": ["Ist", "der", "Pro\u00b7fit", "nicht", "im\u00b7mer", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es kommt der Tag zuletzt im Jahr \u2013", "tokens": ["Es", "kommt", "der", "Tag", "zu\u00b7letzt", "im", "Jahr", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir danken dir, o Herr.", "tokens": ["Wir", "dan\u00b7ken", "dir", ",", "o", "Herr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "FM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der lange Frieden, h\u00f6rt man wohl,", "tokens": ["Der", "lan\u00b7ge", "Frie\u00b7den", ",", "h\u00f6rt", "man", "wohl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "PIS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verweichlicht nur die Leut;", "tokens": ["Ver\u00b7weich\u00b7licht", "nur", "die", "Leut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drum setzest du, ein Feind der Rast,", "tokens": ["Drum", "set\u00b7zest", "du", ",", "ein", "Feind", "der", "Rast", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Frieden fort des Krieges Last \u2013", "tokens": ["Im", "Frie\u00b7den", "fort", "des", "Krie\u00b7ges", "Last", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "ART", "NN", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir danken dir, o Herr.", "tokens": ["Wir", "dan\u00b7ken", "dir", ",", "o", "Herr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "FM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Als Fresser schilt das Ausland uns,", "tokens": ["Als", "Fres\u00b7ser", "schilt", "das", "Aus\u00b7land", "uns", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das f\u00e4llt mir hart ins Gh\u00f6r,", "tokens": ["Das", "f\u00e4llt", "mir", "hart", "ins", "Gh\u00f6r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da schiebst du, vor des \u00dcbels T\u00fcr,", "tokens": ["Da", "schiebst", "du", ",", "vor", "des", "\u00dc\u00b7bels", "T\u00fcr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch Steuer und Gaben ein Riegel f\u00fcr \u2013", "tokens": ["Durch", "Steu\u00b7er", "und", "Ga\u00b7ben", "ein", "Rie\u00b7gel", "f\u00fcr", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "APPR", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Wir danken dir, o Herr.", "tokens": ["Wir", "dan\u00b7ken", "dir", ",", "o", "Herr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "FM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ja, endlich kosts nicht gar so viel,", "tokens": ["Ja", ",", "end\u00b7lich", "kosts", "nicht", "gar", "so", "viel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "PTKNEG", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sonst w\u00e4r es wohl zu schwer;", "tokens": ["Sonst", "w\u00e4r", "es", "wohl", "zu", "schwer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Statt barer Zahlung, gradem Kauf", "tokens": ["Statt", "ba\u00b7rer", "Zah\u00b7lung", ",", "gra\u00b7dem", "Kauf"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Nimmst du ein neues Anlehn auf \u2013", "tokens": ["Nimmst", "du", "ein", "neu\u00b7es", "An\u00b7lehn", "auf", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir danken dir, o Herr.", "tokens": ["Wir", "dan\u00b7ken", "dir", ",", "o", "Herr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "FM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und wird der Jud nun endlich stolz,", "tokens": ["Und", "wird", "der", "Jud", "nun", "end\u00b7lich", "stolz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schaut an uns nach der Quer:", "tokens": ["Schaut", "an", "uns", "nach", "der", "Quer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du leidst kein Hochmut in deim Reich,", "tokens": ["Du", "leidst", "kein", "Hoch\u00b7mut", "in", "deim", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Staatsbankrut macht alle gleich \u2013", "tokens": ["Ein", "Staats\u00b7bank\u00b7rut", "macht", "al\u00b7le", "gleich", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir danken dir, o Herr.", "tokens": ["Wir", "dan\u00b7ken", "dir", ",", "o", "Herr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "FM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und was nun, das ein Bauer gfreut,", "tokens": ["Und", "was", "nun", ",", "das", "ein", "Bau\u00b7er", "gfreut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Nutzen und die Ehr;", "tokens": ["Der", "Nut\u00b7zen", "und", "die", "Ehr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcr unser bi\u00dfl Einl\u00f6sschein", "tokens": ["F\u00fcr", "un\u00b7ser", "bi\u00dfl", "Ein\u00b7l\u00f6s\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Setzst du ein K\u00f6nig in Spanien ein \u2013", "tokens": ["Setz\u00b7st", "du", "ein", "K\u00f6\u00b7nig", "in", "Spa\u00b7ni\u00b7en", "ein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "NE", "ART", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "Wir danken dir, o Herr.", "tokens": ["Wir", "dan\u00b7ken", "dir", ",", "o", "Herr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "FM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und f\u00fchrst du d'Jesuiten ein,", "tokens": ["Und", "f\u00fchrst", "du", "d'\u00b7Je\u00b7su\u00b7i\u00b7ten", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Steigt d' Glori immer mehr;", "tokens": ["Steigt", "d'", "Glo\u00b7ri", "im\u00b7mer", "mehr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NE", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Man arbeit' leicht, wenn noch so hart,", "tokens": ["Man", "ar\u00b7beit'", "leicht", ",", "wenn", "noch", "so", "hart", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJD", "$,", "KOUS", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist einem 's Denken erst erspart \u2013", "tokens": ["Ist", "ei\u00b7nem", "'s", "Den\u00b7ken", "erst", "er\u00b7spart", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "PPER", "NN", "ADV", "VVPP", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wir danken dir, o Herr.", "tokens": ["Wir", "dan\u00b7ken", "dir", ",", "o", "Herr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "FM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Nur auf dich selber schau mir auch,", "tokens": ["Nur", "auf", "dich", "sel\u00b7ber", "schau", "mir", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist noch mein letzts Begehr;", "tokens": ["Ist", "noch", "mein", "letzts", "Be\u00b7gehr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Denn wer im Rohr nicht Pfeifen schneidt,", "tokens": ["Denn", "wer", "im", "Rohr", "nicht", "Pfei\u00b7fen", "schneidt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPRART", "NN", "PTKNEG", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verzeih mirs Wort, der ist nicht gscheit \u2013", "tokens": ["Ver\u00b7zeih", "mirs", "Wort", ",", "der", "ist", "nicht", "gscheit", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "$,", "PRELS", "VAFIN", "PTKNEG", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drum schneid und pfeif, o Herr.", "tokens": ["Drum", "schneid", "und", "pfeif", ",", "o", "Herr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "KON", "VVFIN", "$,", "FM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}