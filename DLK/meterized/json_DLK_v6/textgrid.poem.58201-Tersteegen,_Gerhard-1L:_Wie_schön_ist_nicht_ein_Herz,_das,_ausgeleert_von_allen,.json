{"textgrid.poem.58201": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie sch\u00f6n ist nicht ein Herz, das, ausgeleert von allen,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sch\u00f6n ist nicht ein Herz, das, ausgeleert von allen,", "tokens": ["Wie", "sch\u00f6n", "ist", "nicht", "ein", "Herz", ",", "das", ",", "aus\u00b7ge\u00b7leert", "von", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PTKNEG", "ART", "NN", "$,", "PDS", "$,", "VVPP", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nichts in sich heget mehr als Gottes Wohlgefallen,", "tokens": ["Nichts", "in", "sich", "he\u00b7get", "mehr", "als", "Got\u00b7tes", "Wohl\u00b7ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PRF", "VVFIN", "ADV", "KOUS", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das, durch viel Kreuz und Leid geschmolzen und gefegt,", "tokens": ["Das", ",", "durch", "viel", "Kreuz", "und", "Leid", "ge\u00b7schmol\u00b7zen", "und", "ge\u00b7fegt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "APPR", "PIAT", "NN", "KON", "NN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die h\u00f6chste Majest\u00e4t im stillen Grunde tr\u00e4gt!", "tokens": ["Die", "h\u00f6chs\u00b7te", "Ma\u00b7jes\u00b7t\u00e4t", "im", "stil\u00b7len", "Grun\u00b7de", "tr\u00e4gt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}