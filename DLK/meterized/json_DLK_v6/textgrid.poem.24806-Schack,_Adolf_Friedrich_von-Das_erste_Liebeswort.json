{"textgrid.poem.24806": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Das erste Liebeswort", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das war der s\u00fc\u00dfeste der Laute!", "tokens": ["Das", "war", "der", "s\u00fc\u00b7\u00dfes\u00b7te", "der", "Lau\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie sprach's, das erste Liebeswort;", "tokens": ["Sie", "sprach's", ",", "das", "ers\u00b7te", "Lie\u00b7bes\u00b7wort", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Herzen nun trag' ich das traute,", "tokens": ["Im", "Her\u00b7zen", "nun", "trag'", "ich", "das", "trau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "PPER", "PDS", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Tiefselige Geheimnis fort.", "tokens": ["Tief\u00b7se\u00b7li\u00b7ge", "Ge\u00b7heim\u00b7nis", "fort", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Allein wo berg' ich meine Wonne,", "tokens": ["Al\u00b7lein", "wo", "ber\u00b7g'", "ich", "mei\u00b7ne", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Da\u00df ich sie wohl beh\u00fcten mag?", "tokens": ["Da\u00df", "ich", "sie", "wohl", "be\u00b7h\u00fc\u00b7ten", "mag", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Licht verh\u00fclle, l\u00e4st'ge Sonne!", "tokens": ["Dein", "Licht", "ver\u00b7h\u00fcl\u00b7le", ",", "l\u00e4st'\u00b7ge", "Son\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verstumme, l\u00e4rmbewegter Tag!", "tokens": ["Ver\u00b7stum\u00b7me", ",", "l\u00e4rm\u00b7be\u00b7weg\u00b7ter", "Tag", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Weltfern sei meines Gl\u00fcckes F\u00fclle", "tokens": ["Welt\u00b7fern", "sei", "mei\u00b7nes", "Gl\u00fc\u00b7ckes", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Begraben, wo sie nichts verr\u00e4t", "tokens": ["Be\u00b7gra\u00b7ben", ",", "wo", "sie", "nichts", "ver\u00b7r\u00e4t"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "PPER", "PIS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nur durch Nacht und heil'ge Stille", "tokens": ["Und", "nur", "durch", "Nacht", "und", "heil'\u00b7ge", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des s\u00fc\u00dfen Wortes Nachhall weht.", "tokens": ["Des", "s\u00fc\u00b7\u00dfen", "Wor\u00b7tes", "Nach\u00b7hall", "weht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}