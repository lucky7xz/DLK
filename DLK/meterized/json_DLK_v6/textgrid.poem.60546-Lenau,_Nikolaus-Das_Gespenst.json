{"textgrid.poem.60546": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Das Gespenst", "genre": "verse", "period": "N.A.", "pub_year": 1841, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dies war einmal ein Edelhaus,", "tokens": ["Dies", "war", "ein\u00b7mal", "ein", "E\u00b7del\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun ist es trauriglich zerfallen,", "tokens": ["Nun", "ist", "es", "trau\u00b7rig\u00b7lich", "zer\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es schneit und regnet in die Hallen,", "tokens": ["Es", "schneit", "und", "reg\u00b7net", "in", "die", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur Rauher gehn dort ein und aus.", "tokens": ["Nur", "Rau\u00b7her", "gehn", "dort", "ein", "und", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "ADV", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Sohn einst mit dem Vater stritt,", "tokens": ["Der", "Sohn", "einst", "mit", "dem", "Va\u00b7ter", "stritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer auf der Jagd die Ent erschossen;", "tokens": ["Wer", "auf", "der", "Jagd", "die", "Ent", "er\u00b7schos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da ist des Alten Blut geflossen,", "tokens": ["Da", "ist", "des", "Al\u00b7ten", "Blut", "ge\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der wilde Sohn zum Teufel ritt.", "tokens": ["Der", "wil\u00b7de", "Sohn", "zum", "Teu\u00b7fel", "ritt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Weib, Knecht und Dirne flohn den Ort,", "tokens": ["Weib", ",", "Knecht", "und", "Dir\u00b7ne", "flohn", "den", "Ort", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat keins das Blut nur aufgescheuert;", "tokens": ["Hat", "keins", "das", "Blut", "nur", "auf\u00b7ge\u00b7scheu\u00b7ert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nun hei\u00dfts: bei Nacht auf Enten feuert", "tokens": ["Nun", "hei\u00dfts", ":", "bei", "Nacht", "auf", "En\u00b7ten", "feu\u00b7ert"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$.", "APPR", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Alten Geist durchs Fenster dort.", "tokens": ["Des", "Al\u00b7ten", "Geist", "durchs", "Fens\u00b7ter", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein R\u00e4uber spukt im Haus umher,", "tokens": ["Ein", "R\u00e4u\u00b7ber", "spukt", "im", "Haus", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den toten alten Grafen spielend,", "tokens": ["Den", "to\u00b7ten", "al\u00b7ten", "Gra\u00b7fen", "spie\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Im wei\u00dfen Hemd, auf Enten zielend,", "tokens": ["Im", "wei\u00b7\u00dfen", "Hemd", ",", "auf", "En\u00b7ten", "zie\u00b7lend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durchs Fenster feuernd sein Gewehr.", "tokens": ["Durchs", "Fens\u00b7ter", "feu\u00b7ernd", "sein", "Ge\u00b7wehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Hirte sieht im Mondschein hell", "tokens": ["Der", "Hir\u00b7te", "sieht", "im", "Mond\u00b7schein", "hell"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von fern das Hemd des Geistes flattern,", "tokens": ["Von", "fern", "das", "Hemd", "des", "Geis\u00b7tes", "flat\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "H\u00f6rt in der Luft die Enten schnattern,", "tokens": ["H\u00f6rt", "in", "der", "Luft", "die", "En\u00b7ten", "schnat\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Schu\u00df \u2013 und kriecht ins L\u00e4mmerfell.", "tokens": ["Den", "Schu\u00df", "\u2013", "und", "kriecht", "ins", "L\u00e4m\u00b7mer\u00b7fell", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Er staunte j\u00fcngst in dunkler Nacht,", "tokens": ["Er", "staun\u00b7te", "j\u00fcngst", "in", "dunk\u00b7ler", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Lichter im Gem\u00e4uer brannten,", "tokens": ["Wie", "Lich\u00b7ter", "im", "Ge\u00b7m\u00e4u\u00b7er", "brann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den wirren L\u00e4rm von Musikanten", "tokens": ["Den", "wir\u00b7ren", "L\u00e4rm", "von", "Mu\u00b7si\u00b7kan\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Heidewind ihm zugebracht.", "tokens": ["Der", "Hei\u00b7de\u00b7wind", "ihm", "zu\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Hei! lustig klangs im alten Nest", "tokens": ["Hei", "!", "lus\u00b7tig", "klangs", "im", "al\u00b7ten", "Nest"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADJD", "ADV", "APPRART", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Von Schmaus und Saus, Zigeunergeigen;", "tokens": ["Von", "Schmaus", "und", "Saus", ",", "Zi\u00b7geu\u00b7ner\u00b7gei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die R\u00e4uber tanzen tollen Reigen,", "tokens": ["Die", "R\u00e4u\u00b7ber", "tan\u00b7zen", "tol\u00b7len", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Hauptmann h\u00e4lt sein Hochzeitsfest,", "tokens": ["Der", "Haupt\u00b7mann", "h\u00e4lt", "sein", "Hoch\u00b7zeits\u00b7fest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Doch leuchtet nicht am Firmament", "tokens": ["Doch", "leuch\u00b7tet", "nicht", "am", "Fir\u00b7ma\u00b7ment"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem R\u00e4ubersmann und seinem Schatze", "tokens": ["Dem", "R\u00e4u\u00b7bers\u00b7mann", "und", "sei\u00b7nem", "Schat\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Brautnacht Mond, des Pfaffen Glatze;", "tokens": ["Der", "Braut\u00b7nacht", "Mond", ",", "des", "Pfaf\u00b7fen", "Glat\u00b7ze", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Lust vereint, der Scherge trennt.", "tokens": ["Die", "Lust", "ver\u00b7eint", ",", "der", "Scher\u00b7ge", "trennt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Den Hirten lockt es Schritt um Schritt,", "tokens": ["Den", "Hir\u00b7ten", "lockt", "es", "Schritt", "um", "Schritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er sp\u00fcrt beherzt in diesen T\u00f6nen", "tokens": ["Er", "sp\u00fcrt", "be\u00b7herzt", "in", "die\u00b7sen", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das warme Blut von Erdens\u00f6hnen;", "tokens": ["Das", "war\u00b7me", "Blut", "von", "Er\u00b7den\u00b7s\u00f6h\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er trinkt und tanzt und jubelt mit.", "tokens": ["Er", "trinkt", "und", "tanzt", "und", "ju\u00b7belt", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Des alten Edelmannes Geist", "tokens": ["Des", "al\u00b7ten", "E\u00b7del\u00b7man\u00b7nes", "Geist"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Spielt nun der Hirte gern vor allen,", "tokens": ["Spielt", "nun", "der", "Hir\u00b7te", "gern", "vor", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADV", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er l\u00e4\u00dft die Entenflinte knallen,", "tokens": ["Er", "l\u00e4\u00dft", "die", "En\u00b7ten\u00b7flin\u00b7te", "knal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sein weites Hemd im Monde glei\u00dft.", "tokens": ["Sein", "wei\u00b7tes", "Hemd", "im", "Mon\u00b7de", "glei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPRART", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Der Alte \u00fcbte Raub und Trutz", "tokens": ["Der", "Al\u00b7te", "\u00fcb\u00b7te", "Raub", "und", "Trutz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Dickicht finstrer Adelsbr\u00e4uche,", "tokens": ["Im", "Di\u00b7ckicht", "finst\u00b7rer", "A\u00b7dels\u00b7br\u00e4u\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nun dient er als Pandurenscheuche", "tokens": ["Nun", "dient", "er", "als", "Pan\u00b7du\u00b7ren\u00b7scheu\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Den R\u00e4ubern noch zu gutem Nutz.", "tokens": ["Den", "R\u00e4u\u00b7bern", "noch", "zu", "gu\u00b7tem", "Nutz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}