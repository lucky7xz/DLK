{"textgrid.poem.45258": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[was wundert ihr euch, da\u00df er Wunder tut]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was wundert ihr euch, da\u00df er Wunder tut,", "tokens": ["Was", "wun\u00b7dert", "ihr", "euch", ",", "da\u00df", "er", "Wun\u00b7der", "tut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "$,", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er, der ja selber ein Wunder,", "tokens": ["Er", ",", "der", "ja", "sel\u00b7ber", "ein", "Wun\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der im Alter, wo andern erloschen die Glut,", "tokens": ["Der", "im", "Al\u00b7ter", ",", "wo", "an\u00b7dern", "er\u00b7lo\u00b7schen", "die", "Glut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "$,", "PWAV", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Noch hei\u00df von der Jugend Zunder.", "tokens": ["Noch", "hei\u00df", "von", "der", "Ju\u00b7gend", "Zun\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Spart euer Wundern noch manches Jahr,", "tokens": ["Spart", "eu\u00b7er", "Wun\u00b7dern", "noch", "man\u00b7ches", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bis er statt achtzig, hundert,", "tokens": ["Bis", "er", "statt", "acht\u00b7zig", ",", "hun\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "CARD", "$,", "CARD", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Bis grau seine Kraft, wie leider sein Haar,", "tokens": ["Bis", "grau", "sei\u00b7ne", "Kraft", ",", "wie", "lei\u00b7der", "sein", "Haar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "PPOSAT", "NN", "$,", "PWAV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Jetzt, statt euch zu wundern, bewundert.", "tokens": ["Jetzt", ",", "statt", "euch", "zu", "wun\u00b7dern", ",", "be\u00b7wun\u00b7dert", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$,", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}