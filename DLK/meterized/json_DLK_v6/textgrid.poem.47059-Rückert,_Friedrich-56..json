{"textgrid.poem.47059": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "56.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welch rasches T\u00f6nen wundersanfter Glocken,", "tokens": ["Welch", "ra\u00b7sches", "T\u00f6\u00b7nen", "wun\u00b7der\u00b7sanf\u00b7ter", "Glo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das widerklingt in meines Herzens Mitten!", "tokens": ["Das", "wi\u00b7der\u00b7klingt", "in", "mei\u00b7nes", "Her\u00b7zens", "Mit\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Liebste kommt, verh\u00fcllt, im leichten Schlitten", "tokens": ["Die", "Liebs\u00b7te", "kommt", ",", "ver\u00b7h\u00fcllt", ",", "im", "leich\u00b7ten", "Schlit\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "VVPP", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dahergeflogen durch den Tanz der Flocken.", "tokens": ["Da\u00b7her\u00b7ge\u00b7flo\u00b7gen", "durch", "den", "Tanz", "der", "Flo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die stolzen Hengste sch\u00fctteln ihre Locken", "tokens": ["Die", "stol\u00b7zen", "Hengs\u00b7te", "sch\u00fct\u00b7teln", "ih\u00b7re", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und drehn das Haupt r\u00fcckw\u00e4rts, mit art'gen Sitten", "tokens": ["Und", "drehn", "das", "Haupt", "r\u00fcck\u00b7w\u00e4rts", ",", "mit", "art'\u00b7gen", "Sit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "CARD", "ART", "NN", "ADV", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Zuwiehernd: Lieber als am Z\u00fcgel schritten", "tokens": ["Zu\u00b7wie\u00b7hernd", ":", "Lie\u00b7ber", "als", "am", "Z\u00fc\u00b7gel", "schrit\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$.", "ADJD", "KOKOM", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An einem F\u00e4dlein wir von deinem Rocken.", "tokens": ["An", "ei\u00b7nem", "F\u00e4d\u00b7lein", "wir", "von", "dei\u00b7nem", "Ro\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hast du den Rocken lassen k\u00f6nnen, Flei\u00dfige?", "tokens": ["Hast", "du", "den", "Ro\u00b7cken", "las\u00b7sen", "k\u00f6n\u00b7nen", ",", "Flei\u00b7\u00dfi\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVINF", "VMINF", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn nun indes ihn m\u00fc\u00dfig sieht ein Freier?", "tokens": ["Wenn", "nun", "in\u00b7des", "ihn", "m\u00fc\u00b7\u00dfig", "sieht", "ein", "Frei\u00b7er", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "PPER", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch sieh, f\u00fcr deine Hand spinnt eine fremde,", "tokens": ["Doch", "sieh", ",", "f\u00fcr", "dei\u00b7ne", "Hand", "spinnt", "ei\u00b7ne", "frem\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Winter selber spinnt f\u00fcr dich, der eisige;", "tokens": ["Der", "Win\u00b7ter", "sel\u00b7ber", "spinnt", "f\u00fcr", "dich", ",", "der", "ei\u00b7si\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "PPER", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schneeweben wird er bald zum Hochzeitschleier", "tokens": ["Schnee\u00b7we\u00b7ben", "wird", "er", "bald", "zum", "Hoch\u00b7zeit\u00b7schlei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Gesponnen haben und zum Hochzeithemde.", "tokens": ["Ge\u00b7spon\u00b7nen", "ha\u00b7ben", "und", "zum", "Hoch\u00b7zei\u00b7them\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}