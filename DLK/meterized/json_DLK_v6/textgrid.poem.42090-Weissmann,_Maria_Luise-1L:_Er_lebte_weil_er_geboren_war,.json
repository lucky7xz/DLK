{"textgrid.poem.42090": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Er lebte weil er geboren war,", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er lebte weil er geboren war,", "tokens": ["Er", "leb\u00b7te", "weil", "er", "ge\u00b7bo\u00b7ren", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Er fand keinen anderen Grund.", "tokens": ["Er", "fand", "kei\u00b7nen", "an\u00b7de\u00b7ren", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Die Mutter liebte ganz fr\u00fch sein Haar,", "tokens": ["Die", "Mut\u00b7ter", "lieb\u00b7te", "ganz", "fr\u00fch", "sein", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Einmal Eine dann seinen Mund,", "tokens": ["Ein\u00b7mal", "Ei\u00b7ne", "dann", "sei\u00b7nen", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Doch war es nicht wichtig und verging", "tokens": ["Doch", "war", "es", "nicht", "wich\u00b7tig", "und", "ver\u00b7ging"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "ADJD", "KON", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Auch schnell, bevor ers ermessen.", "tokens": ["Auch", "schnell", ",", "be\u00b7vor", "ers", "er\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PIS", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Alles in allem war so gering \u2013", "tokens": ["Al\u00b7les", "in", "al\u00b7lem", "war", "so", "ge\u00b7ring", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PIS", "VAFIN", "ADV", "ADJD", "$("], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Er hatte als er zu sterben anfing", "tokens": ["Er", "hat\u00b7te", "als", "er", "zu", "ster\u00b7ben", "an\u00b7fing"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "KOUS", "PPER", "PTKZU", "VVINF", "VVFIN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Sich schon seit Jahren vergessen.", "tokens": ["Sich", "schon", "seit", "Jah\u00b7ren", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}