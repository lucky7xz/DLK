{"textgrid.poem.40868": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Max vor Dendermonde", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie freundlich winkt dem Wandrer die Festung Dendermond',", "tokens": ["Wie", "freund\u00b7lich", "winkt", "dem", "Wand\u00b7rer", "die", "Fes\u00b7tung", "Den\u00b7der\u00b7mond'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wenn sie die blanken Zinnen im Abendgolde sonnt!", "tokens": ["Wenn", "sie", "die", "blan\u00b7ken", "Zin\u00b7nen", "im", "A\u00b7bend\u00b7gol\u00b7de", "sonnt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Dir, Max, winkt sie nicht freundlich, dir sperren Meuter das Thor", "tokens": ["Dir", ",", "Max", ",", "winkt", "sie", "nicht", "freund\u00b7lich", ",", "dir", "sper\u00b7ren", "Meu\u00b7ter", "das", "Thor"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "NE", "$,", "VVFIN", "PPER", "PTKNEG", "ADJD", "$,", "PPER", "ADJA", "NN", "ART", "NN"], "meter": "--+--+--+-+--+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Und pflanzen ihre Fahnen auf Mauer und Thurm empor.", "tokens": ["Und", "pflan\u00b7zen", "ih\u00b7re", "Fah\u00b7nen", "auf", "Mau\u00b7er", "und", "Thurm", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Abt von Dendermonde mit seinen M\u00f6nchen sa\u00df", "tokens": ["Der", "Abt", "von", "Den\u00b7der\u00b7mon\u00b7de", "mit", "sei\u00b7nen", "M\u00f6n\u00b7chen", "sa\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Beim kargen Klostermahle und leerte Glas auf Glas:", "tokens": ["Beim", "kar\u00b7gen", "Klos\u00b7ter\u00b7mah\u00b7le", "und", "leer\u00b7te", "Glas", "auf", "Glas", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "KON", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}, "line.4": {"text": "Stets th\u00e4tig in der Pflicht sein, drum la\u00dft uns spazieren gehn!\u00ab", "tokens": ["Stets", "th\u00e4\u00b7tig", "in", "der", "Pflicht", "sein", ",", "drum", "la\u00dft", "uns", "spa\u00b7zie\u00b7ren", "gehn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "VAINF", "$,", "PAV", "VVFIN", "PPER", "VVINF", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "Zu Dendermond' die Aebtissin, das Aug' von Thr\u00e4nen na\u00df,", "tokens": ["Zu", "Den\u00b7der\u00b7mond'", "die", "A\u00b7eb\u00b7tis\u00b7sin", ",", "das", "Aug'", "von", "Thr\u00e4\u00b7nen", "na\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ART", "NN", "$,", "ART", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Sankt Abelards Legende mit ihren Nonnen las:", "tokens": ["Sankt", "A\u00b7be\u00b7lards", "Le\u00b7gen\u00b7de", "mit", "ih\u00b7ren", "Non\u00b7nen", "las", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "\u00bbsch\u00f6n ist der Abend, la\u00dft uns chorsingen heut im Frei'n,", "tokens": ["\u00bb", "sch\u00f6n", "ist", "der", "A\u00b7bend", ",", "la\u00dft", "uns", "chor\u00b7sin\u00b7gen", "heut", "im", "Frei'n", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "ART", "NN", "$,", "VVIMP", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Da heulen doch keine Doggen in heil'ge Psalme drein.\u00ab", "tokens": ["Da", "heu\u00b7len", "doch", "kei\u00b7ne", "Dog\u00b7gen", "in", "heil'\u00b7ge", "Psal\u00b7me", "drein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "ADV", "PIAT", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Die Nonnen und die M\u00f6nche, mit Rosenkranz und Brevier,", "tokens": ["Die", "Non\u00b7nen", "und", "die", "M\u00f6n\u00b7che", ",", "mit", "Ro\u00b7sen\u00b7kranz", "und", "Bre\u00b7vier", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die wallen hinaus zum Stadtthor, ins gr\u00fcnende Revier.", "tokens": ["Die", "wal\u00b7len", "hin\u00b7aus", "zum", "Stadt\u00b7thor", ",", "ins", "gr\u00fc\u00b7nen\u00b7de", "Re\u00b7vier", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APZR", "APPRART", "NN", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+--+--++", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die Nonnen singen: \u00bbO Christe, du Br\u00e4ut'gam s\u00fc\u00df und traut!\u00ab", "tokens": ["Die", "Non\u00b7nen", "sin\u00b7gen", ":", "\u00bb", "O", "Chris\u00b7te", ",", "du", "Br\u00e4ut'\u00b7gam", "s\u00fc\u00df", "und", "traut", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVINF", "$.", "$(", "NE", "NE", "$,", "PPER", "NE", "ADJD", "KON", "VVFIN", "$.", "$("], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die M\u00f6nche seufzen: \u00bbMaria, o komm', du s\u00fc\u00dfe Braut!\u00ab", "tokens": ["Die", "M\u00f6n\u00b7che", "seuf\u00b7zen", ":", "\u00bb", "Ma\u00b7ria", ",", "o", "komm'", ",", "du", "s\u00fc\u00b7\u00dfe", "Braut", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVINF", "$.", "$(", "NE", "$,", "FM", "VVFIN", "$,", "PPER", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Und als sie kamen selbander in einen gr\u00fcnen Wald,", "tokens": ["Und", "als", "sie", "ka\u00b7men", "sel\u00b7ban\u00b7der", "in", "ei\u00b7nen", "gr\u00fc\u00b7nen", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da rauscht es in den Zweigen, da br\u00fcllt es donnernd: halt!", "tokens": ["Da", "rauscht", "es", "in", "den", "Zwei\u00b7gen", ",", "da", "br\u00fcllt", "es", "don\u00b7nernd", ":", "halt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "ADJD", "$.", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Es brechen gewappnete Krieger durchs struppige Gestr\u00e4uch,", "tokens": ["Es", "bre\u00b7chen", "ge\u00b7wapp\u00b7ne\u00b7te", "Krie\u00b7ger", "durchs", "strup\u00b7pi\u00b7ge", "Ge\u00b7str\u00e4uch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-+-+", "measure": "amphibrach.tetra.plus"}, "line.4": {"text": "Den M\u00f6nchen klappern die Z\u00e4hne, die Nonnen werden bleich.", "tokens": ["Den", "M\u00f6n\u00b7chen", "klap\u00b7pern", "die", "Z\u00e4h\u00b7ne", ",", "die", "Non\u00b7nen", "wer\u00b7den", "bleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Und muthig durch das Dickicht w\u00fchlt sich ein schnaubend Ro\u00df,", "tokens": ["Und", "mut\u00b7hig", "durch", "das", "Di\u00b7ckicht", "w\u00fchlt", "sich", "ein", "schnau\u00b7bend", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VVFIN", "PRF", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der Reiter, hoch und edel, h\u00e4lt mitten im Kriegertro\u00df:", "tokens": ["Der", "Rei\u00b7ter", ",", "hoch", "und", "e\u00b7del", ",", "h\u00e4lt", "mit\u00b7ten", "im", "Krie\u00b7ger\u00b7tro\u00df", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "\u00bbei, Gottwillkommen!\u00ab rief er, \u00bbhabt weiten Weg gehabt.", "tokens": ["\u00bb", "ei", ",", "Gott\u00b7will\u00b7kom\u00b7men", "!", "\u00ab", "rief", "er", ",", "\u00bb", "habt", "wei\u00b7ten", "Weg", "ge\u00b7habt", "."], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "NN", "$.", "$(", "VVFIN", "PPER", "$,", "$(", "VAFIN", "ADJA", "NN", "VAPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Gott gr\u00fc\u00df' euch, Frau Aebtissin, willkommen sch\u00f6n, Herr Abt.", "tokens": ["Gott", "gr\u00fc\u00df'", "euch", ",", "Frau", "A\u00b7eb\u00b7tis\u00b7sin", ",", "will\u00b7kom\u00b7men", "sch\u00f6n", ",", "Herr", "Abt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "NN", "NN", "$,", "ADJD", "ADJD", "$,", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.7": {"line.1": {"text": "Euch gr\u00fc\u00dfet Max von Oestreich; \u2013 bin jetzt zwar selbst im Feld;", "tokens": ["Euch", "gr\u00fc\u00b7\u00dfet", "Max", "von", "O\u00b7e\u00b7streich", ";", "\u2013", "bin", "jetzt", "zwar", "selbst", "im", "Feld", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "APPR", "NE", "$.", "$(", "VAFIN", "ADV", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Doch r\u00e4um' ich gern zur Herberg' heut' Nacht euch Zelt an Zelt.", "tokens": ["Doch", "r\u00e4um'", "ich", "gern", "zur", "Her\u00b7ber\u00b7g'", "heut'", "Nacht", "euch", "Zelt", "an", "Zelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "NN", "ADV", "NN", "PPER", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Zwar halt' ich karge Tafel, f\u00fcr Durst und Hunger genug,", "tokens": ["Zwar", "halt'", "ich", "kar\u00b7ge", "Ta\u00b7fel", ",", "f\u00fcr", "Durst", "und", "Hun\u00b7ger", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "NN", "$,", "APPR", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Doch dampft noch manche Sch\u00fcssel und blinkt manch voller Krug.", "tokens": ["Doch", "dampft", "noch", "man\u00b7che", "Sch\u00fcs\u00b7sel", "und", "blinkt", "manch", "vol\u00b7ler", "Krug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIAT", "NN", "KON", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Mir geht's nicht grad' zum Besten, drum m\u00f6cht' ich mich zerstreun,", "tokens": ["Mir", "geht's", "nicht", "grad'", "zum", "Bes\u00b7ten", ",", "drum", "m\u00f6cht'", "ich", "mich", "zer\u00b7streun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "APPRART", "NN", "$,", "PAV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Doch lust'ge Mummereien vor Allem mich erfreun;", "tokens": ["Doch", "lust'\u00b7ge", "Mum\u00b7me\u00b7rei\u00b7en", "vor", "Al\u00b7lem", "mich", "er\u00b7freun", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "PIS", "PRF", "VVINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Jetzt kommt mir just so 'n Schwank ein, drum hab' ich mir gedacht:", "tokens": ["Jetzt", "kommt", "mir", "just", "so", "'", "n", "Schwank", "ein", ",", "drum", "hab", "'", "ich", "mir", "ge\u00b7dacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$(", "XY", "NN", "PTKVZ", "$,", "PAV", "VAFIN", "$(", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Ihr leiht uns Kapuz' und Kutten und Schleier f\u00fcr heut Nacht.\u00ab", "tokens": ["Ihr", "leiht", "uns", "Ka\u00b7puz'", "und", "Kut\u00b7ten", "und", "Schlei\u00b7er", "f\u00fcr", "heut", "Nacht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN", "KON", "NN", "APPR", "ADV", "NN", "$.", "$("], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Den M\u00f6nchen wackeln die B\u00e4rte, die Nonnen werden roth,", "tokens": ["Den", "M\u00f6n\u00b7chen", "wa\u00b7ckeln", "die", "B\u00e4r\u00b7te", ",", "die", "Non\u00b7nen", "wer\u00b7den", "roth", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und leis im Chore lispelt's: \u00bbHerr, hilf uns aus der Noth!\u00ab", "tokens": ["Und", "leis", "im", "Cho\u00b7re", "lis\u00b7pelt's", ":", "\u00bb", "Herr", ",", "hilf", "uns", "aus", "der", "Noth", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "NE", "$.", "$(", "NN", "$,", "VVIMP", "PPER", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Doch Max spricht zu den Kriegern: \u00bbIhr Treuen auf und theilt", "tokens": ["Doch", "Max", "spricht", "zu", "den", "Krie\u00b7gern", ":", "\u00bb", "Ihr", "Treu\u00b7en", "auf", "und", "theilt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "VVFIN", "APPR", "ART", "NN", "$.", "$(", "PPOSAT", "NN", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Euch in Kapuz' und Schleier; dann rasch in die Stadt geeilt!", "tokens": ["Euch", "in", "Ka\u00b7puz'", "und", "Schlei\u00b7er", ";", "dann", "rasch", "in", "die", "Stadt", "ge\u00b7eilt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "$.", "ADV", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "Es fahr' in des Abtes Kutte mein Barbanson hinein,", "tokens": ["Es", "fahr'", "in", "des", "Ab\u00b7tes", "Kut\u00b7te", "mein", "Bar\u00b7ban\u00b7son", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Mein lust'ger Rath, Freund Kunze, du sollst Aebtissin sein.", "tokens": ["Mein", "lust'\u00b7ger", "Rath", ",", "Freund", "Kun\u00b7ze", ",", "du", "sollst", "A\u00b7eb\u00b7tis\u00b7sin", "sein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "NN", "NN", "$,", "PPER", "VMFIN", "NN", "VAINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Es ist ein n\u00e4rr'scher Feldzug, drum gibt's zu thun f\u00fcr dich;", "tokens": ["Es", "ist", "ein", "n\u00e4rr'\u00b7scher", "Feld\u00b7zug", ",", "drum", "gibt's", "zu", "thun", "f\u00fcr", "dich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "PAV", "VVFIN", "PTKZU", "VVINF", "APPR", "PPER", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Will's Gott, so seht ihr balde in Dendermond' auch mich!\u00ab", "tokens": ["Will's", "Gott", ",", "so", "seht", "ihr", "bal\u00b7de", "in", "Den\u00b7der\u00b7mond'", "auch", "mich", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "APPR", "NE", "ADV", "PPER", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.11": {"line.1": {"text": "Schon stehn in Kutt' und Schleier jetzt M\u00f6nch' und Nonnen gereiht.", "tokens": ["Schon", "stehn", "in", "Kutt'", "und", "Schlei\u00b7er", "jetzt", "M\u00f6n\u00b7ch'", "und", "Non\u00b7nen", "ge\u00b7reiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN", "ADV", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+--++-+--+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Wie l\u00e4\u00dft so stattlich Kunzen das falt'ge Nonnenkleid!", "tokens": ["Wie", "l\u00e4\u00dft", "so", "statt\u00b7lich", "Kun\u00b7zen", "das", "falt'\u00b7ge", "Non\u00b7nen\u00b7kleid", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ADJD", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und als die sch\u00f6ne Aebtissin den Schleier ihm umgeh\u00e4ngt,", "tokens": ["Und", "als", "die", "sch\u00f6\u00b7ne", "A\u00b7eb\u00b7tis\u00b7sin", "den", "Schlei\u00b7er", "ihm", "um\u00b7ge\u00b7h\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+--+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Da dacht' er sich so Manches, was so ein Narr sich denkt.", "tokens": ["Da", "dacht'", "er", "sich", "so", "Man\u00b7ches", ",", "was", "so", "ein", "Narr", "sich", "denkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "PIS", "$,", "PRELS", "ADV", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.12": {"line.1": {"text": "Zu Dendermond' auf dem Walle, da steht ein Mann zur Wacht,", "tokens": ["Zu", "Den\u00b7der\u00b7mond'", "auf", "dem", "Wal\u00b7le", ",", "da", "steht", "ein", "Mann", "zur", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der lehnt am Speere, singend hinaus in die Vollmondnacht:", "tokens": ["Der", "lehnt", "am", "Spee\u00b7re", ",", "sin\u00b7gend", "hin\u00b7aus", "in", "die", "Voll\u00b7mond\u00b7nacht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "$,", "VVPP", "APZR", "APPR", "ART", "NN", "$."], "meter": "-+-+-+---+-+-+", "measure": "unknown.measure.hexa"}, "line.3": {"text": "\u00bbeine Affe und ein Pfaffe, der Reim pa\u00dft gut und fein,", "tokens": ["\u00bb", "ei\u00b7ne", "Af\u00b7fe", "und", "ein", "Pfaf\u00b7fe", ",", "der", "Reim", "pa\u00dft", "gut", "und", "fein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "KON", "ART", "NN", "$,", "ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Es liebt ja Pfaff' und Affe die Dirnen und den Wein.\u00ab", "tokens": ["Es", "liebt", "ja", "Pfaff'", "und", "Af\u00b7fe", "die", "Dir\u00b7nen", "und", "den", "Wein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "KON", "NN", "ART", "NN", "KON", "ART", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.13": {"line.1": {"text": "\u00bbho, ho, verbrenn' dir der Donner den ungewaschnen Schlund!", "tokens": ["\u00bb", "ho", ",", "ho", ",", "ver\u00b7brenn'", "dir", "der", "Don\u00b7ner", "den", "un\u00b7ge\u00b7waschnen", "Schlund", "!"], "token_info": 