{"textgrid.poem.33105": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "Schiller", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdas H\u00f6chste, was uns kann der Dichter geben,", "tokens": ["\u00bb", "das", "H\u00f6chs\u00b7te", ",", "was", "uns", "kann", "der", "Dich\u00b7ter", "ge\u00b7ben", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "$,", "PRELS", "PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das ist sein ", "tokens": ["Das", "ist", "sein"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VAFIN", "PPOSAT"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Vor denen, welche mit und nach ihm leben,", "tokens": ["Vor", "de\u00b7nen", ",", "wel\u00b7che", "mit", "und", "nach", "ihm", "le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "$,", "PRELS", "APPR", "KON", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sei dieses w\u00fcrdig, ausgestellt zu sein;", "tokens": ["Sei", "die\u00b7ses", "w\u00fcr\u00b7dig", ",", "aus\u00b7ge\u00b7stellt", "zu", "sein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "ADJD", "$,", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zum reinsten Menschthum es empor zu heben,", "tokens": ["Zum", "reins\u00b7ten", "Menscht\u00b7hum", "es", "em\u00b7por", "zu", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPER", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Es ganz zu l\u00e4utern, sei sein Ziel allein \u2013", "tokens": ["Es", "ganz", "zu", "l\u00e4u\u00b7tern", ",", "sei", "sein", "Ziel", "al\u00b7lein", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "$,", "VAFIN", "PPOSAT", "NN", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Denn h\u00f6chste Gluth mu\u00df ", "tokens": ["Denn", "h\u00f6chs\u00b7te", "Gluth", "mu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VMFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Der 's wagen will, die G\u00f6ttlichen zu r\u00fchren!\u00ab", "tokens": ["Der", "'s", "wa\u00b7gen", "will", ",", "die", "G\u00f6tt\u00b7li\u00b7chen", "zu", "r\u00fch\u00b7ren", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPER", "VVINF", "VMFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "So sprach der Dichter Schiller \u2013 und geblieben", "tokens": ["So", "sprach", "der", "Dich\u00b7ter", "Schil\u00b7ler", "\u2013", "und", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NE", "$(", "KON", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist nicht f\u00fcr ihn ein leerer Schall das Wort,", "tokens": ["Ist", "nicht", "f\u00fcr", "ihn", "ein", "lee\u00b7rer", "Schall", "das", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPR", "PPER", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo ihn das Innerste nicht angetrieben,", "tokens": ["Wo", "ihn", "das", "In\u00b7ners\u00b7te", "nicht", "an\u00b7ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erklang von seiner Leier kein Accord.", "tokens": ["Er\u00b7klang", "von", "sei\u00b7ner", "Lei\u00b7er", "kein", "Ac\u00b7cord", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nicht ", "tokens": ["Nicht"], "token_info": ["word"], "pos": ["PTKNEG"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Die nicht entquoll der tiefsten Seele Hort \u2013", "tokens": ["Die", "nicht", "ent\u00b7quoll", "der", "tiefs\u00b7ten", "See\u00b7le", "Hort", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADJD", "ART", "ADJA", "NN", "NE", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Was er auch schuf, voll sittlich reiner Klarheit,", "tokens": ["Was", "er", "auch", "schuf", ",", "voll", "sitt\u00b7lich", "rei\u00b7ner", "Klar\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "$,", "ADJD", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Abglanz nur ist's seiner ", "tokens": ["Der", "Ab\u00b7glanz", "nur", "ist's", "sei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VAFIN", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er, dem der Freiheit sch\u00f6nster Sang gelungen,", "tokens": ["Er", ",", "dem", "der", "Frei\u00b7heit", "sch\u00f6ns\u00b7ter", "Sang", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "War selber frei in innerster Natur;", "tokens": ["War", "sel\u00b7ber", "frei", "in", "in\u00b7ners\u00b7ter", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Nach einem edlen Weib nur hat gerungen", "tokens": ["Nach", "ei\u00b7nem", "ed\u00b7len", "Weib", "nur", "hat", "ge\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der deutschen Frauen erster Troubadour;", "tokens": ["Der", "deut\u00b7schen", "Frau\u00b7en", "ers\u00b7ter", "Trou\u00b7ba\u00b7dour", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie er der Freundschaft Ideal besungen,", "tokens": ["Wie", "er", "der", "Freund\u00b7schaft", "I\u00b7deal", "be\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "NN", "VVPP", "$,"], "meter": "---+-++-+-", "measure": "unknown.measure.tetra"}, "line.6": {"text": "War er voll Treu' ein Freund, wie Wen'ge nur \u2013", "tokens": ["War", "er", "voll", "Treu'", "ein", "Freund", ",", "wie", "Wen'\u00b7ge", "nur", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "NN", "ART", "NN", "$,", "PWAV", "PIS", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So wissen kaum wir, wunderbar getrieben,", "tokens": ["So", "wis\u00b7sen", "kaum", "wir", ",", "wun\u00b7der\u00b7bar", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ob mehr der Dichter \u2013 mehr der Mensch zu lieben!", "tokens": ["Ob", "mehr", "der", "Dich\u00b7ter", "\u2013", "mehr", "der", "Mensch", "zu", "lie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "$(", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Zum ", "tokens": ["Zum"], "token_info": ["word"], "pos": ["APPRART"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "O, Jugend, die begeistert auf ihn lauscht!", "tokens": ["O", ",", "Ju\u00b7gend", ",", "die", "be\u00b7geis\u00b7tert", "auf", "ihn", "lauscht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "PRELS", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Damit nicht nur dein Ohr sein Wort entz\u00fccke,", "tokens": ["Da\u00b7mit", "nicht", "nur", "dein", "Ohr", "sein", "Wort", "ent\u00b7z\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKNEG", "ADV", "PPOSAT", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sein Sang nicht nur die Phantasie berauscht;", "tokens": ["Sein", "Sang", "nicht", "nur", "die", "Phan\u00b7ta\u00b7sie", "be\u00b7rauscht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sein ganzes Bild der vollen Seele dr\u00fccke", "tokens": ["Sein", "gan\u00b7zes", "Bild", "der", "vol\u00b7len", "See\u00b7le", "dr\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dir ein, und sorg', da\u00df nie die Zeit es tauscht,", "tokens": ["Dir", "ein", ",", "und", "sor\u00b7g'", ",", "da\u00df", "nie", "die", "Zeit", "es", "tauscht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "KON", "NN", "$,", "KOUS", "ADV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+----+-+-+", "measure": "dactylic.init"}, "line.7": {"text": "Lass' es in's Herz dir wachsen, in das weiche,", "tokens": ["Lass'", "es", "in's", "Herz", "dir", "wach\u00b7sen", ",", "in", "das", "wei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "PPER", "VVINF", "$,", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wie Heil'genbilder in den Stamm der Eiche!", "tokens": ["Wie", "Heil'\u00b7gen\u00b7bil\u00b7der", "in", "den", "Stamm", "der", "Ei\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Denn auf des Lebens wild verworr'nen Wegen,", "tokens": ["Denn", "auf", "des", "Le\u00b7bens", "wild", "ver\u00b7worr'\u00b7nen", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo oft ", "tokens": ["Wo", "oft"], "token_info": ["word", "word"], "pos": ["PWAV", "ADV"], "meter": "++", "measure": "spondeus"}, "line.3": {"text": "Ist's dem entt\u00e4uschten Herzen Himmelssegen,", "tokens": ["Ist's", "dem", "ent\u00b7t\u00e4uschten", "Her\u00b7zen", "Him\u00b7mels\u00b7se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wenn's noch durch Einen an die Menschheit glaubt,", "tokens": ["Wenn's", "noch", "durch", "Ei\u00b7nen", "an", "die", "Menschheit", "glaubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn's noch ", "tokens": ["Wenn's", "noch"], "token_info": ["word", "word"], "pos": ["KOUS", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Das nie der T\u00e4uschung kalte Hand ihm raubt:", "tokens": ["Das", "nie", "der", "T\u00e4u\u00b7schung", "kal\u00b7te", "Hand", "ihm", "raubt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Denn zweifellos, im reinsten Sch\u00f6nheitslichte,", "tokens": ["Denn", "zwei\u00b7fel\u00b7los", ",", "im", "reins\u00b7ten", "Sch\u00f6n\u00b7heits\u00b7lich\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Steht er auf ew'ge Zeit in der Geschichte.", "tokens": ["Steht", "er", "auf", "ew'\u00b7ge", "Zeit", "in", "der", "Ge\u00b7schich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Und auf den ", "tokens": ["Und", "auf", "den"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Nie sprach er: Was der Genius thut ist recht!", "tokens": ["Nie", "sprach", "er", ":", "Was", "der", "Ge\u00b7nius", "thut", "ist", "recht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "PWS", "ART", "NN", "VVFIN", "VAFIN", "ADJD", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Nie wollt' er, da\u00df die Welt ein mildrer Richter", "tokens": ["Nie", "wollt'", "er", ",", "da\u00df", "die", "Welt", "ein", "mild\u00b7rer", "Rich\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "$,", "KOUS", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihm sei, als einem staubgebornen Knecht.", "tokens": ["Ihm", "sei", ",", "als", "ei\u00b7nem", "staub\u00b7ge\u00b7bor\u00b7nen", "Knecht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Des Geistes Hoheit war ihm der Verpflichter,", "tokens": ["Des", "Geis\u00b7tes", "Ho\u00b7heit", "war", "ihm", "der", "Ver\u00b7pflich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Auch gro\u00df zu ", "tokens": ["Auch", "gro\u00df", "zu"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "PTKZU"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "F\u00fcr sich verlangt der Genius, soll er zeigen,", "tokens": ["F\u00fcr", "sich", "ver\u00b7langt", "der", "Ge\u00b7nius", ",", "soll", "er", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVFIN", "ART", "NN", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "++-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Da\u00df er auch kann als Mensch den G\u00f6ttern gleichen!", "tokens": ["Da\u00df", "er", "auch", "kann", "als", "Mensch", "den", "G\u00f6t\u00b7tern", "glei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VMFIN", "KOUS", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ihn trug das Leben nicht auf glatter Welle,", "tokens": ["Ihn", "trug", "das", "Le\u00b7ben", "nicht", "auf", "glat\u00b7ter", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sein Tod erst war's, der Kampf und Noth gestillt,", "tokens": ["Sein", "Tod", "erst", "wa\u00b7r's", ",", "der", "Kampf", "und", "Noth", "ge\u00b7stillt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "V