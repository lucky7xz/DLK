{"textgrid.poem.23884": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Traurig war ich, ach so sehr,", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Traurig war ich, ach so sehr,", "tokens": ["Trau\u00b7rig", "war", "ich", ",", "ach", "so", "sehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "ITJ", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und viel Thr\u00e4nen sind geflossen,", "tokens": ["Und", "viel", "Thr\u00e4\u00b7nen", "sind", "ge\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Klagen hab' ich hin und her", "tokens": ["Kla\u00b7gen", "hab'", "ich", "hin", "und", "her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "PTKVZ", "KON", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In die leere Luft gest\u00f6hnt \u2013", "tokens": ["In", "die", "lee\u00b7re", "Luft", "ge\u00b7st\u00f6hnt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach so sehr!", "tokens": ["Ach", "so", "sehr", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Nur das Echo hat geh\u00f6hnt;", "tokens": ["Nur", "das", "E\u00b7cho", "hat", "ge\u00b7h\u00f6hnt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Niemand nahte, mich zu tr\u00f6sten,", "tokens": ["Nie\u00b7mand", "nah\u00b7te", ",", "mich", "zu", "tr\u00f6s\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und mein Herz blieb unvers\u00f6hnt,", "tokens": ["Und", "mein", "Herz", "blieb", "un\u00b7ver\u00b7s\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Kummerschwer und freudeleer.", "tokens": ["Kum\u00b7mer\u00b7schwer", "und", "freu\u00b7de\u00b7leer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da kam heut ein sch\u00f6ner Tag,", "tokens": ["Da", "kam", "heut", "ein", "sch\u00f6\u00b7ner", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sonne kam und trieb die Wolken,", "tokens": ["Son\u00b7ne", "kam", "und", "trieb", "die", "Wol\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sah mich, der ich m\u00fcde lag,", "tokens": ["Sah", "mich", ",", "der", "ich", "m\u00fc\u00b7de", "lag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit verliebten Augen an.", "tokens": ["Mit", "ver\u00b7lieb\u00b7ten", "Au\u00b7gen", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sch\u00f6ner Tag!", "tokens": ["Sch\u00f6\u00b7ner", "Tag", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Und ich wurde wieder Mann,", "tokens": ["Und", "ich", "wur\u00b7de", "wie\u00b7der", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Blies auf meiner lieben Fl\u00f6te,", "tokens": ["Blies", "auf", "mei\u00b7ner", "lie\u00b7ben", "Fl\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sang mir dieses Liedchen dann,", "tokens": ["Sang", "mir", "die\u00b7ses", "Lied\u00b7chen", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Das mein M\u00e4dchen h\u00f6ren mag.", "tokens": ["Das", "mein", "M\u00e4d\u00b7chen", "h\u00f6\u00b7ren", "mag."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["PDS", "PPOSAT", "NN", "VVINF", "VMFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}