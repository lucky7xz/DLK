{"textgrid.poem.40965": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Die schw\u00e4bischen S\u00e4nger", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Nachtigall im frischen Hain", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "im", "fri\u00b7schen", "Hain"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Singt wohl gar sch\u00f6ne Weisen,", "tokens": ["Singt", "wohl", "gar", "sch\u00f6\u00b7ne", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch ist ", "tokens": ["Doch", "ist"], "token_info": ["word", "word"], "pos": ["KON", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Ob solcher Kunst zu preisen.", "tokens": ["Ob", "sol\u00b7cher", "Kunst", "zu", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Kein K\u00f6nig ist im freien Wald,", "tokens": ["Kein", "K\u00f6\u00b7nig", "ist", "im", "frei\u00b7en", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wo bunt ringsum Gesang erschallt.", "tokens": ["Wo", "bunt", "ring\u00b7sum", "Ge\u00b7sang", "er\u00b7schallt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da singet jeder seine Weis'", "tokens": ["Da", "sin\u00b7get", "je\u00b7der", "sei\u00b7ne", "Weis'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach seinem eignen Schnabel,", "tokens": ["Nach", "sei\u00b7nem", "eig\u00b7nen", "Schna\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ob Nachtigall, ob Fink er hei\u00df',", "tokens": ["Ob", "Nach\u00b7ti\u00b7gall", ",", "ob", "Fink", "er", "hei\u00df'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "KOUS", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn sch\u00f6n nicht, doch passabel.", "tokens": ["Wenn", "sch\u00f6n", "nicht", ",", "doch", "pas\u00b7sa\u00b7bel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PTKNEG", "$,", "ADV", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die Wachtel bleibt beim Wachtelschlag,", "tokens": ["Die", "Wach\u00b7tel", "bleibt", "beim", "Wach\u00b7tel\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Fink nicht wie Lerche singen mag.", "tokens": ["Fink", "nicht", "wie", "Ler\u00b7che", "sin\u00b7gen", "mag."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["NN", "PTKNEG", "KOKOM", "NN", "VVFIN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So ist's im schw\u00e4b'schen S\u00e4ngerhain;", "tokens": ["So", "ist's", "im", "schw\u00e4b'\u00b7schen", "S\u00e4n\u00b7ger\u00b7hain", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Preis, S\u00e4nger dir von Thule!", "tokens": ["Preis", ",", "S\u00e4n\u00b7ger", "dir", "von", "Thu\u00b7le", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "PPER", "APPR", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch h\u00f6r' es unterm Leichenstein:", "tokens": ["Doch", "h\u00f6r'", "es", "un\u00b7term", "Lei\u00b7chen\u00b7stein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bei uns gibt's ", "tokens": ["Bei", "uns", "gibt's"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Mit ", "tokens": ["Mit"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Was halt ihm aus dem Herzen springt.", "tokens": ["Was", "halt", "ihm", "aus", "dem", "Her\u00b7zen", "springt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}