{"textgrid.poem.45374": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Naturwissenschaften", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbder Mensch wird doch t\u00e4glich gescheiter.\u00ab", "tokens": ["\u00bb", "der", "Mensch", "wird", "doch", "t\u00e4g\u00b7lich", "ge\u00b7schei\u00b7ter", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "ADV", "ADJD", "ADJD", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Zuletzt ist doch vieles nur Schein.", "tokens": ["Zu\u00b7letzt", "ist", "doch", "vie\u00b7les", "nur", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIS", "ADV", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "\u00bbzum wenigsten kommen wir weiter.\u00ab", "tokens": ["\u00bb", "zum", "we\u00b7nigs\u00b7ten", "kom\u00b7men", "wir", "wei\u00b7ter", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPRART", "PIS", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ja, weiter in den Wald hinein.", "tokens": ["Ja", ",", "wei\u00b7ter", "in", "den", "Wald", "hin\u00b7ein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}