{"textgrid.poem.58089": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "127. Den S\u00e4ugling geht die Haushaltung nichts an", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie t\u00f6richt ist ein Kind, da es im Scho\u00df k\u00f6nnt' ruhn,", "tokens": ["Wie", "t\u00f6\u00b7richt", "ist", "ein", "Kind", ",", "da", "es", "im", "Scho\u00df", "k\u00f6nnt'", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$,", "KOUS", "PPER", "APPRART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn's selber sorgen will und gro\u00dfe Dinge tun!", "tokens": ["Wenn's", "sel\u00b7ber", "sor\u00b7gen", "will", "und", "gro\u00b7\u00dfe", "Din\u00b7ge", "tun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVINF", "VMFIN", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer klug ist, bleibe still in Jesu Scho\u00df verborgen,", "tokens": ["Wer", "klug", "ist", ",", "blei\u00b7be", "still", "in", "Je\u00b7su", "Scho\u00df", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "VVFIN", "ADJD", "APPR", "NE", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er sauge Gnad' um Gnad' und la\u00df die Mutter sorgen!", "tokens": ["Er", "sau\u00b7ge", "Gnad'", "um", "Gnad'", "und", "la\u00df", "die", "Mut\u00b7ter", "sor\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN", "KON", "VVIMP", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}