{"textgrid.poem.31656": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: So sey es halt! ich will das Vngl\u00fcck auf mich nehmen /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So sey es halt! ich will das Vngl\u00fcck auf mich nehmen /", "tokens": ["So", "sey", "es", "halt", "!", "ich", "will", "das", "Vn\u00b7gl\u00fcck", "auf", "mich", "neh\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "$.", "PPER", "VMFIN", "ART", "NN", "APPR", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "das schwere Creutz / aufmeiner Schultern schwachen Berg /", "tokens": ["das", "schwe\u00b7re", "Creutz", "/", "auf\u00b7mei\u00b7ner", "Schul\u00b7tern", "schwa\u00b7chen", "Berg", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ADJA", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "wie Atlas th\u00e4t' / ertragend mich dazu bequemen.", "tokens": ["wie", "At\u00b7las", "th\u00e4t'", "/", "er\u00b7tra\u00b7gend", "mich", "da\u00b7zu", "be\u00b7que\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VVFIN", "$(", "VVPP", "PRF", "PAV", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der H\u00f6chste wei\u00df / das Menschen-Krafft ist wie ein Zwerg /", "tokens": ["Der", "H\u00f6chs\u00b7te", "wei\u00df", "/", "das", "Men\u00b7schen\u00b7Krafft", "ist", "wie", "ein", "Zwerg", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "ART", "NN", "VAFIN", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "ob schon der Will dem vorgedachten Riesen gleicht.", "tokens": ["ob", "schon", "der", "Will", "dem", "vor\u00b7ge\u00b7dach\u00b7ten", "Rie\u00b7sen", "gleicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "VMFIN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der fliegend Lust / Gott inniglich bedienen will", "tokens": ["Der", "flie\u00b7gend", "Lust", "/", "Gott", "in\u00b7nig\u00b7lich", "be\u00b7die\u00b7nen", "will"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "$(", "NN", "ADJD", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "nach Reigers-Art / durch alls Gew\u00fclk zur Klarheit streichet.", "tokens": ["nach", "Rei\u00b7ger\u00b7sArt", "/", "durch", "alls", "Ge\u00b7w\u00fclk", "zur", "Klar\u00b7heit", "strei\u00b7chet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "APPR", "PIAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der schwere Leib / hat tausend Hindernus zum Ziel:", "tokens": ["Der", "schwe\u00b7re", "Leib", "/", "hat", "tau\u00b7send", "Hin\u00b7der\u00b7nus", "zum", "Ziel", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "VAFIN", "CARD", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "der Will' ist Feur / flammt Himmel-an und alls verzehret.", "tokens": ["der", "Will'", "ist", "Feur", "/", "flammt", "Him\u00b7mel\u00b7an", "und", "alls", "ver\u00b7zeh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$(", "VVFIN", "NE", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Krafft / ein Rauch / im Vngl\u00fccks Wind verschwindt geschwind:", "tokens": ["Die", "Krafft", "/", "ein", "Rauch", "/", "im", "Vn\u00b7gl\u00fccks", "Wind", "ver\u00b7schwindt", "ge\u00b7schwind", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "$(", "APPRART", "NN", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "es sey dann da\u00df sie Christus Wunden-Safft ernehret /", "tokens": ["es", "sey", "dann", "da\u00df", "sie", "Chris\u00b7tus", "Wun\u00b7den\u00b7\u00b7S\u00b7afft", "er\u00b7neh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "KOUS", "PPER", "NE", "NN", "VVFIN", "$("], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "und da\u00df man St\u00e4rk' in Kennung eigner Schwachheit find.", "tokens": ["und", "da\u00df", "man", "St\u00e4rk'", "in", "Ken\u00b7nung", "eig\u00b7ner", "Schwach\u00b7heit", "find", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "NN", "APPR", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Offt will mein Geist / mit Milo / einen Ochsen tragen /", "tokens": ["Offt", "will", "mein", "Geist", "/", "mit", "Mi\u00b7lo", "/", "ei\u00b7nen", "Och\u00b7sen", "tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "$(", "APPR", "NE", "$(", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "der Hydra K\u00f6pff' all' unerm\u00fcdet hauen ab:", "tokens": ["der", "Hyd\u00b7ra", "K\u00f6pf\u00b7f'", "all'", "un\u00b7er\u00b7m\u00fc\u00b7det", "hau\u00b7en", "ab", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "PIAT", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "-+-++-+-+-+-+", "measure": "unknown.measure.septa"}, "line.15": {"text": "bald ist er ganz vor Schmerz und Angst in mir erschlagen /", "tokens": ["bald", "ist", "er", "ganz", "vor", "Schmerz", "und", "Angst", "in", "mir", "er\u00b7schla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "da\u00df ich an ihm ein feigen Sardanapel hab.", "tokens": ["da\u00df", "ich", "an", "ihm", "ein", "fei\u00b7gen", "Sar\u00b7da\u00b7na\u00b7pel", "hab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ach Gott! ach wer den steiffen Muht stets w\u00e4hrhafft heget'", "tokens": ["Ach", "Gott", "!", "ach", "wer", "den", "steif\u00b7fen", "Muht", "stets", "w\u00e4hr\u00b7hafft", "he\u00b7get'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "$.", "XY", "PWS", "ART", "ADJA", "NN", "ADV", "VVFIN", "VVFIN"], "meter": "-+-+-+-+-++-+", "measure": "unknown.measure.septa"}, "line.18": {"text": "in Noht und Spott / in Krankheit / Vnlust / Schmerzen-Pein!", "tokens": ["in", "Noht", "und", "Spott", "/", "in", "Krank\u00b7heit", "/", "Vn\u00b7lust", "/", "Schmer\u00b7zen\u00b7Pein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$(", "APPR", "NN", "$(", "NN", "$(", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "den nichts auf Erd / was man auch nennen kan / beweget' /", "tokens": ["den", "nichts", "auf", "Erd", "/", "was", "man", "auch", "nen\u00b7nen", "kan", "/", "be\u00b7we\u00b7get'", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PIS", "APPR", "NN", "$(", "PWS", "PIS", "ADV", "VVINF", "VMFIN", "$(", "VVFIN", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "in ihm selbst k\u00f6nnt' in Vngl\u00fccks-W\u00fcrbel standhafft seyn!", "tokens": ["in", "ihm", "selbst", "k\u00f6nnt'", "in", "Vn\u00b7gl\u00fccks\u00b7W\u00fcr\u00b7bel", "stand\u00b7hafft", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VMFIN", "APPR", "NN", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "denkstu / mein Geist! wer kan doch da best\u00e4ndig leben /", "tokens": ["denks\u00b7tu", "/", "mein", "Geist", "!", "wer", "kan", "doch", "da", "be\u00b7st\u00e4n\u00b7dig", "le\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$(", "PPOSAT", "NN", "$.", "PWS", "VMFIN", "ADV", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "wo Vnbestand den Herrschung-Z\u00fcgel leitt und f\u00fchrt?", "tokens": ["wo", "Vn\u00b7be\u00b7stand", "den", "Herr\u00b7schung\u00b7Z\u00fc\u00b7gel", "leitt", "und", "f\u00fchrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "ach warum nicht? schau / ob die Wankel-Wellen heben", "tokens": ["ach", "wa\u00b7rum", "nicht", "?", "schau", "/", "ob", "die", "Wan\u00b7kel\u00b7Wel\u00b7len", "he\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["XY", "PWAV", "PTKNEG", "$.", "PTKVZ", "$(", "KOUS", "ART", "NN", "VVINF"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.24": {"text": "die Fels im Meer? nein! ihrer keiner wird ger\u00fchrt.", "tokens": ["die", "Fels", "im", "Meer", "?", "nein", "!", "ih\u00b7rer", "kei\u00b7ner", "wird", "ge\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$.", "PTKANT", "$.", "PPOSAT", "PIS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Der Diamant / das Bild der st\u00e4t-Best\u00e4ndigkeiten /", "tokens": ["Der", "Di\u00b7a\u00b7mant", "/", "das", "Bild", "der", "st\u00e4t\u00b7Be\u00b7st\u00e4n\u00b7dig\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "wird in der See / des Gegentheils Bewei\u00df / erzeugt.", "tokens": ["wird", "in", "der", "See", "/", "des", "Ge\u00b7gen\u00b7theils", "Be\u00b7wei\u00df", "/", "er\u00b7zeugt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "$(", "ART", "NN", "NN", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Der Perlen-Schneck / verschliest sich f\u00e4st zu allen Zeiten", "tokens": ["Der", "Per\u00b7len\u00b7Schneck", "/", "ver\u00b7schliest", "sich", "f\u00e4st", "zu", "al\u00b7len", "Zei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "VVFIN", "PRF", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "im sauren Reich / bis sich ein Himmel-Thau herneigt.", "tokens": ["im", "sau\u00b7ren", "Reich", "/", "bis", "sich", "ein", "Him\u00b7mel\u00b7\u00b7T\u00b7hau", "her\u00b7neigt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$(", "APPR", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.29": {"text": "Machs auch also / sey Herzf\u00e4st in den Vngl\u00fccks-L\u00e4uffen /", "tokens": ["Machs", "auch", "al\u00b7so", "/", "sey", "Herz\u00b7f\u00e4st", "in", "den", "Vn\u00b7gl\u00fccks\u00b7L\u00e4uf\u00b7fen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "$(", "VAFIN", "NN", "APPR", "ART", "NN", "$("], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.30": {"text": "und Demant-hart / will man dir st\u00f6ren deine Ruh.", "tokens": ["und", "De\u00b7mant\u00b7hart", "/", "will", "man", "dir", "st\u00f6\u00b7ren", "dei\u00b7ne", "Ruh", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$(", "VMFIN", "PIS", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "La\u00df Gottes Wort und s\u00fcsse Gnaden in dich treuffen.", "tokens": ["La\u00df", "Got\u00b7tes", "Wort", "und", "s\u00fcs\u00b7se", "Gna\u00b7den", "in", "dich", "treuf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "NN", "KON", "ADJA", "NN", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Der Gierden-Sitz / das Herz / schlie\u00df' durch Entschliessung zu:", "tokens": ["Der", "Gier\u00b7den\u00b7Sitz", "/", "das", "Herz", "/", "schlie\u00df'", "durch", "Ent\u00b7schlies\u00b7sung", "zu", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "$(", "VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "da\u00df nicht hinein die Erden-Bitterkeiten rinnen.", "tokens": ["da\u00df", "nicht", "hin\u00b7ein", "die", "Er\u00b7den\u00b7Bit\u00b7ter\u00b7kei\u00b7ten", "rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "die k\u00f6stlich Ruh' / ist dann dein Perlen-Zier und Pracht:", "tokens": ["die", "k\u00f6st\u00b7lich", "Ruh'", "/", "ist", "dann", "dein", "Per\u00b7len\u00b7Zier", "und", "Pracht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$(", "VAFIN", "ADV", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "die wirstu durch so kluges Kunst-Beginnen gwinnen.", "tokens": ["die", "wirs\u00b7tu", "durch", "so", "klu\u00b7ges", "Kunst\u00b7Be\u00b7gin\u00b7nen", "gwin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "APPR", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "HeRR / gibe du zum Willen Krafft und W\u00fcrckungs Macht.", "tokens": ["HeRR", "/", "gi\u00b7be", "du", "zum", "Wil\u00b7len", "Krafft", "und", "W\u00fcr\u00b7ckungs", "Macht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VVFIN", "PPER", "APPRART", "NN", "NN", "KON", "NN", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}}}}