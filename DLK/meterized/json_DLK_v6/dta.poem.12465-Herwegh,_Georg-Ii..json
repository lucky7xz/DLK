{"dta.poem.12465": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Ii.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kaum haben sie ", "tokens": ["Kaum", "ha\u00b7ben", "sie"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Vor ihrem Gas und Kerzenlicht,", "tokens": ["Vor", "ih\u00b7rem", "Gas", "und", "Ker\u00b7zen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Thr\u00e4nen selbst sind falsche Steine", "tokens": ["Die", "Thr\u00e4\u00b7nen", "selbst", "sind", "fal\u00b7sche", "Stei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und lohnen ihre Fassung nicht.", "tokens": ["Und", "loh\u00b7nen", "ih\u00b7re", "Fas\u00b7sung", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Gemalt sind ihre sch\u00f6nsten Rosen", "tokens": ["Ge\u00b7malt", "sind", "ih\u00b7re", "sch\u00f6ns\u00b7ten", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und kaum die Dornen \u00e4cht daran \u2014", "tokens": ["Und", "kaum", "die", "Dor\u00b7nen", "\u00e4cht", "da\u00b7ran"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADJD", "PAV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So st\u00fcrmen sie, die Ruhelosen,", "tokens": ["So", "st\u00fcr\u00b7men", "sie", ",", "die", "Ru\u00b7he\u00b7lo\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf ihrer ausgetretnen Bahn.", "tokens": ["Auf", "ih\u00b7rer", "aus\u00b7ge\u00b7tret\u00b7nen", "Bahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das ist ein Schachern, ein Erwerben,", "tokens": ["Das", "ist", "ein", "Scha\u00b7chern", ",", "ein", "Er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein R\u00e4derrasseln Tag und Nacht \u2014", "tokens": ["Ein", "R\u00e4\u00b7der\u00b7ras\u00b7seln", "Tag", "und", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich m\u00f6cht' in dieser Stadt nicht sterben,", "tokens": ["Ich", "m\u00f6cht'", "in", "die\u00b7ser", "Stadt", "nicht", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PDAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die auf den Gr\u00e4bern Hochzeit macht.", "tokens": ["Die", "auf", "den", "Gr\u00e4\u00b7bern", "Hoch\u00b7zeit", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Welch Gl\u00fcck, da\u00df ihr in dem Getriebe", "tokens": ["Welch", "Gl\u00fcck", ",", "da\u00df", "ihr", "in", "dem", "Ge\u00b7trie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein deutsches Spinnrad nicht vermi\u00dft,", "tokens": ["Mein", "deut\u00b7sches", "Spinn\u00b7rad", "nicht", "ver\u00b7mi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ihr nicht ahnt, was deutsche Liebe,", "tokens": ["Da\u00df", "ihr", "nicht", "ahnt", ",", "was", "deut\u00b7sche", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "$,", "PWS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht ahnt, was deutsche Narrheit ist!", "tokens": ["Nicht", "ahnt", ",", "was", "deut\u00b7sche", "Nar\u00b7rheit", "ist", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "PWS", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}