{"textgrid.poem.38696": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Amors Zauber", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo Amors Fl\u00fcgel weben,", "tokens": ["Wo", "A\u00b7mors", "Fl\u00fc\u00b7gel", "we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist nie die Sch\u00f6pfung todt;", "tokens": ["Ist", "nie", "die", "Sch\u00f6p\u00b7fung", "todt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Wildni\u00df gibt er Leben,", "tokens": ["Der", "Wild\u00b7ni\u00df", "gibt", "er", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Sturmnacht Morgenroth!", "tokens": ["Der", "Sturm\u00b7nacht", "Mor\u00b7gen\u00b7roth", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Im Ocean entfalten", "tokens": ["Im", "O\u00b7cean", "ent\u00b7fal\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Geklippe, nackt und stumm,", "tokens": ["Ge\u00b7klip\u00b7pe", ",", "nackt", "und", "stumm", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wenn seine Zauber walten,", "tokens": ["Wenn", "sei\u00b7ne", "Zau\u00b7ber", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sich zum Elysium!", "tokens": ["Sich", "zum", "E\u00b7ly\u00b7si\u00b7um", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}