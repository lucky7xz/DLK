{"dta.poem.11590": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Die Anti-Sympathetiker.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Wie w\u00fcrde sie nicht durch den Geist bezwungen?", "tokens": ["Wie", "w\u00fcr\u00b7de", "sie", "nicht", "durch", "den", "Geist", "be\u00b7zwun\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn sich getrennte Kr\u00e4fte wiederkennen,", "tokens": ["Wenn", "sich", "ge\u00b7trenn\u00b7te", "Kr\u00e4f\u00b7te", "wie\u00b7der\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf ein Erinn'rungswort entbrennen,", "tokens": ["Auf", "ein", "Er\u00b7inn'\u00b7rungs\u00b7wort", "ent\u00b7bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Krankheit weicht, das Blut sich pl\u00f6tzlich stillt,", "tokens": ["Die", "Krank\u00b7heit", "weicht", ",", "das", "Blut", "sich", "pl\u00f6tz\u00b7lich", "stillt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das ist das Wunder, wie's die Dummheit schilt!", "tokens": ["Das", "ist", "das", "Wun\u00b7der", ",", "wie's", "die", "Dumm\u00b7heit", "schilt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "\u2014 La\u00df die Schwachmatiker nur immer r\u00e4sonniren,", "tokens": ["La\u00df", "die", "Schwach\u00b7ma\u00b7ti\u00b7ker", "nur", "im\u00b7mer", "r\u00e4\u00b7son\u00b7ni\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "ART", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Und rechn' es ihnen allzu hoch nicht an!", "tokens": ["Und", "rechn'", "es", "ih\u00b7nen", "all\u00b7zu", "hoch", "nicht", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "PTKA", "ADJD", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Denn, wenn sie Gott und die Natur borniren,", "tokens": ["Denn", ",", "wenn", "sie", "Gott", "und", "die", "Na\u00b7tur", "bor\u00b7ni\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "NN", "KON", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Es streckt sich Keiner l\u00e4nger als er kann.", "tokens": ["Es", "streckt", "sich", "Kei\u00b7ner", "l\u00e4n\u00b7ger", "als", "er", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PIS", "ADJD", "KOKOM", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}