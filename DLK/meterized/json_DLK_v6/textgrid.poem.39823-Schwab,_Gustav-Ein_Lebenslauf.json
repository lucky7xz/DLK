{"textgrid.poem.39823": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Ein Lebenslauf", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In unser armes Fleisch und Blut", "tokens": ["In", "un\u00b7ser", "ar\u00b7mes", "Fleisch", "und", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verkleidet sich das ew'ge Gut.", "tokens": ["Ver\u00b7klei\u00b7det", "sich", "das", "ew'\u00b7ge", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Den aller Weltkreis nicht beschlo\u00df,", "tokens": ["Den", "al\u00b7ler", "Welt\u00b7kreis", "nicht", "be\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der liegt in einer Mutter Schoos.", "tokens": ["Der", "liegt", "in", "ei\u00b7ner", "Mut\u00b7ter", "Schoos", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sp\u00e4t, aus der H\u00fctte, tritt hinaus", "tokens": ["Sp\u00e4t", ",", "aus", "der", "H\u00fct\u00b7te", ",", "tritt", "hin\u00b7aus"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "$,", "APPR", "ART", "NN", "$,", "VVFIN", "APZR"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der Gottessohn in Vaters Haus.", "tokens": ["Der", "Got\u00b7tes\u00b7sohn", "in", "Va\u00b7ters", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Glimmenden Docht, zersto\u00dfnes Rohr,", "tokens": ["Glim\u00b7men\u00b7den", "Docht", ",", "zer\u00b7sto\u00df\u00b7nes", "Rohr", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Er facht ihn an, richtet's empor.", "tokens": ["Er", "facht", "ihn", "an", ",", "rich\u00b7tet's", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "In Geist und Wahrheit beten hei\u00dft", "tokens": ["In", "Geist", "und", "Wahr\u00b7heit", "be\u00b7ten", "hei\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er alles Volk zum Einen Geist.", "tokens": ["Er", "al\u00b7les", "Volk", "zum", "Ei\u00b7nen", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "APPRART", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00bbdas fleischgeborne Fleisch lebt nicht,", "tokens": ["\u00bb", "das", "fleischge\u00b7bor\u00b7ne", "Fleisch", "lebt", "nicht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VVFIN", "PTKNEG", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Bis es als Geist sein Band zerbricht.\u00ab", "tokens": ["Bis", "es", "als", "Geist", "sein", "Band", "zer\u00b7bricht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "KOUS", "NN", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Zum Zeugni\u00df dr\u00fcckt er der Natur", "tokens": ["Zum", "Zeug\u00b7ni\u00df", "dr\u00fcckt", "er", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Allm\u00e4chtig ein des Geistes Spur:", "tokens": ["All\u00b7m\u00e4ch\u00b7tig", "ein", "des", "Geis\u00b7tes", "Spur", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Er wandelt Wassers Eigenschaft", "tokens": ["Er", "wan\u00b7delt", "Was\u00b7sers", "Ei\u00b7gen\u00b7schaft"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In geist'ger Traube Saft und Kraft;", "tokens": ["In", "geist'\u00b7ger", "Trau\u00b7be", "Saft", "und", "Kraft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Sein Wort vertausendf\u00e4ltigt Brot,", "tokens": ["Sein", "Wort", "ver\u00b7tau\u00b7send\u00b7f\u00e4l\u00b7tigt", "Brot", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Athem haucht hinweg den Tod.", "tokens": ["Sein", "A\u00b7them", "haucht", "hin\u00b7weg", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Er schlie\u00dft der See den Wellenmund", "tokens": ["Er", "schlie\u00dft", "der", "See", "den", "Wel\u00b7len\u00b7mund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und hei\u00dft sie schweigen bis zum Grund;", "tokens": ["Und", "hei\u00dft", "sie", "schwei\u00b7gen", "bis", "zum", "Grund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Es greift sein Fluch der Bl\u00e4tter Saum:", "tokens": ["Es", "greift", "sein", "Fluch", "der", "Bl\u00e4t\u00b7ter", "Saum", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nacht\u00fcber dorrt der Feigenbaum.", "tokens": ["Nach\u00b7t\u00fc\u00b7ber", "dorrt", "der", "Fei\u00b7gen\u00b7baum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Dann beut er, S\u00fcndern zum Gewinn,", "tokens": ["Dann", "beut", "er", ",", "S\u00fcn\u00b7dern", "zum", "Ge\u00b7winn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Schlangenstich die Ferse hin;", "tokens": ["Dem", "Schlan\u00b7gen\u00b7stich", "die", "Fer\u00b7se", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Er heftet sich ans Kreuz als Schuld,", "tokens": ["Er", "hef\u00b7tet", "sich", "ans", "Kreuz", "als", "Schuld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "KOUS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Zorn verb\u00fc\u00dft er als Geduld;", "tokens": ["Den", "Zorn", "ver\u00b7b\u00fc\u00dft", "er", "als", "Ge\u00b7duld", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KOUS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Er keimt als Saatkorn einer Welt,", "tokens": ["Er", "keimt", "als", "Saat\u00b7korn", "ei\u00b7ner", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er fliegt als Licht zum Sternenzelt.", "tokens": ["Er", "fliegt", "als", "Licht", "zum", "Ster\u00b7nen\u00b7zelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Von dort besucht auf Geistespfad", "tokens": ["Von", "dort", "be\u00b7sucht", "auf", "Geis\u00b7te\u00b7spfad"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er Kind um Kind im Wasserbad;", "tokens": ["Er", "Kind", "um", "Kind", "im", "Was\u00b7ser\u00b7bad", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Von dort geht er durch Lippen ein", "tokens": ["Von", "dort", "geht", "er", "durch", "Lip\u00b7pen", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "APPR", "NN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als Fleisch und Blut in Brot und Wein.", "tokens": ["Als", "Fleisch", "und", "Blut", "in", "Brot", "und", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Von dort kehrt er in Herzen rein", "tokens": ["Von", "dort", "kehrt", "er", "in", "Her\u00b7zen", "rein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "APPR", "NN", "ADJD"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Als Demut und als Sanftmut ein.", "tokens": ["Als", "De\u00b7mut", "und", "als", "Sanft\u00b7mut", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "KOUS", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.18": {"line.1": {"text": "Das B\u00f6se straft sein letzt Gericht,", "tokens": ["Das", "B\u00f6\u00b7se", "straft", "sein", "letzt", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAINF", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es macht die Finsterni\u00df zu nicht.", "tokens": ["Es", "macht", "die", "Fins\u00b7ter\u00b7ni\u00df", "zu", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.19": {"line.1": {"text": "Und ist die Nacht in Tag verkl\u00e4rt,", "tokens": ["Und", "ist", "die", "Nacht", "in", "Tag", "ver\u00b7kl\u00e4rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Leben aller Tod verkehrt:", "tokens": ["In", "Le\u00b7ben", "al\u00b7ler", "Tod", "ver\u00b7kehrt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.20": {"line.1": {"text": "Dann wird der Sohn im Vater frei,", "tokens": ["Dann", "wird", "der", "Sohn", "im", "Va\u00b7ter", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df Alles Gott in Allem sei!", "tokens": ["Da\u00df", "Al\u00b7les", "Gott", "in", "Al\u00b7lem", "sei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "NN", "APPR", "PIS", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}