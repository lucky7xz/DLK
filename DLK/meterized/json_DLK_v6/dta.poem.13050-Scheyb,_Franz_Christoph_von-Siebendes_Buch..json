{"dta.poem.13050": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201emein, sagt mir! ist die Welt nicht alles dessen voll?", "tokens": ["\u201e", "mein", ",", "sagt", "mir", "!", "ist", "die", "Welt", "nicht", "al\u00b7les", "des\u00b7sen", "voll", "?"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "$,", "VVFIN", "PPER", "$.", "VAFIN", "ART", "NN", "PTKNEG", "PIS", "PDS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "430\u201dSagt! wie man ihrem Glanz mehr Schimmer geben soll?", "tokens": ["\"", "Sagt", "!", "wie", "man", "ih\u00b7rem", "Glanz", "mehr", "Schim\u00b7mer", "ge\u00b7ben", "soll", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$.", "PWAV", "PIS", "PPOSAT", "NN", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eund konntet ihr nicht oft in ihren Augen lesen,", "tokens": ["\u201e", "und", "konn\u00b7tet", "ihr", "nicht", "oft", "in", "ih\u00b7ren", "Au\u00b7gen", "le\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VMFIN", "PPER", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eda\u00df allzeit ihr die Pracht unangenehm gewesen?", "tokens": ["\u201e", "da\u00df", "all\u00b7zeit", "ihr", "die", "Pracht", "un\u00b7an\u00b7ge\u00b7nehm", "ge\u00b7we\u00b7sen", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "ADV", "PPER", "ART", "NN", "ADJD", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201ewas Pomp ist, ha\u00dft ihr Herz. Jedoch gesezt, ich blieb", "tokens": ["\u201e", "was", "Pomp", "ist", ",", "ha\u00dft", "ihr", "Herz", ".", "Je\u00b7doch", "ge\u00b7sezt", ",", "ich", "blieb"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "PWS", "NN", "VAFIN", "$,", "VVFIN", "PPOSAT", "NN", "$.", "ADV", "VVPP", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201emit euerm Vorschlag eins; ist die\u00df nicht euer Trieb?", "tokens": ["\u201e", "mit", "eu\u00b7erm", "Vor\u00b7schlag", "eins", ";", "ist", "die\u00df", "nicht", "eu\u00b7er", "Trieb", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "PIS", "$.", "VAFIN", "PDS", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "435\u201dWie taugte dieser Streit nach W\u00fcrde sie zu loben?", "tokens": ["\"", "Wie", "taug\u00b7te", "die\u00b7ser", "Streit", "nach", "W\u00fcr\u00b7de", "sie", "zu", "lo\u00b7ben", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PDAT", "NN", "APPR", "VAFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201ebeweiset diesen Saz, und macht etwelche Proben!", "tokens": ["\u201e", "be\u00b7wei\u00b7set", "die\u00b7sen", "Saz", ",", "und", "macht", "et\u00b7wel\u00b7che", "Pro\u00b7ben", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PDAT", "NN", "$,", "KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201enein! alles ist umsonst! vernehmet die Geschicht,", "tokens": ["\u201e", "nein", "!", "al\u00b7les", "ist", "um\u00b7sonst", "!", "ver\u00b7neh\u00b7met", "die", "Ge\u00b7schicht", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$.", "PIS", "VAFIN", "ADV", "$.", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201evon welcher die Natur in ihren B\u00fcchern spricht.", "tokens": ["\u201e", "von", "wel\u00b7cher", "die", "Na\u00b7tur", "in", "ih\u00b7ren", "B\u00fc\u00b7chern", "spricht", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PRELS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}