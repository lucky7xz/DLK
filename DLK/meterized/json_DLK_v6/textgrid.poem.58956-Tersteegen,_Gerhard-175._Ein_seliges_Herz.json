{"textgrid.poem.58956": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "175. Ein seliges Herz", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O sel'ges Herz, drin Gottes Sohn", "tokens": ["O", "sel'\u00b7ges", "Herz", ",", "drin", "Got\u00b7tes", "Sohn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "ADV", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sitzt als auf seinem Friedensthron!", "tokens": ["Sitzt", "als", "auf", "sei\u00b7nem", "Frie\u00b7denst\u00b7hron", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch la\u00df ihn erst die Feinde d\u00e4mpfen,", "tokens": ["Doch", "la\u00df", "ihn", "erst", "die", "Fein\u00b7de", "d\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Man kann nicht siegen ohne K\u00e4mpfen!", "tokens": ["Man", "kann", "nicht", "sie\u00b7gen", "oh\u00b7ne", "K\u00e4mp\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PTKNEG", "VVINF", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}