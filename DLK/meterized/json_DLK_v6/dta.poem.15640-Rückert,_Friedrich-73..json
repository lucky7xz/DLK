{"dta.poem.15640": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "73.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wol hat ein eigenes Bewu\u00dftseyn jede Zeit", "tokens": ["Wol", "hat", "ein", "ei\u00b7ge\u00b7nes", "Be\u00b7wu\u00df\u00b7tseyn", "je\u00b7de", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PIAT", "NN"], "meter": "+--+---+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Des was ihr widersteht, und des was ihr gedeiht.", "tokens": ["Des", "was", "ihr", "wi\u00b7der\u00b7steht", ",", "und", "des", "was", "ihr", "ge\u00b7deiht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PWS", "PPER", "VVFIN", "$,", "KON", "ART", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und jeder Einzelne hat ein Bewu\u00dftseyn dessen,", "tokens": ["Und", "je\u00b7der", "Ein\u00b7zel\u00b7ne", "hat", "ein", "Be\u00b7wu\u00df\u00b7tseyn", "des\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "VAFIN", "ART", "NN", "PDS", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wie dem Bewu\u00dftseyn er der Zeit ist angemessen.", "tokens": ["Wie", "dem", "Be\u00b7wu\u00df\u00b7tseyn", "er", "der", "Zeit", "ist", "an\u00b7ge\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn ein Bewu\u00dftseyn nicht, doch ein geheim Gef\u00fchl,", "tokens": ["Wenn", "ein", "Be\u00b7wu\u00df\u00b7tseyn", "nicht", ",", "doch", "ein", "ge\u00b7heim", "Ge\u00b7f\u00fchl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das bald behaglich wohl, bald macht unheimlich schw\u00fcl.", "tokens": ["Das", "bald", "be\u00b7hag\u00b7lich", "wohl", ",", "bald", "macht", "un\u00b7heim\u00b7lich", "schw\u00fcl", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "ADV", "$,", "ADV", "VVFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Verdenkt es keinem, wenn er tobt, doch ist sein Toben", "tokens": ["Ver\u00b7denkt", "es", "kei\u00b7nem", ",", "wenn", "er", "tobt", ",", "doch", "ist", "sein", "To\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PIS", "$,", "KOUS", "PPER", "VVFIN", "$,", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Umsonst, der von der Zeit sich f\u00fchlet aufgehoben.", "tokens": ["Um\u00b7sonst", ",", "der", "von", "der", "Zeit", "sich", "f\u00fch\u00b7let", "auf\u00b7ge\u00b7ho\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PRELS", "APPR", "ART", "NN", "PRF", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}