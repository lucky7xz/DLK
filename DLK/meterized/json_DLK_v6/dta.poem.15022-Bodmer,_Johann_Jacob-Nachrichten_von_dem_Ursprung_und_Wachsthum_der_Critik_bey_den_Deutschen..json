{"dta.poem.15022": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Nachrichten von dem Ursprung und  \n Wachsthum der Critik bey den  \n Deutschen.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1741", "urn": "urn:nbn:de:kobv:b4-200905198526", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir m\u00fcssen freylich nur, wir armes Volck, bekennen,", "tokens": ["Wir", "m\u00fcs\u00b7sen", "frey\u00b7lich", "nur", ",", "wir", "ar\u00b7mes", "Volck", ",", "be\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "$,", "PPER", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df Erde, Feuer, Luft, und Wasser, schrecklich brennen,", "tokens": ["Da\u00df", "Er\u00b7de", ",", "Feu\u00b7er", ",", "Luft", ",", "und", "Was\u00b7ser", ",", "schreck\u00b7lich", "bren\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "NN", "$,", "NN", "$,", "KON", "NN", "$,", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Aus Eifer gegen uns: Des hohen Himmels Haus,", "tokens": ["Aus", "Ei\u00b7fer", "ge\u00b7gen", "uns", ":", "Des", "ho\u00b7hen", "Him\u00b7mels", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPER", "$.", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das sch\u00fcttet seinen Zorn mit Blitz und Donner aus,", "tokens": ["Das", "sch\u00fct\u00b7tet", "sei\u00b7nen", "Zorn", "mit", "Blitz", "und", "Don\u00b7ner", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Von allen Ecken her, nachdem so schwere Plagen,", "tokens": ["Von", "al\u00b7len", "E\u00b7cken", "her", ",", "nach\u00b7dem", "so", "schwe\u00b7re", "Pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$,", "KOUS", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In dieser ", "tokens": ["In", "die\u00b7ser"], "token_info": ["word", "word"], "pos": ["APPR", "PDAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Auf unsren S\u00fcndenhals; doch gleichwohl ist ein Gott,", "tokens": ["Auf", "un\u00b7sren", "S\u00fcn\u00b7den\u00b7hals", ";", "doch", "gleich\u00b7wohl", "ist", "ein", "Gott", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "ADV", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der seiner Gnaden Licht l\u00e4\u00dft scheinen in der Noth,", "tokens": ["Der", "sei\u00b7ner", "Gna\u00b7den", "Licht", "l\u00e4\u00dft", "schei\u00b7nen", "in", "der", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "VVFIN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wann Caurus um den Mast mit harten St\u00fcrmen sauset,", "tokens": ["Wann", "Cau\u00b7rus", "um", "den", "Mast", "mit", "har\u00b7ten", "St\u00fcr\u00b7men", "sau\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wann die erz\u00f6rnte Flut um alle Seiten brauset,", "tokens": ["Wann", "die", "er\u00b7z\u00f6rn\u00b7te", "Flut", "um", "al\u00b7le", "Sei\u00b7ten", "brau\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und will mit uns Grundab: Dem nichts sich bergen kan;", "tokens": ["Und", "will", "mit", "uns", "Grund\u00b7ab", ":", "Dem", "nichts", "sich", "ber\u00b7gen", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PPER", "NN", "$.", "ART", "PIS", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der, ob er schon betr\u00fcbt, nimmt dennoch wieder an,", "tokens": ["Der", ",", "ob", "er", "schon", "be\u00b7tr\u00fcbt", ",", "nimmt", "den\u00b7noch", "wie\u00b7der", "an", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "ADV", "VVPP", "$,", "VVFIN", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Nur da\u00df man eifrig bl\u00e4\u00dft die starken Bu\u00dfvosaunen", "tokens": ["Nur", "da\u00df", "man", "eif\u00b7rig", "bl\u00e4\u00dft", "die", "star\u00b7ken", "Bu\u00df\u00b7vo\u00b7sau\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PIS", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Aus Feindschaft unsrer Schuld, und auch die Bethcarthaunen,", "tokens": ["Aus", "Feind\u00b7schaft", "uns\u00b7rer", "Schuld", ",", "und", "auch", "die", "Beth\u00b7cart\u00b7hau\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$,", "KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die Himmelsbrecher, pflanzt vor Gottes sch\u00f6ne Stadt,", "tokens": ["Die", "Him\u00b7mels\u00b7bre\u00b7cher", ",", "pflanzt", "vor", "Got\u00b7tes", "sch\u00f6\u00b7ne", "Stadt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Uns zeucht nicht eher ab, bis man das Jawort hat.", "tokens": ["Uns", "zeucht", "nicht", "e\u00b7her", "ab", ",", "bis", "man", "das", "Ja\u00b7wort", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "PTKVZ", "$,", "KOUS", "PIS", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Das g\u00fcldne Sonnenrad hat f\u00fcnfmahl seinen Wagen", "tokens": ["Das", "g\u00fcld\u00b7ne", "Son\u00b7nen\u00b7rad", "hat", "f\u00fcnf\u00b7mahl", "sei\u00b7nen", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Durch alle Zeichen schon am Himmel durchgetragen,", "tokens": ["Durch", "al\u00b7le", "Zei\u00b7chen", "schon", "am", "Him\u00b7mel", "durch\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Seit mich das Vaterland hat heissen fremde seyn. rc.", "tokens": ["Seit", "mich", "das", "Va\u00b7ter\u00b7land", "hat", "heis\u00b7sen", "frem\u00b7de", "seyn", ".", "rc", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VAFIN", "ADJA", "ADJA", "VAINF", "$.", "FM.la", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}}}}