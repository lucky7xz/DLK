{"dta.poem.15817": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O meine Blume, die dereinst mein Grab soll zieren,", "tokens": ["O", "mei\u00b7ne", "Blu\u00b7me", ",", "die", "de\u00b7reinst", "mein", "Grab", "soll", "zie\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "PRELS", "ADV", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie zittert' ich! du warst mir nah dran zu erfrieren.", "tokens": ["Wie", "zit\u00b7tert'", "ich", "!", "du", "warst", "mir", "nah", "dran", "zu", "er\u00b7frie\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "PPER", "ADJD", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dem G\u00e4rtner Dank, der dich entrissen der Gefahr!", "tokens": ["Dem", "G\u00e4rt\u00b7ner", "Dank", ",", "der", "dich", "ent\u00b7ris\u00b7sen", "der", "Ge\u00b7fahr", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Allein wie ist das Herz des Menschen undankbar:", "tokens": ["Al\u00b7lein", "wie", "ist", "das", "Herz", "des", "Men\u00b7schen", "un\u00b7dank\u00b7bar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "VAFIN", "ART", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Gerettet seh' ich dich, doch nun seh' ich dich serben;", "tokens": ["Ge\u00b7ret\u00b7tet", "seh'", "ich", "dich", ",", "doch", "nun", "seh'", "ich", "dich", "ser\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "PRF", "$,", "ADV", "ADV", "VVFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Soll nie gesundes Roth die Wange mehr dir f\u00e4rben?", "tokens": ["Soll", "nie", "ge\u00b7sun\u00b7des", "Roth", "die", "Wan\u00b7ge", "mehr", "dir", "f\u00e4r\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADJA", "NN", "ART", "NN", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So sah ich besser dich mit einmal sterben.", "tokens": ["So", "sah", "ich", "bes\u00b7ser", "dich", "mit", "ein\u00b7mal", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PRF", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}